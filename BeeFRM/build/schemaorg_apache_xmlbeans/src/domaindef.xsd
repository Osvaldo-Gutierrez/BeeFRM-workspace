<xsd:schema xmlns:xsd='http://www.w3.org/2001/XMLSchema'
           targetNamespace='http://cl/bee/genfrmjava/domaindef/schema'
                     xmlns='http://cl/bee/genfrmjava/domaindef/schema'>

 <!-- ========================================================================================== ==
   == domains
   == ========================================================================================== -->

 <xsd:element name='domains'>

  <xsd:complexType>

   <xsd:sequence>
     <xsd:element ref='domain' minOccurs='0' maxOccurs='unbounded'/>
   </xsd:sequence>

  </xsd:complexType>

 </xsd:element>

 <!-- ========================================================================================== ==
   == domain
   == ========================================================================================== -->

 <xsd:element name='domain'>

  <xsd:complexType>

    <xsd:attribute name='name'       type='xsd:string' use='required'/>
    <xsd:attribute name='system'     type='xsd:string' use='required'/>
    <xsd:attribute name='registry'   type='xsd:string' default=''/>
    <xsd:attribute name='table'      type='xsd:string' use='required'/>
    <xsd:attribute name='validation' type='xsd:string' default=''/>
    <xsd:attribute name='search'     type='xsd:string' default=''/>

  </xsd:complexType>

 </xsd:element>

</xsd:schema>