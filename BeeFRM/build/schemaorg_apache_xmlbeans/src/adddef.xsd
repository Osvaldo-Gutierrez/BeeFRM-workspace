<xsd:schema xmlns:xsd='http://www.w3.org/2001/XMLSchema'
           targetNamespace='http://cl/bee/genfrmjava/adddef/schema'
                     xmlns='http://cl/bee/genfrmjava/adddef/schema'>

 <!-- ========================================================================================== ==
   == additional
   == ========================================================================================== -->

 <xsd:element name='additional'>

  <xsd:complexType>

   <xsd:sequence>
     <xsd:element ref='section' minOccurs='0' maxOccurs='unbounded'/>
   </xsd:sequence>

   <xsd:attribute name='system' type='xsd:string' use='required'/>
   <xsd:attribute name='form'   type='xsd:string' use='required'/>

  </xsd:complexType>

 </xsd:element>

 <!-- ========================================================================================== ==
   == section
   == ========================================================================================== -->

 <xsd:element name='section'>

  <xsd:complexType>

    <xsd:sequence>
      <xsd:element ref='code'/>
    </xsd:sequence>

    <xsd:attribute name='name' type='xsd:string' use='required'/>
  </xsd:complexType>

 </xsd:element>

 <!-- ========================================================================================== ==
   == code
   == ========================================================================================== -->

 <xsd:element name='code' type='xsd:string'/>

</xsd:schema>