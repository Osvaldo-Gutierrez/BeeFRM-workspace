           IF     CAC-COD-CPTO IN CCA-FLD NOT > SPACES
              AND DBC-NUM-ICLI IN CCA-FLD NOT > ZEROES
              AND DBC-VRF-ICLI IN CCA-FLD NOT > SPACES
              AND DBC-IND-ICLI IN CCA-FLD NOT > SPACES
              AND DBC-GLS-ICLI IN CCA-FLD NOT > IDC-INIC-GLS
              AND CAC-NUM-DINI IN CCA-FLD NOT > ZEROES
              AND CAC-NUM-MINI IN CCA-FLD NOT > ZEROES
              AND CAC-NUM-SINI IN CCA-FLD NOT > ZEROES
              AND CAC-NUM-AINI IN CCA-FLD NOT > ZEROES
              AND CAC-HRA-HINI IN CCA-FLD NOT > ZEROES
              AND CAC-IND-VIGE IN CCA-FLD NOT > SPACES
                PERFORM ERR-KEY
                PERFORM FST-KEY-CCA.
       FIN-VAL-CON-IKY-CCA.
           EXIT.

       VAL-NUM-KEY-CCA SECTION.
       INI-VAL-NUM-KEY-CCA.
           PERFORM VAL-NUM-IKY-CCA.
           PERFORM VAL-NUM-MKY-CCA.
           PERFORM VAL-NUM-AKY-CCA.
       FIN-VAL-NUM-KEY-CCA.
           EXIT.

       VAL-NUM-IKY-CCA SECTION.
       INI-VAL-NUM-IKY-CCA.
       VAL-NUM-DBC-NUM-ICLI.
           MOVE DBC-NUM-ICLI-ALF IN CCA-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO DBC-NUM-ICLI IN CCA-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO DBC-NUM-ICLI-LEN IN CCA-FLD
                GO TO FIN-VAL-NUM-IKY-CCA.
       VAL-NUM-CAC-HRA-HINI.
           MOVE CAC-HRA-HINI-ALF IN CCA-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 6 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO CAC-HRA-HINI IN CCA-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO CAC-HRA-HINI-LEN IN CCA-FLD
                GO TO FIN-VAL-NUM-IKY-CCA.
       FIN-VAL-NUM-IKY-CCA.
           EXIT.

       VAL-NUM-MKY-CCA SECTION.
       INI-VAL-NUM-MKY-CCA.
       FIN-VAL-NUM-MKY-CCA.
           EXIT.

       VAL-NUM-AKY-CCA SECTION.
       INI-VAL-NUM-AKY-CCA.
       VAL-NUM-FRM-HRA-INIS.
           MOVE FRM-HRA-INIS-ALF IN CCA-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 6 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO FRM-HRA-INIS IN CCA-FLD
           ELSE
