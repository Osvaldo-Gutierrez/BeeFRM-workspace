<?xml version = "1.0" encoding = "ISO-8859-1" ?>

<additional xmlns        = "http://cl/bee/genfrmjava/adddef/schema"
      xmlns:xsi          = "http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation = "http://cl/bee/genfrmjava/adddef/schema adddef.xsd"
      system             = "SGC"
      form               = "CCO">

  <!-- ========================================================================================== ==
    == FIN_PUT_DBC_CCO
    == ========================================================================================== -->

  <section name="FIN_PUT_DBC_CCO">

    <code><![CDATA[      *<<<< FIN_PUT_DBC_CCO
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.
           PERFORM CPT-BLKS.
           MOVE CPT-STRN TO DBC-GLS-NOMC IN CCO-FLD.
      *>>>>
]]></code>

  </section>

  <!-- ========================================================================================== ==
    == FIN_PUT_RCO_CCO
    == ========================================================================================== -->

  <section name="FIN_PUT_RCO_CCO">

    <code><![CDATA[      *<<<< FIN_PUT_RCO_CCO
      *     IF NOT FIO-STAT-OKS
      *         MOVE ZEROES TO FRM-NUM-AANT IN CCO-FLD.
      *         MOVE ZEROES TO FRM-NUM-MANT IN CCO-FLD.
      *         MOVE ZEROES TO FRM-FEC-FUOB IN CCO-FLD.
      *         GO TO FIN-PUT-RCO-CCO.
      *     PERFORM GET-FHOY.
      *     IF FEC-MHOY NOT < DBC-NUM-MIOP IN DBC
      *         SUBTRACT DBC-NUM-AIOP IN DBC FROM FEC-AHOY
      *           GIVING FRM-NUM-AANT IN CCO-FLD
      *         SUBTRACT DBC-NUM-MIOP IN DBC FROM FEC-MHOY
      *           GIVING FRM-NUM-MANT IN CCO-FLD
      *     ELSE
      *         SUBTRACT DBC-NUM-AIOP IN DBC FROM FEC-AHOY
      *           GIVING FRM-NUM-AANT IN CCO-FLD
      *         SUBTRACT 1 FROM FRM-NUM-AANT IN CCO-FLD
      *         COMPUTE FRM-NUM-MANT IN CCO-FLD = FEC-MHOY +
      *                            12 - DBC-NUM-MIOP IN DBC.
      **    IF DBC-NUM-OVIG IN DBC NOT = ZEROES
      **       MOVE ZEROES TO FRM-FEC-FUOB IN CCO-FLD
      **    ELSE
      *        MOVE DBC-NUM-STOP IN DBC TO FRM-NUM-SUOB IN CCO-FLD.
      *        MOVE DBC-NUM-ATOP IN DBC TO FRM-NUM-AUOB IN CCO-FLD.
      *        MOVE DBC-NUM-MTOP IN DBC TO FRM-NUM-MUOB IN CCO-FLD.
      *        MOVE DBC-NUM-DTOP IN DBC TO FRM-NUM-DUOB IN CCO-FLD.
      *>>>>
]]></code>

  </section>

  <!-- ========================================================================================== ==
    == INI_VAL_CON_KEY_CCO
    == ========================================================================================== -->

  <section name="INI_VAL_CON_KEY_CCO">

    <code><![CDATA[      *<<<< INI_VAL_CON_KEY_CCO
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8
               GO TO FIN-VAL-CON-KEY-CCO.
      *>>>>
]]></code>

  </section>

  <!-- ========================================================================================== ==
    == NOT PGM_PER
    == ========================================================================================== -->

  <section name="NOT PGM_PER">

    <code><![CDATA[]]></code>

  </section>

  <!-- ========================================================================================== ==
    == NOT PGM_PER AND DBC_GLS_ICLI
    == ========================================================================================== -->

  <section name="NOT PGM_PER AND DBC_GLS_ICLI" concatenate="1">

    <code><![CDATA[      *<<<< DBC_GLS_ICLI
            MOVE DBC-GLS-ICLI IN CCO-FLD TO IDC-GLS-RUTV.
            PERFORM VAL-GLS.
            MOVE IDC-GLS-RUTV TO DBC-GLS-ICLI IN CCO-FLD.
      *>>>>
]]></code>

  </section>

  <!-- ========================================================================================== ==
    == NOT PGM_PER AND DBC_IND_ICLI
    == ========================================================================================== -->

  <section name="NOT PGM_PER AND DBC_IND_ICLI" concatenate="1">

    <code><![CDATA[      *<<<< DBC_IND_ICLI
            MOVE DBC-IND-ICLI IN CCO-FLD TO IDC-IND-RUTV.
            PERFORM VAL-IDC.
            MOVE IDC-IND-RUTV TO DBC-IND-ICLI IN CCO-FLD.
      *>>>>
]]></code>

  </section>

  <!-- ========================================================================================== ==
    == NOT PGM_PER AND DBC_VRF_ICLI
    == ========================================================================================== -->

  <section name="NOT PGM_PER AND DBC_VRF_ICLI" concatenate="1">

    <code><![CDATA[      *<<<< DBC_VRF_ICLI
           MOVE IDC-ZERO-S TO IDC-ZERO.
           MOVE DBC-NUM-ICLI IN CCO-FLD TO IDC-NUM-RUTV.
           MOVE DBC-VRF-ICLI IN CCO-FLD TO IDC-VRF-RUTV.
           PERFORM VAL-RUT.
      *>>>>
]]></code>

  </section>

  <!-- ========================================================================================== ==
    == NOT PGM_PER AND INI_PUT_DBC_CCO
    == ========================================================================================== -->

  <section name="NOT PGM_PER AND INI_PUT_DBC_CCO" concatenate="1">

    <code><![CDATA[      *<<<< INI_PUT_DBC_CCO
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-CCO.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-CCO
               GO TO FIN-PUT-DBC-CCO.
      *>>>>
]]></code>

  </section>

  <!-- ========================================================================================== ==
    == NOT PGM_PER AND INI_PUT_RCO_CCO
    == ========================================================================================== -->

  <section name="NOT PGM_PER AND INI_PUT_RCO_CCO" concatenate="1">

    <code><![CDATA[      *<<<< INI_PUT_RCO_CCO
           IF PGM-STAT-RCO-NUL
               GO TO FIN-PUT-RCO-CCO.
           IF NOT PGM-STAT-RCO-OKS
               PERFORM CLR-RCO-CCO
               GO TO FIN-PUT-RCO-CCO.
      *>>>>
]]></code>

  </section>

