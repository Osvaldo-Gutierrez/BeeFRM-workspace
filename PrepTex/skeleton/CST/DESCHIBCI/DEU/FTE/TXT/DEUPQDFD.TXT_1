*%! Codigo adicional para progarma
*% NO_HDR_CST = FALSE
*% HDR_CST = TRUE
*% INC_FIO = FALSE
*% PGM_PTC = TRUE
*% NO_TIENE_TIMESTAMP = FALSE
*%! IF PGM_CHI AND PGM_BCI
*%!     PGM_PTC = TRUE
*%! END
*% GNSTCA = 270
*% IF FIN_SCR_VARI
*% END
*% IF WSS_DTC
*% END
*% IF WSS
      *<<<< WSS                                                         
       01  WSS-VARI.                                                    
           03  WSS-CIC-ICLI                   PIC X(12).                
       COPY SGCBRDBC.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.        
       COPY DEUBRESC.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-ESC-REQA==.        
       COPY DEUWGDFD.                                                   
       COPY GNSWGCPT.                                                   
       COPY GNSWGSTR.                                                   
      *>>>>                                                             
*% END
*% IF FIN_PGM_STAT
           03 WSS-TOT                   VALUE ZEROES    PIC 9(03).      
*% END
*% IF MI_COMMAREA
      *<<<< MI_COMMAREA                                                 
           03 CMA-VARI.                                                 
              05 SCR-IMAX      COMP                   PIC S9(04).       
              05 SCR-I         COMP                   PIC S9(04).       
      *>>>>                                                             
*% END
*% IF INI_FIO
           IF SCR-OFRM-SRV                                              
              MOVE TSK-TERM-ALF TO DFD-COD-TERM                         
              MOVE TSK-TERM-ALF TO DFD-COD-TERM-MAS                     
           ELSE                                                         
              MOVE EIBTRMID     TO DFD-COD-TERM                         
              MOVE EIBTRMID     TO DFD-COD-TERM-MAS.                    
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC                                                 
           IF FRM-FFLD = FRM-FFLD-PF6                                   
                    OR   FRM-FFLD-PF5                                   
               PERFORM VAL-NUM-KEY-DFD                                  
               PERFORM VAL-NUM-FLD-DFD                                  
               PERFORM RST-STAT                                         
               PERFORM FND-SCR-KEY THRU FIN-SCR-KEY                     
               IF SCR-STAT NOT = SCR-STAT-ABT                           
                   PERFORM SCR-TRL                                      
               ELSE                                                     
                   PERFORM DESSCRTRL                                    
                   GO TO FIN-SCR-ACC.                                   
      *>>>>                                                             
*% END
*% IF PFS_SCR_GKEY
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON OR PFS_GET_ACC                                  
           IF NOT (FRM-FFLD = FRM-FFLD-PF6                              
                         OR   FRM-FFLD-PF5)                             
      *>>>>                                                             
*% END
*% IF FIO
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY                                                 
           MOVE SPACES TO FRM-MEN1.                                     
           IF FRM-FFLD = FRM-FFLD-PF5                                   
              MOVE PGM-SNUL TO PGM-STAT-FIA                             
              SUBTRACT 1 FROM SCR-I GIVING DFD-I                        
              IF DFD-I NOT > ZEROES                                     
                 MOVE 'DFD DEUPRV' TO MSG-COD-MENS                      
                 GO TO ERR-FND-KEY                                      
              ELSE                                                      
                 GO TO CARGA-DFD                                        
           ELSE                                                         
           IF FRM-FFLD = FRM-FFLD-PF6                                   
              MOVE PGM-SNUL TO PGM-STAT-FIA                             
              ADD 1 SCR-I GIVING DFD-I                                  
              IF DFD-I > SCR-IMAX                                       
                MOVE 'DFD DEUNXT' TO MSG-COD-MENS                       
                GO TO ERR-FND-KEY                                       
              ELSE                                                      
                GO TO CARGA-DFD.                                        
           GO TO CON-FND-KEY.                                           
      *>>>>                                                             
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY                                                 
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-FND-KEY.                                       
       CON-FND-KEY.                                                     
           MOVE ZEROES TO SCR-IMAX.                                     
           MOVE 1 TO SCR-I.                                             
           MOVE '00' TO FIO-STAT.                                       
           PERFORM OBT-DAT-TS.                                          
           IF MSG-COD-MENS > SPACES                                     
               GO TO ERR-FND-KEY.                                       
           MOVE 1 TO SCR-I.                                             
           MOVE SCR-I TO DFD-I.                                         
       CARGA-DFD.                                                       
           MOVE SPACES TO MSG-COD-MENS.                                 
           IF NOT SCR-OFRM-SRV                                          
             PERFORM PUT-TAB-DFD.                                       
             MOVE '00' TO FIO-STAT.                                     
             GO TO FIN-FND-KEY.                                         
       ERR-FND-KEY.                                                     
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           MOVE 'DEU'  TO MSG-COD-SIST.                                 
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MEN1.                               
           MOVE '00' TO FIO-STAT.                                       
           GO TO FIN-FND-KEY.                                           
      *>>>>                                                             
*% END
*% IF EOF
      *<<<<                                                             
       COPY GNSBBTAB.                                                   
      *>>>>                                                             
      *<<<<                                                             
       CAR-MOV-DFD SECTION.                                             
       INI-CAR-MOV-DFD.                                                 
           IF DBC-NUM-ICLI IN DFD-FLD > ZEROES                          
               MOVE DBC-NUM-ICLI IN DFD-FLD TO DBC-NUM-ICLI IN DBC      
               MOVE DBC-IND-ICLI IN DFD-FLD TO DBC-IND-ICLI IN DBC      
               MOVE DBC-GLS-ICLI IN DFD-FLD TO DBC-GLS-ICLI IN DBC      
               MOVE 'DBC-IDC-ICLI' TO FIO-AKEY                          
               MOVE FIO-GET-KEY TO FIO-CMND                             
               PERFORM SGC-FIO-DBC                                      
               IF NOT FIO-STAT-OKS                                      
                    MOVE 'FRMNOMCNOSGC' TO MSG-COD-MENS                 
                    GO TO FIN-CAR-MOV-DFD                               
               ELSE                                                     
                   MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN                 
                   PERFORM CPT-BLKS                                     
                   MOVE CPT-STRN TO DBC-GLS-NOMC IN DFD-FLD             
           ELSE                                                         
               IF DBC-GLS-NOMC IN DFD-FLD > SPACES                      
                   PERFORM BUS-NMB-CIC-DEU                              
                   IF MSG-COD-MENS > SPACES                             
                       GO TO FIN-CAR-MOV-DFD.                           
           MOVE DBC-CIC-ICLI IN DBC TO WSS-CIC-ICLI IN WSS-VARI.        
           MOVE DBC-CIC-ICLI IN DBC TO FIA-CIC-ICLI IN FIA.             
           IF FRM-NUM-AVAL IN DFD-FLD > ZEROES                          
               MOVE FRM-NUM-AVAL IN DFD-FLD TO DBC-NUM-ICLI IN DBC      
               MOVE FRM-IND-AVAL IN DFD-FLD TO DBC-IND-ICLI IN DBC      
               MOVE FRM-GLS-AVAL IN DFD-FLD TO DBC-GLS-ICLI IN DBC      
               MOVE 'DBC-IDC-ICLI' TO FIO-AKEY                          
               MOVE FIO-GET-KEY TO FIO-CMND                             
               PERFORM SGC-FIO-DBC                                      
               MOVE DBC-CIC-ICLI IN DBC TO FIA-CIC-ICFS IN FIA          
           ELSE                                                         
               MOVE LOW-VALUES          TO FIA-CIC-ICFS IN FIA.         
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-CAR-MOV-DFD.                                                 
           MOVE 'FIA-GLS-ICYF'  TO FIO-AKEY.                            
           PERFORM DEU-FIO-FIA.                                         
           IF NOT (FIO-STAT-OKS AND                                     
              WSS-CIC-ICLI IN WSS-VARI = FIA-CIC-ICLI IN FIA)           
               IF DFD-I = 1                                             
                    MOVE 'CICICLINODEU' TO MSG-COD-MENS                 
                    GO TO FIN-CAR-MOV-DFD                               
               ELSE                                                     
                   GO TO FIN-CAR-MOV-DFD.                               
           IF FIA-COD-TOFS IN DFD-FLD > SPACES                          
               IF FIA-COD-TOFS IN DFD-FLD  NOT = FIA-COD-TOFS IN FIA    
                   GO TO NXT-CAR-MOV-DFD.                               
           IF FIA-COD-AOFS IN DFD-FLD > SPACES                          
               IF FIA-COD-AOFS IN DFD-FLD  NOT = FIA-COD-AOFS IN FIA    
                   GO TO NXT-CAR-MOV-DFD.                               
           MOVE FIA-CIC-ICFS IN FIA TO DBC-CIC-ICLI IN DBC.             
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM SGC-FIO-DBC.                                         
           MOVE FIA-CAI-IESC IN FIA TO DFD-CAI-IFIA(DFD-I).             
           MOVE FIA-IIC-IESC IN FIA TO DFD-IIC-IFIA(DFD-I).             
           MOVE FIA-NUM-IFIA IN FIA TO DFD-NUM-IFIA(DFD-I).             
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.                        
           PERFORM CPT-BLKS.                                            
           MOVE CPT-STRN TO DBC-GLS-NOMC IN DBC.                        
           MOVE DBC-GLS-NOMC IN DBC TO DFD-GLS-NOMC(DFD-I).             
           MOVE DBC-NUM-ICLI IN DBC TO DFD-NUM-ICLI(DFD-I).             
           MOVE DBC-IND-ICLI IN DBC TO DFD-IND-ICLI(DFD-I).             
           MOVE DBC-VRF-ICLI IN DBC TO DFD-VRF-ICLI(DFD-I).             
           MOVE DBC-GLS-ICLI IN DBC TO DFD-GLS-ICLI(DFD-I).             
           MOVE FIA-MSC-STAT IN FIA TO DFD-COD-ESTA(DFD-I).             
           MOVE FIA-CIC-IESC IN FIA TO ESC-CIC-IESC IN ESC.             
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM DEU-FIO-ESC.                                         
           MOVE ESC-COD-OFEF IN ESC TO DFD-COD-OFIC(DFD-I).             
           PERFORM INC-REG-TS.                                          
           ADD 1 TO WSS-TOT.                                            
       NXT-CAR-MOV-DFD.                                                 
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-CAR-MOV-DFD.                                       
       FIN-CAR-MOV-DFD.                                                 
           IF SCR-OFRM-SRV                                              
              PERFORM PUT-DFD-MAS.                                      
           EXIT.                                                        
                                                                        
       PUT-DFD-MAS SECTION.                                             
       INI-PUT-DFD-MAS.                                                 
           MOVE 1                  TO QUE-NITM.                         
           MOVE DFD-LEN-TSDFD-MAS  TO QUE-LITM.                         
           MOVE DFD-NOM-TSDFD-MAS  TO QUE-COLA.                         
           IF WSS-TOT > DFD-IMAX-MTX                                    
              MOVE 'S'             TO DFD-MAS-IND-HMAS                  
           ELSE                                                         
              MOVE 'N'             TO DFD-MAS-IND-HMAS.                 
           MOVE WSS-TOT            TO DFD-MAS-NUM-TOCC.                 
           MOVE DFD-MAS-ITEM       TO QUE-ITEM.                         
           MOVE QUE-PUT            TO QUE-CMND.                         
           PERFORM GNS-PRO-QUE.                                         
       FIN-PUT-DFD-MAS.                                                 
           EXIT.                                                        
                                                                        
       OBT-PARAM SECTION.                                               
       INI-OBT-PARAM.                                                   
       FIN-OBT-PARAM.                                                   
           EXIT.                                                        
                                                                        
       OBT-DAT-TS SECTION.                                              
       INI-OBT-DAT-TS.                                                  
           EXEC CICS HANDLE CONDITION QIDERR(CON-OBT-DAT-TS)            
                                     ITEMERR(CON-OBT-DAT-TS)            
           END-EXEC.                                                    
           PERFORM DEL-QUE-DFD.                                         
       CON-OBT-DAT-TS.                                                  
           MOVE DFD-SI-IMP TO DFD-IMP.                                  
           PERFORM CLR-VEC-TS VARYING DFD-I FROM 1 BY 1 UNTIL           
                   DFD-I > DFD-IMAX.                                    
           MOVE 1 TO DFD-I.                                             
           PERFORM CAR-MOV-DFD.                                         
       NXT-OBT-DAT-TS.                                                  
           IF DFD-IMP = DFD-SI-IMP                                      
               PERFORM WRT-TS.                                          
       FIN-OBT-DAT-TS.                                                  
           EXIT.                                                        
                                                                        
       PUT-TAB-DFD SECTION.                                             
       INI-PUT-TAB-DFD.                                                 
           PERFORM RED-TS.                                              
           PERFORM LLE-DAT-PNT VARYING DFD-I FROM 1 BY 1                
                   UNTIL DFD-I > DFD-IMAX.                              
       FIN-PUT-TAB-DFD.                                                 
           EXIT.                                                        
                                                                        
       LLE-DAT-PNT SECTION.                                             
       INI-LLE-DAT-PNT.                                                 
           MOVE DFD-CAI-IFIA(DFD-I) TO FRM-CAI-IFIA IN DFD-FLD(DFD-I).  
           MOVE DFD-IIC-IFIA(DFD-I) TO FRM-IIC-IFIA IN DFD-FLD(DFD-I).  
           MOVE DFD-NUM-IFIA(DFD-I) TO FRM-NUM-IFIA IN DFD-FLD(DFD-I).  
           MOVE DFD-NUM-ICLI(DFD-I) TO FRM-NUM-ICLI IN DFD-FLD(DFD-I).  
           MOVE DFD-IND-ICLI(DFD-I) TO FRM-IND-ICLI IN DFD-FLD(DFD-I).  
           MOVE DFD-GLS-ICLI(DFD-I) TO FRM-GLS-ICLI IN DFD-FLD(DFD-I).  
           MOVE DFD-VRF-ICLI(DFD-I) TO FRM-VRF-ICLI IN DFD-FLD(DFD-I).  
           MOVE DFD-GLS-NOMC(DFD-I) TO FRM-GLS-NOMC IN DFD-FLD(DFD-I).  
           MOVE DFD-COD-OFIC(DFD-I) TO FRM-COD-OFIC IN DFD-FLD(DFD-I).  
           MOVE DFD-COD-ESTA(DFD-I) TO FRM-COD-ESTA IN DFD-FLD(DFD-I).  
       FIN-LLE-DAT-PNT.                                                 
           EXIT.                                                        
                                                                        
       WRT-TS SECTION.                                                  
       INI-WRT-TS.                                                      
           ADD 1 TO SCR-IMAX.                                           
           MOVE DFD-LEN-TSDFD     TO QUE-LITM.                          
           MOVE DFD-TSDFD         TO QUE-COLA.                          
           MOVE DFD-TS            TO QUE-ITEM.                          
           MOVE DFD-I             TO QUE-NITM.                          
           MOVE QUE-PUT           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
       FIN-WRT-TS.                                                      
           EXIT.                                                        
                                                                        
       DEL-QUE-DFD SECTION.                                             
       INI-DEL-QUE-DFD.                                                 
           MOVE DFD-NOM-TSDFD     TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE DFD-NOM-TSDFD-MAS TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
       FIN-DEL-QUE-DFD.                                                 
           EXIT.                                                        
                                                                        
       RED-TS SECTION.                                                  
       INI-RED-TS.                                                      
           MOVE DFD-I TO SCR-I.                                         
           EXEC CICS HANDLE CONDITION QIDERR(FIN-RED-TS)                
                                     ITEMERR(FIN-RED-TS)                
           END-EXEC.                                                    
           MOVE DFD-LEN-TSDFD     TO QUE-LITM.                          
           MOVE DFD-TSDFD         TO QUE-COLA.                          
           MOVE DFD-TS            TO QUE-ITEM.                          
           MOVE DFD-I             TO QUE-NITM.                          
           MOVE QUE-GET           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
           MOVE QUE-ITEM          TO DFD-TS.                            
       FIN-RED-TS.                                                      
           EXIT.                                                        
                                                                        
       CLR-VEC-TS SECTION.                                              
       INI-CLR-VEC-TS.                                                  
           MOVE SPACES TO  DFD-CAI-IFIA(DFD-I).                         
           MOVE SPACES TO  DFD-IIC-IFIA(DFD-I).                         
           MOVE ZEROES TO  DFD-NUM-IFIA(DFD-I).                         
           MOVE SPACES TO  DFD-NUM-ICLI(DFD-I).                         
           MOVE SPACES TO  DFD-IND-ICLI(DFD-I).                         
           MOVE SPACES TO  DFD-GLS-ICLI(DFD-I).                         
           MOVE SPACES TO  DFD-VRF-ICLI(DFD-I).                         
           MOVE SPACES TO  DFD-GLS-NOMC(DFD-I).                         
           MOVE SPACES TO  DFD-COD-OFIC(DFD-I).                         
           MOVE SPACES TO  DFD-COD-ESTA(DFD-I).                         
       FIN-CLR-VEC-TS.                                                  
           EXIT.                                                        
                                                                        
       INC-REG-TS SECTION.                                              
       INI-INC-REG-TS.                                                  
           MOVE DFD-SI-IMP TO DFD-IMP.                                  
           ADD 1 TO DFD-I.                                              
           IF DFD-I > DFD-IMAX                                          
               PERFORM WRT-TS                                           
               PERFORM CLR-VEC-TS VARYING DFD-I FROM 1 BY 1             
                                    UNTIL DFD-I > DFD-IMAX              
               MOVE 1 TO DFD-I                                          
               MOVE DFD-NO-IMP TO DFD-IMP.                              
       FIN-INC-REG-TS.                                                  
           EXIT.                                                        
                                                                        
       RST-STAT SECTION.                                                
       INI-RST-STAT.                                                    
           MOVE SCR-STPP     TO SCR-STAT.                               
           MOVE SCR-MFLD-NOT TO SCR-MFLD.                               
       FIN-RST-STAT.                                                    
           EXIT.                                                        
                                                                        
       COPY SGCBFDBC.                                                   
       COPY GNSBGCPT.                                                   
       COPY DEUBFESC.                                                   
                                                                        
       BUS-NMB-CIC-DEU SECTION.                                         
       INI-BUS-NMB-CIC-DEU.                                             
           MOVE SPACES TO DBC-GLS-NOMC IN DBC.                          
           MOVE DBC-GLS-NOMC IN DFD-FLD TO STR-GLS-GSTR                 
           PERFORM STR-UNS                                              
           MOVE STR-GLS-FUNS IN STR-VARI(1) TO DBC-GLS-APAT IN DBC      
           MOVE STR-GLS-FUNS IN STR-VARI(2) TO DBC-GLS-AMAT IN DBC      
           MOVE STR-GLS-FUNS IN STR-VARI(3) TO DBC-GLS-NOMB IN DBC.     
           MOVE 'DBC-GLS-NOMC' TO FIO-AKEY.                             
           MOVE FIO-GET-NLS    TO FIO-CMND.                             
           PERFORM SGC-FIO-DBC.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE 'FRMNOMCNOSGC' TO MSG-COD-MENS                      
           ELSE                                                         
               MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN                     
               PERFORM CPT-BLKS                                         
               MOVE CPT-STRN            TO DBC-GLS-NOMC IN DFD-FLD      
               MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN DFD-FLD      
               MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN DFD-FLD      
               MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN DFD-FLD      
               MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN DFD-FLD.     
       FIN-BUS-NMB-CIC-DEU.                                             
           EXIT.                                                        
                                                                        
       COPY GNSBGSTR.                                                   
      *>>>>                                                             
*% END
