*%! Codigo adicional para archivo
*% PGM_PTC = TRUE
*% GNSTCA  = 270
*% IF WSS
      *<<<< WSS                                                         
       COPY DEUWGCPN.                                                   
       COPY GNSWGCPT.                                                   
       COPY GNSWGPES.                                                   
                                                                        
       01  WSS-VARI.                                                    
           03  WSS-I                              PIC 9(02).            
           03  WSS-FEC-CNT                        PIC X(08).            
           03  WSS-COD-PESO                       PIC X(06).            
      *>>>>                                                             
*% END
*% IF MI_COMMAREA
      *<<<< MI_COMMAREA                                                 
           03 CPN-VARI.                                                 
              05 SCR-IMAX      COMP                   PIC S9(04).       
              05 SCR-I         COMP                   PIC S9(04).       
      *>>>>                                                             
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON OR PFS_GET_ACC                                  
           IF NOT (FRM-FFLD = FRM-FFLD-PF6                              
                         OR   FRM-FFLD-PF5)                             
      *>>>>                                                             
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC                                                 
           IF FRM-FFLD = FRM-FFLD-PF6                                   
                    OR   FRM-FFLD-PF5                                   
               PERFORM VAL-NUM-KEY-CPN                                  
               PERFORM VAL-NUM-FLD-CPN                                  
               PERFORM RST-STAT                                         
               PERFORM FND-SCR-KEY THRU FIN-SCR-KEY                     
               IF SCR-STAT NOT = SCR-STAT-ABT                           
                   PERFORM SCR-TRL                                      
               ELSE                                                     
                   PERFORM DESSCRTRL                                    
                   GO TO FIN-SCR-ACC.                                   
      *>>>>                                                             
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY                                                 
           MOVE SPACES TO FRM-MEN1.                                     
           IF FRM-FFLD = FRM-FFLD-PF5                                   
               MOVE PGM-SNUL TO PGM-STAT-PNE                            
               SUBTRACT 1 FROM SCR-I GIVING CPN-I                       
               IF CPN-I NOT > ZEROES                                    
                   MOVE 'PNE PRV' TO MSG-COD-MENS                       
                   GO TO ERR-FND-KEY                                    
               ELSE                                                     
                   GO TO CARGA-CPN                                      
           ELSE                                                         
           IF FRM-FFLD = FRM-FFLD-PF6                                   
               MOVE PGM-SNUL TO PGM-STAT-PNE                            
               ADD 1 SCR-I GIVING CPN-I                                 
               IF CPN-I > SCR-IMAX                                      
                   MOVE 'PNE NXT' TO MSG-COD-MENS                       
                   GO TO ERR-FND-KEY                                    
               ELSE                                                     
                   GO TO CARGA-CPN.                                     
           GO TO CON-FND-KEY.                                           
      *>>>>                                                             
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY                                                 
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-FND-KEY.                                       
       CON-FND-KEY.                                                     
                                                                        
           MOVE ZEROES TO SCR-IMAX.                                     
           MOVE 1 TO SCR-I.                                             
           MOVE '00' TO FIO-STAT.                                       
           PERFORM OBT-DAT-TS.                                          
           MOVE 1 TO SCR-I.                                             
           MOVE SCR-I TO CPN-I.                                         
       CARGA-CPN.                                                       
           MOVE SPACES TO MSG-COD-MENS.                                 
           IF NOT SCR-OFRM-SRV                                          
           PERFORM PUT-TAB-CPN.                                         
           MOVE '00' TO FIO-STAT.                                       
           GO TO FIN-FND-KEY.                                           
      *                                                                 
       ERR-FND-KEY.                                                     
           MOVE SCR-STAT-CON TO SCR-STAT.                               
                                                                        
           MOVE 'DEU'  TO MSG-COD-SIST.                                 
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MEN1.                               
           MOVE '00' TO FIO-STAT.                                       
           GO TO FIN-FND-KEY.                                           
      *>>>>                                                             
*% END
*% IF EOF
       COPY GNSBEGLS.                                                   
      *<<<< EOF                                                         
       COPY GNSBGCPT.                                                   
       COPY GNSBBTAB.                                                   
       COPY GNSBGPES.                                                   
                                                                        
       CAR-MOR-PNE SECTION.                                             
       INI-CAR-MOR-PNE.                                                 
           MOVE PNE-CAI-ICAR IN CPN-FLD TO PNE-CAI-ICAR IN PNE.         
           MOVE PNE-IIC-ICAR IN CPN-FLD TO PNE-IIC-ICAR IN PNE.         
           MOVE ZEROES                  TO PNE-NUM-SPNE IN PNE.         
           MOVE ZEROES                  TO PNE-NUM-APNE IN PNE.         
           MOVE ZEROES                  TO PNE-NUM-MPNE IN PNE.         
           MOVE ZEROES                  TO PNE-NUM-DPNE IN PNE.         
           MOVE ZEROES                  TO PNE-HRA-HRPN IN PNE.         
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-CAR-MOR-PNE.                                                 
           PERFORM DEU-FIO-PNE.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
               PNE-CAI-ICAR IN PNE = PNE-CAI-ICAR IN CPN-FLD AND        
               PNE-IIC-ICAR IN PNE = PNE-IIC-ICAR IN CPN-FLD )          
               GO TO FIN-CAR-MOR-PNE.                                   
                                                                        
       VAL-CAR-MOR-PNE.                                                 
           MOVE PNE-FEC-FPNE IN PNE TO CPN-FEC-FPNE(CPN-I).             
           MOVE PNE-HRA-HRPN IN PNE TO CPN-HRA-HRPN(CPN-I).             
           MOVE PNE-COD-TLGD IN PNE TO CPN-COD-TLGD(CPN-I).             
           MOVE PNE-COD-TLGM IN PNE TO CPN-COD-TLGM(CPN-I).             
           MOVE PNE-COD-TLGC IN PNE TO CPN-COD-TLGC(CPN-I).             
           MOVE PNE-SGV-PDEF IN PNE TO CPN-SGV-PDEF(CPN-I).             
           MOVE PNE-SGV-PVPE IN PNE TO CPN-SGV-PVPE(CPN-I).             
           MOVE PNE-SGV-CONF IN PNE TO CPN-SGV-CONF(CPN-I).             
           MOVE PNE-SGV-CAPI IN PNE TO CPN-SGV-CAPI(CPN-I).             
           MOVE PNE-SGV-PPK1 IN PNE TO CPN-SGV-PPK1(CPN-I).             
           MOVE PNE-SGV-PPK2 IN PNE TO CPN-SGV-PPK2(CPN-I).             
       NXT-CAR-MOR-PNE.                                                 
           PERFORM INC-REG-TS.                                          
       SIG-CAR-MOR-PNE.                                                 
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-CAR-MOR-PNE.                                       
                                                                        
       FIN-CAR-MOR-PNE.                                                 
           EXIT.                                                        
                                                                        
       OBT-PARAM SECTION.                                               
       INI-OBT-PARAM.                                                   
       FIN-OBT-PARAM.                                                   
           EXIT.                                                        
                                                                        
       OBT-DAT-TS SECTION.                                              
       INI-OBT-DAT-TS.                                                  
           EXEC CICS HANDLE CONDITION QIDERR(CON-OBT-DAT-TS)            
                                     ITEMERR(CON-OBT-DAT-TS)            
           END-EXEC.                                                    
                                                                        
      *     EXEC CICS DELETEQ TS QUEUE(CPN-NAME-QUE)                    
      *     END-EXEC.                                                   
      *<<<<                                                             
           MOVE EIBTRMID      TO CPN-TERM.                              
           MOVE CPN-NAME-QUE   TO QUE-COLA.                             
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
      *>>>>                                                             
       CON-OBT-DAT-TS.                                                  
           MOVE CPN-SI-IMP TO CPN-IMP.                                  
           PERFORM CLR-VEC-TS VARYING CPN-I FROM 1 BY 1 UNTIL           
                   CPN-I > CPN-IMAX.                                    
           MOVE 1 TO CPN-I.                                             
           PERFORM CAR-MOR-PNE.                                         
                                                                        
       NXT-OBT-DAT-TS.                                                  
           IF CPN-IMP = CPN-SI-IMP                                      
               PERFORM WRT-TS.                                          
       FIN-OBT-DAT-TS.                                                  
           EXIT.                                                        
                                                                        
       PUT-TAB-CPN SECTION.                                             
       INI-PUT-TAB-CPN.                                                 
           PERFORM RED-TS.                                              
           PERFORM LLE-DAT-PNT VARYING CPN-I FROM 1 BY 1                
                   UNTIL CPN-I > CPN-IMAX.                              
       FIN-PUT-TAB-CPN.                                                 
           EXIT.                                                        
                                                                        
       LLE-DAT-PNT SECTION.                                             
       INI-LLE-DAT-PNT.                                                 
           MOVE CPN-FEC-FPNE(CPN-I) TO FRM-FEC-FPNE IN CPN-FLD(CPN-I).  
           MOVE CPN-HRA-HRPN(CPN-I) TO FRM-HRA-HRPN IN CPN-FLD(CPN-I).  
           MOVE CPN-COD-TLGD(CPN-I) TO FRM-COD-TLGD IN CPN-FLD(CPN-I).  
           MOVE CPN-COD-TLGM(CPN-I) TO FRM-COD-TLGM IN CPN-FLD(CPN-I).  
           MOVE CPN-COD-TLGC(CPN-I) TO FRM-COD-TLGC IN CPN-FLD(CPN-I).  
           MOVE CPN-SGV-PDEF(CPN-I) TO FRM-SGV-PDEF IN CPN-FLD(CPN-I).  
           MOVE CPN-SGV-PVPE(CPN-I) TO FRM-SGV-PVPE IN CPN-FLD(CPN-I).  
           MOVE CPN-SGV-CONF(CPN-I) TO FRM-SGV-CONF IN CPN-FLD(CPN-I).  
           MOVE CPN-SGV-CAPI(CPN-I) TO FRM-SGV-CAPI IN CPN-FLD(CPN-I).  
           MOVE CPN-SGV-PPK1(CPN-I) TO FRM-SGV-PPK1 IN CPN-FLD(CPN-I).  
           MOVE CPN-SGV-PPK2(CPN-I) TO FRM-SGV-PPK2 IN CPN-FLD(CPN-I).  
       FIN-LLE-DAT-PNT.                                                 
           EXIT.                                                        
                                                                        
       INC-REG-TS SECTION.                                              
       INI-INC-REG-TS.                                                  
            MOVE CPN-SI-IMP TO CPN-IMP.                                 
      *     IF SCR-OFRM-SRV                                             
      *       MOVE CPN-VAL-CPTO(CPN-I) TO CPN-VAL-CPTO-EDT              
      *       MOVE CPN-VAL-CPTO-EDT TO CPN-VAL-CPTO-ALF(CPN-I).         
           ADD 1 TO CPN-I.                                              
           IF CPN-I > CPN-IMAX                                          
               PERFORM WRT-TS                                           
               PERFORM CLR-VEC-TS VARYING CPN-I FROM 1 BY 1             
                                     UNTIL CPN-I > CPN-IMAX             
               MOVE 1 TO CPN-I                                          
               MOVE CPN-NO-IMP TO CPN-IMP.                              
       FIN-INC-REG-TS.                                                  
           EXIT.                                                        
                                                                        
      *<<<<                                                             
      *>>>>                                                             
                                                                        
       WRT-TS SECTION.                                                  
       INI-WRT-TS.                                                      
           ADD 1 TO SCR-IMAX.                                           
      *     EXEC CICS WRITEQ TS QUEUE(CPN-NAME-QUE)                     
      *                         FROM(CPN-TS)                            
      *                         LENGTH(CPN-LONG-TS)                     
      *     END-EXEC.                                                   
      *<<<<                                                             
           MOVE EIBTRMID      TO CPN-TERM.                              
           MOVE CPN-NAME-QUE   TO QUE-COLA.                             
           MOVE CPN-TS         TO QUE-ITEM.                             
           MOVE CPN-LONG-TS    TO QUE-LITM.                             
           MOVE QUE-PUT        TO QUE-CMND.                             
           PERFORM GNS-PRO-QUE.                                         
      *>>>>                                                             
       FIN-WRT-TS.                                                      
           EXIT.                                                        
                                                                        
       RED-TS SECTION.                                                  
       INI-RED-TS.                                                      
           MOVE CPN-I TO SCR-I.                                         
           EXEC CICS HANDLE CONDITION QIDERR(FIN-RED-TS)                
                                     ITEMERR(FIN-RED-TS)                
           END-EXEC.                                                    
      *     EXEC CICS READQ TS QUEUE(CPN-NAME-QUE)                      
      *                         INTO(CPN-TS)                            
      *                          LENGTH(CPN-LONG-TS)                    
      *                         ITEM(CPN-I)                             
      *     END-EXEC.                                                   
      *<<<<                                                             
           MOVE EIBTRMID      TO CPN-TERM.                              
           MOVE CPN-NAME-QUE   TO QUE-COLA.                             
           MOVE CPN-TS         TO QUE-ITEM.                             
           MOVE CPN-LONG-TS    TO QUE-LITM.                             
           MOVE CPN-I              TO QUE-NITM.                         
           MOVE QUE-GET            TO QUE-CMND.                         
           PERFORM GNS-PRO-QUE.                                         
           MOVE QUE-ITEM       TO CPN-TS.                               
      *>>>>                                                             
       FIN-RED-TS.                                                      
           EXIT.                                                        
                                                                        
       CLR-VEC-TS SECTION.                                              
       INI-CLR-VEC-TS.                                                  
           MOVE ZEROES TO CPN-FEC-FPNE(CPN-I).                          
           MOVE ZEROES TO CPN-HRA-HRPN(CPN-I).                          
           MOVE SPACES TO CPN-COD-TLGD(CPN-I).                          
           MOVE SPACES TO CPN-COD-TLGM(CPN-I).                          
           MOVE SPACES TO CPN-COD-TLGC(CPN-I).                          
           MOVE ZEROES TO CPN-SGV-PDEF(CPN-I).                          
           MOVE ZEROES TO CPN-SGV-PVPE(CPN-I).                          
           MOVE ZEROES TO CPN-SGV-CONF(CPN-I).                          
           MOVE ZEROES TO CPN-SGV-CAPI(CPN-I).                          
           MOVE ZEROES TO CPN-SGV-PPK1(CPN-I).                          
           MOVE ZEROES TO CPN-SGV-PPK2(CPN-I).                          
       FIN-CLR-VEC-TS.                                                  
           EXIT.                                                        
                                                                        
       RST-STAT SECTION.                                                
       INI-RST-STAT.                                                    
           MOVE SCR-STPP     TO SCR-STAT.                               
           MOVE SCR-MFLD-NOT TO SCR-MFLD.                               
       FIN-RST-STAT.                                                    
           EXIT.                                                        
                                                                        
      *>>>>                                                             
*% END
