*%! Codigo adicional para progarma
*% HDR_CST = FALSE
*% INC_FIO = TRUE
*%! PGM_PTC = TRUE
*% NO_TIENE_TIMESTAMP = TRUE
*%! IF PGM_CHI AND PGM_BCI
*%!     PGM_PTC = TRUE
*%! END
*% GNSTCA = 3450
*% IF FIN_SCR_VARI
      *<<<< FIN_SCR_VARI                                                
       COPY GNSWGRQA REPLACING ==01  ADR-REQA== BY ==02  SCR-RQA1==.    
           02  SCR-R-ASC                                PIC 9(06).      
           02  SCR-R-DSC                                PIC 9(06).      
           02  SCR-R-RMAX                               PIC 9(06).      
           02  SCR-CIC-ICLI                             PIC X(12).      
           02  SCR-TAB-IRDE.                                            
               03  SCR-VEC-IRDE OCCURS 100.                             
                   05  SCR-KEY-IRDE.                                    
                       07  SCR-IND-TIPD                 PIC X(03).      
                       07  SCR-IND-SCRT                 PIC X(03).      
                       07  SCR-IND-SCTB                 PIC X(03).      
                       07  SCR-IND-SCBZ                 PIC X(05).      
                       07  SCR-COD-COCR                 PIC X(06).      
                       07  SCR-COD-VCCR                 PIC X(06).      
                       07  SCR-COD-OFCR                 PIC X(03).      
                   05  SCR-VAL-DEUD     COMP-3          PIC 9(11)V9(04).
      *>>>>                                                             
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC                                                     
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CAM-REQA==.        
      *ALD                                                              
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-REC-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-ESC-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-FIA-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-AMF-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OPC-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RDC-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DLC-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RDT-REQA==.        
      *COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CAN-REQA==.        
      *                                                                 
      *>>>>                                                             
*% END
*% IF WSS_IBM
      *<<<< WSS_IBM                                                     
      *ALD                                                              
       COPY SGCBRREC.                                                   
       COPY DEUBRESC.                                                   
       COPY DEUBRFIA.                                                   
       COPY DEUBRAMF.                                                   
       COPY COLBROPC.                                                   
       COPY COLBRRDC.                                                   
       COPY COLBRDLC.                                                   
       COPY COLBRRDT.                                                   
      *COPY COLBRCAN.                                                   
      *                                                                 
       COPY SGCBRDBC.                                                   
       COPY TABBRCAM.                                                   
       COPY GNSWGHOY.                                                   
       COPY GNSWGFEC.                                                   
       COPY DEUWGDEU.                                                   
      *MHM-INI 24-DIC-2002                                              
       COPY DEUWGMRG.                                                   
       COPY COLBRDVG.                                                   
       COPY COLBRDVO.                                                   
       COPY BGLBRGES.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GES-REQA==.        
       COPY BGLBRGDG.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GDG-REQA==.        
       COPY BGLBRGDD.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GDD-REQA==.        
       COPY BGLBRGVT.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GVT-REQA==.        
      *MHM-FIN 24-DIC-2002                                              
       COPY GNSWGCPT.                                                   
       COPY GNSWGSTR.                                                   
       COPY GNSWGCVT.                                                   
       01   WSS-VARI.                                                   
            03  WSS-VAL-TPES                             PIC 9(11).     
            03  WSS-IND-VCCR                VALUE SPACES PIC X(02).     
                 88  MONEDA-NO-VIGENTE      VALUE 'N '.                 
                 88  MONEDA-NO-EXISTENTE    VALUE ' X'.                 
                 88  MONEDA-NEX-NVG         VALUE 'NX'.                 
            03  WSS-IND-VCCR-RED REDEFINES WSS-IND-VCCR.                
                 05  WSS-IND-NVIG                        PIC X(01).     
                 05  WSS-IND-NEXI                        PIC X(01).     
            03  WSS-I                                    PIC 9(03).     
            03  WSS-IMAX                    VALUE 10     PIC 9(02).     
      *>>>>                                                             
*% END
*% IF INI_FIO
      *<<<< INI_FIO                                                     
           MOVE +3450 TO SCR-TCMA.                                      
           IF SCR-DESDE-CMN                                             
               MOVE SPACES TO SCR-RQA1.                                 
      *>>>>                                                             
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC                                                 
           IF FRM-FFLD = FRM-FFLD-PF8 OR FRM-FFLD-PF7                   
               PERFORM VAL-NUM-KEY-DEU                                  
               PERFORM VAL-NUM-FLD-DEU                                  
               PERFORM RST-STAT                                         
               PERFORM FND-SCR-KEY THRU FIN-SCR-KEY                     
               IF SCR-STAT NOT = SCR-STAT-ABT                           
                   PERFORM SCR-TRL                                      
               ELSE                                                     
                   MOVE SPACES TO SCR-RQA1                              
                   PERFORM DESSCRTRL                                    
                   GO TO FIN-SCR-ACC.                                   
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6                   
               PERFORM VAL-NUM-KEY-DEU                                  
               PERFORM VAL-NUM-FLD-DEU                                  
               PERFORM SCRL-TAB-DEU                                     
               PERFORM SCR-TRL                                          
           ELSE                                                         
               MOVE SPACES TO SCR-RQA1.                                 
      *>>>>                                                             
*% END
*% IF PFS_SCR_GKEY
      *<<<< PFS_GET_CON OR PFS_SCR_GKEY                                 
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 OR FRM-FFLD = FRM-FFLD-PF7 )
      *>>>>                                                             
      *<<<< PFS_GET_CON OR PFS_SCR_GKEY                                 
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 OR FRM-FFLD = FRM-FFLD-PF7 )
      *>>>>                                                             
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON OR PFS_SCR_GKEY                                 
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 OR FRM-FFLD = FRM-FFLD-PF7 )
      *>>>>                                                             
      *<<<< PFS_GET_CON OR PFS_SCR_GKEY                                 
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 OR FRM-FFLD = FRM-FFLD-PF7 )
      *>>>>                                                             
      *<<<< PFS_GET_CON                                                 
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )           
      *>>>>                                                             
*% END
*% IF FIO
      *COPY DEUBFRDE.                                                   
       DEU-FIO-RDE SECTION.                                                 
       INI-DEU-FIO-RDE.                                                 
           PERFORM GET-RDE.                                             
       FIN-DEU-FIO-RDE.                                                 
           EXIT.                                                        
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY                                                 
           PERFORM KEY-ALL-DEU.                                         
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8                   
               PERFORM LEE-NXT-PRV                                      
               IF FIO-STAT-OKS                                          
                   PERFORM SCR-CLRA-DEU                                 
                   MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN                 
                   PERFORM CPT-BLKS                                     
                   MOVE CPT-STRN TO DBC-GLS-NOMC IN DEU-FLD             
                   MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN DEU-FLD  
                   MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN DEU-FLD  
                   MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN DEU-FLD  
                   MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN DEU-FLD  
                   MOVE DBC-CAI-ICLI IN DBC TO RDE-CAI-ICLI IN DEU-FLD  
                   MOVE DBC-IIC-ICLI IN DBC TO RDE-IIC-ICLI IN DEU-FLD  
                   GO TO CON-FND-KEY                                    
               ELSE                                                     
                   MOVE SPACES TO PGM-STAT                              
                   IF FRM-FFLD = FRM-FFLD-PF7                           
                       MOVE 'DEU' TO MSG-COD-SIST                   
                       MOVE 'DEU    NOPRV' TO MSG-COD-MENS              
                       PERFORM GET-MSG                                  
                       MOVE MSG-GLS-DESC TO FRM-MENS                    
                       MOVE '00' TO FIO-STAT                            
                       MOVE SCR-STAT-CON TO SCR-STAT                    
                       GO TO FIN-FND-KEY                                
                   ELSE                                                 
                       MOVE 'DEU' TO MSG-COD-SIST                       
                       MOVE 'DEU    NONXT' TO MSG-COD-MENS              
                       PERFORM GET-MSG                                  
                       MOVE MSG-GLS-DESC TO FRM-MENS                    
                       MOVE '00' TO FIO-STAT                            
                       MOVE SCR-STAT-CON TO SCR-STAT                    
                       GO TO FIN-FND-KEY                                
           ELSE                                                         
           IF DBC-NUM-ICLI IN DEU-FLD > ZEROES                          
      *IDC-CLI Busca Cliente por medio de IdC                           
               MOVE DBC-NUM-ICLI IN DEU-FLD TO DBC-NUM-ICLI IN DBC      
               MOVE DBC-IND-ICLI IN DEU-FLD TO DBC-IND-ICLI IN DBC      
               MOVE DBC-GLS-ICLI IN DEU-FLD TO DBC-GLS-ICLI IN DBC      
               MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY                         
      *JSS     MOVE FIO-GET-KEY-ALT TO FIO-CMND                         
               MOVE FIO-GET-KEY TO FIO-CMND                             
               PERFORM GET-CLI                                          
               MOVE FIO-STAT TO PGM-STAT-DBC                            
               IF MSG-COD-MENS > SPACES                                 
                   MOVE MSG-GLS-DESC TO FRM-MENS                        
                   MOVE FIO-STAT-NEX TO FIO-STAT                        
                   GO TO FIN-FND-KEY                                    
               ELSE                                                     
                   MOVE ADR-DBC-REQA TO SCR-RQA1                        
                   MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN DEU-FLD  
                   MOVE DBC-CAI-ICLI IN DBC TO RDE-CAI-ICLI IN DEU-FLD  
                   MOVE DBC-IIC-ICLI IN DBC TO RDE-IIC-ICLI IN DEU-FLD  
           ELSE                                                         
      *NOM-CLI Busca Cliente por medio del Nombre                       
      *        UNSTRING DBC-GLS-NOMC IN DEU-FLD DELIMITED BY '/' INTO   
      *                                 DBC-GLS-APAT IN DBC             
      *                                 DBC-GLS-AMAT IN DBC             
      *                                 DBC-GLS-NOMB IN DBC             
               MOVE DBC-GLS-NOMC IN DEU-FLD TO STR-GLS-GSTR             
               PERFORM STR-UNS                                          
               MOVE STR-GLS-FUNS IN STR-VARI(1) TO DBC-GLS-APAT IN DBC  
               MOVE STR-GLS-FUNS IN STR-VARI(2) TO DBC-GLS-AMAT IN DBC  
               MOVE STR-GLS-FUNS IN STR-VARI(3) TO DBC-GLS-NOMB IN DBC  
               MOVE 'DBC-GLS-NOMC'  TO FIO-AKEY                         
      *JSS     MOVE FIO-GET-NLS-ALT TO FIO-CMND                         
               MOVE FIO-GET-NLS TO FIO-CMND                             
               PERFORM GET-CLI                                          
               MOVE FIO-STAT TO PGM-STAT-DBC                            
               IF MSG-COD-MENS > SPACES                                 
                   MOVE MSG-GLS-DESC TO FRM-MENS                        
                   MOVE FIO-STAT-NEX TO FIO-STAT                        
                   GO TO FIN-FND-KEY                                    
               ELSE                                                     
                   MOVE ADR-DBC-REQA TO SCR-RQA1                        
                   MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN DEU-FLD  
                   MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN DEU-FLD  
                   MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN DEU-FLD  
                   MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN DEU-FLD  
                   MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN DEU-FLD  
                   MOVE DBC-CAI-ICLI IN DBC TO RDE-CAI-ICLI IN DEU-FLD  
                   MOVE DBC-IIC-ICLI IN DBC TO RDE-IIC-ICLI IN DEU-FLD. 
       CON-FND-KEY.                                                     
           PERFORM KEY-RDE-DEU.                                         
      * Mover complemento de la llave                                   
      *ALD                                                              
           MOVE ZEROES TO SCR-R-RMAX.                                   
           PERFORM CLR-SCR-RDE VARYING WSS-I FROM 1 BY 1                
                           UNTIL WSS-I > 100.                           
      * Rutina PGV                                                      
           PERFORM GET-FHOY.                                            
           PERFORM INI-CAM.                                             
           MOVE HOY-FHOY TO DEU-FEC-FPRO.                               
           MOVE 'R' TO DEU-FPRO IN DEU.                                 
           MOVE DBC-CIC-ICLI IN DBC TO DEU-CIC-ICLI IN DEU.             
           PERFORM CAL-DEU.                                             
           IF DEU-STAT NOT = DEU-STAT-OKS                               
               MOVE 'DEU' TO MSG-COD-SIST                               
               MOVE DEU-STAT TO MSG-COD-MENS                            
               PERFORM GET-MSG                                          
               MOVE '00' TO FIO-STAT                                    
               MOVE SCR-STAT-CON TO SCR-STAT                            
               IF DEU-MEN2 > SPACES                                     
                   MOVE MSG-GLS-DESC TO FRM-MEN1                        
                   MOVE DEU-MEN2     TO FRM-MEN2                        
                   MOVE ADR-DBC-REQA TO SCR-RQA1                        
                   GO TO FIN-FND-KEY                                    
               ELSE                                                     
                   MOVE MSG-GLS-DESC TO FRM-MENS                        
                   MOVE ADR-DBC-REQA TO SCR-RQA1                        
                   GO TO FIN-FND-KEY.                                   
      *                                                                 
      *ALD                                                              
           PERFORM PUT-TOT-DEU.                                         
           PERFORM PUT-TAB.                                             
           IF NOT FIO-STAT-OKS                                          
               MOVE 'DEU' TO MSG-COD-SIST                               
               MOVE 'RDE    NEX' TO MSG-COD-MENS                        
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN2                            
               MOVE '00' TO FIO-STAT                                    
           ELSE                                                         
               IF MONEDA-NO-VIGENTE                                     
                   MOVE 'DEU' TO MSG-COD-SIST                           
                   MOVE 'VCR    N' TO MSG-COD-MENS                      
                   PERFORM GET-MSG                                      
                   MOVE MSG-GLS-DESC TO FRM-MEN2                        
                   MOVE '00' TO FIO-STAT                                
               ELSE                                                     
                   IF MONEDA-NO-EXISTENTE                               
                       MOVE 'DEU' TO MSG-COD-SIST                       
                       MOVE 'VCR    X' TO MSG-COD-MENS                  
                       PERFORM GET-MSG                                  
                       MOVE MSG-GLS-DESC TO FRM-MEN2                    
                       MOVE '00' TO FIO-STAT                            
                   ELSE                                                 
                       IF MONEDA-NEX-NVG                                
                           MOVE 'DEU' TO MSG-COD-SIST                   
                           MOVE 'VCR    NX' TO MSG-COD-MENS             
                           PERFORM GET-MSG                              
                           MOVE MSG-GLS-DESC TO FRM-MEN2                
                           MOVE '00' TO FIO-STAT                        
                       ELSE                                             
                           IF SCR-R-ASC < DEU-RMAX                      
                               MOVE 'DEU' TO MSG-COD-SIST               
                               MOVE 'RDEDEUDMAS' TO MSG-COD-MENS        
                               PERFORM GET-MSG                          
                               MOVE MSG-GLS-DESC TO FRM-MEN2            
                               MOVE '00' TO FIO-STAT.                   
           MOVE DBC-CIC-ICLI IN DBC TO SCR-CIC-ICLI.                    
           MOVE DEU-RMAX            TO SCR-R-RMAX.                      
           MOVE RDE-TAB-IRDE        TO SCR-TAB-IRDE.                    
           MOVE ADR-DBC-REQA        TO SCR-RQA1.                        
           GO TO FIN-FND-KEY.                                           
      *>>>>                                                             
*% END
*% IF EOF
      *<<<< EOF                                                         
       RST-STAT SECTION.                                                
       INI-RST-STAT.                                                    
           MOVE SCR-STPP     TO SCR-STAT.                               
           MOVE SCR-MFLD-NOT TO SCR-MFLD.                               
       FIN-RST-STAT.                                                    
           EXIT.                                                        
       LEE-NXT-PRV SECTION.                                             
       INI-LEE-NXT-PRV.                                                 
           IF SCR-RQA1 > SPACES                                         
               PERFORM LEE-NXT-PRV-DBC                                  
           ELSE                                                         
               PERFORM LEE-NXT-PRV-NOM.                                 
       FIN-LEE-NXT-PRV.                                                 
           EXIT.                                                        
       LEE-NXT-PRV-DBC SECTION.                                         
       INI-LEE-NXT-PRV-DBC.                                             
           MOVE SCR-RQA1 TO ADR-DBC-REQA.                               
           MOVE FIO-KDEF-S TO FIO-KDEF.                                 
           IF FRM-FFLD = FRM-FFLD-PF7                                   
               MOVE FIO-GET-PRV TO FIO-CMND                             
           ELSE                                                         
               MOVE FIO-GET-NXT TO FIO-CMND.                            
           MOVE 'DBC-GLS-NOMC' TO FIO-AKEY.                             
           PERFORM SGC-FIO-DBC.                                         
           MOVE FIO-STAT TO PGM-STAT-DBC.                               
           IF FIO-STAT-OKS                                              
               MOVE ADR-DBC-REQA TO SCR-RQA1.                           
       FIN-LEE-NXT-PRV-DBC.                                             
           EXIT.                                                        
       LEE-NXT-PRV-NOM SECTION.                                         
       INI-LEE-NXT-PRV-NOM.                                             
           IF DBC-GLS-NOMC IN DBC > SPACES                              
               MOVE FIO-GET-KEY    TO FIO-CMND                          
               MOVE 'DBC-GLS-NOMC' TO FIO-AKEY                          
               PERFORM SGC-FIO-DBC                                      
               IF FIO-STAT-OKS                                          
                   IF FRM-FFLD = FRM-FFLD-PF7                           
                       MOVE FIO-GET-PRV TO FIO-CMND                     
                   ELSE                                                 
                       MOVE FIO-GET-NXT TO FIO-CMND                     
               ELSE                                                     
                   IF FRM-FFLD = FRM-FFLD-PF7                           
                       MOVE FIO-GET-LEQ TO FIO-CMND                     
                   ELSE                                                 
                       MOVE FIO-GET-NLS TO FIO-CMND                     
           ELSE                                                         
               IF FRM-FFLD = FRM-FFLD-PF7                               
                   MOVE FIO-GET-LEQ TO FIO-CMND                         
               ELSE                                                     
                   MOVE FIO-GET-NLS TO FIO-CMND.                        
           MOVE 'DBC-GLS-NOMC' TO FIO-AKEY.                             
           PERFORM SGC-FIO-DBC.                                         
           MOVE FIO-STAT TO PGM-STAT-DBC.                               
           IF FIO-STAT-OKS                                              
               MOVE ADR-DBC-REQA TO SCR-RQA1.                           
       FIN-LEE-NXT-PRV-NOM.                                             
           EXIT.                                                        
       PUT-TAB SECTION.                                                 
       INI-PUT-TAB.                                                     
           MOVE ZEROES TO SCR-R-ASC.                                    
           MOVE ZEROES TO SCR-R-DSC.                                    
           MOVE 0 TO FRM-IFLD                                           
                     DEU-R.                                             
           MOVE FIO-GET-NLS TO FIO-CMND.                                
           PERFORM DEU-FIO-RDE.                                         
           IF NOT FIO-STAT-OKS OR                                       
              RDE-CAI-ICLI IN DEU-FLD NOT = RDE-CAI-ICLI IN RDE OR      
              RDE-IIC-ICLI IN DEU-FLD NOT = RDE-IIC-ICLI IN RDE         
               MOVE FIO-STAT-NEX TO FIO-STAT                            
               GO TO FIN-PUT-TAB.                                       
           MOVE DEU-R TO SCR-R-DSC.                                     
       LUP-PUT-TAB.                                                     
           MOVE DEU-R TO SCR-R-ASC.                                     
           ADD 1  TO FRM-IFLD.                                          
           PERFORM PUT-TAB-DEU.                                         
           IF FRM-IFLD < WSS-IMAX                                       
               MOVE FIO-GET-NXT TO FIO-CMND                             
               PERFORM DEU-FIO-RDE                                      
               IF FIO-STAT-OKS AND                                      
                   RDE-CAI-ICLI IN DEU-FLD = RDE-CAI-ICLI IN RDE AND    
                   RDE-IIC-ICLI IN DEU-FLD = RDE-IIC-ICLI IN RDE        
                   GO TO LUP-PUT-TAB.                                   
           MOVE '00' TO FIO-STAT.                                       
       FIN-PUT-TAB.                                                     
           EXIT.                                                        
       SCRL-TAB-DEU SECTION.                                            
       INI-SCRL-TAB-DEU.                                                
           PERFORM GET-FHOY.                                            
           PERFORM INI-CAM.                                             
           MOVE SPACES TO MSG-COD-MENS.                                 
           MOVE SCR-CIC-ICLI TO DEU-CIC-ICLI IN DEU.                    
           MOVE SCR-R-RMAX   TO DEU-RMAX.                               
           MOVE SCR-TAB-IRDE TO RDE-TAB-IRDE.                           
           IF FRM-FFLD = FRM-FFLD-PF6                                   
               IF SCR-R-ASC < DEU-RMAX                                  
                   PERFORM PUT-TAB-ASC                                  
                   IF FIO-STAT = FIO-STAT-NEX                           
                       IF DEU-R > 50                                    
                           MOVE 'RDEDEUD50>' TO MSG-COD-MENS            
                       ELSE                                             
                           MOVE 'RDEDEUDNONXT' TO MSG-COD-MENS          
                   ELSE                                                 
                       IF SCR-R-ASC < DEU-RMAX                          
                           MOVE 'RDEDEUDMAS' TO MSG-COD-MENS            
                       ELSE                                             
                           MOVE 'RDEDEUDNONXT' TO MSG-COD-MENS          
               ELSE                                                     
                   MOVE 'RDEDEUDNONXT' TO MSG-COD-MENS                  
           ELSE                                                         
               IF SCR-R-DSC > 1                                         
                   PERFORM PUT-TAB-DSC                                  
                   IF FIO-STAT = FIO-STAT-NEX                           
                       MOVE 'RDEDEUDNOPRV' TO MSG-COD-MENS              
                   ELSE                                                 
                       IF SCR-R-DSC > 1                                 
                           MOVE 'RDEDEUDMAS' TO MSG-COD-MENS            
                       ELSE                                             
                           MOVE 'RDEDEUDNOPRV' TO MSG-COD-MENS          
               ELSE                                                     
                   MOVE 'RDEDEUDNOPRV' TO MSG-COD-MENS.                 
           IF MSG-COD-MENS > SPACES                                     
               MOVE 'DEU' TO MSG-COD-SIST                               
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN2.                           
       FIN-SCRL-TAB-DEU.                                                
           EXIT.                                                        
       PUT-TAB-ASC SECTION.                                             
       INI-PUT-TAB-ASC.                                                 
           MOVE SCR-R-ASC TO DEU-R.                                     
           MOVE 0 TO FRM-IFLD.                                          
           MOVE FIO-GET-NLS TO FIO-CMND.                                
           PERFORM DEU-FIO-RDE.                                         
           IF NOT FIO-STAT-OKS OR                                       
              RDE-CAI-ICLI IN DEU-FLD NOT = RDE-CAI-ICLI IN RDE OR      
              RDE-IIC-ICLI IN DEU-FLD NOT = RDE-IIC-ICLI IN RDE         
               MOVE FIO-STAT-NEX TO FIO-STAT                            
               GO TO FIN-PUT-TAB-ASC.                                   
           MOVE DEU-R TO SCR-R-DSC.                                     
       LUP-PUT-TAB-ASC.                                                 
           MOVE DEU-R TO SCR-R-ASC.                                     
           ADD 1  TO FRM-IFLD.                                          
           IF FRM-IFLD = 1                                              
               ADD 1 FRM-IFLD GIVING WSS-I                              
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1            
                           UNTIL WSS-I > WSS-IMAX.                      
           PERFORM PUT-TAB-DEU.                                         
           IF FRM-IFLD < WSS-IMAX                                       
               MOVE FIO-GET-NXT TO FIO-CMND                             
               PERFORM DEU-FIO-RDE                                      
               IF FIO-STAT-OKS AND                                      
                   RDE-CAI-ICLI IN DEU-FLD = RDE-CAI-ICLI IN RDE AND    
                   RDE-IIC-ICLI IN DEU-FLD = RDE-IIC-ICLI IN RDE        
                   GO TO LUP-PUT-TAB-ASC.                               
           MOVE '00' TO FIO-STAT.                                       
       FIN-PUT-TAB-ASC.                                                 
           EXIT.                                                        
       PUT-TAB-DSC SECTION.                                             
       INI-PUT-TAB-DSC.                                                 
           MOVE SCR-R-DSC TO DEU-R.                                     
           ADD 1 WSS-IMAX GIVING FRM-IFLD.                              
           MOVE FIO-GET-LEQ TO FIO-CMND.                                
           PERFORM DEU-FIO-RDE.                                         
           IF NOT FIO-STAT-OKS OR                                       
              RDE-CAI-ICLI IN DEU-FLD NOT = RDE-CAI-ICLI IN RDE OR      
              RDE-IIC-ICLI IN DEU-FLD NOT = RDE-IIC-ICLI IN RDE         
               MOVE FIO-STAT-NEX TO FIO-STAT                            
               GO TO FIN-PUT-TAB-DSC.                                   
           MOVE DEU-R TO SCR-R-ASC.                                     
       LUP-PUT-TAB-DSC.                                                 
           MOVE DEU-R TO SCR-R-DSC.                                     
           SUBTRACT 1 FROM FRM-IFLD.                                    
           IF FRM-IFLD = WSS-IMAX                                       
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I                    
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1           
                           UNTIL WSS-I < 1.                             
           PERFORM PUT-TAB-DEU.                                         
           IF FRM-IFLD > 1                                              
               MOVE FIO-GET-PRV TO FIO-CMND                             
               PERFORM DEU-FIO-RDE                                      
               IF FIO-STAT-OKS AND                                      
                   RDE-CAI-ICLI IN DEU-FLD = RDE-CAI-ICLI IN RDE AND    
                   RDE-IIC-ICLI IN DEU-FLD = RDE-IIC-ICLI IN RDE        
                   GO TO LUP-PUT-TAB-DSC.                               
           MOVE '00' TO FIO-STAT.                                       
       FIN-PUT-TAB-DSC.                                                 
           EXIT.                                                        
       PUT-TAB-DEU SECTION.                                             
       INI-PUT-TAB-DEU.                                                 
           MOVE RDE-IND-TIPD IN RDE TO FRM-IND-TIPD IN                  
                                           DEU-FLD(FRM-IFLD).           
           MOVE RDE-IND-SCRT IN RDE TO FRM-IND-SCRT IN                  
                                           DEU-FLD(FRM-IFLD).           
           MOVE RDE-IND-SCTB IN RDE TO FRM-IND-SCTB IN                  
                                           DEU-FLD(FRM-IFLD).           
           MOVE RDE-IND-SCBZ IN RDE TO FRM-IND-SCBZ IN                  
                                           DEU-FLD(FRM-IFLD).           
      *BUS-TAB busca glosa de codigo en tabla                           
           IF RDE-COD-COCR IN RDE > SPACES                              
               MOVE 'COL' TO TAB-COD-SIST                               
               MOVE 'AUX' TO TAB-COD-TTAB IN TAB                        
               MOVE RDE-COD-COCR IN RDE TO TAB-COD-CTAB IN TAB          
               PERFORM BUS-TAB                                          
               MOVE TAB-GLS-ABRV IN TAB TO FRM-GLS-COCR IN              
                                           DEU-FLD(FRM-IFLD)            
           ELSE                                                         
               MOVE SPACES              TO FRM-GLS-COCR IN              
                                           DEU-FLD(FRM-IFLD).           
      *BUS-TAB busca glosa de codigo en tabla                           
           IF  RDE-COD-VCCR IN RDE > SPACES                             
               MOVE 'TAB' TO TAB-COD-SIST                               
               MOVE 'VLR' TO TAB-COD-TTAB IN TAB                        
               MOVE RDE-COD-VCCR IN RDE TO TAB-COD-CTAB IN TAB          
               PERFORM BUS-TAB                                          
               MOVE TAB-GLS-ABRV IN TAB TO FRM-GLS-VCCR IN              
                                           DEU-FLD(FRM-IFLD)            
           ELSE                                                         
               MOVE SPACES              TO FRM-GLS-VCCR IN              
                                           DEU-FLD(FRM-IFLD).           
           MOVE SPACES              TO FRM-IND-VCCR IN                  
                                           DEU-FLD(FRM-IFLD).           
           MOVE RDE-VAL-DEUD IN RDE TO FRM-VAL-DEUD IN                  
                                           DEU-FLD(FRM-IFLD).           
      * Calcula deuda en pesos                                          
           PERFORM CAL-DPES.                                            
       FIN-PUT-TAB-DEU.                                                 
           EXIT.                                                        
       CAL-DPES SECTION.                                                
       INI-CAL-DPES.                                                    
           IF RDE-COD-VCCR IN RDE NOT > SPACES                          
               MOVE RDE-VAL-DEUD IN RDE TO                              
                    FRM-VAL-DPES IN DEU-FLD(FRM-IFLD)                   
               MOVE ZEROES TO FRM-VAL-DEUD IN DEU-FLD(FRM-IFLD)         
           ELSE                                                         
           IF RDE-COD-VCCR IN RDE = CAM-COD-MNAC                        
               MOVE RDE-VAL-DEUD IN RDE TO                              
                    FRM-VAL-DPES IN DEU-FLD(FRM-IFLD)                   
           ELSE                                                         
               MOVE RDE-COD-VCCR IN RDE TO CAM-COD-VCAM IN CAM(1)       
               PERFORM GET-CAM                                          
               IF NOT FIO-STAT-OKS                                      
                   MOVE 'X'  TO FRM-IND-VCCR   IN DEU-FLD(FRM-IFLD)     
                   MOVE 'X'  TO WSS-IND-NEXI                            
                   MOVE ZEROES TO FRM-VAL-DPES IN DEU-FLD(FRM-IFLD)     
               ELSE                                                     
                   IF CAM-IND-VIGE IN CAM = 'N'                         
                       MOVE 'N'  TO FRM-IND-VCCR   IN DEU-FLD(FRM-IFLD) 
                       MOVE 'N'  TO WSS-IND-NVIG                        
                       MOVE ZEROES TO FRM-VAL-DPES IN DEU-FLD(FRM-IFLD) 
                   ELSE                                                 
                       MULTIPLY RDE-VAL-DEUD IN RDE BY                  
                                CAM-SGV-VCAM IN CAM                     
                       GIVING   FRM-VAL-DPES IN DEU-FLD(FRM-IFLD).      
       FIN-CAL-DPES.                                                    
           EXIT.                                                        
       GET-RDE SECTION.                                                 
       INI-GET-RDE.                                                     
           MOVE '00' TO FIO-STAT.                                       
           MOVE FIO-STAT TO PGM-STAT-RDE.                               
           IF FIO-CMND = FIO-GET-PRV  OR                                
              FIO-CMND = FIO-GET-LEQ                                    
               SUBTRACT 1 FROM DEU-R                                    
               IF DEU-R NOT > ZEROES                                    
                   MOVE '10' TO FIO-STAT                                
                   MOVE '23' TO PGM-STAT-RDE                            
                   GO TO FIN-GET-RDE                                    
               ELSE                                                     
                   NEXT SENTENCE                                        
           ELSE                                                         
               ADD 1 TO DEU-R                                           
               IF DEU-R > DEU-RMAX                                      
                   MOVE '10' TO FIO-STAT                                
                   MOVE '23' TO PGM-STAT-RDE                            
                   GO TO FIN-GET-RDE.                                   
           IF DEU-R > 50                                                
               MOVE '10' TO FIO-STAT                                    
               MOVE '23' TO PGM-STAT-RDE                                
               GO TO FIN-GET-RDE.                                       
           MOVE DEU-CIC-ICLI IN DEU        TO RDE-CIC-ICLI IN RDE.      
           MOVE RDE-IND-TIPD IN DEU(DEU-R) TO RDE-IND-TIPD IN RDE.      
           MOVE RDE-IND-SCRT IN DEU(DEU-R) TO RDE-IND-SCRT IN RDE.      
           MOVE RDE-IND-SCTB IN DEU(DEU-R) TO RDE-IND-SCTB IN RDE.      
           MOVE RDE-IND-SCBZ IN DEU(DEU-R) TO RDE-IND-SCBZ IN RDE.      
           MOVE RDE-COD-COCR IN DEU(DEU-R) TO RDE-COD-COCR IN RDE.      
           MOVE RDE-COD-VCCR IN DEU(DEU-R) TO RDE-COD-VCCR IN RDE.      
           MOVE RDE-COD-OFCR IN DEU(DEU-R) TO RDE-COD-OFCR IN RDE.      
           MOVE RDE-VAL-DEUD IN DEU(DEU-R) TO RDE-VAL-DEUD IN RDE.      
      *    MOVE RDE-VAL-DPES IN DEU(DEU-R) TO RDE-VAL-DPES IN RDE.      
       FIN-GET-RDE.                                                     
           EXIT.                                                        
       CLR-FRM SECTION.                                                 
       INI-CLR-FRM.                                                     
           MOVE SPACES TO FRM-IND-TIPD IN DEU-FLD(WSS-I).               
           MOVE SPACES TO FRM-IND-SCRT IN DEU-FLD(WSS-I).               
           MOVE SPACES TO FRM-IND-SCTB IN DEU-FLD(WSS-I).               
           MOVE SPACES TO FRM-IND-SCBZ IN DEU-FLD(WSS-I).               
           MOVE SPACES TO FRM-GLS-COCR IN DEU-FLD(WSS-I).               
           MOVE SPACES TO FRM-GLS-VCCR IN DEU-FLD(WSS-I).               
           MOVE SPACES TO FRM-IND-VCCR IN DEU-FLD(WSS-I).               
           MOVE ZEROES TO FRM-VAL-DEUD IN DEU-FLD(WSS-I).               
           MOVE ZEROES TO FRM-VAL-DPES IN DEU-FLD(WSS-I).               
       FIN-CLR-FRM.                                                     
           EXIT.                                                        
       CLR-SCR-RDE SECTION.                                             
       INI-CLR-SCR-RDE.                                                 
           MOVE SPACES TO SCR-KEY-IRDE(WSS-I).                          
           MOVE ZEROES TO SCR-VAL-DEUD(WSS-I).                          
       FIN-CLR-SCR-RDE.                                                 
           EXIT.                                                        
       PUT-TOT-DEU SECTION.                                             
       INI-PUT-TOT-DEU.                                                 
           MOVE ZEROES TO WSS-VAL-TPES IN WSS-VARI.                     
           MOVE 0 TO DEU-R.                                             
           MOVE FIO-GET-NLS TO FIO-CMND.                                
           PERFORM DEU-FIO-RDE.                                         
           IF NOT FIO-STAT-OKS OR                                       
              RDE-CAI-ICLI IN DEU-FLD NOT = RDE-CAI-ICLI IN RDE OR      
              RDE-IIC-ICLI IN DEU-FLD NOT = RDE-IIC-ICLI IN RDE         
               MOVE ZEROES TO FRM-VAL-TPES IN DEU-FLD                   
               GO TO FIN-PUT-TOT-DEU.                                   
       LUP-PUT-TOT-DEU.                                                 
           PERFORM CAL-TPES.                                            
           ADD WSS-VAL-TPES IN WSS-VARI TO FRM-VAL-TPES IN DEU-FLD.     
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           PERFORM DEU-FIO-RDE.                                         
           IF FIO-STAT-OKS AND                                          
              RDE-CAI-ICLI IN DEU-FLD = RDE-CAI-ICLI IN RDE AND         
              RDE-IIC-ICLI IN DEU-FLD = RDE-IIC-ICLI IN RDE             
               GO TO LUP-PUT-TOT-DEU.                                   
       FIN-PUT-TOT-DEU.                                                 
      *ADA-INI 02-OCT-2002                                              
      *Resta la deuda directa/indirecta que NO afecta Margen            
           SUBTRACT RDE-VAL-DDNA IN DEU FROM FRM-VAL-TPES IN DEU-FLD.   
           SUBTRACT RDE-VAL-DINA IN DEU FROM FRM-VAL-TPES IN DEU-FLD.   
       EXT-PUT-TOT-DEU.                                                 
      *ADA-FIN 02-OCT-2002                                              
           EXIT.                                                        
       CAL-TPES SECTION.                                                
       INI-CAL-TPES.                                                    
           IF RDE-COD-VCCR IN RDE NOT > SPACES                          
               MOVE RDE-VAL-DEUD IN RDE TO WSS-VAL-TPES IN WSS-VARI     
           ELSE                                                         
           IF RDE-COD-VCCR IN RDE = CAM-COD-MNAC                        
               MOVE RDE-VAL-DEUD IN RDE TO WSS-VAL-TPES IN WSS-VARI     
           ELSE                                                         
               MOVE RDE-COD-VCCR IN RDE TO CAM-COD-VCAM IN CAM(1)       
               PERFORM GET-CAM                                          
               IF NOT FIO-STAT-OKS                                      
                   MOVE ZEROES TO WSS-VAL-TPES IN WSS-VARI              
               ELSE                                                     
                   IF CAM-IND-VIGE IN CAM = 'N'                         
                       MOVE ZEROES TO WSS-VAL-TPES IN WSS-VARI          
                   ELSE                                                 
                       MULTIPLY RDE-VAL-DEUD IN RDE BY                  
                                CAM-SGV-VCAM IN CAM                     
                       GIVING   WSS-VAL-TPES IN WSS-VARI.               
       FIN-CAL-TPES.                                                    
           EXIT.                                                        
