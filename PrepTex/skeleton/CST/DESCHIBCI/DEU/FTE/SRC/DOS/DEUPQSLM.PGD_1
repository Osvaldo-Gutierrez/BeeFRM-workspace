       PROCEDURE DIVISION.
      *===================

       MAIN SECTION.
       INI-MAIN.
           PERFORM INI.
       LUP-MAIN.
           IF SCR-INST = SCR-INST-HCM
                 PERFORM SCR-COM.
           IF SCR-CCPP = 'MOD'
                 PERFORM SCR-MOD
           ELSE
           IF SCR-CCPP = 'ING'
                 PERFORM SCR-ING
           ELSE
           IF SCR-CCPP = 'ACC'
                 PERFORM SCR-ACC
           ELSE
           IF SCR-CCPP = 'ELI'
                 PERFORM SCR-ELI
           ELSE
           IF SCR-CCPP = 'ACT'
                 PERFORM SCR-ACT
           ELSE
           IF SCR-CCPP = 'PGM'
                 PERFORM SCR-PGM
           ELSE
                 MOVE SCR-INST-HCM TO SCR-INST.
           GO TO LUP-MAIN.
       FIN-MAIN.
           EXIT.

       INI SECTION.
       INI-INI.
       COPY GNSBGEIB.
       COPY GNSBGEDB.
           MOVE LOW-VALUES TO SLM-FLD.
           MOVE LOW-VALUES TO SLM-EDT.
           PERFORM GNS-PRO-RCK.
           PERFORM GNS-BUS-VCA.
           MOVE +3000 TO SCR-TCMA.
           PERFORM GNS-HDL-SYS.
           PERFORM GNS-HDL-FRM.
           MOVE 'DEUPQSLM' TO FIO-PROG.
           PERFORM GNS-BUS-IDD.
      *PTC-INI
           PERFORM GNS-ERR-QUE.
           PERFORM GET-TSK-TERM.
      *PTC-FIN
      *<<<< INI_FIO
           IF SCR-OFRM-SRV
               PERFORM CLR-FRM VARYING WSS-I FROM 1 BY 1
                                 UNTIL WSS-I > WSS-IMAX
                MOVE TSK-TERM-ALF TO SLM-COD-TERM
                MOVE TSK-TERM-ALF TO SLM-COD-TERM-MAS
           ELSE
                MOVE EIBTRMID     TO SLM-COD-TERM
                MOVE EIBTRMID     TO SLM-COD-TERM-MAS.
           MOVE +3859 TO SCR-TCMA.
           PERFORM GNS-ERR-QUE.
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )
               MOVE SCR-SLM-FLD TO SLM-FLD
               GO TO FIN-INI.

           IF ( FRM-FFLD = FRM-FFLD-PF6 )
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM
               ADD 1              TO QUE-NITM
               MOVE SLM-NOM-TSSLM TO QUE-COLA
               MOVE SLM-LEN-TSSLM TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT NOT = QUE-STAT-OKS
                  MOVE SCR-SLM-FLD TO SLM-FLD
                  GO TO FIN-INI.
      *>>>>
       FIN-INI.
           EXIT.

       COPY GNSBHSYS.
       COPY GNSBHFRM.
       COPY GNSBPRCK.
       GNS-BUS-VCA SECTION.
       INI-GNS-BUS-VCA.
           MOVE 0 TO FRM-IFLD.
       LUP-GNS-BUS-VCA.
           ADD 1 TO FRM-IFLD.
           IF FRM-IFLD NOT > EIBCALEN AND
              FRM-IFLD NOT > 3000
                MOVE DFH-CMMA(FRM-IFLD) TO PGM-CMMA-CHR(FRM-IFLD)
                GO TO LUP-GNS-BUS-VCA.
           MOVE PGM-CMMA TO SCR-VARI.
       FIN-GNS-BUS-VCA.
           EXIT.
       COPY GNSBBIDD.

       SCR-COM SECTION.
       INI-SCR-COM.
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-CLRA-SLM.
           PERFORM EDT-KEY-SLM.
           PERFORM EDT-FLD-SLM.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-HCM TO SCR-INST.
           MOVE SCR-CMNP     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       XCT-ERR.
           MOVE 'XCTERR    ' TO MSG-COD-MENS.
           MOVE 'GNS'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO SYS-MEN1.
           MOVE SYS-PROG     TO SYS-MEN2.
           MOVE SYS-ABOR     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       COPY GNSBGGBK.
       FIN-SCR-COM.
           EXIT.

       SCR-MOD SECTION.
       INI-SCR-MOD.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-MOD
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-MOD.
           PERFORM DESSCRTRL.
       MSG-SCR-MOD.
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-MOD.
           EXIT.

       BRW-SCR-MOD SECTION.
       INI-BRW-SCR-MOD.
       FIN-BRW-SCR-MOD.
           EXIT.

       SCR-ING SECTION.
       INI-SCR-ING.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-IKY
               MOVE SCR-INST-GKY TO SCR-INST
      *PTC-INI
               IF NOT SCR-OFRM-SRV
                   PERFORM SCR-DMSG
               ELSE
                   PERFORM SCR-PUTA
      *PTC-FIN
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-RST
                PERFORM GET-IKY
                IF NOT FRM-STAT-ABT
                        MOVE SPACES TO FRM-MENS
                        PERFORM FND-KEY
                        IF NOT FIO-STAT-OKS AND
                           FIO-STAT NOT = FIO-STAT-NEX
      *JSS                  PERFORM CLR-FLD-SLM
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
                            PERFORM SCR-TRL
                        ELSE
      *PTC-INI
                        MOVE 'S' TO SCR-IERR
      *PTC-FIN
                        MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR
                        IF FRM-MENS NOT > SPACES
                            MOVE 'REGEXI    ' TO MSG-COD-MENS
                            MOVE 'GNS' TO MSG-COD-SIST
                            PERFORM GET-MSG
                            MOVE MSG-GLS-DESC TO FRM-MENS
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-ING
                        ELSE
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-ING
                ELSE
                        MOVE SPACES TO FRM-MENS
                        PERFORM SCR-MSSG
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ING
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON.
           PERFORM DESSCRTRL.
       MSG-SCR-ING.
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ING.
           EXIT.

       SCR-ACC SECTION.
       INI-SCR-ACC.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ACC
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-ACC.
           PERFORM DESSCRTRL.
      *<<<< PFS_SCR_ACC
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM VAL-NUM-KEY-SLM
               PERFORM VAL-NUM-FLD-SLM
               PERFORM SCRL-TAB-SLM
               PERFORM SCR-TRL.
      *>>>>
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ACC.
           EXIT.

       BRW-SCR-ACC SECTION.
       INI-BRW-SCR-ACC.
       FIN-BRW-SCR-ACC.
           EXIT.

       SCR-ELI SECTION.
       INI-SCR-ELI.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ELI
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-ELI.
           PERFORM DESSCRTRL.
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ELI.
           EXIT.

       BRW-SCR-ELI SECTION.
       INI-BRW-SCR-ELI.
       FIN-BRW-SCR-ELI.
           EXIT.

       SCR-ACT SECTION.
       INI-SCR-ACT.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-RST
                PERFORM GET-KEY
                IF NOT FRM-STAT-ABT
                        MOVE SPACES TO FRM-MENS
                        PERFORM FND-KEY
                        IF FIO-STAT-OKS
                            MOVE SCR-ACTU-MOD TO SCR-ACTU
                            PERFORM PUT-SCR
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
                            PERFORM SCR-TRL
                        ELSE
                        MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR
                        IF FIO-STAT-EOF
                            IF FRM-MENS NOT > SPACES
                                MOVE 'REGEOF    ' TO MSG-COD-MENS
                                MOVE 'GNS' TO MSG-COD-SIST
                                PERFORM GET-MSG
                                MOVE MSG-GLS-DESC TO FRM-MENS
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                            ELSE
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                        ELSE
                        IF FIO-STAT = FIO-STAT-NEX
                            IF FRM-MENS NOT > SPACES
                                MOVE 'REGNEX    ' TO MSG-COD-MENS
                                MOVE 'GNS' TO MSG-COD-SIST
                                PERFORM GET-MSG
                                MOVE MSG-GLS-DESC TO FRM-MENS
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                            ELSE
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                        ELSE
                            MOVE SCR-ACTU-ING TO SCR-ACTU
      *JSS                  PERFORM CLR-FLD-SLM
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
                            PERFORM SCR-TRL
                ELSE
                        MOVE SPACES TO FRM-MENS
                        PERFORM SCR-MSSG
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ACT
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-ACT.
           PERFORM DESSCRTRL.
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ACT.
           EXIT.

       BRW-SCR-ACT SECTION.
       INI-BRW-SCR-ACT.
       FIN-BRW-SCR-ACT.
           EXIT.

       SCR-PGM SECTION.
       INI-SCR-PGM.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-RST
                PERFORM GET-KEY
                IF FRM-STAT-ABT
                    MOVE SPACES TO FRM-MENS
                    PERFORM SCR-MSSG
                    PERFORM DESSCRTRL
                    GO TO FIN-SCR-PGM
                ELSE
                    MOVE SPACES TO FRM-MENS
                    PERFORM FND-KEY
                    IF FIO-STAT = FIO-STAT-NEX
                        IF FRM-MENS NOT > SPACES
                            MOVE 'REGNEX    ' TO MSG-COD-MENS
                            MOVE 'GNS' TO MSG-COD-SIST
                            PERFORM GET-MSG
                            MOVE MSG-GLS-DESC TO FRM-MENS
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-PGM
                        ELSE
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-PGM
                    ELSE
                        MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
                        PERFORM SCR-TRL
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-PGM.
       TRL-SCR-PGM.
           PERFORM DESSCRTRL.
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-PGM.
           EXIT.

       BRW-SCR-PGM SECTION.
       INI-BRW-SCR-PGM.
       FIN-BRW-SCR-PGM.
           EXIT.

       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               PERFORM SCR-CLRA-SLM
               PERFORM EDT-KEY-SLM
               PERFORM EDT-FLD-SLM
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-SLM
               PERFORM EDT-KEY-SLM
           ELSE
               PERFORM CLR-FLD-SLM
               PERFORM EDT-FLD-SLM.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

       SCR-KEY SECTION.
       INI-SCR-KEY.
           PERFORM SCR-RST.
           PERFORM GET-KEY.
           IF FRM-STAT-ABT
                   MOVE SPACES TO FRM-MENS
                   PERFORM SCR-MSSG
                   GO TO FIN-SCR-KEY.
       FND-SCR-KEY.
           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
           IF FIO-STAT-OKS
               PERFORM PUT-SCR
               MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
               GO TO FIN-SCR-KEY.
      *JSS ELSE
      *        PERFORM CLR-FLD-SLM.
       ERR-SCR-KEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR.
           IF FRM-MENS NOT > SPACES
               IF FIO-STAT-EOF
                   MOVE 'REGEOF    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
               IF FIO-STAT = FIO-STAT-NEX
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
           MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-SCR-KEY.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO SLM-FLD.
           MOVE LOW-VALUES TO SLM-EDT.
       FIN-SCR-RST.
           EXIT.

       GET-KEY SECTION.
       INI-GET-KEY.
           PERFORM SCR-GKEY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-KEY.
           EXIT.

       GET-IKY SECTION.
       INI-GET-IKY.
           PERFORM SCR-GIKY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-IKY.
           EXIT.

       FND-KEY SECTION.
       INI-FND-KEY.
      *<<<< INI_FND_KEY
           MOVE SLM-NOM-TSSLM     TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE SLM-NOM-TSSLM-MAS TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
      *>>>>
           PERFORM KEY-ALL-SLM.
           MOVE FIO-GET-KEY TO FIO-CMND.
      *<<<< FND_KEY_CMND
           MOVE SOL-COD-DOCU IN SLM-FLD TO WSS-REQ-DOCU.

           IF SOL-CAI-IOPC IN SLM-FLD > SPACES AND
              SOL-IIC-IOPC IN SLM-FLD > ZEROES
              MOVE 02 TO WSS-FIO-AKEY
              MOVE 'SOL-KEY-IOPC' TO FIO-AKEY
              MOVE FIO-GET-NLS TO FIO-CMND.
           IF SOL-COD-ESTA IN SLM-FLD > SPACES AND
              NOT (SOL-CAI-IOPC IN SLM-FLD > SPACES AND
                   SOL-IIC-IOPC IN SLM-FLD > ZEROES)
              MOVE 04 TO WSS-FIO-AKEY
              MOVE 'SOL-COD-ESTA' TO FIO-AKEY
              MOVE FIO-GET-NLS TO FIO-CMND.
           IF DBC-NUM-ICLI IN SLM-FLD > ZEROES AND
              DBC-VRF-ICLI IN SLM-FLD > SPACES AND
              NOT (SOL-COD-ESTA IN SLM-FLD > SPACES) AND
              NOT (SOL-CAI-IOPC IN SLM-FLD > SPACES AND
                   SOL-IIC-IOPC IN SLM-FLD > ZEROES)
              MOVE 03 TO WSS-FIO-AKEY
              PERFORM OBT-CIC-ICLI
              IF NOT FIO-STAT-OKS
                 MOVE 'CLI    NEX' TO MSG-COD-MENS
                 MOVE 'TAB' TO MSG-COD-SIST
                 PERFORM GET-MSG
                 MOVE MSG-GLS-DESC TO FRM-MEN1
                 SET FIO-STAT-ERR TO TRUE
                 GO TO FIN-FND-KEY-SOL
              ELSE
                 MOVE 'SOL-CIC-ICLI' TO FIO-AKEY
                 MOVE FIO-GET-NLS TO FIO-CMND
                 MOVE DBC-CAI-ICLI IN DBC TO SOL-CAI-ICLI IN SOL
                 MOVE DBC-IIC-ICLI IN DBC TO SOL-IIC-ICLI IN SOL
                 MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI-ALF IN SLM-FLD
                 MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN SLM-FLD
                 MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
                 PERFORM CPT-BLKS
                 MOVE CPT-STRN TO FRM-GLS-NOMC IN SLM-FLD.
      *>>>>
           PERFORM DEU-FIO-SOL.
           MOVE FIO-STAT TO PGM-STAT-SOL.
      *<<<< FIN_FND_KEY
           IF FIO-STAT = FIO-STAT-NEX
               MOVE 'S' TO SCR-IERR.

           IF NOT FIO-STAT-OKS
               GO TO FIN-FND-KEY-SOL.

           IF WSS-FIO-AKEY = 02
              PERFORM BUS-DAT-IOPC
           ELSE
           IF WSS-FIO-AKEY = 03
              PERFORM BUS-DAT-ICLI
           ELSE
           IF WSS-FIO-AKEY = 04
              PERFORM BUS-DAT-ESTA.

       FIN-FND-KEY-SOL.
           MOVE WSS-FLD-CAI-IOPC TO SOL-CAI-IOPC IN SOL.
           MOVE WSS-FLD-IIC-IOPC TO SOL-IIC-IOPC IN SOL.
           MOVE WSS-FLD-COD-ESTA TO SOL-COD-ESTA IN SOL.
           MOVE WSS-FLD-NUM-ICLI TO DBC-NUM-ICLI IN SLM-FLD.
           MOVE WSS-FLD-VRF-ICLI TO DBC-VRF-ICLI IN SLM-FLD.
           MOVE WSS-FLD-GLS-NOMC TO FRM-GLS-NOMC IN SLM-FLD.
           MOVE WSS-FLD-COD-DOCU TO SOL-COD-DOCU IN SOL.
      *>>>>
       FIN-FND-KEY.
           IF FIO-STAT = FIO-STAT-NEX
               MOVE 'S' TO SCR-IERR.
       EXT-FND-KEY.
           EXIT.
 
       PUT-SCR SECTION.
       INI-PUT-SCR.
           PERFORM PUT-ALL-SLM.
       FIN-PUT-SCR.
           EXIT.

       STP-EXIT SECTION.
       INI-STP-EXIT.
           MOVE SCR-PROG TO PGM-PROG.
           MOVE SCR-NTRN TO PGM-NTRN.

           MOVE SCR-PROV TO SCR-PROG.
           MOVE SCR-NTRV TO SCR-NTRN.

           MOVE PGM-PROG TO SCR-PROV
           MOVE PGM-NTRN TO SCR-NTRV.
       FIN-STP-EXIT.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO SYS-NTRN.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
      *PTC-INI
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE SLM-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO SLM-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GKEY SECTION.
       INI-SCR-GKEY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-SLM
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO SLM-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GKEY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-SLM.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-SLM
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-SLM.
           PERFORM VAL-IKY-SLM.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-MKY-SLM.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-AKY-SLM.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-CON-KEY-SLM.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GKEY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-SLM.
       ERR-SCR-GKEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-SLM.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GKEY.
           PERFORM SCR-MSSG.

       SCR-GIKY SECTION.
       INI-SCR-GIKY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-SLM
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO SLM-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GIKY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-SLM.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-IKY-SLM
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-SLM.
           PERFORM VAL-IKY-SLM.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GIKY.
           PERFORM VAL-CON-IKY-SLM.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GIKY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-SLM.
       ERR-SCR-GIKY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-IKY-SLM.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GIKY.
           PERFORM SCR-MSSG.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
      *>>>>
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO SLM-GLS-MEN2
               MOVE SCR-PRMT     TO SLM-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO SLM-FLD.
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO SLM-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE SLM-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO SLM-FLD
      * FLB  7-NOV-1994
               PERFORM CLR-KEY-SLM

               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO SLM-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO SLM-GLS-MENS.
           PERFORM PRO-MKY-SLM.
           PERFORM PRO-AKY-SLM.
           PERFORM UNP-IKY-SLM.
           PERFORM FST-KEY-SLM.
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE SLM-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO SLM-FLD
               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO SLM-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO SLM-GLS-MENS.
           PERFORM UNP-KEY-SLM.
           PERFORM FST-KEY-SLM.
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
           PERFORM PRO-IKY-SLM.
           PERFORM PRO-AKY-SLM.
           PERFORM UNP-MKY-SLM.
           PERFORM UNP-FLD-SLM.
           PERFORM FST-FLD-SLM.
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO SLM-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-SLM.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
      *PTC-INI
           IF SCR-STAT NOT = SCR-STPP
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-SLM
               PERFORM EDT-FLD-SLM
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-SLM
               PERFORM EDT-FLD-SLM
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-SLM.
           PERFORM EDT-FLD-SLM.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

      * PGM-EBR-ALL SECTION.
      * INI-PGM-EBR-ALL.
      *     MOVE 'GNS' TO FIO-SIST.
      *     MOVE FIO-EBR-ALL TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
      * FIN-PGM-EBR-ALL.
      *     EXIT.

       COPY DEUBQSLM.
       COPY GNSBVNUM.
       COPY DEUBFSOL.
       COPY GNSBFMSG.
       COPY GNSBGSYS.
       COPY GNSBGDTC.
      *PTC-INI
      *PTC-SUB
      * ISP  10-MAY-2001 13:46:35 
      * COPY GNSBGFRM.
       COPY GNSBG3CF.
      *PTC-FIN
       COPY GNSBGMSG.
       COPY GNSBEKEY.
       COPY GNSBIABT.
       COPY GNSBVCOD.
       COPY GNSBVTAB.
       COPY GNSBFTAB.
       COPY GNSBVIND.
       COPY GNSBVMSC.
       COPY GNSBFMSC.
       COPY GNSBVIDC.
       COPY GNSBPVRF.
       COPY GNSBGFEC.
       COPY GNSBGHOY.
      *PTC-INI
       COPY GNSBGQUE.
       COPY GNSBGTSK.
      *PTC-FIN

      *<<<< EOF
       COPY SGCBFDBC.
       COPY GNSBGCPT.

       OBT-CIC-ICLI SECTION.
       INI-OBT-CIC-ICLI.
           MOVE DBC-NUM-ICLI IN SLM-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE SPACES                  TO DBC-IND-ICLI IN DBC.
           MOVE SPACES                  TO DBC-GLS-ICLI IN DBC.
           MOVE 'DBC-IDC-ICLI'          TO FIO-AKEY.
           MOVE FIO-GET-KEY             TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
       FIN-OBT-CIC-ICLI.
           EXIT.


       BUS-DAT-IOPC SECTION.
       INI-BUS-DAT-IOPC.
           MOVE SOL-CAI-IOPC IN SLM-FLD TO WSS-FLD-CAI-IOPC
           MOVE SOL-IIC-IOPC IN SLM-FLD TO WSS-FLD-IIC-IOPC
           MOVE SPACES                  TO WSS-FLD-COD-ESTA
           MOVE ZEROES                  TO WSS-FLD-NUM-ICLI
           MOVE SPACES                  TO WSS-FLD-VRF-ICLI
           MOVE SPACES                  TO WSS-FLD-GLS-NOMC
           MOVE SOL-COD-DOCU IN SLM-FLD TO WSS-FLD-COD-DOCU

           IF SOL-CAI-IOPC IN SLM-FLD NOT = SOL-CAI-IOPC IN SOL OR
              SOL-IIC-IOPC IN SLM-FLD NOT = SOL-IIC-IOPC IN SOL
              MOVE 'REGNEX    ' TO MSG-COD-MENS
              MOVE 'GNS' TO MSG-COD-SIST
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              SET FIO-STAT-ERR TO TRUE
              MOVE 'S' TO SCR-IERR
              GO TO FIN-FND-KEY-SOL.

           MOVE SPACES TO MSG-COD-MENS.
           PERFORM CAR-VEC-IOPC.
           IF MSG-COD-MENS > SPACES
              MOVE 'TAB' TO MSG-COD-SIST
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE '00' TO FIO-STAT
              MOVE SCR-STAT-CON TO SCR-STAT
              GO TO FIN-FND-KEY-SOL.

           IF WSS-IND-VEC =  WSS-GLS-UNO
              MOVE 'REGNEX    ' TO MSG-COD-MENS
              MOVE 'GNS' TO MSG-COD-SIST
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              SET FIO-STAT-ERR TO TRUE
              MOVE 'S' TO SCR-IERR
              GO TO FIN-FND-KEY-SOL.

           PERFORM CAR-FLD-SLM.

           PERFORM PUT-TAB.
           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.

       FIN-BUS-DAT-IOPC.
           EXIT.

       BUS-DAT-ICLI SECTION.
       INI-BUS-DAT-ICLI.
           MOVE SPACES                  TO WSS-FLD-CAI-IOPC
           MOVE ZEROES                  TO WSS-FLD-IIC-IOPC
           MOVE SPACES                  TO WSS-FLD-COD-ESTA
           MOVE DBC-NUM-ICLI IN SLM-FLD TO WSS-FLD-NUM-ICLI
           MOVE DBC-VRF-ICLI IN SLM-FLD TO WSS-FLD-VRF-ICLI
           MOVE FRM-GLS-NOMC IN SLM-FLD TO WSS-FLD-GLS-NOMC
           MOVE SOL-COD-DOCU IN SLM-FLD TO WSS-FLD-COD-DOCU

           IF DBC-CAI-ICLI IN DBC NOT = SOL-CAI-ICLI IN SOL OR
              DBC-IIC-ICLI IN DBC NOT = SOL-IIC-ICLI IN SOL
              MOVE 'REGNEX    ' TO MSG-COD-MENS
              MOVE 'GNS' TO MSG-COD-SIST
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              SET FIO-STAT-ERR TO TRUE
              MOVE 'S' TO SCR-IERR
              GO TO FIN-FND-KEY-SOL.

           MOVE SPACES TO MSG-COD-MENS.
           PERFORM CAR-VEC-ICLI.
           IF MSG-COD-MENS > SPACES
              MOVE 'TAB' TO MSG-COD-SIST
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE '00' TO FIO-STAT
              MOVE SCR-STAT-CON TO SCR-STAT
              GO TO FIN-FND-KEY-SOL.

           IF WSS-IND-VEC =  WSS-GLS-UNO
              MOVE 'REGNEX    ' TO MSG-COD-MENS
              MOVE 'GNS' TO MSG-COD-SIST
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              SET FIO-STAT-ERR TO TRUE
              MOVE 'S' TO SCR-IERR
              GO TO FIN-FND-KEY-SOL.

           PERFORM CAR-FLD-SLM.

           PERFORM PUT-TAB.
           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.

       FIN-BUS-DAT-ICLI.
           EXIT.

       BUS-DAT-ESTA SECTION.
           MOVE SPACES                  TO WSS-FLD-CAI-IOPC
           MOVE ZEROES                  TO WSS-FLD-IIC-IOPC
           MOVE SOL-COD-ESTA IN SLM-FLD TO WSS-FLD-COD-ESTA
           MOVE ZEROES                  TO WSS-FLD-NUM-ICLI
           MOVE SPACES                  TO WSS-FLD-VRF-ICLI
           MOVE SPACES                  TO WSS-FLD-GLS-NOMC
           MOVE SOL-COD-DOCU IN SLM-FLD TO WSS-FLD-COD-DOCU

           IF SOL-COD-ESTA IN SLM-FLD NOT = SOL-COD-ESTA IN SOL
              MOVE 'REGNEX    ' TO MSG-COD-MENS
              MOVE 'GNS' TO MSG-COD-SIST
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              SET FIO-STAT-ERR TO TRUE
              MOVE 'S' TO SCR-IERR
              GO TO FIN-FND-KEY-SOL.

           MOVE SPACES TO MSG-COD-MENS.
           PERFORM CAR-VEC-ESTA.
           IF MSG-COD-MENS > SPACES
              MOVE 'TAB' TO MSG-COD-SIST
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE '00' TO FIO-STAT
              MOVE SCR-STAT-CON TO SCR-STAT
              GO TO FIN-FND-KEY-SOL.

           IF WSS-IND-VEC =  WSS-GLS-UNO
              MOVE 'REGNEX    ' TO MSG-COD-MENS
              MOVE 'GNS' TO MSG-COD-SIST
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              SET FIO-STAT-ERR TO TRUE
              MOVE 'S' TO SCR-IERR
              GO TO FIN-FND-KEY-SOL.

           PERFORM CAR-FLD-SLM.

           PERFORM PUT-TAB.
           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.

       INI-BUS-DAT-ESTA.

       FIN-BUS-DAT-ESTA.
           EXIT.

       CAR-VEC-IOPC SECTION.
       INI-CAR-VEC-IOPC.
           MOVE WSS-GLS-UNO IN WSS-VARI TO WSS-IND-VEC IN WSS-VARI.
           MOVE SPACES                  TO WSS-IND-SOL.

           MOVE SOL-CIC-ICLI IN SOL TO DBC-CIC-ICLI IN DBC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           IF NOT FIO-STAT-OKS
              MOVE SPACES TO DBC-NUM-ICLI IN DBC
              MOVE SPACES TO DBC-VRF-ICLI IN DBC.

       LOOP-BUS-SOL-IOPC.
           IF WSS-REQ-DOCU > SPACES AND
              WSS-REQ-DOCU NOT = SOL-COD-DOCU IN SOL
               GO TO FIN-CAR-VEC-IOPC.

           MOVE WSS-GLS-SI              TO WSS-IND-SOL.
           MOVE WSS-IND-VEC             TO WSS-NUM-SOLI(WSS-IND-VEC).
           MOVE SOL-COD-DOCU IN SOL     TO WSS-COD-DOCU(WSS-IND-VEC).
           MOVE SOL-CAI-ISOL IN SOL     TO WSS-CAI-ISOL(WSS-IND-VEC).
           MOVE SOL-IIC-ISOL IN SOL     TO WSS-IIC-ISOL(WSS-IND-VEC).
           MOVE SOL-CAI-IOPC IN SOL     TO WSS-CAI-IOPC(WSS-IND-VEC).
           MOVE SOL-IIC-IOPC IN SOL     TO WSS-IIC-IOPC(WSS-IND-VEC).
           MOVE DBC-NUM-ICLI IN DBC     TO WSS-NUM-ICLI(WSS-IND-VEC).
           MOVE DBC-VRF-ICLI IN DBC     TO WSS-DIG-ICLI(WSS-IND-VEC).
           MOVE SOL-COD-ESTA IN SOL     TO WSS-COD-ESTA(WSS-IND-VEC).
           MOVE SOL-FEC-FFE1 IN SOL     TO WSS-FEC-FFE1(WSS-IND-VEC).

       NXT-BUS-SOL-IOPC.
           IF WSS-IND-SOL = WSS-GLS-SI
              ADD WSS-GLS-UNO IN WSS-VARI TO WSS-IND-VEC IN WSS-VARI.
           MOVE SPACES TO WSS-IND-SOL.
           MOVE 'SOL-KEY-IOPC' TO FIO-AKEY.
           MOVE FIO-GET-NXT TO FIO-CMND
           PERFORM DEU-FIO-SOL.

           IF NOT (FIO-STAT-OKS AND
              SOL-CAI-IOPC IN SLM-FLD = SOL-CAI-IOPC IN SOL AND
              SOL-IIC-IOPC IN SLM-FLD = SOL-IIC-IOPC IN SOL)
               GO TO FIN-CAR-VEC-IOPC.

           IF WSS-IND-VEC IN WSS-VARI > WSS-VEC-MAX IN WSS-VARI
               GO TO FIN-CAR-VEC-IOPC.

           GO TO LOOP-BUS-SOL-IOPC.

       FIN-CAR-VEC-IOPC.
           EXIT.

       CAR-VEC-ICLI SECTION.
       INI-CAR-VEC-ICLI.
           MOVE WSS-GLS-UNO IN WSS-VARI TO WSS-IND-VEC IN WSS-VARI.
           MOVE SPACES                  TO WSS-IND-SOL.

       LOOP-BUS-SOL-ICLI.
           IF WSS-REQ-DOCU > SPACES AND
              WSS-REQ-DOCU NOT = SOL-COD-DOCU IN SOL
               GO TO FIN-CAR-VEC-ICLI.

           MOVE WSS-GLS-SI              TO WSS-IND-SOL.
           MOVE WSS-IND-VEC             TO WSS-NUM-SOLI(WSS-IND-VEC).
           MOVE SOL-COD-DOCU IN SOL     TO WSS-COD-DOCU(WSS-IND-VEC).
           MOVE SOL-CAI-ISOL IN SOL     TO WSS-CAI-ISOL(WSS-IND-VEC).
           MOVE SOL-IIC-ISOL IN SOL     TO WSS-IIC-ISOL(WSS-IND-VEC).
           MOVE SOL-CAI-IOPC IN SOL     TO WSS-CAI-IOPC(WSS-IND-VEC).
           MOVE SOL-IIC-IOPC IN SOL     TO WSS-IIC-IOPC(WSS-IND-VEC).
           MOVE DBC-NUM-ICLI IN DBC     TO WSS-NUM-ICLI(WSS-IND-VEC).
           MOVE DBC-VRF-ICLI IN DBC     TO WSS-DIG-ICLI(WSS-IND-VEC).
           MOVE SOL-COD-ESTA IN SOL     TO WSS-COD-ESTA(WSS-IND-VEC).
           MOVE SOL-FEC-FFE1 IN SOL     TO WSS-FEC-FFE1(WSS-IND-VEC).

       NXT-BUS-SOL-ICLI.
           IF WSS-IND-SOL = WSS-GLS-SI
              ADD WSS-GLS-UNO IN WSS-VARI TO WSS-IND-VEC IN WSS-VARI.
           MOVE SPACES TO WSS-IND-SOL.
           MOVE 'SOL-CIC-ICLI' TO FIO-AKEY.
           MOVE FIO-GET-NXT TO FIO-CMND
           PERFORM DEU-FIO-SOL.

           IF NOT (FIO-STAT-OKS AND
              DBC-CAI-ICLI IN DBC = SOL-CAI-ICLI IN SOL AND
              DBC-IIC-ICLI IN DBC = SOL-IIC-ICLI IN SOL)
               GO TO FIN-CAR-VEC-ICLI.

           IF WSS-IND-VEC IN WSS-VARI > WSS-VEC-MAX IN WSS-VARI
               GO TO FIN-CAR-VEC-ICLI.

           GO TO LOOP-BUS-SOL-ICLI.

       FIN-CAR-VEC-ICLI.
           EXIT.

       CAR-VEC-ESTA SECTION.
       INI-CAR-VEC-ESTA.
           MOVE WSS-GLS-UNO IN WSS-VARI TO WSS-IND-VEC IN WSS-VARI.
           MOVE SPACES                  TO WSS-IND-SOL.

           MOVE SOL-CIC-ICLI IN SOL TO DBC-CIC-ICLI IN DBC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           IF NOT FIO-STAT-OKS
              MOVE SPACES TO DBC-NUM-ICLI IN DBC
              MOVE SPACES TO DBC-VRF-ICLI IN DBC.

       LOOP-BUS-SOL-ESTA.
           IF WSS-REQ-DOCU > SPACES AND
              WSS-REQ-DOCU NOT = SOL-COD-DOCU IN SOL
               GO TO NXT-BUS-SOL-ESTA.

           MOVE WSS-GLS-SI              TO WSS-IND-SOL.
           MOVE WSS-IND-VEC             TO WSS-NUM-SOLI(WSS-IND-VEC).
           MOVE SOL-COD-DOCU IN SOL     TO WSS-COD-DOCU(WSS-IND-VEC).
           MOVE SOL-CAI-ISOL IN SOL     TO WSS-CAI-ISOL(WSS-IND-VEC).
           MOVE SOL-IIC-ISOL IN SOL     TO WSS-IIC-ISOL(WSS-IND-VEC).
           MOVE SOL-CAI-IOPC IN SOL     TO WSS-CAI-IOPC(WSS-IND-VEC).
           MOVE SOL-IIC-IOPC IN SOL     TO WSS-IIC-IOPC(WSS-IND-VEC).
           MOVE DBC-NUM-ICLI IN DBC     TO WSS-NUM-ICLI(WSS-IND-VEC).
           MOVE DBC-VRF-ICLI IN DBC     TO WSS-DIG-ICLI(WSS-IND-VEC).
           MOVE SOL-COD-ESTA IN SOL     TO WSS-COD-ESTA(WSS-IND-VEC).
           MOVE SOL-FEC-FFE1 IN SOL     TO WSS-FEC-FFE1(WSS-IND-VEC).

       NXT-BUS-SOL-ESTA.
           IF WSS-IND-SOL = WSS-GLS-SI
              ADD WSS-GLS-UNO IN WSS-VARI TO WSS-IND-VEC IN WSS-VARI.
           MOVE SPACES TO WSS-IND-SOL.
           MOVE 'SOL-COD-ESTA' TO FIO-AKEY.
           MOVE FIO-GET-NXT TO FIO-CMND
           PERFORM DEU-FIO-SOL.

           IF NOT (FIO-STAT-OKS AND
              SOL-COD-ESTA IN SLM-FLD = SOL-COD-ESTA IN SOL)
               GO TO FIN-CAR-VEC-ESTA.

           MOVE SOL-CIC-ICLI IN SOL TO DBC-CIC-ICLI IN DBC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           IF NOT FIO-STAT-OKS
              MOVE SPACES TO DBC-NUM-ICLI IN DBC
              MOVE SPACES TO DBC-VRF-ICLI IN DBC.

           IF WSS-IND-VEC IN WSS-VARI > WSS-VEC-MAX IN WSS-VARI
               GO TO FIN-CAR-VEC-ESTA.

           GO TO LOOP-BUS-SOL-ESTA.

       FIN-CAR-VEC-ESTA.
           EXIT.

       CAR-FLD-SLM SECTION.
       INI-CAR-FLD-SLM.

           MOVE ZEROES TO WSS-J IN SLM-VARI.

       LOOP-CARGA.
           ADD 1 TO WSS-J IN SLM-VARI.
           IF WSS-J IN SLM-VARI > WSS-JMAX IN SLM-VARI
               GO TO FIN-CAR-FLD-SLM.

           IF (WSS-J IN SLM-VARI < WSS-IND-VEC IN WSS-VARI)
                NEXT SENTENCE
           ELSE
               GO TO FIN-CAR-FLD-SLM.

           PERFORM CLR-SLM-VEC.
           MOVE WSS-NUM-SOLI(WSS-J)  TO FRM-NUM-SOLI-V(WSS-J).
           MOVE WSS-COD-DOCU(WSS-J)  TO FRM-COD-DOCU-V(WSS-J).
           MOVE WSS-CAI-ISOL(WSS-J)  TO FRM-CAI-ISOL-V(WSS-J).
           MOVE WSS-IIC-ISOL(WSS-J)  TO FRM-IIC-ISOL-V(WSS-J).
           MOVE WSS-CAI-IOPC(WSS-J)  TO FRM-CAI-IOPC-V(WSS-J).
           MOVE WSS-IIC-IOPC(WSS-J)  TO FRM-IIC-IOPC-V(WSS-J).
           MOVE WSS-NUM-ICLI(WSS-J)  TO FRM-NUM-ICLI-V(WSS-J).
           MOVE WSS-DIG-ICLI(WSS-J)  TO FRM-DIG-ICLI-V(WSS-J).
           MOVE WSS-COD-ESTA(WSS-J)  TO FRM-COD-ESTA-V(WSS-J).
           MOVE WSS-FEC-FFE1(WSS-J)  TO FRM-FEC-FFE1-V(WSS-J).

           GO TO LOOP-CARGA.

       FIN-CAR-FLD-SLM.
           EXIT.

       CLR-SLM-VEC SECTION.
       INI-CLR-SLM-VEC.
           MOVE ZEROES TO FRM-NUM-SOLI-V(WSS-J).
           MOVE SPACES TO FRM-COD-DOCU-V(WSS-J).
           MOVE SPACES TO FRM-CAI-ISOL-V(WSS-J).
           MOVE SPACES TO FRM-IIC-ISOL-V(WSS-J).
           MOVE SPACES TO FRM-CAI-IOPC-V(WSS-J).
           MOVE SPACES TO FRM-IIC-IOPC-V(WSS-J).
           MOVE ZEROES TO FRM-NUM-ICLI-V(WSS-J).
           MOVE SPACES TO FRM-DIG-ICLI-V(WSS-J).
           MOVE SPACES TO FRM-COD-ESTA-V(WSS-J).
           MOVE SPACES TO FRM-FEC-FFE1-V(WSS-J).
       FIN-CLR-SLM-VEC.
           EXIT.

       PUT-TAB SECTION.
       INI-PUT-TAB.
           MOVE SLM-NOM-TSSLM     TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE SLM-NOM-TSSLM-MAS TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE 1 TO SCR-NUM-PANT.
           MOVE SPACES TO SLM-VEC-TS.
           MOVE LOW-VALUES TO WSS-NUM-NCRE-IDX.
      *
           MOVE 1 TO FRM-IFLD.
           MOVE ZEROES  TO WSS-NITM IN SLM-VARI.

           MOVE 1 TO WSS-F IN SLM-VARI.

      *    MOVE FRM-IND-HMAS IN SLM-FLD TO WSS-IND-HMAS IN SLM-VARI.

       LUP-PUT-TAB.
           IF WSS-F IN SLM-VARI > WSS-FMAX IN SLM-VARI
              GO TO FIN-PUT-TAB.

           IF WSS-IND-VEC IN WSS-VARI NOT > WSS-GLS-UNO IN WSS-VARI
               MOVE 'REGNEX    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE '00' TO FIO-STAT
               GO TO FIN-PUT-TAB.

           IF WSS-F = WSS-IND-VEC IN WSS-VARI
               MOVE '00' TO FIO-STAT
               GO TO FIN-PUT-TAB.

           IF WSS-COD-DOCU(WSS-F) > SPACES
               PERFORM PUT-TAB-SLM
               PERFORM PUT-SLM-QUE
               PERFORM SET-FRM-IFLD
           ELSE
               IF FRM-IFLD = 1 AND WSS-F = 1
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE '00' TO FIO-STAT
                   GO TO FIN-PUT-TAB
               ELSE
                   MOVE '00' TO FIO-STAT
                   GO TO FIN-PUT-TAB.

           ADD 1 TO WSS-F IN SLM-VARI.
           GO TO LUP-PUT-TAB.

       FIN-PUT-TAB.
           MOVE 1                  TO QUE-NITM.
           MOVE SLM-LEN-TSSLM-MAS  TO QUE-LITM.
           MOVE SLM-NOM-TSSLM-MAS  TO QUE-COLA.

           IF WSS-NITM > WSS-NITMMAX
              MOVE 'S'           TO SLM-MAS-IND-HMAS
           ELSE
              MOVE 'N'           TO SLM-MAS-IND-HMAS.

           IF WSS-NITM > WSS-NITMMAX
               MOVE WSS-NITMMAX TO WSS-NUM-ICUR
           ELSE
               MOVE WSS-NITM TO WSS-NUM-ICUR.

           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.
           MOVE WSS-NUM-ICUR-ALF TO SLM-MAS-NUM-TOCC.
           MOVE SLM-MAS-ITEM     TO QUE-ITEM.
           MOVE QUE-PUT          TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF WSS-NITM > WSS-IMAX
               MOVE 'TAB' TO MSG-COD-SIST
               MOVE 'SLMTABDMAS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       EXT-PUT-TAB.
           EXIT.
      *----------------------------------------------------------------
       PUT-TAB-SLM SECTION.
       INI-PUT-TAB-SLM.
           PERFORM CLR-WSS.

           MOVE FRM-NUM-SOLI-ALF-V(WSS-F)
                      TO FRM-NUM-SOLI-ALF-SLM IN SLM-VARI.
           MOVE FRM-COD-DOCU-V(WSS-F)
                      TO FRM-COD-DOCU-SLM     IN SLM-VARI.
           MOVE FRM-CAI-ISOL-V(WSS-F)
                      TO FRM-CAI-ISOL-SLM     IN SLM-VARI.
           MOVE FRM-IIC-ISOL-V(WSS-F)
                      TO FRM-IIC-ISOL-SLM     IN SLM-VARI.
           MOVE FRM-CAI-IOPC-V(WSS-F)
                      TO FRM-CAI-IOPC-SLM     IN SLM-VARI.
           MOVE FRM-IIC-IOPC-V(WSS-F)
                      TO FRM-IIC-IOPC-SLM     IN SLM-VARI.
           MOVE FRM-NUM-ICLI-ALF-V(WSS-F)
                      TO FRM-NUM-ICLI-ALF-SLM IN SLM-VARI.
           MOVE FRM-DIG-ICLI-V(WSS-F)
                      TO FRM-DIG-ICLI-SLM     IN SLM-VARI.
           MOVE FRM-COD-ESTA-V(WSS-F)
                      TO FRM-COD-ESTA-SLM     IN SLM-VARI.
           MOVE FRM-FEC-FFE1-V(WSS-F)
                      TO FRM-FEC-FFE1-SLM     IN SLM-VARI.

           MOVE FRM-NUM-SOLI-ALF-SLM TO FRM-NUM-SOLI-ALF-FRM.
           MOVE FRM-COD-DOCU-SLM     TO FRM-COD-DOCU-FRM.
           MOVE FRM-CAI-ISOL-SLM     TO FRM-CAI-ISOL-FRM.
           MOVE FRM-IIC-ISOL-SLM     TO FRM-IIC-ISOL-FRM.
           MOVE FRM-CAI-IOPC-SLM     TO FRM-CAI-IOPC-FRM.
           MOVE FRM-IIC-IOPC-SLM     TO FRM-IIC-IOPC-FRM.
           MOVE FRM-NUM-ICLI-ALF-SLM TO FRM-NUM-ICLI-ALF-FRM.
           MOVE FRM-DIG-ICLI-SLM     TO FRM-DIG-ICLI-FRM.
           MOVE FRM-COD-ESTA-SLM     TO FRM-COD-ESTA-FRM.
           MOVE FRM-FEC-FFE1-SLM     TO FRM-FEC-FFE1-FRM.

           IF FRM-IFLD > WSS-IMAX
              GO TO FIN-PUT-TAB-SLM.
           MOVE FRM-NUM-SOLI-ALF-V(WSS-F)
                      TO FRM-NUM-SOLI-ALF IN SLM-FLD(FRM-IFLD).
           MOVE FRM-COD-DOCU-V(WSS-F)
                      TO FRM-COD-DOCU     IN SLM-FLD(FRM-IFLD).
           MOVE FRM-CAI-ISOL-V(WSS-F)
                      TO FRM-CAI-ISOL     IN SLM-FLD(FRM-IFLD).
           MOVE FRM-IIC-ISOL-V(WSS-F)
                      TO FRM-IIC-ISOL     IN SLM-FLD(FRM-IFLD).
           MOVE FRM-CAI-IOPC-V(WSS-F)
                      TO FRM-CAI-IOPC     IN SLM-FLD(FRM-IFLD).
           MOVE FRM-IIC-IOPC-V(WSS-F)
                      TO FRM-IIC-IOPC     IN SLM-FLD(FRM-IFLD).
           MOVE FRM-NUM-ICLI-ALF-V(WSS-F)
                      TO FRM-NUM-ICLI-ALF IN SLM-FLD(FRM-IFLD).
           MOVE FRM-DIG-ICLI-V(WSS-F)
                      TO FRM-DIG-ICLI     IN SLM-FLD(FRM-IFLD).
           MOVE FRM-COD-ESTA-V(WSS-F)
                      TO FRM-COD-ESTA     IN SLM-FLD(FRM-IFLD).
           MOVE FRM-FEC-FFE1-V(WSS-F)
                      TO FRM-FEC-FFE1     IN SLM-FLD(FRM-IFLD).

       FIN-PUT-TAB-SLM.
           EXIT.
      *----------------------------------------------------------------
       CLR-WSS SECTION.
       INI-CLR-WSS.

           MOVE ZEROES TO FRM-NUM-SOLI-ALF-SLM IN SLM-VARI.
           MOVE SPACES TO FRM-COD-DOCU-SLM     IN SLM-VARI.
           MOVE SPACES TO FRM-CAI-ISOL-SLM     IN SLM-VARI.
           MOVE SPACES TO FRM-IIC-ISOL-SLM     IN SLM-VARI.
           MOVE SPACES TO FRM-CAI-IOPC-SLM     IN SLM-VARI.
           MOVE SPACES TO FRM-IIC-IOPC-SLM     IN SLM-VARI.
           MOVE ZEROES TO FRM-NUM-ICLI-ALF-SLM IN SLM-VARI.
           MOVE SPACES TO FRM-DIG-ICLI-SLM     IN SLM-VARI.
           MOVE SPACES TO FRM-COD-ESTA-SLM     IN SLM-VARI.
           MOVE SPACES TO FRM-FEC-FFE1-SLM     IN SLM-VARI.

           IF SCR-OFRM-SRV
               PERFORM CLR-FRM-MTX.

       FIN-CLR-WSS.
           EXIT.
      *-----------------------------------------------------------------
       CLR-FRM-MTX SECTION.
       INI-CLR-FRM-MTX.

           MOVE ZEROES TO FRM-NUM-SOLI-ALF IN SLM-FLD(1).
           MOVE SPACES TO FRM-COD-DOCU     IN SLM-FLD(1).
           MOVE SPACES TO FRM-CAI-ISOL     IN SLM-FLD(1).
           MOVE SPACES TO FRM-IIC-ISOL     IN SLM-FLD(1).
           MOVE SPACES TO FRM-CAI-IOPC     IN SLM-FLD(1).
           MOVE SPACES TO FRM-IIC-IOPC     IN SLM-FLD(1).
           MOVE ZEROES TO FRM-NUM-ICLI-ALF IN SLM-FLD(1).
           MOVE SPACES TO FRM-DIG-ICLI     IN SLM-FLD(1).
           MOVE SPACES TO FRM-COD-ESTA     IN SLM-FLD(1).
           MOVE SPACES TO FRM-FEC-FFE1     IN SLM-FLD(1).

       FIN-CLR-FRM-MTX.
           EXIT.
      *----------------------------------------------------------------
       PUT-SLM-QUE SECTION.
       INI-PUT-SLM-QUE.
           PERFORM PRO-FLD-QUE.
           ADD 1 TO WSS-NITM.
           MOVE SLM-NOM-TSSLM    TO QUE-COLA.
           MOVE SLM-LEN-TSSLM    TO QUE-LITM.
           MOVE WSS-NITM         TO QUE-NITM.
           MOVE SPACES           TO QUE-ITEM.
           IF SCR-OFRM-SRV
               IF NOT (WSS-NITM > WSS-NITMMAX)

                   MOVE FRM-NUM-SOLI-V(WSS-F)
                           TO FRM-NUM-SOLI-EDT
                   MOVE FRM-NUM-SOLI-EDT IN SLM-EDT
                           TO FRM-NUM-SOLI-ALF-SLM IN SLM-VARI
                   MOVE FRM-NUM-ICLI-V(WSS-F)
                           TO FRM-NUM-ICLI-EDT
                   MOVE FRM-NUM-ICLI-EDT IN SLM-EDT
                           TO FRM-NUM-ICLI-ALF-SLM IN SLM-VARI
                   MOVE SLM-VEC-TS    TO QUE-ITEM
               ELSE
                   GO TO FIN-PUT-SLM-QUE
           ELSE
               MOVE WSS-NUM-NCRE-IDX TO QUE-ITEM.

           MOVE QUE-PUT  TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

       FIN-PUT-SLM-QUE.
           EXIT.
      *----------------------------------------------------------------
       PRO-FLD-QUE SECTION.
       INI-PRO-FLD-QUE.

           MOVE 'P' TO FRM-NUM-SOLI-ATR-SLM.
           MOVE 'P' TO FRM-COD-DOCU-ATR-SLM.
           MOVE 'P' TO FRM-CAI-ISOL-ATR-SLM.
           MOVE 'P' TO FRM-IIC-ISOL-ATR-SLM.
           MOVE 'P' TO FRM-CAI-IOPC-ATR-SLM.
           MOVE 'P' TO FRM-IIC-IOPC-ATR-SLM.
           MOVE 'P' TO FRM-NUM-ICLI-ATR-SLM.
           MOVE 'P' TO FRM-DIG-ICLI-ATR-SLM.
           MOVE 'P' TO FRM-COD-ESTA-ATR-SLM.
           MOVE 'P' TO FRM-FEC-FFE1-ATR-SLM.

       FIN-PRO-FLD-QUE.
           EXIT.
      *----------------------------------------------------------------
      *PUT-VEN-SOL SECTION.
      *INI-PUT-VEN-SOL.
      *
      *    IF (WSS-F IN SLM-VARI >= FRM-NUM-IVEN IN LOG-FLD)
      *        IF WSS-IND-HMAS IN SLM-VARI = '+' AND
      *           WSS-NUM-NCRE-V(WSS-F) > ZEROES
      *            PERFORM PUT-TAB-SLM
      *            PERFORM PUT-SLM-QUE
      *            PERFORM SET-FRM-IFLD
      *        ELSE
      *            IF WSS-IND-HMAS IN SLM-VARI = '-' AND
      *               FRM-NUM-IVEN IN SLM-FLD > 1
      *                SUBTRACT 1 FROM WSS-F IN SLM-VARI
      *                MOVE '+' TO WSS-IND-HMAS IN SLM-VARI
      *                IF WSS-NUM-NCRE-V(WSS-F) > ZEROES
      *                    PERFORM PUT-TAB-SLM
      *                    PERFORM PUT-SLM-QUE
      *                    PERFORM SET-FRM-IFLD.
      *
      *    ADD 1 TO WSS-F IN SLM-VARI.
      *
      *FIN-PUT-VEN-SOL.
      *    EXIT.
      *----------------------------------------------------------------
       SET-FRM-IFLD SECTION.
       INI-SET-FRM-IFLD.
           IF SCR-OFRM-SRV
                MOVE 1 TO FRM-IFLD
           ELSE
                ADD 1 TO FRM-IFLD.
       FIN-SET-FRM-IFLD.
           EXIT.
      *----------------------------------------------------------------
       PUT-TAB-ASC SECTION.
       INI-PUT-TAB-ASC.
           MOVE SPACES TO MSG-COD-MENS.
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.
           MOVE 0 TO FRM-IFLD.
       LUP-PUT-TAB-ASC.
           ADD 1              TO QUE-NITM.
           MOVE SLM-NOM-TSSLM TO QUE-COLA.
           MOVE SLM-LEN-TSSLM TO QUE-LITM.
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD = 0
                   MOVE 'SLMTABNONXT' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-ASC
               ELSE
                   GO TO FIN-PUT-TAB-ASC.
           ADD 1 TO FRM-IFLD.
           MOVE QUE-ITEM TO FRM-NUM-SOLI-IDX(FRM-IFLD).
           IF FRM-IFLD < WSS-IMAX
               GO TO LUP-PUT-TAB-ASC.
       FIN-PUT-TAB-ASC.
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX
               ADD 1 FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1
                                 UNTIL WSS-I > WSS-IMAX.
           IF FRM-IFLD > 0
               ADD 1 TO SCR-NUM-PANT.
           IF FRM-IFLD = WSS-IMAX
               ADD 1              TO QUE-NITM
               MOVE SLM-NOM-TSSLM TO QUE-COLA
               MOVE SLM-LEN-TSSLM TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT = QUE-STAT-OKS
                   MOVE 'TAB'        TO MSG-COD-SIST
                   MOVE 'SLMTABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-ASC.
           EXIT.
      *---------------------------------------------------------------
       CLR-FRM SECTION.
       INI-CLR-FRM.

           MOVE ZEROES TO FRM-NUM-SOLI IN SLM-FLD(WSS-I).
           MOVE SPACES TO FRM-COD-DOCU IN SLM-FLD(WSS-I).
           MOVE SPACES TO FRM-CAI-ISOL IN SLM-FLD(WSS-I).
           MOVE SPACES TO FRM-IIC-ISOL IN SLM-FLD(WSS-I).
           MOVE SPACES TO FRM-CAI-IOPC IN SLM-FLD(WSS-I).
           MOVE SPACES TO FRM-IIC-IOPC IN SLM-FLD(WSS-I).
           MOVE ZEROES TO FRM-NUM-ICLI IN SLM-FLD(WSS-I).
           MOVE SPACES TO FRM-DIG-ICLI IN SLM-FLD(WSS-I).
           MOVE SPACES TO FRM-COD-ESTA IN SLM-FLD(WSS-I).
           MOVE SPACES TO FRM-FEC-FFE1 IN SLM-FLD(WSS-I).

       FIN-CLR-FRM.
           EXIT.
      *----------------------------------------------------------------
       PUT-TAB-DSC SECTION.
       INI-PUT-TAB-DSC.
           MOVE SPACES TO MSG-COD-MENS.
           IF SCR-NUM-PANT = 1
               MOVE 'SLMTABNOPRV' TO MSG-COD-MENS
               GO TO EXT-PUT-TAB-DSC.

           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.
           ADD 1 TO QUE-NITM.
           ADD 1 WSS-IMAX GIVING FRM-IFLD.
       LUP-PUT-TAB-DSC.
           SUBTRACT  1 FROM QUE-NITM.
           MOVE SLM-NOM-TSSLM TO QUE-COLA
           MOVE SLM-LEN-TSSLM TO QUE-LITM
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD > WSS-IMAX
                   MOVE 'SLMTABNOPRV' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-DSC
               ELSE
                   GO TO FIN-PUT-TAB-DSC.
           SUBTRACT 1 FROM FRM-IFLD.
           MOVE QUE-ITEM TO FRM-NUM-SOLI-IDX(FRM-IFLD).
           IF FRM-IFLD > 1
               GO TO LUP-PUT-TAB-DSC.
       FIN-PUT-TAB-DSC.

           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1
                                 UNTIL WSS-I < 1.

           IF FRM-IFLD NOT > WSS-IMAX
               SUBTRACT 1 FROM SCR-NUM-PANT
               MOVE 'TAB'        TO MSG-COD-SIST
               MOVE 'SLMTABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-DSC.
           EXIT.
      *-----------------------------------------------------------------
       SCRL-TAB-SLM SECTION.
       INI-SCRL-TAB-SLM.
           MOVE SPACES TO MSG-COD-MENS.

           IF FRM-FFLD = FRM-FFLD-PF6
               PERFORM PUT-TAB-ASC
           ELSE
               PERFORM PUT-TAB-DSC.
           IF MSG-COD-MENS > SPACES
               MOVE 'TAB' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-SLM.
           EXIT.
      *>>>>
