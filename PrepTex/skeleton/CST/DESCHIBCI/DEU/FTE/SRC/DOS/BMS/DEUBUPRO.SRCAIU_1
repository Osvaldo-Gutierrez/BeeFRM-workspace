      *
      * DEUBUPRO
      * Version 3.0.1 Nov-2005
      * 22-Jan-10 11:27 AM
      *
       SET-TAG-DEUBUPRO SECTION.
       INI-SET-TAG-DEUBUPRO.
           MOVE
           '<IDNMOD NOM=$DEUBUPRO$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$22-Jan-10 11:27 AM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-DEUBUPRO.
           EXIT.
           
       RET-ALL-PRO SECTION.
       INI-RET-ALL-PRO.
           PERFORM KEY-ALL-PRO.
           PERFORM RET-PRO-PRO.
       FIN-RET-ALL-PRO.
           EXIT.
       
       RET-PRO-PRO SECTION.
       INI-RET-PRO-PRO.
           MOVE PRO-SGV-MDES IN PRO-FLD TO PRO-SGV-MDES IN PRO.
           MOVE PRO-SGV-PCAR IN PRO-FLD TO PRO-SGV-PCAR IN PRO.
           MOVE PRO-SGV-CONF IN PRO-FLD TO PRO-SGV-CONF IN PRO.
           MOVE PRO-SGV-CAPI IN PRO-FLD TO PRO-SGV-CAPI IN PRO.
       FIN-RET-PRO-PRO.
           EXIT.
       
       KEY-ALL-PRO SECTION.
       INI-KEY-ALL-PRO.
           PERFORM KEY-PRO-PRO.
       FIN-KEY-ALL-PRO.
           EXIT.
       
       KEY-PRO-PRO SECTION.
       INI-KEY-PRO-PRO.
           MOVE PRO-CAI-ICAR IN PRO-FLD TO PRO-CAI-ICAR IN PRO.
           MOVE PRO-IIC-ICAR IN PRO-FLD TO PRO-IIC-ICAR IN PRO.
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE PRO-NUM-DPRO IN PRO-FLD TO PRO-NUM-DPRO IN PRO.
           MOVE PRO-NUM-MPRO IN PRO-FLD TO PRO-NUM-MPRO IN PRO.
           MOVE PRO-NUM-SPRO IN PRO-FLD TO PRO-NUM-SPRO IN PRO.
           MOVE PRO-NUM-APRO IN PRO-FLD TO PRO-NUM-APRO IN PRO.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
       FIN-KEY-PRO-PRO.
           EXIT.
       
       SCR-CLRA-PRO SECTION.
       INI-SCR-CLRA-PRO.
           PERFORM CLR-KEY-PRO.
           PERFORM CLR-FLD-PRO.
       FIN-SCR-CLRA-PRO.
           EXIT.
       
       CLR-KEY-PRO SECTION.
       INI-CLR-KEY-PRO.
           MOVE SPACES TO PRO-CAI-ICAR IN PRO-FLD.
           MOVE SPACES TO PRO-IIC-ICAR IN PRO-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO PRO-FEC-FPRO IN PRO-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
       FIN-CLR-KEY-PRO.
           EXIT.
       
       CLR-FLD-PRO SECTION.
       INI-CLR-FLD-PRO.
           PERFORM CLR-PRO-PRO.
       FIN-CLR-FLD-PRO.
           EXIT.
       
       CLR-PRO-PRO SECTION.
       INI-CLR-PRO-PRO.
           MOVE ZEROES TO PRO-SGV-MDES IN PRO-FLD.
           MOVE ZEROES TO PRO-SGV-PCAR IN PRO-FLD.
           MOVE ZEROES TO PRO-SGV-CONF IN PRO-FLD.
           MOVE ZEROES TO PRO-SGV-CAPI IN PRO-FLD.
       FIN-CLR-PRO-PRO.
           EXIT.
       
       FST-KEY-PRO SECTION.
       INI-FST-KEY-PRO.
           MOVE -1 TO PRO-CAI-ICAR-LEN IN PRO-FLD.
       FIN-FST-KEY-PRO.
           EXIT.
       
       FST-FLD-PRO SECTION.
       INI-FST-FLD-PRO.
           MOVE -1 TO PRO-SGV-MDES-LEN IN PRO-FLD.
       FIN-FST-FLD-PRO.
           EXIT.
       
       PRO-KEY-PRO SECTION.
       INI-PRO-KEY-PRO.
           PERFORM PRO-MKY-PRO.
       FIN-PRO-KEY-PRO.
           EXIT.
       
       PRO-MKY-PRO SECTION.
       INI-PRO-MKY-PRO.
       FIN-PRO-MKY-PRO.
           EXIT.
       
       PRO-FLD-PRO SECTION.
       INI-PRO-FLD-PRO.
           PERFORM PRO-PRO-PRO.
       FIN-PRO-FLD-PRO.
           EXIT.
       
       PRO-PRO-PRO SECTION.
       INI-PRO-PRO-PRO.
           MOVE FRM-CPIM-AEY TO PRO-SGV-MDES-ATR IN PRO-FLD.
           MOVE FRM-CPIM-AEY TO PRO-SGV-PCAR-ATR IN PRO-FLD.
           MOVE FRM-CPIM-AEY TO PRO-SGV-CONF-ATR IN PRO-FLD.
           MOVE FRM-CPIM-AEY TO PRO-SGV-CAPI-ATR IN PRO-FLD.
       FIN-PRO-PRO-PRO.
           EXIT.
       
       UNP-KEY-PRO SECTION.
       INI-UNP-KEY-PRO.
           PERFORM UNP-IKY-PRO.
           PERFORM UNP-AKY-PRO.
           PERFORM UNP-MKY-PRO.
       FIN-UNP-KEY-PRO.
           EXIT.
       
       UNP-IKY-PRO SECTION.
       INI-UNP-IKY-PRO.
           MOVE FRM-CPIM-UBY TO PRO-CAI-ICAR-ATR.
           MOVE FRM-CPIM-UBY TO PRO-IIC-ICAR-ATR.
           MOVE FRM-CPIM-UBY TO PRO-FEC-FPRO-ATR IN PRO-FLD.
       FIN-UNP-IKY-PRO.
           EXIT.
       
       UNP-MKY-PRO SECTION.
       INI-UNP-MKY-PRO.
       FIN-UNP-MKY-PRO.
           EXIT.
       
       UNP-AKY-PRO SECTION.
       INI-UNP-AKY-PRO.
       FIN-UNP-AKY-PRO.
           EXIT.
       
       UNP-FLD-PRO SECTION.
       INI-UNP-FLD-PRO.
           PERFORM UNP-PRO-PRO.
       FIN-UNP-FLD-PRO.
           EXIT.
       
       UNP-PRO-PRO SECTION.
       INI-UNP-PRO-PRO.
           MOVE FRM-CPIM-UBY TO PRO-SGV-MDES-ATR IN PRO-FLD.
           MOVE FRM-CPIM-UBY TO PRO-SGV-PCAR-ATR IN PRO-FLD.
           MOVE FRM-CPIM-UBY TO PRO-SGV-CONF-ATR IN PRO-FLD.
           MOVE FRM-CPIM-UBY TO PRO-SGV-CAPI-ATR IN PRO-FLD.
       FIN-UNP-PRO-PRO.
           EXIT.
       
       VAL-MKY-PRO SECTION.
       INI-VAL-MKY-PRO.
       FIN-VAL-MKY-PRO.
           EXIT.
       
       VAL-FLD-PRO SECTION.
       INI-VAL-FLD-PRO.
           PERFORM VAL-PRO-SGV-MDES.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PRO-SGV-MDES-LEN IN PRO-FLD
                GO TO FIN-VAL-FLD-PRO.
           PERFORM VAL-PRO-SGV-PCAR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PRO-SGV-PCAR-LEN IN PRO-FLD
                GO TO FIN-VAL-FLD-PRO.
           PERFORM VAL-PRO-SGV-CONF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PRO-SGV-CONF-LEN IN PRO-FLD
                GO TO FIN-VAL-FLD-PRO.
           PERFORM VAL-PRO-SGV-CAPI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PRO-SGV-CAPI-LEN IN PRO-FLD
                GO TO FIN-VAL-FLD-PRO.
       FIN-VAL-FLD-PRO.
           EXIT.
       
       VAL-PRO-SGV-MDES SECTION.
       INI-VAL-PRO-SGV-MDES.
       FIN-VAL-PRO-SGV-MDES.
           EXIT.
       
       VAL-PRO-SGV-PCAR SECTION.
       INI-VAL-PRO-SGV-PCAR.
       FIN-VAL-PRO-SGV-PCAR.
           EXIT.
       
       VAL-PRO-SGV-CONF SECTION.
       INI-VAL-PRO-SGV-CONF.
       FIN-VAL-PRO-SGV-CONF.
           EXIT.
       
       VAL-PRO-SGV-CAPI SECTION.
       INI-VAL-PRO-SGV-CAPI.
       FIN-VAL-PRO-SGV-CAPI.
           EXIT.
       
       VAL-CON-FLD-PRO SECTION.
       INI-VAL-CON-FLD-PRO.
       FIN-VAL-CON-FLD-PRO.
           EXIT.
       
       VAL-CON-PRO SECTION.
       INI-VAL-CON-PRO.
       FIN-VAL-CON-PRO.
           EXIT.
       
       VAL-NUM-KEY-PRO SECTION.
       INI-VAL-NUM-KEY-PRO.
           PERFORM VAL-NUM-IKY-PRO.
           PERFORM VAL-NUM-MKY-PRO.
           PERFORM VAL-NUM-AKY-PRO.
       FIN-VAL-NUM-KEY-PRO.
           EXIT.
       
       VAL-NUM-IKY-PRO SECTION.
       INI-VAL-NUM-IKY-PRO.
       FIN-VAL-NUM-IKY-PRO.
           EXIT.
       
       VAL-NUM-MKY-PRO SECTION.
       INI-VAL-NUM-MKY-PRO.
       FIN-VAL-NUM-MKY-PRO.
           EXIT.
       
       VAL-NUM-AKY-PRO SECTION.
       INI-VAL-NUM-AKY-PRO.
       FIN-VAL-NUM-AKY-PRO.
           EXIT.
       
       VAL-NUM-FLD-PRO SECTION.
       INI-VAL-NUM-FLD-PRO.
       VAL-NUM-PRO-SGV-MDES.
           MOVE PRO-SGV-MDES-ALF IN PRO-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 5 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NS9V TO PRO-SGV-MDES IN PRO-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO PRO-SGV-MDES-LEN IN PRO-FLD
                GO TO FIN-VAL-NUM-FLD-PRO.
       VAL-NUM-PRO-SGV-PCAR.
           MOVE PRO-SGV-PCAR-ALF IN PRO-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 5 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NS9V TO PRO-SGV-PCAR IN PRO-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO PRO-SGV-PCAR-LEN IN PRO-FLD
                GO TO FIN-VAL-NUM-FLD-PRO.
       VAL-NUM-PRO-SGV-CONF.
           MOVE PRO-SGV-CONF-ALF IN PRO-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 12 TO NUM-NENT.
           MOVE 5 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NS9V TO PRO-SGV-CONF IN PRO-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO PRO-SGV-CONF-LEN IN PRO-FLD
                GO TO FIN-VAL-NUM-FLD-PRO.
       VAL-NUM-PRO-SGV-CAPI.
           MOVE PRO-SGV-CAPI-ALF IN PRO-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 5 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NS9V TO PRO-SGV-CAPI IN PRO-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO PRO-SGV-CAPI-LEN IN PRO-FLD
                GO TO FIN-VAL-NUM-FLD-PRO.
       FIN-VAL-NUM-FLD-PRO.
           EXIT.
       
       EDT-KEY-PRO SECTION.
       INI-EDT-KEY-PRO.
           PERFORM EDT-IKY-PRO.
           PERFORM EDT-MKY-PRO.
           PERFORM EDT-AKY-PRO.
       FIN-EDT-KEY-PRO.
           EXIT.
       
       EDT-IKY-PRO SECTION.
       INI-EDT-IKY-PRO.
       FIN-EDT-IKY-PRO.
           EXIT.
       
       EDT-MKY-PRO SECTION.
       INI-EDT-MKY-PRO.
       FIN-EDT-MKY-PRO.
           EXIT.
       
       EDT-AKY-PRO SECTION.
       INI-EDT-AKY-PRO.
       FIN-EDT-AKY-PRO.
           EXIT.
       
       EDT-FLD-PRO SECTION.
       INI-EDT-FLD-PRO.
           IF PRO-SGV-MDES-LEN IN PRO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PRO-SGV-MDES-LEN IN PRO-FLD
                GO TO FIN-EDT-FLD-PRO.
           MOVE PRO-SGV-MDES IN PRO-FLD TO PRO-SGV-MDES-EDT IN PRO-EDT.
           MOVE PRO-SGV-MDES-EDT IN PRO-EDT TO 
                PRO-SGV-MDES-ALF IN PRO-FLD.
           IF PRO-SGV-PCAR-LEN IN PRO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PRO-SGV-PCAR-LEN IN PRO-FLD
                GO TO FIN-EDT-FLD-PRO.
           MOVE PRO-SGV-PCAR IN PRO-FLD TO PRO-SGV-PCAR-EDT IN PRO-EDT.
           MOVE PRO-SGV-PCAR-EDT IN PRO-EDT TO 
                PRO-SGV-PCAR-ALF IN PRO-FLD.
           IF PRO-SGV-CONF-LEN IN PRO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PRO-SGV-CONF-LEN IN PRO-FLD
                GO TO FIN-EDT-FLD-PRO.
           MOVE PRO-SGV-CONF IN PRO-FLD TO PRO-SGV-CONF-EDT IN PRO-EDT.
           MOVE PRO-SGV-CONF-EDT IN PRO-EDT TO 
                PRO-SGV-CONF-ALF IN PRO-FLD.
           IF PRO-SGV-CAPI-LEN IN PRO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PRO-SGV-CAPI-LEN IN PRO-FLD
                GO TO FIN-EDT-FLD-PRO.
           MOVE PRO-SGV-CAPI IN PRO-FLD TO PRO-SGV-CAPI-EDT IN PRO-EDT.
           MOVE PRO-SGV-CAPI-EDT IN PRO-EDT TO 
                PRO-SGV-CAPI-ALF IN PRO-FLD.
       FIN-EDT-FLD-PRO.
           EXIT.
