      *
      * DEUBUMAT
      * Version 3.0.1 Nov-2005
      * 22-Jan-10 09:51 AM
      *
       SET-TAG-DEUBUMAT SECTION.
       INI-SET-TAG-DEUBUMAT.
           MOVE
           '<IDNMOD NOM=$DEUBUMAT$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$22-Jan-10 09:51 AM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-DEUBUMAT.
           EXIT.
           
       RET-ALL-MAT SECTION.
       INI-RET-ALL-MAT.
           PERFORM KEY-ALL-MAT.
           PERFORM RET-MAT-MAT.
       FIN-RET-ALL-MAT.
           EXIT.
       
       RET-MAT-MAT SECTION.
       INI-RET-MAT-MAT.
           MOVE MAT-COD-NDEF IN MAT-FLD TO MAT-COD-NDEF IN MAT.
           MOVE MAT-COD-CLAU IN MAT-FLD TO MAT-COD-CLAU IN MAT.
           MOVE MAT-SGV-MVPI IN MAT-FLD TO MAT-SGV-MVPI IN MAT.
       FIN-RET-MAT-MAT.
           EXIT.
       
       KEY-ALL-MAT SECTION.
       INI-KEY-ALL-MAT.
           PERFORM KEY-MAT-MAT.
       FIN-KEY-ALL-MAT.
           EXIT.
       
       KEY-MAT-MAT SECTION.
       INI-KEY-MAT-MAT.
           MOVE MAT-COD-ICAR IN MAT-FLD TO MAT-COD-ICAR IN MAT.
           MOVE MAT-CIC-ITIP IN MAT-FLD TO MAT-CIC-ITIP IN MAT.
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE MAT-NUM-DPRO IN MAT-FLD TO MAT-NUM-DPRO IN MAT.
           MOVE MAT-NUM-MPRO IN MAT-FLD TO MAT-NUM-MPRO IN MAT.
           MOVE MAT-NUM-SPRO IN MAT-FLD TO MAT-NUM-SPRO IN MAT.
           MOVE MAT-NUM-APRO IN MAT-FLD TO MAT-NUM-APRO IN MAT.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE MAT-NUM-IMAT IN MAT-FLD TO MAT-NUM-IMAT IN MAT.
       FIN-KEY-MAT-MAT.
           EXIT.
       
       SCR-CLRA-MAT SECTION.
       INI-SCR-CLRA-MAT.
           PERFORM CLR-KEY-MAT.
           PERFORM CLR-FLD-MAT.
       FIN-SCR-CLRA-MAT.
           EXIT.
       
       CLR-KEY-MAT SECTION.
       INI-CLR-KEY-MAT.
           MOVE SPACES TO MAT-COD-ICAR IN MAT-FLD.
           MOVE SPACES TO MAT-CIC-ITIP IN MAT-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO MAT-FEC-FPRO IN MAT-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO MAT-NUM-IMAT IN MAT-FLD.
       FIN-CLR-KEY-MAT.
           EXIT.
       
       CLR-FLD-MAT SECTION.
       INI-CLR-FLD-MAT.
           PERFORM CLR-MAT-MAT.
       FIN-CLR-FLD-MAT.
           EXIT.
       
       CLR-MAT-MAT SECTION.
       INI-CLR-MAT-MAT.
           MOVE SPACES TO MAT-COD-NDEF IN MAT-FLD.
           MOVE SPACES TO MAT-COD-CLAU IN MAT-FLD.
           MOVE ZEROES TO MAT-SGV-MVPI IN MAT-FLD.
       FIN-CLR-MAT-MAT.
           EXIT.
       
       FST-KEY-MAT SECTION.
       INI-FST-KEY-MAT.
           MOVE -1 TO MAT-COD-ICAR-LEN IN MAT-FLD.
       FIN-FST-KEY-MAT.
           EXIT.
       
       FST-FLD-MAT SECTION.
       INI-FST-FLD-MAT.
           MOVE -1 TO MAT-COD-NDEF-LEN IN MAT-FLD.
       FIN-FST-FLD-MAT.
           EXIT.
       
       PRO-KEY-MAT SECTION.
       INI-PRO-KEY-MAT.
           PERFORM PRO-MKY-MAT.
       FIN-PRO-KEY-MAT.
           EXIT.
       
       PRO-MKY-MAT SECTION.
       INI-PRO-MKY-MAT.
       FIN-PRO-MKY-MAT.
           EXIT.
       
       PRO-FLD-MAT SECTION.
       INI-PRO-FLD-MAT.
           PERFORM PRO-MAT-MAT.
       FIN-PRO-FLD-MAT.
           EXIT.
       
       PRO-MAT-MAT SECTION.
       INI-PRO-MAT-MAT.
           MOVE FRM-CPIM-AEY TO MAT-COD-NDEF-ATR IN MAT-FLD.
           MOVE FRM-CPIM-AEY TO MAT-COD-CLAU-ATR IN MAT-FLD.
           MOVE FRM-CPIM-AEY TO MAT-SGV-MVPI-ATR IN MAT-FLD.
       FIN-PRO-MAT-MAT.
           EXIT.
       
       UNP-KEY-MAT SECTION.
       INI-UNP-KEY-MAT.
           PERFORM UNP-IKY-MAT.
           PERFORM UNP-AKY-MAT.
           PERFORM UNP-MKY-MAT.
       FIN-UNP-KEY-MAT.
           EXIT.
       
       UNP-IKY-MAT SECTION.
       INI-UNP-IKY-MAT.
           MOVE FRM-CPIM-UBY TO MAT-COD-ICAR-ATR.
           MOVE FRM-CPIM-UBY TO MAT-CIC-ITIP-ATR.
           MOVE FRM-CPIM-UBY TO MAT-FEC-FPRO-ATR IN MAT-FLD.
           MOVE FRM-CPIM-UBY TO MAT-NUM-IMAT-ATR.
       FIN-UNP-IKY-MAT.
           EXIT.
       
       UNP-MKY-MAT SECTION.
       INI-UNP-MKY-MAT.
       FIN-UNP-MKY-MAT.
           EXIT.
       
       UNP-AKY-MAT SECTION.
       INI-UNP-AKY-MAT.
       FIN-UNP-AKY-MAT.
           EXIT.
       
       UNP-FLD-MAT SECTION.
       INI-UNP-FLD-MAT.
           PERFORM UNP-MAT-MAT.
       FIN-UNP-FLD-MAT.
           EXIT.
       
       UNP-MAT-MAT SECTION.
       INI-UNP-MAT-MAT.
           MOVE FRM-CPIM-UBY TO MAT-COD-NDEF-ATR IN MAT-FLD.
           MOVE FRM-CPIM-UBY TO MAT-COD-CLAU-ATR IN MAT-FLD.
           MOVE FRM-CPIM-UBY TO MAT-SGV-MVPI-ATR IN MAT-FLD.
       FIN-UNP-MAT-MAT.
           EXIT.
       
       VAL-MKY-MAT SECTION.
       INI-VAL-MKY-MAT.
       FIN-VAL-MKY-MAT.
           EXIT.
       
       VAL-FLD-MAT SECTION.
       INI-VAL-FLD-MAT.
           PERFORM VAL-MAT-COD-NDEF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO MAT-COD-NDEF-LEN IN MAT-FLD
                GO TO FIN-VAL-FLD-MAT.
           PERFORM VAL-MAT-COD-CLAU.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO MAT-COD-CLAU-LEN IN MAT-FLD
                GO TO FIN-VAL-FLD-MAT.
           PERFORM VAL-MAT-SGV-MVPI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO MAT-SGV-MVPI-LEN IN MAT-FLD
                GO TO FIN-VAL-FLD-MAT.
       FIN-VAL-FLD-MAT.
           EXIT.
       
       VAL-MAT-COD-NDEF SECTION.
       INI-VAL-MAT-COD-NDEF.
           IF MAT-COD-NDEF IN MAT-FLD NOT > SPACES
              MOVE SPACES TO MAT-COD-NDEF IN MAT-FLD.
       FIN-VAL-MAT-COD-NDEF.
           EXIT.
       
       VAL-MAT-COD-CLAU SECTION.
       INI-VAL-MAT-COD-CLAU.
           IF MAT-COD-CLAU IN MAT-FLD NOT > SPACES
              MOVE SPACES TO MAT-COD-CLAU IN MAT-FLD.
       FIN-VAL-MAT-COD-CLAU.
           EXIT.
       
       VAL-MAT-SGV-MVPI SECTION.
       INI-VAL-MAT-SGV-MVPI.
       FIN-VAL-MAT-SGV-MVPI.
           EXIT.
       
       VAL-CON-FLD-MAT SECTION.
       INI-VAL-CON-FLD-MAT.
       FIN-VAL-CON-FLD-MAT.
           EXIT.
       
       VAL-CON-MAT SECTION.
       INI-VAL-CON-MAT.
       FIN-VAL-CON-MAT.
           EXIT.
       
       VAL-NUM-KEY-MAT SECTION.
       INI-VAL-NUM-KEY-MAT.
           PERFORM VAL-NUM-IKY-MAT.
           PERFORM VAL-NUM-MKY-MAT.
           PERFORM VAL-NUM-AKY-MAT.
       FIN-VAL-NUM-KEY-MAT.
           EXIT.
       
       VAL-NUM-IKY-MAT SECTION.
       INI-VAL-NUM-IKY-MAT.
       VAL-NUM-MAT-NUM-IMAT.
           MOVE MAT-NUM-IMAT-ALF IN MAT-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO MAT-NUM-IMAT IN MAT-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO MAT-NUM-IMAT-LEN IN MAT-FLD
                GO TO FIN-VAL-NUM-IKY-MAT.
       FIN-VAL-NUM-IKY-MAT.
           EXIT.
       
       VAL-NUM-MKY-MAT SECTION.
       INI-VAL-NUM-MKY-MAT.
       FIN-VAL-NUM-MKY-MAT.
           EXIT.
       
       VAL-NUM-AKY-MAT SECTION.
       INI-VAL-NUM-AKY-MAT.
       FIN-VAL-NUM-AKY-MAT.
           EXIT.
       
       VAL-NUM-FLD-MAT SECTION.
       INI-VAL-NUM-FLD-MAT.
       VAL-NUM-MAT-SGV-MVPI.
           MOVE MAT-SGV-MVPI-ALF IN MAT-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 5 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NS9V TO MAT-SGV-MVPI IN MAT-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO MAT-SGV-MVPI-LEN IN MAT-FLD
                GO TO FIN-VAL-NUM-FLD-MAT.
       FIN-VAL-NUM-FLD-MAT.
           EXIT.
       
       EDT-KEY-MAT SECTION.
       INI-EDT-KEY-MAT.
           PERFORM EDT-IKY-MAT.
           PERFORM EDT-MKY-MAT.
           PERFORM EDT-AKY-MAT.
       FIN-EDT-KEY-MAT.
           EXIT.
       
       EDT-IKY-MAT SECTION.
       INI-EDT-IKY-MAT.
           IF MAT-NUM-IMAT-LEN IN MAT-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO MAT-NUM-IMAT-LEN IN MAT-FLD
                GO TO FIN-EDT-IKY-MAT.
           MOVE MAT-NUM-IMAT IN MAT-FLD TO MAT-NUM-IMAT-EDT IN MAT-EDT.
           MOVE MAT-NUM-IMAT-EDT IN MAT-EDT TO 
                MAT-NUM-IMAT-ALF IN MAT-FLD.
       FIN-EDT-IKY-MAT.
           EXIT.
       
       EDT-MKY-MAT SECTION.
       INI-EDT-MKY-MAT.
       FIN-EDT-MKY-MAT.
           EXIT.
       
       EDT-AKY-MAT SECTION.
       INI-EDT-AKY-MAT.
       FIN-EDT-AKY-MAT.
           EXIT.
       
       EDT-FLD-MAT SECTION.
       INI-EDT-FLD-MAT.
           IF MAT-SGV-MVPI-LEN IN MAT-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO MAT-SGV-MVPI-LEN IN MAT-FLD
                GO TO FIN-EDT-FLD-MAT.
           MOVE MAT-SGV-MVPI IN MAT-FLD TO MAT-SGV-MVPI-EDT IN MAT-EDT.
           MOVE MAT-SGV-MVPI-EDT IN MAT-EDT TO 
                MAT-SGV-MVPI-ALF IN MAT-FLD.
       FIN-EDT-FLD-MAT.
           EXIT.
