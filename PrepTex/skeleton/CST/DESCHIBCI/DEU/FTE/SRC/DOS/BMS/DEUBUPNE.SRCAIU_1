       SET-TAG-DEUBUPNE SECTION.
       INI-SET-TAG-DEUBUPNE.
           MOVE
           '<IDNMOD NOM=$DEUBUPNE$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$05-Dec-18 06:19 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-DEUBUPNE.
           EXIT.
           
       RET-ALL-PNE SECTION.
       INI-RET-ALL-PNE.
           PERFORM KEY-ALL-PNE.
           PERFORM RET-PNE-PNE.
       FIN-RET-ALL-PNE.
           EXIT.
       
       RET-PNE-PNE SECTION.
       INI-RET-PNE-PNE.
           MOVE PNE-COD-TLGD IN PNE-FLD TO PNE-COD-TLGD IN PNE.
           MOVE PNE-COD-TLGM IN PNE-FLD TO PNE-COD-TLGM IN PNE.
           MOVE PNE-COD-TLGC IN PNE-FLD TO PNE-COD-TLGC IN PNE.
           MOVE PNE-SGV-PDEF IN PNE-FLD TO PNE-SGV-PDEF IN PNE.
           MOVE PNE-SGV-PVPE IN PNE-FLD TO PNE-SGV-PVPE IN PNE.
           MOVE PNE-SGV-CONF IN PNE-FLD TO PNE-SGV-CONF IN PNE.
           MOVE PNE-SGV-CAPI IN PNE-FLD TO PNE-SGV-CAPI IN PNE.
           MOVE PNE-SGV-PPK1 IN PNE-FLD TO PNE-SGV-PPK1 IN PNE.
           MOVE PNE-SGV-PPK2 IN PNE-FLD TO PNE-SGV-PPK2 IN PNE.
       FIN-RET-PNE-PNE.
           EXIT.
       
       KEY-ALL-PNE SECTION.
       INI-KEY-ALL-PNE.
           PERFORM KEY-PNE-PNE.
       FIN-KEY-ALL-PNE.
           EXIT.
       
       KEY-PNE-PNE SECTION.
       INI-KEY-PNE-PNE.
           MOVE PNE-CAI-ICAR IN PNE-FLD TO PNE-CAI-ICAR IN PNE.
           MOVE PNE-IIC-ICAR IN PNE-FLD TO PNE-IIC-ICAR IN PNE.
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE PNE-NUM-DPNE IN PNE-FLD TO PNE-NUM-DPNE IN PNE.
           MOVE PNE-NUM-MPNE IN PNE-FLD TO PNE-NUM-MPNE IN PNE.
           MOVE PNE-NUM-SPNE IN PNE-FLD TO PNE-NUM-SPNE IN PNE.
           MOVE PNE-NUM-APNE IN PNE-FLD TO PNE-NUM-APNE IN PNE.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE PNE-HRA-HRPN IN PNE-FLD TO PNE-HRA-HRPN IN PNE.
       FIN-KEY-PNE-PNE.
           EXIT.
       
       SCR-CLRA-PNE SECTION.
       INI-SCR-CLRA-PNE.
           PERFORM CLR-KEY-PNE.
           PERFORM CLR-FLD-PNE.
       FIN-SCR-CLRA-PNE.
           EXIT.
       
       CLR-KEY-PNE SECTION.
       INI-CLR-KEY-PNE.
           MOVE SPACES TO PNE-CAI-ICAR IN PNE-FLD.
           MOVE SPACES TO PNE-IIC-ICAR IN PNE-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO PNE-FEC-FPNE IN PNE-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO PNE-HRA-HRPN IN PNE-FLD.
       FIN-CLR-KEY-PNE.
           EXIT.
       
       CLR-FLD-PNE SECTION.
       INI-CLR-FLD-PNE.
           PERFORM CLR-PNE-PNE.
       FIN-CLR-FLD-PNE.
           EXIT.
       
       CLR-PNE-PNE SECTION.
       INI-CLR-PNE-PNE.
           MOVE SPACES TO PNE-COD-TLGD IN PNE-FLD.
           MOVE SPACES TO PNE-COD-TLGM IN PNE-FLD.
           MOVE SPACES TO PNE-COD-TLGC IN PNE-FLD.
           MOVE ZEROES TO PNE-SGV-PDEF IN PNE-FLD.
           MOVE ZEROES TO PNE-SGV-PVPE IN PNE-FLD.
           MOVE ZEROES TO PNE-SGV-CONF IN PNE-FLD.
           MOVE ZEROES TO PNE-SGV-CAPI IN PNE-FLD.
           MOVE ZEROES TO PNE-SGV-PPK1 IN PNE-FLD.
           MOVE ZEROES TO PNE-SGV-PPK2 IN PNE-FLD.
       FIN-CLR-PNE-PNE.
           EXIT.
       
       FST-KEY-PNE SECTION.
       INI-FST-KEY-PNE.
           MOVE -1 TO PNE-CAI-ICAR-LEN IN PNE-FLD.
       FIN-FST-KEY-PNE.
           EXIT.
       
       FST-FLD-PNE SECTION.
       INI-FST-FLD-PNE.
           MOVE -1 TO PNE-COD-TLGD-LEN IN PNE-FLD.
       FIN-FST-FLD-PNE.
           EXIT.
       
       PRO-KEY-PNE SECTION.
       INI-PRO-KEY-PNE.
           PERFORM PRO-MKY-PNE.
       FIN-PRO-KEY-PNE.
           EXIT.
       
       PRO-MKY-PNE SECTION.
       INI-PRO-MKY-PNE.
       FIN-PRO-MKY-PNE.
           EXIT.
       
       PRO-FLD-PNE SECTION.
       INI-PRO-FLD-PNE.
           PERFORM PRO-PNE-PNE.
       FIN-PRO-FLD-PNE.
           EXIT.
       
       PRO-PNE-PNE SECTION.
       INI-PRO-PNE-PNE.
           MOVE FRM-CPIM-AEY TO PNE-COD-TLGD-ATR IN PNE-FLD.
           MOVE FRM-CPIM-AEY TO PNE-COD-TLGM-ATR IN PNE-FLD.
           MOVE FRM-CPIM-AEY TO PNE-COD-TLGC-ATR IN PNE-FLD.
           MOVE FRM-CPIM-AEY TO PNE-SGV-PDEF-ATR IN PNE-FLD.
           MOVE FRM-CPIM-AEY TO PNE-SGV-PVPE-ATR IN PNE-FLD.
           MOVE FRM-CPIM-AEY TO PNE-SGV-CONF-ATR IN PNE-FLD.
           MOVE FRM-CPIM-AEY TO PNE-SGV-CAPI-ATR IN PNE-FLD.
           MOVE FRM-CPIM-AEY TO PNE-SGV-PPK1-ATR IN PNE-FLD.
           MOVE FRM-CPIM-AEY TO PNE-SGV-PPK2-ATR IN PNE-FLD.
       FIN-PRO-PNE-PNE.
           EXIT.
       
       UNP-KEY-PNE SECTION.
       INI-UNP-KEY-PNE.
           PERFORM UNP-IKY-PNE.
           PERFORM UNP-AKY-PNE.
           PERFORM UNP-MKY-PNE.
       FIN-UNP-KEY-PNE.
           EXIT.
       
       UNP-IKY-PNE SECTION.
       INI-UNP-IKY-PNE.
           MOVE FRM-CPIM-UBY TO PNE-CAI-ICAR-ATR.
           MOVE FRM-CPIM-UBY TO PNE-IIC-ICAR-ATR.
           MOVE FRM-CPIM-UBY TO PNE-FEC-FPNE-ATR IN PNE-FLD.
           MOVE FRM-CPIM-UBY TO PNE-HRA-HRPN-ATR.
       FIN-UNP-IKY-PNE.
           EXIT.
       
       UNP-MKY-PNE SECTION.
       INI-UNP-MKY-PNE.
       FIN-UNP-MKY-PNE.
           EXIT.
       
       UNP-AKY-PNE SECTION.
       INI-UNP-AKY-PNE.
       FIN-UNP-AKY-PNE.
           EXIT.
       
       UNP-FLD-PNE SECTION.
       INI-UNP-FLD-PNE.
           PERFORM UNP-PNE-PNE.
       FIN-UNP-FLD-PNE.
           EXIT.
       
       UNP-PNE-PNE SECTION.
       INI-UNP-PNE-PNE.
           MOVE FRM-CPIM-UBY TO PNE-COD-TLGD-ATR IN PNE-FLD.
           MOVE FRM-CPIM-UBY TO PNE-COD-TLGM-ATR IN PNE-FLD.
           MOVE FRM-CPIM-UBY TO PNE-COD-TLGC-ATR IN PNE-FLD.
           MOVE FRM-CPIM-UBY TO PNE-SGV-PDEF-ATR IN PNE-FLD.
           MOVE FRM-CPIM-UBY TO PNE-SGV-PVPE-ATR IN PNE-FLD.
           MOVE FRM-CPIM-UBY TO PNE-SGV-CONF-ATR IN PNE-FLD.
           MOVE FRM-CPIM-UBY TO PNE-SGV-CAPI-ATR IN PNE-FLD.
           MOVE FRM-CPIM-UBY TO PNE-SGV-PPK1-ATR IN PNE-FLD.
           MOVE FRM-CPIM-UBY TO PNE-SGV-PPK2-ATR IN PNE-FLD.
       FIN-UNP-PNE-PNE.
           EXIT.
       
       VAL-MKY-PNE SECTION.
       INI-VAL-MKY-PNE.
       FIN-VAL-MKY-PNE.
           EXIT.
       
       VAL-FLD-PNE SECTION.
       INI-VAL-FLD-PNE.
           PERFORM VAL-PNE-COD-TLGD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PNE-COD-TLGD-LEN IN PNE-FLD
                GO TO FIN-VAL-FLD-PNE.
           PERFORM VAL-PNE-COD-TLGM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PNE-COD-TLGM-LEN IN PNE-FLD
                GO TO FIN-VAL-FLD-PNE.
           PERFORM VAL-PNE-COD-TLGC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PNE-COD-TLGC-LEN IN PNE-FLD
                GO TO FIN-VAL-FLD-PNE.
           PERFORM VAL-PNE-SGV-PDEF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PNE-SGV-PDEF-LEN IN PNE-FLD
                GO TO FIN-VAL-FLD-PNE.
           PERFORM VAL-PNE-SGV-PVPE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PNE-SGV-PVPE-LEN IN PNE-FLD
                GO TO FIN-VAL-FLD-PNE.
           PERFORM VAL-PNE-SGV-CONF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PNE-SGV-CONF-LEN IN PNE-FLD
                GO TO FIN-VAL-FLD-PNE.
           PERFORM VAL-PNE-SGV-CAPI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PNE-SGV-CAPI-LEN IN PNE-FLD
                GO TO FIN-VAL-FLD-PNE.
           PERFORM VAL-PNE-SGV-PPK1.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PNE-SGV-PPK1-LEN IN PNE-FLD
                GO TO FIN-VAL-FLD-PNE.
           PERFORM VAL-PNE-SGV-PPK2.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PNE-SGV-PPK2-LEN IN PNE-FLD
                GO TO FIN-VAL-FLD-PNE.
       FIN-VAL-FLD-PNE.
           EXIT.
       
       VAL-PNE-COD-TLGD SECTION.
       INI-VAL-PNE-COD-TLGD.
           IF PNE-COD-TLGD IN PNE-FLD NOT > SPACES
              MOVE SPACES TO PNE-COD-TLGD IN PNE-FLD.
      *VAL-COD Valida codigo de tabla
           MOVE 'DEU' TO TAB-COD-SIST.
           MOVE 'LGD ' TO TAB-COD-TTAB IN TAB.
           MOVE PNE-COD-TLGD IN PNE-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
       FIN-VAL-PNE-COD-TLGD.
           EXIT.
       
       VAL-PNE-COD-TLGM SECTION.
       INI-VAL-PNE-COD-TLGM.
           IF PNE-COD-TLGM IN PNE-FLD NOT > SPACES
              MOVE SPACES TO PNE-COD-TLGM IN PNE-FLD.
      *VAL-COD Valida codigo de tabla
           MOVE 'DEU' TO TAB-COD-SIST.
           MOVE 'LGM ' TO TAB-COD-TTAB IN TAB.
           MOVE PNE-COD-TLGM IN PNE-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
       FIN-VAL-PNE-COD-TLGM.
           EXIT.
       
       VAL-PNE-COD-TLGC SECTION.
       INI-VAL-PNE-COD-TLGC.
      *<<<< INI_PNE_COD_TLGC IGUALACION VAX DEU
           IF PNE-COD-TLGC IN PNE-FLD NOT > SPACES                      
              MOVE SPACES TO PNE-COD-TLGC IN PNE-FLD.                   
      *VAL-COD Valida codigo de tabla                                   
           MOVE 'DEU' TO TAB-COD-SIST.                                  
           MOVE 'MTZ ' TO TAB-COD-TTAB IN TAB.                          
           MOVE PNE-COD-TLGC IN PNE-FLD TO TAB-COD-CTAB IN TAB.         
           PERFORM VAL-COD. 
           GO TO FIN-VAL-PNE-COD-TLGC.
      *>>>> INI_PNE_COD_TLGC
           IF PNE-COD-TLGC IN PNE-FLD NOT > SPACES
              MOVE SPACES TO PNE-COD-TLGC IN PNE-FLD.
      *VAL-COD Valida codigo de tabla
           MOVE 'DEU' TO TAB-COD-SIST.
           MOVE 'MAT ' TO TAB-COD-TTAB IN TAB.
           MOVE PNE-COD-TLGC IN PNE-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
       FIN-VAL-PNE-COD-TLGC.
           EXIT.
       
       VAL-PNE-SGV-PDEF SECTION.
       INI-VAL-PNE-SGV-PDEF.
       FIN-VAL-PNE-SGV-PDEF.
           EXIT.
       
       VAL-PNE-SGV-PVPE SECTION.
       INI-VAL-PNE-SGV-PVPE.
       FIN-VAL-PNE-SGV-PVPE.
           EXIT.
       
       VAL-PNE-SGV-CONF SECTION.
       INI-VAL-PNE-SGV-CONF.
       FIN-VAL-PNE-SGV-CONF.
           EXIT.
       
       VAL-PNE-SGV-CAPI SECTION.
       INI-VAL-PNE-SGV-CAPI.
       FIN-VAL-PNE-SGV-CAPI.
           EXIT.
       
       VAL-PNE-SGV-PPK1 SECTION.
       INI-VAL-PNE-SGV-PPK1.
       FIN-VAL-PNE-SGV-PPK1.
           EXIT.
       
       VAL-PNE-SGV-PPK2 SECTION.
       INI-VAL-PNE-SGV-PPK2.
       FIN-VAL-PNE-SGV-PPK2.
           EXIT.
       
       VAL-CON-FLD-PNE SECTION.
       INI-VAL-CON-FLD-PNE.
       FIN-VAL-CON-FLD-PNE.
           EXIT.
       
       VAL-CON-PNE SECTION.
       INI-VAL-CON-PNE.
       FIN-VAL-CON-PNE.
           EXIT.
       
       VAL-NUM-KEY-PNE SECTION.
       INI-VAL-NUM-KEY-PNE.
           PERFORM VAL-NUM-IKY-PNE.
           PERFORM VAL-NUM-MKY-PNE.
           PERFORM VAL-NUM-AKY-PNE.
       FIN-VAL-NUM-KEY-PNE.
           EXIT.
       
       VAL-NUM-IKY-PNE SECTION.
       INI-VAL-NUM-IKY-PNE.
       VAL-NUM-PNE-HRA-HRPN.
           MOVE PNE-HRA-HRPN-ALF IN PNE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 6 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO PNE-HRA-HRPN IN PNE-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO PNE-HRA-HRPN-LEN IN PNE-FLD
                GO TO FIN-VAL-NUM-IKY-PNE.
       FIN-VAL-NUM-IKY-PNE.
           EXIT.
       
       VAL-NUM-MKY-PNE SECTION.
       INI-VAL-NUM-MKY-PNE.
       FIN-VAL-NUM-MKY-PNE.
           EXIT.
       
       VAL-NUM-AKY-PNE SECTION.
       INI-VAL-NUM-AKY-PNE.
       FIN-VAL-NUM-AKY-PNE.
           EXIT.
       
       VAL-NUM-FLD-PNE SECTION.
       INI-VAL-NUM-FLD-PNE.
       VAL-NUM-PNE-SGV-PDEF.
           MOVE PNE-SGV-PDEF-ALF IN PNE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 5 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NS9V TO PNE-SGV-PDEF IN PNE-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO PNE-SGV-PDEF-LEN IN PNE-FLD
                GO TO FIN-VAL-NUM-FLD-PNE.
       VAL-NUM-PNE-SGV-PVPE.
           MOVE PNE-SGV-PVPE-ALF IN PNE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 5 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NS9V TO PNE-SGV-PVPE IN PNE-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO PNE-SGV-PVPE-LEN IN PNE-FLD
                GO TO FIN-VAL-NUM-FLD-PNE.
       VAL-NUM-PNE-SGV-CONF.
           MOVE PNE-SGV-CONF-ALF IN PNE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 5 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NS9V TO PNE-SGV-CONF IN PNE-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO PNE-SGV-CONF-LEN IN PNE-FLD
                GO TO FIN-VAL-NUM-FLD-PNE.
       VAL-NUM-PNE-SGV-CAPI.
           MOVE PNE-SGV-CAPI-ALF IN PNE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 5 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NS9V TO PNE-SGV-CAPI IN PNE-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO PNE-SGV-CAPI-LEN IN PNE-FLD
                GO TO FIN-VAL-NUM-FLD-PNE.
       VAL-NUM-PNE-SGV-PPK1.
           MOVE PNE-SGV-PPK1-ALF IN PNE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 5 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NS9V TO PNE-SGV-PPK1 IN PNE-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO PNE-SGV-PPK1-LEN IN PNE-FLD
                GO TO FIN-VAL-NUM-FLD-PNE.
       VAL-NUM-PNE-SGV-PPK2.
           MOVE PNE-SGV-PPK2-ALF IN PNE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 5 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NS9V TO PNE-SGV-PPK2 IN PNE-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO PNE-SGV-PPK2-LEN IN PNE-FLD
                GO TO FIN-VAL-NUM-FLD-PNE.
       FIN-VAL-NUM-FLD-PNE.
           EXIT.
       
       EDT-KEY-PNE SECTION.
       INI-EDT-KEY-PNE.
           PERFORM EDT-IKY-PNE.
           PERFORM EDT-MKY-PNE.
           PERFORM EDT-AKY-PNE.
       FIN-EDT-KEY-PNE.
           EXIT.
       
       EDT-IKY-PNE SECTION.
       INI-EDT-IKY-PNE.
           IF PNE-HRA-HRPN-LEN IN PNE-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PNE-HRA-HRPN-LEN IN PNE-FLD
                GO TO FIN-EDT-IKY-PNE.
           MOVE PNE-HRA-HRPN IN PNE-FLD TO PNE-HRA-HRPN-EDT IN PNE-EDT.
           MOVE PNE-HRA-HRPN-EDT IN PNE-EDT TO 
                PNE-HRA-HRPN-ALF IN PNE-FLD.
       FIN-EDT-IKY-PNE.
           EXIT.
       
       EDT-MKY-PNE SECTION.
       INI-EDT-MKY-PNE.
       FIN-EDT-MKY-PNE.
           EXIT.
       
       EDT-AKY-PNE SECTION.
       INI-EDT-AKY-PNE.
       FIN-EDT-AKY-PNE.
           EXIT.
       
       EDT-FLD-PNE SECTION.
       INI-EDT-FLD-PNE.
           IF PNE-SGV-PDEF-LEN IN PNE-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PNE-SGV-PDEF-LEN IN PNE-FLD
                GO TO FIN-EDT-FLD-PNE.
           MOVE PNE-SGV-PDEF IN PNE-FLD TO PNE-SGV-PDEF-EDT IN PNE-EDT.
           MOVE PNE-SGV-PDEF-EDT IN PNE-EDT TO 
                PNE-SGV-PDEF-ALF IN PNE-FLD.
           IF PNE-SGV-PVPE-LEN IN PNE-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PNE-SGV-PVPE-LEN IN PNE-FLD
                GO TO FIN-EDT-FLD-PNE.
           MOVE PNE-SGV-PVPE IN PNE-FLD TO PNE-SGV-PVPE-EDT IN PNE-EDT.
           MOVE PNE-SGV-PVPE-EDT IN PNE-EDT TO 
                PNE-SGV-PVPE-ALF IN PNE-FLD.
           IF PNE-SGV-CONF-LEN IN PNE-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PNE-SGV-CONF-LEN IN PNE-FLD
                GO TO FIN-EDT-FLD-PNE.
           MOVE PNE-SGV-CONF IN PNE-FLD TO PNE-SGV-CONF-EDT IN PNE-EDT.
           MOVE PNE-SGV-CONF-EDT IN PNE-EDT TO 
                PNE-SGV-CONF-ALF IN PNE-FLD.
           IF PNE-SGV-CAPI-LEN IN PNE-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PNE-SGV-CAPI-LEN IN PNE-FLD
                GO TO FIN-EDT-FLD-PNE.
           MOVE PNE-SGV-CAPI IN PNE-FLD TO PNE-SGV-CAPI-EDT IN PNE-EDT.
           MOVE PNE-SGV-CAPI-EDT IN PNE-EDT TO 
                PNE-SGV-CAPI-ALF IN PNE-FLD.
           IF PNE-SGV-PPK1-LEN IN PNE-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PNE-SGV-PPK1-LEN IN PNE-FLD
                GO TO FIN-EDT-FLD-PNE.
           MOVE PNE-SGV-PPK1 IN PNE-FLD TO PNE-SGV-PPK1-EDT IN PNE-EDT.
           MOVE PNE-SGV-PPK1-EDT IN PNE-EDT TO 
                PNE-SGV-PPK1-ALF IN PNE-FLD.
           IF PNE-SGV-PPK2-LEN IN PNE-FLD = FRM-ERR-NUME
