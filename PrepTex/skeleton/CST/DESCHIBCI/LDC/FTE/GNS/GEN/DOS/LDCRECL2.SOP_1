      *    12-APR-99 07:06 PM     ,PAI= ,CLI= ,SOP=MVS  V 1.1.3
       WRT-SEC-HDR-CL2 SECTION.
       I-WRT-SEC-HDR-CL2.
           MOVE HDR-CL2-L1 TO GRC-LINEA-MRG.
           PERFORM CL2-WRT-LIN-REP.
           MOVE HDR-CL2-L2 TO GRC-LINEA-MRG.
           PERFORM CL2-WRT-LIN-REP.
           MOVE HDR-CL2-L3 TO GRC-LINEA-MRG.
           PERFORM CL2-WRT-LIN-REP.
       F-WRT-SEC-HDR-CL2.
           EXIT.

       WRT-SEC-DL-CL2 SECTION.
       I-WRT-SEC-DL-CL2.
           MOVE 'S' TO WSS-CTL-LIN-ESC IN WSS-CTL-MRG
           MOVE DL-CL2-L1 TO GRC-LINEA-MRG.
           PERFORM CL2-WRT-LIN-REP.
           MOVE DL-CL2-L2 TO GRC-LINEA-MRG.
           PERFORM CL2-WRT-LIN-REP.
       F-WRT-SEC-DL-CL2.
           EXIT.

       WRT-SEC-TRL-CL2 SECTION.
       I-WRT-SEC-TRL-CL2.
       F-WRT-SEC-TRL-CL2.
           EXIT.

       CL2-CLR-VAR-CTL SECTION.
       I-CL2-CLR-VAR-CTL.
       F-CL2-CLR-VAR-CTL.
           EXIT.

       CL2-FST-PRO-CTL SECTION.
       I-CL2-FST-PRO-CTL.
           PERFORM D-PH-CL2.
           PERFORM M-PH-CL2.
           PERFORM WRT-SEC-PH-CL2.
       F-CL2-FST-PRO-CTL.
           EXIT.

       CL2-PRO-RLL-TAM-CF SECTION.
       I-CL2-PRO-RLL-TAM-CF.
           PERFORM CL2-CLR-SEM-NWP.
           PERFORM CL2-CLR-CTL-SIZ.
           PERFORM CL2-SUM-SIZ-CTL-CF.
           PERFORM CL2-CHK-SIZ-DIS.
           PERFORM CL2-SET-SMF-NWP-CF.
           IF WSS-MRG-NO-CABE = 'S'
              PERFORM CL2-WRT-RLL-YPH.
       F-CL2-PRO-RLL-TAM-CF.
           EXIT.

       CL2-WRT-RLL-YPH SECTION.
       I-CL2-WRT-RLL-YPH.
           PERFORM CL2-WRT-PH-SRE.
           PERFORM CL2-CLR-CTL-SIZ.
       F-CL2-WRT-RLL-YPH.
           EXIT.

       CL2-WRT-PH-SRE SECTION.
       I-CL2-WRT-PH-SRE.
           MOVE RPT-MRG-PAGE-LIMIT TO RPT-MRG-TOT-LDIS.
           MOVE  ZEROES TO RPT-MRG-LINE-COUNTER.
           ADD 1 TO RPT-MRG-PAGE-COUNTER IN RPT-VARI-MRG.
           PERFORM D-PH-CL2.
           PERFORM M-PH-CL2.
           PERFORM WRT-SEC-PH-CL2.
       F-CL2-WRT-PH-SRE.
           EXIT.

       INITIATE-RPTCL2 SECTION.
       I-INITIATE-RPTCL2.
           MOVE RPT-PAGE-LIMIT TO RPT-CL2-PAGE-LIMIT.
       F-INITIATE-RPTCL2.
           EXIT.

       TERMINATE-RPTCL2 SECTION.
       I-TERMINATE-RPTCL2.
           PERFORM CL2-CLR-CTL-SIZ.
           PERFORM CL2-CLR-SEM-NWP.
           PERFORM CL2-SUM-SIZ-CTL-CF.
           PERFORM CL2-CHK-SIZ-DIS.
           PERFORM CL2-SET-SMF-NWP-CF.
           IF WSS-MRG-NO-CABE = 'S'
           PERFORM CL2-WRT-RLL-YPH.
           PERFORM CL2-PRO-VAR-CTL-CF.
       F-TERMINATE-RPTCL2.
           EXIT.

       CL2-WRT-LIN-REP SECTION.
       I-CL2-WRT-LIN-REP.
           WRITE GRC-LINEA-MRG.
           ADD 1 TO RPT-MRG-LINE-COUNTER.
       F-CL2-WRT-LIN-REP.
           EXIT.

       CL2-WRT-RLL-NWP SECTION.
       I-CL2-WRT-RLL-NWP.
           IF GRC-1WRT = 'S'
           WRITE GRC-LINEA-MRG AFTER SALTO
           MOVE 'N' TO GRC-1WRT
           ELSE
           WRITE GRC-LINEA-MRG AFTER SALTO
           ADD 1 TO RPT-MRG-LINE-COUNTER.
       F-CL2-WRT-RLL-NWP.
           EXIT.

       CL2-WRT-RLL-YPF SECTION.
       I-CL2-WRT-RLL-YPF.
           ADD 1 TO RPT-MRG-TOT-LDIS.
           WRITE GRC-LINEA-MRG AFTER RPT-MRG-TOT-LDIS.
           ADD 1 TO RPT-MRG-LINE-COUNTER.
       F-CL2-WRT-RLL-YPF.
           EXIT.

       CL2-CHK-VAR-CTL SECTION.
       I-CL2-CHK-VAR-CTL.
       F-CL2-CHK-VAR-CTL.
           EXIT.

       CL2-SET-NWP-DIB-CH SECTION.
       I-CL2-SET-NWP-DIB-CH.
       F-CL2-SET-NWP-DIB-CH.
           EXIT.



       CL2-SET-SMF-NWP-CF SECTION.
       I-CL2-SET-SMF-NWP-CF.
       F-CL2-SET-SMF-NWP-CF.
           EXIT.


       CL2-CHK-SIZ-HDR-CL2 SECTION.
       I-CL2-CHK-SIZ-HDR-CL2.
           COMPUTE RPT-MRG-TOT-LDIS =
                RPT-MRG-PAGE-LIMIT - RPT-MRG-LINE-COUNTER.
           IF RPT-MRG-TOT-LDIS < WSS-SIZ-HDR-CL2
               MOVE 'S' TO WSS-MRG-NO-CABE IN WSS-NCAE-MRG.
       F-CL2-CHK-SIZ-HDR-CL2.
           EXIT.


       CL2-CHK-SIZ-DL-CL2 SECTION.
       I-CL2-CHK-SIZ-DL-CL2.
           COMPUTE RPT-MRG-TOT-LDIS =
                RPT-MRG-PAGE-LIMIT - RPT-MRG-LINE-COUNTER.
           IF RPT-MRG-TOT-LDIS < WSS-SIZ-DL-CL2
               MOVE 'S' TO WSS-MRG-NO-CABE IN WSS-NCAE-MRG.
       F-CL2-CHK-SIZ-DL-CL2.
           EXIT.


       CL2-CHK-SIZ-TRL-CL2 SECTION.
       I-CL2-CHK-SIZ-TRL-CL2.
           COMPUTE RPT-MRG-TOT-LDIS =
                RPT-MRG-PAGE-LIMIT - RPT-MRG-LINE-COUNTER.
           IF RPT-MRG-TOT-LDIS < WSS-SIZ-TRL-CL2
               MOVE 'S' TO WSS-MRG-NO-CABE IN WSS-NCAE-MRG.
       F-CL2-CHK-SIZ-TRL-CL2.
           EXIT.


       PRO-HDR-CL2 SECTION.
       I-PRO-HDR-CL2.
           PERFORM CL2-CHK-SIZ-HDR-CL2.
           IF SI-MRG-SEM-NP OR SI-MRG-NO-CABE
              PERFORM CL2-WRT-RLL-YPH
              MOVE 'N' TO WSS-MRG-SEM-NP IN WSS-SEM-MRG.
           PERFORM D-HDR-CL2.
           PERFORM M-HDR-CL2.
           IF GRC-PRLD = GRC-PRLD-NOT
           MOVE GRC-PRLD-YES TO GRC-PRLD
              GO TO F-PRO-HDR-CL2.
           PERFORM WRT-SEC-HDR-CL2.
       F-PRO-HDR-CL2.
           EXIT.


       PRO-DL-CL2 SECTION.
       I-PRO-DL-CL2.
           PERFORM CL2-CHK-SIZ-DL-CL2.
           IF SI-MRG-SEM-NP OR SI-MRG-NO-CABE
              PERFORM CL2-WRT-RLL-YPH
              MOVE 'N' TO WSS-MRG-SEM-NP IN WSS-SEM-MRG.
           PERFORM D-DL-CL2.
           PERFORM M-DL-CL2.
           IF GRC-PRLD = GRC-PRLD-NOT
           MOVE GRC-PRLD-YES TO GRC-PRLD
              GO TO F-PRO-DL-CL2.
           PERFORM WRT-SEC-DL-CL2.
       F-PRO-DL-CL2.
           EXIT.


       PRO-TRL-CL2 SECTION.
       I-PRO-TRL-CL2.
           PERFORM CL2-CHK-SIZ-TRL-CL2.
           IF SI-MRG-SEM-NP OR SI-MRG-NO-CABE
              PERFORM CL2-WRT-RLL-YPH
              MOVE 'N' TO WSS-MRG-SEM-NP IN WSS-SEM-MRG.
           PERFORM D-TRL-CL2.
           PERFORM M-TRL-CL2.
           IF GRC-PRLD = GRC-PRLD-NOT
           MOVE GRC-PRLD-YES TO GRC-PRLD
              GO TO F-PRO-TRL-CL2.
           PERFORM WRT-SEC-TRL-CL2.
       F-PRO-TRL-CL2.
           EXIT.


       GENERATE-HDR-CL2 SECTION.
       INI-GENERATE-HDR-CL2.
           IF GRC-1WRT = 'S'
              PERFORM CL2-FST-PRO-CTL
              MOVE SRT TO SRT-OLD-MRG.
           PERFORM CL2-CLR-VAR-CTL.
           PERFORM CL2-CHK-VAR-CTL.
           MOVE SRT TO SRT-CUR-MRG.
           MOVE SRT-OLD-MRG TO SRT.
           PERFORM CL2-PRO-VAR-CTL-CF.
           MOVE SRT-CUR-MRG TO SRT.
           PERFORM CL2-PRO-VAR-CTL-CH.
           PERFORM PRO-HDR-CL2.
           MOVE 'S' TO PGM-HRSR-MRG.
           MOVE SRT TO SRT-OLD-MRG.
       FIN-GENERATE-HDR-CL2.
           EXIT.


       GENERATE-DL-CL2 SECTION.
       INI-GENERATE-DL-CL2.
           IF GRC-1WRT = 'S'
              PERFORM CL2-FST-PRO-CTL
              MOVE SRT TO SRT-OLD-MRG.
           PERFORM CL2-CLR-VAR-CTL.
           PERFORM CL2-CHK-VAR-CTL.
           MOVE SRT TO SRT-CUR-MRG.
           MOVE SRT-OLD-MRG TO SRT.
           PERFORM CL2-PRO-VAR-CTL-CF.
           MOVE SRT-CUR-MRG TO SRT.
           PERFORM CL2-PRO-VAR-CTL-CH.
           PERFORM PRO-DL-CL2.
           MOVE 'S' TO PGM-HRSR-MRG.
           MOVE SRT TO SRT-OLD-MRG.
       FIN-GENERATE-DL-CL2.
           EXIT.


       GENERATE-TRL-CL2 SECTION.
       INI-GENERATE-TRL-CL2.
           IF GRC-1WRT = 'S'
              PERFORM CL2-FST-PRO-CTL
              MOVE SRT TO SRT-OLD-MRG.
           PERFORM CL2-CLR-VAR-CTL.
           PERFORM CL2-CHK-VAR-CTL.
           MOVE SRT TO SRT-CUR-MRG.
           MOVE SRT-OLD-MRG TO SRT.
           PERFORM CL2-PRO-VAR-CTL-CF.
           MOVE SRT-CUR-MRG TO SRT.
           PERFORM CL2-PRO-VAR-CTL-CH.
           PERFORM PRO-TRL-CL2.
           MOVE 'S' TO PGM-HRSR-MRG.
           MOVE SRT TO SRT-OLD-MRG.
       FIN-GENERATE-TRL-CL2.
           EXIT.


       CL2-CLR-CTL-SIZ SECTION.
       I-CL2-CLR-CTL-SIZ.
           MOVE 'N' TO WSS-MRG-NO-CABE IN WSS-NCAE-MRG.
           MOVE ZEROES TO RPT-CL2-TOT-LCTL.
       F-CL2-CLR-CTL-SIZ.
           EXIT.


       CL2-CLR-SEM-NWP SECTION.
       I-CL2-CLR-SEM-NWP.
           MOVE 'N' TO WSS-MRG-SEM-NP IN WSS-SEM-MRG.
       F-CL2-CLR-SEM-NWP.
           EXIT.


       CL2-PRO-VAR-CTL-CF SECTION.
       I-CL2-PRO-VAR-CTL-CF.
           PERFORM CL2-PRO-RLL-TAM-CF.
       F-CL2-PRO-VAR-CTL-CF.
           EXIT.


       CL2-SUM-SIZ-CTL-CF SECTION.
       I-CL2-SUM-SIZ-CTL-CF.
       F-CL2-SUM-SIZ-CTL-CF.
           EXIT.

       CL2-SUM-SIZ-CTL-CH SECTION.
       I-CL2-SUM-SIZ-CTL-CH.
       F-CL2-SUM-SIZ-CTL-CH.
           EXIT.

       CL2-PRO-RLL-NWP-CH SECTION.
       I-CL2-PRO-RLL-NWP-CH.
           IF SI-MRG-SEM-NP
              GO TO CAM-PAG.
           PERFORM CL2-CLR-SEM-NWP.
           PERFORM CL2-SUM-SIZ-CTL-CH.
           PERFORM CL2-CHK-SIZ-DIS.
           IF SI-MRG-SEM-NP
              GO TO CAM-PAG.
           PERFORM CL2-SET-NWP-DIB-CH.
           IF SI-MRG-SEM-NP
              GO TO CAM-PAG.
       CAM-PAG.
       F-CL2-PRO-RLL-NWP-CH.
           EXIT.

       CL2-CHK-SIZ-DIS SECTION.
       I-CL2-CHK-SIZ-DIS.
           COMPUTE RPT-MRG-TOT-LDIS =
               RPT-MRG-PAGE-LIMIT - RPT-MRG-LINE-COUNTER.
           IF RPT-MRG-TOT-LDIS < RPT-MRG-TOT-LCTL
               MOVE 'S' TO WSS-MRG-NO-CABE IN WSS-NCAE-MRG.
           MOVE ZEROES TO RPT-CL2-TOT-LCTL.
       F-CL2-CHK-SIZ-DIS.
           EXIT.


       CL2-PRO-VAR-CTL-CH SECTION.
       I-CL2-PRO-VAR-CTL-CH.
           PERFORM CL2-PRO-RLL-NWP-CH.
           IF WSS-MRG-NO-CABE = 'S' OR WSS-MRG-SEM-NP = 'S'
              PERFORM CL2-WRT-RLL-YPH
              MOVE 'N' TO WSS-MRG-SEM-NP
              MOVE 'N' TO WSS-MRG-NO-CABE.
       F-CL2-PRO-VAR-CTL-CH.
           EXIT.

       M-HDR-CL2 SECTION.
       I-M-HDR-CL2.
       F-M-HDR-CL2.
           EXIT.

       M-DL-CL2 SECTION.
       I-M-DL-CL2.
           MOVE WSS-NUM-ILDC IN WSS-CL2 TO
                RT-WSS-NUM-ILDC IN DL-CL2.
           MOVE WSS-GLS-DESC IN WSS-CL2 TO
                RT-WSS-GLS-DESC IN DL-CL2.
           MOVE WSS-IND-VIGE IN WSS-CL2 TO
                RT-WSS-IND-VIGE IN DL-CL2.
           MOVE WSS-COD-TOOC IN WSS-CL2 TO
                RT-WSS-COD-TOOC IN DL-CL2.
           MOVE WSS-COD-AOOC IN WSS-CL2 TO
                RT-WSS-COD-AOOC IN DL-CL2.
           MOVE WSS-GLS-VCCR IN WSS-CL2 TO
                RT-WSS-GLS-VCCR IN DL-CL2.
           MOVE WSS-COD-TGAR IN WSS-CL2 TO
                RT-WSS-COD-TGAR IN DL-CL2.
           MOVE WSS-COD-TOGA IN WSS-CL2 TO
                RT-WSS-COD-TOGA IN DL-CL2.
           MOVE WSS-COD-AOGA IN WSS-CL2 TO
                RT-WSS-COD-AOGA IN DL-CL2.
           MOVE WSS-NUM-PORG IN WSS-CL2 TO
                RT-WSS-NUM-PORG IN DL-CL2.
           MOVE WSS-IND-MGAR IN WSS-CL2 TO
                RT-WSS-IND-MGAR IN DL-CL2.
           MOVE WSS-NUM-DVEN IN WSS-CL2 TO
                RT-WSS-NUM-DVEN IN DL-CL2.
           MOVE WSS-GLS-GUI1 IN WSS-CL2 TO
                RT-WSS-GLS-GUI1 IN DL-CL2.
           MOVE WSS-NUM-MVEN IN WSS-CL2 TO
                RT-WSS-NUM-MVEN IN DL-CL2.
           MOVE WSS-GLS-GUI2 IN WSS-CL2 TO
                RT-WSS-GLS-GUI2 IN DL-CL2.
           MOVE WSS-NUM-SVEN IN WSS-CL2 TO
                RT-WSS-NUM-SVEN IN DL-CL2.
           MOVE WSS-NUM-AVEN IN WSS-CL2 TO
                RT-WSS-NUM-AVEN IN DL-CL2.
           MOVE WSS-VAL-CDLC IN WSS-CL2 TO
                RT-WSS-VAL-CDLC IN DL-CL2.
           MOVE WSS-VAL-CMLC IN WSS-CL2 TO
                RT-WSS-VAL-CMLC IN DL-CL2.
           MOVE SPACES  TO
                RT-WSS-GLS-BLK0 IN DL-CL2.
       F-M-DL-CL2.
           EXIT.

       M-TRL-CL2 SECTION.
       I-M-TRL-CL2.
       F-M-TRL-CL2.
           EXIT.
