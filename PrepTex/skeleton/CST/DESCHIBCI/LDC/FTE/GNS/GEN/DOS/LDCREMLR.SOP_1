           PERFORM MLR-SUM-SIZ-CTL-CH.
           PERFORM MLR-CHK-SIZ-DIS.
           IF SI-MLR-SEM-NP
              GO TO CAM-PAG.
           PERFORM MLR-SET-NWP-DIB-CH.
           IF SI-MLR-SEM-NP
              GO TO CAM-PAG.
       CAM-PAG.
       F-MLR-PRO-RLL-NWP-CH.
           EXIT.

       MLR-CHK-SIZ-DIS SECTION.
       I-MLR-CHK-SIZ-DIS.
           COMPUTE RPT-MLR-TOT-LDIS =
               RPT-MLR-PAGE-LIMIT - RPT-MLR-LINE-COUNTER.
           IF RPT-MLR-TOT-LDIS < RPT-MLR-TOT-LCTL
               MOVE 'S' TO WSS-MLR-NO-CABE IN WSS-NCAE-MLR.
           MOVE ZEROES TO RPT-MLR-TOT-LCTL.
       F-MLR-CHK-SIZ-DIS.
           EXIT.


       MLR-PRO-VAR-CTL-CH SECTION.
       I-MLR-PRO-VAR-CTL-CH.
           PERFORM MLR-PRO-RLL-NWP-CH.
           IF WSS-MLR-NO-CABE = 'S' OR WSS-MLR-SEM-NP = 'S'
              PERFORM MLR-WRT-RLL-YPH
              MOVE 'N' TO WSS-MLR-SEM-NP
              MOVE 'N' TO WSS-MLR-NO-CABE.
       F-MLR-PRO-VAR-CTL-CH.
           EXIT.

       M-PH-MLR SECTION.
       I-M-PH-MLR.
           MOVE RPT-DUSU IN RPT-VARI TO
                RT-RPT-DUSU IN PH-MLR.
           MOVE WSS-NUM-DHAS IN WSS-MLR TO
                RT-WSS-NUM-DHAS IN PH-MLR.
           MOVE WSS-NUM-MHAS IN WSS-MLR TO
                RT-WSS-NUM-MHAS IN PH-MLR.
           MOVE WSS-NUM-SHAS IN WSS-MLR TO
                RT-WSS-NUM-SHAS IN PH-MLR.
           MOVE WSS-NUM-AHAS IN WSS-MLR TO
                RT-WSS-NUM-AHAS IN PH-MLR.
           MOVE RPT-FDMA IN RPT-VARI TO
                RT-RPT-FDMA IN PH-MLR.
           MOVE RPT-DSIS IN RPT-VARI TO
                RT-RPT-DSIS IN PH-MLR.
           MOVE RPT-MLR-PAGE-COUNTER IN RPT-VARI-MLR TO
                RT-RPT-MLR-PAGE-COUNTER IN PH-MLR.
           MOVE WSS-COD-REGI IN WSS-MLR TO
                RT-WSS-COD-REGI IN PH-MLR.
           MOVE WSS-GLS-REGI IN WSS-MLR TO
                RT-WSS-GLS-REGI IN PH-MLR.
           MOVE WSS-COD-OFIC IN WSS-MLR TO
                RT-WSS-COD-OFIC IN PH-MLR.
           MOVE WSS-GLS-OFIC IN WSS-MLR TO
                RT-WSS-GLS-OFIC IN PH-MLR.
       F-M-PH-MLR.
           EXIT.

       M-DL-TBAN SECTION.
       I-M-DL-TBAN.
           MOVE WSS-COD-TBAN IN WSS-MLR TO
                RT-WSS-COD-TBAN IN DL-TBAN.
           MOVE WSS-GLS-TBAN IN WSS-MLR TO
                RT-WSS-GLS-TBAN IN DL-TBAN.
       F-M-DL-TBAN.
           EXIT.

       M-DL-MLR SECTION.
       I-M-DL-MLR.
           MOVE WSS-NUM-TLIN IN WSS-MLR TO
                RT-WSS-NUM-TLIN IN DL-MLR.
           ADD WSS-NUM-TLIN IN WSS-MLR TO SUM-NUM-TLIN.
           ADD WSS-NUM-TLIN IN WSS-MLR TO SUM-NUM-TLIN-TREG.
           ADD WSS-NUM-TLIN IN WSS-MLR TO SUM-NUM-TLIN-TOFI.
           MOVE WSS-VAL-ATRZ IN WSS-MLR TO
                RT-WSS-VAL-ATRZ IN DL-MLR.
           ADD WSS-VAL-ATRZ IN WSS-MLR TO SUM-VAL-ATRZ.
           ADD WSS-VAL-ATRZ IN WSS-MLR TO SUM-VAL-ATRZ-TREG.
           ADD WSS-VAL-ATRZ IN WSS-MLR TO SUM-VAL-ATRZ-TOFI.
           MOVE WSS-VAL-UTIL IN WSS-MLR TO
                RT-WSS-VAL-UTIL IN DL-MLR.
           ADD WSS-VAL-UTIL IN WSS-MLR TO SUM-VAL-UTIL.
           ADD WSS-VAL-UTIL IN WSS-MLR TO SUM-VAL-UTIL-TREG.
           ADD WSS-VAL-UTIL IN WSS-MLR TO SUM-VAL-UTIL-TOFI.
       F-M-DL-MLR.
           EXIT.

       M-DL-TOFI SECTION.
       I-M-DL-TOFI.
           MOVE WSS-GLS-OFI2 IN WSS-MLR TO
                RT-WSS-GLS-OFI2 IN DL-TOFI.
           MOVE SUM-NUM-TLIN-TOFI TO
                RT-SUM-NUM-TLIN-TOFI IN DL-TOFI.
           MOVE ZEROES TO  SUM-NUM-TLIN-TOFI.
           MOVE SUM-VAL-ATRZ-TOFI TO
                RT-SUM-VAL-ATRZ-TOFI IN DL-TOFI.
           MOVE ZEROES TO  SUM-VAL-ATRZ-TOFI.
           MOVE SUM-VAL-UTIL-TOFI TO
                RT-SUM-VAL-UTIL-TOFI IN DL-TOFI.
           MOVE ZEROES TO  SUM-VAL-UTIL-TOFI.
       F-M-DL-TOFI.
           EXIT.

       M-DL-TREG SECTION.
       I-M-DL-TREG.
           MOVE WSS-GLS-REG2 IN WSS-MLR TO
                RT-WSS-GLS-REG2 IN DL-TREG.
           MOVE SUM-NUM-TLIN-TREG TO
                RT-SUM-NUM-TLIN-TREG IN DL-TREG.
           MOVE ZEROES TO  SUM-NUM-TLIN-TREG.
           MOVE SUM-VAL-ATRZ-TREG TO
                RT-SUM-VAL-ATRZ-TREG IN DL-TREG.
           MOVE ZEROES TO  SUM-VAL-ATRZ-TREG.
           MOVE SUM-VAL-UTIL-TREG TO
                RT-SUM-VAL-UTIL-TREG IN DL-TREG.
           MOVE ZEROES TO  SUM-VAL-UTIL-TREG.
       F-M-DL-TREG.
           EXIT.

       M-DL-TBCO SECTION.
       I-M-DL-TBCO.
       F-M-DL-TBCO.
           EXIT.

       M-DL-DBCO SECTION.
       I-M-DL-DBCO.
           MOVE WSS-COD-TBA2 IN WSS-MLR TO
                RT-WSS-COD-TBA2 IN DL-DBCO.
           MOVE WSS-GLS-TBA2 IN WSS-MLR TO
                RT-WSS-GLS-TBA2 IN DL-DBCO.
           MOVE WSS-NUM-TLIN-TBCO IN WSS-MLR TO
                RT-WSS-NUM-TLIN-TBCO IN DL-DBCO.
           MOVE WSS-VAL-ATRZ-TBCO IN WSS-MLR TO
                RT-WSS-VAL-ATRZ-TBCO IN DL-DBCO.
           MOVE WSS-VAL-UTIL-TBCO IN WSS-MLR TO
                RT-WSS-VAL-UTIL-TBCO IN DL-DBCO.
       F-M-DL-DBCO.
           EXIT.

       M-DL-TGBC SECTION.
       I-M-DL-TGBC.
           MOVE SUM-NUM-TLIN TO
                RT-SUM-NUM-TLIN IN DL-TGBC.
           MOVE ZEROES TO  SUM-NUM-TLIN.
           MOVE SUM-VAL-ATRZ TO
                RT-SUM-VAL-ATRZ IN DL-TGBC.
           MOVE ZEROES TO  SUM-VAL-ATRZ.
           MOVE SUM-VAL-UTIL TO
                RT-SUM-VAL-UTIL IN DL-TGBC.
           MOVE ZEROES TO  SUM-VAL-UTIL.
       F-M-DL-TGBC.
           EXIT.

       M-DL-BLCO SECTION.
       I-M-DL-BLCO.
           MOVE WSS-SPACES IN WSS-MLR TO
                RT-WSS-SPACES IN DL-BLCO.
       F-M-DL-BLCO.
           EXIT.
