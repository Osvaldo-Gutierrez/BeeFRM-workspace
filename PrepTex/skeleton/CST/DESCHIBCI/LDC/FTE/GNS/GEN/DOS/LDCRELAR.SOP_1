           PERFORM LAR-SUM-SIZ-CTL-CH.
           PERFORM LAR-CHK-SIZ-DIS.
           IF SI-LAR-SEM-NP
              GO TO CAM-PAG.
           PERFORM LAR-SET-NWP-DIB-CH.
           IF SI-LAR-SEM-NP
              GO TO CAM-PAG.
       CAM-PAG.
       F-LAR-PRO-RLL-NWP-CH.
           EXIT.

       LAR-CHK-SIZ-DIS SECTION.
       I-LAR-CHK-SIZ-DIS.
           COMPUTE RPT-LAR-TOT-LDIS =
               RPT-LAR-PAGE-LIMIT - RPT-LAR-LINE-COUNTER.
           IF RPT-LAR-TOT-LDIS < RPT-LAR-TOT-LCTL
               MOVE 'S' TO WSS-LAR-NO-CABE IN WSS-NCAE-LAR.
           MOVE ZEROES TO RPT-LAR-TOT-LCTL.
       F-LAR-CHK-SIZ-DIS.
           EXIT.


       LAR-PRO-VAR-CTL-CH SECTION.
       I-LAR-PRO-VAR-CTL-CH.
           PERFORM LAR-PRO-RLL-NWP-CH.
           IF WSS-LAR-NO-CABE = 'S' OR WSS-LAR-SEM-NP = 'S'
              PERFORM LAR-WRT-RLL-YPH
              MOVE 'N' TO WSS-LAR-SEM-NP
              MOVE 'N' TO WSS-LAR-NO-CABE.
       F-LAR-PRO-VAR-CTL-CH.
           EXIT.

       M-PH-LAR SECTION.
       I-M-PH-LAR.
           MOVE RPT-DUSU IN RPT-VARI TO
                RT-RPT-DUSU IN PH-LAR.
           MOVE WSS-NUM-DHAS IN WSS-LAR TO
                RT-WSS-NUM-DHAS IN PH-LAR.
           MOVE WSS-NUM-MHAS IN WSS-LAR TO
                RT-WSS-NUM-MHAS IN PH-LAR.
           MOVE WSS-NUM-SHAS IN WSS-LAR TO
                RT-WSS-NUM-SHAS IN PH-LAR.
           MOVE WSS-NUM-AHAS IN WSS-LAR TO
                RT-WSS-NUM-AHAS IN PH-LAR.
           MOVE RPT-FDMA IN RPT-VARI TO
                RT-RPT-FDMA IN PH-LAR.
           MOVE RPT-DSIS IN RPT-VARI TO
                RT-RPT-DSIS IN PH-LAR.
           MOVE RPT-LAR-PAGE-COUNTER IN RPT-VARI-LAR TO
                RT-RPT-LAR-PAGE-COUNTER IN PH-LAR.
           MOVE WSS-COD-OFIC IN WSS-LAR TO
                RT-WSS-COD-OFIC IN PH-LAR.
           MOVE WSS-GLS-REGI IN WSS-LAR TO
                RT-WSS-GLS-REGI IN PH-LAR.
           MOVE WSS-COD-REGI IN WSS-LAR TO
                RT-WSS-COD-REGI IN PH-LAR.
           MOVE WSS-GLS-OFIC IN WSS-LAR TO
                RT-WSS-GLS-OFIC IN PH-LAR.
       F-M-PH-LAR.
           EXIT.

       M-DL-TBAN SECTION.
       I-M-DL-TBAN.
           MOVE WSS-COD-TBAN IN WSS-LAR TO
                RT-WSS-COD-TBAN IN DL-TBAN.
           MOVE WSS-GLS-TBAN IN WSS-LAR TO
                RT-WSS-GLS-TBAN IN DL-TBAN.
       F-M-DL-TBAN.
           EXIT.

       M-DL-LAR SECTION.
       I-M-DL-LAR.
           MOVE WSS-NUM-TLIN IN WSS-LAR TO
                RT-WSS-NUM-TLIN IN DL-LAR.
           ADD WSS-NUM-TLIN IN WSS-LAR TO SUM-NUM-TLIN.
           ADD WSS-NUM-TLIN IN WSS-LAR TO SUM-NUM-TLIN-TREG.
           ADD WSS-NUM-TLIN IN WSS-LAR TO SUM-NUM-TLIN-TOFI.
           MOVE WSS-VAL-ATRZ IN WSS-LAR TO
                RT-WSS-VAL-ATRZ IN DL-LAR.
           ADD WSS-VAL-ATRZ IN WSS-LAR TO SUM-VAL-ATRZ.
           ADD WSS-VAL-ATRZ IN WSS-LAR TO SUM-VAL-ATRZ-TREG.
           ADD WSS-VAL-ATRZ IN WSS-LAR TO SUM-VAL-ATRZ-TOFI.
           MOVE WSS-VAL-UTIL IN WSS-LAR TO
                RT-WSS-VAL-UTIL IN DL-LAR.
           ADD WSS-VAL-UTIL IN WSS-LAR TO SUM-VAL-UTIL.
           ADD WSS-VAL-UTIL IN WSS-LAR TO SUM-VAL-UTIL-TREG.
           ADD WSS-VAL-UTIL IN WSS-LAR TO SUM-VAL-UTIL-TOFI.
       F-M-DL-LAR.
           EXIT.

       M-DL-TOFI SECTION.
       I-M-DL-TOFI.
           MOVE WSS-GLS-OFI2 IN WSS-LAR TO
                RT-WSS-GLS-OFI2 IN DL-TOFI.
           MOVE SUM-NUM-TLIN-TOFI TO
                RT-SUM-NUM-TLIN-TOFI IN DL-TOFI.
           MOVE ZEROES TO  SUM-NUM-TLIN-TOFI.
           MOVE SUM-VAL-ATRZ-TOFI TO
                RT-SUM-VAL-ATRZ-TOFI IN DL-TOFI.
           MOVE ZEROES TO  SUM-VAL-ATRZ-TOFI.
           MOVE SUM-VAL-UTIL-TOFI TO
                RT-SUM-VAL-UTIL-TOFI IN DL-TOFI.
           MOVE ZEROES TO  SUM-VAL-UTIL-TOFI.
       F-M-DL-TOFI.
           EXIT.

       M-DL-TREG SECTION.
       I-M-DL-TREG.
           MOVE WSS-GLS-REG2 IN WSS-LAR TO
                RT-WSS-GLS-REG2 IN DL-TREG.
           MOVE SUM-NUM-TLIN-TREG TO
                RT-SUM-NUM-TLIN-TREG IN DL-TREG.
           MOVE ZEROES TO  SUM-NUM-TLIN-TREG.
           MOVE SUM-VAL-ATRZ-TREG TO
                RT-SUM-VAL-ATRZ-TREG IN DL-TREG.
           MOVE ZEROES TO  SUM-VAL-ATRZ-TREG.
           MOVE SUM-VAL-UTIL-TREG TO
                RT-SUM-VAL-UTIL-TREG IN DL-TREG.
           MOVE ZEROES TO  SUM-VAL-UTIL-TREG.
       F-M-DL-TREG.
           EXIT.

       M-DL-TBCO SECTION.
       I-M-DL-TBCO.
       F-M-DL-TBCO.
           EXIT.

       M-DL-DBCO SECTION.
       I-M-DL-DBCO.
           MOVE WSS-COD-TBA2 IN WSS-LAR TO
                RT-WSS-COD-TBA2 IN DL-DBCO.
           MOVE WSS-GLS-TBA2 IN WSS-LAR TO
                RT-WSS-GLS-TBA2 IN DL-DBCO.
           MOVE WSS-NUM-TLIN-TBCO IN WSS-LAR TO
                RT-WSS-NUM-TLIN-TBCO IN DL-DBCO.
           MOVE WSS-VAL-ATRZ-TBCO IN WSS-LAR TO
                RT-WSS-VAL-ATRZ-TBCO IN DL-DBCO.
           MOVE WSS-VAL-UTIL-TBCO IN WSS-LAR TO
                RT-WSS-VAL-UTIL-TBCO IN DL-DBCO.
       F-M-DL-DBCO.
           EXIT.

       M-DL-TGBC SECTION.
       I-M-DL-TGBC.
           MOVE SUM-NUM-TLIN TO
                RT-SUM-NUM-TLIN IN DL-TGBC.
           MOVE ZEROES TO  SUM-NUM-TLIN.
           MOVE SUM-VAL-ATRZ TO
                RT-SUM-VAL-ATRZ IN DL-TGBC.
           MOVE ZEROES TO  SUM-VAL-ATRZ.
           MOVE SUM-VAL-UTIL TO
                RT-SUM-VAL-UTIL IN DL-TGBC.
           MOVE ZEROES TO  SUM-VAL-UTIL.
       F-M-DL-TGBC.
           EXIT.

       M-DL-BLCO SECTION.
       I-M-DL-BLCO.
           MOVE WSS-SPACES IN WSS-LAR TO
                RT-WSS-SPACES IN DL-BLCO.
       F-M-DL-BLCO.
           EXIT.
