*%! Codigo adicional para archivo LDCBMACG
*% IF INI_FRM_VAL_MMIN
      *<<<< INI_FRM_VAL_MMIN
           IF FRM-COD-BENE IN ACG-FLD(FRM-IFLD) > SPACES AND            
              FRM-VAL-MMIN IN ACG-FLD(FRM-IFLD) NOT > ZEROES            
              PERFORM ERR-GLS.                                          
           IF FRM-COD-BENE IN ACG-FLD(FRM-IFLD) > SPACES
              IF FRM-VAL-MMIN IN ACG-FLD(FRM-IFLD) > 
                 FRM-VAL-MMAX IN ACG-FLD(FRM-IFLD)
                 MOVE 'DEU'          TO MSG-COD-SIST
                 MOVE 'MMIN > MMAX ' TO MSG-COD-MENS
                 PERFORM GET-MSG
                 MOVE MSG-GLS-DESC   TO FRM-MENS
                 MOVE FRM-SUAR-MAL   TO FRM-SUAR.
      *>>>>
*% END
*% IF FIN_PUT_LCC_ACG
           MOVE LCC-CAI-ILIC IN LCC TO ACG-CAI-ILIC IN ACG-FLD.         
           MOVE LCC-IIC-ILIC IN LCC TO ACG-IIC-ILIC IN ACG-FLD.         
*% END
*% IF INI_FRM_NUM_PMAX
      *<<<< INI_FRM_NUM_PMAX
           IF FRM-COD-BENE IN ACG-FLD(FRM-IFLD) > SPACES
              IF FRM-IND-PMAX IN ACG-FLD(FRM-IFLD) > SPACES AND
                 FRM-NUM-PMAX IN ACG-FLD(FRM-IFLD) NOT > ZEROES
                 PERFORM ERR-GLS
                 GO TO FIN-VAL-FRM-NUM-PMAX.
      *>>>>
*% END
*% IF INI_FRM_NUM_PMIN
      *<<<< INI_FRM_NUM_PMIN
           IF FRM-COD-BENE IN ACG-FLD(FRM-IFLD) > SPACES
              IF FRM-IND-PMIN IN ACG-FLD(FRM-IFLD) > SPACES AND
                 FRM-NUM-PMIN IN ACG-FLD(FRM-IFLD) NOT > ZEROES
                 PERFORM ERR-GLS
                 GO TO FIN-VAL-FRM-NUM-PMIN.
           IF FRM-COD-BENE IN ACG-FLD(FRM-IFLD) > SPACES                
              IF FRM-IND-PMIN IN ACG-FLD(FRM-IFLD) NOT > SPACES AND     
                 FRM-NUM-PMIN IN ACG-FLD(FRM-IFLD) NOT > ZEROES         
                 PERFORM ERR-GLS.                                       
           IF FRM-IND-PMIN IN ACG-FLD(FRM-IFLD) = FRM-IND-PMAX          
                                            IN ACG-FLD(FRM-IFLD)        
              IF FRM-NUM-PMIN IN ACG-FLD(FRM-IFLD) >                    
                 FRM-NUM-PMAX IN ACG-FLD(FRM-IFLD)                      
                 MOVE 'DEU'          TO MSG-COD-SIST                    
                 MOVE 'PMIN > PMAX' TO MSG-COD-MENS                     
                 PERFORM GET-MSG                                        
                 MOVE MSG-GLS-DESC   TO FRM-MENS                        
                 MOVE FRM-SUAR-MAL   TO FRM-SUAR                        
                 GO TO FIN-VAL-FRM-NUM-PMIN.                            
      *>>>>
*% END
*% IF INI_FRM_NUM_PREN
      *<<<< INI_FRM_NUM_PREN
           IF FRM-COD-BENE IN ACG-FLD(FRM-IFLD) > SPACES
              IF FRM-IND-PREN IN ACG-FLD(FRM-IFLD) > SPACES AND
                 FRM-NUM-PREN IN ACG-FLD(FRM-IFLD) NOT > ZEROES
                 PERFORM ERR-GLS
                 GO TO FIN-VAL-FRM-NUM-PREN.
           IF FRM-COD-BENE IN ACG-FLD(FRM-IFLD) > SPACES                
              IF FRM-IND-PREN IN ACG-FLD(FRM-IFLD) NOT > SPACES AND     
                 FRM-NUM-PREN IN ACG-FLD(FRM-IFLD) NOT > ZEROES         
                 PERFORM ERR-GLS.                                       
      *>>>>
*% END
*% IF FRM_IND_PMAX
      *<<<< FRM_IND_PMAX
           IF FRM-COD-BENE IN ACG-FLD(FRM-IFLD) > SPACES
              IF FRM-NUM-PMAX IN ACG-FLD(FRM-IFLD) > ZEROES AND
                 FRM-IND-PMAX IN ACG-FLD(FRM-IFLD) NOT > SPACES
                 PERFORM ERR-GLS
                 GO TO FIN-VAL-FRM-IND-PMAX.               
      *>>>>
*% END
*% IF FRM_IND_PMIN
      *<<<< FRM_IND_PMIN
           IF FRM-COD-BENE IN ACG-FLD(FRM-IFLD) > SPACES
              IF FRM-NUM-PMIN IN ACG-FLD(FRM-IFLD) > ZEROES AND
                 FRM-IND-PMIN IN ACG-FLD(FRM-IFLD) NOT > SPACES
                 PERFORM ERR-GLS
                 GO TO FIN-VAL-FRM-IND-PMIN.               
      *>>>>
*% END
*% IF FRM_IND_PREN
      *<<<< FRM_IND_PREN
           IF FRM-COD-BENE IN ACG-FLD(FRM-IFLD) > SPACES
              IF FRM-NUM-PREN IN ACG-FLD(FRM-IFLD) > ZEROES AND
                 FRM-IND-PREN IN ACG-FLD(FRM-IFLD) NOT > SPACES
                 PERFORM ERR-GLS
                 GO TO FIN-VAL-FRM-IND-PREN.               
      *>>>>
*% END
*% IF INI_DBC_GLS_ICLI
           IF DBC-GLS-ICLI IN ACG-FLD NOT > IDC-INIC-GLS                
              MOVE IDC-INIC-GLS TO DBC-GLS-ICLI IN ACG-FLD.             
      *VAL-GLS para IDC valida glosa ( extension ) de identificador     
      *    MOVE DBC-GLS-ICLI IN ACG-FLD TO IDC-GLS-RUTV.                
      *    PERFORM VAL-GLS.                                             
      *    MOVE IDC-GLS-RUTV TO DBC-GLS-ICLI IN ACG-FLD.                
           GO TO FIN-VAL-DBC-GLS-ICLI.
*% END
*% IF FIN_FRM_COD_BENE
           IF FRM-COD-BENE IN ACG-FLD(FRM-IFLD) > SPACES                
               COMPUTE WSS-FLD = FRM-IFLD + 1                           
               IF WSS-FLD NOT > 6                                       
               IF ( FRM-COD-BENE IN ACG-FLD(FRM-IFLD) =                 
                    FRM-COD-BENE IN ACG-FLD(WSS-FLD) ) AND              
                  ( FRM-VAL-MMAX IN ACG-FLD(FRM-IFLD) =                 
                    FRM-VAL-MMAX IN ACG-FLD(WSS-FLD) ) AND              
                  ( FRM-VAL-MMIN IN ACG-FLD(FRM-IFLD) =                 
                    FRM-VAL-MMIN IN ACG-FLD(WSS-FLD) ) AND              
                  ( FRM-VAL-PGAR IN ACG-FLD(FRM-IFLD) =                 
                    FRM-VAL-PGAR IN ACG-FLD(WSS-FLD) ) AND              
                  ( FRM-VAL-PGAR IN ACG-FLD(FRM-IFLD) =                 
                    FRM-VAL-PGAR IN ACG-FLD(WSS-FLD) ) AND              
                  ( FRM-NUM-PMAX IN ACG-FLD(FRM-IFLD) =                 
                    FRM-NUM-PMAX IN ACG-FLD(WSS-FLD) ) AND              
                  ( FRM-IND-PMAX IN ACG-FLD(FRM-IFLD) =                 
                    FRM-IND-PMAX IN ACG-FLD(WSS-FLD) ) AND              
                  ( FRM-NUM-PMIN IN ACG-FLD(FRM-IFLD) =                 
                    FRM-NUM-PMIN IN ACG-FLD(WSS-FLD) ) AND              
                  ( FRM-IND-PMIN IN ACG-FLD(FRM-IFLD) =                 
                    FRM-IND-PMIN IN ACG-FLD(WSS-FLD) ) AND              
                  ( FRM-NUM-PREN IN ACG-FLD(FRM-IFLD) =                 
                    FRM-NUM-PREN IN ACG-FLD(WSS-FLD) ) AND              
                  ( FRM-IND-PREN IN ACG-FLD(FRM-IFLD) =                 
                    FRM-IND-PREN IN ACG-FLD(WSS-FLD) )                  
                     MOVE 'DEU'          TO MSG-COD-SIST                
                     MOVE 'BENE=BENE-AN' TO MSG-COD-MENS                
                     PERFORM GET-MSG                                    
                     MOVE MSG-GLS-DESC   TO FRM-MENS                    
                     MOVE FRM-SUAR-MAL   TO FRM-SUAR                    
                     GO TO FIN-VAL-FRM-COD-BENE.                        
*% END
*% IF FIN_FRM_VAL_MMAX
           IF FRM-COD-BENE IN ACG-FLD(FRM-IFLD) > SPACES AND            
              FRM-VAL-MMAX IN ACG-FLD(FRM-IFLD) NOT > ZEROES            
              PERFORM ERR-GLS.                                          
*% END
*% IF FIN_FRM_VAL_PGAR
           IF FRM-COD-BENE IN ACG-FLD(FRM-IFLD) > SPACES AND            
              FRM-VAL-PGAR IN ACG-FLD(FRM-IFLD) NOT > ZEROES            
              PERFORM ERR-GLS.                                          
           IF FRM-VAL-PGAR IN ACG-FLD(FRM-IFLD) NOT < 100               
              MOVE 'DEU'          TO MSG-COD-SIST                       
              MOVE '% > 100' TO MSG-COD-MENS                            
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC   TO FRM-MENS                           
              MOVE FRM-SUAR-MAL   TO FRM-SUAR                           
              GO TO FIN-VAL-FRM-VAL-PGAR.                               
       FIN-VAL-LCC-VAL-MSEC.                                            
*% END
