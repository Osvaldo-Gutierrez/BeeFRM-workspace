*% PFSQUECOPY = "LDCWXCAV"
*% USA_PFSCONQUE = TRUE
*% LARGO_MI_COMMAREA = 6
*% PFSQUEMAXPAN= "6"
*% PFSQUEENT = "CAV"
*% PFSQUEPRV = "PF5"
*% PFSQUENXT = "PF6"
*% PFSQUESIS = "LDC"
*% PGM_PTC = TRUE
*% IF WSS
      *<<<< WSS
       COPY LDCWXCAV.
       01  WSS-VARI-PFS-QUE.
           03 WSS-FRM-FFLD     VALUE SPACES        PIC X(01).
           03 WSS-MAX-PANT     VALUE 6 PIC 9(05).
           03 WSS-IND-PANT     VALUE 1             PIC 9(05).
           03 WSS-IND-HMAS     VALUE 'N'           PIC X(01).
      *>>>> WSS
      *<<<< WSS
       01  WSS-VARI.
           03 WSS-IND-EMLT     VALUE SPACES        PIC X(01).
      *    AMF
           03 WSS-COD-TOOA     VALUE SPACES        PIC X(03).
           03 WSS-AMF-HMAS     VALUE 'N'           PIC X(01).
      *    LDC-TLC
           03 WSS-CIC-EMLT     VALUE 'MLT'         PIC X(03).
      *    TAB-TIO
           03 WSS-COD-EMLT     VALUE SPACES        PIC X(03).
      *     03 WSS-MSC-STAT     VALUE 'S'           PIC X(01).
           03 WSS-IND-TMPS     VALUE ZEROES        PIC 9(05).
           03 WSS-CIC-ICLI     VALUE SPACES        PIC X(12).
           03 WSS-CAI-ICLF     VALUE SPACES        PIC X(04).
           03 WSS-IIC-ICLF     VALUE SPACES        PIC X(08).
           03 WSS-NUM-ILDC     VALUE ZEROES        PIC 9(02).
           03 WSS-FEC-COMP.
              05 WSS-FEC-DDD                       PIC 9(02).
              05 WSS-FEC-MMM                       PIC 9(02).
              05 WSS-FEC-SSS                       PIC 9(02).
              05 WSS-FEC-AAA                       PIC 9(02).
           03 WSS-ENC-FIA      VALUE SPACES        PIC X(01).
           03 WSS-GET-AMF      VALUE SPACES        PIC X(01).
           03 WSS-AUX-AMF      VALUE SPACES        PIC X(01).
       COPY LDCBRCLF.
       COPY LDCBRLDC.
       COPY LDCBRRDC.
       COPY DEUBRFIA.
       COPY DEUBRAMF.
       COPY GNSWGCPT.
       COPY GNSWGSTR.
      *>>>> WSS
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CLF-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-LDC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RDC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-FIA-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-AMF-REQA==.
      *>>>> WSS_DTC
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-OFRM-SRV
              MOVE TSK-TERM-ALF TO CAV-COD-TERM
              MOVE TSK-TERM-ALF TO CAV-COD-TERM-MAS
           ELSE
              MOVE EIBTRMID     TO CAV-COD-TERM
              MOVE EIBTRMID     TO CAV-COD-TERM-MAS.

           IF FRM-FFLD = FRM-FFLD-PF5 AND
              SCR-INST = SCR-INST-GAB
              MOVE FRM-FFLD-PF5 TO WSS-FRM-FFLD
              MOVE FRM-FFLD-RET TO FRM-FFLD
              MOVE SCR-INST-GKY TO SCR-INST
           ELSE
           IF FRM-FFLD = FRM-FFLD-PF6 AND
              SCR-INST = SCR-INST-GAB
              MOVE FRM-FFLD-PF6 TO WSS-FRM-FFLD
              MOVE FRM-FFLD-RET TO FRM-FFLD
              MOVE SCR-INST-GKY TO SCR-INST
           ELSE
              MOVE FRM-FFLD-RET TO WSS-FRM-FFLD.
      *>>>>
      *<<<< INI_FIO
           MOVE WSS-CIC-EMLT TO WSS-COD-EMLT.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF

       DEL-QUE-CAV SECTION.
       INI-DEL-QUE-CAV.
           MOVE CAV-NOM-TSCAV TO QUE-COLA.
           MOVE QUE-DEL TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
       FIN-DEL-QUE-CAV.
           EXIT.

       CAV-MOV-TS-FRM SECTION.
       INI-CAV-MOV-TS-FRM.
           IF SCR-OFRM-SRV
               GO TO FIN-CAV-MOV-TS-FRM
           ELSE
               IF WSS-IND-PANT > WSS-MAX-PANT
                   GO TO FIN-CAV-MOV-TS-FRM.

           PERFORM CAV-MOV-TS-FRM-ELM.
      *     MOVE CAV-FLD01 TO
      *                FLD01 IN CAV-FLD(WSS-IND-PANT).
           ADD 1 TO WSS-IND-PANT.
       FIN-CAV-MOV-TS-FRM.
           EXIT.
      *<<<< EOF

       CAV-PUT-SCR SECTION.
       INI-CAV-PUT-SCR.
           MOVE WSS-CIC-ICLI TO DBC-CIC-ICLI IN DBC.
           IF LDC-CAI-ICLF IN CAV-FLD = SPACES
              MOVE WSS-CAI-ICLF TO LDC-CAI-ICLF IN CAV-FLD
              MOVE WSS-IIC-ICLF TO LDC-IIC-ICLF IN CAV-FLD.
           MOVE WSS-NUM-ILDC TO LDC-NUM-ILDC IN CAV-FLD.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           IF FIO-STAT-OKS
                 MOVE '00' TO PGM-STAT-DBC
                 MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
                 PERFORM CPT-BLKS
                 MOVE CPT-STRN            TO DBC-GLS-NOMC IN DBC.
       FIN-CAV-PUT-SCR.
           EXIT.

       CAV-ANL-KEY SECTION.
       INI-CAV-ANL-KEY.
           IF WSS-COD-EMLT NOT > SPACES
               MOVE 'LDC'        TO MSG-COD-SIST
               MOVE 'CODMLT NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FIO-STAT-NEX TO FIO-STAT
               GO TO FIN-CAV-ANL-KEY.
           MOVE CAV-NOM-TSCAV  TO QUE-COLA.
           MOVE QUE-DEL TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
      * Si existe numero de linea
           IF LDC-CAI-ICLF IN CAV-FLD > SPACES
               PERFORM GET-LDC-MLT
               IF FIO-STAT-OKS
                  PERFORM GET-FIA-TAB
               ELSE
                  GO TO FIN-CAV-ANL-KEY
           ELSE
      * Si existe rut del deudor
           IF DBC-NUM-ICLI IN CAV-FLD NOT = ZEROES
              PERFORM BUS-IDC-CIC-DEU
              IF FIO-STAT-OKS
                  PERFORM BUS-LDC-MLT
                  IF FIO-STAT-OKS
                     PERFORM GET-FIA-TAB
                  ELSE
                     GO TO FIN-CAV-ANL-KEY
             ELSE
                  GO TO FIN-CAV-ANL-KEY
           ELSE
      * Si existe nombre del cliente
           IF DBC-GLS-NOMC IN CAV-FLD > SPACES
              PERFORM BUS-NMB-CIC-DEU
              IF FIO-STAT-OKS
                  PERFORM BUS-LDC-MLT
                  IF FIO-STAT-OKS
                     PERFORM GET-FIA-TAB
                  ELSE
                     GO TO FIN-CAV-ANL-KEY
              ELSE
                  GO TO FIN-CAV-ANL-KEY.
       FIN-CAV-ANL-KEY.
           EXIT.

       PUT-TSN-ALF SECTION.
       INI-PUT-TSN-ALF.
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.
           PERFORM CPT-BLKS.
           MOVE CPT-STRN TO CAV-FRM-GLS-NOMC.
           IF SCR-OFRM-SRV
              MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI-EDT
              MOVE DBC-NUM-ICLI-EDT TO CAV-FRM-NUM-ICLI-ALF

              MOVE FIA-IIC-IESC IN FIA TO FRM-IIC-IESC-EDT
              MOVE FRM-IIC-IESC-EDT TO CAV-FRM-IIC-IESC-ALF

              MOVE FIA-NUM-IFIA IN FIA TO FRM-NUM-IFIA-EDT
              MOVE FRM-NUM-IFIA-EDT TO CAV-FRM-NUM-IFIA-ALF

              MOVE FIA-VAL-LIMF IN FIA TO FRM-VAL-LIMF-EDT
              MOVE FRM-VAL-LIMF-EDT TO CAV-FRM-VAL-LIMF-ALF
           ELSE
              MOVE DBC-NUM-ICLI IN DBC TO CAV-FRM-NUM-ICLI
              MOVE FIA-IIC-IESC IN FIA TO CAV-FRM-IIC-IESC
              MOVE FIA-NUM-IFIA IN FIA TO CAV-FRM-NUM-IFIA
              MOVE FIA-VAL-LIMF IN FIA TO CAV-FRM-VAL-LIMF.

           MOVE DBC-VRF-ICLI IN DBC TO CAV-FRM-VRF-ICLI.
           MOVE DBC-IND-ICLI IN DBC TO CAV-FRM-IND-ICLI.
           MOVE DBC-GLS-ICLI IN DBC TO CAV-FRM-GLS-ICLI.
           MOVE FIA-CAI-IESC IN FIA TO CAV-FRM-CAI-IESC.
           MOVE FIA-COD-VCLF IN FIA TO CAV-FRM-COD-VCLF.
           MOVE FIA-MSC-STAT IN FIA TO CAV-FRM-MSC-STAT.
           MOVE FIA-COD-TOFS IN FIA TO CAV-FRM-COD-TOFS.
           MOVE WSS-COD-TOOA        TO CAV-FRM-COD-TOOA.

           MOVE CAV-NOM-TSCAV  TO QUE-COLA.
           MOVE CAV-LEN-TSCAV  TO QUE-LITM.
           MOVE CAV-ITEM       TO QUE-ITEM.
           MOVE QUE-PUT        TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
       FIN-PUT-TSN-ALF.
           EXIT.

       CAV-MOV-TS-FRM-ELM SECTION.
       INI-CAV-MOV-TS-FRM-ELM.
           MOVE CAV-FRM-NUM-ICLI     TO
                      FRM-NUM-ICLI IN CAV-FLD(WSS-IND-PANT).
           MOVE CAV-FRM-VRF-ICLI     TO
                      FRM-VRF-ICLI IN CAV-FLD(WSS-IND-PANT).
           MOVE CAV-FRM-IND-ICLI     TO
                      FRM-IND-ICLI IN CAV-FLD(WSS-IND-PANT).
           MOVE CAV-FRM-GLS-ICLI     TO
                      FRM-GLS-ICLI IN CAV-FLD(WSS-IND-PANT).
           MOVE CAV-FRM-GLS-NOMC     TO
                      FRM-GLS-NOMC IN CAV-FLD(WSS-IND-PANT).
           MOVE CAV-FRM-CAI-IESC     TO
                      FRM-CAI-IESC IN CAV-FLD(WSS-IND-PANT).
           MOVE CAV-FRM-IIC-IESC     TO
                      FRM-IIC-IESC IN CAV-FLD(WSS-IND-PANT).
           MOVE CAV-FRM-NUM-IFIA     TO
                      FRM-NUM-IFIA IN CAV-FLD(WSS-IND-PANT).
           MOVE CAV-FRM-VAL-LIMF     TO
                      FRM-VAL-LIMF IN CAV-FLD(WSS-IND-PANT).
           MOVE CAV-FRM-COD-VCLF     TO
                      FRM-COD-VCLF IN CAV-FLD(WSS-IND-PANT).
           MOVE CAV-FRM-MSC-STAT     TO
                      FRM-MSC-STAT IN CAV-FLD(WSS-IND-PANT).
           MOVE CAV-FRM-COD-TOFS     TO
                      FRM-COD-TOFS IN CAV-FLD(WSS-IND-PANT).
           MOVE CAV-FRM-COD-TOOA     TO
                      FRM-COD-TOOA IN CAV-FLD(WSS-IND-PANT).
       FIN-CAV-MOV-TS-FRM-ELM.
           EXIT.

       BUS-IDC-CIC-DEU SECTION.
       INI-BUS-IDC-CIC-DEU.
           PERFORM KEY-DBC-CAV.
           MOVE FIO-GET-KEY TO FIO-CMND.
           MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY.
           PERFORM SGC-FIO-DBC.
           IF FIO-STAT-OKS
              MOVE DBC-CIC-ICLI IN DBC TO WSS-CIC-ICLI
              MOVE '00' TO PGM-STAT-DBC
              MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
              PERFORM CPT-BLKS
              MOVE CPT-STRN TO DBC-GLS-NOMC IN DBC
           ELSE
               MOVE 'LDC'          TO MSG-COD-SIST
               MOVE 'CICICLINOSGC' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FIO-STAT-NEX TO FIO-STAT.
       FIN-BUS-IDC-CIC-DEU.
           EXIT.

       BUS-LDC-MLT SECTION.
       INI-BUS-LDC-MLT.
           MOVE SPACES TO RDC-KEY-IRDC IN RDC.
           MOVE DBC-CIC-ICLI IN DBC TO RDC-CIC-ICLI IN RDC.
           MOVE FIO-GET-NLS TO FIO-CMND.
       CIC-BUS-LDC-MLT.
           MOVE 'RDC-KEY-IRDC' TO FIO-AKEY.
           PERFORM LDC-FIO-RDC.
           IF NOT FIO-STAT-OKS
              MOVE 'LDC'        TO MSG-COD-SIST
              MOVE 'RDC    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE FIO-STAT-NEX TO FIO-STAT
              GO TO FIN-BUS-LDC-MLT.
           IF DBC-CIC-ICLI IN DBC = RDC-CIC-ICLI IN RDC
              MOVE RDC-CIC-ICLF IN RDC TO CLF-CIC-ICLF IN CLF
              MOVE FIO-GET-KEY TO FIO-CMND
              PERFORM LDC-FIO-CLF
              IF NOT FIO-STAT-OKS
                 MOVE 'LDC'        TO MSG-COD-SIST
                 MOVE 'CLF    NEX' TO MSG-COD-MENS
                 PERFORM GET-MSG
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 MOVE FIO-STAT-NEX TO FIO-STAT
                 GO TO FIN-BUS-LDC-MLT
              ELSE
      *AFRI-INI
                   IF CLF-COD-TOCF IN CLF = 'PRY'
                       MOVE FIO-GET-NXT TO FIO-CMND
                       GO TO CIC-BUS-LDC-MLT
                  ELSE
      *AFRI-FIN
                 IF NOT (CLF-MSC-STAT IN CLF = 'A' OR
                         CLF-MSC-STAT IN CLF = 'V' OR
                          CLF-MSC-STAT IN CLF = 'I')
                     MOVE FIO-GET-NXT TO FIO-CMND
                     GO TO CIC-BUS-LDC-MLT
                  ELSE
                     MOVE '00' TO PGM-STAT-CLF
                     MOVE 'N' TO WSS-IND-EMLT
                     PERFORM BUS-LDC-LDC
                     IF WSS-IND-EMLT = 'N'
                        MOVE FIO-GET-NXT TO FIO-CMND
                        GO TO CIC-BUS-LDC-MLT
                     ELSE
                        GO TO FIN-BUS-LDC-MLT
                     MOVE 'LDC'          TO MSG-COD-SIST
                     MOVE 'CICICLINOSGC' TO MSG-COD-MENS
                     PERFORM GET-MSG
                     MOVE MSG-GLS-DESC TO FRM-MENS
                     MOVE FIO-STAT-NEX TO FIO-STAT.
       FIN-BUS-LDC-MLT.
           EXIT.

       BUS-LDC-LDC SECTION.
       INI-BUS-LDC-LDC.
           MOVE RDC-CIC-ICLF IN RDC TO LDC-CIC-ICLF IN LDC.
           MOVE ZEROES              TO LDC-NUM-ILDC IN LDC.
           MOVE FIO-GET-NLS TO FIO-CMND.
       CIC-BUS-LDC-LDC.
           PERFORM LDC-FIO-LDC.
           IF NOT FIO-STAT-OKS
              MOVE 'LDC'        TO MSG-COD-SIST
              MOVE 'RDC    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE FIO-STAT-NEX TO FIO-STAT
              GO TO FIN-BUS-LDC-LDC.
           IF RDC-CIC-ICLF IN RDC = LDC-CIC-ICLF IN LDC
      * REEMPLAZAR POR COMPARACION DE CIC
              MOVE LDC-CAI-ICLF IN LDC TO WSS-CAI-ICLF
              MOVE LDC-IIC-ICLF IN LDC TO WSS-IIC-ICLF
              MOVE LDC-NUM-ILDC IN LDC TO WSS-NUM-ILDC
      *        IF LDC-NUM-ILDC IN CAV-FLD > SPACES
      *           IF LDC-NUM-ILDC IN LDC = LDC-NUM-ILDC IN CAV-FLD
      *              MOVE '00' TO PGM-STAT-LDC
      *              MOVE 'S' TO WSS-IND-EMLT
      *              MOVE LDC-NUM-ILDC IN LDC TO WSS-NUM-ILDC
      *              GO TO FIN-BUS-LDC-LDC
      *           ELSE
      *              MOVE FIO-GET-NXT TO FIO-CMND
      *              GO TO CIC-BUS-LDC-LDC
      *        ELSE
              IF LDC-COD-TIPO IN LDC = WSS-COD-EMLT
                 MOVE '00' TO PGM-STAT-LDC
                 MOVE 'S' TO WSS-IND-EMLT
                 GO TO FIN-BUS-LDC-LDC
              ELSE
                 MOVE FIO-GET-NXT TO FIO-CMND
                 GO TO CIC-BUS-LDC-LDC.
       FIN-BUS-LDC-LDC.
           EXIT.

       BUS-NMB-CIC-DEU SECTION.
       INI-BUS-NMB-CIC-DEU.
      *NOM-CLI Busca Cliente por medio del Nombre
      *ALD
           MOVE SPACES TO DBC-GLS-NOMC IN DBC.
      *    UNSTRING FRM-GLS-NOMC IN CAV-FLD DELIMITED BY '/' INTO
      *                            DBC-GLS-APAT IN DBC
      *                            DBC-GLS-AMAT IN DBC
      *                            DBC-GLS-NOMB IN DBC.
           MOVE DBC-GLS-NOMC IN CAV-FLD TO STR-GLS-GSTR.
           PERFORM STR-UNS.
           MOVE STR-GLS-FUNS IN STR-VARI(1) TO DBC-GLS-APAT IN DBC.
           MOVE STR-GLS-FUNS IN STR-VARI(2) TO DBC-GLS-AMAT IN DBC.
           MOVE STR-GLS-FUNS IN STR-VARI(3) TO DBC-GLS-NOMB IN DBC.
           MOVE 'DBC-GLS-NOMC' TO FIO-AKEY.
           MOVE FIO-GET-NLS    TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           IF NOT FIO-STAT-OKS
               MOVE ZEROES TO DBC-NUM-ICLI IN CAV-FLD
               MOVE SPACES TO DBC-VRF-ICLI IN CAV-FLD
               MOVE SPACES TO DBC-IND-ICLI IN CAV-FLD
               MOVE SPACES TO DBC-GLS-ICLI IN CAV-FLD
               MOVE 'LDC'          TO MSG-COD-SIST
               MOVE 'FRMNOMCNOSGC' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FIO-STAT-NEX TO FIO-STAT
           ELSE
               MOVE '00' TO PGM-STAT-DBC
               MOVE DBC-CIC-ICLI IN DBC TO WSS-CIC-ICLI
               MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
               PERFORM CPT-BLKS
               MOVE CPT-STRN            TO DBC-GLS-NOMC IN DBC.
       FIN-BUS-NMB-CIC-DEU.
           EXIT.

       GET-LDC-MLT SECTION.
       INI-GET-LDC-MLT.
      *AFRI-INI
           MOVE LDC-CAI-ICLF IN CAV-FLD TO CLF-CAI-ICLF IN CLF.
           MOVE LDC-IIC-ICLF IN CAV-FLD TO CLF-IIC-ICLF IN CLF.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM LDC-FIO-CLF.
           IF NOT FIO-STAT-OKS
              MOVE 'LDC'        TO MSG-COD-SIST
              MOVE 'CLF    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE FIO-STAT-NEX TO FIO-STAT
              GO TO FIN-GET-LDC-MLT.
           IF CLF-COD-TOCF IN CLF = 'PRY'
              MOVE 'LDC'        TO MSG-COD-SIST
              MOVE 'TIO    NOCLF' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE FIO-STAT-NEX TO FIO-STAT
              GO TO FIN-GET-LDC-MLT.
      *AFRI-FIN
           MOVE SPACES                  TO RDC-KEY-IRDC IN RDC.
           MOVE LDC-CAI-ICLF IN CAV-FLD TO RDC-CAI-ICLF IN RDC.
           MOVE LDC-IIC-ICLF IN CAV-FLD TO RDC-IIC-ICLF IN RDC.
           MOVE FIO-GET-NLS TO FIO-CMND.
        CIC-GET-LDC-MLT.
           MOVE 'RDC-KEY-ICLF'  TO FIO-AKEY.
           PERFORM LDC-FIO-RDC.
           IF NOT FIO-STAT-OKS
              MOVE 'LDC'        TO MSG-COD-SIST
              MOVE 'RDC    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE FIO-STAT-NEX TO FIO-STAT
              GO TO FIN-GET-LDC-MLT.
           IF LDC-CAI-ICLF IN CAV-FLD = RDC-CAI-ICLF IN RDC AND
              LDC-IIC-ICLF IN CAV-FLD = RDC-IIC-ICLF IN RDC
              MOVE SPACE TO DBC-KEY-IDBC
              MOVE RDC-CIC-ICLI IN RDC TO DBC-CIC-ICLI IN DBC
              MOVE FIO-GET-KEY TO FIO-CMND
              MOVE 'DBC-KEY-IDBC'  TO FIO-AKEY
              PERFORM SGC-FIO-DBC
              IF FIO-STAT-OKS
                 MOVE '00' TO PGM-STAT-DBC
                 MOVE DBC-CIC-ICLI IN DBC TO WSS-CIC-ICLI
                 MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
                 PERFORM CPT-BLKS
                 MOVE CPT-STRN            TO DBC-GLS-NOMC IN CAV-FLD
                 MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN CAV-FLD
                 MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN CAV-FLD
                 MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN CAV-FLD
                 MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN CAV-FLD
                 MOVE RDC-CAI-ICLF IN RDC TO CLF-CAI-ICLF IN CLF
                 MOVE RDC-IIC-ICLF IN RDC TO CLF-IIC-ICLF IN CLF
                 MOVE FIO-GET-KEY TO FIO-CMND
                 PERFORM LDC-FIO-CLF
                 IF NOT FIO-STAT-OKS
                    MOVE 'LDC'        TO MSG-COD-SIST
                    MOVE 'CLF    NEX' TO MSG-COD-MENS
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    MOVE FIO-STAT-NEX TO FIO-STAT
                    GO TO FIN-GET-LDC-MLT
                 ELSE
      *AFRI-INI
                     IF CLF-COD-TOCF IN CLF = 'PRY'
                         MOVE FIO-GET-NXT TO FIO-CMND
                         GO TO CIC-GET-LDC-MLT
                     ELSE
      *AFRI-FIN
                    IF NOT (CLF-MSC-STAT IN CLF = 'A' OR
                            CLF-MSC-STAT IN CLF = 'V' OR
                            CLF-MSC-STAT IN CLF = 'I')
                       MOVE FIO-GET-NXT TO FIO-CMND
                       GO TO CIC-GET-LDC-MLT
                    ELSE
                       MOVE '00' TO PGM-STAT-CLF
                       MOVE 'N' TO WSS-IND-EMLT
                       PERFORM BUS-LDC-LDC
                       IF WSS-IND-EMLT = 'N'
                          MOVE FIO-GET-NXT TO FIO-CMND
                          GO TO CIC-GET-LDC-MLT
                       ELSE
                          GO TO FIN-GET-LDC-MLT
                       MOVE ZEROES TO DBC-NUM-ICLI IN CAV-FLD
                       MOVE SPACES TO DBC-VRF-ICLI IN CAV-FLD
                       MOVE SPACES TO DBC-IND-ICLI IN CAV-FLD
                       MOVE SPACES TO DBC-GLS-ICLI IN CAV-FLD
                       MOVE 'LDC'          TO MSG-COD-SIST
                       MOVE 'CICICLINOSGC' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE FIO-STAT-NEX TO FIO-STAT.
       FIN-GET-LDC-MLT.
           EXIT.

       GET-FIA-TAB SECTION.
       INI-GET-FIA-TAB.
           MOVE 'N' TO WSS-ENC-FIA.
           MOVE ZEROES TO SCR-TOT-OCCU.
           MOVE SPACES TO FIA-GLS-ICYF IN FIA.
           MOVE 'N'    TO WSS-GET-AMF.
           MOVE DBC-CIC-ICLI IN DBC TO FIA-CIC-ICLI IN FIA.
           MOVE FIO-GET-NLS TO FIO-CMND.
       CIC-GET-FIA-TAB.
           MOVE 'FIA-GLS-ICYF' TO FIO-AKEY.
           PERFORM DEU-FIO-FIA.
           IF WSS-ENC-FIA = 'N'
           IF FIO-STAT-OKS
                 MOVE 'S' TO WSS-ENC-FIA.
           IF NOT ( FIO-STAT-OKS AND
                    FIA-CIC-ICLI IN FIA = WSS-CIC-ICLI )
                GO TO FIN-GET-FIA-TAB.
      *      IF  FIA-MSC-STAT IN FIA = WSS-MSC-STAT
           MOVE FIA-NUM-SVEN IN FIA TO WSS-FEC-SSS
           MOVE FIA-NUM-AVEN IN FIA TO WSS-FEC-AAA
           MOVE FIA-NUM-MVEN IN FIA TO WSS-FEC-MMM
           MOVE FIA-NUM-DVEN IN FIA TO WSS-FEC-DDD
           MOVE WSS-FEC-COMP TO FEC-FECH
           MOVE FEC-FORM-FEC TO FEC-FORM
           MOVE FEC-FBLK-S TO FEC-FBLK
           MOVE FEC-CHOY-G TO FEC-CHOY
           MOVE FEC-VHBL-N TO FEC-VHBL
           MOVE FEC-VAL-FEC TO FEC-CMND
           PERFORM PRO-FEC
           IF FEC-STAT-OKS
              MOVE FIA-NUM-SALZ IN FIA TO WSS-FEC-SSS
              MOVE FIA-NUM-AALZ IN FIA TO WSS-FEC-AAA
              MOVE FIA-NUM-MALZ IN FIA TO WSS-FEC-MMM
              MOVE FIA-NUM-DALZ IN FIA TO WSS-FEC-DDD
              MOVE WSS-FEC-COMP TO FEC-FECH
              MOVE FEC-FORM-FEC TO FEC-FORM
              MOVE FEC-FBLK-S TO FEC-FBLK
              MOVE FEC-CHOY-G TO FEC-CHOY
              MOVE FEC-VHBL-N TO FEC-VHBL
              MOVE FEC-VAL-FEC TO FEC-CMND
              PERFORM PRO-FEC
              IF FEC-STAT-OKS
                 MOVE 'N' TO WSS-AMF-HMAS
                 MOVE 'N' TO WSS-GET-AMF
                 IF AMF-COD-TOOA IN CAV-FLD = SPACES
                    PERFORM LUP-LEE-VAMF
                 ELSE
                    IF FIA-COD-TOFS IN CAV-FLD = SPACES OR
                       FIA-COD-TOFS IN FIA = FIA-COD-TOFS IN CAV-FLD
                       PERFORM GET-AMF-COD
                       IF WSS-GET-AMF = 'S'
                          MOVE SPACES TO DBC-KEY-IDBC IN DBC
                          MOVE FIA-CIC-ICFS IN FIA TO
                               DBC-CIC-ICLI IN DBC
                          MOVE FIO-GET-KEY TO FIO-CMND
                          PERFORM SGC-FIO-DBC
                          IF FIO-STAT-OKS
                             ADD 1 TO SCR-TOT-OCCU
                             PERFORM PUT-TSN-ALF.
           MOVE FIO-GET-NXT TO FIO-CMND.
           GO TO CIC-GET-FIA-TAB.
       FIN-GET-FIA-TAB.
           IF WSS-ENC-FIA = 'S' AND
              FIO-STAT = '10'
                MOVE '00' TO FIO-STAT.
           EXIT.

       LUP-LEE-VAMF SECTION.
       INI-LUP-LEE-VAMF.
           MOVE 'N' TO WSS-AUX-AMF.
           MOVE 'N' TO WSS-AMF-HMAS.
           MOVE FIA-KEY-IFIA IN FIA TO AMF-KEY-IFIA IN AMF.
           MOVE SPACES              TO AMF-COD-TOOA IN AMF.
           MOVE FIO-GET-NLS         TO FIO-CMND.
           MOVE 'AMF-KEY-IAMF'      TO FIO-AKEY.
       FIA-GET-AMF-VCOD.
           PERFORM DEU-FIO-AMF.
           IF NOT FIO-STAT-OKS
              MOVE 'N' TO WSS-AMF-HMAS
              MOVE '00' TO FIO-STAT
              MOVE 'N' TO WSS-GET-AMF
              MOVE SPACES TO WSS-COD-TOOA
              GO TO PUT-TSN-AMF
      *        GO TO FIN-LUP-LEE-VAMF
           ELSE
              IF FIA-KEY-IFIA IN FIA = AMF-KEY-IFIA IN AMF
              MOVE AMF-COD-TOOA IN AMF TO WSS-COD-TOOA
                 MOVE 'S' TO WSS-AUX-AMF
              IF AMF-COD-TOOA IN CAV-FLD = SPACES
                    MOVE 'S' TO WSS-GET-AMF
                 MOVE 'S' TO WSS-AMF-HMAS
                    GO TO PUT-TSN-AMF
                 ELSE
                    MOVE 'N' TO WSS-AMF-HMAS
              ELSE
                 MOVE 'N' TO WSS-GET-AMF
                 MOVE SPACES TO WSS-COD-TOOA.
       PUT-TSN-AMF.
           IF WSS-GET-AMF = 'S'
              MOVE SPACES TO DBC-KEY-IDBC IN DBC
              MOVE FIA-CIC-ICFS IN FIA TO DBC-CIC-ICLI IN DBC
              MOVE FIO-GET-KEY TO FIO-CMND
              PERFORM SGC-FIO-DBC
              IF FIO-STAT-OKS
                 IF FIA-COD-TOFS IN FIA = FIA-COD-TOFS IN CAV-FLD
                    ADD 1 TO SCR-TOT-OCCU
                    PERFORM PUT-TSN-ALF
                 ELSE
                    IF FIA-COD-TOFS IN CAV-FLD = SPACES
                       ADD 1 TO SCR-TOT-OCCU
                       PERFORM PUT-TSN-ALF.
           IF WSS-GET-AMF = 'N'
              IF WSS-AUX-AMF = 'N'
                 MOVE SPACES TO DBC-KEY-IDBC IN DBC
                 MOVE FIA-CIC-ICFS IN FIA TO DBC-CIC-ICLI IN DBC
                 MOVE FIO-GET-KEY TO FIO-CMND
                 PERFORM SGC-FIO-DBC
                 IF FIO-STAT-OKS
                    ADD 1 TO SCR-TOT-OCCU
                    PERFORM PUT-TSN-ALF.
       FIN-LUP-LEE-VAMF.
           EXIT.

       GET-AMF-COD SECTION.
       INI-GET-AMF-COD.
           MOVE 'N'                 TO WSS-GET-AMF.
           MOVE SPACES              TO AMF-KEY-IAMF IN AMF.
           MOVE FIA-KEY-IFIA IN FIA TO AMF-KEY-IFIA IN AMF.
           MOVE SPACES         TO AMF-COD-TOOA IN AMF.
           MOVE FIO-GET-NLS TO FIO-CMND.
       FIA-GET-AMF-COD.
           MOVE 'AMF-KEY-IAMF'      TO FIO-AKEY.
           PERFORM DEU-FIO-AMF.
           IF NOT FIO-STAT-OKS
              MOVE '00' TO FIO-STAT
                 GO TO FIN-GET-AMF-COD
              ELSE
              IF FIA-KEY-IFIA IN FIA = AMF-KEY-IFIA IN AMF
              IF AMF-COD-TOOA IN AMF = AMF-COD-TOOA IN CAV-FLD
                    MOVE AMF-COD-TOOA IN AMF TO WSS-COD-TOOA
                 MOVE 'S' TO WSS-GET-AMF
                 GO TO FIN-GET-AMF-COD
              ELSE
                 MOVE FIO-GET-NXT TO FIO-CMND
                    GO TO FIA-GET-AMF-COD
              ELSE
                 MOVE FIO-GET-NXT TO FIO-CMND
                 GO TO FIA-GET-AMF-COD.
       FIN-GET-AMF-COD.
           EXIT.

       COPY LDCBFCLF.
       COPY LDCBFLDC.
       COPY LDCBFRDC.
       COPY DEUBFFIA.
       COPY DEUBFAMF.
       COPY GNSBGCPT.
       COPY GNSBGSTR.
       COPY GNSBBTAB.

       GNX-PRO-QUE SECTION.
       INI-GNX-PRO-QUE.
      *      PERFORM GNS-PRO-QUE.
       FIN-GNX-PRO-QUE.
           EXIT.

       PGM-EBR-ALL SECTION.
       INI-PGM-EBR-ALL.
           MOVE FIO-EBR-ALL TO FIO-CMND.
           PERFORM LDC-FIO-LDC.
       FIN-PGM-EBR-ALL.
           EXIT.
*% END
*% IF PFS_SCR_GKEY_PREV
      *<<<< PFS_SCR_GKEY_PREV
           IF WSS-FRM-FFLD = FRM-FFLD-PF5 OR
              WSS-FRM-FFLD = FRM-FFLD-PF6
                 IF FRM-FFLD = FRM-FFLD-PF5
                     MOVE FRM-FFLD-RET TO FRM-FFLD
              ELSE
                 IF FRM-FFLD = FRM-FFLD-PF6
                     MOVE FRM-FFLD-RET TO FRM-FFLD.
      *>>>>
*% END
*% IF FIN_PUT_SCR
      *<<<< FIN_PUT_SCR
      * AQUI MOVER LOS DATOS DE TS-CAV A PANTALLA
           MOVE SCR-TOT-OCCU TO CAV-MAS-NUM-TOCC.
           MOVE WSS-IND-HMAS TO CAV-MAS-IND-HMAS.
           IF WSS-FRM-FFLD = FRM-FFLD-RET
              MOVE ZEROES TO QUE-NITM
           ELSE
           IF WSS-FRM-FFLD = FRM-FFLD-PF6
              COMPUTE QUE-NITM = (SCR-NUM-PANT * WSS-MAX-PANT)
              IF QUE-NITM NOT < SCR-TOT-OCCU
                  MOVE 'LDC' TO MSG-COD-SIST
                  MOVE 'CAVPF6 NEX' TO MSG-COD-MENS
                  PERFORM GET-MSG
                  MOVE MSG-GLS-DESC TO FRM-MENS
                  COMPUTE QUE-NITM =
                         (SCR-NUM-PANT * WSS-MAX-PANT) - WSS-MAX-PANT
              ELSE
                  ADD 1 TO SCR-NUM-PANT
           ELSE
           IF WSS-FRM-FFLD = FRM-FFLD-PF5
              COMPUTE QUE-NITM =
                     (SCR-NUM-PANT * WSS-MAX-PANT) -
                     (2 * WSS-MAX-PANT - 1)
              IF QUE-NITM < 1
                 MOVE 'LDC' TO MSG-COD-SIST
                 MOVE 'CAVPF5 NEX' TO MSG-COD-MENS
                 PERFORM GET-MSG
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 COMPUTE QUE-NITM =
                        (SCR-NUM-PANT * WSS-MAX-PANT) - WSS-MAX-PANT
              ELSE
                 SUBTRACT 1 FROM QUE-NITM
                 SUBTRACT 1 FROM SCR-NUM-PANT.
           MOVE CAV-NOM-TSCAV  TO QUE-COLA.
           MOVE CAV-LEN-TSCAV  TO QUE-LITM.
       CAV-CIC-LEE-TS.
           ADD 1 TO QUE-NITM.
           MOVE QUE-GET TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT = QUE-STAT-OKS
               MOVE QUE-ITEM TO CAV-ITEM
               PERFORM CAV-MOV-TS-FRM
               GO TO CAV-CIC-LEE-TS
           ELSE
           IF QUE-NITM = 1
               MOVE 'LDC' TO MSG-COD-SIST
               MOVE 'CAV    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS.
      *>>>>
*% END
*% IF PUT_SCR
      *<<<< PUT_SCR
           IF WSS-FRM-FFLD = FRM-FFLD-PF5 OR
              WSS-FRM-FFLD = FRM-FFLD-PF6
              GO TO EXT-PUT-SCR.

           PERFORM CAV-PUT-SCR.
       EXT-PUT-SCR.
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
           IF WSS-FRM-FFLD = FRM-FFLD-PF5 OR
              WSS-FRM-FFLD = FRM-FFLD-PF6
              MOVE '00' TO FIO-STAT
              GO TO FIN-FND-KEY.

           PERFORM DEL-QUE-CAV.
           PERFORM CAV-ANL-KEY.
           MOVE 1 TO SCR-NUM-PANT.
           GO TO FIN-FND-KEY.

           MOVE ZEROES TO SCR-TOT-OCCU.

      *>>>>
*% END
*% IF FIN_SCR_VARI
      *<<<< FIN-SCR-VARI
           03  SCR-NUM-PANT                           PIC 9(03).
           03  SCR-TOT-OCCU                           PIC 9(03).
      *>>>>
*% END
*% IF INI_SCR_COM
      *<<<< INI_SCR_COM
           PERFORM DEL-QUE-CAV.
      *>>>>
*% END
