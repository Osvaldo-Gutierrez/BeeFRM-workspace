*% END
*% IF LCC_IIC_ILIC
      *<<<<
           IF SCR-CMND NOT = 'ING' AND
              SCR-CMND NOT = 'MOD'
              GO TO FIN-VAL-LCC-IIC-ILIC.
           MOVE LCC-CAI-ILIC IN LCC-FLD TO LCC-CAI-ILIC IN LCC.
           MOVE LCC-IIC-ILIC IN LCC-FLD TO LCC-IIC-ILIC IN LCC.
           MOVE FIO-GET-NLS TO FIO-CMND.
       LUP-VAL-LCC-IIC-ILIC.
           MOVE 'LCC-KEY-ILIC' TO FIO-AKEY.
           PERFORM LDC-FIO-LCC
           IF FIO-STAT-OKS AND
              LCC-CAI-ILIC IN LCC-FLD = LCC-CAI-ILIC IN LCC AND
              LCC-IIC-ILIC IN LCC-FLD = LCC-IIC-ILIC IN LCC
               IF LCC-CAI-ICLF IN LCC-FLD NOT = LCC-CAI-ICLF IN LCC
               OR  LCC-CAI-ICLF IN LCC-FLD NOT = LCC-CAI-ICLF IN LCC
               OR  LCC-IIC-ICLF IN LCC-FLD NOT = LCC-IIC-ICLF IN LCC
               OR  LCC-NUM-ILCC IN LCC-FLD NOT = LCC-NUM-ILCC IN LCC
                MOVE 'DEU'          TO MSG-COD-SIST
                MOVE 'LIC    YAEXI' TO MSG-COD-MENS
                PERFORM GET-MSG
                MOVE MSG-GLS-DESC   TO FRM-MENS
                MOVE FRM-SUAR-MAL   TO FRM-SUAR
                GO TO FIN-VAL-LCC-IIC-ILIC
            ELSE
                MOVE FIO-GET-NXT TO FIO-CMND
                GO TO LUP-VAL-LCC-IIC-ILIC.
      *>>>>
*% END
*% IF INI_FRM_VAL_MDIS
      *<<<<
           COMPUTE FRM-VAL-MDIS IN LCC-FLD = LCC-VAL-MADJ IN LCC-FLD -
                                             LCC-VAL-MUTI IN LCC-FLD -
                                             LCC-VAL-RENU IN LCC-FLD.
      *>>>>
*% END
*% IF INI_LCC_VAL_RENU
      *<<<<
           IF SCR-CMND NOT = 'RPG'
               MOVE ZEROES TO LCC-VAL-RENU IN LCC-FLD
               MOVE ZEROES TO LCC-FEC-FRNC IN LCC-FLD
               GO TO FIN-VAL-LCC-VAL-RENU.
           COMPUTE WSS-VAL-TREN =  ( LCC-VAL-PREN IN LCC-FLD *
                                     LCC-VAL-MADJ IN LCC-FLD ) / 100.
           IF LCC-VAL-RENU IN LCC-FLD > WSS-VAL-TREN
              MOVE 'DEU'          TO MSG-COD-SIST
              MOVE 'RENU > TREN ' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC   TO FRM-MENS
              MOVE FRM-SUAR-MAL   TO FRM-SUAR
              GO TO FIN-VAL-LCC-VAL-RENU.
           MOVE LCC-NUM-DREN IN LCC-FLD TO WSS-NUM-DUNO.
           MOVE LCC-NUM-MREN IN LCC-FLD TO WSS-NUM-MUNO.
           MOVE LCC-NUM-SREN IN LCC-FLD TO WSS-NUM-SUNO.
           MOVE LCC-NUM-AREN IN LCC-FLD TO WSS-NUM-AUNO.
           MOVE LCC-NUM-DRNC IN LCC-FLD TO WSS-NUM-DDOS.
           MOVE LCC-NUM-MRNC IN LCC-FLD TO WSS-NUM-MDOS.
           MOVE LCC-NUM-SRNC IN LCC-FLD TO WSS-NUM-SDOS.
           MOVE LCC-NUM-ARNC IN LCC-FLD TO WSS-NUM-ADOS.
           IF WSS-FEC-FDOS  > WSS-FEC-FUNO
              MOVE 'DEU'          TO MSG-COD-SIST
              MOVE 'FECRENU>MAXI' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC   TO FRM-MENS
              MOVE FRM-SUAR-MAL   TO FRM-SUAR
              GO TO FIN-VAL-LCC-VAL-RENU.
      *>>>>
*% END
