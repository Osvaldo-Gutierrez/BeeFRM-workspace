*%! NO_TIENE_TIMESTAMP = TRUE
*% FRM = 'LCC'
*% FNCPQ = "PROYECTO-LOF"
*% VSNPQ = "07.08.14.1"
*% GEN_BCI = TRUE
*% FMT_CHI = TRUE
*% LARGO_MI_COMMAREA = 550
*% PGM_PTC = TRUE
*% IF MI_COMMAREA
      *<<<< MI_COMMAREA                                                 
       COPY LDCWGENG.                                                   

           03 CMA-VARI.                                                 
              05 SCR-IMAX      COMP                   PIC S9(04).       
              05 SCR-I         COMP                   PIC S9(04).       
      *>>>>                                                             
*% END
*% IF WSS
      *<<<< WSS                                                         
       COPY LDCBRLDC.                                                   
       COPY LDCBRRDC.                                                   
       COPY SGCBRDBC.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-LDC-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.        

       COPY LDCWGPMP.                                                   
       COPY GNSBGCPT.                                                   
*% IF PGM_PU
       01   WSS-VARI.                                                   
             03 WSS-VAL-TREN                PIC 9(11)V9(04).            
             03 WSS-FEC-FUNO.                                           
                05  WSS-NUM-SUNO            PIC 9(02).                  
                05  WSS-NUM-AUNO            PIC 9(02).                  
                05  WSS-NUM-MUNO            PIC 9(02).                  
                05  WSS-NUM-DUNO            PIC 9(02).                  
             03 WSS-FEC-FDOS.                                           
                05  WSS-NUM-SDOS            PIC 9(02).                  
                05  WSS-NUM-ADOS            PIC 9(02).                  
                05  WSS-NUM-MDOS            PIC 9(02).                  
                05  WSS-NUM-DDOS            PIC 9(02).                  
*% END
       COPY GNSWGCPT.                                                   
       COPY TABBRCAM.                                                   
       COPY GNSWGPES.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CAM-REQA==.        
*% IF PGM_PU
       COPY LDCBRCLF.
*% END                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CLF-REQA==.        
*% IF PGM_PQ
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RDC-REQA==.        
                                                                        
       01 WSS.                                                          
          03 WSS-IFIN                    VALUE SPACES   PIC X(01).      
          03 WSS-VEC-DATA.                                              
             05 WSS-VEC-CLI OCCURS 4 TIMES.                             
                07 WSS-FILL-1                           PIC X(01).      
                07 WSS-DBC-NUM-ICLI                     PIC X(08).      
                07 WSS-DBC-VRF-ICLI                     PIC X(01).      
                07 WSS-DBC-IND-ICLI                     PIC X(01).      
                07 WSS-DBC-GLS-ICLI                     PIC X(03).      
                07 WSS-FILL-2                           PIC X(01).      
*% END                                                                        
      *>>>>
*% END
*% IF CMN_MOD
      *<<<<                                                                 
           IF SCR-CMND = 'TML'                                          
             COMPUTE LCC-VAL-MADJ IN LCC = LCC-VAL-MADJ IN LCC-FLD      
                                         - FRM-VAL-MTRA IN LCC-FLD.     
      *>>>>
*% END
*% IF FIN_CMN_MOD
      *<<<<
           IF SCR-CMND = 'TML'                                          
              MOVE FRM-CAI-ILDC IN LCC-FLD TO LCC-CAI-ILIC IN LCC       
              MOVE FRM-IIC-ILDC IN LCC-FLD TO LCC-IIC-ILIC IN LCC       
              MOVE 'LCC-KEY-ILIC'     TO FIO-AKEY                       
              MOVE FIO-GET-KEY-UPD    TO FIO-CMND                       
              PERFORM LDC-FIO-LCC                                       
              IF NOT FIO-STAT-OKS                                       
                 GO TO FIN-CMN-MOD                                      
              ELSE                                                      
                 COMPUTE LCC-VAL-MADJ IN LCC = LCC-VAL-MADJ IN LCC      
                                             + FRM-VAL-MTRA IN LCC-FLD  
                 MOVE 'LCC-KEY-ILIC'     TO FIO-AKEY                    
                 MOVE FIO-MOD TO FIO-CMND                               
                 PERFORM LDC-FIO-LCC.                                   
                                                                        
      *>>>>
*% END
*% IF FND_KEY
      *<<<<
           IF LCC-NUM-ILCC IN LCC NOT > ZEROES                          
               PERFORM BUS-COR-LCC.                                     
      *INI-AFRI-JUN07                                                   
           IF LCC-CAI-ILIC IN LCC-FLD > SPACES AND                      
              LCC-IIC-ILIC IN LCC-FLD > SPACES                          
              MOVE 'LCC-KEY-ILIC' TO FIO-AKEY.                          
      *FIN-AFRI-JUN07                                                   
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<<
      *CODIGO ADICIONAL                                                 
      *BUSCAR CLF                                                       
      *<<<< FIN_FND_KEY                                                 
           IF LCC-NUM-ILCC IN LCC NOT > ZEROES                          
               PERFORM BUS-COR-LCC.                                     
           IF FIO-STAT-OKS                                              
           COMPUTE FRM-VAL-MDIS IN LCC-FLD = LCC-VAL-MADJ IN LCC -      
                                             LCC-VAL-MUTI IN LCC -      
                                             LCC-VAL-RENU IN LCC        
           IF SCR-CMND = 'RPG'                                          
               MOVE LCC-CAI-ICLF IN LCC TO CLF-CAI-ICLF IN CLF          
               MOVE LCC-IIC-ICLF IN LCC TO CLF-IIC-ICLF IN CLF          
               MOVE FIO-GET-KEY    TO FIO-CMND                          
               PERFORM LDC-FIO-CLF                                      
               IF NOT FIO-STAT-OKS                                      
      *          LDC NO TIENE CLF ASOCIADO                              
                   MOVE 'DEU' TO MSG-COD-SIST                           
                   MOVE 'LIC    NEX' TO MSG-COD-MENS                    
                   PERFORM GET-MSG                                      
                   MOVE MSG-GLS-DESC TO FRM-MENS                        
                   MOVE '00' TO FIO-STAT                                
                   MOVE SCR-STAT-CON TO SCR-STAT                        
                   GO TO FIN-FND-KEY                                    
               ELSE                                                     
               IF CLF-MSC-STAT IN CLF NOT = 'F'                         
                   MOVE 'DEU' TO MSG-COD-SIST                           
                   MOVE 'RPG  NOAUT' TO MSG-COD-MENS                    
                   PERFORM GET-MSG                                      
                   MOVE MSG-GLS-DESC TO FRM-MENS                        
                   MOVE '00' TO FIO-STAT                                
                   MOVE SCR-STAT-CON TO SCR-STAT                        
                   GO TO FIN-FND-KEY                                    
               ELSE                                                     
               IF LCC-FEC-FRNC IN LCC  > ZEROES                         
                   MOVE 'DEU' TO MSG-COD-SIST                           
                   MOVE 'LCC  RPGYA' TO MSG-COD-MENS                    
                   PERFORM GET-MSG                                      
                   MOVE MSG-GLS-DESC TO FRM-MENS                        
                   MOVE '00' TO FIO-STAT                                
                   MOVE SCR-STAT-CON TO SCR-STAT                        
                   GO TO FIN-FND-KEY                                    
               ELSE                                                     
                   PERFORM GET-FHOY                                     
                   MOVE HOY-DHOY TO LCC-NUM-DRNC IN LCC                 
                   MOVE HOY-MHOY TO LCC-NUM-MRNC IN LCC                 
                   MOVE HOY-SHOY TO LCC-NUM-SRNC IN LCC                 
                   MOVE HOY-AHOY TO LCC-NUM-ARNC IN LCC                 
                   GO TO FIN-FND-KEY.                                   
           IF SCR-CCPP = 'ING' AND PGM-STAT-LDC-OKS OR                  
              SCR-CCPP NOT = 'ING' AND NOT PGM-STAT-LCC-OKS             
               GO TO FIN-FND-KEY.                                       
           IF (SCR-CCPP = 'ING') OR (SCR-CCPP = 'ACT' AND               
               NOT PGM-STAT-LCC-OKS)                                    
               PERFORM BUS-ENT-ANEX                                     
               IF WSS-IFIN = 'S'                                        
                   GO TO FIN-FND-KEY.                                   
           MOVE PGM-STAT-LCC TO FIO-STAT.                               
      *                                                                 
      *>>>>                                                             
*% END
*% IF PUT_SCR
      *<<<<
      *****AFRI                                                         
           PERFORM BUS-ENT-ANEX.                                        
      *                                                                 
      *>>>>
*% END
*% IF FIN_HBL_FLD
      *<<<<     
           IF SCR-CMND = 'RPG'                                          
               PERFORM PRO-FLD-LCC                                      
               MOVE FRM-CPIM-UBY TO LCC-VAL-RENU-ATR IN LCC-FLD         
               MOVE 0 TO LCC-COD-VCOC-LEN IN LCC-FLD                    
               MOVE -1 TO LCC-VAL-RENU-LEN IN LCC-FLD                   
           ELSE                                                         
               MOVE FRM-CPIM-AEY TO LCC-FEC-FRNC-ATR IN LCC-FLD         
               MOVE FRM-CPIM-AEY TO LCC-VAL-RENU-ATR IN LCC-FLD.        
           IF SCR-CMND = 'TML'                                          
               PERFORM PRO-FLD-LCC                                      
               MOVE 0            TO LCC-COD-VCOC-LEN IN LCC-FLD         
               MOVE -1           TO FRM-CAI-ILDC-LEN IN LCC-FLD         
               MOVE FRM-CPIM-UBY TO FRM-CAI-ILDC-ATR IN LCC-FLD         
               MOVE FRM-CPIM-UBY TO FRM-IIC-ILDC-ATR IN LCC-FLD         
               MOVE FRM-CPIM-UBY TO FRM-VAL-MTRA-ATR IN LCC-FLD.        
                                                                        
      *>>>>
*% END                                                                      
*%IF EOF_PQ
      *<<<< PGM_PQ                                                      
       COPY LDCBFCLF.                                                   
       COPY LDCBFRDC.                                                   
       COPY LDCBFLDC.                                                   
       COPY SGCBFDBC.                                                   

       ING-FPEN-CIC SECTION.                                            
       INI-ING-FPEN-CIC.                                                
           MOVE LOW-VALUES TO LCC-FLD.                                  
           PERFORM ING-SETE.                                            
           PERFORM SCR-CLRA-LCC.                                        
           PERFORM HBL-IKY.                                             
           PERFORM HBL-KEY-LCC.                                         
       FIN-ING-FPEN-CIC.                                                
           EXIT.                                                        
                                                                        
       ING-SETE SECTION.                                                
       INI-ING-SETE.                                                    
           PERFORM CRG-TBL-ENG.                                         
           MOVE WSS-NTRN(SCR-IPRO)     TO SCR-NTRN.                     
           MOVE WSS-PROG(SCR-IPRO)     TO SCR-PROG.                     
           MOVE WSS-NMAP(SCR-IPRO)     TO SCR-NMAP.                     
           MOVE WSS-NLIB(SCR-IPRO)     TO SCR-NLIB.                     
                                                                        
           MOVE WSS-NTRV(SCR-IPRO)     TO SCR-NTRV.                     
           MOVE WSS-PROV(SCR-IPRO)     TO SCR-PROV.                     
       FIN-ING-SETE.                                                    
           EXIT.                                                        
                                                                        
       HBL-KEY-LCC SECTION.                                             
       INI-HBL-KEY-LCC.                                                 
           MOVE SPACES TO MSG-COD-MENS.                                 
           MOVE LOW-VALUES TO LCC-FLD.                                  
           PERFORM SCR-CLRA-LCC.                                        
           MOVE SCR-CAI-ICLF IN SCR-VARI TO LCC-CAI-ICLF IN LCC-FLD.    
           MOVE SCR-IIC-ICLF IN SCR-VARI TO LCC-IIC-ICLF IN LCC-FLD.    
           MOVE SCR-NUM-ILDC IN SCR-VARI TO LCC-NUM-ILCC IN LCC-FLD.    
                                                                        
           MOVE 'ING'                  TO SCR-CCPP.                     
           MOVE 'ING'                  TO SCR-CMND.                     
           PERFORM VAL-LCC-NUM-ILCC.                                    
                                                                        
           IF MSG-COD-MENS NOT > SPACES                                 
               PERFORM FND-KEY                                          
               IF MSG-COD-MENS > SPACES                                 
                   MOVE MSG-GLS-DESC TO FRM-MENS.                       
                                                                        
           IF MSG-COD-MENS > SPACES                                     
      *        PERFORM CLR-CLF-LCC                                      
      *         MOVE SCR-STAT-CON TO SCR-STAT                           
      *         MOVE SCR-INST-GAB TO SCR-INST                           
                MOVE SCR-INST-GKY TO SCR-INST                           
                                                                        
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY                            
               PERFORM PRO-ALL                                          
               PERFORM UNP-IKY-LCC                                      
                                                                        
               PERFORM SCR-MSSG                                         
               PERFORM EDT-KEY-LCC                                      
      *         PERFORM EDT-FLD-LCC                                     
               IF SCR-IND-FST IN SCR-VEC-FST(SCR-IPRO) = 'S'            
                   MOVE SPACES TO SCR-IND-FST IN SCR-VEC-FST(SCR-IPRO)  
                   PERFORM SCR-INIT                                     
                   PERFORM SCR-EXIT                                     
                   GO TO FIN-HBL-KEY-LCC                                
               ELSE                                                     
                   PERFORM SCR-PUTA                                     
                   PERFORM SCR-EXIT                                     
                   GO TO FIN-HBL-KEY-LCC.                               
                                                                        
           MOVE +4 TO LCC-CAI-ICLF-LEN IN LCC-FLD.                      
      *     INHIBE MOVE FRM-CPSF TO FRM-CPIM-AEY                        
           MOVE '1' TO FRM-CPIM-AEY.                                    
           PERFORM HBL-FLD.                                             
           MOVE FRM-CPIM-AEY TO LCC-CAI-ICLF-ATR.                       
           MOVE FRM-CPIM-AEY TO LCC-IIC-ICLF-ATR.                       
           MOVE FRM-CPIM-AEY TO LCC-NUM-ILCC-ATR.                       
      *                                                                 
           MOVE SCR-INST-GFL TO SCR-INST.                               
                                                                        
       CIC-HBL-KEY-CLF.                                                 
      *                                                                 
           PERFORM EDT-KEY-LCC.                                         
           PERFORM EDT-FLD-LCC.                                         
                                                                        
           MOVE 'GNS' TO MSG-COD-SIST.                                  
           MOVE 'FLDING' TO MSG-COD-MENS.                               
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MEN1.                               
           MOVE FRM-MEN1     TO LCC-GLS-MEN1 IN LCC-FLD.                
                                                                        
           MOVE 'F10LCCNINCLF' TO MSG-COD-MENS.                         
           PERFORM MSG-PF10.                                            
                                                                        
           PERFORM SCR-MSSG.                                            
                                                                        
           IF SCR-IND-FST IN SCR-VEC-FST(SCR-IPRO) = 'S'                
               MOVE SPACES TO SCR-IND-FST IN SCR-VEC-FST(SCR-IPRO)      
               PERFORM SCR-INIT                                         
               PERFORM STP-EXIT                                         
           ELSE                                                         
               PERFORM SCR-PUTA.                                        
           IF SCR-OFRM-SRV                                              
                EXEC CICS                                               
                 RETURN                                                 
                END-EXEC.                                               
           PERFORM SCR-EXIT.                                            
       FIN-HBL-KEY-LCC.                                                 
           EXIT.                                                        
                                                                        
       MSG-PF10 SECTION.                                                
       INI-MSG-PF10.                                                    
      *GET-MSG Busca mensaje en tabla                                   
           MOVE 'DEU' TO MSG-COD-SIST.                                  
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MEN2.                               
           MOVE FRM-MEN2     TO LCC-GLS-MEN2 IN LCC-FLD.                
       FIN-MSG-PF10.                                                    
           EXIT.                                                        
                                                                        
      *>>>>                                                             
       EGR-FPEN-CIC SECTION.                                            
       INI-EGR-FPEN-CIC.                                                
           PERFORM CRG-TBL-ENG.                                         
           ADD 1 TO SCR-IPRO.                                           
           IF SCR-IPRO > WSS-TOT-IIDX                                   
                  PERFORM EGR-FTER-CIC                                  
                  GO TO FIN-EGR-FPEN-CIC.                               
           IF SCR-IND-ENG IN SCR-VEC-ENG(SCR-IPRO) = SPACES             
               GO TO INI-EGR-FPEN-CIC.                                  
           MOVE WSS-INST-FPEN-CIC TO SCR-INST-EXT.                      
           MOVE WSS-PROG(SCR-IPRO) TO SYS-PROG.                         
           MOVE SCR-VARI TO SYS-CMMA.                                   
           MOVE SCR-TCMA TO SYS-TCMA.                                   
           MOVE SYS-XCTL TO SYS-CMND.                                   
           PERFORM GNS-PRO-SYS.                                         
      *>>>>                                                             
       FIN-EGR-FPEN-CIC.                                                
           EXIT.                                                        
                                                                        
       EGR-FTER-CIC SECTION.                                            
       INI-EGR-FTER-CIC.                                                
           PERFORM CRG-TBL-ENG.                                         
           MOVE WSS-IDX-ICLF TO SCR-IPRO.                               
           MOVE WSS-INST-FTER-CIC TO SCR-INST-EXT.                      
           MOVE WSS-PROG(SCR-IPRO) TO SYS-PROG.                         
           MOVE SCR-VARI TO SYS-CMMA.                                   
           MOVE SCR-TCMA TO SYS-TCMA.                                   
           MOVE SYS-XCTL TO SYS-CMND.                                   
           PERFORM GNS-PRO-SYS.                                         
      *>>>>                                                             
       FIN-EGR-FTER-CIC.                                                
           EXIT.                                                        
                                                                        
       SCR-INIT SECTION.                                                
       INI-SCR-INIT.                                                    
           MOVE 3  TO FRM-PLIN.                                         
           MOVE 19 TO FRM-NLIN.                                         
           MOVE FRM-CLR-FRM TO FRM-CMND.                                
           PERFORM GNS-PRO-FRM.                                         
           MOVE FRM-SINI-DBNF TO FRM-SINI.                              
           MOVE SCR-NMAP TO FRM-NAME.                                   
           MOVE SCR-NLIB TO FRM-NLIB.                                   
           MOVE LCC-FLD TO FRM-DFLD.                                    
           MOVE FRM-INI TO FRM-CMND.                                    
           PERFORM GNS-PRO-FRM.                                         
       FIN-SCR-INIT.                                                    
           EXIT.                                                        
                                                                        
       COPY LDCBGMPT.                                                   
      *FIN ENGANCHE                                                     
      *CODIGO ADICIONAL                                                 
       BUS-ENT-ANEX SECTION.                                            
       INI-BUS-ENT-ANEX.                                                
           MOVE SPACES              TO WSS-IFIN IN WSS.                 
           MOVE LCC-CAI-ICLF IN LCC TO CLF-CAI-ICLF IN CLF.             
           MOVE LCC-IIC-ICLF IN LCC TO CLF-IIC-ICLF IN CLF.             
           MOVE FIO-GET-KEY    TO FIO-CMND.                             
           PERFORM LDC-FIO-CLF.                                         
           IF NOT FIO-STAT-OKS                                          
      *        LDC no tiene CLF asociado                                
               MOVE 'DEU' TO MSG-COD-SIST                               
               MOVE 'LIC    NEX' TO MSG-COD-MENS                        
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE '00' TO FIO-STAT                                    
               MOVE SCR-STAT-CON TO SCR-STAT                            
               MOVE 'S'          TO WSS-IFIN                            
               GO TO FIN-BUS-ENT-ANEX.                                  
           MOVE CLF-NUM-DVEN IN CLF TO FRM-NUM-DVEN IN LCC-FLD.         
           MOVE CLF-NUM-MVEN IN CLF TO FRM-NUM-MVEN IN LCC-FLD.         
           MOVE CLF-NUM-SVEN IN CLF TO FRM-NUM-SVEN IN LCC-FLD.         
           MOVE CLF-NUM-AVEN IN CLF TO FRM-NUM-AVEN IN LCC-FLD.         
           MOVE LCC-CAI-ICLF IN LCC TO LDC-CAI-ICLF IN LDC.             
           MOVE LCC-IIC-ICLF IN LCC TO LDC-IIC-ICLF IN LDC.             
           MOVE LCC-NUM-ILCC IN LCC TO LDC-NUM-ILDC IN LDC.             
           MOVE FIO-GET-KEY    TO FIO-CMND.                             
           PERFORM LDC-FIO-LDC.                                         
           MOVE FIO-STAT TO PGM-STAT-LDC.                               
           IF NOT FIO-STAT-OKS                                          
      *        LDC no tiene CLF asociado                                
               MOVE 'DEU' TO MSG-COD-SIST                               
               MOVE 'LDC    NEX' TO MSG-COD-MENS                        
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE '00' TO FIO-STAT                                    
               MOVE SCR-STAT-CON TO SCR-STAT                            
               MOVE 'S'          TO WSS-IFIN                            
               GO TO FIN-BUS-ENT-ANEX.                                  
      *    >>>>                                                         
                                                                        
           MOVE 'RDC-KEY-ICLF' TO FIO-AKEY.                             
           MOVE CLF-CAI-ICLF   TO RDC-CAI-ICLF.                         
           MOVE CLF-IIC-ICLF   TO RDC-IIC-ICLF.                         
           MOVE FIO-GET-NLS    TO FIO-CMND.                             
           PERFORM LDC-FIO-RDC.                                         
           IF (NOT FIO-STAT-OKS) OR                                     
              (RDC-CIC-ICLF IN RDC NOT = CLF-CIC-ICLF IN CLF)           
      *        CLF no tiene RDC asociado                                
               MOVE 'DEU'         TO MSG-COD-SIST                       
               MOVE 'RDC    NEX'  TO MSG-COD-MENS                       
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC  TO FRM-MENS                           
               MOVE '00'          TO FIO-STAT                           
               MOVE SCR-STAT-CON  TO SCR-STAT                           
               MOVE 'S'           TO WSS-IFIN                           
               GO TO FIN-BUS-ENT-ANEX                                   
           ELSE                                                         
               MOVE RDC-CIC-ICLI   TO DBC-CIC-ICLI IN DBC               
               MOVE FIO-GET-KEY  TO FIO-CMND                            
               PERFORM SGC-FIO-DBC                                      
               MOVE FIO-STAT     TO PGM-STAT-DBC                        
               IF NOT FIO-STAT-OKS                                      
      *           RDC NO TIENE DBC ASOCIADO                             
                  MOVE 'DEU'         TO MSG-COD-SIST                    
                  MOVE 'DBC    NEX'  TO MSG-COD-MENS                    
                  PERFORM GET-MSG                                       
                  MOVE MSG-GLS-DESC  TO FRM-MENS                        
                  MOVE '00'          TO FIO-STAT                        
                  MOVE SCR-STAT-CON  TO SCR-STAT                        
                  MOVE 'S'           TO WSS-IFIN                        
                  GO TO FIN-BUS-ENT-ANEX                                
               ELSE                                                     
                  MOVE DBC-GLS-NOMC IN DBC  TO CPT-STRN                 
                  PERFORM CPT-BLKS                                      
                  MOVE CPT-STRN TO DBC-GLS-NOMC IN DBC.                 
      *    <<<<                                                         
           MOVE 'DEU'                 TO TAB-COD-SIST FIO-SIST.         
           MOVE 'TLL'                 TO TAB-COD-TTAB IN TAB.           
           MOVE LDC-COD-TIPO  IN LDC  TO TAB-COD-CTAB IN TAB.           
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM GNS-FIO-TAB                                          
           IF NOT ( FIO-STAT-OKS AND                                    
              TAB-IND-VIGE IN TAB = 'S' )                               
               MOVE 'DEU' TO MSG-COD-SIST                               
               MOVE 'TLL    NEX' TO MSG-COD-MENS                        
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE '00' TO FIO-STAT                                    
               MOVE SCR-STAT-CON TO SCR-STAT                            
               MOVE 'S'          TO WSS-IFIN                            
               GO TO FIN-BUS-ENT-ANEX.                                  
           MOVE TAB-GLS-DATA IN TAB TO WSS-VEC-DATA.                    
                                                                        
           IF NOT (DBC-NUM-ICLI IN DBC = WSS-DBC-NUM-ICLI(1) AND        
              DBC-VRF-ICLI IN DBC = WSS-DBC-VRF-ICLI(1) AND             
              DBC-IND-ICLI IN DBC = WSS-DBC-IND-ICLI(1) AND             
              DBC-GLS-ICLI IN DBC = WSS-DBC-GLS-ICLI(1))                
           IF NOT(DBC-NUM-ICLI IN DBC = WSS-DBC-NUM-ICLI(2) AND         
              DBC-VRF-ICLI IN DBC = WSS-DBC-VRF-ICLI(2) AND             
              DBC-IND-ICLI IN DBC = WSS-DBC-IND-ICLI(2) AND             
              DBC-GLS-ICLI IN DBC = WSS-DBC-GLS-ICLI(2))                
           IF NOT (DBC-NUM-ICLI IN DBC = WSS-DBC-NUM-ICLI(3) AND        
              DBC-VRF-ICLI IN DBC = WSS-DBC-VRF-ICLI(3) AND             
              DBC-IND-ICLI IN DBC = WSS-DBC-IND-ICLI(3) AND             
              DBC-GLS-ICLI IN DBC = WSS-DBC-GLS-ICLI(3))                
           IF NOT (DBC-NUM-ICLI IN DBC = WSS-DBC-NUM-ICLI(4) AND        
              DBC-VRF-ICLI IN DBC = WSS-DBC-VRF-ICLI(4) AND             
              DBC-IND-ICLI IN DBC = WSS-DBC-IND-ICLI(4) AND             
              DBC-GLS-ICLI IN DBC = WSS-DBC-GLS-ICLI(4))                
               MOVE 'DEU'           TO MSG-COD-SIST                     
               MOVE 'CLI    NOAUTO' TO MSG-COD-MENS                     
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE SCR-STAT-CON TO SCR-STAT                            
               MOVE '00' TO FIO-STAT                                    
               MOVE 'S'          TO WSS-IFIN                            
               GO TO FIN-BUS-ENT-ANEX.                                  
                                                                        
           IF SCR-CCPP = 'ACC' AND SCR-STAT = SCR-STAT-CON              
               MOVE PGM-STAT-LCC TO FIO-STAT                            
               MOVE 'S'          TO WSS-IFIN                            
               GO TO FIN-BUS-ENT-ANEX.                                  
                                                                        
           IF CLF-MSC-STAT IN CLF = 'N'                                 
      *        LDC eliminada                                            
               MOVE 'DEU'           TO MSG-COD-SIST                     
               MOVE 'LIC    EXTING' TO MSG-COD-MENS                     
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE SCR-STAT-CON TO SCR-STAT                            
               MOVE '00' TO FIO-STAT                                    
               MOVE 'S'          TO WSS-IFIN                            
               GO TO FIN-BUS-ENT-ANEX.                                  
                                                                        
           IF CLF-MSC-STAT IN CLF = 'R'                                 
      *        CLF fue renovada                                         
               MOVE 'DEU'           TO MSG-COD-SIST                     
               MOVE 'LIC    MCAYA' TO MSG-COD-MENS                      
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE SCR-STAT-CON TO SCR-STAT                            
               MOVE '00' TO FIO-STAT                                    
               MOVE 'S'          TO WSS-IFIN                            
               GO TO FIN-BUS-ENT-ANEX.                                  
                                                                        
           IF CLF-MSC-STAT IN CLF = 'F' AND                             
              SCR-CMND NOT = 'TML'                                      
      *        LDC activada                                             
               MOVE 'DEU'          TO MSG-COD-SIST                      
               MOVE 'CLF    ACAYA' TO MSG-COD-MENS                      
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE SCR-STAT-CON TO SCR-STAT                            
               MOVE '00' TO FIO-STAT                                    
               MOVE 'S'          TO WSS-IFIN                            
               GO TO FIN-BUS-ENT-ANEX.                                  
           IF SCR-CMND = 'RPG'                                          
              IF CLF-MSC-STAT IN CLF NOT = 'F'                          
      *        LDC activada                                             
               MOVE 'DEU'          TO MSG-COD-SIST                      
               MOVE 'LIC    NOACA' TO MSG-COD-MENS                      
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE SCR-STAT-CON TO SCR-STAT                            
               MOVE '00' TO FIO-STAT                                    
               MOVE 'S'          TO WSS-IFIN                            
               GO TO FIN-BUS-ENT-ANEX.                                  
                                                                        
           IF SCR-CCPP = 'ING'                                          
               PERFORM PUT-LDC-LCC                                      
               PERFORM PUT-DBC-LCC                                      
               MOVE PGM-STAT-LCC TO FIO-STAT                            
               MOVE 'S'          TO WSS-IFIN                            
               GO TO FIN-BUS-ENT-ANEX.                                  
                                                                        
       FIN-BUS-ENT-ANEX.                                                
           EXIT.                                                        
                                                                        
       PRO-FLD-LCC SECTION.                                             
       INI-PRO-FLD-LCC.                                                 
           PERFORM PRO-LCC-LCC.                                         
           PERFORM PRO-DBC-LCC.                                         
           PERFORM PRO-LDC-LCC.                                         
       FIN-PRO-FLD-LCC.                                                 
           EXIT.                                                        
                                                                        
       PRO-LCC-LCC SECTION.                                             
       INI-PRO-LCC-LCC.                                                 
           MOVE FRM-CPIM-AEY TO LCC-COD-VCOC-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-CAI-ILIC-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-IIC-ILIC-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-VAL-MADJ-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-VAL-MUTI-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-VAL-RENU-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-SGV-CORI-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-VAL-MNTO-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-VAL-EXPE-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-VAL-MTO1-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-VAL-MTO2-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-NUM-PPIN-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-IND-PPIN-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-NUM-PUGI-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-IND-PUGI-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-VAL-PREN-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-FEC-FREN-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-FEC-FRNC-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-VAL-UTIL-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-SGV-CBCO-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-VAL-MSEC-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-VAL-MTOS-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-NUM-PCAP-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-IND-PCAP-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-NUM-PPGI-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-IND-PPGI-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-NUM-PPUG-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-IND-PPUG-ATR IN LCC-FLD.            
           MOVE FRM-CPIM-AEY TO LCC-COD-CRIE-ATR IN LCC-FLD.            
       FIN-PRO-LCC-LCC.                                                 
           EXIT.                                                        
                                                                        
       PRO-DBC-LCC SECTION.                                             
       INI-PRO-DBC-LCC.                                                 
       FIN-PRO-DBC-LCC.                                                 
           EXIT.                                                        
                                                                        
       PRO-LDC-LCC SECTION.                                             
       INI-PRO-LDC-LCC.                                                 
       FIN-PRO-LDC-LCC.                                                 
           EXIT.                                                        
                                                                        
       BUS-COR-LCC SECTION.                                             
       INI-BUS-COR-LCC.                                                 
           MOVE 999 TO LCC-NUM-ILCC IN LCC.                             
           MOVE FIO-GET-LEQ TO FIO-CMND.                                
           PERFORM LDC-FIO-LCC.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    LCC-CAI-ICLF IN LCC-FLD = LCC-CAI-ICLF IN LCC AND   
                    LCC-IIC-ICLF IN LCC-FLD = LCC-IIC-ICLF IN LCC )     
               PERFORM KEY-ALL-LCC                                      
               MOVE ZEROES TO LCC-NUM-ILCC IN LCC.                      
           IF SCR-CCPP = 'ING' OR 'ACT'                                 
               ADD 1 TO LCC-NUM-ILCC IN LCC                             
               MOVE LCC-NUM-ILCC IN LCC TO LCC-NUM-ILCC IN LCC-FLD.     
       FIN-BUS-COR-LCC.                                                 
           EXIT.                                                        
                                                                        
                                                                        
                                                                        
      *>>>>>
*% END
*% IF EOF_PU
      *<<<<
       COPY LDCBFLDC.                                                   
       COPY LDCBFCLF.                                                   
       COPY SGCBFDBC.                                                   
      *<<<< PGM_BMS AND EOF                                             
                                                                        
       EGR-FTER-CIC SECTION.                                            
       INI-EGR-FTER-CIC.                                                
           PERFORM CRG-TBL-ENG.                                         
           MOVE WSS-IDX-ILDC TO SCR-IPRO.                               
           MOVE 'S' TO SCR-IND-ENG IN SCR-VEC-ENG(SCR-IPRO).            
           MOVE 'S' TO SCR-IND-FST IN SCR-VEC-FST(SCR-IPRO).            
           MOVE WSS-INST-FPEN-CIC TO SCR-INST-EXT.                      
           MOVE WSS-PROG(SCR-IPRO) TO SYS-PROG.                         
           MOVE SCR-VARI TO SYS-CMMA.                                   
      *<<<< NOT PGM_DB2                                                 
           MOVE SCR-TCMA TO SYS-TCMA.                                   
           MOVE SYS-XCTL TO SYS-CMND.                                   
      *<<<< ISP-INI  24-JAN-1996 11:35:48                               
           IF SCR-OFRM-SRV                                              
               MOVE WSS-IDX-ILCC TO SCR-IPRO                            
           ELSE                                                         
      *<<<< ISP-FIN  24-JAN-1996 11:35:48                               
           PERFORM GNS-PRO-SYS.                                         
      *>>>>                                                             
       FIN-EGR-FTER-CIC.                                                
           EXIT.                                                        
                                                                        
       EGR-FPEN-CIC SECTION.                                            
       INI-EGR-FPEN-CIC.                                                
           PERFORM CRG-TBL-ENG.                                         
           ADD 1 TO SCR-IPRO.                                           
           IF SCR-IPRO > WSS-TOT-IIDX                                   
                  PERFORM EGR-FTER-CIC                                  
                  GO TO FIN-EGR-FPEN-CIC.                               
           IF SCR-IND-ENG IN SCR-VEC-ENG(SCR-IPRO) = SPACES             
               GO TO INI-EGR-FPEN-CIC.                                  
           MOVE WSS-INST-FPEN-CIC TO SCR-INST-EXT.                      
           MOVE WSS-PROG(SCR-IPRO) TO SYS-PROG.                         
           MOVE SCR-VARI TO SYS-CMMA.                                   
      *<<<< NOT PGM_DB2                                                 
           MOVE SCR-TCMA TO SYS-TCMA.                                   
           MOVE SYS-XCTL TO SYS-CMND.                                   
      *<<<< ISP-INI  24-JAN-1996 11:35:48                               
           IF SCR-OFRM-SRV                                              
               MOVE WSS-IDX-ILCC TO SCR-IPRO                            
           ELSE                                                         
      *<<<< ISP-FIN  24-JAN-1996 11:35:48                               
           PERFORM GNS-PRO-SYS.                                         
       FIN-EGR-FPEN-CIC.                                                
           EXIT.                                                        
                                                                        
       SCR-INIT SECTION.                                                
       INI-SCR-INIT.                                                    
           MOVE 3  TO FRM-PLIN.                                         
           MOVE 19 TO FRM-NLIN.                                         
           MOVE FRM-CLR-FRM TO FRM-CMND.                                
           PERFORM GNS-PRO-FRM.                                         
           MOVE FRM-SINI-DBNF TO FRM-SINI.                              
           MOVE SCR-NMAP TO FRM-NAME.                                   
           MOVE SCR-NLIB TO FRM-NLIB.                                   
           MOVE LCC-FLD TO FRM-DFLD.                                    
           MOVE FRM-INI TO FRM-CMND.                                    
           PERFORM GNS-PRO-FRM.                                         
       FIN-SCR-INIT.                                                    
           EXIT.                                                        
                                                                        
       COPY LDCBGMPT.                                                   
                                                                        
      *>>>>
*% END
*% IF FIN_CMN_ING
      *<<<< PGM_BMS                                                     
           IF SCR-IND-ENG IN SCR-VEC-ENG(SCR-IPRO) = 'S'                
               PERFORM EGR-FPEN-CIC.                                    
      *>>>>                                                             
*% END
*% IF INI_FIO_IBM
*% IF PGM_PQ
      *<<<<
           IF SCR-DESDE-CMN                                             
               MOVE SPACES TO SCR-VEC-ENG                               
               MOVE WSS-IDX-ILDC TO SCR-IPRO.                           
           IF SCR-INST-EXT = WSS-INST-FPEN-CIC                          
               MOVE WSS-INST-NULL TO SCR-INST-EXT                       
               MOVE SPACES TO MSG-COD-MENS                              
               PERFORM ING-FPEN-CIC                                     
           ELSE                                                         
           IF SCR-INST-EXT = WSS-INST-FPEN-CIC                          
               MOVE SPACES TO SCR-VEC-ENG                               
               MOVE WSS-IDX-ILCC TO SCR-IPRO                            
               MOVE LOW-VALUES TO LCC-FLD                               
               MOVE 'ING' TO SCR-CCPP SCR-CMND                          
               MOVE SCR-INST-HKY TO SCR-INST                            
               MOVE SPACES TO SCR-IND-FST IN SCR-VEC-FST(SCR-IPRO)      
               PERFORM ING-SETE                                         
               PERFORM SCR-INIT                                         
               MOVE 'C' TO SCR-ICMN                                     
               MOVE ZEROES TO SCR-INST-EXT.                             
      *>>>>
*% END
*% END
*% IF PGM_BMS AND FIN_SCR_GCNF OR FIN_SCR_GFLD
      *<<<< PGM_BMS AND FIN_SCR_GIKY OR FIN_SCR_GCNF OR FIN_SCR_GFLD    
           IF FRM-FFLD = FRM-FFLD-PF3 AND                               
              SCR-IND-ENG IN SCR-VEC-ENG(SCR-IPRO) = 'S'                
               PERFORM EGR-FTER-CIC.                                    
      *>>>>                                                             
*% END
