      *
      * LDCBQCLE
      * Version 3.0.1 Nov-2005
      * 08-May-19 12:08 PM
      *
       SET-TAG-LDCBQCLE SECTION.
       INI-SET-TAG-LDCBQCLE.
           MOVE
           '<IDNMOD NOM=$LDCBQCLE$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$08-May-19 12:08 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-LDCBQCLE.
           EXIT.
           
       PUT-ALL-CLE SECTION.
       INI-PUT-ALL-CLE.
           PERFORM PUT-DBC-CLE.
       FIN-PUT-ALL-CLE.
           EXIT.
       
       PUT-DBC-CLE SECTION.
       INI-PUT-DBC-CLE.
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-CLE.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-CLE
               GO TO FIN-PUT-DBC-CLE.
           MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN CLE-FLD.
      * IDC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN CLE-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN CLE-FLD.
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN CLE-FLD.
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN CLE-FLD.
      * IDC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-CAI-ICLI IN DBC TO DBC-CAI-ICLI IN CLE-FLD.
           MOVE DBC-IIC-ICLI IN DBC TO DBC-IIC-ICLI IN CLE-FLD.
      *<<<< FIN_PUT_DBC_CLE
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.
           PERFORM CPT-BLKS.
           MOVE CPT-STRN            TO DBC-GLS-NOMC IN CLE-FLD.
      *>>>>
       FIN-PUT-DBC-CLE.
           EXIT.
       
       KEY-ALL-CLE SECTION.
       INI-KEY-ALL-CLE.
           PERFORM KEY-DBC-CLE.
       FIN-KEY-ALL-CLE.
           EXIT.
       
       KEY-DBC-CLE SECTION.
       INI-KEY-DBC-CLE.
           MOVE DBC-GLS-NOMC IN CLE-FLD TO DBC-GLS-NOMC IN DBC.
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN CLE-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN CLE-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN CLE-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN CLE-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-CAI-ICLI IN CLE-FLD TO DBC-CAI-ICLI IN DBC.
           MOVE DBC-IIC-ICLI IN CLE-FLD TO DBC-IIC-ICLI IN DBC.
       FIN-KEY-DBC-CLE.
           EXIT.
       
       SCR-CLRA-CLE SECTION.
       INI-SCR-CLRA-CLE.
           PERFORM CLR-KEY-CLE.
           PERFORM CLR-FLD-CLE.
       FIN-SCR-CLRA-CLE.
           EXIT.
       
       CLR-KEY-CLE SECTION.
       INI-CLR-KEY-CLE.
           MOVE SPACES TO DBC-GLS-NOMC IN CLE-FLD.
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN CLE-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN CLE-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN CLE-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN CLE-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-CAI-ISLC IN CLE-FLD.
           MOVE SPACES TO FRM-IIC-ISLC IN CLE-FLD.
           MOVE SPACES TO FRM-IND-TCON IN CLE-FLD.
           MOVE SPACES TO DBC-CAI-ICLI IN CLE-FLD.
           MOVE SPACES TO DBC-IIC-ICLI IN CLE-FLD.
       FIN-CLR-KEY-CLE.
           EXIT.
       
       CLR-FLD-CLE SECTION.
       INI-CLR-FLD-CLE.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FINF IN CLE-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-VAL-MARE IN CLE-FLD.
           MOVE ZEROES TO FRM-VAL-APRE IN CLE-FLD.
           MOVE ZEROES TO FRM-VAL-UTIE IN CLE-FLD.
           MOVE ZEROES TO FRM-VAL-DISE IN CLE-FLD.
           MOVE ZEROES TO FRM-VAL-GARE IN CLE-FLD.
           MOVE ZEROES TO FRM-SGV-MGRP IN CLE-FLD.
           MOVE ZEROES TO FRM-SGV-UTRP IN CLE-FLD.
           MOVE SPACES TO FRM-GLS-NTLC IN CLE-FLD(1).
           MOVE SPACES TO FRM-GLS-NTLC IN CLE-FLD(2).
           MOVE SPACES TO FRM-GLS-NTLC IN CLE-FLD(3).
           MOVE SPACES TO FRM-GLS-NTLC IN CLE-FLD(4).
           MOVE SPACES TO FRM-GLS-NTLC IN CLE-FLD(5).
           MOVE ZEROES TO FRM-VAL-CAPR IN CLE-FLD(1).
           MOVE ZEROES TO FRM-VAL-CAPR IN CLE-FLD(2).
           MOVE ZEROES TO FRM-VAL-CAPR IN CLE-FLD(3).
           MOVE ZEROES TO FRM-VAL-CAPR IN CLE-FLD(4).
           MOVE ZEROES TO FRM-VAL-CAPR IN CLE-FLD(5).
           MOVE ZEROES TO FRM-VAL-CUTI IN CLE-FLD(1).
           MOVE ZEROES TO FRM-VAL-CUTI IN CLE-FLD(2).
           MOVE ZEROES TO FRM-VAL-CUTI IN CLE-FLD(3).
           MOVE ZEROES TO FRM-VAL-CUTI IN CLE-FLD(4).
           MOVE ZEROES TO FRM-VAL-CUTI IN CLE-FLD(5).
           MOVE ZEROES TO FRM-VAL-CDIS IN CLE-FLD(1).
           MOVE ZEROES TO FRM-VAL-CDIS IN CLE-FLD(2).
           MOVE ZEROES TO FRM-VAL-CDIS IN CLE-FLD(3).
           MOVE ZEROES TO FRM-VAL-CDIS IN CLE-FLD(4).
           MOVE ZEROES TO FRM-VAL-CDIS IN CLE-FLD(5).
           PERFORM CLR-DBC-CLE.
       FIN-CLR-FLD-CLE.
           EXIT.
       
       CLR-DBC-CLE SECTION.
       INI-CLR-DBC-CLE.
       FIN-CLR-DBC-CLE.
           EXIT.
       
       FST-KEY-CLE SECTION.
       INI-FST-KEY-CLE.
      *<<<< FIN_FST_KEY_CLE
           MOVE -1 TO DBC-NUM-ICLI-LEN IN CLE-FLD.
      *>>>>
       FIN-FST-KEY-CLE.
           EXIT.
       
       FST-FLD-CLE SECTION.
       INI-FST-FLD-CLE.
       FIN-FST-FLD-CLE.
           EXIT.
       
       PRO-KEY-CLE SECTION.
       INI-PRO-KEY-CLE.
           MOVE FRM-CPIM-AEY TO FRM-CAI-ISLC-ATR IN CLE-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IIC-ISLC-ATR IN CLE-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-TCON-ATR IN CLE-FLD.
           PERFORM PRO-IKY-CLE.
           PERFORM PRO-AKY-CLE.
           PERFORM PRO-MKY-CLE.
       FIN-PRO-KEY-CLE.
           EXIT.
       
       PRO-IKY-CLE SECTION.
       INI-PRO-IKY-CLE.
       FIN-PRO-IKY-CLE.
           EXIT.
       
       PRO-MKY-CLE SECTION.
       INI-PRO-MKY-CLE.
       FIN-PRO-MKY-CLE.
           EXIT.
       
       PRO-AKY-CLE SECTION.
       INI-PRO-AKY-CLE.
           MOVE FRM-CPIM-AEY TO DBC-GLS-NOMC-ATR IN CLE-FLD.
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO DBC-NUM-ICLI-ATR IN CLE-FLD.
           MOVE FRM-CPIM-AEY TO DBC-VRF-ICLI-ATR IN CLE-FLD.
           MOVE FRM-CPIM-AEY TO DBC-IND-ICLI-ATR IN CLE-FLD.
           MOVE FRM-CPIM-AEY TO DBC-GLS-ICLI-ATR IN CLE-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-CAI-ISLC-ATR IN CLE-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IIC-ISLC-ATR IN CLE-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-TCON-ATR IN CLE-FLD.
       FIN-PRO-AKY-CLE.
           EXIT.
       
       UNP-KEY-CLE SECTION.
       INI-UNP-KEY-CLE.
           MOVE FRM-CPIM-UBY TO FRM-CAI-ISLC-ATR IN CLE-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IIC-ISLC-ATR IN CLE-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IND-TCON-ATR IN CLE-FLD.
           PERFORM UNP-IKY-CLE.
           PERFORM UNP-AKY-CLE.
           PERFORM UNP-MKY-CLE.
       FIN-UNP-KEY-CLE.
           EXIT.
       
       UNP-IKY-CLE SECTION.
       INI-UNP-IKY-CLE.
       FIN-UNP-IKY-CLE.
           EXIT.
       
       UNP-MKY-CLE SECTION.
       INI-UNP-MKY-CLE.
       FIN-UNP-MKY-CLE.
           EXIT.
       
       UNP-AKY-CLE SECTION.
       INI-UNP-AKY-CLE.
           MOVE FRM-CPIM-UBY TO DBC-GLS-NOMC-ATR.
      * IDC-INI --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO DBC-NUM-ICLI-ATR IN CLE-FLD.
           MOVE FRM-CPIM-UBY TO DBC-VRF-ICLI-ATR IN CLE-FLD.
           MOVE FRM-CPIM-UBY TO DBC-IND-ICLI-ATR IN CLE-FLD.
           MOVE FRM-CPIM-UBY TO DBC-GLS-ICLI-ATR IN CLE-FLD.
      * IDC-FIN --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO FRM-CAI-ISLC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-IIC-ISLC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-IND-TCON-ATR.
       FIN-UNP-AKY-CLE.
           EXIT.
       
       UNP-FLD-CLE SECTION.
       INI-UNP-FLD-CLE.
           PERFORM UNP-DBC-CLE.
       FIN-UNP-FLD-CLE.
           EXIT.
       
       UNP-DBC-CLE SECTION.
       INI-UNP-DBC-CLE.
       FIN-UNP-DBC-CLE.
           EXIT.
       
       VAL-KEY-CLE SECTION.
       INI-VAL-KEY-CLE.
           PERFORM VAL-IKY-CLE.
           PERFORM VAL-MKY-CLE.
           PERFORM VAL-AKY-CLE.
       FIN-VAL-KEY-CLE.
           EXIT.
       
       VAL-IKY-CLE SECTION.
       INI-VAL-IKY-CLE.
       FIN-VAL-IKY-CLE.
           EXIT.
       
       VAL-MKY-CLE SECTION.
       INI-VAL-MKY-CLE.
       FIN-VAL-MKY-CLE.
           EXIT.
       
       VAL-AKY-CLE SECTION.
       INI-VAL-AKY-CLE.
           PERFORM VAL-DBC-GLS-NOMC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DBC-GLS-NOMC-LEN IN CLE-FLD
                GO TO FIN-VAL-AKY-CLE.
      * VRF-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * VRF-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * IDC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DBC-IND-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-IND-ICLI-LEN IN CLE-FLD
                GO TO FIN-VAL-AKY-CLE.
           PERFORM VAL-DBC-GLS-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-GLS-ICLI-LEN IN CLE-FLD
                GO TO FIN-VAL-AKY-CLE.
           PERFORM VAL-DBC-NUM-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-NUM-ICLI-LEN IN CLE-FLD
                GO TO FIN-VAL-AKY-CLE.
           PERFORM VAL-DBC-VRF-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-VRF-ICLI-LEN IN CLE-FLD
                GO TO FIN-VAL-AKY-CLE.
      * IDC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-CAI-ISLC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-ISLC-LEN IN CLE-FLD
                GO TO FIN-VAL-AKY-CLE.
           PERFORM VAL-FRM-IIC-ISLC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-ISLC-LEN IN CLE-FLD
                GO TO FIN-VAL-AKY-CLE.
           PERFORM VAL-FRM-IND-TCON.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IND-TCON-LEN IN CLE-FLD
                GO TO FIN-VAL-AKY-CLE.
           PERFORM VAL-DBC-CAI-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DBC-CAI-ICLI-LEN IN CLE-FLD
                GO TO FIN-VAL-AKY-CLE.
           PERFORM VAL-DBC-IIC-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DBC-IIC-ICLI-LEN IN CLE-FLD
                GO TO FIN-VAL-AKY-CLE.
       FIN-VAL-AKY-CLE.
           EXIT.
       
       VAL-DBC-GLS-NOMC SECTION.
       INI-VAL-DBC-GLS-NOMC.
           IF DBC-GLS-NOMC IN CLE-FLD NOT > SPACES
              MOVE SPACES TO DBC-GLS-NOMC IN CLE-FLD.
       FIN-VAL-DBC-GLS-NOMC.
           EXIT.
      * VRF-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
      * VRF-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
      * IDC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION

       VAL-DBC-NUM-ICLI SECTION.
       INI-VAL-DBC-NUM-ICLI.
       FIN-VAL-DBC-NUM-ICLI.
           EXIT.

       VAL-DBC-VRF-ICLI SECTION.
       INI-VAL-DBC-VRF-ICLI.
           IF DBC-VRF-ICLI IN CLE-FLD NOT > SPACES
              MOVE SPACES TO DBC-VRF-ICLI IN CLE-FLD.
      *VAL-RUT Valida Rut para campo IDC
           MOVE IDC-ZERO-S TO IDC-ZERO.
           MOVE DBC-NUM-ICLI IN CLE-FLD TO IDC-NUM-RUTV.
           MOVE DBC-VRF-ICLI IN CLE-FLD TO IDC-VRF-RUTV.
           PERFORM VAL-RUT.
       FIN-VAL-DBC-VRF-ICLI.
           EXIT.
       VAL-DBC-IND-ICLI SECTION.
       INI-VAL-DBC-IND-ICLI.
           IF DBC-IND-ICLI IN CLE-FLD NOT > SPACES
              MOVE SPACES TO DBC-IND-ICLI IN CLE-FLD.
      *VAL-IDC Valida identificador
           MOVE DBC-IND-ICLI IN CLE-FLD TO IDC-IND-RUTV.
           PERFORM VAL-IDC.
           MOVE IDC-IND-RUTV TO DBC-IND-ICLI IN CLE-FLD.
       FIN-VAL-DBC-IND-ICLI.
           EXIT.

       VAL-DBC-GLS-ICLI SECTION.
       INI-VAL-DBC-GLS-ICLI.
           IF DBC-GLS-ICLI IN CLE-FLD NOT > IDC-INIC-GLS 
              MOVE IDC-INIC-GLS TO DBC-GLS-ICLI IN CLE-FLD.
      *VAL-GLS para IDC valida glosa ( extension ) de identificador
           MOVE DBC-GLS-ICLI IN CLE-FLD TO IDC-GLS-RUTV.
           PERFORM VAL-GLS.
           MOVE IDC-GLS-RUTV TO DBC-GLS-ICLI IN CLE-FLD.
       FIN-VAL-DBC-GLS-ICLI.
           EXIT.

      * IDC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-FRM-CAI-ISLC SECTION.
       INI-VAL-FRM-CAI-ISLC.
           IF FRM-CAI-ISLC IN CLE-FLD NOT > SPACES
              MOVE SPACES TO FRM-CAI-ISLC IN CLE-FLD.
       FIN-VAL-FRM-CAI-ISLC.
           EXIT.
       
       VAL-FRM-IIC-ISLC SECTION.
       INI-VAL-FRM-IIC-ISLC.
           IF FRM-IIC-ISLC IN CLE-FLD NOT > SPACES
              MOVE SPACES TO FRM-IIC-ISLC IN CLE-FLD.
       FIN-VAL-FRM-IIC-ISLC.
           EXIT.
       
       VAL-FRM-IND-TCON SECTION.
       INI-VAL-FRM-IND-TCON.
           IF FRM-IND-TCON IN CLE-FLD NOT > SPACES
              MOVE SPACES TO FRM-IND-TCON IN CLE-FLD.
       FIN-VAL-FRM-IND-TCON.
           EXIT.
       
       VAL-DBC-CAI-ICLI SECTION.
       INI-VAL-DBC-CAI-ICLI.
       FIN-VAL-DBC-CAI-ICLI.
           EXIT.
       
       VAL-DBC-IIC-ICLI SECTION.
       INI-VAL-DBC-IIC-ICLI.
       FIN-VAL-DBC-IIC-ICLI.
           EXIT.
       
       VAL-CON-KEY-CLE SECTION.
       INI-VAL-CON-KEY-CLE.
           IF     DBC-GLS-NOMC IN CLE-FLD NOT > SPACES 
      * IDC-INI --> BMCHIBCI009.SRCAIU VAL-CON
              AND DBC-NUM-ICLI IN CLE-FLD NOT > ZEROES
              AND DBC-VRF-ICLI IN CLE-FLD NOT > SPACES
              AND DBC-IND-ICLI IN CLE-FLD NOT > SPACES
              AND DBC-GLS-ICLI IN CLE-FLD NOT > IDC-INIC-GLS
      * IDC-FIN --> BMCHIBCI009.SRCAIU VAL-CON
              AND FRM-CAI-ISLC IN CLE-FLD NOT > SPACES 
              AND FRM-IIC-ISLC IN CLE-FLD NOT > SPACES 
              AND FRM-IND-TCON IN CLE-FLD NOT > SPACES 
              AND DBC-CAI-ICLI IN CLE-FLD NOT > SPACES 
              AND DBC-IIC-ICLI IN CLE-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CLE.
       FIN-VAL-CON-KEY-CLE.
           EXIT.
       
       VAL-CON-IKY-CLE SECTION.
       INI-VAL-CON-IKY-CLE.
                PERFORM ERR-KEY
                PERFORM FST-KEY-CLE.
       FIN-VAL-CON-IKY-CLE.
           EXIT.
       
       VAL-NUM-KEY-CLE SECTION.
       INI-VAL-NUM-KEY-CLE.
           PERFORM VAL-NUM-IKY-CLE.
           PERFORM VAL-NUM-MKY-CLE.
           PERFORM VAL-NUM-AKY-CLE.
       FIN-VAL-NUM-KEY-CLE.
           EXIT.
       
       VAL-NUM-IKY-CLE SECTION.
       INI-VAL-NUM-IKY-CLE.
       FIN-VAL-NUM-IKY-CLE.
           EXIT.
       
       VAL-NUM-MKY-CLE SECTION.
       INI-VAL-NUM-MKY-CLE.
       FIN-VAL-NUM-MKY-CLE.
           EXIT.
       
       VAL-NUM-AKY-CLE SECTION.
       INI-VAL-NUM-AKY-CLE.
      * IDC-INI --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
       VAL-NUM-DBC-NUM-ICLI.
           MOVE DBC-NUM-ICLI-ALF IN CLE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO DBC-NUM-ICLI IN CLE-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO DBC-NUM-ICLI-LEN IN CLE-FLD
                GO TO FIN-VAL-NUM-AKY-CLE.
      * IDC-FIN --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
       FIN-VAL-NUM-AKY-CLE.
           EXIT.
       
       VAL-NUM-FLD-CLE SECTION.
       INI-VAL-NUM-FLD-CLE.
       VAL-NUM-FRM-VAL-MARE.
           MOVE FRM-VAL-MARE-ALF IN CLE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO FRM-VAL-MARE IN CLE-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-VAL-MARE-LEN IN CLE-FLD
                GO TO FIN-VAL-NUM-FLD-CLE.
       VAL-NUM-FRM-VAL-APRE.
           MOVE FRM-VAL-APRE-ALF IN CLE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO FRM-VAL-APRE IN CLE-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-VAL-APRE-LEN IN CLE-FLD
                GO TO FIN-VAL-NUM-FLD-CLE.
       VAL-NUM-FRM-VAL-UTIE.
           MOVE FRM-VAL-UTIE-ALF IN CLE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO FRM-VAL-UTIE IN CLE-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-VAL-UTIE-LEN IN CLE-FLD
                GO TO FIN-VAL-NUM-FLD-CLE.
       VAL-NUM-FRM-VAL-DISE.
           MOVE FRM-VAL-DISE-ALF IN CLE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NS9N TO FRM-VAL-DISE IN CLE-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-VAL-DISE-LEN IN CLE-FLD
                GO TO FIN-VAL-NUM-FLD-CLE.
       VAL-NUM-FRM-VAL-GARE.
           MOVE FRM-VAL-GARE-ALF IN CLE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO FRM-VAL-GARE IN CLE-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-VAL-GARE-LEN IN CLE-FLD
                GO TO FIN-VAL-NUM-FLD-CLE.
       VAL-NUM-FRM-SGV-MGRP.
