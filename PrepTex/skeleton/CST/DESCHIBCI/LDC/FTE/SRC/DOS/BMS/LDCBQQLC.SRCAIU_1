       SET-TAG-LDCBQQLC SECTION.
       INI-SET-TAG-LDCBQQLC.
           MOVE
           '<IDNMOD NOM=$LDCBQQLC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$08-May-19 12:11 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-LDCBQQLC.
           EXIT.
           
       PUT-ALL-QLC SECTION.
       INI-PUT-ALL-QLC.
           PERFORM PUT-LDC-QLC.
       FIN-PUT-ALL-QLC.
           EXIT.
       
       PUT-LDC-QLC SECTION.
       INI-PUT-LDC-QLC.
           IF PGM-STAT-LDC-NUL
               GO TO FIN-PUT-LDC-QLC.
           IF NOT PGM-STAT-LDC-OKS
               PERFORM CLR-LDC-QLC
               GO TO FIN-PUT-LDC-QLC.
           MOVE LDC-CAI-ICLF IN LDC TO LDC-CAI-ICLF IN QLC-FLD.
           MOVE LDC-IIC-ICLF IN LDC TO LDC-IIC-ICLF IN QLC-FLD.
       FIN-PUT-LDC-QLC.
           EXIT.
       
       KEY-ALL-QLC SECTION.
       INI-KEY-ALL-QLC.
           PERFORM KEY-LDC-QLC.
       FIN-KEY-ALL-QLC.
           EXIT.
       
       KEY-LDC-QLC SECTION.
       INI-KEY-LDC-QLC.
           MOVE LDC-CAI-ICLF IN QLC-FLD TO LDC-CAI-ICLF IN LDC.
           MOVE LDC-IIC-ICLF IN QLC-FLD TO LDC-IIC-ICLF IN LDC.
       FIN-KEY-LDC-QLC.
           EXIT.
       
       SCR-CLRA-QLC SECTION.
       INI-SCR-CLRA-QLC.
           PERFORM CLR-KEY-QLC.
           PERFORM CLR-FLD-QLC.
       FIN-SCR-CLRA-QLC.
           EXIT.
       
       CLR-KEY-QLC SECTION.
       INI-CLR-KEY-QLC.
           MOVE SPACES TO LDC-CAI-ICLF IN QLC-FLD.
           MOVE ZEROES TO LDC-IIC-ICLF IN QLC-FLD.
           MOVE SPACES TO FRM-IND-HMAS IN QLC-FLD.
           MOVE SPACES TO FRM-CAI-HCLF IN QLC-FLD.
           MOVE ZEROES TO FRM-IIC-HCLF IN QLC-FLD.
           MOVE ZEROES TO FRM-NUM-HLDC IN QLC-FLD.
       FIN-CLR-KEY-QLC.
           EXIT.
       
       CLR-FLD-QLC SECTION.
       INI-CLR-FLD-QLC.
           MOVE SPACES TO FRM-GLS-NOMC IN QLC-FLD.
           MOVE ZEROES TO FRM-NUM-ILDC IN QLC-FLD(1).
           MOVE ZEROES TO FRM-NUM-ILDC IN QLC-FLD(2).
           MOVE ZEROES TO FRM-NUM-ILDC IN QLC-FLD(3).
           MOVE ZEROES TO FRM-NUM-ILDC IN QLC-FLD(4).
           MOVE ZEROES TO FRM-NUM-ILDC IN QLC-FLD(5).
           MOVE ZEROES TO FRM-NUM-ILDC IN QLC-FLD(6).
           MOVE SPACES TO FRM-GLS-STAT IN QLC-FLD(1).
           MOVE SPACES TO FRM-GLS-STAT IN QLC-FLD(2).
           MOVE SPACES TO FRM-GLS-STAT IN QLC-FLD(3).
           MOVE SPACES TO FRM-GLS-STAT IN QLC-FLD(4).
           MOVE SPACES TO FRM-GLS-STAT IN QLC-FLD(5).
           MOVE SPACES TO FRM-GLS-STAT IN QLC-FLD(6).
           MOVE SPACES TO FRM-COD-TIPO IN QLC-FLD(1).
           MOVE SPACES TO FRM-COD-TIPO IN QLC-FLD(2).
           MOVE SPACES TO FRM-COD-TIPO IN QLC-FLD(3).
           MOVE SPACES TO FRM-COD-TIPO IN QLC-FLD(4).
           MOVE SPACES TO FRM-COD-TIPO IN QLC-FLD(5).
           MOVE SPACES TO FRM-COD-TIPO IN QLC-FLD(6).
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FVEN IN QLC-FLD(1).
           MOVE ZEROES TO FRM-FEC-FVEN IN QLC-FLD(2).
           MOVE ZEROES TO FRM-FEC-FVEN IN QLC-FLD(3).
           MOVE ZEROES TO FRM-FEC-FVEN IN QLC-FLD(4).
           MOVE ZEROES TO FRM-FEC-FVEN IN QLC-FLD(5).
           MOVE ZEROES TO FRM-FEC-FVEN IN QLC-FLD(6).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-COD-VCLC IN QLC-FLD(1).
           MOVE SPACES TO FRM-COD-VCLC IN QLC-FLD(2).
           MOVE SPACES TO FRM-COD-VCLC IN QLC-FLD(3).
           MOVE SPACES TO FRM-COD-VCLC IN QLC-FLD(4).
           MOVE SPACES TO FRM-COD-VCLC IN QLC-FLD(5).
           MOVE SPACES TO FRM-COD-VCLC IN QLC-FLD(6).
           MOVE ZEROES TO FRM-MTO-PROV IN QLC-FLD(1).
           MOVE ZEROES TO FRM-MTO-PROV IN QLC-FLD(2).
           MOVE ZEROES TO FRM-MTO-PROV IN QLC-FLD(3).
           MOVE ZEROES TO FRM-MTO-PROV IN QLC-FLD(4).
           MOVE ZEROES TO FRM-MTO-PROV IN QLC-FLD(5).
           MOVE ZEROES TO FRM-MTO-PROV IN QLC-FLD(6).
           MOVE ZEROES TO FRM-VAL-CMAX IN QLC-FLD(1).
           MOVE ZEROES TO FRM-VAL-CMAX IN QLC-FLD(2).
           MOVE ZEROES TO FRM-VAL-CMAX IN QLC-FLD(3).
           MOVE ZEROES TO FRM-VAL-CMAX IN QLC-FLD(4).
           MOVE ZEROES TO FRM-VAL-CMAX IN QLC-FLD(5).
           MOVE ZEROES TO FRM-VAL-CMAX IN QLC-FLD(6).
           MOVE ZEROES TO FRM-VAL-MUTI IN QLC-FLD(1).
           MOVE ZEROES TO FRM-VAL-MUTI IN QLC-FLD(2).
           MOVE ZEROES TO FRM-VAL-MUTI IN QLC-FLD(3).
           MOVE ZEROES TO FRM-VAL-MUTI IN QLC-FLD(4).
           MOVE ZEROES TO FRM-VAL-MUTI IN QLC-FLD(5).
           MOVE ZEROES TO FRM-VAL-MUTI IN QLC-FLD(6).
           MOVE ZEROES TO FRM-MTO-UTIL IN QLC-FLD(1).
           MOVE ZEROES TO FRM-MTO-UTIL IN QLC-FLD(2).
           MOVE ZEROES TO FRM-MTO-UTIL IN QLC-FLD(3).
           MOVE ZEROES TO FRM-MTO-UTIL IN QLC-FLD(4).
           MOVE ZEROES TO FRM-MTO-UTIL IN QLC-FLD(5).
           MOVE ZEROES TO FRM-MTO-UTIL IN QLC-FLD(6).
           MOVE ZEROES TO FRM-VAL-DISP IN QLC-FLD(1).
           MOVE ZEROES TO FRM-VAL-DISP IN QLC-FLD(2).
           MOVE ZEROES TO FRM-VAL-DISP IN QLC-FLD(3).
           MOVE ZEROES TO FRM-VAL-DISP IN QLC-FLD(4).
           MOVE ZEROES TO FRM-VAL-DISP IN QLC-FLD(5).
           MOVE ZEROES TO FRM-VAL-DISP IN QLC-FLD(6).
           MOVE ZEROES TO FRM-VAL-MADJ IN QLC-FLD(1).
           MOVE ZEROES TO FRM-VAL-MADJ IN QLC-FLD(2).
           MOVE ZEROES TO FRM-VAL-MADJ IN QLC-FLD(3).
           MOVE ZEROES TO FRM-VAL-MADJ IN QLC-FLD(4).
           MOVE ZEROES TO FRM-VAL-MADJ IN QLC-FLD(5).
           MOVE ZEROES TO FRM-VAL-MADJ IN QLC-FLD(6).
           PERFORM CLR-LDC-QLC.
       FIN-CLR-FLD-QLC.
           EXIT.
       
       CLR-LDC-QLC SECTION.
       INI-CLR-LDC-QLC.
       FIN-CLR-LDC-QLC.
           EXIT.
       
       FST-KEY-QLC SECTION.
       INI-FST-KEY-QLC.
           MOVE -1 TO LDC-CAI-ICLF-LEN IN QLC-FLD.
      *<<<< FIN_FST_KEY_QLC
           MOVE -1 TO LDC-CAI-ICLF-LEN IN QLC-FLD.
      *>>>>
       FIN-FST-KEY-QLC.
           EXIT.
       
       FST-FLD-QLC SECTION.
       INI-FST-FLD-QLC.
           MOVE -1 TO FRM-IND-HMAS-LEN IN QLC-FLD.
       FIN-FST-FLD-QLC.
           EXIT.
       
       PRO-KEY-QLC SECTION.
       INI-PRO-KEY-QLC.
           MOVE FRM-CPIM-AEY TO FRM-IND-HMAS-ATR IN QLC-FLD.
           MOVE FRM-CPIM-AEY TO FRM-CAI-HCLF-ATR IN QLC-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IIC-HCLF-ATR IN QLC-FLD.
           MOVE FRM-CPIM-AEY TO FRM-NUM-HLDC-ATR IN QLC-FLD.
           PERFORM PRO-IKY-QLC.
           PERFORM PRO-AKY-QLC.
           PERFORM PRO-MKY-QLC.
       FIN-PRO-KEY-QLC.
           EXIT.
       
       PRO-IKY-QLC SECTION.
       INI-PRO-IKY-QLC.
           MOVE FRM-CPIM-AEY TO LDC-CAI-ICLF-ATR IN QLC-FLD.
           MOVE FRM-CPIM-AEY TO LDC-IIC-ICLF-ATR IN QLC-FLD.
       FIN-PRO-IKY-QLC.
           EXIT.
       
       PRO-MKY-QLC SECTION.
       INI-PRO-MKY-QLC.
       FIN-PRO-MKY-QLC.
           EXIT.
       
       PRO-AKY-QLC SECTION.
       INI-PRO-AKY-QLC.
       FIN-PRO-AKY-QLC.
           EXIT.
       
       UNP-KEY-QLC SECTION.
       INI-UNP-KEY-QLC.
           MOVE FRM-CPIM-UBY TO FRM-IND-HMAS-ATR IN QLC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-CAI-HCLF-ATR IN QLC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IIC-HCLF-ATR IN QLC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-NUM-HLDC-ATR IN QLC-FLD.
           PERFORM UNP-IKY-QLC.
           PERFORM UNP-AKY-QLC.
           PERFORM UNP-MKY-QLC.
       FIN-UNP-KEY-QLC.
           EXIT.
       
       UNP-IKY-QLC SECTION.
       INI-UNP-IKY-QLC.
           MOVE FRM-CPIM-UBY TO LDC-CAI-ICLF-ATR.
           MOVE FRM-CPIM-UBY TO LDC-IIC-ICLF-ATR.
       FIN-UNP-IKY-QLC.
           EXIT.
       
       UNP-MKY-QLC SECTION.
       INI-UNP-MKY-QLC.
           MOVE FRM-CPIM-UBY TO FRM-IND-HMAS-ATR.
           MOVE FRM-CPIM-UBY TO FRM-CAI-HCLF-ATR.
           MOVE FRM-CPIM-UBY TO FRM-IIC-HCLF-ATR.
           MOVE FRM-CPIM-UBY TO FRM-NUM-HLDC-ATR.
       FIN-UNP-MKY-QLC.
           EXIT.
       
       UNP-AKY-QLC SECTION.
       INI-UNP-AKY-QLC.
       FIN-UNP-AKY-QLC.
           EXIT.
       
       UNP-FLD-QLC SECTION.
       INI-UNP-FLD-QLC.
           PERFORM UNP-LDC-QLC.
       FIN-UNP-FLD-QLC.
           EXIT.
       
       UNP-LDC-QLC SECTION.
       INI-UNP-LDC-QLC.
       FIN-UNP-LDC-QLC.
           EXIT.
       
       VAL-KEY-QLC SECTION.
       INI-VAL-KEY-QLC.
           PERFORM VAL-IKY-QLC.
           PERFORM VAL-MKY-QLC.
           PERFORM VAL-AKY-QLC.
       FIN-VAL-KEY-QLC.
           EXIT.
       
       VAL-IKY-QLC SECTION.
       INI-VAL-IKY-QLC.
           PERFORM VAL-LDC-CAI-ICLF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO LDC-CAI-ICLF-LEN IN QLC-FLD
                GO TO FIN-VAL-IKY-QLC.
           PERFORM VAL-LDC-IIC-ICLF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO LDC-IIC-ICLF-LEN IN QLC-FLD
                GO TO FIN-VAL-IKY-QLC.
       FIN-VAL-IKY-QLC.
           EXIT.
       
       VAL-MKY-QLC SECTION.
       INI-VAL-MKY-QLC.
           PERFORM VAL-FRM-IND-HMAS.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IND-HMAS-LEN IN QLC-FLD
                GO TO FIN-VAL-MKY-QLC.
           PERFORM VAL-FRM-CAI-HCLF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-HCLF-LEN IN QLC-FLD
                GO TO FIN-VAL-MKY-QLC.
           PERFORM VAL-FRM-IIC-HCLF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-HCLF-LEN IN QLC-FLD
                GO TO FIN-VAL-MKY-QLC.
           PERFORM VAL-FRM-NUM-HLDC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-NUM-HLDC-LEN IN QLC-FLD
                GO TO FIN-VAL-MKY-QLC.
       FIN-VAL-MKY-QLC.
           EXIT.
       
       VAL-AKY-QLC SECTION.
       INI-VAL-AKY-QLC.
       FIN-VAL-AKY-QLC.
           EXIT.
       
       VAL-LDC-CAI-ICLF SECTION.
       INI-VAL-LDC-CAI-ICLF.
           IF LDC-CAI-ICLF IN QLC-FLD NOT > SPACES
              MOVE SPACES TO LDC-CAI-ICLF IN QLC-FLD.
       FIN-VAL-LDC-CAI-ICLF.
           EXIT.
       
       VAL-LDC-IIC-ICLF SECTION.
       INI-VAL-LDC-IIC-ICLF.
      *<<<< LDC_IIC_ICLF
           IF MSG-COD-MENS > SPACES
              GO TO FIN-VAL-LDC-IIC-ICLF
           ELSE
               MOVE LDC-CAI-ICLF IN QLC-FLD TO CLF-CAI-ICLF IN CLF
               MOVE LDC-IIC-ICLF IN QLC-FLD TO CLF-IIC-ICLF IN CLF
               MOVE 'CLF-KEY-ICLF'  TO FIO-AKEY
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM LDC-FIO-CLF
               IF NOT FIO-STAT-OKS
      *             MOVE 'CLF'        TO MSG-COD-SIST
                   MOVE 'DEU'        TO MSG-COD-SIST
                   MOVE 'CLF    NEX' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE SPACES       TO FRM-GLS-NOMC IN QLC-FLD
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   GO TO FIN-VAL-LDC-IIC-ICLF
               ELSE               
                   MOVE CLF-CIC-ICLF IN CLF TO SCR-CIC-ICLF.
      *>>>>
       FIN-VAL-LDC-IIC-ICLF.
           EXIT.
       
       VAL-FRM-IND-HMAS SECTION.
       INI-VAL-FRM-IND-HMAS.
           IF FRM-IND-HMAS IN QLC-FLD NOT > SPACES
              MOVE SPACES TO FRM-IND-HMAS IN QLC-FLD.
       FIN-VAL-FRM-IND-HMAS.
           EXIT.
       
       VAL-FRM-CAI-HCLF SECTION.
       INI-VAL-FRM-CAI-HCLF.
           IF FRM-CAI-HCLF IN QLC-FLD NOT > SPACES
              MOVE SPACES TO FRM-CAI-HCLF IN QLC-FLD.
       FIN-VAL-FRM-CAI-HCLF.
           EXIT.
       
       VAL-FRM-IIC-HCLF SECTION.
       INI-VAL-FRM-IIC-HCLF.
       FIN-VAL-FRM-IIC-HCLF.
           EXIT.
       
       VAL-FRM-NUM-HLDC SECTION.
       INI-VAL-FRM-NUM-HLDC.
       FIN-VAL-FRM-NUM-HLDC.
           EXIT.
       
       VAL-CON-KEY-QLC SECTION.
       INI-VAL-CON-KEY-QLC.
           IF     LDC-CAI-ICLF IN QLC-FLD NOT > SPACES 
              AND LDC-IIC-ICLF IN QLC-FLD NOT > ZEROES 
              AND FRM-IND-HMAS IN QLC-FLD NOT > SPACES 
              AND FRM-CAI-HCLF IN QLC-FLD NOT > SPACES 
              AND FRM-IIC-HCLF IN QLC-FLD NOT > ZEROES 
              AND FRM-NUM-HLDC IN QLC-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-QLC.
       FIN-VAL-CON-KEY-QLC.
           EXIT.
       
       VAL-CON-IKY-QLC SECTION.
       INI-VAL-CON-IKY-QLC.
           IF     LDC-CAI-ICLF IN QLC-FLD NOT > SPACES 
              AND LDC-IIC-ICLF IN QLC-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-QLC.
       FIN-VAL-CON-IKY-QLC.
           EXIT.
       
       VAL-NUM-KEY-QLC SECTION.
       INI-VAL-NUM-KEY-QLC.
           PERFORM VAL-NUM-IKY-QLC.
           PERFORM VAL-NUM-MKY-QLC.
           PERFORM VAL-NUM-AKY-QLC.
       FIN-VAL-NUM-KEY-QLC.
           EXIT.
       
       VAL-NUM-IKY-QLC SECTION.
       INI-VAL-NUM-IKY-QLC.
       VAL-NUM-LDC-IIC-ICLF.
           MOVE LDC-IIC-ICLF-ALF IN QLC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO LDC-IIC-ICLF IN QLC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO LDC-IIC-ICLF-LEN IN QLC-FLD
                GO TO FIN-VAL-NUM-IKY-QLC.
       FIN-VAL-NUM-IKY-QLC.
           EXIT.
       
       VAL-NUM-MKY-QLC SECTION.
       INI-VAL-NUM-MKY-QLC.
       VAL-NUM-FRM-IIC-HCLF.
           MOVE FRM-IIC-HCLF-ALF IN QLC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO FRM-IIC-HCLF IN QLC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-IIC-HCLF-LEN IN QLC-FLD
                GO TO FIN-VAL-NUM-MKY-QLC.
       VAL-NUM-FRM-NUM-HLDC.
           MOVE FRM-NUM-HLDC-ALF IN QLC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 2 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO FRM-NUM-HLDC IN QLC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-NUM-HLDC-LEN IN QLC-FLD
                GO TO FIN-VAL-NUM-MKY-QLC.
       FIN-VAL-NUM-MKY-QLC.
           EXIT.
       
       VAL-NUM-AKY-QLC SECTION.
       INI-VAL-NUM-AKY-QLC.
       FIN-VAL-NUM-AKY-QLC.
           EXIT.
       
       VAL-NUM-FLD-QLC SECTION.
       INI-VAL-NUM-FLD-QLC.
       VAL-NUM-FRM-NUM-ILDC-1.
           MOVE FRM-NUM-ILDC-ALF IN QLC-FLD(1) TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 2 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO FRM-NUM-ILDC IN QLC-FLD(1)
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-NUM-ILDC-LEN IN QLC-FLD(1)
                GO TO FIN-VAL-NUM-FLD-QLC.
       VAL-NUM-FRM-NUM-ILDC-2.
           MOVE FRM-NUM-ILDC-ALF IN QLC-FLD(2) TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 2 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO FRM-NUM-ILDC IN QLC-FLD(2)
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-NUM-ILDC-LEN IN QLC-FLD(2)
                GO TO FIN-VAL-NUM-FLD-QLC.
       VAL-NUM-FRM-NUM-ILDC-3.
           MOVE FRM-NUM-ILDC-ALF IN QLC-FLD(3) TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 2 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO FRM-NUM-ILDC IN QLC-FLD(3)
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-NUM-ILDC-LEN IN QLC-FLD(3)
                GO TO FIN-VAL-NUM-FLD-QLC.
       VAL-NUM-FRM-NUM-ILDC-4.
           MOVE FRM-NUM-ILDC-ALF IN QLC-FLD(4) TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 2 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO FRM-NUM-ILDC IN QLC-FLD(4)
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-NUM-ILDC-LEN IN QLC-FLD(4)
                GO TO FIN-VAL-NUM-FLD-QLC.
       VAL-NUM-FRM-NUM-ILDC-5.
           MOVE FRM-NUM-ILDC-ALF IN QLC-FLD(5) TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 2 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO FRM-NUM-ILDC IN QLC-FLD(5)
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-NUM-ILDC-LEN IN QLC-FLD(5)
                GO TO FIN-VAL-NUM-FLD-QLC.
       VAL-NUM-FRM-NUM-ILDC-6.
           MOVE FRM-NUM-ILDC-ALF IN QLC-FLD(6) TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 2 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO FRM-NUM-ILDC IN QLC-FLD(6)
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
