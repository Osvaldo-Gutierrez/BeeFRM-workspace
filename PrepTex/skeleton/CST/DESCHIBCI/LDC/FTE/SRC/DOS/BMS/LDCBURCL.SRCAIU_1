      *
      * LDCBURCL
      * Version 3.0.1 Nov-2005
      * 27-Mar-19 09:39 PM
      *
       SET-TAG-LDCBURCL SECTION.
       INI-SET-TAG-LDCBURCL.
           MOVE
           '<IDNMOD NOM=$LDCBURCL$ VSN=$ $ VGM=$3.0.1$
      -    'GEN=$27-Mar-19 09:39 PM$ FNC=$ $/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-LDCBURCL.
           EXIT.
           
       RET-ALL-RCL SECTION.
       INI-RET-ALL-RCL.
           PERFORM KEY-ALL-RCL.
           PERFORM RET-CLF-RCL.
           PERFORM RET-RCL-RCL.
           PERFORM RET-DBC-RCL.
       FIN-RET-ALL-RCL.
           EXIT.
       
       RET-CLF-RCL SECTION.
       INI-RET-CLF-RCL.
           MOVE CLF-COD-VCCF IN RCL-FLD TO CLF-COD-VCCF IN CLF.
           MOVE CLF-COD-TOCF IN RCL-FLD TO CLF-COD-TOCF IN CLF.
           MOVE CLF-COD-AOCF IN RCL-FLD TO CLF-COD-AOCF IN CLF.
       FIN-RET-CLF-RCL.
           EXIT.
       
       RET-RCL-RCL SECTION.
       INI-RET-RCL-RCL.
           MOVE RCL-VAL-PRCL IN RCL-FLD TO RCL-VAL-PRCL IN RCL.
       FIN-RET-RCL-RCL.
           EXIT.
       
       RET-DBC-RCL SECTION.
       INI-RET-DBC-RCL.
       FIN-RET-DBC-RCL.
           EXIT.
       
       KEY-ALL-RCL SECTION.
       INI-KEY-ALL-RCL.
           PERFORM KEY-CLF-RCL.
           PERFORM KEY-RCL-RCL.
           PERFORM KEY-DBC-RCL.
       FIN-KEY-ALL-RCL.
           EXIT.
       
       KEY-CLF-RCL SECTION.
       INI-KEY-CLF-RCL.
       FIN-KEY-CLF-RCL.
           EXIT.
       
       KEY-RCL-RCL SECTION.
       INI-KEY-RCL-RCL.
           MOVE RCL-CAI-ICLF IN RCL-FLD TO RCL-CAI-ICLF IN RCL.
           MOVE RCL-IIC-ICLF IN RCL-FLD TO RCL-IIC-ICLF IN RCL.
           MOVE RCL-COD-TREL IN RCL-FLD TO RCL-COD-TREL IN RCL.
      *<<<< FIN_KEY_RCL_RCL
           MOVE 1                       TO RCL-NUM-TREC IN RCL.
           MOVE DBC-CAI-ICLI IN RCL-FLD TO RCL-CAI-ICLI IN RCL.
           MOVE DBC-IIC-ICLI IN RCL-FLD TO RCL-IIC-ICLI IN RCL.
      *>>>>
       FIN-KEY-RCL-RCL.
           EXIT.
       
       KEY-DBC-RCL SECTION.
       INI-KEY-DBC-RCL.
           MOVE DBC-GLS-NOMC IN RCL-FLD TO DBC-GLS-NOMC IN DBC.
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN RCL-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN RCL-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN RCL-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN RCL-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-CAI-ICLI IN RCL-FLD TO DBC-CAI-ICLI IN DBC.
           MOVE DBC-IIC-ICLI IN RCL-FLD TO DBC-IIC-ICLI IN DBC.
       FIN-KEY-DBC-RCL.
           EXIT.
       
       SCR-CLRA-RCL SECTION.
       INI-SCR-CLRA-RCL.
           PERFORM CLR-KEY-RCL.
           PERFORM CLR-FLD-RCL.
       FIN-SCR-CLRA-RCL.
           EXIT.
       
       CLR-KEY-RCL SECTION.
       INI-CLR-KEY-RCL.
           MOVE SPACES TO RCL-CAI-ICLF IN RCL-FLD.
           MOVE ZEROES TO RCL-IIC-ICLF IN RCL-FLD.
           MOVE SPACES TO RCL-COD-TREL IN RCL-FLD.
           MOVE SPACES TO DBC-GLS-NOMC IN RCL-FLD.
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN RCL-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN RCL-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN RCL-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN RCL-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO DBC-CAI-ICLI IN RCL-FLD.
           MOVE ZEROES TO DBC-IIC-ICLI IN RCL-FLD.
       FIN-CLR-KEY-RCL.
           EXIT.
       
       CLR-FLD-RCL SECTION.
       INI-CLR-FLD-RCL.
           MOVE SPACES TO FRM-GLS-VCCF IN RCL-FLD.
           MOVE SPACES TO FRM-GLS-TOCF IN RCL-FLD.
           MOVE SPACES TO FRM-GLS-AOCF IN RCL-FLD.
           MOVE SPACES TO FRM-GLS-TREL IN RCL-FLD.
           MOVE ZEROES TO FRM-VAL-MASO IN RCL-FLD.
           MOVE SPACES TO FRM-COD-VCCF IN RCL-FLD.
           PERFORM CLR-CLF-RCL.
           PERFORM CLR-RCL-RCL.
           PERFORM CLR-DBC-RCL.
       FIN-CLR-FLD-RCL.
           EXIT.
       
       CLR-CLF-RCL SECTION.
       INI-CLR-CLF-RCL.
           MOVE SPACES TO CLF-COD-VCCF IN RCL-FLD.
           MOVE SPACES TO CLF-COD-TOCF IN RCL-FLD.
           MOVE SPACES TO CLF-COD-AOCF IN RCL-FLD.
       FIN-CLR-CLF-RCL.
           EXIT.
       
       CLR-RCL-RCL SECTION.
       INI-CLR-RCL-RCL.
           MOVE ZEROES TO RCL-VAL-PRCL IN RCL-FLD.
       FIN-CLR-RCL-RCL.
           EXIT.
       
       CLR-DBC-RCL SECTION.
       INI-CLR-DBC-RCL.
       FIN-CLR-DBC-RCL.
           EXIT.
       
       FST-KEY-RCL SECTION.
       INI-FST-KEY-RCL.
           MOVE -1 TO RCL-CAI-ICLF-LEN IN RCL-FLD.
       FIN-FST-KEY-RCL.
           EXIT.
       
       FST-FLD-RCL SECTION.
       INI-FST-FLD-RCL.
           MOVE -1 TO RCL-VAL-PRCL-LEN IN RCL-FLD.
       FIN-FST-FLD-RCL.
           EXIT.
       
       PRO-KEY-RCL SECTION.
       INI-PRO-KEY-RCL.
           PERFORM PRO-MKY-RCL.
       FIN-PRO-KEY-RCL.
           EXIT.
       
       PRO-MKY-RCL SECTION.
       INI-PRO-MKY-RCL.
       FIN-PRO-MKY-RCL.
           EXIT.
       
       PRO-FLD-RCL SECTION.
       INI-PRO-FLD-RCL.
           PERFORM PRO-CLF-RCL.
           PERFORM PRO-RCL-RCL.
           PERFORM PRO-DBC-RCL.
       FIN-PRO-FLD-RCL.
           EXIT.
       
       PRO-CLF-RCL SECTION.
       INI-PRO-CLF-RCL.
       FIN-PRO-CLF-RCL.
           EXIT.
       
       PRO-RCL-RCL SECTION.
       INI-PRO-RCL-RCL.
           MOVE FRM-CPIM-AEY TO RCL-VAL-PRCL-ATR IN RCL-FLD.
       FIN-PRO-RCL-RCL.
           EXIT.
       
       PRO-DBC-RCL SECTION.
       INI-PRO-DBC-RCL.
       FIN-PRO-DBC-RCL.
           EXIT.
       
       UNP-KEY-RCL SECTION.
       INI-UNP-KEY-RCL.
           PERFORM UNP-IKY-RCL.
           PERFORM UNP-AKY-RCL.
           PERFORM UNP-MKY-RCL.
       FIN-UNP-KEY-RCL.
           EXIT.
       
       UNP-IKY-RCL SECTION.
       INI-UNP-IKY-RCL.
           MOVE FRM-CPIM-UBY TO RCL-CAI-ICLF-ATR.
           MOVE FRM-CPIM-UBY TO RCL-IIC-ICLF-ATR.
           MOVE FRM-CPIM-UBY TO RCL-COD-TREL-ATR.
      * IDC-INI --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO DBC-NUM-ICLI-ATR IN RCL-FLD.
           MOVE FRM-CPIM-UBY TO DBC-VRF-ICLI-ATR IN RCL-FLD.
           MOVE FRM-CPIM-UBY TO DBC-IND-ICLI-ATR IN RCL-FLD.
           MOVE FRM-CPIM-UBY TO DBC-GLS-ICLI-ATR IN RCL-FLD.
      * IDC-FIN --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
       FIN-UNP-IKY-RCL.
           EXIT.
       
       UNP-MKY-RCL SECTION.
       INI-UNP-MKY-RCL.
       FIN-UNP-MKY-RCL.
           EXIT.
       
       UNP-AKY-RCL SECTION.
       INI-UNP-AKY-RCL.
           MOVE FRM-CPIM-UBY TO DBC-GLS-NOMC-ATR.
       FIN-UNP-AKY-RCL.
           EXIT.
       
       UNP-FLD-RCL SECTION.
       INI-UNP-FLD-RCL.
           PERFORM UNP-CLF-RCL.
           PERFORM UNP-RCL-RCL.
           PERFORM UNP-DBC-RCL.
       FIN-UNP-FLD-RCL.
           EXIT.
       
       UNP-CLF-RCL SECTION.
       INI-UNP-CLF-RCL.
       FIN-UNP-CLF-RCL.
           EXIT.
       
       UNP-RCL-RCL SECTION.
       INI-UNP-RCL-RCL.
           MOVE FRM-CPIM-UBY TO RCL-VAL-PRCL-ATR IN RCL-FLD.
       FIN-UNP-RCL-RCL.
           EXIT.
       
       UNP-DBC-RCL SECTION.
       INI-UNP-DBC-RCL.
       FIN-UNP-DBC-RCL.
           EXIT.
       
       VAL-MKY-RCL SECTION.
       INI-VAL-MKY-RCL.
       FIN-VAL-MKY-RCL.
           EXIT.
       
       VAL-FLD-RCL SECTION.
       INI-VAL-FLD-RCL.
           PERFORM VAL-CLF-COD-VCCF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CLF-COD-VCCF-LEN IN RCL-FLD
                GO TO FIN-VAL-FLD-RCL.
           PERFORM VAL-CLF-COD-TOCF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CLF-COD-TOCF-LEN IN RCL-FLD
                GO TO FIN-VAL-FLD-RCL.
           PERFORM VAL-CLF-COD-AOCF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CLF-COD-AOCF-LEN IN RCL-FLD
                GO TO FIN-VAL-FLD-RCL.
           PERFORM VAL-FRM-GLS-VCCF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-VCCF-LEN IN RCL-FLD
                GO TO FIN-VAL-FLD-RCL.
           PERFORM VAL-FRM-GLS-TOCF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TOCF-LEN IN RCL-FLD
                GO TO FIN-VAL-FLD-RCL.
           PERFORM VAL-FRM-GLS-AOCF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-AOCF-LEN IN RCL-FLD
                GO TO FIN-VAL-FLD-RCL.
           PERFORM VAL-FRM-GLS-TREL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TREL-LEN IN RCL-FLD
                GO TO FIN-VAL-FLD-RCL.
           PERFORM VAL-RCL-VAL-PRCL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RCL-VAL-PRCL-LEN IN RCL-FLD
                GO TO FIN-VAL-FLD-RCL.
           PERFORM VAL-FRM-VAL-MASO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-MASO-LEN IN RCL-FLD
                GO TO FIN-VAL-FLD-RCL.
           PERFORM VAL-FRM-COD-VCCF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-VCCF-LEN IN RCL-FLD
                GO TO FIN-VAL-FLD-RCL.
       FIN-VAL-FLD-RCL.
           EXIT.
       
       VAL-CLF-COD-VCCF SECTION.
       INI-VAL-CLF-COD-VCCF.
       FIN-VAL-CLF-COD-VCCF.
           EXIT.
       
       VAL-CLF-COD-TOCF SECTION.
       INI-VAL-CLF-COD-TOCF.
       FIN-VAL-CLF-COD-TOCF.
           EXIT.
       
       VAL-CLF-COD-AOCF SECTION.
       INI-VAL-CLF-COD-AOCF.
       FIN-VAL-CLF-COD-AOCF.
           EXIT.
       
       VAL-FRM-GLS-VCCF SECTION.
       INI-VAL-FRM-GLS-VCCF.
       FIN-VAL-FRM-GLS-VCCF.
           EXIT.
       
       VAL-FRM-GLS-TOCF SECTION.
       INI-VAL-FRM-GLS-TOCF.
       FIN-VAL-FRM-GLS-TOCF.
           EXIT.
       
       VAL-FRM-GLS-AOCF SECTION.
       INI-VAL-FRM-GLS-AOCF.
       FIN-VAL-FRM-GLS-AOCF.
           EXIT.
       
       VAL-FRM-GLS-TREL SECTION.
       INI-VAL-FRM-GLS-TREL.
       FIN-VAL-FRM-GLS-TREL.
           EXIT.
       
       VAL-RCL-VAL-PRCL SECTION.
       INI-VAL-RCL-VAL-PRCL.

      *<<<<RCL_VAL_PRCL
           IF RCL-VAL-PRCL IN RCL-FLD NOT > ZEROES
               MOVE 100 TO RCL-VAL-PRCL IN RCL-FLD.
      *AFRI-INI/AGO2008(MAE)
           MOVE RCL-VAL-PRCL IN RCL-FLD TO RCL-VAL-PRCL IN PRY-VARI.
           PERFORM  VAL-MAX-POR.
           IF (PRY-IND-ERRO IN PRY-VARI     = 'S' OR
               PRY-IND-MPOR IN PRY-VARI NOT = 'S')
                MOVE 'DEU'                    TO MSG-COD-SIST
                MOVE PRY-COD-MENS IN PRY-VARI TO MSG-COD-MENS
                PERFORM GET-MSG
                MOVE MSG-GLS-DESC             TO FRM-MENS
                MOVE FRM-SUAR-MAL             TO FRM-SUAR
                MOVE ZEROES TO RCL-VAL-PRCL IN RCL-FLD
                GO TO FIN-VAL-RCL-VAL-PRCL.
      *AFRI-FIN/AGO2008(MAE)
      *>>>>


       FIN-VAL-RCL-VAL-PRCL.
           EXIT.
       
       VAL-FRM-VAL-MASO SECTION.
       INI-VAL-FRM-VAL-MASO.
      *<<<<FRM_VAL_MASO
      *AFRI-INI/AGO2008(MAE)                                            
           MOVE RCL-CAI-ICLF IN RCL-FLD TO CLF-CAI-ICLF IN PRY-VARI.
           MOVE RCL-IIC-ICLF IN RCL-FLD TO CLF-IIC-ICLF IN PRY-VARI.
           MOVE RCL-VAL-PRCL IN RCL-FLD TO RCL-VAL-PRCL IN PRY-VARI.
           PERFORM  CAL-MTO-ASO.                                        
           IF (PRY-IND-ERRO IN PRY-VARI     = 'S' OR                    
               PRY-IND-MASO IN PRY-VARI NOT = 'S')
                MOVE 'DEU'                    TO MSG-COD-SIST           
                MOVE PRY-COD-MENS IN PRY-VARI TO MSG-COD-MENS           
                PERFORM GET-MSG                                         
                MOVE MSG-GLS-DESC             TO FRM-MENS               
                MOVE FRM-SUAR-MAL             TO FRM-SUAR               
                MOVE ZEROES     TO FRM-VAL-MASO IN RCL-FLD
                GO TO FIN-VAL-FRM-VAL-MASO
            ELSE
                MOVE WSS-VAL-MASO IN PRY-VARI TO FRM-VAL-MASO
                GO TO FIN-VAL-FRM-VAL-MASO.
      *AFRI-FIN/AGO2008(MAE)                                            
      *>>>>

       FIN-VAL-FRM-VAL-MASO.
           EXIT.
       
       VAL-FRM-COD-VCCF SECTION.
       INI-VAL-FRM-COD-VCCF.
       FIN-VAL-FRM-COD-VCCF.
           EXIT.
       
       VAL-CON-FLD-RCL SECTION.
       INI-VAL-CON-FLD-RCL.
       FIN-VAL-CON-FLD-RCL.
           EXIT.
       
       VAL-CON-RCL SECTION.
       INI-VAL-CON-RCL.
       FIN-VAL-CON-RCL.
           EXIT.
       
       VAL-NUM-KEY-RCL SECTION.
       INI-VAL-NUM-KEY-RCL.
           PERFORM VAL-NUM-IKY-RCL.
           PERFORM VAL-NUM-MKY-RCL.
           PERFORM VAL-NUM-AKY-RCL.
       FIN-VAL-NUM-KEY-RCL.
           EXIT.
       
       VAL-NUM-IKY-RCL SECTION.
       INI-VAL-NUM-IKY-RCL.
       VAL-NUM-RCL-IIC-ICLF.
           MOVE RCL-IIC-ICLF-ALF IN RCL-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO RCL-IIC-ICLF IN RCL-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO RCL-IIC-ICLF-LEN IN RCL-FLD
                GO TO FIN-VAL-NUM-IKY-RCL.
      * IDC-INI --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
       VAL-NUM-DBC-NUM-ICLI.
           MOVE DBC-NUM-ICLI-ALF IN RCL-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO DBC-NUM-ICLI IN RCL-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO DBC-NUM-ICLI-LEN IN RCL-FLD
                GO TO FIN-VAL-NUM-IKY-RCL.
      * IDC-FIN --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
       VAL-NUM-DBC-IIC-ICLI.
           MOVE DBC-IIC-ICLI-ALF IN RCL-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO DBC-IIC-ICLI IN RCL-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO DBC-IIC-ICLI-LEN IN RCL-FLD
                GO TO FIN-VAL-NUM-IKY-RCL.
       FIN-VAL-NUM-IKY-RCL.
           EXIT.
       
       VAL-NUM-MKY-RCL SECTION.
       INI-VAL-NUM-MKY-RCL.
       FIN-VAL-NUM-MKY-RCL.
           EXIT.
       
       VAL-NUM-AKY-RCL SECTION.
       INI-VAL-NUM-AKY-RCL.
       FIN-VAL-NUM-AKY-RCL.
           EXIT.
       
       VAL-NUM-FLD-RCL SECTION.
       INI-VAL-NUM-FLD-RCL.
       VAL-NUM-RCL-VAL-PRCL.
           MOVE RCL-VAL-PRCL-ALF IN RCL-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9V TO RCL-VAL-PRCL IN RCL-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO RCL-VAL-PRCL-LEN IN RCL-FLD
                GO TO FIN-VAL-NUM-FLD-RCL.
       VAL-NUM-FRM-VAL-MASO.
           MOVE FRM-VAL-MASO-ALF IN RCL-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9V TO FRM-VAL-MASO IN RCL-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-VAL-MASO-LEN IN RCL-FLD
                GO TO FIN-VAL-NUM-FLD-RCL.
       FIN-VAL-NUM-FLD-RCL.
           EXIT.
       
       EDT-KEY-RCL SECTION.
       INI-EDT-KEY-RCL.
           PERFORM EDT-IKY-RCL.
           PERFORM EDT-MKY-RCL.
           PERFORM EDT-AKY-RCL.
       FIN-EDT-KEY-RCL.
           EXIT.
       
       EDT-IKY-RCL SECTION.
       INI-EDT-IKY-RCL.
           IF RCL-IIC-ICLF-LEN IN RCL-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO RCL-IIC-ICLF-LEN IN RCL-FLD
                GO TO FIN-EDT-IKY-RCL.
           MOVE RCL-IIC-ICLF IN RCL-FLD TO RCL-IIC-ICLF-EDT IN RCL-EDT.
           MOVE RCL-IIC-ICLF-EDT IN RCL-EDT TO 
                RCL-IIC-ICLF-ALF IN RCL-FLD.
      * IDC-INI --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF DBC-NUM-ICLI-LEN IN RCL-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DBC-NUM-ICLI-LEN IN RCL-FLD
                GO TO FIN-EDT-IKY-RCL.
           MOVE DBC-NUM-ICLI IN RCL-FLD TO
                DBC-NUM-ICLI-EDT IN RCL-EDT.
           MOVE DBC-NUM-ICLI-EDT IN RCL-EDT TO
                DBC-NUM-ICLI-ALF IN RCL-FLD.
      * IDC-FIN --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF DBC-IIC-ICLI-LEN IN RCL-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DBC-IIC-ICLI-LEN IN RCL-FLD
                GO TO FIN-EDT-IKY-RCL.
           MOVE DBC-IIC-ICLI IN RCL-FLD TO DBC-IIC-ICLI-EDT IN RCL-EDT.
           MOVE DBC-IIC-ICLI-EDT IN RCL-EDT TO 
                DBC-IIC-ICLI-ALF IN RCL-FLD.
       FIN-EDT-IKY-RCL.
           EXIT.
       
       EDT-MKY-RCL SECTION.
       INI-EDT-MKY-RCL.
       FIN-EDT-MKY-RCL.
           EXIT.
       
       EDT-AKY-RCL SECTION.
       INI-EDT-AKY-RCL.
       FIN-EDT-AKY-RCL.
           EXIT.
       
       EDT-FLD-RCL SECTION.
       INI-EDT-FLD-RCL.
           IF RCL-VAL-PRCL-LEN IN RCL-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO RCL-VAL-PRCL-LEN IN RCL-FLD
                GO TO FIN-EDT-FLD-RCL.
           MOVE RCL-VAL-PRCL IN RCL-FLD TO RCL-VAL-PRCL-EDT IN RCL-EDT.
           MOVE RCL-VAL-PRCL-EDT IN RCL-EDT TO 
                RCL-VAL-PRCL-ALF IN RCL-FLD.
           IF FRM-VAL-MASO-LEN IN RCL-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-MASO-LEN IN RCL-FLD
                GO TO FIN-EDT-FLD-RCL.
           MOVE FRM-VAL-MASO IN RCL-FLD TO FRM-VAL-MASO-EDT IN RCL-EDT.
           MOVE FRM-VAL-MASO-EDT IN RCL-EDT TO 
                FRM-VAL-MASO-ALF IN RCL-FLD.
       FIN-EDT-FLD-RCL.
           EXIT.
