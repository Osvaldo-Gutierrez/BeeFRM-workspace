       SET-TAG-LDCBQACG SECTION.
       INI-SET-TAG-LDCBQACG.
           MOVE
           '<IDNMOD NOM=$LDCBQACG$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$27-Mar-19 04:01 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-LDCBQACG.
           EXIT.
           
       PUT-ALL-ACG SECTION.
       INI-PUT-ALL-ACG.
           PERFORM PUT-ACG-ACG.
           PERFORM PUT-LCC-ACG.
           PERFORM PUT-DBC-ACG.
           PERFORM PUT-LDC-ACG.
       FIN-PUT-ALL-ACG.
           EXIT.
       
       PUT-ACG-ACG SECTION.
       INI-PUT-ACG-ACG.
           IF PGM-STAT-ACG-NUL
               GO TO FIN-PUT-ACG-ACG.
           IF NOT PGM-STAT-ACG-OKS
               PERFORM CLR-ACG-ACG
               GO TO FIN-PUT-ACG-ACG.
           MOVE ACG-CAI-ILIC IN ACG TO ACG-CAI-ILIC IN ACG-FLD.
           MOVE ACG-IIC-ILIC IN ACG TO ACG-IIC-ILIC IN ACG-FLD.
           MOVE ACG-NUM-IACG IN ACG TO ACG-NUM-IACG IN ACG-FLD.
       FIN-PUT-ACG-ACG.
           EXIT.
       
       PUT-LCC-ACG SECTION.
       INI-PUT-LCC-ACG.
           IF PGM-STAT-LCC-NUL
               GO TO FIN-PUT-LCC-ACG.
           IF NOT PGM-STAT-LCC-OKS
               PERFORM CLR-LCC-ACG
               GO TO FIN-PUT-LCC-ACG.
           MOVE LCC-CAI-ICLF IN LCC TO LCC-CAI-ICLF IN ACG-FLD.
           MOVE LCC-IIC-ICLF IN LCC TO LCC-IIC-ICLF IN ACG-FLD.
           MOVE LCC-NUM-ILCC IN LCC TO LCC-NUM-ILCC IN ACG-FLD.
           MOVE LCC-CAI-ILIC IN LCC TO ACG-CAI-ILIC IN ACG-FLD.         
           MOVE LCC-IIC-ILIC IN LCC TO ACG-IIC-ILIC IN ACG-FLD.         
       FIN-PUT-LCC-ACG.
           EXIT.
       
       PUT-DBC-ACG SECTION.
       INI-PUT-DBC-ACG.
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-ACG.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-ACG
               GO TO FIN-PUT-DBC-ACG.
      * IDC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN ACG-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN ACG-FLD.
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN ACG-FLD.
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN ACG-FLD.
      * IDC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN ACG-FLD.
       FIN-PUT-DBC-ACG.
           EXIT.
       
       PUT-LDC-ACG SECTION.
       INI-PUT-LDC-ACG.
           IF PGM-STAT-LDC-NUL
               GO TO FIN-PUT-LDC-ACG.
           IF NOT PGM-STAT-LDC-OKS
               PERFORM CLR-LDC-ACG
               GO TO FIN-PUT-LDC-ACG.
           MOVE LDC-COD-TIPO IN LDC TO LDC-COD-TIPO IN ACG-FLD.
           MOVE LDC-GLS-DESC IN LDC TO LDC-GLS-DESC IN ACG-FLD.
       FIN-PUT-LDC-ACG.
           EXIT.
       
       KEY-ALL-ACG SECTION.
       INI-KEY-ALL-ACG.
           PERFORM KEY-ACG-ACG.
           PERFORM KEY-LCC-ACG.
           PERFORM KEY-DBC-ACG.
           PERFORM KEY-LDC-ACG.
       FIN-KEY-ALL-ACG.
           EXIT.
       
       KEY-ACG-ACG SECTION.
       INI-KEY-ACG-ACG.
           MOVE ACG-NUM-IACG IN ACG-FLD TO ACG-NUM-IACG IN ACG.
       FIN-KEY-ACG-ACG.
           EXIT.
       
       KEY-LCC-ACG SECTION.
       INI-KEY-LCC-ACG.
           MOVE LCC-CAI-ICLF IN ACG-FLD TO LCC-CAI-ICLF IN LCC.
           MOVE LCC-IIC-ICLF IN ACG-FLD TO LCC-IIC-ICLF IN LCC.
           MOVE LCC-NUM-ILCC IN ACG-FLD TO LCC-NUM-ILCC IN LCC.
       FIN-KEY-LCC-ACG.
           EXIT.
       
       KEY-DBC-ACG SECTION.
       INI-KEY-DBC-ACG.
       FIN-KEY-DBC-ACG.
           EXIT.
       
       KEY-LDC-ACG SECTION.
       INI-KEY-LDC-ACG.
       FIN-KEY-LDC-ACG.
           EXIT.
       
       SCR-CLRA-ACG SECTION.
       INI-SCR-CLRA-ACG.
           PERFORM CLR-KEY-ACG.
           PERFORM CLR-FLD-ACG.
       FIN-SCR-CLRA-ACG.
           EXIT.
       
       CLR-KEY-ACG SECTION.
       INI-CLR-KEY-ACG.
           MOVE SPACES TO LCC-CAI-ICLF IN ACG-FLD.
           MOVE ZEROES TO LCC-IIC-ICLF IN ACG-FLD.
           MOVE ZEROES TO LCC-NUM-ILCC IN ACG-FLD.
           MOVE ZEROES TO ACG-NUM-IACG IN ACG-FLD.
       FIN-CLR-KEY-ACG.
           EXIT.
       
       CLR-FLD-ACG SECTION.
       INI-CLR-FLD-ACG.
           MOVE SPACES TO FRM-GLS-ENTI IN ACG-FLD.
           MOVE SPACES TO FRM-COD-BENE IN ACG-FLD(1).
           MOVE SPACES TO FRM-COD-BENE IN ACG-FLD(2).
           MOVE SPACES TO FRM-COD-BENE IN ACG-FLD(3).
           MOVE SPACES TO FRM-COD-BENE IN ACG-FLD(4).
           MOVE SPACES TO FRM-COD-BENE IN ACG-FLD(5).
           MOVE SPACES TO FRM-COD-BENE IN ACG-FLD(6).
           MOVE ZEROES TO FRM-VAL-MMIN IN ACG-FLD(1).
           MOVE ZEROES TO FRM-VAL-MMIN IN ACG-FLD(2).
           MOVE ZEROES TO FRM-VAL-MMIN IN ACG-FLD(3).
           MOVE ZEROES TO FRM-VAL-MMIN IN ACG-FLD(4).
           MOVE ZEROES TO FRM-VAL-MMIN IN ACG-FLD(5).
           MOVE ZEROES TO FRM-VAL-MMIN IN ACG-FLD(6).
           MOVE ZEROES TO FRM-VAL-MMAX IN ACG-FLD(1).
           MOVE ZEROES TO FRM-VAL-MMAX IN ACG-FLD(2).
           MOVE ZEROES TO FRM-VAL-MMAX IN ACG-FLD(3).
           MOVE ZEROES TO FRM-VAL-MMAX IN ACG-FLD(4).
           MOVE ZEROES TO FRM-VAL-MMAX IN ACG-FLD(5).
           MOVE ZEROES TO FRM-VAL-MMAX IN ACG-FLD(6).
           MOVE ZEROES TO FRM-VAL-PGAR IN ACG-FLD(1).
           MOVE ZEROES TO FRM-VAL-PGAR IN ACG-FLD(2).
           MOVE ZEROES TO FRM-VAL-PGAR IN ACG-FLD(3).
           MOVE ZEROES TO FRM-VAL-PGAR IN ACG-FLD(4).
           MOVE ZEROES TO FRM-VAL-PGAR IN ACG-FLD(5).
           MOVE ZEROES TO FRM-VAL-PGAR IN ACG-FLD(6).
           MOVE SPACES TO FRM-GLS-BENE IN ACG-FLD(1).
           MOVE SPACES TO FRM-GLS-BENE IN ACG-FLD(2).
           MOVE SPACES TO FRM-GLS-BENE IN ACG-FLD(3).
           MOVE SPACES TO FRM-GLS-BENE IN ACG-FLD(4).
           MOVE SPACES TO FRM-GLS-BENE IN ACG-FLD(5).
           MOVE SPACES TO FRM-GLS-BENE IN ACG-FLD(6).
           MOVE ZEROES TO FRM-NUM-PMIN IN ACG-FLD(1).
           MOVE ZEROES TO FRM-NUM-PMIN IN ACG-FLD(2).
           MOVE ZEROES TO FRM-NUM-PMIN IN ACG-FLD(3).
           MOVE ZEROES TO FRM-NUM-PMIN IN ACG-FLD(4).
           MOVE ZEROES TO FRM-NUM-PMIN IN ACG-FLD(5).
           MOVE ZEROES TO FRM-NUM-PMIN IN ACG-FLD(6).
           MOVE SPACES TO FRM-IND-PMIN IN ACG-FLD(1).
           MOVE SPACES TO FRM-IND-PMIN IN ACG-FLD(2).
           MOVE SPACES TO FRM-IND-PMIN IN ACG-FLD(3).
           MOVE SPACES TO FRM-IND-PMIN IN ACG-FLD(4).
           MOVE SPACES TO FRM-IND-PMIN IN ACG-FLD(5).
           MOVE SPACES TO FRM-IND-PMIN IN ACG-FLD(6).
           MOVE ZEROES TO FRM-NUM-PMAX IN ACG-FLD(1).
           MOVE ZEROES TO FRM-NUM-PMAX IN ACG-FLD(2).
           MOVE ZEROES TO FRM-NUM-PMAX IN ACG-FLD(3).
           MOVE ZEROES TO FRM-NUM-PMAX IN ACG-FLD(4).
           MOVE ZEROES TO FRM-NUM-PMAX IN ACG-FLD(5).
           MOVE ZEROES TO FRM-NUM-PMAX IN ACG-FLD(6).
           MOVE SPACES TO FRM-IND-PMAX IN ACG-FLD(1).
           MOVE SPACES TO FRM-IND-PMAX IN ACG-FLD(2).
           MOVE SPACES TO FRM-IND-PMAX IN ACG-FLD(3).
           MOVE SPACES TO FRM-IND-PMAX IN ACG-FLD(4).
           MOVE SPACES TO FRM-IND-PMAX IN ACG-FLD(5).
           MOVE SPACES TO FRM-IND-PMAX IN ACG-FLD(6).
           MOVE ZEROES TO FRM-NUM-PREN IN ACG-FLD(1).
           MOVE ZEROES TO FRM-NUM-PREN IN ACG-FLD(2).
           MOVE ZEROES TO FRM-NUM-PREN IN ACG-FLD(3).
           MOVE ZEROES TO FRM-NUM-PREN IN ACG-FLD(4).
           MOVE ZEROES TO FRM-NUM-PREN IN ACG-FLD(5).
           MOVE ZEROES TO FRM-NUM-PREN IN ACG-FLD(6).
           MOVE SPACES TO FRM-IND-PREN IN ACG-FLD(1).
           MOVE SPACES TO FRM-IND-PREN IN ACG-FLD(2).
           MOVE SPACES TO FRM-IND-PREN IN ACG-FLD(3).
           MOVE SPACES TO FRM-IND-PREN IN ACG-FLD(4).
           MOVE SPACES TO FRM-IND-PREN IN ACG-FLD(5).
           MOVE SPACES TO FRM-IND-PREN IN ACG-FLD(6).
           PERFORM CLR-ACG-ACG.
           PERFORM CLR-LCC-ACG.
           PERFORM CLR-DBC-ACG.
           PERFORM CLR-LDC-ACG.
       FIN-CLR-FLD-ACG.
           EXIT.
       
       CLR-ACG-ACG SECTION.
       INI-CLR-ACG-ACG.
           MOVE SPACES TO ACG-CAI-ILIC IN ACG-FLD.
           MOVE SPACES TO ACG-IIC-ILIC IN ACG-FLD.
       FIN-CLR-ACG-ACG.
           EXIT.
       
       CLR-LCC-ACG SECTION.
       INI-CLR-LCC-ACG.
       FIN-CLR-LCC-ACG.
           EXIT.
       
       CLR-DBC-ACG SECTION.
       INI-CLR-DBC-ACG.
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN ACG-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN ACG-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN ACG-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN ACG-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO DBC-GLS-NOMC IN ACG-FLD.
       FIN-CLR-DBC-ACG.
           EXIT.
       
       CLR-LDC-ACG SECTION.
       INI-CLR-LDC-ACG.
           MOVE SPACES TO LDC-COD-TIPO IN ACG-FLD.
           MOVE SPACES TO LDC-GLS-DESC IN ACG-FLD.
       FIN-CLR-LDC-ACG.
           EXIT.
       
       FST-KEY-ACG SECTION.
       INI-FST-KEY-ACG.
           MOVE -1 TO LCC-CAI-ICLF-LEN IN ACG-FLD.
       FIN-FST-KEY-ACG.
           EXIT.
       
       FST-FLD-ACG SECTION.
       INI-FST-FLD-ACG.
           MOVE -1 TO FRM-COD-BENE-LEN IN ACG-FLD(1).
       FIN-FST-FLD-ACG.
           EXIT.
       
       PRO-KEY-ACG SECTION.
       INI-PRO-KEY-ACG.
           PERFORM PRO-IKY-ACG.
           PERFORM PRO-AKY-ACG.
           PERFORM PRO-MKY-ACG.
       FIN-PRO-KEY-ACG.
           EXIT.
       
       PRO-IKY-ACG SECTION.
       INI-PRO-IKY-ACG.
           MOVE FRM-CPIM-AEY TO LCC-CAI-ICLF-ATR IN ACG-FLD.
           MOVE FRM-CPIM-AEY TO LCC-IIC-ICLF-ATR IN ACG-FLD.
           MOVE FRM-CPIM-AEY TO LCC-NUM-ILCC-ATR IN ACG-FLD.
           MOVE FRM-CPIM-AEY TO ACG-NUM-IACG-ATR IN ACG-FLD.
       FIN-PRO-IKY-ACG.
           EXIT.
       
       PRO-MKY-ACG SECTION.
       INI-PRO-MKY-ACG.
       FIN-PRO-MKY-ACG.
           EXIT.
       
       PRO-AKY-ACG SECTION.
       INI-PRO-AKY-ACG.
       FIN-PRO-AKY-ACG.
           EXIT.
       
       UNP-KEY-ACG SECTION.
       INI-UNP-KEY-ACG.
           PERFORM UNP-IKY-ACG.
           PERFORM UNP-AKY-ACG.
           PERFORM UNP-MKY-ACG.
       FIN-UNP-KEY-ACG.
           EXIT.
       
       UNP-IKY-ACG SECTION.
       INI-UNP-IKY-ACG.
           MOVE FRM-CPIM-UBY TO LCC-CAI-ICLF-ATR.
           MOVE FRM-CPIM-UBY TO LCC-IIC-ICLF-ATR.
           MOVE FRM-CPIM-UBY TO LCC-NUM-ILCC-ATR.
           MOVE FRM-CPIM-UBY TO ACG-NUM-IACG-ATR.
       FIN-UNP-IKY-ACG.
           EXIT.
       
       UNP-MKY-ACG SECTION.
       INI-UNP-MKY-ACG.
       FIN-UNP-MKY-ACG.
           EXIT.
       
       UNP-AKY-ACG SECTION.
       INI-UNP-AKY-ACG.
       FIN-UNP-AKY-ACG.
           EXIT.
       
       UNP-FLD-ACG SECTION.
       INI-UNP-FLD-ACG.
           MOVE FRM-CPIM-UBY TO FRM-COD-BENE-ATR IN ACG-FLD(1).
           MOVE FRM-CPIM-UBY TO FRM-COD-BENE-ATR IN ACG-FLD(2).
           MOVE FRM-CPIM-UBY TO FRM-COD-BENE-ATR IN ACG-FLD(3).
           MOVE FRM-CPIM-UBY TO FRM-COD-BENE-ATR IN ACG-FLD(4).
           MOVE FRM-CPIM-UBY TO FRM-COD-BENE-ATR IN ACG-FLD(5).
           MOVE FRM-CPIM-UBY TO FRM-COD-BENE-ATR IN ACG-FLD(6).
           MOVE FRM-CPIM-UBY TO FRM-VAL-MMIN-ATR IN ACG-FLD(1).
           MOVE FRM-CPIM-UBY TO FRM-VAL-MMIN-ATR IN ACG-FLD(2).
           MOVE FRM-CPIM-UBY TO FRM-VAL-MMIN-ATR IN ACG-FLD(3).
           MOVE FRM-CPIM-UBY TO FRM-VAL-MMIN-ATR IN ACG-FLD(4).
           MOVE FRM-CPIM-UBY TO FRM-VAL-MMIN-ATR IN ACG-FLD(5).
           MOVE FRM-CPIM-UBY TO FRM-VAL-MMIN-ATR IN ACG-FLD(6).
           MOVE FRM-CPIM-UBY TO FRM-VAL-MMAX-ATR IN ACG-FLD(1).
           MOVE FRM-CPIM-UBY TO FRM-VAL-MMAX-ATR IN ACG-FLD(2).
           MOVE FRM-CPIM-UBY TO FRM-VAL-MMAX-ATR IN ACG-FLD(3).
           MOVE FRM-CPIM-UBY TO FRM-VAL-MMAX-ATR IN ACG-FLD(4).
           MOVE FRM-CPIM-UBY TO FRM-VAL-MMAX-ATR IN ACG-FLD(5).
           MOVE FRM-CPIM-UBY TO FRM-VAL-MMAX-ATR IN ACG-FLD(6).
           MOVE FRM-CPIM-UBY TO FRM-VAL-PGAR-ATR IN ACG-FLD(1).
           MOVE FRM-CPIM-UBY TO FRM-VAL-PGAR-ATR IN ACG-FLD(2).
           MOVE FRM-CPIM-UBY TO FRM-VAL-PGAR-ATR IN ACG-FLD(3).
           MOVE FRM-CPIM-UBY TO FRM-VAL-PGAR-ATR IN ACG-FLD(4).
           MOVE FRM-CPIM-UBY TO FRM-VAL-PGAR-ATR IN ACG-FLD(5).
           MOVE FRM-CPIM-UBY TO FRM-VAL-PGAR-ATR IN ACG-FLD(6).
           MOVE FRM-CPIM-UBY TO FRM-NUM-PMIN-ATR IN ACG-FLD(1).
           MOVE FRM-CPIM-UBY TO FRM-NUM-PMIN-ATR IN ACG-FLD(2).
           MOVE FRM-CPIM-UBY TO FRM-NUM-PMIN-ATR IN ACG-FLD(3).
           MOVE FRM-CPIM-UBY TO FRM-NUM-PMIN-ATR IN ACG-FLD(4).
           MOVE FRM-CPIM-UBY TO FRM-NUM-PMIN-ATR IN ACG-FLD(5).
           MOVE FRM-CPIM-UBY TO FRM-NUM-PMIN-ATR IN ACG-FLD(6).
           MOVE FRM-CPIM-UBY TO FRM-IND-PMIN-ATR IN ACG-FLD(1).
           MOVE FRM-CPIM-UBY TO FRM-IND-PMIN-ATR IN ACG-FLD(2).
           MOVE FRM-CPIM-UBY TO FRM-IND-PMIN-ATR IN ACG-FLD(3).
           MOVE FRM-CPIM-UBY TO FRM-IND-PMIN-ATR IN ACG-FLD(4).
           MOVE FRM-CPIM-UBY TO FRM-IND-PMIN-ATR IN ACG-FLD(5).
           MOVE FRM-CPIM-UBY TO FRM-IND-PMIN-ATR IN ACG-FLD(6).
           MOVE FRM-CPIM-UBY TO FRM-NUM-PMAX-ATR IN ACG-FLD(1).
           MOVE FRM-CPIM-UBY TO FRM-NUM-PMAX-ATR IN ACG-FLD(2).
           MOVE FRM-CPIM-UBY TO FRM-NUM-PMAX-ATR IN ACG-FLD(3).
           MOVE FRM-CPIM-UBY TO FRM-NUM-PMAX-ATR IN ACG-FLD(4).
           MOVE FRM-CPIM-UBY TO FRM-NUM-PMAX-ATR IN ACG-FLD(5).
           MOVE FRM-CPIM-UBY TO FRM-NUM-PMAX-ATR IN ACG-FLD(6).
           MOVE FRM-CPIM-UBY TO FRM-IND-PMAX-ATR IN ACG-FLD(1).
           MOVE FRM-CPIM-UBY TO FRM-IND-PMAX-ATR IN ACG-FLD(2).
           MOVE FRM-CPIM-UBY TO FRM-IND-PMAX-ATR IN ACG-FLD(3).
           MOVE FRM-CPIM-UBY TO FRM-IND-PMAX-ATR IN ACG-FLD(4).
           MOVE FRM-CPIM-UBY TO FRM-IND-PMAX-ATR IN ACG-FLD(5).
           MOVE FRM-CPIM-UBY TO FRM-IND-PMAX-ATR IN ACG-FLD(6).
           MOVE FRM-CPIM-UBY TO FRM-NUM-PREN-ATR IN ACG-FLD(1).
           MOVE FRM-CPIM-UBY TO FRM-NUM-PREN-ATR IN ACG-FLD(2).
           MOVE FRM-CPIM-UBY TO FRM-NUM-PREN-ATR IN ACG-FLD(3).
           MOVE FRM-CPIM-UBY TO FRM-NUM-PREN-ATR IN ACG-FLD(4).
           MOVE FRM-CPIM-UBY TO FRM-NUM-PREN-ATR IN ACG-FLD(5).
           MOVE FRM-CPIM-UBY TO FRM-NUM-PREN-ATR IN ACG-FLD(6).
           MOVE FRM-CPIM-UBY TO FRM-IND-PREN-ATR IN ACG-FLD(1).
           MOVE FRM-CPIM-UBY TO FRM-IND-PREN-ATR IN ACG-FLD(2).
           MOVE FRM-CPIM-UBY TO FRM-IND-PREN-ATR IN ACG-FLD(3).
           MOVE FRM-CPIM-UBY TO FRM-IND-PREN-ATR IN ACG-FLD(4).
           MOVE FRM-CPIM-UBY TO FRM-IND-PREN-ATR IN ACG-FLD(5).
           MOVE FRM-CPIM-UBY TO FRM-IND-PREN-ATR IN ACG-FLD(6).
           PERFORM UNP-ACG-ACG.
           PERFORM UNP-LCC-ACG.
           PERFORM UNP-DBC-ACG.
           PERFORM UNP-LDC-ACG.
       FIN-UNP-FLD-ACG.
           EXIT.
       
       UNP-ACG-ACG SECTION.
       INI-UNP-ACG-ACG.
       FIN-UNP-ACG-ACG.
           EXIT.
       
       UNP-LCC-ACG SECTION.
       INI-UNP-LCC-ACG.
       FIN-UNP-LCC-ACG.
           EXIT.
       
       UNP-DBC-ACG SECTION.
       INI-UNP-DBC-ACG.
       FIN-UNP-DBC-ACG.
           EXIT.
       
       UNP-LDC-ACG SECTION.
       INI-UNP-LDC-ACG.
       FIN-UNP-LDC-ACG.
           EXIT.
       
       VAL-KEY-ACG SECTION.
       INI-VAL-KEY-ACG.
           PERFORM VAL-IKY-ACG.
           PERFORM VAL-MKY-ACG.
           PERFORM VAL-AKY-ACG.
       FIN-VAL-KEY-ACG.
           EXIT.
       
       VAL-IKY-ACG SECTION.
       INI-VAL-IKY-ACG.
           PERFORM VAL-LCC-CAI-ICLF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO LCC-CAI-ICLF-LEN IN ACG-FLD
                GO TO FIN-VAL-IKY-ACG.
           PERFORM VAL-LCC-IIC-ICLF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO LCC-IIC-ICLF-LEN IN ACG-FLD
                GO TO FIN-VAL-IKY-ACG.
           PERFORM VAL-LCC-NUM-ILCC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO LCC-NUM-ILCC-LEN IN ACG-FLD
                GO TO FIN-VAL-IKY-ACG.
           PERFORM VAL-ACG-NUM-IACG.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO ACG-NUM-IACG-LEN IN ACG-FLD
                GO TO FIN-VAL-IKY-ACG.
       FIN-VAL-IKY-ACG.
           EXIT.
       
       VAL-MKY-ACG SECTION.
       INI-VAL-MKY-ACG.
       FIN-VAL-MKY-ACG.
           EXIT.
       
       VAL-AKY-ACG SECTION.
       INI-VAL-AKY-ACG.
       FIN-VAL-AKY-ACG.
           EXIT.
       
       VAL-LCC-CAI-ICLF SECTION.
       INI-VAL-LCC-CAI-ICLF.
           IF LCC-CAI-ICLF IN ACG-FLD NOT > SPACES
              MOVE SPACES TO LCC-CAI-ICLF IN ACG-FLD.
       FIN-VAL-LCC-CAI-ICLF.
           EXIT.
       
       VAL-LCC-IIC-ICLF SECTION.
       INI-VAL-LCC-IIC-ICLF.
       FIN-VAL-LCC-IIC-ICLF.
           EXIT.
       
       VAL-LCC-NUM-ILCC SECTION.
       INI-VAL-LCC-NUM-ILCC.
       FIN-VAL-LCC-NUM-ILCC.
           EXIT.
       
       VAL-ACG-NUM-IACG SECTION.
       INI-VAL-ACG-NUM-IACG.
       FIN-VAL-ACG-NUM-IACG.
           EXIT.
       
       VAL-CON-KEY-ACG SECTION.
       INI-VAL-CON-KEY-ACG.
           IF     LCC-CAI-ICLF IN ACG-FLD NOT > SPACES 
              AND LCC-IIC-ICLF IN ACG-FLD NOT > ZEROES 
              AND LCC-NUM-ILCC IN ACG-FLD NOT > ZEROES 
              AND ACG-NUM-IACG IN ACG-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-ACG.
