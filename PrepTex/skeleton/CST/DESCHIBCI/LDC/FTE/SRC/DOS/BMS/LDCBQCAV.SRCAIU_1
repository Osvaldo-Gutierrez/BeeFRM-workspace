       SET-TAG-LDCBQCAV SECTION.
       INI-SET-TAG-LDCBQCAV.
           MOVE
           '<IDNMOD NOM=$LDCBQCAV$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$08-May-19 12:06 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-LDCBQCAV.
           EXIT.
           
       PUT-ALL-CAV SECTION.
       INI-PUT-ALL-CAV.
           PERFORM PUT-DBC-CAV.
           PERFORM PUT-LDC-CAV.
           PERFORM PUT-FIA-CAV.
           PERFORM PUT-AMF-CAV.
           PERFORM PUT-CLF-CAV.
       FIN-PUT-ALL-CAV.
           EXIT.
       
       PUT-DBC-CAV SECTION.
       INI-PUT-DBC-CAV.
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-CAV.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-CAV
               GO TO FIN-PUT-DBC-CAV.
      * IDC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN CAV-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN CAV-FLD.
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN CAV-FLD.
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN CAV-FLD.
      * IDC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN CAV-FLD.
       FIN-PUT-DBC-CAV.
           EXIT.
       
       PUT-LDC-CAV SECTION.
       INI-PUT-LDC-CAV.
           IF PGM-STAT-LDC-NUL
               GO TO FIN-PUT-LDC-CAV.
           IF NOT PGM-STAT-LDC-OKS
               PERFORM CLR-LDC-CAV
               GO TO FIN-PUT-LDC-CAV.
           MOVE LDC-CAI-ICLF IN LDC TO LDC-CAI-ICLF IN CAV-FLD.
           MOVE LDC-IIC-ICLF IN LDC TO LDC-IIC-ICLF IN CAV-FLD.
           MOVE LDC-NUM-ILDC IN LDC TO LDC-NUM-ILDC IN CAV-FLD.
       FIN-PUT-LDC-CAV.
           EXIT.
       
       PUT-FIA-CAV SECTION.
       INI-PUT-FIA-CAV.
           IF PGM-STAT-FIA-NUL
               GO TO FIN-PUT-FIA-CAV.
           IF NOT PGM-STAT-FIA-OKS
               PERFORM CLR-FIA-CAV
               GO TO FIN-PUT-FIA-CAV.
           MOVE FIA-COD-TOFS IN FIA TO FIA-COD-TOFS IN CAV-FLD.
       FIN-PUT-FIA-CAV.
           EXIT.
       
       PUT-AMF-CAV SECTION.
       INI-PUT-AMF-CAV.
           IF PGM-STAT-AMF-NUL
               GO TO FIN-PUT-AMF-CAV.
           IF NOT PGM-STAT-AMF-OKS
               PERFORM CLR-AMF-CAV
               GO TO FIN-PUT-AMF-CAV.
           MOVE AMF-COD-TOOA IN AMF TO AMF-COD-TOOA IN CAV-FLD.
       FIN-PUT-AMF-CAV.
           EXIT.
       
       PUT-CLF-CAV SECTION.
       INI-PUT-CLF-CAV.
           IF PGM-STAT-CLF-NUL
               GO TO FIN-PUT-CLF-CAV.
           IF NOT PGM-STAT-CLF-OKS
               PERFORM CLR-CLF-CAV
               GO TO FIN-PUT-CLF-CAV.
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CLF-NUM-DVEN IN CLF TO CLF-NUM-DVEN IN CAV-FLD.
           MOVE CLF-NUM-MVEN IN CLF TO CLF-NUM-MVEN IN CAV-FLD.
           MOVE CLF-NUM-SVEN IN CLF TO CLF-NUM-SVEN IN CAV-FLD.
           MOVE CLF-NUM-AVEN IN CLF TO CLF-NUM-AVEN IN CAV-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CLF-NUM-DAPE IN CLF TO CLF-NUM-DAPE IN CAV-FLD.
           MOVE CLF-NUM-MAPE IN CLF TO CLF-NUM-MAPE IN CAV-FLD.
           MOVE CLF-NUM-SAPE IN CLF TO CLF-NUM-SAPE IN CAV-FLD.
           MOVE CLF-NUM-AAPE IN CLF TO CLF-NUM-AAPE IN CAV-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
       FIN-PUT-CLF-CAV.
           EXIT.
       
       KEY-ALL-CAV SECTION.
       INI-KEY-ALL-CAV.
           PERFORM KEY-DBC-CAV.
           PERFORM KEY-LDC-CAV.
           PERFORM KEY-FIA-CAV.
           PERFORM KEY-AMF-CAV.
           PERFORM KEY-CLF-CAV.
       FIN-KEY-ALL-CAV.
           EXIT.
       
       KEY-DBC-CAV SECTION.
       INI-KEY-DBC-CAV.
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN CAV-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN CAV-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN CAV-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN CAV-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-GLS-NOMC IN CAV-FLD TO DBC-GLS-NOMC IN DBC.
       FIN-KEY-DBC-CAV.
           EXIT.
       
       KEY-LDC-CAV SECTION.
       INI-KEY-LDC-CAV.
           MOVE LDC-CAI-ICLF IN CAV-FLD TO LDC-CAI-ICLF IN LDC.
           MOVE LDC-IIC-ICLF IN CAV-FLD TO LDC-IIC-ICLF IN LDC.
           MOVE LDC-NUM-ILDC IN CAV-FLD TO LDC-NUM-ILDC IN LDC.
       FIN-KEY-LDC-CAV.
           EXIT.
       
       KEY-FIA-CAV SECTION.
       INI-KEY-FIA-CAV.
           MOVE FIA-COD-TOFS IN CAV-FLD TO FIA-COD-TOFS IN FIA.
       FIN-KEY-FIA-CAV.
           EXIT.
       
       KEY-AMF-CAV SECTION.
       INI-KEY-AMF-CAV.
           MOVE AMF-COD-TOOA IN CAV-FLD TO AMF-COD-TOOA IN AMF.
       FIN-KEY-AMF-CAV.
           EXIT.
       
       KEY-CLF-CAV SECTION.
       INI-KEY-CLF-CAV.
       FIN-KEY-CLF-CAV.
           EXIT.
       
       SCR-CLRA-CAV SECTION.
       INI-SCR-CLRA-CAV.
           PERFORM CLR-KEY-CAV.
           PERFORM CLR-FLD-CAV.
       FIN-SCR-CLRA-CAV.
           EXIT.
       
       CLR-KEY-CAV SECTION.
       INI-CLR-KEY-CAV.
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN CAV-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN CAV-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN CAV-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN CAV-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO DBC-GLS-NOMC IN CAV-FLD.
           MOVE SPACES TO LDC-CAI-ICLF IN CAV-FLD.
           MOVE ZEROES TO LDC-IIC-ICLF IN CAV-FLD.
           MOVE ZEROES TO LDC-NUM-ILDC IN CAV-FLD.
           MOVE SPACES TO FIA-COD-TOFS IN CAV-FLD.
           MOVE SPACES TO AMF-COD-TOOA IN CAV-FLD.
       FIN-CLR-KEY-CAV.
           EXIT.
       
       CLR-FLD-CAV SECTION.
       INI-CLR-FLD-CAV.
