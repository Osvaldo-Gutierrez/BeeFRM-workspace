       PROCEDURE DIVISION.
      *===================

       MAIN SECTION.
       INI-MAIN.
           PERFORM INI.
       LUP-MAIN.
           IF SCR-INST = SCR-INST-HCM
                 PERFORM SCR-COM.
           IF SCR-CCPP = 'ACC'
                 PERFORM SCR-ACC
           ELSE
                 MOVE SCR-INST-HCM TO SCR-INST.
           GO TO LUP-MAIN.
       FIN-MAIN.
           EXIT.

       INI SECTION.
       INI-INI.
       COPY GNSBGEIB.
       COPY GNSBGEDB.
           MOVE LOW-VALUES TO QPR-FLD.
           MOVE LOW-VALUES TO QPR-EDT.
           PERFORM GNS-PRO-RCK.
           PERFORM GNS-BUS-VCA.
           MOVE +3000 TO SCR-TCMA.
           PERFORM GNS-HDL-SYS.
           PERFORM GNS-HDL-FRM.
           MOVE 'LDCPQQPR' TO FIO-PROG.
           PERFORM GNS-BUS-IDD.
      *PTC-INI
           PERFORM GNS-ERR-QUE.
           PERFORM GET-TSK-TERM.
      *PTC-FIN
      *<<<< INI_FIO
           IF SCR-OFRM-SRV
                MOVE TSK-TERM-ALF TO QPR-COD-TERM
                MOVE TSK-TERM-ALF TO QPR-COD-TERM-MAS
           ELSE
                MOVE EIBTRMID     TO QPR-COD-TERM
                MOVE EIBTRMID     TO QPR-COD-TERM-MAS.
           MOVE +3859 TO SCR-TCMA.
           PERFORM GNS-ERR-QUE.
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )
               MOVE SCR-QPR-FLD TO QPR-FLD
               GO TO FIN-INI.

           IF ( FRM-FFLD = FRM-FFLD-PF6 )
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM
               ADD 1              TO QUE-NITM
               MOVE QPR-NOM-TSQPR TO QUE-COLA
               MOVE QPR-LEN-TSQPR TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT NOT = QUE-STAT-OKS
                  MOVE SCR-QPR-FLD TO QPR-FLD
                  GO TO FIN-INI.

      *>>>>
       FIN-INI.
           EXIT.

       COPY GNSBHSYS.
       COPY GNSBHFRM.
       COPY GNSBPRCK.
       GNS-BUS-VCA SECTION.
       INI-GNS-BUS-VCA.
           MOVE 0 TO FRM-IFLD.
       LUP-GNS-BUS-VCA.
           ADD 1 TO FRM-IFLD.
           IF FRM-IFLD NOT > EIBCALEN AND
              FRM-IFLD NOT > 3000
                MOVE DFH-CMMA(FRM-IFLD) TO PGM-CMMA-CHR(FRM-IFLD)
                GO TO LUP-GNS-BUS-VCA.
           MOVE PGM-CMMA TO SCR-VARI.
       FIN-GNS-BUS-VCA.
           EXIT.
       COPY GNSBBIDD.

       SCR-COM SECTION.
       INI-SCR-COM.
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-CLRA-QPR.
           PERFORM EDT-KEY-QPR.
           PERFORM EDT-FLD-QPR.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-HCM TO SCR-INST.
           MOVE SCR-CMNP     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       XCT-ERR.
           MOVE 'XCTERR    ' TO MSG-COD-MENS.
           MOVE 'GNS'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO SYS-MEN1.
           MOVE SYS-PROG     TO SYS-MEN2.
           MOVE SYS-ABOR     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       COPY GNSBGGBK.
       FIN-SCR-COM.
           EXIT.



       SCR-ACC SECTION.
       INI-SCR-ACC.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ACC
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-ACC.
           PERFORM DESSCRTRL.
      *<<<< PFS_SCR_ACC
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM VAL-NUM-KEY-QPR
               PERFORM VAL-NUM-FLD-QPR
               PERFORM SCRL-TAB-QPR
               PERFORM SCR-TRL.
      *>>>>
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ACC.
           EXIT.

       BRW-SCR-ACC SECTION.
       INI-BRW-SCR-ACC.
       FIN-BRW-SCR-ACC.
           EXIT.




       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               PERFORM SCR-CLRA-QPR
               PERFORM EDT-KEY-QPR
               PERFORM EDT-FLD-QPR
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-QPR
               PERFORM EDT-KEY-QPR
           ELSE
               PERFORM CLR-FLD-QPR
               PERFORM EDT-FLD-QPR.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

       SCR-KEY SECTION.
       INI-SCR-KEY.
           PERFORM SCR-RST.
           PERFORM GET-KEY.
           IF FRM-STAT-ABT
                   MOVE SPACES TO FRM-MENS
                   PERFORM SCR-MSSG
                   GO TO FIN-SCR-KEY.
       FND-SCR-KEY.
           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
           IF FIO-STAT-OKS
               PERFORM PUT-SCR
               MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
               GO TO FIN-SCR-KEY.
      *JSS ELSE
      *        PERFORM CLR-FLD-QPR.
       ERR-SCR-KEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR.
           IF FRM-MENS NOT > SPACES
               IF FIO-STAT-EOF
                   MOVE 'REGEOF    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
               IF FIO-STAT = FIO-STAT-NEX
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
           MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-SCR-KEY.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO QPR-FLD.
           MOVE LOW-VALUES TO QPR-EDT.
       FIN-SCR-RST.
           EXIT.

       GET-KEY SECTION.
       INI-GET-KEY.
           PERFORM SCR-GKEY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-KEY.
           EXIT.

       GET-IKY SECTION.
       INI-GET-IKY.
           PERFORM SCR-GIKY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-IKY.
           EXIT.

       FND-KEY SECTION.
       INI-FND-KEY.
      *<<<< INI_FND_KEY
      * HRS 20081203  INI                                               
           IF FRM-IND-HMAS IN QPR-FLD = 'S'                             
              MOVE FRM-CAI-HCLF IN QPR-FLD TO RDC-CAI-ICLF IN RDC       
              MOVE FRM-IIC-HCLF IN QPR-FLD TO RDC-IIC-ICLF IN RDC       
           ELSE                                                         
              MOVE SPACES                  TO RDC-KEY-ICLF IN RDC.      
           MOVE SCR-CIC-ICLI   TO RDC-CIC-ICLI IN RDC.
           MOVE ZEROES         TO WSS-J IN WSS-VARI.
           MOVE FIO-GET-NLS    TO FIO-CMND.
       LUP-LEE-RDC.
      *    Se lee cada RDC con el CIC del cliente
           PERFORM LDC-FIO-RDC.
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF
               GO TO CON-FND-KEY.

           IF (SCR-CIC-ICLI            NOT = RDC-CIC-ICLI IN RDC)
               GO TO CON-FND-KEY.

      *    Se lee con cada RDC cada CLF para el cliente
           MOVE RDC-CIC-ICLF IN RDC TO CLF-CIC-ICLF IN CLF.
           MOVE FIO-GET-KEY         TO FIO-CMND.
           PERFORM LDC-FIO-CLF.
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF
               GO TO NXT-LEE-RDC.
      *        GO TO CON-FND-KEY.

           IF (RDC-CIC-ICLF IN RDC  NOT = CLF-CIC-ICLF IN CLF)
               GO TO NXT-LEE-RDC.
      *        GO TO CON-FND-KEY.

      *    Se modifica el estado desde 'A', ahora en 'F'
      *       mail Carlos Maturana 20080813 16:00 hrs.
      *    IF NOT (CLF-MSC-STAT IN CLF = 'A' AND
      *SOLICITUD   CARLOS MATURANA 20081203 13:00 HRS.                  
      *    IF NOT (CLF-MSC-STAT IN CLF = 'F' AND
           IF NOT (CLF-COD-TOCF IN CLF = 'PRY')
               GO TO NXT-LEE-RDC.
      *        GO TO CON-FND-KEY.

      *    Se lee con cada CLF su correspondiente ACL para el cliente
           MOVE CLF-CIC-ICLF IN CLF TO ACL-CIC-ICLF IN ACL.
           MOVE FIO-GET-KEY         TO FIO-CMND.
           PERFORM LDC-FIO-ACL.
           
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF
               GO TO NXT-LEE-RDC.
      *        GO TO CON-FND-KEY.

           IF (CLF-CIC-ICLF IN CLF  NOT = ACL-CIC-ICLF IN ACL)
               GO TO NXT-LEE-RDC.
      *        GO TO CON-FND-KEY.


      *    Se carga el vector con los datos obtenidos hasta ahora
           PERFORM CRG-VEC-QPR.

           IF (SCR-OFRM-SRV AND WSS-J > WSS-NITMMAX)
               GO TO CON-FND-KEY.

           IF ((NOT SCR-OFRM-SRV) AND WSS-J > WSS-JMAX)
               GO TO CON-FND-KEY.

       NXT-LEE-RDC.
           MOVE FIO-GET-NXT TO FIO-CMND.
           GO TO LUP-LEE-RDC.
      *>>>>
           PERFORM KEY-ALL-QPR.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM LDC-FIO-ACL.
           MOVE FIO-STAT TO PGM-STAT-ACL.
      *<<<< FIN_FND_KEY
       CON-FND-KEY.
           PERFORM PUT-TAB.

           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *----------------------------------------------------------
       MSG-FND-KEY.
           MOVE 'TAB' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *>>>>
       FIN-FND-KEY.
           IF FIO-STAT = FIO-STAT-NEX
               MOVE 'S' TO SCR-IERR.
       EXT-FND-KEY.
           EXIT.
 
       PUT-SCR SECTION.
       INI-PUT-SCR.
           PERFORM PUT-ALL-QPR.
       FIN-PUT-SCR.
           EXIT.

       STP-EXIT SECTION.
       INI-STP-EXIT.
           MOVE SCR-PROG TO PGM-PROG.
           MOVE SCR-NTRN TO PGM-NTRN.

           MOVE SCR-PROV TO SCR-PROG.
           MOVE SCR-NTRV TO SCR-NTRN.

           MOVE PGM-PROG TO SCR-PROV
           MOVE PGM-NTRN TO SCR-NTRV.
       FIN-STP-EXIT.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO SYS-NTRN.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
      *PTC-INI
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE QPR-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO QPR-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GKEY SECTION.
       INI-SCR-GKEY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-QPR
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO QPR-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GKEY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-QPR.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-QPR
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-QPR.
           PERFORM VAL-IKY-QPR.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-MKY-QPR.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-AKY-QPR.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-CON-KEY-QPR.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GKEY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-QPR.
       ERR-SCR-GKEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-QPR.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GKEY.
           PERFORM SCR-MSSG.

       SCR-GIKY SECTION.
       INI-SCR-GIKY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-QPR
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO QPR-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GIKY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-QPR.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-IKY-QPR
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-QPR.
           PERFORM VAL-IKY-QPR.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GIKY.
           PERFORM VAL-CON-IKY-QPR.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GIKY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-QPR.
       ERR-SCR-GIKY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-IKY-QPR.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GIKY.
           PERFORM SCR-MSSG.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
      *>>>>
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO QPR-GLS-MEN2
               MOVE SCR-PRMT     TO QPR-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO QPR-FLD.
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO QPR-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE QPR-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO QPR-FLD
      * FLB  7-NOV-1994
               PERFORM CLR-KEY-QPR

               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO QPR-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO QPR-GLS-MENS.
           PERFORM PRO-MKY-QPR.
           PERFORM PRO-AKY-QPR.
           PERFORM UNP-IKY-QPR.
           PERFORM FST-KEY-QPR.
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE QPR-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO QPR-FLD
               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO QPR-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO QPR-GLS-MENS.
           PERFORM UNP-KEY-QPR.
           PERFORM FST-KEY-QPR.
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
           PERFORM PRO-IKY-QPR.
           PERFORM PRO-AKY-QPR.
           PERFORM UNP-MKY-QPR.
           PERFORM UNP-FLD-QPR.
           PERFORM FST-FLD-QPR.
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO QPR-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-QPR.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
      *PTC-INI
           IF SCR-STAT NOT = SCR-STPP
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-QPR
               PERFORM EDT-FLD-QPR
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-QPR
               PERFORM EDT-FLD-QPR
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-QPR.
           PERFORM EDT-FLD-QPR.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

      * PGM-EBR-ALL SECTION.
      * INI-PGM-EBR-ALL.
      *     MOVE 'GNS' TO FIO-SIST.
      *     MOVE FIO-EBR-ALL TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
      * FIN-PGM-EBR-ALL.
      *     EXIT.

       COPY LDCBQQPR.
       COPY GNSBVNUM.
       COPY LDCBFACL.
       COPY GNSBFMSG.
       COPY GNSBGSYS.
       COPY GNSBGDTC.
      *PTC-INI
      *PTC-SUB
      * ISP  10-MAY-2001 13:46:35 
      * COPY GNSBGFRM.
       COPY GNSBG3CF.
      *PTC-FIN
       COPY GNSBGMSG.
       COPY GNSBEKEY.
       COPY GNSBIABT.
       COPY GNSBFTAB.
       COPY GNSBVIND.
       COPY GNSBVMSC.
       COPY GNSBFMSC.
       COPY GNSBVIDC.
       COPY GNSBPVRF.
      *PTC-INI
       COPY GNSBGQUE.
       COPY GNSBGTSK.
      *PTC-FIN

      *<<<< EOF
       COPY GNSBBTAB.
       COPY GNSBVTAB.
       COPY GNSBVCOD.
       COPY SGCBFDBC.
       COPY LDCBFRDC.
       COPY LDCBFCLF.

       PUT-TAB SECTION.
       INI-PUT-TAB.
           MOVE QPR-NOM-TSQPR     TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE QPR-NOM-TSQPR-MAS TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE 1 TO SCR-NUM-PANT.
           MOVE LOW-VALUES TO WSS-GLS-NQPR-IDX-MTI.
           MOVE LOW-VALUES TO WSS-GLS-NQPR-IDX-FRM.
      *
           MOVE 1 TO FRM-IFLD.
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.

           MOVE 1 TO WSS-F IN WSS-VARI.
       LUP-PUT-TAB.
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI
              GO TO FIN-PUT-TAB.
           IF WSS-IIC-ICLF-V(WSS-F) > ZEROES
              PERFORM PUT-TAB-QPR
              PERFORM PUT-QPR-QUE
              PERFORM SET-FRM-IFLD
           ELSE
           IF FRM-IFLD = 1 AND WSS-F = 1
      *        MOVE 'TAB' TO MSG-COD-SIST
              MOVE 'DEU' TO MSG-COD-SIST
              MOVE 'QPR    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE '00' TO FIO-STAT
      *       MOVE 'S' TO SCR-IERR
              GO TO FIN-PUT-TAB
           ELSE
              MOVE '00' TO FIO-STAT
              GO TO FIN-PUT-TAB.

           ADD 1 TO WSS-F IN WSS-VARI.
           GO TO LUP-PUT-TAB.
       FIN-PUT-TAB.
           MOVE 1                  TO QUE-NITM.
           MOVE QPR-LEN-TSQPR-MAS  TO QUE-LITM.
           MOVE QPR-NOM-TSQPR-MAS  TO QUE-COLA.

           IF WSS-NITM > WSS-NITMMAX
              MOVE 'S'           TO QPR-MAS-IND-HMAS
           ELSE
              MOVE 'N'           TO QPR-MAS-IND-HMAS.

           MOVE WSS-NITM         TO WSS-NUM-ICUR.
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.
           MOVE WSS-NUM-ICUR-ALF TO QPR-MAS-NUM-TOCC.
           MOVE QPR-MAS-ITEM     TO QUE-ITEM.
           MOVE QUE-PUT          TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF WSS-NITM > WSS-IMAX
      *         MOVE 'TAB' TO MSG-COD-SIST
               MOVE 'DEU' TO MSG-COD-SIST
               MOVE 'QPRTABDMAS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       EXT-PUT-TAB.
           EXIT.

       SET-FRM-IFLD SECTION.
       INI-SET-FRM-IFLD.
           IF SCR-OFRM-SRV
                MOVE 1 TO FRM-IFLD
           ELSE
                ADD  1 TO FRM-IFLD.
       FIN-SET-FRM-IFLD.
           EXIT.
      *--------------------------------------------
       SCRL-TAB-QPR SECTION.
       INI-SCRL-TAB-QPR.
           MOVE SPACES TO MSG-COD-MENS.

           IF FRM-FFLD = FRM-FFLD-PF6
               PERFORM PUT-TAB-ASC
           ELSE
               PERFORM PUT-TAB-DSC.
           IF MSG-COD-MENS > SPACES
      *         MOVE 'TAB' TO MSG-COD-SIST
               MOVE 'DEU' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-QPR.
           EXIT.

      *--------------------------------------------
       PUT-TAB-QPR SECTION.
       INI-PUT-TAB-QPR.

           PERFORM CLR-WSS.
           MOVE WSS-CAI-ICLF-V(WSS-F) TO WSS-CAI-ICLF-QPR IN WSS-VARI.
           MOVE WSS-IIC-ICLF-V(WSS-F) TO WSS-IIC-ICLF-QPR IN WSS-VARI.
           MOVE WSS-GLS-NOMP-V(WSS-F) TO WSS-GLS-NOMP-QPR IN WSS-VARI.
           MOVE WSS-GLS-STAT-V(WSS-F) TO WSS-GLS-STAT-QPR IN WSS-VARI.
           MOVE WSS-GLS-AOCF-V(WSS-F) TO WSS-GLS-AOCF-QPR IN WSS-VARI.
           MOVE WSS-COD-VCCF-V(WSS-F) TO WSS-COD-VCCF-QPR IN WSS-VARI.
           MOVE WSS-VAL-MCON-V(WSS-F) TO WSS-VAL-MCON-QPR IN WSS-VARI.
           MOVE WSS-GLS-DESC-V(WSS-F) TO WSS-GLS-DESC-QPR IN WSS-VARI.

           MOVE WSS-CAI-ICLF-QPR IN WSS-VARI TO WSS-CAI-ICLF-FRM.
           MOVE WSS-IIC-ICLF-QPR IN WSS-VARI TO WSS-IIC-ICLF-FRM.
           MOVE WSS-GLS-NOMP-QPR IN WSS-VARI TO WSS-GLS-NOMP-FRM.
           MOVE WSS-GLS-STAT-QPR IN WSS-VARI TO WSS-GLS-STAT-FRM.
           MOVE WSS-GLS-AOCF-QPR IN WSS-VARI TO WSS-GLS-AOCF-FRM.
           MOVE WSS-COD-VCCF-QPR IN WSS-VARI TO WSS-COD-VCCF-FRM.
           MOVE WSS-VAL-MCON-QPR IN WSS-VARI TO WSS-VAL-MCON-FRM.
           MOVE WSS-GLS-DESC-QPR IN WSS-VARI TO WSS-GLS-DESC-FRM.

           IF FRM-IFLD > WSS-IMAX
              GO TO FIN-PUT-TAB-QPR.

           MOVE WSS-CAI-ICLF-QPR IN WSS-VARI
             TO FRM-CAI-ICLF     IN QPR-FLD(FRM-IFLD).
           MOVE WSS-IIC-ICLF-QPR IN WSS-VARI
             TO FRM-IIC-ICLF     IN QPR-FLD(FRM-IFLD).
           MOVE WSS-GLS-NOMP-QPR IN WSS-VARI
             TO FRM-GLS-NOMP     IN QPR-FLD(FRM-IFLD).
           MOVE WSS-GLS-STAT-QPR IN WSS-VARI
             TO FRM-GLS-STAT     IN QPR-FLD(FRM-IFLD).
           MOVE WSS-GLS-AOCF-QPR IN WSS-VARI
             TO FRM-GLS-AOCF     IN QPR-FLD(FRM-IFLD).
           MOVE WSS-COD-VCCF-QPR IN WSS-VARI
             TO FRM-COD-VCCF     IN QPR-FLD(FRM-IFLD).
           MOVE WSS-VAL-MCON-QPR IN WSS-VARI
             TO FRM-VAL-MCON     IN QPR-FLD(FRM-IFLD).
           MOVE WSS-GLS-DESC-QPR IN WSS-VARI
             TO FRM-GLS-DESC     IN QPR-FLD(FRM-IFLD).

       FIN-PUT-TAB-QPR.
           EXIT.

      *----------------------------------------------------
       PUT-QPR-QUE SECTION.
       INI-PUT-QPR-QUE.
           PERFORM PRO-FLD-QUE.
           ADD 1 TO WSS-NITM.
           MOVE QPR-NOM-TSQPR    TO QUE-COLA.
           MOVE QPR-LEN-TSQPR    TO QUE-LITM.
           MOVE WSS-NITM         TO QUE-NITM.
           MOVE SPACES           TO QUE-ITEM.
           IF SCR-OFRM-SRV
              MOVE WSS-GLS-NQPR-IDX-MTI TO QUE-ITEM
           ELSE
              MOVE WSS-GLS-NQPR-IDX-FRM TO QUE-ITEM.
           MOVE QUE-PUT  TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
       FIN-PUT-QPR-QUE.
           EXIT.
      *----------------------------------------------------

       PUT-TAB-ASC SECTION.
       INI-PUT-TAB-ASC.
           MOVE SPACES TO MSG-COD-MENS.
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.
           MOVE 0 TO FRM-IFLD.
       LUP-PUT-TAB-ASC.
           ADD 1              TO QUE-NITM.
           MOVE QPR-NOM-TSQPR TO QUE-COLA.
           MOVE QPR-LEN-TSQPR TO QUE-LITM.
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD = 0
                   MOVE 'QPRTABNONXT' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-ASC
               ELSE
                   GO TO FIN-PUT-TAB-ASC.
           ADD 1 TO FRM-IFLD.
           MOVE QUE-ITEM TO FRM-CAI-ICLF-IDX(FRM-IFLD).
           IF FRM-IFLD < WSS-IMAX
               GO TO LUP-PUT-TAB-ASC.
       FIN-PUT-TAB-ASC.
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX
               ADD 1 FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1
                                 UNTIL WSS-I > WSS-IMAX.
           IF FRM-IFLD > 0
               ADD 1 TO SCR-NUM-PANT.
           IF FRM-IFLD = WSS-IMAX
               ADD 1              TO QUE-NITM
               MOVE QPR-NOM-TSQPR TO QUE-COLA
               MOVE QPR-LEN-TSQPR TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT = QUE-STAT-OKS
      *             MOVE 'LDC'        TO MSG-COD-SIST
                   MOVE 'DEU'        TO MSG-COD-SIST
                   MOVE 'QPRTABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-ASC.
           EXIT.

       PUT-TAB-DSC SECTION.
       INI-PUT-TAB-DSC.
           MOVE SPACES TO MSG-COD-MENS.
           IF SCR-NUM-PANT = 1
               MOVE 'QPRTABNOPRV' TO MSG-COD-MENS
               GO TO EXT-PUT-TAB-DSC.

           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.
           ADD 1 TO QUE-NITM.
           ADD 1 WSS-IMAX GIVING FRM-IFLD.
       LUP-PUT-TAB-DSC.
           SUBTRACT  1 FROM QUE-NITM.
           MOVE QPR-NOM-TSQPR TO QUE-COLA
           MOVE QPR-LEN-TSQPR TO QUE-LITM
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD > WSS-IMAX
                   MOVE 'QPRTABNOPRV' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-DSC
               ELSE
                   GO TO FIN-PUT-TAB-DSC.
           SUBTRACT 1 FROM FRM-IFLD.
           MOVE QUE-ITEM TO FRM-CAI-ICLF-IDX(FRM-IFLD).
           IF FRM-IFLD > 1
               GO TO LUP-PUT-TAB-DSC.
       FIN-PUT-TAB-DSC.

           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1
                                 UNTIL WSS-I < 1.

           IF FRM-IFLD NOT > WSS-IMAX
               SUBTRACT 1 FROM SCR-NUM-PANT
      *         MOVE 'LDC'        TO MSG-COD-SIST
               MOVE 'DEU'        TO MSG-COD-SIST
               MOVE 'QPRTABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-DSC.
           EXIT.

       COPY GNSBGCPT.
       COPY GNSBGSTR.

       CLR-FRM SECTION.
       INI-CLR-FRM.
           MOVE SPACES TO FRM-CAI-ICLF IN QPR-FLD(WSS-I).
           MOVE ZEROES TO FRM-IIC-ICLF IN QPR-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-NOMP IN QPR-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-STAT IN QPR-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-AOCF IN QPR-FLD(WSS-I).
           MOVE SPACES TO FRM-COD-VCCF IN QPR-FLD(WSS-I).
           MOVE ZEROES TO FRM-VAL-MCON IN QPR-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-DESC IN QPR-FLD(WSS-I).
       FIN-CLR-FRM.
           EXIT.

       CLR-WSS SECTION.
       INI-CLR-WSS.
           MOVE SPACES TO WSS-CAI-ICLF-QPR IN WSS-VARI.
           MOVE ZEROES TO WSS-IIC-ICLF-QPR IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-NOMP-QPR IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-STAT-QPR IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-AOCF-QPR IN WSS-VARI.
           MOVE SPACES TO WSS-COD-VCCF-QPR IN WSS-VARI.
           MOVE ZEROES TO WSS-VAL-MCON-QPR IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-DESC-QPR IN WSS-VARI.
       FIN-CLR-WSS.
           EXIT.

       CLR-FRM-MTX SECTION.
       INI-CLR-FRM-MTX.
           MOVE SPACES TO FRM-CAI-ICLF IN QPR-FLD(1).
           MOVE ZEROES TO FRM-IIC-ICLF IN QPR-FLD(1).
           MOVE SPACES TO FRM-GLS-NOMP IN QPR-FLD(1).
           MOVE SPACES TO FRM-GLS-STAT IN QPR-FLD(1).
           MOVE SPACES TO FRM-GLS-AOCF IN QPR-FLD(1).
           MOVE SPACES TO FRM-COD-VCCF IN QPR-FLD(1).
           MOVE ZEROES TO FRM-VAL-MCON IN QPR-FLD(1).
           MOVE SPACES TO FRM-GLS-DESC IN QPR-FLD(1).
       FIN-CLR-FRM-MTX.
           EXIT.

       CRG-VEC-QPR SECTION.
       INI-CRG-VEC-QPR.
           ADD 1 TO WSS-J.
           IF WSS-J IN WSS-VARI > WSS-JMAX IN WSS-VARI
               GO TO FIN-CRG-VEC-QPR.
           PERFORM CLR-QPR-VEC.
                                                                        
           MOVE CLF-CAI-ICLF   IN CLF                                   
             TO WSS-CAI-ICLF-V IN WSS-VARI(WSS-J).                      
           MOVE CLF-IIC-ICLF   IN CLF                                   
             TO WSS-IIC-ICLF-V IN WSS-VARI(WSS-J).                      
           MOVE ACL-GLS-NOMB   IN ACL                                   
             TO WSS-GLS-NOMP-V IN WSS-VARI(WSS-J).                      
                                                                        
HRS   *    CAMBIO SOLICITADO POR CMATURANA 22-DIC-2008                  
HRS   *    MOVE 'DEU'               TO TAB-COD-SIST.                    
           MOVE 'TAB'               TO TAB-COD-SIST.                    
           MOVE 'AUX'               TO TAB-COD-TTAB IN TAB. 
           MOVE CLF-COD-COCF IN CLF TO TAB-COD-CTAB IN TAB.
           PERFORM BUS-TAB.
           IF MSG-COD-MENS IN MSG NOT > SPACES
              MOVE TAB-GLS-DESC IN TAB
                TO WSS-GLS-AOCF-V IN WSS-VARI(WSS-J)
           ELSE
              MOVE 'SIN TIPO  '                                         
                TO WSS-GLS-AOCF-V IN WSS-VARI(WSS-J).                   
                                                                        
     ***   IF CLF-COD-STAT IN CLF = 'A'  MODIFICADO 2008-08-13          
      *    IF CLF-COD-STAT IN CLF = 'F'                                 
      *       MOVE 'ACTIVO    ' TO WSS-GLS-STAT-V IN WSS-VARI(WSS-J)    
      *    ELSE                                                         
      *       MOVE 'SIN ESTADO' TO WSS-GLS-STAT-V IN WSS-VARI(WSS-J).   
                                                                        
           MOVE 'DEU'               TO TAB-COD-SIST.                    
           MOVE 'ESP'               TO TAB-COD-TTAB IN TAB.             
           MOVE CLF-MSC-STAT IN CLF TO TAB-COD-CTAB IN TAB.             
           PERFORM BUS-TAB.                                             
           IF MSG-COD-MENS IN MSG NOT > SPACES                          
              MOVE TAB-GLS-DESC IN TAB                                  
                TO WSS-GLS-STAT-V IN WSS-VARI(WSS-J)                    
           ELSE
              MOVE 'SIN ESTADO'                                         
                TO WSS-GLS-STAT-V IN WSS-VARI(WSS-J).                   

           MOVE CLF-COD-VCCF   IN CLF
             TO WSS-COD-VCCF-V IN WSS-VARI(WSS-J).

           IF SCR-OFRM-SRV                                              
              MOVE ACL-VAL-MCON   IN ACL                                
                TO FRM-VAL-MCON-EDT                                     
              MOVE FRM-VAL-MCON-EDT                                     
                TO WSS-VAL-MCON-V-ALF IN WSS-VARI(WSS-J)                
           ELSE                                                         
              MOVE ACL-VAL-MCON   IN ACL                                
                TO WSS-VAL-MCON-V     IN WSS-VARI(WSS-J).               

           MOVE ACL-GLS-DESC   IN ACL
             TO WSS-GLS-DESC-V IN WSS-VARI(WSS-J).

       FIN-CRG-VEC-QPR.
           EXIT.

       CLR-QPR-VEC SECTION.
       INI-CLR-QPR-VEC.
           MOVE SPACES TO WSS-CAI-ICLF-V(WSS-J).
           MOVE ZEROES TO WSS-IIC-ICLF-V(WSS-J).
           MOVE SPACES TO WSS-GLS-NOMP-V(WSS-J).
           MOVE SPACES TO WSS-GLS-STAT-V(WSS-J).
           MOVE SPACES TO WSS-GLS-AOCF-V(WSS-J).
           MOVE SPACES TO WSS-COD-VCCF-V(WSS-J).
           MOVE ZEROES TO WSS-VAL-MCON-V(WSS-J).
           MOVE SPACES TO WSS-GLS-DESC-V(WSS-J).
       FIN-CLR-QPR-VEC.
           EXIT.

       PRO-FLD-QUE SECTION.
       INI-PRO-FLD-QUE.
           MOVE 'P' TO WSS-CAI-ICLF-ATR.
           MOVE 'P' TO WSS-IIC-ICLF-ATR.
           MOVE 'P' TO WSS-GLS-NOMP-ATR.
           MOVE 'P' TO WSS-GLS-STAT-ATR.
           MOVE 'P' TO WSS-GLS-AOCF-ATR.
           MOVE 'P' TO WSS-COD-VCCF-ATR.
           MOVE 'P' TO WSS-VAL-MCON-ATR.
           MOVE 'P' TO WSS-GLS-DESC-ATR.
       FIN-PRO-FLD-QUE.
           EXIT.
      *>>>> EOF
