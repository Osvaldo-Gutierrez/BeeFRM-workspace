       PROCEDURE DIVISION.
      *------------------*
           ENTRY 'DBMSCBL'.
           PERFORM 010-LEER-PARAMETRO.
           PERFORM 020-ABRIR-ARCHIVOS
           PERFORM 030-LEER-ENTRADA
           PERFORM 040-PROCESO UNTIL FIN-ENTRADA = 1.
           PERFORM 100-ESTADISTICAS
           GOBACK.

       010-LEER-PARAMETRO.
      *------------------*
           ACCEPT W-PARAMETRO
           MOVE 'INFO'          TO CALEAREA-FUNCION
           MOVE  W-SIG-PAR      TO CALEAREA-FECHA-1-AM
           MOVE  W-ANO-PAR      TO CALEAREA-FECHA-1-AA
           MOVE  W-MES-PAR      TO CALEAREA-FECHA-1-M
           MOVE  W-DIA-PAR      TO CALEAREA-FECHA-1-D
           CALL 'RUTUCALE' USING RTNWCALE
           IF CALEAREA-COD-RET = 0
              DISPLAY 'PARAMETRO : ' W-PARAMETRO
           ELSE
              DISPLAY 'ERROR EN FECHA DE PARAMETRO : ' W-PARAMETRO
              DISPLAY 'AL LEER RUTINA FECHA, COD-RET. : '
                    CALEAREA-COD-RET
              DISPLAY '*** SE CANCELA PROGRAMA LDCPG001 ***'
              MOVE 100 TO RETURN-CODE
              GOBACK
           END-IF.
           ACCEPT W-FEC-SIS FROM DATE
           IF W-ANO-SIS > 50
              MOVE 19   TO W-SIG-SIS
           ELSE
              MOVE 20   TO W-SIG-SIS
           END-IF.
           ACCEPT W-HOR-SISTEMA FROM TIME.

       020-ABRIR-ARCHIVOS.
      *------------------*
           OPEN INPUT ENTRADA
           IF FS-ENTRADA NOT = 0
              DISPLAY ' ERROR AL ABRIR ENTRADA F.S. : '
                     FS-ENTRADA
              DISPLAY '*** SE CANCELA PROGRAMA LDCPG001 ***'
              MOVE 100 TO RETURN-CODE
              GOBACK
           END-IF.

       030-LEER-ENTRADA.
      *----------------*
           READ ENTRADA AT END MOVE 1 TO FIN-ENTRADA.
           IF FIN-ENTRADA = 0
              ADD 1 TO WL-ENTRADA
           END-IF.

       040-PROCESO.
      *-----------*
           IF LDC-FEC-FVEN NOT < W-FECHA-PAR
              IF LDC-FEC-FINI NOT = W-FECHA-PAR
      *          MOVE LDC-CIC-ICLF   TO CLF-VAL-KEY
      *          CALL 'DBNTRY' USING UIB REQ-AREA-CLF REG-CLF
      *                 ELEM-LIST-CLF
      *          IF CLF-RET-COD = SPACES
      *             ADD 1 TO WL-CLF
                    PERFORM 050-ANALIZA-REGISTRO
      *          ELSE
      *          IF CLF-RET-COD = '14'
      *             ADD 1 TO WN-CLF
      *             DISPLAY 'NO EXISTE REGISTRO EN TABLA CLF : '
      *                     CLF-RET-COD
      *             DISPLAY 'LLAVE           : ' CLF-VAL-KEY
      *          ELSE
      *            DISPLAY 'ERROR AL LEER TABLA CLF : ' CLF-RET-COD
      *            DISPLAY 'LLAVE           : ' CLF-VAL-KEY
      *            DISPLAY '*** SE CANCELA PROGRAMA LDCPG001 ***'
      *            PERFORM 100-ESTADISTICAS
      *            MOVE 100 TO RETURN-CODE
      *            GOBACK
      *          END-IF
      *          END-IF
              ELSE
                 ADD 1 TO WN-ENT2
           ELSE
              ADD 1 TO WN-ENT1
           END-IF.
           PERFORM 030-LEER-ENTRADA.

       050-ANALIZA-REGISTRO.
      *--------------------*
      *    IF CLF-MSC-STAT = 'A'
      *       IF CLF-FEC-FVEN NOT < W-FECHA-PAR
                 ADD 1 TO WP-ENTRADA
                 MOVE SPACES TO REG-CCL
                 MOVE LDC-CIC-ICLF    TO  CCL-CIC-LDC
                 MOVE LDC-COD-TIPO    TO  CCL-COD-TIPO
                 MOVE LDC-FEC-FINI    TO  CCL-FEC-CBR-ANT  CCL-FEC-CBR
                 MOVE 'LDCPG001'      TO  CCL-IDN-ATR-ACT
                 MOVE W-FEC-SISTEMA   TO  CCL-FEC-CRE      CCL-FEC-ACT
                 MOVE W-HORA-SIS      TO  CCL-HOR-ACT      CCL-HOR-CRE
                 MOVE 0               TO  CCL-MTO-CBR-ANT  CCL-MTO-CBR
                                          CCL-PCT-DCT-ANT  CCL-PCT-DCT
                                          CCL-PCT-DCT-PXM
                 PERFORM 070-CALCULA-FECH-PROX-COBRO
                 PERFORM 090-GRABAR-CCL.
      *       ELSE
      *         ADD 1 TO WN-ENT3
      *    ELSE
      *       ADD 1 TO WN-ENTRADA.

       070-CALCULA-FECH-PROX-COBRO.
      *---------------------------*
           MOVE 'AVNDC'             TO CALEAREA-FUNCION
           MOVE LDC-NUM-SVEN        TO CALEAREA-FECHA-1-AM
           MOVE LDC-NUM-AVEN        TO CALEAREA-FECHA-1-AA
           MOVE LDC-NUM-MVEN        TO CALEAREA-FECHA-1-M
           MOVE LDC-NUM-DVEN        TO CALEAREA-FECHA-1-D
           MOVE 1                   TO CALEAREA-NUM-DIA
           PERFORM 080-CALL-RUTUCALE
           MOVE CALEAREA-FECHA-2-AM TO CCL-CBR-PXM-FS
           MOVE CALEAREA-FECHA-2-AA TO CCL-CBR-PXM-FA
           MOVE CALEAREA-FECHA-2-M  TO CCL-CBR-PXM-FM
           MOVE CALEAREA-FECHA-2-D  TO CCL-CBR-PXM-FD.

       080-CALL-RUTUCALE.
      *-----------------*
           CALL 'RUTUCALE' USING RTNWCALE
           IF CALEAREA-COD-RET = 0
              NEXT SENTENCE
           ELSE
              DISPLAY 'ERROR AL LEER RUTINA FECHA, COD-RET. : '
                       CALEAREA-COD-RET
              DISPLAY '*** SE CANCELA PROGRAMA LDCPG001 ***'
              PERFORM 100-ESTADISTICAS
              MOVE 100 TO RETURN-CODE
              GOBACK
           END-IF.

       090-GRABAR-CCL.
      *--------------*
           CALL 'DBNTRY' USING UIB REQ-AREA-CCL REG-CCL
                               ELEM-LIST-CCL
           IF CCL-RET-COD = SPACES
              ADD 1 TO WG-CCL
           ELSE
              DISPLAY 'ERROR AL ADIT TABLA CCL : ' CCL-RET-COD
              DISPLAY 'LLAVE           : ' CCL-KEY-LDC
              DISPLAY '*** SE CANCELA PROGRAMA LDCPG001 ***'
              MOVE 100 TO RETURN-CODE
              PERFORM 100-ESTADISTICAS
              GOBACK
           END-IF.

       100-ESTADISTICAS.
      *----------------*
           DISPLAY '************************************************** '
           DISPLAY '*  E S T A D I S T I C A S      L D C P G 0 0 1  * '
           DISPLAY '************************************************** '
           DISPLAY '*                                                * '
           DISPLAY '* REG. LEIDOS DESDE ENTRADA          : ' WL-ENTRADA
           DISPLAY '* REG. PROCESADOS DESDE ENTRADA      : ' WP-ENTRADA
           DISPLAY '* REG. NO PROC. POR FEC-VCT-LDC < QUE FEC-PARAM '
           DISPLAY '*                                    : ' WN-ENT1
           DISPLAY '* REG. NO PROC. POR FEC-INI-LDC = QUE FEC-PARAM '
           DISPLAY '*                                    : ' WN-ENT2
      *    DISPLAY '* REG. NO PROC. POR STATUS CLF NOT = A '
      *    DISPLAY '*                                    : ' WN-ENTRADA
      *    DISPLAY '* REG. NO PROC. POR FEC-VCT-CLF < QUE FEC-PARAM '
      *    DISPLAY '*                                    : ' WN-ENT3
      *    DISPLAY '* REG. ENCONTRADOS EN TABLA CLF      : ' WL-CLF
      *    DISPLAY '* REG. NO ENCONTRADOS EN TABLA CLF   : ' WN-CLF
           DISPLAY '* REG. GRABADOS EN CCL               : ' WG-CCL
           DISPLAY '*                                                * '
           DISPLAY '************************************************** '
           CLOSE ENTRADA.
