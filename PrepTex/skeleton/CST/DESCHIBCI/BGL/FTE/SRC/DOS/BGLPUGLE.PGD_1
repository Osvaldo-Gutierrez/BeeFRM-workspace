       INI-SCR-DMSG.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               PERFORM SCR-CLRA-GLE
               PERFORM EDT-KEY-GLE
               PERFORM EDT-FLD-GLE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-GLE
               PERFORM EDT-KEY-GLE
           ELSE
               PERFORM CLR-FLD-GLE
               PERFORM EDT-FLD-GLE.
           PERFORM STP-EXIT.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

