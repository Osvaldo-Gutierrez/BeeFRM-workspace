       VAL-KEY-COG SECTION.
       INI-VAL-KEY-COG.
           PERFORM VAL-IKY-COG.
           PERFORM VAL-MKY-COG.
           PERFORM VAL-AKY-COG.
       FIN-VAL-KEY-COG.
           EXIT.
       
       VAL-IKY-COG SECTION.
       INI-VAL-IKY-COG.
           PERFORM VAL-GDG-CAI-GTIA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO GDG-CAI-GTIA-LEN IN COG-FLD
                GO TO FIN-VAL-IKY-COG.
           PERFORM VAL-GDG-IIC-GTIA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO GDG-IIC-GTIA-LEN IN COG-FLD
                GO TO FIN-VAL-IKY-COG.
       FIN-VAL-IKY-COG.
           EXIT.
       
       VAL-MKY-COG SECTION.
       INI-VAL-MKY-COG.
       FIN-VAL-MKY-COG.
           EXIT.
       
       VAL-AKY-COG SECTION.
       INI-VAL-AKY-COG.
       FIN-VAL-AKY-COG.
           EXIT.
       
       VAL-GDG-CAI-GTIA SECTION.
       INI-VAL-GDG-CAI-GTIA.
           IF GDG-CAI-GTIA IN COG-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-GDG-CAI-GTIA.
       FIN-VAL-GDG-CAI-GTIA.
           EXIT.
       
       VAL-GDG-IIC-GTIA SECTION.
       INI-VAL-GDG-IIC-GTIA.
           IF GDG-IIC-GTIA IN COG-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-GDG-IIC-GTIA.
       FIN-VAL-GDG-IIC-GTIA.
           EXIT.
       
       VAL-CON-KEY-COG SECTION.
       INI-VAL-CON-KEY-COG.
           IF     GDG-CAI-GTIA IN COG-FLD NOT > SPACES 
              AND GDG-IIC-GTIA IN COG-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-COG.
       FIN-VAL-CON-KEY-COG.
           EXIT.
       
       VAL-CON-IKY-COG SECTION.
       INI-VAL-CON-IKY-COG.
           IF     GDG-CAI-GTIA IN COG-FLD NOT > SPACES 
              AND GDG-IIC-GTIA IN COG-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-COG.
       FIN-VAL-CON-IKY-COG.
           EXIT.
       
       VAL-NUM-KEY-COG SECTION.
       INI-VAL-NUM-KEY-COG.
           PERFORM VAL-NUM-IKY-COG.
           PERFORM VAL-NUM-MKY-COG.
           PERFORM VAL-NUM-AKY-COG.
       FIN-VAL-NUM-KEY-COG.
           EXIT.
       
       VAL-NUM-IKY-COG SECTION.
       INI-VAL-NUM-IKY-COG.
       VAL-NUM-GDG-IIC-GTIA.
           MOVE GDG-IIC-GTIA-ALF IN COG-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO GDG-IIC-GTIA IN COG-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO GDG-IIC-GTIA-LEN IN COG-FLD
                GO TO FIN-VAL-NUM-IKY-COG.
       FIN-VAL-NUM-IKY-COG.
           EXIT.
       
       VAL-NUM-MKY-COG SECTION.
       INI-VAL-NUM-MKY-COG.
       FIN-VAL-NUM-MKY-COG.
           EXIT.
       
       VAL-NUM-AKY-COG SECTION.
