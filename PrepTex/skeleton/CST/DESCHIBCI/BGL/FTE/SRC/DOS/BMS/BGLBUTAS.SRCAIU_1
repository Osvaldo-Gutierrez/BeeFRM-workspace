      *
      * BGLBUTAS
      * Version 3.0.1 Nov-2005
      * 26-Feb-19 08:04 PM
      *
       SET-TAG-BGLBUTAS SECTION.
       INI-SET-TAG-BGLBUTAS.
           MOVE
           '<IDNMOD NOM=$BGLBUTAS$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$26-Feb-19 08:04 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-BGLBUTAS.
           EXIT.
           
       RET-ALL-TAS SECTION.
       INI-RET-ALL-TAS.
           PERFORM KEY-ALL-TAS.
           PERFORM RET-TAS-TAS.
           PERFORM RET-GDG-TAS.
       FIN-RET-ALL-TAS.
           EXIT.
       
       RET-TAS-TAS SECTION.
       INI-RET-TAS-TAS.
           MOVE TAS-IND-ESTA IN TAS-FLD TO TAS-IND-ESTA IN TAS.
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE TAS-NUM-DTSN IN TAS-FLD TO TAS-NUM-DTSN IN TAS.
           MOVE TAS-NUM-MTSN IN TAS-FLD TO TAS-NUM-MTSN IN TAS.
           MOVE TAS-NUM-STSN IN TAS-FLD TO TAS-NUM-STSN IN TAS.
           MOVE TAS-NUM-ATSN IN TAS-FLD TO TAS-NUM-ATSN IN TAS.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE TAS-VAL-MNTO IN TAS-FLD TO TAS-VAL-MNTO IN TAS.
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE TAS-NUM-DVEX IN TAS-FLD TO TAS-NUM-DVEX IN TAS.
           MOVE TAS-NUM-MVEX IN TAS-FLD TO TAS-NUM-MVEX IN TAS.
           MOVE TAS-NUM-SVEX IN TAS-FLD TO TAS-NUM-SVEX IN TAS.
           MOVE TAS-NUM-AVEX IN TAS-FLD TO TAS-NUM-AVEX IN TAS.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE TAS-GLS-EXCE IN TAS-FLD TO TAS-GLS-EXCE IN TAS.
      *<<<< FIN_RET_TAS_TAS
           MOVE GDG-TIP-CGTA IN TAS-FLD TO TAS-COD-TOPE IN TAS.
           MOVE GDG-STP-CGTA IN TAS-FLD TO TAS-COD-SOPE IN TAS.
      *>>>>
       FIN-RET-TAS-TAS.
           EXIT.
       
       RET-GDG-TAS SECTION.
       INI-RET-GDG-TAS.
           MOVE GDG-TIP-CGTA IN TAS-FLD TO GDG-TIP-CGTA IN GDG.
           MOVE GDG-STP-CGTA IN TAS-FLD TO GDG-STP-CGTA IN GDG.
           MOVE GDG-COD-OFIC IN TAS-FLD TO GDG-COD-OFIC IN GDG.
           MOVE GDG-COD-VCAM IN TAS-FLD TO GDG-COD-VCAM IN GDG.
           MOVE GDG-GLS-DGTA IN TAS-FLD TO GDG-GLS-DGTA IN GDG.
           MOVE GDG-GLS-DGT1 IN TAS-FLD TO GDG-GLS-DGT1 IN GDG.
       FIN-RET-GDG-TAS.
           EXIT.
       
       KEY-ALL-TAS SECTION.
       INI-KEY-ALL-TAS.
           PERFORM KEY-TAS-TAS.
           PERFORM KEY-GDG-TAS.
       FIN-KEY-ALL-TAS.
           EXIT.
       
       KEY-TAS-TAS SECTION.
       INI-KEY-TAS-TAS.
           MOVE TAS-CAI-GTIA IN TAS-FLD TO TAS-CAI-GTIA IN TAS.
           MOVE TAS-IIC-GTIA IN TAS-FLD TO TAS-IIC-GTIA IN TAS.
       FIN-KEY-TAS-TAS.
           EXIT.
       
       KEY-GDG-TAS SECTION.
       INI-KEY-GDG-TAS.
       FIN-KEY-GDG-TAS.
           EXIT.
       
       SCR-CLRA-TAS SECTION.
       INI-SCR-CLRA-TAS.
           PERFORM CLR-KEY-TAS.
           PERFORM CLR-FLD-TAS.
       FIN-SCR-CLRA-TAS.
           EXIT.
       
       CLR-KEY-TAS SECTION.
       INI-CLR-KEY-TAS.
           MOVE SPACES TO TAS-CAI-GTIA IN TAS-FLD.
           MOVE ZEROES TO TAS-IIC-GTIA IN TAS-FLD.
       FIN-CLR-KEY-TAS.
           EXIT.
       
       CLR-FLD-TAS SECTION.
       INI-CLR-FLD-TAS.
           MOVE SPACES TO FRM-GLS-TREA IN TAS-FLD.
           MOVE SPACES TO FRM-GLS-CGTA IN TAS-FLD.
           MOVE SPACES TO FRM-GLS-OFIC IN TAS-FLD.
           MOVE SPACES TO FRM-GLS-VCAM IN TAS-FLD.
           MOVE SPACES TO FRM-GLS-ESTA IN TAS-FLD.
           PERFORM CLR-TAS-TAS.
           PERFORM CLR-GDG-TAS.
       FIN-CLR-FLD-TAS.
           EXIT.
       
       CLR-TAS-TAS SECTION.
       INI-CLR-TAS-TAS.
           MOVE SPACES TO TAS-IND-ESTA IN TAS-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO TAS-FEC-FTSN IN TAS-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO TAS-VAL-MNTO IN TAS-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO TAS-FEC-FVEX IN TAS-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO TAS-GLS-EXCE IN TAS-FLD.
       FIN-CLR-TAS-TAS.
           EXIT.
       
       CLR-GDG-TAS SECTION.
       INI-CLR-GDG-TAS.
           MOVE SPACES TO GDG-TIP-CGTA IN TAS-FLD.
           MOVE SPACES TO GDG-STP-CGTA IN TAS-FLD.
           MOVE SPACES TO GDG-COD-OFIC IN TAS-FLD.
           MOVE SPACES TO GDG-COD-VCAM IN TAS-FLD.
           MOVE SPACES TO GDG-GLS-DGTA IN TAS-FLD.
           MOVE SPACES TO GDG-GLS-DGT1 IN TAS-FLD.
       FIN-CLR-GDG-TAS.
           EXIT.
       
       FST-KEY-TAS SECTION.
       INI-FST-KEY-TAS.
           MOVE -1 TO TAS-CAI-GTIA-LEN IN TAS-FLD.
       FIN-FST-KEY-TAS.
           EXIT.
       
       FST-FLD-TAS SECTION.
       INI-FST-FLD-TAS.
      *<<<< INI_FST_FLD_TAS
           IF NOT SCR-CMND = 'ING'
              MOVE -1 TO TAS-IND-ESTA-LEN IN TAS-FLD.
           GO TO FIN-FST-FLD-TAS.
      *>>>>
           MOVE -1 TO GDG-STP-CGTA-LEN IN TAS-FLD.
       FIN-FST-FLD-TAS.
           EXIT.
       
       PRO-KEY-TAS SECTION.
       INI-PRO-KEY-TAS.
           PERFORM PRO-MKY-TAS.
       FIN-PRO-KEY-TAS.
           EXIT.
       
       PRO-MKY-TAS SECTION.
       INI-PRO-MKY-TAS.
       FIN-PRO-MKY-TAS.
           EXIT.
       
       PRO-FLD-TAS SECTION.
       INI-PRO-FLD-TAS.
           PERFORM PRO-TAS-TAS.
           PERFORM PRO-GDG-TAS.
       FIN-PRO-FLD-TAS.
           EXIT.
       
       PRO-TAS-TAS SECTION.
       INI-PRO-TAS-TAS.
           MOVE FRM-CPIM-AEY TO TAS-IND-ESTA-ATR IN TAS-FLD.
       FIN-PRO-TAS-TAS.
           EXIT.
       
       PRO-GDG-TAS SECTION.
       INI-PRO-GDG-TAS.
           MOVE FRM-CPIM-AEY TO GDG-STP-CGTA-ATR IN TAS-FLD.
       FIN-PRO-GDG-TAS.
           EXIT.
       
       UNP-KEY-TAS SECTION.
       INI-UNP-KEY-TAS.
           PERFORM UNP-IKY-TAS.
           PERFORM UNP-AKY-TAS.
           PERFORM UNP-MKY-TAS.
       FIN-UNP-KEY-TAS.
           EXIT.
       
       UNP-IKY-TAS SECTION.
       INI-UNP-IKY-TAS.
           MOVE FRM-CPIM-UBY TO TAS-CAI-GTIA-ATR.
           MOVE FRM-CPIM-UBY TO TAS-IIC-GTIA-ATR.
       FIN-UNP-IKY-TAS.
           EXIT.
       
       UNP-MKY-TAS SECTION.
       INI-UNP-MKY-TAS.
       FIN-UNP-MKY-TAS.
           EXIT.
       
       UNP-AKY-TAS SECTION.
       INI-UNP-AKY-TAS.
       FIN-UNP-AKY-TAS.
           EXIT.
       
       UNP-FLD-TAS SECTION.
       INI-UNP-FLD-TAS.
           PERFORM UNP-TAS-TAS.
           PERFORM UNP-GDG-TAS.
       FIN-UNP-FLD-TAS.
           EXIT.
       
       UNP-TAS-TAS SECTION.
       INI-UNP-TAS-TAS.
           MOVE FRM-CPIM-UBY TO TAS-IND-ESTA-ATR IN TAS-FLD.
      *<<<< FIN_UNP_TAS_TAS
           IF SCR-CMND = 'ING'
               MOVE FRM-CPIM-AEY TO TAS-IND-ESTA-ATR IN TAS-FLD.
      *>>>>
       FIN-UNP-TAS-TAS.
           EXIT.
       
       UNP-GDG-TAS SECTION.
       INI-UNP-GDG-TAS.
           MOVE FRM-CPIM-UBY TO GDG-STP-CGTA-ATR IN TAS-FLD.
       FIN-UNP-GDG-TAS.
           EXIT.
       
       VAL-MKY-TAS SECTION.
       INI-VAL-MKY-TAS.
       FIN-VAL-MKY-TAS.
           EXIT.
       
       VAL-FLD-TAS SECTION.
       INI-VAL-FLD-TAS.
           PERFORM VAL-FRM-GLS-TREA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TREA-LEN IN TAS-FLD
                GO TO FIN-VAL-FLD-TAS.
           PERFORM VAL-GDG-TIP-CGTA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO GDG-TIP-CGTA-LEN IN TAS-FLD
                GO TO FIN-VAL-FLD-TAS.
           PERFORM VAL-GDG-STP-CGTA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO GDG-STP-CGTA-LEN IN TAS-FLD
                GO TO FIN-VAL-FLD-TAS.
           PERFORM VAL-FRM-GLS-CGTA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-CGTA-LEN IN TAS-FLD
                GO TO FIN-VAL-FLD-TAS.
           PERFORM VAL-GDG-COD-OFIC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO GDG-COD-OFIC-LEN IN TAS-FLD
                GO TO FIN-VAL-FLD-TAS.
           PERFORM VAL-FRM-GLS-OFIC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-OFIC-LEN IN TAS-FLD
                GO TO FIN-VAL-FLD-TAS.
           PERFORM VAL-GDG-COD-VCAM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO GDG-COD-VCAM-LEN IN TAS-FLD
                GO TO FIN-VAL-FLD-TAS.
           PERFORM VAL-FRM-GLS-VCAM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-VCAM-LEN IN TAS-FLD
                GO TO FIN-VAL-FLD-TAS.
           PERFORM VAL-GDG-GLS-DGTA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO GDG-GLS-DGTA-LEN IN TAS-FLD
                GO TO FIN-VAL-FLD-TAS.
           PERFORM VAL-GDG-GLS-DGT1.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO GDG-GLS-DGT1-LEN IN TAS-FLD
                GO TO FIN-VAL-FLD-TAS.
           PERFORM VAL-TAS-IND-ESTA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAS-IND-ESTA-LEN IN TAS-FLD
                GO TO FIN-VAL-FLD-TAS.
           PERFORM VAL-FRM-GLS-ESTA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-ESTA-LEN IN TAS-FLD
                GO TO FIN-VAL-FLD-TAS.
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-TAS-FEC-FTSN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                TAS-FEC-FTSN-LEN IN TAS-FLD
                GO TO FIN-VAL-FLD-TAS.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-TAS-VAL-MNTO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAS-VAL-MNTO-LEN IN TAS-FLD
                GO TO FIN-VAL-FLD-TAS.
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-TAS-FEC-FVEX.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                TAS-FEC-FVEX-LEN IN TAS-FLD
                GO TO FIN-VAL-FLD-TAS.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-TAS-GLS-EXCE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAS-GLS-EXCE-LEN IN TAS-FLD
                GO TO FIN-VAL-FLD-TAS.
       FIN-VAL-FLD-TAS.
           EXIT.
       
       VAL-FRM-GLS-TREA SECTION.
       INI-VAL-FRM-GLS-TREA.
       FIN-VAL-FRM-GLS-TREA.
           EXIT.
       
       VAL-GDG-TIP-CGTA SECTION.
       INI-VAL-GDG-TIP-CGTA.
       FIN-VAL-GDG-TIP-CGTA.
           EXIT.
       
       VAL-GDG-STP-CGTA SECTION.
       INI-VAL-GDG-STP-CGTA.
           IF GDG-STP-CGTA IN TAS-FLD NOT > SPACES
              MOVE SPACES TO GDG-STP-CGTA IN TAS-FLD.
       FIN-VAL-GDG-STP-CGTA.
           EXIT.
       
       VAL-FRM-GLS-CGTA SECTION.
       INI-VAL-FRM-GLS-CGTA.
       FIN-VAL-FRM-GLS-CGTA.
           EXIT.
       
       VAL-GDG-COD-OFIC SECTION.
       INI-VAL-GDG-COD-OFIC.
       FIN-VAL-GDG-COD-OFIC.
           EXIT.
       
       VAL-FRM-GLS-OFIC SECTION.
       INI-VAL-FRM-GLS-OFIC.
       FIN-VAL-FRM-GLS-OFIC.
           EXIT.
       
       VAL-GDG-COD-VCAM SECTION.
       INI-VAL-GDG-COD-VCAM.
       FIN-VAL-GDG-COD-VCAM.
           EXIT.
       
       VAL-FRM-GLS-VCAM SECTION.
       INI-VAL-FRM-GLS-VCAM.
       FIN-VAL-FRM-GLS-VCAM.
           EXIT.
       
       VAL-GDG-GLS-DGTA SECTION.
       INI-VAL-GDG-GLS-DGTA.
       FIN-VAL-GDG-GLS-DGTA.
           EXIT.
       
       VAL-GDG-GLS-DGT1 SECTION.
       INI-VAL-GDG-GLS-DGT1.
       FIN-VAL-GDG-GLS-DGT1.
           EXIT.
       
       VAL-TAS-IND-ESTA SECTION.
       INI-VAL-TAS-IND-ESTA.
           IF TAS-IND-ESTA IN TAS-FLD NOT > SPACES
              MOVE SPACES TO TAS-IND-ESTA IN TAS-FLD.
      *<<<< TAS_IND_ESTA
           MOVE TAS-CAI-GTIA IN TAS-FLD TO TAS-CAI-GTIA IN TAS.
           MOVE TAS-IIC-GTIA IN TAS-FLD TO TAS-IIC-GTIA IN TAS.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM BGL-FIO-TAS.
           IF NOT FIO-STAT-OKS
              GO TO FIN-VAL-TAS-IND-ESTA.
      
           PERFORM LEE-GSO.
      
           IF SCR-CMND = 'MOD' AND
              CMA-IND-LGSO IN CMA-VARI = 'S'
      
              IF TAS-IND-ESTA IN TAS = '11'
                 IF NOT (TAS-IND-ESTA IN TAS-FLD = '11' OR
                    TAS-IND-ESTA IN TAS-FLD = '09')
                    MOVE 'BGL' TO MSG-COD-SIST
                    MOVE 'MODNOPERMITI' TO MSG-COD-MENS
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    MOVE FRM-SUAR-MAL TO FRM-SUAR
                 ELSE
                    GO TO NXT-VAL-TAS-IND-ESTA
                 END-IF
              ELSE
      
                 IF TAS-IND-ESTA IN TAS = '09'
                    IF NOT (TAS-IND-ESTA IN TAS-FLD = '09' OR
                       TAS-IND-ESTA IN TAS-FLD = '13')
                       MOVE 'BGL' TO MSG-COD-SIST
                       MOVE 'MODNOPERMITI' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE FRM-SUAR-MAL TO FRM-SUAR
                    ELSE
                       GO TO NXT-VAL-TAS-IND-ESTA
                    END-IF
                  ELSE
      
                    IF TAS-IND-ESTA IN TAS = '13'
                       IF NOT (TAS-IND-ESTA IN TAS-FLD = '13' OR
                          TAS-IND-ESTA IN TAS-FLD = '14')
                           MOVE 'BGL' TO MSG-COD-SIST
                           MOVE 'MODNOPERMITI' TO MSG-COD-MENS
                           PERFORM GET-MSG
                           MOVE MSG-GLS-DESC TO FRM-MENS
                           MOVE FRM-SUAR-MAL TO FRM-SUAR
                    ELSE
                       GO TO NXT-VAL-TAS-IND-ESTA
                    END-IF
                  END-IF
              END-IF
           END-IF.
      
           IF SCR-CMND = 'MOD' AND
              CMA-IND-LGSO IN CMA-VARI = 'N'
      
              IF  TAS-IND-ESTA IN TAS = '11'
                  IF NOT (TAS-IND-ESTA IN TAS-FLD = '11' OR
                     TAS-IND-ESTA IN TAS-FLD = '12' OR
      *EVA-AFRI/AGOST2007
                     TAS-IND-ESTA IN TAS-FLD = '09')
      *FIN-AFRI/AGOST2007
                     MOVE 'BGL'            TO MSG-COD-SIST
                     MOVE 'EST INVALIDO'   TO MSG-COD-MENS
                     PERFORM GET-MSG
                     MOVE MSG-GLS-DESC TO FRM-MENS
                     MOVE FRM-SUAR-MAL TO FRM-SUAR
                  ELSE
                     NEXT SENTENCE
              ELSE
              IF TAS-IND-ESTA IN TAS = '12'
                 IF NOT (TAS-IND-ESTA IN TAS-FLD = '12' OR
                      TAS-IND-ESTA IN TAS-FLD = '11')
                      MOVE 'BGL'            TO MSG-COD-SIST
                      MOVE 'EST INVALIDO'   TO MSG-COD-MENS
                      PERFORM GET-MSG
                      MOVE MSG-GLS-DESC TO FRM-MENS
                      MOVE FRM-SUAR-MAL TO FRM-SUAR
      *EVA-AFRI/AGOST2007
              ELSE
              IF TAS-IND-ESTA IN TAS = '09'
                 IF (TAS-IND-ESTA IN TAS-FLD = '11' OR
                     TAS-IND-ESTA IN TAS-FLD = '12')
                      MOVE 'BGL'            TO MSG-COD-SIST
                      MOVE 'EST INVALIDO'   TO MSG-COD-MENS
                      PERFORM GET-MSG
                      MOVE MSG-GLS-DESC TO FRM-MENS
                      MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *FIN-AFRI/AGOST2007
       NXT-VAL-TAS-IND-ESTA.
      *>>>>
      *VAL-COD Valida codigo de tabla
           MOVE 'BGL' TO TAB-COD-SIST.
           MOVE 'EST ' TO TAB-COD-TTAB IN TAB.
           MOVE TAS-IND-ESTA IN TAS-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              TAS-IND-ESTA IN TAS-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-ESTA IN TAS-FLD
           ELSE
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-ESTA IN TAS-FLD.
       FIN-VAL-TAS-IND-ESTA.
           EXIT.
       
       VAL-FRM-GLS-ESTA SECTION.
       INI-VAL-FRM-GLS-ESTA.
       FIN-VAL-FRM-GLS-ESTA.
           EXIT.
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-TAS-FEC-FTSN SECTION.
       INI-VAL-TAS-FEC-FTSN.
           IF TAS-FEC-FTSN IN TAS-FLD  NOT > ZEROES
              MOVE ZEROES TO TAS-FEC-FTSN IN TAS-FLD.
       FIN-VAL-TAS-FEC-FTSN.
           EXIT.
      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-TAS-VAL-MNTO SECTION.
       INI-VAL-TAS-VAL-MNTO.
       FIN-VAL-TAS-VAL-MNTO.
           EXIT.
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-TAS-FEC-FVEX SECTION.
       INI-VAL-TAS-FEC-FVEX.
           IF TAS-FEC-FVEX IN TAS-FLD  NOT > ZEROES
              MOVE ZEROES TO TAS-FEC-FVEX IN TAS-FLD.
       FIN-VAL-TAS-FEC-FVEX.
           EXIT.
      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-TAS-GLS-EXCE SECTION.
       INI-VAL-TAS-GLS-EXCE.
      *<<<< INI_TAS_GLS_EXCE
            IF TAS-IND-ESTA IN TAS-FLD = '11'
               MOVE 'DEBEN SER RETASADAS' TO TAS-GLS-EXCE IN TAS-FLD
             ELSE
               IF TAS-IND-ESTA IN TAS-FLD = '12'
                   MOVE 'NO RETASAR'      TO TAS-GLS-EXCE IN TAS-FLD.
      *>>>>
       FIN-VAL-TAS-GLS-EXCE.
           EXIT.
       
       VAL-CON-FLD-TAS SECTION.
       INI-VAL-CON-FLD-TAS.
       FIN-VAL-CON-FLD-TAS.
           EXIT.
       
       VAL-CON-TAS SECTION.
       INI-VAL-CON-TAS.
       FIN-VAL-CON-TAS.
           EXIT.
       
       VAL-NUM-KEY-TAS SECTION.
       INI-VAL-NUM-KEY-TAS.
           PERFORM VAL-NUM-IKY-TAS.
           PERFORM VAL-NUM-MKY-TAS.
           PERFORM VAL-NUM-AKY-TAS.
       FIN-VAL-NUM-KEY-TAS.
           EXIT.
       
       VAL-NUM-IKY-TAS SECTION.
       INI-VAL-NUM-IKY-TAS.
       VAL-NUM-TAS-IIC-GTIA.
           MOVE TAS-IIC-GTIA-ALF IN TAS-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO TAS-IIC-GTIA IN TAS-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO TAS-IIC-GTIA-LEN IN TAS-FLD
                GO TO FIN-VAL-NUM-IKY-TAS.
       FIN-VAL-NUM-IKY-TAS.
           EXIT.
       
       VAL-NUM-MKY-TAS SECTION.
       INI-VAL-NUM-MKY-TAS.
       FIN-VAL-NUM-MKY-TAS.
           EXIT.
       
       VAL-NUM-AKY-TAS SECTION.
       INI-VAL-NUM-AKY-TAS.
       FIN-VAL-NUM-AKY-TAS.
           EXIT.
       
       VAL-NUM-FLD-TAS SECTION.
       INI-VAL-NUM-FLD-TAS.
       VAL-NUM-TAS-VAL-MNTO.
           MOVE TAS-VAL-MNTO-ALF IN TAS-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 9 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9V TO TAS-VAL-MNTO IN TAS-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO TAS-VAL-MNTO-LEN IN TAS-FLD
                GO TO FIN-VAL-NUM-FLD-TAS.
       FIN-VAL-NUM-FLD-TAS.
           EXIT.
       
       EDT-KEY-TAS SECTION.
       INI-EDT-KEY-TAS.
           PERFORM EDT-IKY-TAS.
           PERFORM EDT-MKY-TAS.
           PERFORM EDT-AKY-TAS.
       FIN-EDT-KEY-TAS.
           EXIT.
       
       EDT-IKY-TAS SECTION.
       INI-EDT-IKY-TAS.
           IF TAS-IIC-GTIA-LEN IN TAS-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO TAS-IIC-GTIA-LEN IN TAS-FLD
                GO TO FIN-EDT-IKY-TAS.
           MOVE TAS-IIC-GTIA IN TAS-FLD TO TAS-IIC-GTIA-EDT IN TAS-EDT.
           MOVE TAS-IIC-GTIA-EDT IN TAS-EDT TO 
                TAS-IIC-GTIA-ALF IN TAS-FLD.
       FIN-EDT-IKY-TAS.
           EXIT.
       
       EDT-MKY-TAS SECTION.
       INI-EDT-MKY-TAS.
       FIN-EDT-MKY-TAS.
           EXIT.
       
       EDT-AKY-TAS SECTION.
       INI-EDT-AKY-TAS.
       FIN-EDT-AKY-TAS.
           EXIT.
       
       EDT-FLD-TAS SECTION.
       INI-EDT-FLD-TAS.
           IF TAS-VAL-MNTO-LEN IN TAS-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO TAS-VAL-MNTO-LEN IN TAS-FLD
                GO TO FIN-EDT-FLD-TAS.
           MOVE TAS-VAL-MNTO IN TAS-FLD TO TAS-VAL-MNTO-EDT IN TAS-EDT.
           MOVE TAS-VAL-MNTO-EDT IN TAS-EDT TO 
                TAS-VAL-MNTO-ALF IN TAS-FLD.
       FIN-EDT-FLD-TAS.
           EXIT.
