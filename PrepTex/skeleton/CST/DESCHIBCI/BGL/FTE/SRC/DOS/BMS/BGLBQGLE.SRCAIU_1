       FIN-VAL-GLE-IIC-GTIA.
           EXIT.
       
       VAL-GLE-COD-PACR SECTION.
       INI-VAL-GLE-COD-PACR.
           IF GLE-COD-PACR IN GLE-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-GLE-COD-PACR.
       FIN-VAL-GLE-COD-PACR.
           EXIT.
       
       VAL-CON-KEY-GLE SECTION.
       INI-VAL-CON-KEY-GLE.
           IF     GLE-CAI-GTIA IN GLE-FLD NOT > SPACES 
              AND GLE-IIC-GTIA IN GLE-FLD NOT > ZEROES 
              AND GLE-COD-PACR IN GLE-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-GLE.
       FIN-VAL-CON-KEY-GLE.
           EXIT.
       
       VAL-CON-IKY-GLE SECTION.
       INI-VAL-CON-IKY-GLE.
           IF     GLE-CAI-GTIA IN GLE-FLD NOT > SPACES 
              AND GLE-IIC-GTIA IN GLE-FLD NOT > ZEROES 
              AND GLE-COD-PACR IN GLE-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-GLE.
       FIN-VAL-CON-IKY-GLE.
           EXIT.
       
       VAL-NUM-KEY-GLE SECTION.
       INI-VAL-NUM-KEY-GLE.
           PERFORM VAL-NUM-IKY-GLE.
           PERFORM VAL-NUM-MKY-GLE.
           PERFORM VAL-NUM-AKY-GLE.
       FIN-VAL-NUM-KEY-GLE.
           EXIT.
       
       VAL-NUM-IKY-GLE SECTION.
       INI-VAL-NUM-IKY-GLE.
       VAL-NUM-GLE-IIC-GTIA.
           MOVE GLE-IIC-GTIA-ALF IN GLE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO GLE-IIC-GTIA IN GLE-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO GLE-IIC-GTIA-LEN IN GLE-FLD
                GO TO FIN-VAL-NUM-IKY-GLE.
       VAL-NUM-GLE-COD-PACR.
           MOVE GLE-COD-PACR-ALF IN GLE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 2 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO GLE-COD-PACR IN GLE-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO GLE-COD-PACR-LEN IN GLE-FLD
                GO TO FIN-VAL-NUM-IKY-GLE.
       FIN-VAL-NUM-IKY-GLE.
           EXIT.
       
       VAL-NUM-MKY-GLE SECTION.
       INI-VAL-NUM-MKY-GLE.
       FIN-VAL-NUM-MKY-GLE.
           EXIT.
       
       VAL-NUM-AKY-GLE SECTION.
       INI-VAL-NUM-AKY-GLE.
       FIN-VAL-NUM-AKY-GLE.
           EXIT.
       
       VAL-NUM-FLD-GLE SECTION.
       INI-VAL-NUM-FLD-GLE.
       VAL-NUM-GLE-NUM-NICR.
           MOVE GLE-NUM-NICR-ALF IN GLE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 5 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO GLE-NUM-NICR IN GLE-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO GLE-NUM-NICR-LEN IN GLE-FLD
                GO TO FIN-VAL-NUM-FLD-GLE.
       VAL-NUM-GLE-NUM-NPHG.
           MOVE GLE-NUM-NPHG-ALF IN GLE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 5 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO GLE-NUM-NPHG IN GLE-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO GLE-NUM-NPHG-LEN IN GLE-FLD
                GO TO FIN-VAL-NUM-FLD-GLE.
       VAL-NUM-GLE-NUM-AICR.
           MOVE GLE-NUM-AICR-ALF IN GLE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 4 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO GLE-NUM-AICR IN GLE-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO GLE-NUM-AICR-LEN IN GLE-FLD
                GO TO FIN-VAL-NUM-FLD-GLE.
       VAL-NUM-GLE-NUM-APHG.
           MOVE GLE-NUM-APHG-ALF IN GLE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 4 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO GLE-NUM-APHG IN GLE-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO GLE-NUM-APHG-LEN IN GLE-FLD
                GO TO FIN-VAL-NUM-FLD-GLE.
       VAL-NUM-GLE-NUM-PROP.
           MOVE GLE-NUM-PROP-ALF IN GLE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 6 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO GLE-NUM-PROP IN GLE-FLD
           ELSE
