      *<<<< GUB_COD_COMU
      *<<<< FMT_CHI
      *BUS-TAB busca glosa de codigo en tabla
           IF GUB-COD-COMU IN GUB-FLD > SPACES
               MOVE 'TAB'  TO TAB-COD-SIST
               MOVE 'COM'  TO TAB-COD-TTAB IN TAB
               MOVE GUB-COD-COMU IN GUB-FLD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB TO COM
               IF MSG-COD-MENS IN MSG NOT > SPACES
                   MOVE COM-GLS-COMU IN COM TO FRM-GLS-COMU IN GUB-FLD 
                   MOVE COM-GLS-CIUD IN COM TO FRM-GLS-CIUD IN GUB-FLD 
      *             MOVE COM-GLS-CIUD IN COM TO FRM-GLS-LOCA IN GUB-FLD 
      *             MOVE COM-GLS-ESTD IN COM TO FRM-GLS-RGON IN GUB-FLD 

                    MOVE 'TAB' TO FIO-SIST
                    MOVE 'TAB' TO TAB-COD-SIST
                    MOVE 'REG' TO TAB-COD-TTAB IN TAB
                    MOVE COM-COD-REGI IN COM TO TAB-COD-CTAB IN TAB
                    PERFORM BUS-TAB
                    MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-REGI IN GUB-FLD

               ELSE
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   MOVE SPACES       TO FRM-GLS-COMU IN GUB-FLD
                   MOVE SPACES       TO FRM-GLS-CIUD IN GUB-FLD
                   MOVE SPACES       TO FRM-GLS-REGI IN GUB-FLD
      *             MOVE SPACES       TO FRM-GLS-LOCA IN GUB-FLD 
      *             MOVE SPACES       TO FRM-GLS-RGON IN GUB-FLD 
                   GO TO FIN-VAL-GUB-COD-COMU.
      *>>>>
      *>>>>
       FIN-VAL-GUB-COD-COMU.
           EXIT.
       
       VAL-FRM-GLS-COMU SECTION.
       INI-VAL-FRM-GLS-COMU.
       FIN-VAL-FRM-GLS-COMU.
           EXIT.
       
       VAL-FRM-GLS-CIUD SECTION.
       INI-VAL-FRM-GLS-CIUD.
       FIN-VAL-FRM-GLS-CIUD.
           EXIT.
       
       VAL-FRM-GLS-REGI SECTION.
       INI-VAL-FRM-GLS-REGI.
       FIN-VAL-FRM-GLS-REGI.
           EXIT.
       
       VAL-GUB-COD-POST SECTION.
       INI-VAL-GUB-COD-POST.
           IF GUB-COD-POST IN GUB-FLD NOT > SPACES
              MOVE SPACES TO GUB-COD-POST IN GUB-FLD.
      *<<<< GUB_COD_POST
      *>>>>
       FIN-VAL-GUB-COD-POST.
           EXIT.
       
       VAL-FRM-GLS-POST SECTION.
       INI-VAL-FRM-GLS-POST.
       FIN-VAL-FRM-GLS-POST.
           EXIT.
       
       VAL-GUB-COD-AADN SECTION.
       INI-VAL-GUB-COD-AADN.
           IF GUB-COD-AADN IN GUB-FLD NOT > SPACES
              MOVE SPACES TO GUB-COD-AADN IN GUB-FLD.
      *<<<< GUB_IIV_AGT_ADN
      *MHM-INI 12-NOV-2003
      *     IF GUB-COD-AADN IN GUB-FLD > SPACES
      *        GO TO CONTINUA-1.
      *MHM-FIN 12-NOV-2003
      
           IF GAR-IND-OCAD IN CMA-VARI NOT = 'D'
               MOVE SPACES TO GUB-COD-AADN IN GUB-FLD
               MOVE SPACES TO FRM-GLS-AADN IN GUB-FLD
               GO TO FIN-VAL-GUB-COD-AADN.
      
      *MHM-INI 12-NOV-2003
      *      IF GUB-COD-AADN IN GUB-FLD NOT > SPACES
      *         PERFORM ERR-GLS
      *         GO TO FIN-VAL-GUB-COD-AADN.
           IF GAR-IND-OCAD IN CMA-VARI = 'D' AND
              GUB-COD-AADN IN GUB-FLD NOT > SPACES
               PERFORM ERR-GLS
               MOVE SPACES TO FRM-GLS-AADN IN GUB-FLD
               GO TO FIN-VAL-GUB-COD-AADN.

      *MHM-FIN 12-NOV-2003
      *>>>>
      *<<<< GUB_COD_AADN
      *VAL-COD Valida codigo en tabla IVB
       CONTINUA-1.
      *<<<< GEN_BCI
      *VAL-COD Valida codigo de tabla
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'AGT ' TO TAB-COD-TTAB IN TAB.
           MOVE GUB-COD-AADN IN GUB-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              GUB-COD-AADN IN GUB-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-AADN IN GUB-FLD
           ELSE
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-AADN IN GUB-FLD.
      *>>>>
      *>>>>
       FIN-VAL-GUB-COD-AADN.
           EXIT.
       
       VAL-FRM-GLS-AADN SECTION.
       INI-VAL-FRM-GLS-AADN.
       FIN-VAL-FRM-GLS-AADN.
           EXIT.
       
       VAL-GUB-COD-IALM SECTION.
       INI-VAL-GUB-COD-IALM.
           IF GUB-COD-IALM IN GUB-FLD NOT > SPACES
              MOVE SPACES TO GUB-COD-IALM IN GUB-FLD.
      *<<<< GUB_IIV_ALM
      *MHM-INI 12-NOV-2003
      *     IF GUB-COD-IALM IN GUB-FLD > SPACES
      *        GO TO CONTINUA-2.
      *MHM-FIN 12-NOV-2003
      
           IF GAR-IND-OCAL IN CMA-VARI NOT = 'A'
               MOVE SPACES TO GUB-COD-IALM IN GUB-FLD
               MOVE SPACES TO FRM-GLS-IALM IN GUB-FLD
               GO TO FIN-VAL-GUB-COD-IALM.
      *>>>>
      *<<<< GUB_COD_IALM
      *MHM-INI 12-NOV-2003
      *     IF GUB-COD-IALM IN GUB-FLD NOT > SPACES
      *          PERFORM ERR-GLS
      *          GO TO FIN-VAL-GUB-COD-IALM.

           IF GAR-IND-OCAL IN CMA-VARI = 'A' AND
              GUB-COD-IALM IN GUB-FLD NOT > SPACES
                PERFORM ERR-GLS
                MOVE SPACES TO FRM-GLS-IALM IN GUB-FLD
               GO TO FIN-VAL-GUB-COD-IALM.

      *MHM-FIN 12-NOV-2003

       CONTINUA-2.
      *<<<< GEN_BCI
      *VAL-COD Valida codigo de tabla
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'ALM ' TO TAB-COD-TTAB IN TAB.
           MOVE GUB-COD-IALM IN GUB-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              GUB-COD-IALM IN GUB-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-IALM IN GUB-FLD
           ELSE
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-IALM IN GUB-FLD.
      *>>>>
      *>>>>
       FIN-VAL-GUB-COD-IALM.
           EXIT.
       
       VAL-FRM-GLS-IALM SECTION.
       INI-VAL-FRM-GLS-IALM.
       FIN-VAL-FRM-GLS-IALM.
           EXIT.
       
       VAL-GUB-COD-UDOC SECTION.
       INI-VAL-GUB-COD-UDOC.
      *<<<< GUB_COD_UBI_DOC
      *MHM-INI 12-NOV-2003
      *     IF GUB-COD-UDOC IN GUB-FLD > SPACES
      *        GO TO CONTINUA-3.
      *MHM-FIN 12-NOV-2003

           IF GAR-IND-OUBI IN CMA-VARI NOT = 'U'
               MOVE SPACES TO GUB-COD-UDOC IN GUB-FLD
               MOVE SPACES TO FRM-GLS-UDOC IN GUB-FLD
               GO TO FIN-VAL-GUB-COD-UDOC.

      *MHM-INI 12-NOV-2003
      *     IF GUB-COD-UDOC IN GUB-FLD NOT > SPACES
      *        PERFORM ERR-GLS
      *        GO TO FIN-VAL-GUB-COD-UDOC.

            IF GAR-IND-OUBI IN CMA-VARI = 'U' AND
               GUB-COD-UDOC IN GUB-FLD NOT > SPACES
                PERFORM ERR-GLS
                MOVE SPACES TO FRM-GLS-UDOC IN GUB-FLD
                GO TO FIN-VAL-GUB-COD-UDOC.
      *MHM-FIN 12-NOV-2003
      
      *VAL-COD Valida codigo de tabla
       CONTINUA-3.
           MOVE 'BGL' TO TAB-COD-SIST.
           MOVE 'UUR ' TO TAB-COD-TTAB IN TAB.
           MOVE GUB-COD-UDOC IN GUB-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              GUB-COD-UDOC IN GUB-FLD NOT > SPACES
               MOVE 'TAB'        TO MSG-COD-SIST
               MOVE 'COD    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE SPACES TO FRM-GLS-UDOC IN GUB-FLD
           ELSE
              MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-UDOC IN GUB-FLD.
      *>>>>
           IF GUB-COD-UDOC IN GUB-FLD NOT > SPACES
              MOVE SPACES TO GUB-COD-UDOC IN GUB-FLD.
       FIN-VAL-GUB-COD-UDOC.
           EXIT.
       
       VAL-FRM-GLS-UDOC SECTION.
       INI-VAL-FRM-GLS-UDOC.
       FIN-VAL-FRM-GLS-UDOC.
           EXIT.
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-GUB-FEC-FDEP SECTION.
       INI-VAL-GUB-FEC-FDEP.
           IF GUB-FEC-FDEP IN GUB-FLD  NOT > ZEROES
              MOVE ZEROES TO GUB-FEC-FDEP IN GUB-FLD.
      *VAL-FEC Validar Fecha
           MOVE GUB-FEC-FDEP IN GUB-FLD TO FEC-FECH.
           MOVE FEC-FORM-FEC TO FEC-FORM.
           MOVE FEC-FBLK-N  TO FEC-FBLK.
           MOVE FEC-CHOY-LE TO FEC-CHOY.
           MOVE FEC-VHBL-S TO FEC-VHBL.
      *<<<< INI_GUB_FEC_FDEP
      *<<<< GEN_BCI
      *MHM-INI 29-OCT-2003
           IF CMA-CIC-UING IN CMA-VARI = 'MIGRACION' AND
              GUB-FEC-FDEP IN GUB-FLD NOT > ZEROES
               GO TO FIN-VAL-GUB-FEC-FDEP.
      *MHM-FIN 29-OCT-2003
           MOVE FEC-FBLK-S  TO FEC-FBLK.

      *MHM-INI 02-DIC-2004                                              
      *    IF GDG-TIP-CGTA IN GUB-FLD = '952' AND
      *       GUB-FEC-FDEP IN GUB-FLD NOT > ZEROES
      *        PERFORM ERR-GLS
      *        GO TO FIN-VAL-GUB-FEC-FDEP.
      *
      *    IF GDG-TIP-CGTA IN GUB-FLD NOT = '952' AND
      *       GUB-FEC-FDEP IN GUB-FLD > ZEROES
      *        MOVE ZEROES TO GUB-FEC-FDEP IN GUB-FLD
      *        GO TO FIN-VAL-GUB-FEC-FDEP.
           MOVE 'TAB' TO TAB-COD-SIST.                                  
           MOVE 'AUX' TO TAB-COD-TTAB IN TAB.                           
           MOVE GDG-TIP-CGTA IN GUB-FLD TO TAG-TIP-AGTA IN TAG.         
           MOVE GDG-STP-CGTA IN GUB-FLD TO TAG-STP-AGTA IN TAG.         
           MOVE TAG-COD-AGTA IN TAG TO TAB-COD-CTAB IN TAB.             
           PERFORM BUS-TAB.                                             
           IF (TAB-CIC-CTAB IN TAB = 'DPBCO       ' OR '954090      ')  
               AND GUB-FEC-FDEP IN GUB-FLD NOT > ZEROES                 
               PERFORM ERR-GLS                                          
               GO TO FIN-VAL-GUB-FEC-FDEP.                              
                                                                        
      *MHM-FIN 02-DIC-2004                                              
      *>>>>
      *>>>>
           MOVE FEC-VAL-FEC TO FEC-CMND.
           PERFORM PRO-FEC.
           IF FEC-STAT-OKS
               MOVE FEC-FECH TO GUB-FEC-FDEP IN GUB-FLD
           ELSE
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE FEC-MENS     TO FRM-MENS
               GO TO FIN-VAL-GUB-FEC-FDEP.
       FIN-VAL-GUB-FEC-FDEP.
           EXIT.
      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-GUB-COD-LOCU SECTION.
       INI-VAL-GUB-COD-LOCU.
           IF GUB-COD-LOCU IN GUB-FLD NOT > SPACES
              MOVE SPACES TO GUB-COD-LOCU IN GUB-FLD.
      *<<<< FIN_GUB_COD_LOCU
      *VAL-COD Valida codigo de tabla
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'LOC' TO TAB-COD-TTAB IN TAB.
           MOVE GUB-COD-LOCU IN GUB-FLD TO WSS-LOC-REG.
           MOVE WSS-COD-LOC TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              GUB-COD-LOCU IN GUB-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-LOCU IN GUB-FLD
              GO TO FIN-VAL-GUB-COD-LOCU.
              IF TAB-COD-DAT2 IN TAB(7) NOT = WSS-COD-REG
                 MOVE 'BGL'       TO MSG-COD-SIST
                 MOVE 'REGIONNEX' TO MSG-COD-MENS
                 PERFORM GET-MSG
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 MOVE FRM-SUAR-MAL TO FRM-SUAR
                 GO TO FIN-VAL-GUB-COD-LOCU.
           MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-LOCU IN GUB-FLD.
      *>>>>
       FIN-VAL-GUB-COD-LOCU.
           EXIT.
       
       VAL-FRM-GLS-LOCU SECTION.
       INI-VAL-FRM-GLS-LOCU.
       FIN-VAL-FRM-GLS-LOCU.
           EXIT.
       
       VAL-CON-FLD-GUB SECTION.
       INI-VAL-CON-FLD-GUB.
       FIN-VAL-CON-FLD-GUB.
           EXIT.
       
       VAL-CON-GUB SECTION.
       INI-VAL-CON-GUB.
       FIN-VAL-CON-GUB.
           EXIT.
       
       VAL-NUM-KEY-GUB SECTION.
       INI-VAL-NUM-KEY-GUB.
           PERFORM VAL-NUM-IKY-GUB.
           PERFORM VAL-NUM-MKY-GUB.
           PERFORM VAL-NUM-AKY-GUB.
       FIN-VAL-NUM-KEY-GUB.
           EXIT.
       
       VAL-NUM-IKY-GUB SECTION.
       INI-VAL-NUM-IKY-GUB.
       VAL-NUM-GUB-IIC-GTIA.
           MOVE GUB-IIC-GTIA-ALF IN GUB-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO GUB-IIC-GTIA IN GUB-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO GUB-IIC-GTIA-LEN IN GUB-FLD
                GO TO FIN-VAL-NUM-IKY-GUB.
       FIN-VAL-NUM-IKY-GUB.
           EXIT.
       
       VAL-NUM-MKY-GUB SECTION.
       INI-VAL-NUM-MKY-GUB.
       FIN-VAL-NUM-MKY-GUB.
           EXIT.
       
       VAL-NUM-AKY-GUB SECTION.
       INI-VAL-NUM-AKY-GUB.
       FIN-VAL-NUM-AKY-GUB.
           EXIT.
       
       VAL-NUM-FLD-GUB SECTION.
       INI-VAL-NUM-FLD-GUB.
       FIN-VAL-NUM-FLD-GUB.
           EXIT.
       
       EDT-KEY-GUB SECTION.
       INI-EDT-KEY-GUB.
           PERFORM EDT-IKY-GUB.
           PERFORM EDT-MKY-GUB.
           PERFORM EDT-AKY-GUB.
       FIN-EDT-KEY-GUB.
           EXIT.
       
       EDT-IKY-GUB SECTION.
       INI-EDT-IKY-GUB.
           IF GUB-IIC-GTIA-LEN IN GUB-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO GUB-IIC-GTIA-LEN IN GUB-FLD
                GO TO FIN-EDT-IKY-GUB.
           MOVE GUB-IIC-GTIA IN GUB-FLD TO GUB-IIC-GTIA-EDT IN GUB-EDT.
           MOVE GUB-IIC-GTIA-EDT IN GUB-EDT TO 
                GUB-IIC-GTIA-ALF IN GUB-FLD.
       FIN-EDT-IKY-GUB.
           EXIT.
       
       EDT-MKY-GUB SECTION.
       INI-EDT-MKY-GUB.
       FIN-EDT-MKY-GUB.
           EXIT.
       
       EDT-AKY-GUB SECTION.
       INI-EDT-AKY-GUB.
       FIN-EDT-AKY-GUB.
           EXIT.
       
       EDT-FLD-GUB SECTION.
       INI-EDT-FLD-GUB.
       FIN-EDT-FLD-GUB.
           EXIT.
