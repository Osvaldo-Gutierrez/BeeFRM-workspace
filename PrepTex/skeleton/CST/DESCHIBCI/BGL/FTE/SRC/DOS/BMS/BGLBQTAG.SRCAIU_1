           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 2 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9V TO TAG-SGV-PPON IN TAG-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO TAG-SGV-PPON-LEN IN TAG-FLD
                GO TO FIN-VAL-NUM-FLD-TAG.
       VAL-NUM-TAG-NUM-PVTS.
           MOVE TAG-NUM-PVTS-ALF IN TAG-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO TAG-NUM-PVTS IN TAG-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO TAG-NUM-PVTS-LEN IN TAG-FLD
                GO TO FIN-VAL-NUM-FLD-TAG.
       VAL-NUM-TAG-NUM-PMAX.
           MOVE TAG-NUM-PMAX-ALF IN TAG-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 2 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO TAG-NUM-PMAX IN TAG-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO TAG-NUM-PMAX-LEN IN TAG-FLD
                GO TO FIN-VAL-NUM-FLD-TAG.
       FIN-VAL-NUM-FLD-TAG.
           EXIT.
       
       EDT-KEY-TAG SECTION.
       INI-EDT-KEY-TAG.
           PERFORM EDT-IKY-TAG.
           PERFORM EDT-MKY-TAG.
           PERFORM EDT-AKY-TAG.
       FIN-EDT-KEY-TAG.
           EXIT.
       
       EDT-IKY-TAG SECTION.
       INI-EDT-IKY-TAG.
       FIN-EDT-IKY-TAG.
           EXIT.
       
       EDT-MKY-TAG SECTION.
       INI-EDT-MKY-TAG.
       FIN-EDT-MKY-TAG.
           EXIT.
       
       EDT-AKY-TAG SECTION.
       INI-EDT-AKY-TAG.
       FIN-EDT-AKY-TAG.
           EXIT.
       
       EDT-FLD-TAG SECTION.
       INI-EDT-FLD-TAG.
           IF TAG-SGV-PAJU-LEN IN TAG-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO TAG-SGV-PAJU-LEN IN TAG-FLD
                GO TO FIN-EDT-FLD-TAG.
           MOVE TAG-SGV-PAJU IN TAG-FLD TO TAG-SGV-PAJU-EDT IN TAG-EDT.
           MOVE TAG-SGV-PAJU-EDT IN TAG-EDT TO 
                TAG-SGV-PAJU-ALF IN TAG-FLD.
           IF TAG-SGV-PPON-LEN IN TAG-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO TAG-SGV-PPON-LEN IN TAG-FLD
                GO TO FIN-EDT-FLD-TAG.
           MOVE TAG-SGV-PPON IN TAG-FLD TO TAG-SGV-PPON-EDT IN TAG-EDT.
           MOVE TAG-SGV-PPON-EDT IN TAG-EDT TO 
                TAG-SGV-PPON-ALF IN TAG-FLD.
           IF TAG-NUM-PVTS-LEN IN TAG-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO TAG-NUM-PVTS-LEN IN TAG-FLD
                GO TO FIN-EDT-FLD-TAG.
           MOVE TAG-NUM-PVTS IN TAG-FLD TO TAG-NUM-PVTS-EDT IN TAG-EDT.
           MOVE TAG-NUM-PVTS-EDT IN TAG-EDT TO 
                TAG-NUM-PVTS-ALF IN TAG-FLD.
           IF TAG-NUM-PMAX-LEN IN TAG-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO TAG-NUM-PMAX-LEN IN TAG-FLD
                GO TO FIN-EDT-FLD-TAG.
           MOVE TAG-NUM-PMAX IN TAG-FLD TO TAG-NUM-PMAX-EDT IN TAG-EDT.
           MOVE TAG-NUM-PMAX-EDT IN TAG-EDT TO 
                TAG-NUM-PMAX-ALF IN TAG-FLD.
       FIN-EDT-FLD-TAG.
           EXIT.
