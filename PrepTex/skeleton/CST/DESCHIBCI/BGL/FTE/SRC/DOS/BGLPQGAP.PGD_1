      *<<<< EOF                                                         
       RST-STAT SECTION.                                                
       INI-RST-STAT.                                                    
           MOVE SCR-STPP     TO SCR-STAT.                               
           MOVE SCR-MFLD-NOT TO SCR-MFLD.                               
       FIN-RST-STAT.                                                    
           EXIT.                                                        
                                                                        
       GNS-CLR-RQA SECTION.                                             
       INI-GNS-CLR-RQA.                                                 
       FIN-GNS-CLR-RQA.                                                 
           EXIT.                                                        
      *>>>>                                                             
      *<<<< EOF AND PGM_DTC                                             
       COPY GNSBGVSM.                                                   
       COPY GNSBHVSM.                                                   
       COPY GNSBGHOY.                                                   
      *>>>>                                                             
      *<<<< EOF                                                         
       COPY BGLBFGDD.                                                   
       COPY BGLBFGMO.                                                   
       COPY GNSBGPES.                                                   
                                                                        
       BUS-IND-TREA SECTION.                                            
       INI-BUS-IND-TREA.                                                
           MOVE SPACES TO MSG-COD-MENS.                                 
           MOVE 'BGL'  TO TAB-COD-SIST.                                 
           MOVE 'RAL'  TO TAB-COD-TTAB IN TAB.                          
           MOVE TAG-IND-TREA IN TAG TO TAB-COD-CTAB IN TAB.             
           PERFORM BUS-TAB.                                             
           IF MSG-COD-MENS = SPACES                                     
              MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-TREA IN GAP-FLD       
           ELSE                                                         
              MOVE SPACES TO FRM-GLS-TREA IN GAP-FLD.                   
       FIN-BUS-IND-TREA.                                                
           EXIT.                                                        
                                                                        
       COPY GNSBVCOD.                                                   
       COPY GNSBVTAB.                                                   
