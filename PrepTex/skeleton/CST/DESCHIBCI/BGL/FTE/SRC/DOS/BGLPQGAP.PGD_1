       COPY BGLBFGES.                                                   
       COPY BGLBFGSO.                                                   
       COPY BGLBFGLE.                                                   
      *<<<< NOT PGM_PU AND NOT PGM_CLB AND NOT PGM_MEX                  
       COPY BGLBGVCG.                                                   
       COPY BGLBKVCG.                                                   
