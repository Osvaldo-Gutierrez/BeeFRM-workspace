*% PGM_PTC = TRUE
*% MI_LARGO_COMMAREA = 20
*% IF MI_COMMAREA
      *<<<< MI_COMMAREA                                                 
           03 CMA-VARI.                                                 
              05 SCR-IMAX      COMP                   PIC S9(04).       
              05 SCR-I         COMP                   PIC S9(04).       
              05 WSS-SGV-LTSN                      PIC S9(11)V9(04).    
              05 WSS-SGV-VCTS                      PIC S9(11)V9(04).    
              05 WSS-FEC-FTSN.                                          
                 07 WSS-NUM-STSN                        PIC 9(02).      
                 07 WSS-NUM-ATSN                        PIC 9(02).      
                 07 WSS-NUM-MTSN                        PIC 9(02).      
                 07 WSS-NUM-DTSN                        PIC 9(02).      
              05 WSS-FEC-FFIN.                                          
                 07 WSS-NUM-SFIN                        PIC 9(02).      
                 07 WSS-NUM-AFIN                        PIC 9(02).      
                 07 WSS-NUM-MFIN                        PIC 9(02).      
                 07 WSS-NUM-DFIN                        PIC 9(02).      
              05 WSS-FEC-FTIN.                                          
                 07 WSS-NUM-STIN                        PIC 9(02).      
                 07 WSS-NUM-ATIN                        PIC 9(02).      
                 07 WSS-NUM-MTIN                        PIC 9(02).      
                 07 WSS-NUM-DTIN                        PIC 9(02).      
              05 WSS-FEC-FTFI.                                          
                 07 WSS-NUM-STFI                        PIC 9(02).      
                 07 WSS-NUM-ATFI                        PIC 9(02).      
                 07 WSS-NUM-MTFI                        PIC 9(02).      
                 07 WSS-NUM-DTFI                        PIC 9(02).      
      *>>>>                                                             
      *MAE                                                              
*% END
*% IF WSS
      *<<<<
      *MAE                                                              
       COPY GNSWGCPT.                                                   
       COPY BGLWGTXF.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.        
       COPY SGCBRDBC.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GDD-REQA==.        
       COPY BGLBRGDD.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GDG-REQA==.        
       COPY BGLBRGDG.                                                   
      *MAE                                                              
                                                                        
      *EVA                                                              
       01  WSS-VARI.                                                    
           03 WSS-CON-DTS               VALUE ZEROES    PIC 9(03).      
           03 WSS-SIN-DTS               VALUE ZEROES    PIC 9(03).      
           03 WSS-EXI-TXF               VALUE ZEROES    PIC 9(04).      
           03 WSS-HMAS                  VALUE ZEROES    PIC 9(04).      
      *EVA                                                              
      *<<<<
*% END
*% IF SYS_SCOD
           MOVE +270 TO SCR-TCMA.
*% END
*% IF INI_FIO
      *<<<< INI_FIO                                                     
      *EVA-INI                                                          
           IF SCR-OFRM-SRV                                              
              MOVE TSK-TERM-ALF TO TXF-COD-TERM                         
              MOVE TSK-TERM-ALF TO TXF-COD-TERM-MAS                     
           ELSE                                                         
              MOVE EIBTRMID     TO TXF-COD-TERM                         
              MOVE EIBTRMID     TO TXF-COD-TERM-MAS.                    
      *EVA-FIN                                                          
      *>>>>                                                             
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC                                                 
           IF FRM-FFLD = FRM-FFLD-PF6                                   
                    OR   FRM-FFLD-PF5                                   
               PERFORM VAL-NUM-KEY-TXF                                  
               PERFORM VAL-NUM-FLD-TXF                                  
               PERFORM RST-STAT                                         
               PERFORM FND-SCR-KEY THRU FIN-SCR-KEY                     
               IF SCR-STAT NOT = SCR-STAT-ABT                           
                   PERFORM SCR-TRL                                      
               ELSE                                                     
                   PERFORM DESSCRTRL                                    
                   GO TO FIN-SCR-ACC.                                   
      *>>>>                                                             
*% END
*% IF PFS_GET_CON 
      *<<<< PFS_GET_CON OR PFS_GET_ACC                                  
           IF NOT (FRM-FFLD = FRM-FFLD-PF6                              
                         OR   FRM-FFLD-PF5)                             
      *>>>>                                                             
*% END
*% IF INI_FND_KEY
      *<<<< MAE
      *<<<< INI_FND_KEY                                                 
           MOVE SPACES TO FRM-MEN1.                                     
           IF FRM-FFLD = FRM-FFLD-PF5                                   
               MOVE PGM-SNUL TO PGM-STAT-GVT                            
               MOVE PGM-SNUL TO PGM-STAT-GVT                            
               SUBTRACT 1 FROM SCR-I GIVING TXF-I                       
               IF TXF-I NOT > ZEROES                                    
                   MOVE 'GVT GTIAPRV' TO MSG-COD-MENS                   
                   GO TO ERR-FND-KEY                                    
               ELSE                                                     
                   GO TO CARGA-TXF                                      
           ELSE                                                         
           IF FRM-FFLD = FRM-FFLD-PF6                                   
               MOVE PGM-SNUL TO PGM-STAT-GVT                            
               MOVE PGM-SNUL TO PGM-STAT-GVT                            
               ADD 1 SCR-I GIVING TXF-I                                 
               IF TXF-I > SCR-IMAX                                      
                   MOVE 'GVT GTIANXT' TO MSG-COD-MENS                   
                   GO TO ERR-FND-KEY                                    
               ELSE                                                     
                   GO TO CARGA-TXF.                                     
           GO TO CON-FND-KEY.                                           
      *>>>>                                                             
      *>>>> MAE
*% END
*% IF FIN_FND_KEY
      *<<<<MAE                                                              
      *<<<< FIN_FND_KEY                                                 
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-FND-KEY.                                       
       CON-FND-KEY.                                                     
           MOVE ZEROES TO SCR-IMAX.                                     
           MOVE 1 TO SCR-I.                                             
           MOVE '00' TO FIO-STAT.                                       
           PERFORM OBT-DAT-TS.                                          
           IF (WSS-SIN-DTS > ZEROES AND WSS-CON-DTS = ZEROES)           
              MOVE 'REGEOF     '   TO MSG-COD-MENS                      
              GO TO ERR-FND-KEY                                         
           ELSE                                                         
              MOVE 1 TO SCR-I                                           
              MOVE SCR-I TO TXF-I.                                      
       CARGA-TXF.                                                       
           MOVE SPACES TO MSG-COD-MENS.                                 
           IF NOT SCR-OFRM-SRV                                          
           PERFORM PUT-TAB-TXF.                                         
           MOVE '00' TO FIO-STAT.                                       
           GO TO FIN-FND-KEY.                                           
      *                                                                 
       ERR-FND-KEY.                                                     
           MOVE SCR-STAT-CON TO SCR-STAT.                               
                                                                        
           MOVE 'BGL'  TO MSG-COD-SIST.                                 
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MEN1.                               
           MOVE '00' TO FIO-STAT.                                       
           GO TO FIN-FND-KEY.                                           
      *>>>>                                                             
      *>>>>MAE                                                              
*% END
*% IF EOF
      *<<<<
      *MAE-INI                                                          
       CAR-MOV-TXF SECTION.                                             
       INI-CAR-MOV-TXF.                                                 
      *EVA                                                              
           MOVE ZEROES TO WSS-CON-DTS.                                  
           MOVE ZEROES TO WSS-SIN-DTS.                                  
      *EVA                                                              
           MOVE GVT-NUM-DTSN IN TXF-FLD TO GVT-NUM-DTSN IN GVT. 
           MOVE GVT-NUM-MTSN IN TXF-FLD TO GVT-NUM-MTSN IN GVT.         
           MOVE GVT-NUM-STSN IN TXF-FLD TO GVT-NUM-STSN IN GVT.         
           MOVE GVT-NUM-ATSN IN TXF-FLD TO GVT-NUM-ATSN IN GVT.         
           MOVE 'GVT-FEC-FTSN'          TO FIO-AKEY.                    
           MOVE FIO-GET-NLS             TO FIO-CMND.                    
                                                                        
       LUP-CAR-MOV-TXF.                                                 
           PERFORM BGL-FIO-GVT.                                         
                                                                        
           IF NOT FIO-STAT-OKS                                          
      *EVA                                                              
                ADD 1 TO WSS-SIN-DTS                                    
      *EVA                                                              
                GO TO FIN-CAR-MOV-TXF.
                                                                        
           MOVE GVT-NUM-DTSN IN TXF-FLD TO WSS-NUM-DTSN IN CMA-VARI.    
           MOVE GVT-NUM-MTSN IN TXF-FLD TO WSS-NUM-MTSN IN CMA-VARI.    
           MOVE GVT-NUM-STSN IN TXF-FLD TO WSS-NUM-STSN IN CMA-VARI.    
           MOVE GVT-NUM-ATSN IN TXF-FLD TO WSS-NUM-ATSN IN CMA-VARI.    
           MOVE FRM-NUM-DFIN IN TXF-FLD TO WSS-NUM-DFIN IN CMA-VARI.    
           MOVE FRM-NUM-MFIN IN TXF-FLD TO WSS-NUM-MFIN IN CMA-VARI.    
           MOVE FRM-NUM-SFIN IN TXF-FLD TO WSS-NUM-SFIN IN CMA-VARI.    
           MOVE FRM-NUM-AFIN IN TXF-FLD TO WSS-NUM-AFIN IN CMA-VARI. 
   
           IF (GVT-FEC-FTSN IN GVT < WSS-FEC-FTSN IN CMA-VARI)          
                GO TO SIG-CAR-MOV-TXF.                                  
                                                                        
           IF (GVT-FEC-FTSN IN GVT > WSS-FEC-FFIN IN CMA-VARI)          
      *EVA                                                              
                ADD 1 TO WSS-SIN-DTS                                    
      *EVA                                                              
                GO TO FIN-CAR-MOV-TXF.                                  
                                                                        
           MOVE GVT-SGV-LTSN IN GVT TO WSS-SGV-LTSN IN CMA-VARI.        
           MOVE GVT-SGV-VCTS IN GVT TO WSS-SGV-VCTS IN CMA-VARI.        
           IF  (WSS-SGV-LTSN IN CMA-VARI NOT = 1 OR                     
                WSS-SGV-VCTS IN CMA-VARI NOT = 1)                       
                GO TO SIG-CAR-MOV-TXF.                                  
                                                                        
      *EXISTE GDG                                                       
           MOVE GVT-CIC-GTIA IN GVT  TO GDG-CIC-GTIA IN GDG.            
           MOVE FIO-GET-KEY          TO FIO-CMND.                       
           PERFORM BGL-FIO-GDG.                                         
           IF (NOT FIO-STAT-OKS )                                       
                GO TO SIG-CAR-MOV-TXF.                                  
                                                                        
      * EVA-INI                                                         
           IF GDG-IND-EST1 IN GDG > '19' OR                             
              GDG-IND-EST1 IN GDG < '10'                                
                GO TO SIG-CAR-MOV-TXF.                                  
      * EVA-FIN                                                         
      *MUEVE-DATOS                                                      
           MOVE GVT-CIC-GTIA IN GVT TO TXF-CIC-GTIA(TXF-I).             
           MOVE GVT-NUM-DTSN IN GVT TO TXF-NUM-DTSN(TXF-I).             
           MOVE GVT-NUM-MTSN IN GVT TO TXF-NUM-MTSN(TXF-I).             
           MOVE GVT-NUM-STSN IN GVT TO TXF-NUM-STSN(TXF-I).             
           MOVE GVT-NUM-ATSN IN GVT TO TXF-NUM-ATSN(TXF-I).             
           MOVE GDG-GLS-DGTA IN GDG TO TXF-GLS-DGTA(TXF-I).            
    
       VAL-CAR-MOV-TXF.           
                                                                            
      *LEE-GDD                                                          
           MOVE GVT-CIC-GTIA IN GVT  TO GDD-CIC-GTIA IN GDD.            
           MOVE 'GDD-KEY-AGDD'       TO FIO-AKEY.                       
           MOVE FIO-GET-KEY          TO FIO-CMND.                       
           PERFORM BGL-FIO-GDD.                                         
           IF (NOT FIO-STAT-OKS )                                       
                MOVE 'BGL'            TO MSG-COD-SIST                   
                MOVE 'GDDFECINEX'     TO MSG-COD-MENS                   
                PERFORM GET-MSG                                         
                MOVE MSG-GLS-DESC     TO FRM-MENS                       
                MOVE FRM-SUAR-MAL     TO FRM-SUAR                       
                MOVE ZEROES           TO TXF-NUM-ICLI(TXF-I)            
                MOVE SPACES           TO TXF-VRF-ICLI(TXF-I)            
                MOVE SPACES           TO TXF-IND-ICLI(TXF-I)            
                MOVE SPACES           TO TXF-GLS-ICLI(TXF-I)            
                MOVE 'CLIENTE INEX'   TO TXF-GLS-NOMC(TXF-I)            
                GO TO CLIENTE-INEXITENTE.                               
                                                                        
      *LEE-DBC                                                          
           MOVE GDD-CIC-DCLI IN GDD  TO  DBC-KEY-IDBC IN DBC.           
           MOVE FIO-GET-KEY          TO  FIO-CMND.                      
           PERFORM SGC-FIO-DBC.                                         
           IF (NOT FIO-STAT-OKS )                                       
                MOVE 'BGL'            TO MSG-COD-SIST                   
                MOVE 'DBCFECINEX'     TO MSG-COD-MENS                   
                PERFORM GET-MSG                                         
                MOVE MSG-GLS-DESC     TO FRM-MENS                       
                MOVE FRM-SUAR-MAL     TO FRM-SUAR                       
                MOVE ZEROES           TO TXF-NUM-ICLI(TXF-I)            
                MOVE SPACES           TO TXF-VRF-ICLI(TXF-I)            
                MOVE SPACES           TO TXF-IND-ICLI(TXF-I)            
                MOVE SPACES           TO TXF-GLS-ICLI(TXF-I)            
                MOVE 'CLIENTE INEX'   TO TXF-GLS-NOMC(TXF-I)            
                GO TO CLIENTE-INEXITENTE.                               
                                                                        
                                                                        
           MOVE DBC-NUM-ICLI IN DBC TO TXF-NUM-ICLI(TXF-I).             
           MOVE DBC-VRF-ICLI IN DBC TO TXF-VRF-ICLI(TXF-I).             
           MOVE DBC-IND-ICLI IN DBC TO TXF-IND-ICLI(TXF-I).             
           MOVE DBC-GLS-ICLI IN DBC TO TXF-GLS-ICLI(TXF-I).             
                                                                        
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.                        
           PERFORM CPT-BLKS.                                            
           MOVE CPT-STRN            TO TXF-GLS-NOMC(TXF-I).             
                                                                        
       CLIENTE-INEXITENTE.                                              
                                                                        
      *VALIDA TABLA (MONEDA)                                            
           MOVE 'TAB'                    TO TAB-COD-SIST FIO-SIST.      
           MOVE 'VLR'                    TO TAB-COD-TTAB IN TAB.        
           MOVE GDG-COD-VCAM IN GDG      TO TAB-COD-CTAB IN TAB.        
                                                                        
           MOVE FIO-GET-KEY  TO FIO-CMND.                               
           PERFORM GNS-FIO-TAB.                                         
           IF NOT (FIO-STAT-OKS AND TAB-IND-VIGE IN TAB = 'S')          
                MOVE SPACES              TO TXF-COD-VCAM(TXF-I)         
                MOVE SPACES              TO TXF-GLS-VCAM(TXF-I)         
           ELSE                                                         
                MOVE GDG-COD-VCAM IN GDG TO TXF-COD-VCAM(TXF-I)         
                MOVE TAB-GLS-DESC IN TAB TO TXF-GLS-VCAM(TXF-I).        
                                                                        
      *VALIDA TABLA (TIPO-SUBTIPO)                                      
           MOVE 'TAB'                    TO TAB-COD-SIST.               
           MOVE 'TAB'                    TO FIO-SIST.                   
           MOVE 'AUX'                    TO TAB-COD-TTAB IN TAB.        
           MOVE GDG-COD-CGTA IN GDG      TO TAB-COD-CTAB IN TAB.        
           MOVE FIO-GET-KEY              TO FIO-CMND.                   
           PERFORM GNS-FIO-TAB.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE SPACES               TO TXF-COD-CGTA(TXF-I)         
               MOVE SPACES               TO TXF-GLS-CGTA(TXF-I)         
           ELSE                                                         
               MOVE GDG-COD-CGTA IN GDG  TO TXF-COD-CGTA(TXF-I)         
               MOVE TAB-GLS-DESC IN TAB  TO TXF-GLS-CGTA(TXF-I).        
                                                                        
      *VALIDA TABLA (OFICINA)                                           
           MOVE 'TAB'                    TO TAB-COD-SIST.               
           MOVE 'TAB'                    TO FIO-SIST.                   
           MOVE 'OFI'                    TO TAB-COD-TTAB IN TAB.        
           MOVE GDG-COD-OFIC IN GDG      TO TAB-COD-CTAB IN TAB.        
           MOVE FIO-GET-KEY              TO FIO-CMND.                   
           PERFORM GNS-FIO-TAB.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE SPACES              TO TXF-COD-OFIC(TXF-I)          
               MOVE SPACES              TO TXF-GLS-OFIC(TXF-I)          
           ELSE                                                         
               MOVE GDG-COD-OFIC IN GDG TO TXF-COD-OFIC(TXF-I)          
               MOVE TAB-GLS-DESC IN TAB TO TXF-GLS-OFIC(TXF-I).         
                                                                        
                                                                        
       NXT-CAR-MOV-TXF.                                                 
      *EVA                                                              
           ADD 1 TO WSS-CON-DTS.                                        
      *EVA                                                              
           PERFORM INC-REG-TS.                                          
           ADD 1 TO WSS-HMAS.                                           
           ADD 1 TO WSS-EXI-TXF.                                        
       SIG-CAR-MOV-TXF.                                                 
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           MOVE 'GVT-FEC-FTSN'          TO FIO-AKEY.                    
      *    MOVE SPACES TO TXF-VEC-TS(TXF-I)                             
           GO TO LUP-CAR-MOV-TXF.                                       
       FIN-CAR-MOV-TXF.                                                 
           IF SCR-OFRM-SRV                                              
              PERFORM PUT-TXF-MAS.                                      
           EXIT.                                                        
                                                                        
       PUT-TXF-MAS SECTION.                                             
       INI-PUT-TXF-MAS.                                                 
           MOVE 1                  TO QUE-NITM.                         
           MOVE TXF-LEN-TSTXF-MAS  TO QUE-LITM.                         
           MOVE TXF-NOM-TSTXF-MAS  TO QUE-COLA.                         
           IF WSS-HMAS > TXF-IMAX-MTX                                   
              MOVE 'S'             TO TXF-MAS-IND-HMAS                  
           ELSE                                                         
              MOVE 'N'             TO TXF-MAS-IND-HMAS.                 
           MOVE WSS-EXI-TXF        TO TXF-MAS-NUM-TOCC.      
           MOVE TXF-MAS-ITEM       TO QUE-ITEM.                         
           MOVE QUE-PUT            TO QUE-CMND.                         
           PERFORM GNS-PRO-QUE.                                         
       FIN-PUT-TXF-MAS.                                                 
           EXIT.                                                        
                                                                        
       OBT-DAT-TS SECTION.                                              
       INI-OBT-DAT-TS.                                                  
           EXEC CICS HANDLE CONDITION QIDERR(CON-OBT-DAT-TS)            
                                     ITEMERR(CON-OBT-DAT-TS)            
           END-EXEC.                                                    
                                                                        
           MOVE TXF-NOM-TSTXF     TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
      *>>>>                                                             
       CON-OBT-DAT-TS.                                                  
           MOVE TXF-SI-IMP TO TXF-IMP.                                  
           PERFORM CLR-VEC-TS VARYING TXF-I FROM 1 BY 1 UNTIL           
                   TXF-I > TXF-IMAX.                                    
           MOVE 1 TO TXF-I.                                             
           PERFORM CAR-MOV-TXF.                                         
                                                                        
       NXT-OBT-DAT-TS.                                          
                 IF TXF-IMP = TXF-SI-IMP                                      
               PERFORM WRT-TS.                                          
       FIN-OBT-DAT-TS.                                                        
           EXIT.                                                        
                                                                        
       PUT-TAB-TXF SECTION.                                             
       INI-PUT-TAB-TXF.                                                 
           PERFORM RED-TS.                                              
           PERFORM LLE-DAT-PNT VARYING TXF-I FROM 1 BY 1                
                   UNTIL TXF-I > TXF-IMAX.                              
       FIN-PUT-TAB-TXF.                                                 
           EXIT.                                                        
                                                                        
       LLE-DAT-PNT SECTION.                                             
       INI-LLE-DAT-PNT.                                                 

           MOVE TXF-CIC-GTIA(TXF-I) TO FRM-CIC-GTIA IN TXF-FLD(TXF-I).  
           MOVE TXF-NUM-ICLI(TXF-I) TO FRM-NUM-ICLI IN TXF-FLD(TXF-I).  
           MOVE TXF-VRF-ICLI(TXF-I) TO FRM-VRF-ICLI IN TXF-FLD(TXF-I).  
           MOVE TXF-IND-ICLI(TXF-I) TO FRM-IND-ICLI IN TXF-FLD(TXF-I).  
           MOVE TXF-GLS-ICLI(TXF-I) TO FRM-GLS-ICLI IN TXF-FLD(TXF-I).  
           MOVE TXF-GLS-NOMC(TXF-I) TO FRM-GLS-NOMC IN TXF-FLD(TXF-I).  
           MOVE TXF-COD-CGTA(TXF-I) TO FRM-COD-CGTA IN TXF-FLD(TXF-I).  
           MOVE TXF-GLS-CGTA(TXF-I) TO FRM-GLS-CGTA IN TXF-FLD(TXF-I).  
           MOVE TXF-NUM-DTSN(TXF-I) TO FRM-NUM-DTSN IN TXF-FLD(TXF-I).  
           MOVE TXF-NUM-MTSN(TXF-I) TO FRM-NUM-MTSN IN TXF-FLD(TXF-I).  
           MOVE TXF-NUM-STSN(TXF-I) TO FRM-NUM-STSN IN TXF-FLD(TXF-I).  
           MOVE TXF-NUM-ATSN(TXF-I) TO FRM-NUM-ATSN IN TXF-FLD(TXF-I).  
           MOVE TXF-COD-OFIC(TXF-I) TO FRM-COD-OFIC IN TXF-FLD(TXF-I).  
           MOVE TXF-GLS-OFIC(TXF-I) TO FRM-GLS-OFIC IN TXF-FLD(TXF-I).  
           MOVE TXF-GLS-DGTA(TXF-I) TO FRM-GLS-DGTA IN TXF-FLD(TXF-I).  
           MOVE TXF-COD-VCAM(TXF-I) TO FRM-COD-VCAM IN TXF-FLD(TXF-I).  
           MOVE TXF-GLS-VCAM(TXF-I) TO FRM-GLS-VCAM IN TXF-FLD(TXF-I).  
                                                                        
       FIN-LLE-DAT-PNT.                                                 
           EXIT.                                                        
                                                                        
                                                                        
       INC-REG-TS SECTION.                                              
       INI-INC-REG-TS.                                                  
           MOVE TXF-SI-IMP TO TXF-IMP.                                  
           ADD 1 TO TXF-I.                                              
           IF TXF-I > TXF-IMAX                                          
               PERFORM WRT-TS                                           
               PERFORM CLR-VEC-TS VARYING TXF-I FROM 1 BY 1             
                                    UNTIL TXF-I > TXF-IMAX              
               MOVE 1 TO TXF-I                                          
               MOVE TXF-NO-IMP TO TXF-IMP.                              
       FIN-INC-REG-TS.                                                  
           EXIT.                                                        
                                                                        
       WRT-TS SECTION.                                                  
       INI-WRT-TS.                                                      
           ADD 1 TO SCR-IMAX.                                           
           MOVE TXF-NOM-TSTXF  TO QUE-COLA.                             
           MOVE TXF-TS         TO QUE-ITEM.                             
           MOVE TXF-LEN-TSTXF  TO QUE-LITM.                             
           MOVE TXF-I          TO QUE-NITM.                             
           MOVE QUE-PUT        TO QUE-CMND.                             
           PERFORM GNS-PRO-QUE.                                         
       FIN-WRT-TS.                                                      
           EXIT.                                                        
                                                      
                  
       CLR-VEC-TS SECTION.                                              
       INI-CLR-VEC-TS.                                                  
                                                                        
           MOVE SPACES              TO TXF-CIC-GTIA(TXF-I).             
           MOVE ZEROES              TO TXF-NUM-ICLI(TXF-I).             
           MOVE SPACES              TO TXF-VRF-ICLI(TXF-I).             
           MOVE SPACES              TO TXF-IND-ICLI(TXF-I).             
           MOVE SPACES              TO TXF-GLS-ICLI(TXF-I).             
           MOVE SPACES              TO TXF-GLS-NOMC(TXF-I).             
           MOVE SPACES              TO TXF-COD-SOPE(TXF-I).             
           MOVE SPACES              TO TXF-COD-TOPE(TXF-I).             
           MOVE SPACES              TO TXF-GLS-CGTA(TXF-I).             
           MOVE ZEROES              TO TXF-NUM-DTSN(TXF-I).             
           MOVE ZEROES              TO TXF-NUM-MTSN(TXF-I).             
           MOVE ZEROES              TO TXF-NUM-STSN(TXF-I).             
           MOVE ZEROES              TO TXF-NUM-ATSN(TXF-I).             
           MOVE SPACES              TO TXF-COD-OFIC(TXF-I).             
           MOVE SPACES              TO TXF-GLS-OFIC(TXF-I).             
           MOVE SPACES              TO TXF-GLS-DGTA(TXF-I).            
           MOVE SPACES              TO TXF-COD-VCAM(TXF-I).             
           MOVE SPACES              TO TXF-GLS-VCAM(TXF-I).             
       FIN-CLR-VEC-TS.                                                  
           EXIT.                                                        
                                                                        
                                                                        
       RED-TS SECTION.                                                  
       INI-RED-TS.                                                      
           MOVE TXF-I TO SCR-I.                                         
           EXEC CICS HANDLE CONDITION QIDERR(FIN-RED-TS)                
                                     ITEMERR(FIN-RED-TS)              
           END-EXEC.                                                    
           MOVE TXF-NOM-TSTXF   TO QUE-COLA.                            
           MOVE TXF-TS          TO QUE-ITEM.                            
           MOVE TXF-LEN-TSTXF   TO QUE-LITM.                            
           MOVE TXF-I           TO QUE-NITM.                            
           MOVE QUE-GET         TO QUE-CMND.                            
           PERFORM GNS-PRO-QUE.                                         
           MOVE QUE-ITEM       TO TXF-TS.                               
       FIN-RED-TS.                                                      
           EXIT.                                                        
                                                                        
