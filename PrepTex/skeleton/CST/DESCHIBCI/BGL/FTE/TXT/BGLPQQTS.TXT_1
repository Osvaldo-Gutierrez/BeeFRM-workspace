*%! Codigo adicional para archivo BGLPQTAS
*% FNCPQ = "OWFBGL"
*% VSNPQ = "06.06.29"
*% LARGO_MI_COMMAREA = 20
*% PGM_PTC = TRUE
*% IF MI_COMMAREA                                                 
      *<<<< MI_COMMAREA                                                 
           03 CMA-VARI.                                                 
              05 SCR-IMAX      COMP                   PIC S9(04).       
              05 SCR-I         COMP                   PIC S9(04).       
      *>>>>
*% END
*% IF WSS                                                         
      *<<<< WSS                                                         
       COPY BGLWGQTS.
       COPY GNSWGCPT.
       COPY BGLBRGDD.                                                   
       COPY BGLBRGVT.                                                   
       COPY SGCBRDBC.                                                   
       COPY BGLBRGDG.                                                   
       COPY TABBRCAM.
       COPY GNSWGPES.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GVT-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CAM-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GDG-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GDD-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.        

       01  WSS-VARI.                                                    
           03  WSS-IND-EXIS                       PIC X(01).             
           03  WSS-VAL-PESO                       PIC S9(11).           
           03  WSS-I                              PIC 9(02).            
           03  WSS-FEC-CNT1                       PIC X(08).
           03  WSS-FEC-CNT                        PIC X(08).
           03  WSS-VAL-CNT     VALUE ZEROES       PIC S9(11)V9(04).
           03  WSS-VAL-CNT1    VALUE ZEROES       PIC S9(11)V9(04).
           03  WSS-COD-PESO                       PIC X(06).         
           03 WSS-COD-CGTA.                                             
              05 WSS-TIP-CGTA                          PIC X(03).       
              05 WSS-STP-CGTA                          PIC X(03).       
           03 WSS-GLS-CGTA.                                             
              05 WSS-GLS-TOPE                   PIC X(12).              
              05 WSS-GLS-SOPE                   PIC X(12).              
           03 WSS-DES-REG    VALUE ZEROES       PIC 9(04).              
           03 WSS-EXI-QTS    VALUE ZEROES       PIC 9(04).              
                                                                        
           03  WSS-FEC-FINI.                                            
               05 WSS-NUM-SINI                    PIC 9(02).            
               05 WSS-NUM-AINI                    PIC 9(02).            
               05 WSS-NUM-MINI                    PIC 9(02).            
               05 WSS-NUM-DINI                    PIC 9(02).            
                                                                        
           03  WSS-FEC-FFIN.                                            
               05 WSS-NUM-SFIN                    PIC 9(02).            
               05 WSS-NUM-AFIN                    PIC 9(02).            
               05 WSS-NUM-MFIN                    PIC 9(02).            
               05 WSS-NUM-DFIN                    PIC 9(02).            
                                                                        
           03  WSS-FEC-FVEN.                                            
               05 WSS-NUM-SVEN                    PIC 9(02).            
               05 WSS-NUM-AVEN                    PIC 9(02).            
               05 WSS-NUM-MVEN                    PIC 9(02).            
               05 WSS-NUM-DVEN                    PIC 9(02).            
      *>>>>
*% END
*% IF INI_FIO
      *<<<< INI_FIO                                                     
      *CAF-INI                                                          
           IF SCR-OFRM-SRV                                              
              MOVE TSK-TERM-ALF TO QTS-COD-TERM                         
              MOVE TSK-TERM-ALF TO QTS-COD-TERM-MAS                     
           ELSE                                                         
              MOVE EIBTRMID     TO QTS-COD-TERM                         
              MOVE EIBTRMID     TO QTS-COD-TERM-MAS.                    
      *CAF-FIN                                                          
      *>>>>                                                             
*% END
*% IF PFS_SCR_ACC                                                 
      *<<<< PFS_SCR_ACC                                                 
           IF FRM-FFLD = FRM-FFLD-PF6                                   
                    OR   FRM-FFLD-PF5                                   
               PERFORM VAL-NUM-KEY-QTS                                  
               PERFORM VAL-NUM-FLD-QTS                                  
               PERFORM RST-STAT                                         
               PERFORM FND-SCR-KEY THRU FIN-SCR-KEY                     
               IF SCR-STAT NOT = SCR-STAT-ABT                           
                   PERFORM SCR-TRL                                      
               ELSE                                                     
                   PERFORM DESSCRTRL                                    
                   GO TO FIN-SCR-ACC.                                   
      *>>>>
*% END
*% IF INI_FND_KEY                                                 
      *<<<< INI_FND_KEY                                                 
           MOVE SPACES TO FRM-MEN1.                                     
           IF (FRM-FFLD = FRM-FFLD-PF5 AND                              
            FRM-IND-HMAS IN QTS-FLD NOT = '-') OR                       
            (FRM-FFLD = FRM-FFLD-PF6 AND                                
            FRM-IND-HMAS IN QTS-FLD = '-')                              
               MOVE PGM-SNUL TO PGM-STAT-TAS                            
               SUBTRACT 1 FROM SCR-I GIVING QTS-I                       
               IF QTS-I NOT > ZEROES                                    
                   MOVE 'TAS GTIAPRV' TO MSG-COD-MENS                   
                   GO TO ERR-FND-KEY                                    
               ELSE                                                     
                   GO TO CARGA-QTS                                      
           ELSE                                                         
           IF (FRM-FFLD = FRM-FFLD-PF6 AND                              
            FRM-IND-HMAS IN QTS-FLD NOT = '-') OR                       
            (FRM-FFLD = FRM-FFLD-PF5 AND                                
            FRM-IND-HMAS IN QTS-FLD = '-')                              
               MOVE PGM-SNUL TO PGM-STAT-TAS                            
               ADD 1 SCR-I GIVING QTS-I                                 
               IF QTS-I > SCR-IMAX                                      
                   MOVE 'TAS GTIANXT' TO MSG-COD-MENS                   
                   GO TO ERR-FND-KEY                                    
               ELSE                                                     
                   GO TO CARGA-QTS.                                     
           GO TO CON-FND-KEY.                                           
      *>>>>
*% END
*% IF FIN_FND_KEY                                                 
      *<<<< FIN_FND_KEY                                                 
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-FND-KEY.                                       
       CON-FND-KEY.                                                     
           MOVE ZEROES TO SCR-IMAX.                                     
           MOVE 1 TO SCR-I.                                             
           MOVE '00' TO FIO-STAT.                                       
           PERFORM OBT-DAT-TS.                                          
           MOVE 1 TO SCR-I.                                             
           MOVE SCR-I TO QTS-I.                                         
       CARGA-QTS.                                                       
           MOVE SPACES TO MSG-COD-MENS.                                 
           IF NOT SCR-OFRM-SRV                                          
           PERFORM PUT-TAB-QTS.                                         
           MOVE '00' TO FIO-STAT.                                       
           GO TO FIN-FND-KEY.                                           
       ERR-FND-KEY.                                                     
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           MOVE 'BGL'  TO MSG-COD-SIST.                                 
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MEN1.                               
           MOVE '00' TO FIO-STAT.                                       
           GO TO FIN-FND-KEY.                                           
      *>>>>
*% END
*% IF PFS_GET_CON OR PFS_GET_ACC                                  
      *<<<< PFS_GET_CON OR PFS_GET_ACC                                  
           IF NOT (FRM-FFLD = FRM-FFLD-PF6                              
                         OR   FRM-FFLD-PF5)                             
      *>>>>
*% END
*% IF EOF                                                         
      *<<<< EOF                                                         
       COPY GNSBEGLS.                                                   
       COPY BGLBFGDD.                                                   
       COPY BGLBFGVT.                                                   
       COPY BGLBFGDG.
       COPY TABBFCAM.                                                  
       COPY GNSBGCPT.
       COPY SGCBFDBC.
       COPY GNSBGPES.                                                   

       CAR-MOV-TAS SECTION.                                             
       INI-CAR-MOV-TAS.                                                 
           MOVE 'N'    TO WSS-IND-EXIS.                                 
           MOVE ZEROES TO WSS-EXI-QTS.                                  
           IF FRM-IND-HMAS IN QTS-FLD  = '-'                            
              MOVE '999999999999'          TO                           
              TAS-CIC-GTIA IN TAS                                       
              MOVE FIO-GET-LEQ TO FIO-CMND                              
              GO TO LUP-CAR-MOV-TAS                                     
           ELSE                                                         
              MOVE SPACES                  TO                           
              TAS-CIC-GTIA IN TAS                                       
              MOVE FIO-GET-NLS TO FIO-CMND.                             
       LUP-CAR-MOV-TAS.                                                 
           PERFORM BGL-FIO-TAS.                                         
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-CAR-MOV-TAS.                                  
          IF TAS-IND-ESTA IN QTS-FLD NOT = TAS-IND-ESTA IN TAS         
                  GO TO SIG-CAR-MOV-TAS.                                
       VAL-CAR-MOV-TAS.                                                 
           MOVE TAS-NUM-SVEN IN TAS TO WSS-NUM-SVEN.                    
           MOVE TAS-NUM-AVEN IN TAS TO WSS-NUM-AVEN.                    
           MOVE TAS-NUM-MVEN IN TAS TO WSS-NUM-MVEN.                    
           MOVE TAS-NUM-DVEN IN TAS TO WSS-NUM-DVEN.                    
           MOVE TAS-CAI-GTIA IN TAS TO GDG-CAI-GTIA IN GDG.             
           MOVE TAS-IIC-GTIA IN TAS TO GDG-IIC-GTIA IN GDG.             
           MOVE FIO-GET-KEY         TO FIO-CMND.                        
           PERFORM BGL-FIO-GDG.                                         
           IF NOT FIO-STAT-OKS                                          
               GO TO SIG-CAR-MOV-TAS.                                   
           IF WSS-IND-EXIS = 'N'                                        
              IF FRM-CAI-GRTA IN QTS-FLD > SPACES AND                   
                 FRM-IIC-GRTA IN QTS-FLD > ZEROES                       
                 IF NOT                                                 
                   (FRM-CAI-GRTA IN QTS-FLD = TAS-CAI-GTIA IN TAS AND   
                    FRM-IIC-GRTA IN QTS-FLD = TAS-IIC-GTIA IN TAS )     
                    GO TO SIG-CAR-MOV-TAS                               
                 ELSE                                                   
                    MOVE 'S' TO WSS-IND-EXIS                            
                    GO TO SIG-CAR-MOV-TAS.                              
           IF FRM-FEC-FINI IN QTS-FLD > ZEROES AND                      
              FRM-FEC-FFIN IN QTS-FLD NOT > ZEROES                      
              MOVE FRM-NUM-DINI IN QTS-FLD TO WSS-NUM-DINI              
              MOVE FRM-NUM-MINI IN QTS-FLD TO WSS-NUM-MINI              
              MOVE FRM-NUM-SINI IN QTS-FLD TO WSS-NUM-SINI              
              MOVE FRM-NUM-AINI IN QTS-FLD TO WSS-NUM-AINI              
              IF (WSS-FEC-FVEN < WSS-FEC-FINI)                          
                    GO TO SIG-CAR-MOV-TAS.                              
           IF FRM-FEC-FINI IN QTS-FLD NOT > ZEROES AND                  
              FRM-FEC-FFIN IN QTS-FLD > ZEROES                          
              MOVE FRM-NUM-DFIN IN QTS-FLD TO WSS-NUM-DFIN              
              MOVE FRM-NUM-MFIN IN QTS-FLD TO WSS-NUM-MFIN              
              MOVE FRM-NUM-SFIN IN QTS-FLD TO WSS-NUM-SFIN              
              MOVE FRM-NUM-AFIN IN QTS-FLD TO WSS-NUM-AFIN              
              IF (WSS-FEC-FVEN > WSS-FEC-FFIN)                          
                    GO TO SIG-CAR-MOV-TAS.                              
           IF FRM-FEC-FINI IN QTS-FLD > ZEROES AND                      
              FRM-FEC-FFIN IN QTS-FLD > ZEROES                          
              MOVE FRM-NUM-DINI IN QTS-FLD TO WSS-NUM-DINI              
              MOVE FRM-NUM-MINI IN QTS-FLD TO WSS-NUM-MINI              
              MOVE FRM-NUM-SINI IN QTS-FLD TO WSS-NUM-SINI              
              MOVE FRM-NUM-AINI IN QTS-FLD TO WSS-NUM-AINI              
              MOVE FRM-NUM-DFIN IN QTS-FLD TO WSS-NUM-DFIN              
              MOVE FRM-NUM-MFIN IN QTS-FLD TO WSS-NUM-MFIN              
              MOVE FRM-NUM-SFIN IN QTS-FLD TO WSS-NUM-SFIN              
              MOVE FRM-NUM-AFIN IN QTS-FLD TO WSS-NUM-AFIN              
      *EVA-INI                                                          
              IF (WSS-FEC-FINI > WSS-FEC-FFIN)                          
                   MOVE 'BGL'            TO MSG-COD-SIST                
                   MOVE 'ERRVALFEC'      TO MSG-COD-MENS                
                   PERFORM GET-MSG                                      
                   MOVE MSG-GLS-DESC     TO FRM-MENS                    
                   GO TO FIN-CAR-MOV-TAS                                
              ELSE                                                      
              IF (WSS-FEC-FVEN < WSS-FEC-FINI OR                        
                  WSS-FEC-FVEN > WSS-FEC-FFIN)                          
                    GO TO SIG-CAR-MOV-TAS.                              
                                                                        
      *EVA-FIN                                                          
           MOVE TAS-CAI-GTIA IN TAS TO QTS-CAI-GTIA(QTS-I).             
           MOVE TAS-IIC-GTIA IN TAS TO QTS-IIC-GTIA(QTS-I).             
           MOVE TAS-NUM-DTSN IN TAS TO QTS-NUM-DTSN(QTS-I).             
           MOVE TAS-NUM-MTSN IN TAS TO QTS-NUM-MTSN(QTS-I).             
           MOVE TAS-NUM-STSN IN TAS TO QTS-NUM-STSN(QTS-I).       
           MOVE TAS-NUM-ATSN IN TAS TO QTS-NUM-ATSN(QTS-I).             
           MOVE TAS-NUM-DVEX IN TAS TO QTS-NUM-DVEX(QTS-I).             
           MOVE TAS-NUM-MVEX IN TAS TO QTS-NUM-MVEX(QTS-I).             
           MOVE TAS-NUM-SVEX IN TAS TO QTS-NUM-SVEX(QTS-I).             
           MOVE TAS-NUM-AVEX IN TAS TO QTS-NUM-AVEX(QTS-I).             
      *CAF-INI                                                          
           IF SCR-OFRM-SRV                                              
              MOVE TAS-VAL-MNTO IN TAS TO QTS-VAL-MNTO-EDT              
              MOVE QTS-VAL-MNTO-EDT TO QTS-VAL-MNTO-ALF(QTS-I)          
           ELSE                                                         
              MOVE TAS-VAL-MNTO IN TAS TO QTS-VAL-MNTO(QTS-I).          
      *CAF-FIN                                                    
           MOVE TAS-NUM-DVEN IN TAS TO QTS-NUM-DVEN(QTS-I).             
           MOVE TAS-NUM-MVEN IN TAS TO QTS-NUM-MVEN(QTS-I).             
           MOVE TAS-NUM-SVEN IN TAS TO QTS-NUM-SVEN(QTS-I).             
           MOVE TAS-NUM-AVEN IN TAS TO QTS-NUM-AVEN(QTS-I).             
           MOVE TAS-COD-TOPE IN TAS TO QTS-COD-TOPE(QTS-I).             
           MOVE TAS-COD-SOPE IN TAS TO QTS-COD-SOPE(QTS-I).             
                                                                        
      *BUS-TAB busca glosa de codigo en tabla                           
           IF TAS-COD-TOPE IN TAS > SPACES                              
               MOVE 'TAB' TO TAB-COD-SIST                               
               MOVE 'AUX ' TO TAB-COD-TTAB IN TAB                       
               MOVE TAS-COD-TOPE IN TAS TO WSS-TIP-CGTA IN WSS-VARI     
               MOVE TAS-COD-SOPE IN TAS TO WSS-STP-CGTA IN WSS-VARI     
               MOVE WSS-COD-CGTA IN WSS-VARI TO TAB-COD-CTAB IN TAB     
               PERFORM BUS-TAB                                          
               MOVE TAB-GLS-DCOR IN TAB   TO WSS-GLS-SOPE IN WSS-VARI   
           ELSE                                                         
               MOVE SPACES TO WSS-GLS-SOPE IN WSS-VARI.         
                                                                              
           IF TAS-COD-TOPE IN TAS > SPACES                              
               MOVE 'TAB'                   TO TAB-COD-SIST             
               MOVE 'TIO'                   TO TAB-COD-TTAB IN TAB      
               MOVE SPACES                  TO TAB-COD-CTAB IN TAB      
               MOVE TAS-COD-TOPE IN TAS     TO TAB-COD-CTAB IN TAB      
               PERFORM BUS-TAB                                          
               MOVE TAB-GLS-DCOR IN TAB     TO WSS-GLS-TOPE IN WSS-VARI 
           ELSE                                                         
               MOVE SPACES TO WSS-GLS-TOPE IN WSS-VARI.                 
           MOVE WSS-GLS-CGTA TO CPT-STRN.                               
           PERFORM CPT-BLKS.                                            
           MOVE CPT-STRN TO QTS-GLS-TIPO(QTS-I).                        
           MOVE TAS-GLS-EXCE IN TAS TO QTS-GLS-EXCE(QTS-I).             
           IF TAS-IND-ESTA IN QTS-FLD = '11'                            
              MOVE '11' TO QTS-GLS-STDO(QTS-I)                          
           ELSE                                                         
           IF TAS-IND-ESTA IN QTS-FLD = '12'                            
              MOVE '12' TO QTS-GLS-STDO(QTS-I).                         
                                                                         
           IF GDG-COD-VCAM IN GDG > SPACES                              
               MOVE 'TAB' TO TAB-COD-SIST                               
               MOVE 'VLR' TO TAB-COD-TTAB IN TAB                        
               MOVE GDG-COD-VCAM IN GDG TO TAB-COD-CTAB IN TAB          
               PERFORM BUS-TAB                                          
               MOVE TAB-GLS-ABRV IN TAB TO                              
                    QTS-GLS-VCOC(QTS-I).                                
       NXT-CAR-MOV-TAS.                                                 
              PERFORM INC-REG-TS.                                       
              ADD 1 TO WSS-EXI-QTS.                                     
       SIG-CAR-MOV-TAS.                                                 
           IF FRM-IND-HMAS IN QTS-FLD  = '-'                            
               MOVE FIO-GET-PRV TO FIO-CMND                             
               GO TO LUP-CAR-MOV-TAS                                    
           ELSE                                                         
               MOVE FIO-GET-NXT TO FIO-CMND                             
               GO TO LUP-CAR-MOV-TAS.                                   
      FIN-CAR-MOV-TAS.    
           IF SCR-OFRM-SRV                                              
              PERFORM PUT-QTS-MAS.                                      
           EXIT.     

       PUT-QTS-MAS SECTION.                                             
       INI-PUT-QTS-MAS.                                                 
           MOVE 1                  TO QUE-NITM.                         
           MOVE QTS-LEN-TSQTS-MAS  TO QUE-LITM.                         
           MOVE QTS-NOM-TSQTS-MAS  TO QUE-COLA.                         
           IF WSS-EXI-QTS > QTS-IMAX-MTX                                
              MOVE 'S'             TO QTS-MAS-IND-HMAS                  
              MOVE QTS-IMAX-MTX    TO QTS-MAS-NUM-TOCC                  
           ELSE                                                         
              MOVE 'N'             TO QTS-MAS-IND-HMAS                  
              MOVE WSS-EXI-QTS     TO QTS-MAS-NUM-TOCC.                 
           MOVE QTS-MAS-ITEM       TO QUE-ITEM.                         
           MOVE QUE-PUT            TO QUE-CMND.                         
           PERFORM GNS-PRO-QUE.                                         
       FIN-PUT-QTS-MAS.                                                 
           EXIT.                                                        
                                                                        
       OBT-PARAM SECTION.                                               
       INI-OBT-PARAM.                                                   
       FIN-OBT-PARAM.                                                   
           EXIT.                                                        
                                                                        
       OBT-DAT-TS SECTION.                                              
       INI-OBT-DAT-TS.                                                  
           EXEC CICS HANDLE CONDITION QIDERR(CON-OBT-DAT-TS)            
                                     ITEMERR(CON-OBT-DAT-TS)            
           END-EXEC.                                                    
                                                                        
           MOVE QTS-NOM-TSQTS     TO QUE-COLA.                             
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
       CON-OBT-DAT-TS.                                                  
           MOVE QTS-SI-IMP TO QTS-IMP.                                  
           PERFORM CLR-VEC-TS VARYING QTS-I FROM 1 BY 1 UNTIL           
                   QTS-I > QTS-IMAX.                                    
           MOVE 1 TO QTS-I.                                         
           IF TAS-IND-ESTA IN QTS-FLD  = '10'                           
              MOVE '11' TO TAS-IND-ESTA IN TAS                          
              MOVE '11' TO TAS-IND-ESTA IN QTS-FLD                      
              PERFORM CAR-MOV-TAS                                       
              MOVE '12' TO TAS-IND-ESTA IN TAS                          
              MOVE '12' TO TAS-IND-ESTA IN QTS-FLD                      
              PERFORM CAR-MOV-TAS                                       
              MOVE '10' TO TAS-IND-ESTA IN QTS-FLD.                     
           PERFORM CAR-MOV-TAS.                                           
       NXT-OBT-DAT-TS.                                                  
           IF QTS-IMP = QTS-SI-IMP                                      
               PERFORM WRT-TS.                                          
       FIN-OBT-DAT-TS.                                                  
           EXIT.                                                        
                                                                        
       PUT-TAB-QTS SECTION.                                             
       INI-PUT-TAB-QTS.                                                 
           PERFORM RED-TS.                                              
           PERFORM LLE-DAT-PNT VARYING QTS-I FROM 1 BY 1                
                   UNTIL QTS-I > QTS-IMAX.                              
       FIN-PUT-TAB-QTS.                                                 
           EXIT.                                                        
                                                                        
       LLE-DAT-PNT SECTION.                                             
       INI-LLE-DAT-PNT.                                                 
           MOVE QTS-CAI-GTIA(QTS-I) TO FRM-CAI-GTIA IN QTS-FLD(QTS-I).  
           MOVE QTS-IIC-GTIA(QTS-I) TO FRM-IIC-GTIA IN QTS-FLD(QTS-I).  
           MOVE QTS-NUM-DTSN(QTS-I) TO FRM-NUM-DTSN IN QTS-FLD(QTS-I).  
           MOVE QTS-NUM-MTSN(QTS-I) TO FRM-NUM-MTSN IN QTS-FLD(QTS-I).  
           MOVE QTS-NUM-STSN(QTS-I) TO FRM-NUM-STSN IN QTS-FLD(QTS-I).  
           MOVE QTS-NUM-ATSN(QTS-I) TO FRM-NUM-ATSN IN QTS-FLD(QTS-I).  
           MOVE QTS-NUM-DVEX(QTS-I) TO FRM-NUM-DVEX IN QTS-FLD(QTS-I).  
           MOVE QTS-NUM-MVEX(QTS-I) TO FRM-NUM-MVEX IN QTS-FLD(QTS-I).  
           MOVE QTS-NUM-SVEX(QTS-I) TO FRM-NUM-SVEX IN QTS-FLD(QTS-I).  
           MOVE QTS-NUM-AVEX(QTS-I) TO FRM-NUM-AVEX IN QTS-FLD(QTS-I).  
           IF SCR-OFRM-SRV                                              
              MOVE QTS-VAL-MNTO-NUM(QTS-I) TO                           
                   FRM-VAL-MNTO IN QTS-FLD(QTS-I)                       
           ELSE                                                         
              MOVE QTS-VAL-MNTO(QTS-I) TO                               
                   FRM-VAL-MNTO IN QTS-FLD(QTS-I).                      
           MOVE QTS-NUM-DVEN(QTS-I) TO FRM-NUM-DVEN IN QTS-FLD(QTS-I).  
           MOVE QTS-NUM-MVEN(QTS-I) TO FRM-NUM-MVEN IN QTS-FLD(QTS-I).  
           MOVE QTS-NUM-SVEN(QTS-I) TO FRM-NUM-SVEN IN QTS-FLD(QTS-I).  
           MOVE QTS-NUM-AVEN(QTS-I) TO FRM-NUM-AVEN IN QTS-FLD(QTS-I).  
           MOVE QTS-COD-TOPE(QTS-I) TO FRM-COD-TOPE IN QTS-FLD(QTS-I).  
           MOVE QTS-COD-SOPE(QTS-I) TO FRM-COD-SOPE IN QTS-FLD(QTS-I).  
           MOVE QTS-GLS-TIPO(QTS-I) TO FRM-GLS-TIPO IN QTS-FLD(QTS-I).  
           MOVE QTS-GLS-EXCE(QTS-I) TO FRM-GLS-EXCE IN QTS-FLD(QTS-I).  
           MOVE QTS-GLS-STDO(QTS-I) TO FRM-GLS-STDO IN QTS-FLD(QTS-I).  
           MOVE QTS-GLS-VCOC(QTS-I) TO FRM-GLS-VCOC IN QTS-FLD(QTS-I).  
       FIN-LLE-DAT-PNT.                                                 
           EXIT.                                                        
                                                                        
       INC-REG-TS SECTION.                                              
       INI-INC-REG-TS.                                                  
           MOVE QTS-SI-IMP TO QTS-IMP.                                 
           ADD 1 TO QTS-I.                                              
           IF QTS-I > QTS-IMAX                                          
               PERFORM WRT-TS                                           
               PERFORM CLR-VEC-TS VARYING QTS-I FROM 1 BY 1             
                                     UNTIL QTS-I > QTS-IMAX             
               MOVE 1 TO QTS-I.                                          
               MOVE QTS-NO-IMP TO QTS-IMP.                              
       FIN-INC-REG-TS.                                                  
           EXIT.               
                                         
                                                                        
       WRT-TS SECTION.                                                  
       INI-WRT-TS.                                                      
           ADD 1 TO SCR-IMAX.                                           
           MOVE QTS-NOM-TSQTS  TO QUE-COLA.                             
           MOVE QTS-TS         TO QUE-ITEM.
           MOVE QTS-LEN-TSQTS  TO QUE-LITM.                             
           MOVE QTS-I          TO QUE-NITM.                             
           MOVE QUE-PUT        TO QUE-CMND.                             
           PERFORM GNS-PRO-QUE.                                         
       FIN-WRT-TS.                                                      
           EXIT.                                                        
                                                                        
       RED-TS SECTION.                                                  
       INI-RED-TS.                                                      
           MOVE QTS-I TO SCR-I.                                         
           EXEC CICS HANDLE CONDITION QIDERR(FIN-RED-TS)                
                                     ITEMERR(FIN-RED-TS)                
           END-EXEC.                                                    
           MOVE QTS-NOM-TSQTS   TO QUE-COLA.                             
           MOVE QTS-TS          TO QUE-ITEM.                             
           MOVE QTS-LEN-TSQTS   TO QUE-LITM.                             
           MOVE QTS-I           TO QUE-NITM.                         
           MOVE QUE-GET         TO QUE-CMND.                         
           PERFORM GNS-PRO-QUE.                                         
