*%! Codigo adicional para archivo BGLPQSEG
*% IF SETTAG
*% FNCPQ = "OWFBGL"
*% VSNPQ = "06.06.29"
*% FNCPU = "OWFBGL"
*% VSNPU = "06.06.29"
*% END
*% PGM_PTC = TRUE
*% IF WSS
      *<<<< WSS
*% IF PGM_PQ
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-TAG-REQA==.        
       COPY BGLBRTAG.                                                   
*% END

       01 WSS-VARI.                                                     
           03 WSS-COD-CGTA.                                             
              05 WSS-TIP-CGTA                   PIC X(03).              
              05 WSS-STP-CGTA                   PIC X(03).              
                                                                        
           03 WSS-GLS-CGTA.                                             
              05 WSS-GLS-TOPE                   PIC X(40).              
              05 WSS-GLS-SOPE                   PIC X(40).              

*% IF PGM_PU
           03 WSS-PER-PERI.                                            
              05 WSS-GLS-CANT                   PIC 9(02).             
              05 WSS-GLS-PERI                   PIC X(01).             
              05 WSS-GLS-REST                   PIC X(57).             
           03 WSS-FEC-PERI.                                            
              05 WSS-NUM-SPER                   PIC 9(02).             
              05 WSS-NUM-APER                   PIC 9(02).             
              05 WSS-NUM-MPER                   PIC 9(02).             
              05 WSS-NUM-DPER                   PIC 9(02).  
              
*% END
       COPY GNSWGCPT.                                                   
       COPY BGLBRGDG.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GDG-REQA==.        
       COPY BGLBRGDD.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GDD-REQA==.        
       COPY SGCBRDBC.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.        
      *MAE                                                             
*% IF PGM_PU
       COPY BGLBRGSE.                                                  
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GSE-REQA==.       
      *MAE                                                             
*% END
      *>>>>
*% END
*% IF FIN_PGM_STAT AND PGM_PQ
      *<<<<
              05 PGM-STAT-TAG.                                          
                 88 PGM-STAT-TAG-NUL    VALUE '  '.                     
                 07 PGM-STT1-TAG        VALUE '2' PIC X(01).            
                    88 PGM-STAT-TAG-OKS VALUE '0'.                      
                 07 PGM-STT2-TAG        VALUE '3' PIC X(01).            
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< FIN_FND_KEY
           MOVE POL-CAI-GTIA IN SEG-FLD TO GDG-CAI-GTIA IN GDG.         
           MOVE POL-IIC-GTIA IN SEG-FLD TO GDG-IIC-GTIA IN GDG.         
           MOVE FIO-GET-KEY         TO FIO-CMND.                        
           PERFORM BGL-FIO-GDG.                                         
           MOVE FIO-STAT TO PGM-STAT-GDG.                               
           IF NOT (FIO-STAT-OKS)                                        
              MOVE 'GDG  NEX' TO MSG-COD-MENS                           
              GO TO ERR-FND-KEY.                                        
      *EVA                                                              
           IF SCR-CMND = 'ING' AND                                      
              GDG-IND-EST1 IN GDG > '19' OR                             
              GDG-IND-EST1 IN GDG < '10'                                
              MOVE 'GDGNOING' TO MSG-COD-MENS                           
              GO TO ERR-FND-KEY.                                        
      *EVA                                                              
           PERFORM PUT-GDG-SEG.                                         
                                                                        
           MOVE POL-CAI-GTIA IN SEG-FLD TO GDD-CAI-GTIA IN GDD.         
           MOVE POL-IIC-GTIA IN SEG-FLD TO GDD-IIC-GTIA IN GDD.         
           MOVE FIO-GET-NLS         TO FIO-CMND.                        
        LUP-FND-KEY.                                                    
           MOVE 'GDD-KEY-AGDD'      TO FIO-AKEY.                        
           PERFORM BGL-FIO-GDD.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
              POL-CAI-GTIA IN SEG-FLD = GDD-CAI-GTIA IN GDD AND         
              POL-IIC-GTIA IN SEG-FLD = GDD-IIC-GTIA IN GDD )           
                MOVE 'GDD    NEX'     TO MSG-COD-MENS                   
                GO TO ERR-FND-KEY.                                      
                                                                        
           MOVE GDD-CIC-DCLI IN GDD TO DBC-CIC-ICLI IN DBC.             
           MOVE FIO-GET-KEY         TO FIO-CMND.                        
           PERFORM SGC-FIO-DBC.                                         
           IF NOT FIO-STAT-OKS                                          
              MOVE 'CLI   INEX'     TO MSG-COD-MENS                     
                GO TO ERR-FND-KEY.                                      
                                                                        
           MOVE FIO-STAT            TO PGM-STAT-DBC.                    
           PERFORM PUT-DBC-SEG.                                         
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.                        
           PERFORM CPT-BLKS.                                            
           MOVE CPT-STRN TO DBC-GLS-NOMC IN SEG-FLD.                    
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<<
           GO TO FIN-FND-KEY.                                           
       ERR-FND-KEY.                                                     
           MOVE 'BGL' TO MSG-COD-SIST.                                  
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MENS.                               
           MOVE SCR-STAT-ABT TO SCR-STAT.                               
           IF SCR-CCPP = 'ING'                                          
              MOVE '00'      TO FIO-STAT                                
           ELSE                                                         
              MOVE FIO-STAT-NEX TO FIO-STAT.                            
      *>>>>                                                         
*% END
*% IF CMN_MOD
      *<<<< CMN_MOD
           IF POL-COD-ESGE IN POL = '13'                                
              MOVE '02' TO POL-COD-SEGU IN SEG-FLD                      
              MOVE POL-COD-SEGU IN SEG-FLD TO POL-COD-SEGU IN POL.      
           IF POL-GLS-REAN IN SEG-FLD NOT > SPACES                     
              MOVE POL-COD-EPRO IN SEG-FLD TO POL-COD-EPRO IN POL      
           ELSE                                                        
              MOVE  02  TO POL-COD-EPRO IN SEG-FLD                     
              MOVE POL-COD-EPRO IN SEG-FLD TO POL-COD-EPRO IN POL.     
           IF POL-COD-SEGU IN SEG-FLD  = '01'                          
                MOVE 'S' TO POL-IND-INVA IN POL.                       
      *EVA-AFRI/JUL2007    [RENOVACION TAS/SEG]                        
           IF POL-COD-ESGE IN SEG-FLD = '11' AND                       
              SCR-OFRM-SRV                                             
              MOVE 'MODAUTJAVA' TO POL-COD-ATRN IN POL                 
              MOVE HOY-FHOY     TO POL-FEC-FTRN IN POL                 
              MOVE HOY-HHOY     TO POL-HRA-HRTR IN POL.                
      *FIN-AFRI/JUL2007    [RENOVACION TAS/SEG]                        
      *>>>>
*% END
*% IF INI_CMN_ING
      *<<<<
           MOVE SPACES TO POL-GLS-FLAG IN POL.                         
           MOVE SPACES TO POL-MSC-TACC IN POL.                         
           MOVE 'I'    TO POL-MSC-STAT IN POL.                         
      *>>>>
*% END
*% IF CMN_ING
      *<<<<
           PERFORM GET-FHOY.                                           
           IF SCR-CCPP = 'ING'                                         
              OR SCR-CMND = 'COP'                                      
               MOVE 'I'      TO POL-MSC-TACC IN POL                    
               MOVE HOY-FHOY TO POL-FEC-FTRN IN POL                    
           ELSE                                                        
               MOVE 'M'      TO POL-MSC-TACC    IN POL.                
      *>>>>
*% END
*% IF EOF                                                        
      *<<<< EOF
*% IF PGM_PU
      *MAE                                                             
       COPY BGLBFGSE.                                                  
      *MAE                                                             
      *MAE                                                             
       COPY GNSBPFEC.                                                  
      *MAE                                                             
*% END
       COPY BGLBFGDG.                                                   
       COPY BGLBFGDD.                                                   
*% IF PGM_PQ
       COPY BGLBFTAG.                                                   
*% END
       COPY SGCBFDBC.                                                   
       COPY GNSBGCPT.    
       COPY GNSBBTAB.                                                   
       COPY GNSBVCOD.                                                   
       COPY GNSBVTAB.                                                   
      *>>>>
*%END
*% IF POL_FEC_FTPL
      *<<<< INI_GSE_FEC_VTO_SEG                                         
           MOVE FEC-CHOY-GE TO FEC-CHOY.                                
           MOVE FEC-VHBL-N TO FEC-VHBL.                                 
           MOVE FEC-FBLK-S TO FEC-FBLK.                                 
                                                                        
           MOVE POL-NUM-STPL IN SEG-FLD TO FEC-SEC2.                    
           MOVE POL-NUM-ATPL IN SEG-FLD TO FEC-AEC2.                    
           MOVE POL-NUM-MTPL IN SEG-FLD TO FEC-MEC2.                    
           MOVE POL-NUM-DTPL IN SEG-FLD TO FEC-DEC2.                    
      *>>>>                                                             
*% END
