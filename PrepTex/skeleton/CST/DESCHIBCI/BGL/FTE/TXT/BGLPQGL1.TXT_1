*% PGM_PTC = TRUE
*% IF WSS
      *<<<<                                                             
       01 WSS-VARI.                                                             
           03 WSS-COD-CGTA.                                                     
               05 WSS-TIP-CGTA                          PIC X(03).              
               05 WSS-STP-CGTA                          PIC X(03).              
                                                                                
           03 WSS-GLS-CGTA.                                                     
              05 WSS-GLS-TOPE                           PIC X(22).              
              05 WSS-GLS-SOPE                           PIC X(23).              
                                                                                
           03 WSS-CIC-ICLI                              PIC X(12).              
           03 WSS-COD-CTAB.                                                     
               05 WSS-COD-TTAB                          PIC X(03).              
               05 WSS-TBL-CGTA                          PIC X(06).              
           03 WSS-IND-TBLA                              PIC X(01).              
           03 WSS-IND-CIND                              PIC X(01).              
           03 WSS-IND-CMER                              PIC X(01).              
           03 WSS-IND-CDES                              PIC X(01).              
           03 WSS-IND-CMOB                              PIC X(01).              
           03 WSS-IND-CAGR                              PIC X(01).              
           03 WSS-IND-COTR                              PIC X(01).              
           03 WSS-IND-ECLI                              PIC X(01).              
           03 WSS-IND-EGDD                              PIC X(01).              
           03 WSS-FEC-FHOY.                                                     
               05 WSS-NUM-SHOY                          PIC 9(02).              
               05 WSS-NUM-AHOY                          PIC 9(02).              
               05 WSS-NUM-MHOY                          PIC 9(02).              
               05 WSS-NUM-DHOY                          PIC 9(02).              
           03 WSS-FEC-FNOT.                                                     
               05 WSS-NUM-SNOT                          PIC 9(02).              
               05 WSS-NUM-ANOT                          PIC 9(02).              
               05 WSS-NUM-MNOT                          PIC 9(02).              
               05 WSS-NUM-DNOT                          PIC 9(02).              
           03 WSS-FEC-FMEN.                                                     
               05 WSS-NUM-SMEN                          PIC 9(02).              
               05 WSS-NUM-AMEN                          PIC 9(02).              
               05 WSS-NUM-MMEN                          PIC 9(02).              
               05 WSS-NUM-DMEN                          PIC 9(02).              
           03 WSS-FEC-FINS.                                                     
               05 WSS-NUM-SINS                          PIC 9(02).              
               05 WSS-NUM-AINS                          PIC 9(02).              
               05 WSS-NUM-MINS                          PIC 9(02).              
               05 WSS-NUM-DINS                          PIC 9(02).              
      *>>>>                                                                   
*% END
*% IF FIN_SCR_VARI
      *<<<< 
       COPY GNSWGCPT.                                                           
                                                                    
       COPY BGLBRGDG.                                                           
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GDG-REQA==.                
       COPY BGLBRGAP.                                                           
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GAP-REQA==.                
       COPY BGLBRGLE.                                                           
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GLE-REQA==.                
       COPY DEUBRFIA.                                                           
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-FIA-REQA==.                
       COPY SGCBRDBC.                                                           
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.                
       COPY BGLBRGDD.                                                           
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GDD-REQA==. 
      *>>>> 
*% END
*% IF INI_FND_KEY
      *<<<<                                        
           PERFORM KEY-ALL-GL1.                                                 
           MOVE GL1-CAI-GTIA IN GL1-FLD TO GDG-CAI-GTIA IN GDG.         
           MOVE GL1-IIC-GTIA IN GL1-FLD TO GDG-IIC-GTIA IN GDG.         
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM BGL-FIO-GDG.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE 'GDG    NEX' TO MSG-COD-MENS                        
               GO TO ERR-FND-KEY.                                       
           MOVE FIO-STAT TO PGM-STAT-GDG.                               
           PERFORM PUT-GDG-GL1.                                         
                                                                        
           IF SCR-CMND = 'MOD' AND GDG-IND-EST1 IN GDG > 19             
               MOVE 'GDGALZ    '     TO MSG-COD-MENS                    
               GO TO ERR-FND-KEY.                                       
                                                                        
           MOVE GL1-CAI-GTIA IN GL1-FLD TO GAP-CAI-GTIA IN GAP.         
           MOVE GL1-IIC-GTIA IN GL1-FLD TO GAP-IIC-GTIA IN GAP.         
           MOVE GL1-COD-PACR IN GL1-FLD TO GAP-COD-PACR IN GAP.         
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM BGL-FIO-GAP.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE 'GAP    NEX'     TO MSG-COD-MENS                    
               GO TO ERR-FND-KEY.                                       
                                                                        
           MOVE GL1-CAI-GTIA IN GL1-FLD TO GLE-CAI-GTIA IN GLE.         
           MOVE GL1-IIC-GTIA IN GL1-FLD TO GLE-IIC-GTIA IN GLE.         
           MOVE GL1-COD-PACR IN GL1-FLD TO GLE-COD-PACR IN GLE.         
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM BGL-FIO-GLE.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE 'GLE    NEX'     TO MSG-COD-MENS                    
               GO TO ERR-FND-KEY.                                       
      *>>>>                                                
                                                                        
*% END
*% IF FIN_FND_KEY
      *<<<< 
           GO TO FIN-FND-KEY.                                           
                                                             
       ERR-FND-KEY.                                                     
           MOVE 'BGL' TO MSG-COD-SIST.                                  
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MENS.                               
           MOVE SCR-STAT-ABT TO SCR-STAT.                               
           IF SCR-CCPP = 'ING'                                          
               MOVE '00' TO FIO-STAT                                    
           ELSE                                                         
               MOVE FIO-STAT-NEX TO FIO-STAT.                           
      *>>>>                                                              
*% END
*% IF EOF
      *<<<<                    
       COPY BGLBFGDG.                                                           
       COPY BGLBFGAP.                                                           
       COPY BGLBFGLE.                                                           
       COPY DEUBFFIA.                                                           
       COPY SGCBFDBC.                                                           
       COPY SGCBGCLI.                                                           
       COPY GNSBGCPT.                                                           
       COPY BGLBFGDD.                                                           
      *>>>>                                                              
*% END
*% IF CMN_ING
      *<<<<                                                                   
           MOVE WSS-CIC-ICLI IN WSS-VARI TO GL1-CIC-ICLI IN GL1.                
      *>>>>                                                                     
*% END
