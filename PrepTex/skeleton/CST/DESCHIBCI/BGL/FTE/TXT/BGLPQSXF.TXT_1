*% PGM_PTC = TRUE
*% IF MI_COMMAREA
      *<<<< MI_COMMAREA                                                 
           03 CMA-VARI.                                       
              05 SCR-IMAX      COMP                   PIC S9(04).       
              05 SCR-I         COMP                   PIC S9(04).       
              05 WSS-SGV-SEGU                      PIC S9(11)V9(04).    
              05 WSS-FEC-FVSG.                                          
                 07 WSS-NUM-SVSG                        PIC 9(02).      
                 07 WSS-NUM-AVSG                        PIC 9(02).      
                 07 WSS-NUM-MVSG                        PIC 9(02).      
                 07 WSS-NUM-DVSG                        PIC 9(02).      
              05 WSS-FEC-FFIN.                                          
                 07 WSS-NUM-SFIN                        PIC 9(02).      
                 07 WSS-NUM-AFIN                        PIC 9(02).      
                 07 WSS-NUM-MFIN                        PIC 9(02).      
                 07 WSS-NUM-DFIN                        PIC 9(02).
              05 WSS-FEC-FTIN.                                          
                 07 WSS-NUM-STIN                        PIC 9(02).      
                 07 WSS-NUM-ATIN                        PIC 9(02).
                 07 WSS-NUM-MTIN                        PIC 9(02).      
                 07 WSS-NUM-DTIN                        PIC 9(02).      
              05 WSS-FEC-FTFI.                        
                 07 WSS-NUM-STFI                        PIC 9(02).      
                 07 WSS-NUM-ATFI                        PIC 9(02).      
                 07 WSS-NUM-MTFI                        PIC 9(02).           
                 07 WSS-NUM-DTFI                        PIC 9(02).      
      *>>>>                                                             
*% END
*% IF WSS
      *<<<<
      *MAE                                                              
       COPY GNSWGCPT.                                                   
       COPY BGLWGSXF.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.        
       COPY SGCBRDBC.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-POL-REQA==.        
       COPY BGLBRPOL.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GDD-REQA==.        
       COPY BGLBRGDD.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GDG-REQA==.        
       COPY BGLBRGDG.                                                   
      *MAE                                                              

      *EVA                                                              
       01  WSS-VARI.                                                    
           03 WSS-CON-DTS               VALUE ZEROES    PIC 9(03).      
           03 WSS-SIN-DTS               VALUE ZEROES    PIC 9(03).      
           03 WSS-EXI-SXF               VALUE ZEROES    PIC 9(04).      
           03 WSS-HMAS                  VALUE ZEROES    PIC 9(04).      
      *EVA                                                              
      *>>>>
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC                                                 
           IF FRM-FFLD = FRM-FFLD-PF6                                   
                    OR   FRM-FFLD-PF5                                   
               PERFORM VAL-NUM-KEY-SXF                                  
               PERFORM VAL-NUM-FLD-SXF                                  
               PERFORM RST-STAT                                         
               PERFORM FND-SCR-KEY THRU FIN-SCR-KEY                     
               IF SCR-STAT NOT = SCR-STAT-ABT                           
                   PERFORM SCR-TRL                                      
               ELSE                                                     
                   PERFORM DESSCRTRL                                    
                   GO TO FIN-SCR-ACC.                                   
      *>>>>                                                             
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY                                                 
           MOVE SPACES TO FRM-MEN1.                                     
           IF FRM-FFLD = FRM-FFLD-PF5                                   
               MOVE PGM-SNUL TO PGM-STAT-GSE                            
               MOVE PGM-SNUL TO PGM-STAT-GSE                            
               SUBTRACT 1 FROM SCR-I GIVING SXF-I                       
               IF SXF-I NOT > ZEROES                                    
                   MOVE 'GSE GTIAPRV' TO MSG-COD-MENS                   
                   GO TO ERR-FND-KEY                                    
               ELSE                                                     
                   GO TO CARGA-SXF                                      
           ELSE                                                         
           IF FRM-FFLD = FRM-FFLD-PF6                                   
               MOVE PGM-SNUL TO PGM-STAT-GSE                            
               MOVE PGM-SNUL TO PGM-STAT-GSE                            
               ADD 1 SCR-I GIVING SXF-I                                 
               IF SXF-I > SCR-IMAX                                      
                   MOVE 'GSE GTIANXT' TO MSG-COD-MENS                   
                   GO TO ERR-FND-KEY                                    
               ELSE                                                     
                   GO TO CARGA-SXF.                                     
           GO TO CON-FND-KEY.                                           
      *>>>>                                                             
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY                                                 
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-FND-KEY.                                       
       CON-FND-KEY.                                                     
                                                                        
           MOVE ZEROES TO SCR-IMAX.                                     
           MOVE 1 TO SCR-I.                                             
           MOVE '00' TO FIO-STAT.                                       
           PERFORM OBT-DAT-TS.                                          
           IF (WSS-SIN-DTS > ZEROES AND WSS-CON-DTS = ZEROES)           
               MOVE 'REGEOF     ' TO MSG-COD-MENS                       
               GO TO ERR-FND-KEY                                        
           ELSE                                                         
               MOVE 1 TO SCR-I                                          
               MOVE SCR-I TO SXF-I.                                     
       CARGA-SXF.                                                       
           MOVE SPACES TO MSG-COD-MENS.                                 
           IF NOT SCR-OFRM-SRV                                          
           PERFORM PUT-TAB-SXF.                                         
           MOVE '00' TO FIO-STAT.                                       
           GO TO FIN-FND-KEY.                                           
      *                                                                 
       ERR-FND-KEY.                                                     
           MOVE SCR-STAT-CON TO SCR-STAT.                               
                                                                        
           MOVE 'BGL'  TO MSG-COD-SIST.                                 
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MEN1.                               
           MOVE '00' TO FIO-STAT.                                       
           GO TO FIN-FND-KEY.                                           
      *>>>>                                                             
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON OR PFS_GET_ACC                                  
           IF NOT (FRM-FFLD = FRM-FFLD-PF6                              
                         OR   FRM-FFLD-PF5)                             
      *>>>>                                                             
*% END
*% IF INI_FIO
      *<<<< INI_FIO                                                     
      *EVA-INI                                                          
           IF SCR-OFRM-SRV                                              
              MOVE TSK-TERM-ALF TO SXF-COD-TERM                         
              MOVE TSK-TERM-ALF TO SXF-COD-TERM-MAS                     
           ELSE                                                         
              MOVE EIBTRMID     TO SXF-COD-TERM                         
              MOVE EIBTRMID     TO SXF-COD-TERM-MAS.                    
      *EVA-FIN                                                          
      *>>>>                                                             
*% END
*% IF EOF
      *<<<<
                                                                        
      *MAE-INI                                                          
       CAR-MOV-SXF SECTION.                                             
       INI-CAR-MOV-SXF.                                                 
      *EVA                                                              
           MOVE ZEROES TO WSS-CON-DTS.                                  
           MOVE ZEROES TO WSS-SIN-DTS.                                  
      *EVA                                                              
           MOVE GSE-NUM-DVSG IN SXF-FLD TO GSE-NUM-DVSG IN GSE.         
           MOVE GSE-NUM-MVSG IN SXF-FLD TO GSE-NUM-MVSG IN GSE.         
           MOVE GSE-NUM-SVSG IN SXF-FLD TO GSE-NUM-SVSG IN GSE.         
           MOVE GSE-NUM-AVSG IN SXF-FLD TO GSE-NUM-AVSG IN GSE.         
           MOVE 'GSE-FEC-FVSG'          TO FIO-AKEY.                    
           MOVE FIO-GET-NLS             TO FIO-CMND.                    
                                                                        
       LUP-CAR-MOV-SXF.                                                 
           PERFORM BGL-FIO-GSE.                                         
           IF NOT FIO-STAT-OKS                                          
      *EVA                                                              
                ADD 1 TO WSS-SIN-DTS                                    
      *EVA                                                              
                GO TO FIN-CAR-MOV-SXF.                                  
           MOVE GSE-NUM-DVSG IN SXF-FLD TO WSS-NUM-DVSG IN CMA-VARI.    
           MOVE GSE-NUM-MVSG IN SXF-FLD TO WSS-NUM-MVSG IN CMA-VARI.    
           MOVE GSE-NUM-SVSG IN SXF-FLD TO WSS-NUM-SVSG IN CMA-VARI.    
           MOVE GSE-NUM-AVSG IN SXF-FLD TO WSS-NUM-AVSG IN CMA-VARI.    
           MOVE FRM-NUM-DFIN IN SXF-FLD TO WSS-NUM-DFIN IN CMA-VARI.    
           MOVE FRM-NUM-MFIN IN SXF-FLD TO WSS-NUM-MFIN IN CMA-VARI.    
           MOVE FRM-NUM-SFIN IN SXF-FLD TO WSS-NUM-SFIN IN CMA-VARI.    
           MOVE FRM-NUM-AFIN IN SXF-FLD TO WSS-NUM-AFIN IN CMA-VARI.    
                                                                        
           IF (GSE-FEC-FVSG IN GSE < WSS-FEC-FVSG IN CMA-VARI)          
                GO TO SIG-CAR-MOV-SXF.                                  
                                                                        
           IF (GSE-FEC-FVSG IN GSE > WSS-FEC-FFIN IN CMA-VARI)          
      *EVA                                                              
                ADD 1 TO WSS-SIN-DTS                                    
      *EVA                                                              
                GO TO FIN-CAR-MOV-SXF.                                  
                                                                        
           MOVE GSE-SGV-SEGU IN GSE TO WSS-SGV-SEGU IN CMA-VARI.        
           IF  (WSS-SGV-SEGU IN CMA-VARI NOT = 1)                       
                GO TO SIG-CAR-MOV-SXF.                                  
                                                                        
      *EXISTE GDG                                                       
           MOVE GSE-CIC-GTIA IN GSE  TO GDG-CIC-GTIA IN GDG.            
           MOVE FIO-GET-KEY          TO FIO-CMND.                       
           PERFORM BGL-FIO-GDG.                                         
           IF (NOT FIO-STAT-OKS )                                       
      *         MOVE 'BGL'            TO MSG-COD-SIST                   
      *         MOVE 'GDGFECINEX'     TO MSG-COD-MENS                   
      *         PERFORM GET-MSG                                         
      *         MOVE MSG-GLS-DESC     TO FRM-MENS                       
      *         MOVE FRM-SUAR-MAL     TO FRM-SUAR                       
                GO TO SIG-CAR-MOV-SXF.                                  
                                                                        
      *EVA                                                              
           IF GDG-IND-EST1 IN GDG > '19' OR                             
              GDG-IND-EST1 IN GDG < '10'                                
                GO TO SIG-CAR-MOV-SXF.                                  
      *EVA                                                              
      *MUEVE-DATOS                                                      
           MOVE GSE-CIC-GTIA IN GSE TO SXF-CIC-GTIA(SXF-I).             
           MOVE GSE-NUM-DVSG IN GSE TO SXF-NUM-DVSG(SXF-I).             
           MOVE GSE-NUM-MVSG IN GSE TO SXF-NUM-MVSG(SXF-I).             
           MOVE GSE-NUM-SVSG IN GSE TO SXF-NUM-SVSG(SXF-I).             
           MOVE GSE-NUM-AVSG IN GSE TO SXF-NUM-AVSG(SXF-I).             
           MOVE GSE-NUM-DVTS IN GSE TO SXF-NUM-DVTS(SXF-I).             
           MOVE GSE-NUM-MVTS IN GSE TO SXF-NUM-MVTS(SXF-I).             
           MOVE GSE-NUM-SVTS IN GSE TO SXF-NUM-SVTS(SXF-I).             
           MOVE GSE-NUM-AVTS IN GSE TO SXF-NUM-AVTS(SXF-I).             
      *EVA-INI                                                          
           IF SCR-OFRM-SRV                                              
              MOVE GSE-SGV-SEGU IN GSE TO SXF-VAL-MPRI-EDT              
              MOVE SXF-VAL-MPRI-EDT TO SXF-VAL-MPRI-ALF(SXF-I)          
           ELSE                                                         
              MOVE GSE-SGV-SEGU IN GSE TO SXF-VAL-MPRI(SXF-I).          
      *EVA-FIN                                                          
                                                                        
       VAL-CAR-MOV-SXF.                                                 
                                                                        
      *LEE-GDD                                                          
           MOVE GSE-CIC-GTIA IN GSE  TO GDD-CIC-GTIA IN GDD.            
           MOVE 'GDD-KEY-AGDD'       TO FIO-AKEY.                       
           MOVE FIO-GET-KEY          TO FIO-CMND.                       
           PERFORM BGL-FIO-GDD.                                         
           IF (NOT FIO-STAT-OKS )                                       
                MOVE 'BGL'            TO MSG-COD-SIST                   
                MOVE 'GDDFECINEX'     TO MSG-COD-MENS                   
                PERFORM GET-MSG                                         
                MOVE MSG-GLS-DESC     TO FRM-MENS                       
                MOVE FRM-SUAR-MAL     TO FRM-SUAR                       
                MOVE SPACES           TO SXF-NUM-ICLI(SXF-I)            
                MOVE SPACES           TO SXF-VRF-ICLI(SXF-I)            
                MOVE SPACES           TO SXF-IND-ICLI(SXF-I)            
                MOVE SPACES           TO SXF-GLS-ICLI(SXF-I)            
                MOVE 'CLIENTE INEX'   TO SXF-GLS-NOMC(SXF-I)            
                GO TO CLIENTE-INEXITENTE.                               
                                                                        
      *LEE-DBC                                                          
           MOVE GDD-CIC-DCLI IN GDD  TO  DBC-KEY-IDBC IN DBC.           
           MOVE FIO-GET-KEY          TO  FIO-CMND.                      
           PERFORM SGC-FIO-DBC.                                         
           IF (NOT FIO-STAT-OKS )                                       
                MOVE 'BGL'            TO MSG-COD-SIST                   
                MOVE 'DBCFECINEX'     TO MSG-COD-MENS                   
                PERFORM GET-MSG                                         
                MOVE MSG-GLS-DESC     TO FRM-MENS                       
                MOVE FRM-SUAR-MAL     TO FRM-SUAR                       
                MOVE SPACES           TO SXF-NUM-ICLI(SXF-I)            
                MOVE SPACES           TO SXF-VRF-ICLI(SXF-I)            
                MOVE SPACES           TO SXF-IND-ICLI(SXF-I)            
                MOVE SPACES           TO SXF-GLS-ICLI(SXF-I)            
                MOVE 'CLIENTE INEX'   TO SXF-GLS-NOMC(SXF-I)            
                GO TO CLIENTE-INEXITENTE.                               
                                                                        
                                                                        
           MOVE DBC-NUM-ICLI IN DBC TO SXF-NUM-ICLI(SXF-I).             
           MOVE DBC-VRF-ICLI IN DBC TO SXF-VRF-ICLI(SXF-I).             
           MOVE DBC-IND-ICLI IN DBC TO SXF-IND-ICLI(SXF-I).             
           MOVE DBC-GLS-ICLI IN DBC TO SXF-GLS-ICLI(SXF-I).             
                                                                        
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.                        
           PERFORM CPT-BLKS.                                            
           MOVE CPT-STRN            TO SXF-GLS-NOMC(SXF-I).             
                                                                        
       CLIENTE-INEXITENTE.                                              
      *LEE-POL                                                          
           MOVE GSE-CIC-GTIA IN GSE  TO POL-CIC-GTIA IN POL.            
           MOVE FIO-GET-KEY          TO FIO-CMND.                       
           PERFORM BGL-FIO-POL.                                         
           IF (NOT FIO-STAT-OKS )                                       
                MOVE 'BGL'            TO MSG-COD-SIST                   
                MOVE 'POLFECINEX'     TO MSG-COD-MENS                   
                PERFORM GET-MSG                                         
                MOVE MSG-GLS-DESC     TO FRM-MENS                       
                MOVE SPACES           TO SXF-COD-SEGU(SXF-I)            
                MOVE '*'              TO SXF-COD-PLZA(SXF-I)            
                MOVE 'PLZA IN'        TO SXF-GLS-SEGU(SXF-I)            
                MOVE 'N' TO SCR-IERR                                    
                GO TO NXT-CAR-MOV-SXF.                                  
                                                                        
           MOVE POL-COD-SEGU IN POL TO SXF-COD-SEGU(SXF-I).             
           MOVE POL-COD-PLZA IN POL TO SXF-COD-PLZA(SXF-I).             
                                                                        
      *LEE-GLOSA SEGURO                                                 
           MOVE 'BGL'                   TO TAB-COD-SIST FIO-SIST.       
           MOVE 'RSE'                   TO TAB-COD-TTAB IN TAB.         
           MOVE POL-COD-SEGU IN POL     TO TAB-COD-CTAB IN TAB.         
           MOVE FIO-GET-KEY    TO FIO-CMND.                             
           PERFORM GNS-FIO-TAB.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE SPACES TO SXF-GLS-SEGU(SXF-I)                       
           ELSE                                                         
               MOVE TAB-GLS-DCOR IN TAB TO                              
                    SXF-GLS-SEGU(SXF-I).                                
       NXT-CAR-MOV-SXF.                                                 
      *EVA                                                              
           ADD 1 TO WSS-CON-DTS.                                        
      *EVA                                                              
           PERFORM INC-REG-TS.                                          
           ADD 1 TO WSS-HMAS.                                           
           ADD 1 TO WSS-EXI-SXF.                                        
       SIG-CAR-MOV-SXF.                                                 
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           MOVE 'GSE-FEC-FVSG'          TO FIO-AKEY.                    
      *    MOVE SPACES TO SXF-VEC-TS(SXF-I)                             
           GO TO LUP-CAR-MOV-SXF.                                       
       FIN-CAR-MOV-SXF.                                                 
           IF SCR-OFRM-SRV                                              
              PERFORM PUT-SXF-MAS.                                      
           EXIT.                                                        
                                                                        
       PUT-SXF-MAS SECTION.                                             
       INI-PUT-SXF-MAS.                                                 
           MOVE 1                  TO QUE-NITM.                         
           MOVE SXF-LEN-TSSXF-MAS  TO QUE-LITM.                         
           MOVE SXF-NOM-TSSXF-MAS  TO QUE-COLA.                         
           IF WSS-HMAS > SXF-IMAX-MTX                                   
              MOVE 'S'             TO SXF-MAS-IND-HMAS                  
           ELSE                                                         
              MOVE 'N'             TO SXF-MAS-IND-HMAS.                 
           MOVE WSS-EXI-SXF        TO SXF-MAS-NUM-TOCC.                 
           MOVE SXF-MAS-ITEM       TO QUE-ITEM.                         
           MOVE QUE-PUT            TO QUE-CMND.                         
           PERFORM GNS-PRO-QUE.                                         
       FIN-PUT-SXF-MAS.                                                 
           EXIT.                                                        
                                                                        
       OBT-DAT-TS SECTION.                                              
       INI-OBT-DAT-TS.                                                  
           EXEC CICS HANDLE CONDITION QIDERR(CON-OBT-DAT-TS)            
                                     ITEMERR(CON-OBT-DAT-TS)            
           END-EXEC.                                                    
                                                                        
           MOVE SXF-NOM-TSSXF     TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
      *>>>>                                                             
       CON-OBT-DAT-TS.                                                  
           MOVE SXF-SI-IMP TO SXF-IMP.                                  
           PERFORM CLR-VEC-TS VARYING SXF-I FROM 1 BY 1 UNTIL           
                   SXF-I > SXF-IMAX.                                    
           MOVE 1 TO SXF-I.                                             
           PERFORM CAR-MOV-SXF.                                         
                                                                        
       NXT-OBT-DAT-TS.                                                  
           IF SXF-IMP = SXF-SI-IMP                                      
               PERFORM WRT-TS.                                          
       FIN-OBT-DAT-TS.                                                  
           EXIT.                                                        
                                                                        
       PUT-TAB-SXF SECTION.                                             
       INI-PUT-TAB-SXF.                                                 
           PERFORM RED-TS.                                              
           PERFORM LLE-DAT-PNT VARYING SXF-I FROM 1 BY 1                
                   UNTIL SXF-I > SXF-IMAX.                              
       FIN-PUT-TAB-SXF.                                                 
           EXIT.                                                        
                                                                        
       LLE-DAT-PNT SECTION.                                             
       INI-LLE-DAT-PNT.                                                 
           MOVE SXF-CIC-GTIA(SXF-I) TO FRM-CIC-GTIA IN SXF-FLD(SXF-I).  
           MOVE SXF-FEC-FVSG(SXF-I) TO FRM-FEC-FVSG IN SXF-FLD(SXF-I).  
           MOVE SXF-FEC-FVTS(SXF-I) TO FRM-FEC-FVTS IN SXF-FLD(SXF-I).  
           MOVE SXF-VAL-MPRI(SXF-I) TO FRM-VAL-MPRI IN SXF-FLD(SXF-I).  
           MOVE SXF-COD-SEGU(SXF-I) TO FRM-COD-SEGU IN SXF-FLD(SXF-I).  
           MOVE SXF-GLS-SEGU(SXF-I) TO FRM-GLS-SEGU IN SXF-FLD(SXF-I).  
           MOVE SXF-COD-PLZA(SXF-I) TO FRM-COD-PLZA IN SXF-FLD(SXF-I).  
           MOVE SXF-NUM-ICLI(SXF-I) TO FRM-NUM-ICLI IN SXF-FLD(SXF-I).  
           MOVE SXF-VRF-ICLI(SXF-I) TO FRM-VRF-ICLI IN SXF-FLD(SXF-I).  
           MOVE SXF-IND-ICLI(SXF-I) TO FRM-IND-ICLI IN SXF-FLD(SXF-I).  
           MOVE SXF-GLS-ICLI(SXF-I) TO FRM-GLS-ICLI IN SXF-FLD(SXF-I).  
           MOVE SXF-GLS-NOMC(SXF-I) TO FRM-GLS-NOMC IN SXF-FLD(SXF-I).  
       FIN-LLE-DAT-PNT.                                                 
           EXIT.                                                        
                                                                        
                                                                        
       INC-REG-TS SECTION.                                              
       INI-INC-REG-TS.                                                  
           MOVE SXF-SI-IMP TO SXF-IMP.                                  
           ADD 1 TO SXF-I.                                              
           IF SXF-I > SXF-IMAX                                          
               PERFORM WRT-TS                                           
               PERFORM CLR-VEC-TS VARYING SXF-I FROM 1 BY 1             
                                    UNTIL SXF-I > SXF-IMAX              
               MOVE 1 TO SXF-I                                          
               MOVE SXF-NO-IMP TO SXF-IMP.                              
       FIN-INC-REG-TS.                                                  
           EXIT.                                                        
                                                                        
       WRT-TS SECTION.                                                  
       INI-WRT-TS.                                                      
           ADD 1 TO SCR-IMAX.                                           
           MOVE SXF-NOM-TSSXF  TO QUE-COLA.                             
           MOVE SXF-TS         TO QUE-ITEM.                             
           MOVE SXF-LEN-TSSXF  TO QUE-LITM.                             
           MOVE SXF-I          TO QUE-NITM.                             
           MOVE QUE-PUT        TO QUE-CMND.                             
           PERFORM GNS-PRO-QUE.                                         
       FIN-WRT-TS.                                                      
           EXIT.                                                        
                                                                        
                                                                        
       CLR-VEC-TS SECTION.                                              
       INI-CLR-VEC-TS.                                                  
           MOVE SPACES              TO SXF-CIC-GTIA(SXF-I).             
           MOVE ZEROES              TO SXF-FEC-FVSG(SXF-I).             
           MOVE ZEROES              TO SXF-FEC-FVTS(SXF-I).             
           MOVE ZEROES              TO SXF-VAL-MPRI(SXF-I).             
           MOVE SPACES              TO SXF-COD-SEGU(SXF-I).             
           MOVE SPACES              TO SXF-GLS-SEGU(SXF-I).             
           MOVE SPACES              TO SXF-COD-PLZA(SXF-I).             
           MOVE SPACES              TO SXF-NUM-ICLI(SXF-I).             
           MOVE SPACES              TO SXF-VRF-ICLI(SXF-I).             
           MOVE SPACES              TO SXF-IND-ICLI(SXF-I).             
           MOVE SPACES              TO SXF-GLS-ICLI(SXF-I).             
           MOVE SPACES              TO SXF-GLS-NOMC(SXF-I).             
       FIN-CLR-VEC-TS.                                                  
           EXIT.                                                        
                                                                        
                                                                        
       RED-TS SECTION.                                                  
       INI-RED-TS.                                                      
           MOVE SXF-I TO SCR-I.                                         
           EXEC CICS HANDLE CONDITION QIDERR(FIN-RED-TS)                
                                     ITEMERR(FIN-RED-TS)                
           END-EXEC.                                                    
           MOVE SXF-NOM-TSSXF   TO QUE-COLA.                            
           MOVE SXF-TS          TO QUE-ITEM.                            
           MOVE SXF-LEN-TSSXF   TO QUE-LITM.                            
           MOVE SXF-I           TO QUE-NITM.                            
           MOVE QUE-GET         TO QUE-CMND.                            
           PERFORM GNS-PRO-QUE.                                         
           MOVE QUE-ITEM       TO SXF-TS.                               
       FIN-RED-TS.                                                      
           EXIT.                                                        
                                                                        
       RST-STAT SECTION.                                                
       INI-RST-STAT.                                                    
           MOVE SCR-STPP     TO SCR-STAT.                               
           MOVE SCR-MFLD-NOT TO SCR-MFLD.                               
       FIN-RST-STAT.                                                    
           EXIT.                                                        
                                                                        
       COPY GNSBGCPT.                                                   
       COPY SGCBFDBC.                                                   
       COPY BGLBFPOL.                                                   
       COPY BGLBFGDD.                                                   
       COPY BGLBFGDG.                                                   
       COPY GNSBBTAB.                                                   
      *MAE-FIN                                                          
                                                                        
      *>>>>
*% END
