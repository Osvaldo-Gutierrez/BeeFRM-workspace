*%! Codigo adicional
*% NO_TIENE_TIMESTAMP = TRUE
*% NO_HDR_CST = TRUE
*% LARGO_TOTAL_COMMAREA = 3000
*% FMT_CHI = TRUE
*% GEN_BCI = TRUE
*% PGM_PTC = TRUE
*% IF SETTAG
*% VSNPQ = "06.05.19"
*% FNCPQ = "GARANTIAS"
*% END
*% IF INI_SCR_ACT
      *<<<<
           GO TO CNT-SCR-ACT.
      *>>>>
*% END
*% IF FIN_SCR_ACT
      *<<<<
       CNT-SCR-ACT.
           IF SCR-INST = SCR-INST-HKY                                   
               PERFORM HBL-KEY                                          
               MOVE SCR-INST-GKY TO SCR-INST                            
               PERFORM SCR-DMSG                                         
           ELSE                                                         
           IF SCR-INST = SCR-INST-GKY                                   
                PERFORM SCR-RST                                         
                PERFORM GET-KEY                                         
                IF NOT FRM-STAT-ABT                                     
                        MOVE SPACES TO FRM-MENS                         
                        PERFORM FND-KEY                                 
                        IF FIO-STAT-OKS                                 
                            MOVE SCR-ACTU-MOD TO SCR-ACTU               
                            PERFORM PUT-SCR                             
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR           
                            PERFORM SCR-TRL                             
                        ELSE                                            
                        MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR             
                        IF FIO-STAT-EOF                                 
      *CAF-INI                                                          
                           MOVE 'S' TO SCR-IERR                         
      *CAF-FIN                                                          
                            IF FRM-MENS NOT > SPACES                    
                                MOVE 'REGEOF    ' TO MSG-COD-MENS       
                                MOVE 'GNS' TO MSG-COD-SIST              
                                PERFORM GET-MSG                         
                                MOVE MSG-GLS-DESC TO FRM-MENS           
                                PERFORM SCR-MSSG                        
                                MOVE SCR-STAT-ABT TO SCR-STAT           
                                PERFORM DESSCRTRL                       
                                GO TO FIN-SCR-ACT                       
                            ELSE                                        
                                PERFORM SCR-MSSG                        
                                MOVE SCR-STAT-ABT TO SCR-STAT           
                                PERFORM DESSCRTRL                       
                                GO TO FIN-SCR-ACT                       
                        ELSE                                            
                        IF FIO-STAT = FIO-STAT-NEX                      
      *CAF-INI                                                          
                           MOVE 'S' TO SCR-IERR                         
      *CAF-FIN                                                          
                            IF FRM-MENS NOT > SPACES                    
                                MOVE 'REGNEX    ' TO MSG-COD-MENS       
                                MOVE 'GNS' TO MSG-COD-SIST              
                                PERFORM GET-MSG                         
                                MOVE MSG-GLS-DESC TO FRM-MENS           
                                PERFORM SCR-MSSG                        
                                MOVE SCR-STAT-ABT TO SCR-STAT           
                                PERFORM DESSCRTRL                       
                                GO TO FIN-SCR-ACT                       
                            ELSE                                        
                                PERFORM SCR-MSSG                        
                                MOVE SCR-STAT-ABT TO SCR-STAT           
                                PERFORM DESSCRTRL                       
                                GO TO FIN-SCR-ACT                       
                        ELSE                                            
                            MOVE SCR-ACTU-ING TO SCR-ACTU               
      *JSS                  PERFORM CLR-FLD-QDO                         
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR           
                            PERFORM SCR-TRL                             
                ELSE                                                    
                        MOVE SPACES TO FRM-MENS                         
                        PERFORM SCR-MSSG                                
                        PERFORM DESSCRTRL                               
                        GO TO FIN-SCR-ACT                               
           ELSE                                                         
           IF SCR-INST = SCR-INST-GAB                                   
                PERFORM GET-CON                                         
           ELSE                                                         
           IF SCR-INST = SCR-INST-BRW                                   
               MOVE SCR-FFLD TO FRM-FFLD                                
               PERFORM BRW-SCR-ACT.                                     
           PERFORM DESSCRTRL.                                           
           MOVE SPACES TO FRM-MENS.                                     
           PERFORM SCR-MSSG.                                            
       FIN-SCR-ACT.                                                     
      *>>>>
*% END
*% IF INI_SCR_PGM
      *<<<<
           GO TO CNT-SCR-PGM.
      *>>>>
*%END
*% IF FIN_SCR_PGM
      *<<<<
       CNT-SCR-PGM.
           IF SCR-INST = SCR-INST-HKY                                   
               PERFORM HBL-KEY                                          
               MOVE SCR-INST-GKY TO SCR-INST                            
               PERFORM SCR-DMSG                                         
           ELSE                                                         
           IF SCR-INST = SCR-INST-GKY                                   
                PERFORM SCR-RST                                         
                PERFORM GET-KEY                                         
                IF FRM-STAT-ABT                                         
                    MOVE SPACES TO FRM-MENS                             
                    PERFORM SCR-MSSG                                    
                    PERFORM DESSCRTRL                                   
                    GO TO FIN-SCR-PGM                                   
                ELSE                                                    
                    MOVE SPACES TO FRM-MENS                             
                    PERFORM FND-KEY                                     
                    IF FIO-STAT = FIO-STAT-NEX                          
      *CAF-INI                                                          
                       MOVE 'S' TO SCR-IERR                             
      *CAF-FIN                                                          
                        IF FRM-MENS NOT > SPACES                        
                            MOVE 'REGNEX    ' TO MSG-COD-MENS           
                            MOVE 'GNS' TO MSG-COD-SIST                  
                            PERFORM GET-MSG                             
                            MOVE MSG-GLS-DESC TO FRM-MENS               
                            PERFORM SCR-MSSG                            
                            MOVE SCR-STAT-ABT TO SCR-STAT               
                            PERFORM DESSCRTRL                           
                            GO TO FIN-SCR-PGM                           
                        ELSE                                            
                            PERFORM SCR-MSSG                            
                            MOVE SCR-STAT-ABT TO SCR-STAT               
                            PERFORM DESSCRTRL                           
                            GO TO FIN-SCR-PGM                           
                    ELSE                                                
                        MOVE PGM-LIMPIAR-TODO TO PGM-ICLR               
                        PERFORM SCR-TRL                                 
           ELSE                                                         
           IF SCR-INST = SCR-INST-GAB                                   
                PERFORM GET-CON                                         
           ELSE                                                         
           IF SCR-INST = SCR-INST-BRW                                   
               MOVE SCR-FFLD TO FRM-FFLD                                
               PERFORM BRW-SCR-PGM.                                     
       TRL-SCR-PGM.                                                     
           PERFORM DESSCRTRL.                                           
           MOVE SPACES TO FRM-MENS.                                     
           PERFORM SCR-MSSG.                                            
       FIN-SCR-PGM.                                                     
      *>>>>
*% END
*% IF WSS
      *<<<< WSS
       COPY BGLWXQDO.
       COPY GNSWGCPT.
       COPY GNSWGSTR.
*% IF NOT PGM_MCP AND NOT PGM_PTC
       COPY GNSWGQUE.
*% END
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
       01 WSS-NUM-CCUR.
           03 WSS-NUM-ICUR-EDT  PIC  ZZ9.
           03 WSS-NUM-ICUR-ALF       PIC X(03).
           03 WSS-NUM-ICUR      REDEFINES
              WSS-NUM-ICUR-ALF       PIC 9(03).

       COPY BGLBRGDG.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GDG-REQA==.
      *>>>>
*% END
*% IF FIN_SCR_VARI
           03 SCR-NUM-PANT               PIC 9(0003).
           03 SCR-QDO-FLD                PIC X(1100).
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-OFRM-SRV
                MOVE TSK-TERM-ALF TO QDO-COD-TERM
                MOVE TSK-TERM-ALF TO QDO-COD-TERM-MAS
           ELSE
                MOVE EIBTRMID     TO QDO-COD-TERM
                MOVE EIBTRMID     TO QDO-COD-TERM-MAS.
           MOVE +3859 TO SCR-TCMA.
           PERFORM GNS-ERR-QUE.
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )
               MOVE SCR-QDO-FLD TO QDO-FLD
               GO TO FIN-INI.

           IF ( FRM-FFLD = FRM-FFLD-PF6 )
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM
               ADD 1              TO QUE-NITM
               MOVE QDO-NOM-TSQDO TO QUE-COLA
               MOVE QDO-LEN-TSQDO TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT NOT = QUE-STAT-OKS
                  MOVE SCR-QDO-FLD TO QDO-FLD
                  GO TO FIN-INI.
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
           MOVE GDO-CAI-GTIA IN QDO-FLD TO GDG-CAI-GTIA IN GDG.
           MOVE GDO-IIC-GTIA IN QDO-FLD TO GDG-IIC-GTIA IN GDG.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM BGL-FIO-GDG.
           MOVE FIO-STAT TO PGM-STAT-GDG.                               
           IF NOT FIO-STAT-OKS
               MOVE 'GDG    NEX' TO MSG-COD-MENS
               MOVE 'S' TO SCR-IERR                                     
               GO TO MSG-FND-KEY.

           PERFORM KEY-ALL-QDO.
           IF FRM-IND-SGTE IN QDO-FLD = 'S'
               MOVE GDO-CAI-GTIA IN QDO-FLD TO GDO-CAI-GTIA IN GDO
               MOVE GDO-IIC-GTIA IN QDO-FLD TO GDO-IIC-GTIA IN GDO
               MOVE FRM-NUM-IGDS IN QDO-FLD TO GDO-NUM-IGDO IN GDO.

           MOVE SPACES TO GDO-BKP.                                      
           MOVE FIO-GET-NLS TO FIO-CMND.
       LUP-LEE-GDO.
           PERFORM BGL-FIO-GDO.
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF
               GO TO CON-FND-KEY.

           IF NOT (GDO-CAI-GTIA IN QDO-FLD = GDO-CAI-GTIA IN GDO AND
                   GDO-IIC-GTIA IN QDO-FLD = GDO-IIC-GTIA IN GDO)
               IF GDO-BKP > SPACES                                      
                   MOVE GDO-BKP TO GDO                                  
                   GO TO CON-FND-KEY                                    
               ELSE                                                     
                   GO TO CON-FND-KEY.
 
           MOVE GDO TO GDO-BKP.                                         
           MOVE FIO-STAT TO PGM-STAT-GDO.                               
           PERFORM CRG-VEC-QDO.

           IF (SCR-OFRM-SRV AND WSS-J > WSS-NITMMAX)
              GO TO CON-FND-KEY.

           IF ((NOT SCR-OFRM-SRV) AND WSS-J > WSS-JMAX)
                 GO TO CON-FND-KEY.
 
       NXT-LEE-GDO.
           MOVE FIO-GET-NXT TO FIO-CMND.
           GO TO LUP-LEE-GDO.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
       CON-FND-KEY.
           PERFORM PUT-TAB.

           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *----------------------------------------------------------
       MSG-FND-KEY.
           MOVE 'TAB' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM VAL-NUM-KEY-QDO
               PERFORM VAL-NUM-FLD-QDO
               PERFORM SCRL-TAB-QDO
               PERFORM SCR-TRL.
      *>>>>
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBVTAB.
       COPY GNSBVCOD.
       COPY GNSBGCPT.
       COPY GNSBGSTR.
       COPY BGLBFGDG.

       PUT-TAB SECTION.
       INI-PUT-TAB.
           MOVE QDO-NOM-TSQDO     TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE QDO-NOM-TSQDO-MAS TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE 1 TO SCR-NUM-PANT.
           MOVE LOW-VALUES TO WSS-GLS-NQDO-IDX-MTI.
           MOVE LOW-VALUES TO WSS-GLS-NQDO-IDX-FRM.
      *
           MOVE 1 TO FRM-IFLD.
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.

           MOVE 1 TO WSS-F IN WSS-VARI.
       LUP-PUT-TAB.
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI
              GO TO FIN-PUT-TAB.
           IF WSS-NUM-IGDO-V(WSS-F) > ZEROES
              PERFORM PUT-TAB-QDO
              PERFORM PUT-QDO-QUE
              PERFORM SET-FRM-IFLD
           ELSE
           IF FRM-IFLD = 1 AND WSS-F = 1
              MOVE 'TAB' TO MSG-COD-SIST
              MOVE 'QDO    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE '00' TO FIO-STAT
              MOVE 'S' TO SCR-IERR
              GO TO FIN-PUT-TAB
           ELSE
              MOVE '00' TO FIO-STAT
              GO TO FIN-PUT-TAB.

           ADD 1 TO WSS-F IN WSS-VARI.
           GO TO LUP-PUT-TAB.
       FIN-PUT-TAB.
           MOVE 1                  TO QUE-NITM.
           MOVE QDO-LEN-TSQDO-MAS  TO QUE-LITM.
           MOVE QDO-NOM-TSQDO-MAS  TO QUE-COLA.

           IF WSS-NITM > WSS-NITMMAX
              MOVE 'S'           TO QDO-MAS-IND-HMAS
           ELSE
              MOVE 'N'           TO QDO-MAS-IND-HMAS.

           MOVE WSS-NITM         TO WSS-NUM-ICUR.
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.
           MOVE WSS-NUM-ICUR-ALF TO QDO-MAS-NUM-TOCC.
           MOVE QDO-MAS-ITEM     TO QUE-ITEM.
           MOVE QUE-PUT          TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF WSS-NITM > WSS-IMAX
               MOVE 'TAB' TO MSG-COD-SIST
               MOVE 'QDOTABDMAS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       EXT-PUT-TAB.
           EXIT.

       SET-FRM-IFLD SECTION.
       INI-SET-FRM-IFLD.
           IF SCR-OFRM-SRV
                MOVE 1 TO FRM-IFLD
           ELSE
                ADD 1 TO FRM-IFLD.
       FIN-SET-FRM-IFLD.
           EXIT.
      *--------------------------------------------
       SCRL-TAB-QDO SECTION.
       INI-SCRL-TAB-QDO.
           MOVE SPACES TO MSG-COD-MENS.

           IF FRM-FFLD = FRM-FFLD-PF6
               PERFORM PUT-TAB-ASC
           ELSE
               PERFORM PUT-TAB-DSC.
           IF MSG-COD-MENS > SPACES
               MOVE 'TAB' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-QDO.
           EXIT.

      *--------------------------------------------
       PUT-TAB-QDO SECTION.
       INI-PUT-TAB-QDO.
           PERFORM CLR-WSS.
           MOVE WSS-NUM-IGDO-V(WSS-F) TO WSS-NUM-IGDO-QDO IN WSS-VARI.
           MOVE WSS-CAI-DOCU-V(WSS-F) TO WSS-CAI-DOCU-QDO IN WSS-VARI.
           MOVE WSS-IIC-DOCU-V(WSS-F) TO WSS-IIC-DOCU-QDO IN WSS-VARI.
           MOVE WSS-COD-TDOC-V(WSS-F) TO WSS-COD-TDOC-QDO IN WSS-VARI.
           MOVE WSS-GLS-TDOC-V(WSS-F) TO WSS-GLS-TDOC-QDO IN WSS-VARI.
           MOVE WSS-FEC-FCAP-V(WSS-F) TO WSS-FEC-FCAP-QDO IN WSS-VARI.
           MOVE WSS-COD-TIEM-V(WSS-F) TO WSS-COD-TIEM-QDO IN WSS-VARI.
           MOVE WSS-COD-INEM-V(WSS-F) TO WSS-COD-INEM-QDO IN WSS-VARI.
           MOVE WSS-COD-SIEM-V(WSS-F) TO WSS-COD-SIEM-QDO IN WSS-VARI.
           MOVE WSS-GLS-INEM-V(WSS-F) TO WSS-GLS-INEM-QDO IN WSS-VARI.

           MOVE WSS-NUM-IGDO-QDO IN WSS-VARI TO WSS-NUM-IGDO-FRM.
           MOVE WSS-CAI-DOCU-QDO IN WSS-VARI TO WSS-CAI-DOCU-FRM.
           MOVE WSS-IIC-DOCU-QDO IN WSS-VARI TO WSS-IIC-DOCU-FRM.
           MOVE WSS-COD-TDOC-QDO IN WSS-VARI TO WSS-COD-TDOC-FRM.
           MOVE WSS-GLS-TDOC-QDO IN WSS-VARI TO WSS-GLS-TDOC-FRM.
           MOVE WSS-FEC-FCAP-QDO IN WSS-VARI TO WSS-FEC-FCAP-FRM.
           MOVE WSS-COD-TIEM-QDO IN WSS-VARI TO WSS-COD-TIEM-FRM.
           MOVE WSS-COD-INEM-QDO IN WSS-VARI TO WSS-COD-INEM-FRM.
           MOVE WSS-COD-SIEM-QDO IN WSS-VARI TO WSS-COD-SIEM-FRM.
           MOVE WSS-GLS-INEM-QDO IN WSS-VARI TO WSS-GLS-INEM-FRM.

           IF FRM-IFLD > WSS-IMAX
              GO TO FIN-PUT-TAB-QDO.

           MOVE WSS-NUM-IGDO-QDO IN WSS-VARI TO
               FRM-NUM-IGDO IN QDO-FLD(FRM-IFLD).
           MOVE WSS-CAI-DOCU-QDO IN WSS-VARI TO
               FRM-CAI-DOCU IN QDO-FLD(FRM-IFLD).
           MOVE WSS-IIC-DOCU-QDO IN WSS-VARI TO
               FRM-IIC-DOCU IN QDO-FLD(FRM-IFLD).
           MOVE WSS-COD-TDOC-QDO IN WSS-VARI TO
               FRM-COD-TDOC IN QDO-FLD(FRM-IFLD).
           MOVE WSS-GLS-TDOC-QDO IN WSS-VARI TO
               FRM-GLS-TDOC IN QDO-FLD(FRM-IFLD).
           MOVE WSS-FEC-FCAP-QDO IN WSS-VARI TO
               FRM-FEC-FCAP IN QDO-FLD(FRM-IFLD).
           MOVE WSS-COD-TIEM-QDO IN WSS-VARI TO
               FRM-COD-TIEM IN QDO-FLD(FRM-IFLD).
           MOVE WSS-COD-INEM-QDO IN WSS-VARI TO
               FRM-COD-INEM IN QDO-FLD(FRM-IFLD).
           MOVE WSS-COD-SIEM-QDO IN WSS-VARI TO
               FRM-COD-SIEM IN QDO-FLD(FRM-IFLD).
           MOVE WSS-GLS-INEM-QDO IN WSS-VARI TO
               FRM-GLS-INEM IN QDO-FLD(FRM-IFLD).
       FIN-PUT-TAB-QDO.
           EXIT.
      *----------------------------------------------------
       PUT-QDO-QUE SECTION.
       INI-PUT-QDO-QUE.
           PERFORM PRO-FLD-QUE.
           ADD 1 TO WSS-NITM.
           MOVE QDO-NOM-TSQDO    TO QUE-COLA.
           MOVE QDO-LEN-TSQDO    TO QUE-LITM.
           MOVE WSS-NITM         TO QUE-NITM.
           MOVE SPACES           TO QUE-ITEM.
           IF SCR-OFRM-SRV
              MOVE WSS-GLS-NQDO-IDX-MTI  TO QUE-ITEM
           ELSE
              MOVE WSS-GLS-NQDO-IDX-FRM  TO QUE-ITEM.
           MOVE QUE-PUT  TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
       FIN-PUT-QDO-QUE.
           EXIT.
      *----------------------------------------------------

       PUT-TAB-ASC SECTION.
       INI-PUT-TAB-ASC.
           MOVE SPACES TO MSG-COD-MENS.
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.
           MOVE 0 TO FRM-IFLD.
       LUP-PUT-TAB-ASC.
           ADD 1              TO QUE-NITM.
           MOVE QDO-NOM-TSQDO TO QUE-COLA.
           MOVE QDO-LEN-TSQDO TO QUE-LITM.
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD = 0
                   MOVE 'QDOTABNONXT' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-ASC
               ELSE
                   GO TO FIN-PUT-TAB-ASC.
           ADD 1 TO FRM-IFLD.
           MOVE QUE-ITEM TO FRM-NUM-IGDO-IDX(FRM-IFLD).
           IF FRM-IFLD < WSS-IMAX
               GO TO LUP-PUT-TAB-ASC.
       FIN-PUT-TAB-ASC.
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX
               ADD 1 FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1
                                 UNTIL WSS-I > WSS-IMAX.
           IF FRM-IFLD > 0
               ADD 1 TO SCR-NUM-PANT.
           IF FRM-IFLD = WSS-IMAX
               ADD 1              TO QUE-NITM
               MOVE QDO-NOM-TSQDO TO QUE-COLA
               MOVE QDO-LEN-TSQDO TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT = QUE-STAT-OKS
                   MOVE 'TAB'        TO MSG-COD-SIST
                   MOVE 'QDOTABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-ASC.
           EXIT.

       PUT-TAB-DSC SECTION.
       INI-PUT-TAB-DSC.
           MOVE SPACES TO MSG-COD-MENS.
           IF SCR-NUM-PANT = 1
               MOVE 'QDOTABNOPRV' TO MSG-COD-MENS
               GO TO EXT-PUT-TAB-DSC.

           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.
           ADD 1 TO QUE-NITM.
           ADD 1 WSS-IMAX GIVING FRM-IFLD.
       LUP-PUT-TAB-DSC.
           SUBTRACT  1 FROM QUE-NITM.
           MOVE QDO-NOM-TSQDO TO QUE-COLA
           MOVE QDO-LEN-TSQDO TO QUE-LITM
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD > WSS-IMAX
                   MOVE 'QDOTABNOPRV' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-DSC
               ELSE
                   GO TO FIN-PUT-TAB-DSC.
           SUBTRACT 1 FROM FRM-IFLD.
           MOVE QUE-ITEM TO FRM-NUM-IGDO-IDX(FRM-IFLD).
           IF FRM-IFLD > 1
               GO TO LUP-PUT-TAB-DSC.
       FIN-PUT-TAB-DSC.

           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1
                                 UNTIL WSS-I < 1.

           IF FRM-IFLD NOT > WSS-IMAX
               SUBTRACT 1 FROM SCR-NUM-PANT
               MOVE 'TAB'        TO MSG-COD-SIST
               MOVE 'QDOTABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-DSC.
           EXIT.

       CLR-FRM SECTION.
       INI-CLR-FRM.
           MOVE ZEROES TO FRM-NUM-IGDO IN QDO-FLD(WSS-I).
           MOVE SPACES TO FRM-CAI-DOCU IN QDO-FLD(WSS-I).
           MOVE SPACES TO FRM-IIC-DOCU IN QDO-FLD(WSS-I).
           MOVE SPACES TO FRM-COD-TDOC IN QDO-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-TDOC IN QDO-FLD(WSS-I).
           MOVE SPACES TO FRM-FEC-FCAP IN QDO-FLD(WSS-I).
           MOVE SPACES TO FRM-COD-TIEM IN QDO-FLD(WSS-I).
           MOVE SPACES TO FRM-COD-INEM IN QDO-FLD(WSS-I).
           MOVE SPACES TO FRM-COD-SIEM IN QDO-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-INEM IN QDO-FLD(WSS-I).
       FIN-CLR-FRM.
           EXIT.

       CLR-WSS SECTION.
       INI-CLR-WSS.
           MOVE ZEROES TO WSS-NUM-IGDO-QDO IN WSS-VARI.
           MOVE SPACES TO WSS-CAI-DOCU-QDO IN WSS-VARI.
           MOVE SPACES TO WSS-IIC-DOCU-QDO IN WSS-VARI.
           MOVE SPACES TO WSS-COD-TDOC-QDO IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-TDOC-QDO IN WSS-VARI.
           MOVE SPACES TO WSS-FEC-FCAP-QDO IN WSS-VARI.
           MOVE SPACES TO WSS-COD-TIEM-QDO IN WSS-VARI.
           MOVE SPACES TO WSS-COD-INEM-QDO IN WSS-VARI.
           MOVE SPACES TO WSS-COD-SIEM-QDO IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-INEM-QDO IN WSS-VARI.
           IF SCR-OFRM-SRV
               PERFORM CLR-FRM-MTX.
       FIN-CLR-WSS.
           EXIT.

       CLR-FRM-MTX SECTION.
       INI-CLR-FRM-MTX.
           MOVE ZEROES TO FRM-NUM-IGDO IN QDO-FLD(1).
           MOVE SPACES TO FRM-CAI-DOCU IN QDO-FLD(1).
           MOVE SPACES TO FRM-IIC-DOCU IN QDO-FLD(1).
           MOVE SPACES TO FRM-COD-TDOC IN QDO-FLD(1).
           MOVE SPACES TO FRM-GLS-TDOC IN QDO-FLD(1).
           MOVE SPACES TO FRM-FEC-FCAP IN QDO-FLD(1).
           MOVE SPACES TO FRM-COD-TIEM IN QDO-FLD(1).
           MOVE SPACES TO FRM-COD-INEM IN QDO-FLD(1).
           MOVE SPACES TO FRM-COD-SIEM IN QDO-FLD(1).
           MOVE SPACES TO FRM-GLS-INEM IN QDO-FLD(1).
       FIN-CLR-FRM-MTX.
           EXIT.

       CRG-VEC-QDO SECTION.
       INI-CRG-VEC-QDO.
           ADD 1 TO WSS-J.
           IF WSS-J IN WSS-VARI > WSS-JMAX IN WSS-VARI
               GO TO FIN-CRG-VEC-QDO.

           PERFORM CLR-QDO-VEC.
           MOVE GDO-NUM-IGDO IN GDO TO 
                                    WSS-NUM-IGDO-V IN WSS-VARI(WSS-J).
           MOVE GDO-COD-DOCU IN GDO TO WSS-COD-DOCU IN WSS-VARI.        
           MOVE WSS-CAI-DOCU IN WSS-VARI TO                             
                                    WSS-CAI-DOCU-V IN WSS-VARI(WSS-J).  
           MOVE WSS-IIC-DOCU IN WSS-VARI TO                             
                                    WSS-IIC-DOCU-V IN WSS-VARI(WSS-J).  
           MOVE GDO-COD-TDOC IN GDO TO 
                                    WSS-COD-TDOC-V IN WSS-VARI(WSS-J).
           MOVE 'TAB'               TO TAB-COD-SIST.
           MOVE 'TIO'               TO TAB-COD-TTAB IN TAB.
           MOVE GDO-COD-TDOC IN GDO TO TAB-COD-CTAB IN TAB.
           PERFORM BUS-TAB.
           MOVE TAB-GLS-DESC TO 
                                    WSS-GLS-TDOC-V IN WSS-VARI(WSS-J).
           MOVE GDO-NUM-DCAP IN GDO TO WSS-NUM-DTRA IN WSS-VARI.
           MOVE GDO-NUM-MCAP IN GDO TO WSS-NUM-MTRA IN WSS-VARI.
           MOVE GDO-NUM-SCAP IN GDO TO WSS-NUM-STRA IN WSS-VARI.
           MOVE GDO-NUM-ACAP IN GDO TO WSS-NUM-ATRA IN WSS-VARI.
           MOVE WSS-FEC-FTRA IN WSS-VARI TO 
                                    WSS-FEC-FCAP-V IN WSS-VARI(WSS-J).
           MOVE GDO-COD-TIEM IN GDO TO 
                                    WSS-COD-TIEM-V IN WSS-VARI(WSS-J).
           MOVE GDO-COD-INEM IN GDO TO 
                                    WSS-COD-INEM-V IN WSS-VARI(WSS-J).
           MOVE GDO-COD-SIEM IN GDO TO 
                                    WSS-COD-SIEM-V IN WSS-VARI(WSS-J).
           MOVE 'TAB'               TO TAB-COD-SIST.
           MOVE GDO-COD-TIEM IN GDO TO TAB-COD-TTAB IN TAB.
           MOVE SPACES              TO TAB-COD-CTAB IN TAB.
           MOVE GDO-COD-INEM IN GDO TO WSS-COD-CTA1 IN WSS-VARI.
           MOVE GDO-COD-SIEM IN GDO TO WSS-COD-CTA2 IN WSS-VARI.
           MOVE WSS-COD-CTAB        TO TAB-COD-CTAB IN TAB.
           PERFORM BUS-TAB.
           IF MSG-COD-MENS = SPACES
               MOVE TAB-GLS-DESC TO 
                                    WSS-GLS-INEM-V IN WSS-VARI(WSS-J)
           ELSE
               MOVE SPACES TO WSS-GLS-INEM-V IN WSS-VARI(WSS-J).
       FIN-CRG-VEC-QDO.
           EXIT.

       CLR-QDO-VEC SECTION.
       INI-CLR-QDO-VEC.
           MOVE ZEROES TO WSS-NUM-IGDO-V(WSS-I).
           MOVE SPACES TO WSS-CAI-DOCU-V(WSS-I).
           MOVE SPACES TO WSS-IIC-DOCU-V(WSS-I).
           MOVE SPACES TO WSS-COD-TDOC-V(WSS-I).
           MOVE SPACES TO WSS-GLS-TDOC-V(WSS-I).
           MOVE SPACES TO WSS-FEC-FCAP-V(WSS-I).
           MOVE SPACES TO WSS-COD-TIEM-V(WSS-I).
           MOVE SPACES TO WSS-COD-INEM-V(WSS-I).
           MOVE SPACES TO WSS-COD-SIEM-V(WSS-I).
           MOVE SPACES TO WSS-GLS-INEM-V(WSS-I).
       FIN-CLR-QDO-VEC.
           EXIT.

       PRO-FLD-QUE SECTION.
       INI-PRO-FLD-QUE.
           MOVE 'P' TO WSS-NUM-IGDO-ATR.
           MOVE 'P' TO WSS-CAI-DOCU-ATR.
           MOVE 'P' TO WSS-IIC-DOCU-ATR.
           MOVE 'P' TO WSS-COD-TDOC-ATR.
           MOVE 'P' TO WSS-GLS-TDOC-ATR.
           MOVE 'P' TO WSS-FEC-FCAP-ATR.
           MOVE 'P' TO WSS-COD-TIEM-ATR.
           MOVE 'P' TO WSS-COD-INEM-ATR.
           MOVE 'P' TO WSS-COD-SIEM-ATR.
           MOVE 'P' TO WSS-GLS-INEM-ATR.
       FIN-PRO-FLD-QUE.
           EXIT.
      *>>>> EOF
*% END
