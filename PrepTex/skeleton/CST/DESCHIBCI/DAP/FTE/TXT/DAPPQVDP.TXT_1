*% PGM_PTC = TRUE
*% IF WSS
      *<<<<
       COPY DAPWGVCP.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSC-REQA==.
       COPY GNSBRMSC.
       
       01  WSS-VARI.
           03 WSS-I                     VALUE ZEROES    PIC 9(03).
       
       01  WSS-CONSTANTES.
           03 WSS-CT-1                  VALUE 1         PIC 9(01).
           03 WSS-CT-15                 VALUE 15        PIC 9(02).
           03 WSS-CT-VRFOK   VALUE 'VERIFICACION OK'    PIC X(15).
           03 WSS-CT-HMASERR                            PIC X(40)
              VALUE 'HAY MAS ERRORES, CORRIJA LOS ACTUALES...'.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<<
           MOVE POD-COD-VCAM IN VDP-FLD TO DAPVCP-COD-VCAM
           MOVE POD-COD-TOPD IN VDP-FLD TO DAPVCP-COD-TOPD
           MOVE POD-COD-AOPD IN VDP-FLD TO DAPVCP-COD-AOPD
           PERFORM DAP-LNK-VCP.
           IF DAPVCP-COD-MENS IN DAPVCP-VARI NOT > SPACES
              MOVE WSS-CT-VRFOK TO FRM-MENS
              GO TO FIN-FND-KEY.
           
           MOVE ZEROES TO WSS-I.
           
       LUP-LLENA-MSG.
           ADD WSS-CT-1 TO WSS-I.
           
           IF DAPVCP-GLS-MENS-V(WSS-I) NOT > SPACES
              GO TO FIN-FND-KEY.
              
           IF WSS-I > WSS-CT-15
              MOVE WSS-CT-HMASERR TO FRM-MENS
              GO TO FIN-FND-KEY.
          
           MOVE WSS-I             TO FRM-NUM-MENS IN VDP-FLD(WSS-I).
           MOVE DAPVCP-GLS-MENS-V(WSS-I) TO 
                                  FRM-GLS-MSJE IN VDP-FLD(WSS-I).
           
           GO TO LUP-LLENA-MSG.
      *>>>>
*% END
*% IF EOF
      *<<<<
       COPY GNSBFMSC.
       COPY GNSBVCOD.
       COPY GNSBVTAB.
       COPY DAPBKVCP.
      *>>>>
*% END
