       SET-TAG-DAPBQDCC SECTION.
       INI-SET-TAG-DAPBQDCC.
           MOVE
           '<IDNMOD NOM=$DAPBQDCC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$26-Jun-18 12:59 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-DAPBQDCC.
           EXIT.
           
       PUT-ALL-DCC SECTION.
       INI-PUT-ALL-DCC.
           PERFORM PUT-DCC-DCC.
           PERFORM PUT-TAB-DCC.
      *<<<< FIN_PUT_ALL_DCC
           IF DCC-COD-VCAM IN DCC-FLD = '*'
               PERFORM GET-TODAS
               MOVE MSG-GLS-DESC TO FRM-GLS-VCAM IN DCC-FLD.
           IF DCC-COD-COPD IN DCC-FLD = '*  *'
               PERFORM GET-TODAS
               MOVE MSG-GLS-DESC TO FRM-GLS-COPD IN DCC-FLD.

           IF DCC-COD-PLZO IN DCC-FLD = '*'
               PERFORM GET-TODAS
               MOVE MSG-GLS-DESC TO FRM-GLS-PLZO IN DCC-FLD.

           IF DCC-GLS-CCSB IN DCC NOT > SPACES
               PERFORM CLR-TAB-DCC
               GO TO FIN-PUT-ALL-DCC
           ELSE
               MOVE 'PCC' TO TAB-COD-TTAB IN TAB
               MOVE DCC-GLS-CCSB IN DCC-FLD TO TAB-COD-CTAB IN TAB
               MOVE 'TAB'       TO FIO-SIST
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM GNS-FIO-TAB
               MOVE FIO-STAT TO PGM-STAT-TAB
               IF FIO-STAT-OKS
                   PERFORM PUT-TAB-DCC
               ELSE
                   MOVE 'DAP' TO MSG-COD-SIST
                   MOVE 'GLSCCTBNEX' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
      *>>>>
       FIN-PUT-ALL-DCC.
           EXIT.
       
       PUT-DCC-DCC SECTION.
       INI-PUT-DCC-DCC.
           IF PGM-STAT-DCC-NUL
               GO TO FIN-PUT-DCC-DCC.
           IF NOT PGM-STAT-DCC-OKS
               PERFORM CLR-DCC-DCC
               GO TO FIN-PUT-DCC-DCC.
           MOVE DCC-COD-TCCT IN DCC TO DCC-COD-TCCT IN DCC-FLD.
           MOVE DCC-COD-VCAM IN DCC TO DCC-COD-VCAM IN DCC-FLD.
           MOVE DCC-COD-COPD IN DCC TO DCC-COD-COPD IN DCC-FLD.
           MOVE DCC-COD-PLZO IN DCC TO DCC-COD-PLZO IN DCC-FLD.
           MOVE DCC-COD-SCTB IN DCC TO DCC-COD-SCTB IN DCC-FLD.
           MOVE DCC-CIC-CCTB IN DCC TO DCC-CIC-CCTB IN DCC-FLD.
           MOVE DCC-EXT-CTAB IN DCC TO DCC-EXT-CTAB IN DCC-FLD.
           MOVE DCC-GLS-CCSB IN DCC TO DCC-GLS-CCSB IN DCC-FLD.
           MOVE DCC-COD-VIGE IN DCC TO DCC-COD-VIGE IN DCC-FLD.
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DCC-NUM-DTRN IN DCC TO DCC-NUM-DTRN IN DCC-FLD.
           MOVE DCC-NUM-MTRN IN DCC TO DCC-NUM-MTRN IN DCC-FLD.
           MOVE DCC-NUM-STRN IN DCC TO DCC-NUM-STRN IN DCC-FLD.
           MOVE DCC-NUM-ATRN IN DCC TO DCC-NUM-ATRN IN DCC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE DCC-GLS-DISP IN DCC TO DCC-GLS-DISP IN DCC-FLD.
      *BUS-TAB busca glosa de codigo en tabla
           IF DCC-COD-TCCT IN DCC-FLD > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'TCC ' TO TAB-COD-TTAB IN TAB
               MOVE DCC-COD-TCCT IN DCC-FLD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-TCCT IN DCC-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-TCCT IN DCC-FLD.
      *BUS-TAB busca glosa de codigo en tabla
           IF DCC-COD-VCAM IN DCC-FLD > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'VLR ' TO TAB-COD-TTAB IN TAB
               MOVE DCC-COD-VCAM IN DCC-FLD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-VCAM IN DCC-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-VCAM IN DCC-FLD.
      *BUS-TAB busca glosa de codigo en tabla
           IF DCC-COD-COPD IN DCC-FLD > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'AUX ' TO TAB-COD-TTAB IN TAB
               MOVE DCC-COD-COPD IN DCC-FLD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-COPD IN DCC-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-COPD IN DCC-FLD.
      *BUS-TAB busca glosa de codigo en tabla
           IF DCC-COD-PLZO IN DCC-FLD > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'PZO ' TO TAB-COD-TTAB IN TAB
               MOVE DCC-COD-PLZO IN DCC-FLD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-PLZO IN DCC-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-PLZO IN DCC-FLD.
       FIN-PUT-DCC-DCC.
           EXIT.
       
       PUT-TAB-DCC SECTION.
       INI-PUT-TAB-DCC.
           IF PGM-STAT-TAB-NUL
               GO TO FIN-PUT-TAB-DCC.
           IF NOT PGM-STAT-TAB-OKS
               PERFORM CLR-TAB-DCC
               GO TO FIN-PUT-TAB-DCC.
           MOVE TAB-CIC-CTAB IN TAB TO TAB-CIC-CTAB IN DCC-FLD.
           MOVE TAB-GLS-DESC IN TAB TO TAB-GLS-DESC IN DCC-FLD.
           MOVE TAB-GLS-DATA IN TAB TO TAB-GLS-DATA IN DCC-FLD.
           MOVE TAB-GLS-ABRV IN TAB TO TAB-GLS-ABRV IN DCC-FLD.
           MOVE TAB-GLS-DCOR IN TAB TO TAB-GLS-DCOR IN DCC-FLD.
       FIN-PUT-TAB-DCC.
           EXIT.
       
       KEY-ALL-DCC SECTION.
       INI-KEY-ALL-DCC.
           PERFORM KEY-DCC-DCC.
           PERFORM KEY-TAB-DCC.
       FIN-KEY-ALL-DCC.
           EXIT.
       
       KEY-DCC-DCC SECTION.
       INI-KEY-DCC-DCC.
           MOVE DCC-COD-TCCT IN DCC-FLD TO DCC-COD-TCCT IN DCC.
           MOVE DCC-COD-VCAM IN DCC-FLD TO DCC-COD-VCAM IN DCC.
           MOVE DCC-COD-COPD IN DCC-FLD TO DCC-COD-COPD IN DCC.
           MOVE DCC-COD-PLZO IN DCC-FLD TO DCC-COD-PLZO IN DCC.
           MOVE DCC-COD-SCTB IN DCC-FLD TO DCC-COD-SCTB IN DCC.
      *<<<< FIN_KEY_DCC_DCC
           MOVE 'DCC' TO DCC-COD-TTAB IN DCC.
      *>>>>
       FIN-KEY-DCC-DCC.
           EXIT.
       
       KEY-TAB-DCC SECTION.
       INI-KEY-TAB-DCC.
       FIN-KEY-TAB-DCC.
           EXIT.
       
       SCR-CLRA-DCC SECTION.
       INI-SCR-CLRA-DCC.
           PERFORM CLR-KEY-DCC.
           PERFORM CLR-FLD-DCC.
       FIN-SCR-CLRA-DCC.
           EXIT.
       
       CLR-KEY-DCC SECTION.
       INI-CLR-KEY-DCC.
           MOVE SPACES TO DCC-COD-TCCT IN DCC-FLD.
           MOVE SPACES TO DCC-COD-VCAM IN DCC-FLD.
           MOVE SPACES TO DCC-COD-COPD IN DCC-FLD.
           MOVE SPACES TO DCC-COD-PLZO IN DCC-FLD.
           MOVE SPACES TO DCC-COD-SCTB IN DCC-FLD.
       FIN-CLR-KEY-DCC.
           EXIT.
       
       CLR-FLD-DCC SECTION.
       INI-CLR-FLD-DCC.
           MOVE SPACES TO FRM-GLS-TCCT IN DCC-FLD.
           MOVE SPACES TO FRM-GLS-VCAM IN DCC-FLD.
           MOVE SPACES TO FRM-GLS-COPD IN DCC-FLD.
           MOVE SPACES TO FRM-GLS-PLZO IN DCC-FLD.
           MOVE SPACES TO FRM-GLS-SCTB IN DCC-FLD.
           PERFORM CLR-DCC-DCC.
           PERFORM CLR-TAB-DCC.
       FIN-CLR-FLD-DCC.
           EXIT.
       
       CLR-DCC-DCC SECTION.
       INI-CLR-DCC-DCC.
      *<<<<
           MOVE SPACES TO DCC-CIC-CCTB IN DCC-FLD.                      
           MOVE SPACES TO DCC-EXT-CTAB IN DCC-FLD.                      
           MOVE SPACES TO DCC-GLS-CCSB IN DCC-FLD.                      
           MOVE SPACES TO DCC-COD-VIGE IN DCC-FLD.                      
           MOVE SPACES TO DCC-FEC-FTRN IN DCC-FLD.                      
           MOVE SPACES TO DCC-GLS-DISP IN DCC-FLD.                      
           GO TO FIN-CLR-DCC-DCC.
      *>>>>
           MOVE SPACES TO DCC-CIC-CCTB IN DCC-FLD.
           MOVE SPACES TO DCC-EXT-CTAB IN DCC-FLD.
           MOVE SPACES TO DCC-GLS-CCSB IN DCC-FLD.
           MOVE SPACES TO DCC-COD-VIGE IN DCC-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DCC-FEC-FTRN IN DCC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO DCC-GLS-DISP IN DCC-FLD.
       FIN-CLR-DCC-DCC.
           EXIT.
       
       CLR-TAB-DCC SECTION.
       INI-CLR-TAB-DCC.
           MOVE SPACES TO TAB-CIC-CTAB IN DCC-FLD.
           MOVE SPACES TO TAB-GLS-DESC IN DCC-FLD.
           MOVE SPACES TO TAB-GLS-DATA IN DCC-FLD.
           MOVE SPACES TO TAB-GLS-ABRV IN DCC-FLD.
           MOVE SPACES TO TAB-GLS-DCOR IN DCC-FLD.
       FIN-CLR-TAB-DCC.
           EXIT.
       
       FST-KEY-DCC SECTION.
       INI-FST-KEY-DCC.
           MOVE -1 TO DCC-COD-TCCT-LEN IN DCC-FLD.
      *<<<< FIN_FST_KEY_DCC
      *OGB-INI 31-MAY-2018 CONSULTA Y ELIMINACION MASIVA DE DCC
           IF SCR-CMND = 'COM' OR 'ELM'
               MOVE ZEROES TO DCC-COD-TCCT-LEN IN DCC-FLD
               MOVE -1 TO DCC-COD-VCAM-LEN IN DCC-FLD.
      *OGB-FIN 31-MAY-2018 CONSULTA Y ELIMINACION MASIVA DE DCC
      *>>>>
       FIN-FST-KEY-DCC.
           EXIT.
       
       FST-FLD-DCC SECTION.
       INI-FST-FLD-DCC.
           MOVE -1 TO DCC-CIC-CCTB-LEN IN DCC-FLD.
       FIN-FST-FLD-DCC.
           EXIT.
       
       PRO-KEY-DCC SECTION.
       INI-PRO-KEY-DCC.
           PERFORM PRO-IKY-DCC.
           PERFORM PRO-AKY-DCC.
           PERFORM PRO-MKY-DCC.
       FIN-PRO-KEY-DCC.
           EXIT.
       
       PRO-IKY-DCC SECTION.
       INI-PRO-IKY-DCC.
           MOVE FRM-CPIM-AEY TO DCC-COD-TCCT-ATR IN DCC-FLD.
           MOVE FRM-CPIM-AEY TO DCC-COD-VCAM-ATR IN DCC-FLD.
           MOVE FRM-CPIM-AEY TO DCC-COD-COPD-ATR IN DCC-FLD.
           MOVE FRM-CPIM-AEY TO DCC-COD-PLZO-ATR IN DCC-FLD.
           MOVE FRM-CPIM-AEY TO DCC-COD-SCTB-ATR IN DCC-FLD.
       FIN-PRO-IKY-DCC.
           EXIT.
       
       PRO-MKY-DCC SECTION.
       INI-PRO-MKY-DCC.
       FIN-PRO-MKY-DCC.
           EXIT.
       
       PRO-AKY-DCC SECTION.
       INI-PRO-AKY-DCC.
       FIN-PRO-AKY-DCC.
           EXIT.
       
       UNP-KEY-DCC SECTION.
       INI-UNP-KEY-DCC.
           PERFORM UNP-IKY-DCC.
           PERFORM UNP-AKY-DCC.
           PERFORM UNP-MKY-DCC.
       FIN-UNP-KEY-DCC.
           EXIT.
       
       UNP-IKY-DCC SECTION.
       INI-UNP-IKY-DCC.
           MOVE FRM-CPIM-UBY TO DCC-COD-TCCT-ATR.
           MOVE FRM-CPIM-UBY TO DCC-COD-VCAM-ATR.
           MOVE FRM-CPIM-UBY TO DCC-COD-COPD-ATR.
           MOVE FRM-CPIM-UBY TO DCC-COD-PLZO-ATR.
           MOVE FRM-CPIM-UBY TO DCC-COD-SCTB-ATR.
      *<<<< FIN_UNP_IKY_DCC
            MOVE FRM-CPIM-AEY TO DCC-COD-SCTB-ATR IN DCC-FLD.
      *OGB-INI 31-MAY-2018 CONSULTA Y ELIMINACION MASIVA DE DCC
           IF SCR-CMND = 'COM' OR 'ELM'
              MOVE FRM-CPIM-AEY TO DCC-COD-TCCT-ATR
              MOVE FRM-CPIM-AEY TO DCC-COD-PLZO-ATR.
      *OGB-FIN 31-MAY-2018 CONSULTA Y ELIMINACION MASIVA DE DCC
      *>>>> END
       FIN-UNP-IKY-DCC.
           EXIT.
       
       UNP-MKY-DCC SECTION.
       INI-UNP-MKY-DCC.
       FIN-UNP-MKY-DCC.
           EXIT.
       
       UNP-AKY-DCC SECTION.
       INI-UNP-AKY-DCC.
       FIN-UNP-AKY-DCC.
           EXIT.
       
       UNP-FLD-DCC SECTION.
       INI-UNP-FLD-DCC.
           PERFORM UNP-DCC-DCC.
           PERFORM UNP-TAB-DCC.
       FIN-UNP-FLD-DCC.
           EXIT.
       
       UNP-DCC-DCC SECTION.
       INI-UNP-DCC-DCC.
           MOVE FRM-CPIM-UBY TO DCC-CIC-CCTB-ATR IN DCC-FLD.
           MOVE FRM-CPIM-UBY TO DCC-EXT-CTAB-ATR IN DCC-FLD.
           MOVE FRM-CPIM-UBY TO DCC-GLS-CCSB-ATR IN DCC-FLD.
           MOVE FRM-CPIM-UBY TO DCC-COD-VIGE-ATR IN DCC-FLD.
           MOVE FRM-CPIM-UBY TO DCC-GLS-DISP-ATR IN DCC-FLD.
       FIN-UNP-DCC-DCC.
           EXIT.
       
       UNP-TAB-DCC SECTION.
       INI-UNP-TAB-DCC.
       FIN-UNP-TAB-DCC.
           EXIT.
       
       VAL-KEY-DCC SECTION.
       INI-VAL-KEY-DCC.
           PERFORM VAL-IKY-DCC.
           PERFORM VAL-MKY-DCC.
           PERFORM VAL-AKY-DCC.
       FIN-VAL-KEY-DCC.
           EXIT.
       
       VAL-IKY-DCC SECTION.
       INI-VAL-IKY-DCC.
           PERFORM VAL-DCC-COD-TCCT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCC-COD-TCCT-LEN IN DCC-FLD
                GO TO FIN-VAL-IKY-DCC.
           PERFORM VAL-DCC-COD-VCAM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCC-COD-VCAM-LEN IN DCC-FLD
                GO TO FIN-VAL-IKY-DCC.
           PERFORM VAL-DCC-COD-COPD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCC-COD-COPD-LEN IN DCC-FLD
                GO TO FIN-VAL-IKY-DCC.
           PERFORM VAL-DCC-COD-PLZO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCC-COD-PLZO-LEN IN DCC-FLD
                GO TO FIN-VAL-IKY-DCC.
           PERFORM VAL-DCC-COD-SCTB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCC-COD-SCTB-LEN IN DCC-FLD
                GO TO FIN-VAL-IKY-DCC.
       FIN-VAL-IKY-DCC.
           EXIT.
       
       VAL-MKY-DCC SECTION.
       INI-VAL-MKY-DCC.
       FIN-VAL-MKY-DCC.
           EXIT.
       
       VAL-AKY-DCC SECTION.
       INI-VAL-AKY-DCC.
       FIN-VAL-AKY-DCC.
           EXIT.
       
       VAL-DCC-COD-TCCT SECTION.
       INI-VAL-DCC-COD-TCCT.
      *<<<<
      *OGB-INI 31-MAY-2018 CONSULTA Y ELIMINACION MASIVA DE DCC
           IF SCR-CMND = 'COM' OR 'ELM'
              MOVE SPACES TO DCC-COD-TCCT IN DCC-FLD
              GO TO FIN-VAL-DCC-COD-TCCT.
      *OGB-FIN 31-MAY-2018 CONSULTA Y ELIMINACION MASIVA DE DCC      
      *>>>>
           IF DCC-COD-TCCT IN DCC-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-DCC-COD-TCCT.
      *VAL-COD Valida codigo de tabla
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'TCC ' TO TAB-COD-TTAB IN TAB.
           MOVE DCC-COD-TCCT IN DCC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              DCC-COD-TCCT IN DCC-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-TCCT IN DCC-FLD
           ELSE
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-TCCT IN DCC-FLD.
       FIN-VAL-DCC-COD-TCCT.
           EXIT.
       
       VAL-DCC-COD-VCAM SECTION.
       INI-VAL-DCC-COD-VCAM.
           IF DCC-COD-VCAM IN DCC-FLD NOT > SPACES
              MOVE SPACES TO DCC-COD-VCAM IN DCC-FLD.
      *<<<< DCC_COD_VCAM
           IF DCC-COD-VCAM IN DCC-FLD = '*'
               PERFORM GET-TODAS
               MOVE MSG-GLS-DESC TO FRM-GLS-VCAM IN DCC-FLD
               GO TO FIN-VAL-DCC-COD-VCAM.
      *>>>> END
      *VAL-COD Valida codigo de tabla
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'VLR ' TO TAB-COD-TTAB IN TAB.
           MOVE DCC-COD-VCAM IN DCC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              DCC-COD-VCAM IN DCC-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-VCAM IN DCC-FLD
           ELSE
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-VCAM IN DCC-FLD.
       FIN-VAL-DCC-COD-VCAM.
           EXIT.
       
       VAL-DCC-COD-COPD SECTION.
       INI-VAL-DCC-COD-COPD.
           IF DCC-COD-COPD IN DCC-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-DCC-COD-COPD.
      *<<<< DCC_COD_COPD 
           IF DCC-COD-COPD IN DCC-FLD = '*  *'
               PERFORM GET-TODAS
               MOVE MSG-GLS-DESC TO FRM-GLS-COPD IN DCC-FLD
               GO TO FIN-VAL-DCC-COD-COPD.
      *>>>> END
      *VAL-COD Valida codigo de tabla
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'AUX ' TO TAB-COD-TTAB IN TAB.
           MOVE DCC-COD-COPD IN DCC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              DCC-COD-COPD IN DCC-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-COPD IN DCC-FLD
           ELSE
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-COPD IN DCC-FLD.
       FIN-VAL-DCC-COD-COPD.
           EXIT.
       
       VAL-DCC-COD-PLZO SECTION.
       INI-VAL-DCC-COD-PLZO.
      *OGB-INI 31-MAY-2018 CONSULTA Y ELIMINACION MASIVA DE DCC
           IF SCR-CMND = 'COM' OR 'ELM'
              MOVE SPACES TO DCC-COD-PLZO IN DCC-FLD
              GO TO FIN-VAL-DCC-COD-PLZO.
      *OGB-FIN 31-MAY-2018 CONSULTA Y ELIMINACION MASIVA DE DCC
           IF DCC-COD-PLZO IN DCC-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-DCC-COD-PLZO.
      *<<<< DCC_COD_PLZO
           IF DCC-COD-PLZO IN DCC-FLD = '*'
               PERFORM GET-TODAS
               MOVE MSG-GLS-DESC TO FRM-GLS-PLZO IN DCC-FLD
               GO TO FIN-VAL-DCC-COD-PLZO.
      *>>>> END
      *VAL-COD Valida codigo de tabla
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'PZO ' TO TAB-COD-TTAB IN TAB.
           MOVE DCC-COD-PLZO IN DCC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              DCC-COD-PLZO IN DCC-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-PLZO IN DCC-FLD
           ELSE
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-PLZO IN DCC-FLD.
       FIN-VAL-DCC-COD-PLZO.
           EXIT.
       
       VAL-DCC-COD-SCTB SECTION.
       INI-VAL-DCC-COD-SCTB.
      *<<<<
           IF DCC-COD-SCTB IN DCC-FLD NOT > SPACES                      
              MOVE SPACES TO DCC-COD-SCTB IN DCC-FLD.                   
           GO TO FIN-VAL-DCC-COD-SCTB.
      *>>>>
           IF DCC-COD-SCTB IN DCC-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-DCC-COD-SCTB.
       FIN-VAL-DCC-COD-SCTB.
           EXIT.
       
       VAL-CON-KEY-DCC SECTION.
       INI-VAL-CON-KEY-DCC.
           IF     DCC-COD-TCCT IN DCC-FLD NOT > SPACES 
              AND DCC-COD-VCAM IN DCC-FLD NOT > SPACES 
              AND DCC-COD-COPD IN DCC-FLD NOT > SPACES 
              AND DCC-COD-PLZO IN DCC-FLD NOT > SPACES 
              AND DCC-COD-SCTB IN DCC-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-DCC.
      *<<<< FIN_VAL_CON_IKY_DCC OR FIN_VAL_CON_KEY_DCC 
      * ISP:
      * VALIDA EXISTENCIA DE PLANTILLA POD
           IF DCC-COD-VCAM IN DCC-FLD = '*' OR
              DCC-COD-COPD IN DCC-FLD = '*  *' OR
              DCC-COD-PLZO IN DCC-FLD = '*'
                GO TO NOT-GET-POD.

           MOVE DCC-COD-VCAM IN DCC-FLD TO POD-COD-VCAM IN POD.
           MOVE DCC-COD-COPD IN DCC-FLD TO POD-COD-COPD IN POD.
           MOVE ZEROES                  TO POD-NUM-IPOD IN POD.
      * ISP: DAPBG001
           PERFORM GET-POD.
           IF MSG-COD-MENS > SPACES
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
       NOT-GET-POD.
      *>>>> END
       FIN-VAL-CON-KEY-DCC.
           EXIT.
       
