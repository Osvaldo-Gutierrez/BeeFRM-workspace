000000
000000*VAL-FEC VALIDA FECHA DE PROCESO
000000     DISPLAY WS-FEC-FPRI
000000     MOVE WS-NUM-DPRI TO RUTWVALF-FECHA-D
000000     MOVE WS-NUM-MPRI TO RUTWVALF-FECHA-M
000000     MOVE WS-NUM-SPRI TO RUTWVALF-FECHA-AM
000000     MOVE WS-NUM-APRI TO RUTWVALF-FECHA-AA
000000     CALL 'RUTUVALF' USING RUTWVALF-FECHA RUTWVALF-CODRET
000000     IF RUTWVALF-CODRET NOT = 0
000000        DISPLAY
000000           '* FECHA PROCESO INVALIDA. ' WS-FEC-FPRI
000000        MOVE 1 TO SW-CANCEL.
000000     MOVE WS-NUM-DPRI TO TIT-02-NUM-DFIN
000000     MOVE WS-NUM-MPRI TO TIT-02-NUM-MFIN
000000     MOVE WS-NUM-SPRI TO TIT-02-NUM-SFIN
000000     MOVE WS-NUM-APRI TO TIT-02-NUM-AFIN
000000*VAL-OFI VALIDA CODIGO DE OFICINA
000000     IF WS-COD-OFIC > SPACES AND
000000        WS-COD-OFIC NOT = '*'
000000        DISPLAY WS-COD-OFIC
000000        MOVE WS-COD-OFIC TO RUTWTOFI-COD-OFIC
000000        CALL 'RUTUTOFI' USING RUTWTOFI
000000        IF RUTWTOFI-RETURN-CODE  = SPACES
000000           NEXT SENTENCE
000000        ELSE
000000            DISPLAY ' ERROR VALIDACION OFICINA: ' WS-COD-OFIC
000000            MOVE 1 TO SW-CANCEL.
000000*
000000*VAL-TAB VALIDA CODIGO DE MONEDA
000000     IF WS-COD-VCAM > SPACES AND
000000        WS-COD-VCAM NOT = '*'
000000        DISPLAY WS-COD-VCAM
000000        MOVE +150                TO RUTWTTAB-BASE
000000        MOVE 'VLR'               TO RUTWTTAB-TABLA
000000        MOVE WS-COD-VCAM         TO RUTWTTAB-CLAVE
000000        CALL 'RUTUTTAB' USING RUTWTTAB
000000        IF RUTWTTAB-RETURN-CODE  = SPACES
000000           NEXT SENTENCE
000000        ELSE
000000           DISPLAY ' '
000000           DISPLAY ' ERROR VALIDACION COD. MONEDA: ' WS-COD-VCAM
000000           MOVE 1 TO SW-CANCEL.
000000*
000000*VAL-TAB VALIDA CLASE DE OPERACION
000000     IF WS-COD-COPD > SPACES AND
000000        WS-COD-COPD NOT = '*'
000000        DISPLAY WS-COD-COPD
000000        MOVE +371                TO RUTWTTAB-BASE
000000        MOVE 'AUX'               TO RUTWTTAB-TABLA
000000        MOVE WS-COD-COPD         TO RUTWTTAB-CLAVE
000000        CALL 'RUTUTTAB' USING RUTWTTAB
