000000        FS-DAPDOPD = 1
000000     GO TO EZZ-FIN-SECC.
000000*
000000 ED-SELECCIONA.
000000** SELECCIONA REGISTROS PARA CARGARLOS EN EL SORT
000000     IF OPD-COD-STAT = 'G' OR
000000        (OPD-COD-STAT = 'L' AND
000000         OPD-COD-SCTB = 'VCD')
000000            PERFORM EE-REVISA-SELEC.
000000     PERFORM EA-LEER-DAPDOPD.
000000*
000000 EE-REVISA-SELEC.
000000*CONTROLA INGRESO DE REGS. VIGENTES O NO VIGENTES VALIDOS
000000     MOVE 1 TO SW-PROC-REG
000000     IF WS-COD-OFIC > SPACES AND
000000        WS-COD-OFIC NOT = '*'
000000        IF WS-COD-OFIC NOT = OPD-COD-OFIC
000000           MOVE 0 TO SW-PROC-REG.
000000     IF WS-COD-COPD > SPACES AND
000000        WS-COD-COPD NOT = '*'
000000        IF WS-COD-COPD NOT = OPD-COD-COPD
000000           MOVE 0 TO SW-PROC-REG.
000000     IF WS-COD-VCAM > SPACES AND
000000        WS-COD-VCAM NOT = '*'
000000        IF WS-COD-VCAM NOT = OPD-COD-VCAM
000000           MOVE 0 TO SW-PROC-REG.
000000     MOVE +0                 TO WS-VAL-CAPT
000000                                WS-VAL-INTE
000000                                WS-VAL-FINA
000000     MOVE ALL '0'            TO WS-FEC-FVEN
000000     IF SW-PROC-REG = 1
000000        MOVE OPD-VAL-CAPT    TO WS-VAL-CAPT
000000        MOVE VEN-VAL-FINA    TO WS-VAL-FINA
000000        COMPUTE WS-VAL-INTE =
000000           WS-VAL-FINA - WS-VAL-CAPT
000000        MOVE VEN-FEC-FVEN    TO WS-FEC-FVEN
000000        IF OPD-COD-STAT = 'L'
000000           PERFORM EF-BUSCA-LIQ.
000000     IF WS-COD-VIGE = 'S' AND
000000        OPD-COD-STAT = 'G'
000000           NEXT SENTENCE
000000     ELSE
000000        IF WS-COD-VIGE = 'N' AND
000000           OPD-COD-STAT = 'L'
000000              NEXT SENTENCE
000000        ELSE
000000           MOVE 0 TO SW-PROC-REG.
000000     IF SW-PROC-REG = 1
000000        ADD 1        TO CNT-DAPDOPD-S
000000        PERFORM EK-GRABA-FSORT.
000000*
000000 EF-BUSCA-LIQ.
000000     MOVE 'REDKY'          TO REQ-CMD-LIQ
000000     MOVE SPACES           TO REQ-RETURN-CODE-LIQ
000000     MOVE OPD-CIC-IOPD     TO REQ-KEY-LIQ-CIC
000000     MOVE 001              TO REQ-KEY-LIQ-NUM
000000     CALL 'DBNTRY' USING UIB REQ-AREA-LIQ REG-LIQ ELM-LIS-LIQ
000000     EVALUATE REQ-RETURN-CODE-LIQ
000000        WHEN SPACES
000000           PERFORM EG-ANALIZA-LIQ
000000        WHEN OTHER
000000* NO EXISTE LIQUIDACION
000000           DISPLAY '* ERROR: REDKY TABLA LIQ *'
000000           DISPLAY '* TABLA          : ' REQ-TBLNM-LIQ
000000           DISPLAY '* CODIGO RETORNO : ' REQ-RETURN-CODE-LIQ
000000           DISPLAY '* REQ. AREA      : ' REQ-AREA-LIQ
000000           PERFORM EY-RECHAZA-OPD
000000     END-EVALUATE.
000000*
000000 EG-ANALIZA-LIQ.
000000     IF LIQ-COD-STAT = 'A'
000000        DISPLAY '* DOCUMENTO LIQUIDADO. '
000000        PERFORM EY-RECHAZA-OPD
000000     ELSE
000000        IF LIQ-COD-LIQU = 'RC' AND
000000           LIQ-COD-STAT = 'S'
000000              MOVE 'G'    TO OPD-COD-STAT
000000              MOVE 'VIG'  TO OPD-COD-SCTB
000000        ELSE
000000           MOVE LIQ-SGV-CAPI TO WS-VAL-CAPT
000000           MOVE LIQ-SGV-TPAG TO WS-VAL-FINA
000000           COMPUTE WS-VAL-INTE =
000000              WS-VAL-FINA - WS-VAL-CAPT
000000           MOVE LIQ-FEC-FREA TO WS-FEC-FVEN
000000           PERFORM EH-TRANSFORMA-VALORES.
000000*
000000 EH-TRANSFORMA-VALORES.
000000     IF OPD-COD-CVAL = '998'
000000* OBTIENE FECHA DE VENCIMIENTO   DOCUMENTO
000000        MOVE WS-FEC-FVEN              TO TB-BUS-TABVUF
000000        PERFORM EI-BUSCA-VALOR-VUF
000000* CONVIERTE A PESOS LOS VALORES DE CAPTACION, INTERES Y FINAL.
000000        COMPUTE WS-VALOR-PESOS ROUNDED =
000000           WS-VAL-CAPT * WR-VAL-VVUF
000000        MOVE WS-VALOR-PESOS           TO WS-VAL-CAPT
000000        COMPUTE WS-VALOR-PESOS ROUNDED =
000000           WS-VAL-INTE  * WR-VAL-VVUF
000000        MOVE WS-VALOR-PESOS           TO WS-VAL-INTE
000000        COMPUTE WS-VALOR-PESOS ROUNDED =
000000           WS-VAL-FINA  * WR-VAL-VVUF
000000        MOVE WS-VALOR-PESOS           TO WS-VAL-FINA.
000000*
000000 EI-BUSCA-VALOR-VUF.
000000     MOVE +0                                   TO WR-VAL-VVUF
000000     SEARCH ALL TB-TABVUF
000000        AT END
000000           MOVE +0                             TO WR-VAL-VVUF
000000           DISPLAY '** NO EXISTE VALOR UF        PARA FECHA.  '
000000                   TB-BUS-TABVUF
000000           PERFORM EY-RECHAZA-OPD
000000        WHEN
000000           TB-TABVUF-FECHA (IX-TABVUF) = TB-BUS-TABVUF
000000              MOVE TB-TABVUF-VALOR (IX-TABVUF) TO WR-VAL-VVUF.
000000*
000000 EK-GRABA-FSORT.
000000     MOVE OPD-KEY-IOPD           TO FSORT-KEY-IOPD
000000     MOVE OPD-COD-VCAM           TO FSORT-COD-VCAM
000000     MOVE OPD-COD-COPD           TO FSORT-COD-COPD
000000     MOVE OPD-COD-OFIC           TO FSORT-COD-OFIC
000000     MOVE OPD-CIC-ISER           TO FSORT-CIC-ISER
000000     MOVE OPD-FEC-FREA           TO FSORT-FEC-FREA
000000     MOVE OPD-COD-TINT           TO FSORT-COD-TINT
000000     MOVE OPD-SGV-TINT           TO FSORT-SGV-TINT
000000     MOVE OPD-VAL-SINT           TO FSORT-VAL-SINT
000000     MOVE OPD-NUM-DURV           TO FSORT-NUM-DURV
000000     MOVE OPD-IND-DURV           TO FSORT-IND-DURV
000000     MOVE OPD-IND-MCLI           TO FSORT-IND-MCLI
000000     MOVE OPD-COD-STAT           TO FSORT-COD-STAT
000000     MOVE WS-VAL-CAPT            TO FSORT-VAL-CAPT
000000     MOVE WS-VAL-INTE            TO FSORT-VAL-INTE
000000     MOVE WS-VAL-FINA            TO FSORT-VAL-FINA
000000     MOVE WS-FEC-FVEN            TO FSORT-FEC-FVEN
000000     RELEASE REG-FSORT
000000     ADD 1                       TO CNT-FSORT-G.
000000*
000000 EY-RECHAZA-OPD.
000000     DISPLAY '* OPD RECHAZADA:  ' OPD-CIC-IOPD '  '
000000             'SERIAL: ' OPD-CIC-ISER
000000     DISPLAY ' '
000000     MOVE 0                      TO SW-PROC-REG.
000000*
000000 EZ-PROCESO-ERROR.
000000     DISPLAY '*****************************************'
000000     DISPLAY '*                                       *'
000000     DISPLAY 'ERROR AL ' WR-MENSAJE 'TABLA' WR-TABLA-ERR
000000     DISPLAY 'CODIGO DE RETORNO : ' WR-CODIGO '       *'
000000     DISPLAY '*                                       *'
000000     DISPLAY '*****************************************'
000000     DISPLAY ' '
000000     MOVE 20 TO SORT-RETURN.
000000*
000000 EZZ-FIN-SECC.
000000     EXIT.
000000     EJECT
000000*
000000*
000000*
000000 F-SALIDA-SORT SECTION.
000000*
000000 FA-LEER-FSORT.
000000     RETURN FSORT AT END MOVE 1 TO FS-FSORT.
000000     IF FS-FSORT = 0
000000        ADD 1 TO CNT-FSORT-L.
000000*
000000 FB-CTRL-PROC.
000000     PERFORM FC-CTRL-X-OFIC UNTIL
000000        FS-FSORT = 1
000000     GO TO FZZ-FIN-SECC.
000000*
000000 FC-CTRL-X-OFIC.
000000* OFICINA
000000     INITIALIZE ACM-OFIC
000000     MOVE 99                TO CNT-LINE-DAPLCAR
000000     MOVE 0                 TO CNT-PAGE-DAPLCAR
000000     MOVE FSORT-COD-OFIC    TO WR-COD-OFIC
000000     PERFORM FC1-BUSCA-GLOSA-OFIC
000000     MOVE WR-COD-OFIC       TO TIT-05-COD-OFIC
000000     MOVE WR-GLS-OFIC       TO TIT-05-GLS-OFIC
000000     PERFORM FD-CTRL-X-VCAM UNTIL
000000        FSORT-COD-OFIC NOT = WR-COD-OFIC OR
000000           FS-FSORT = 1
000000     PERFORM FC9-TOTALES-OFIC.
000000*
000000 FC1-BUSCA-GLOSA-OFIC.
000000     MOVE WR-COD-OFIC     TO RUTWTOFI-COD-OFIC
000000     CALL 'RUTUTOFI' USING RUTWTOFI
000000     EVALUATE RUTWTOFI-RETURN-CODE
000000        WHEN SPACES
000000           MOVE RUTWTOFI-GLS-DESC     TO WR-GLS-OFIC
000000        WHEN '14'
000000           MOVE ALL '?'               TO WR-GLS-OFIC
000000        WHEN OTHER
000000           DISPLAY 'ERROR EN ACCESO TABLA OFI (RUTUTOFI). '
000000                   'COD-RET = ' RUTWTOFI-RETURN-CODE
000000           DISPLAY 'CLAVE   = ' RUTWTOFI-COD-OFIC
000000           MOVE 20 TO SORT-RETURN
000000     END-EVALUATE.
000000*
000000 FC9-TOTALES-OFIC.
000000* TOTALIZAR POR OFIC
000000     IF SALTO-PAG-DAPLCAR
000000        PERFORM FV-TITULOS.
000000     MOVE SPACES                 TO TOT-01
000000     MOVE 'TOTAL OFICINA'        TO TOT-01-TIP-TOT
000000     MOVE ACM-OFIC-NUME          TO TOT-01-TOT-OPE
000000     MOVE 'OPERACION(ES)  '      TO TOT-01-LIT-TOT
000000     WRITE REG-DAPLCAR FROM TOT-01     AFTER 1
000000     ADD 1                       TO CNT-LINE-DAPLCAR.
000000*
000000 FD-CTRL-X-VCAM.
000000* MONEDA
000000     MOVE 99                TO CNT-LINE-DAPLCAR
000000     INITIALIZE ACM-VCAM
000000     MOVE FSORT-COD-VCAM    TO WR-COD-VCAM
000000     PERFORM FD1-BUSCA-GLOSA-VCAM
000000     MOVE WR-GLS-VCAM       TO TIT-05-GLS-VCAM
000000     PERFORM FE-CTRL-X-COPD UNTIL
000000        FSORT-COD-OFIC NOT = WR-COD-OFIC OR
000000           FSORT-COD-VCAM NOT = WR-COD-VCAM OR
000000              FS-FSORT = 1
000000     PERFORM FD9-TOTALES-VCAM.
000000*
000000 FD1-BUSCA-GLOSA-VCAM.
000000* BUSCA GLOSA MONEDA
000000     MOVE +150                TO RUTWTTAB-BASE
000000     MOVE 'VLR'               TO RUTWTTAB-TABLA
000000     MOVE WR-COD-VCAM         TO RUTWTTAB-CLAVE
000000     CALL 'RUTUTTAB' USING RUTWTTAB
000000     EVALUATE RUTWTTAB-RETURN-CODE
000000        WHEN SPACES
000000           MOVE RUTWTTAB-REGISTRO-TAB TO REG-TAB
000000           MOVE TAB-GLS-DESC          TO WR-GLS-VCAM
000000           MOVE TAB-GLS-ABRV          TO WR-ABR-VCAM
000000        WHEN '14'
000000           MOVE ALL '?'               TO WR-GLS-VCAM
000000           MOVE ALL '?'               TO WR-ABR-VCAM
000000        WHEN OTHER
000000           DISPLAY 'ERROR EN ACCESO TABLA TAB (RUTUTTAB). '
000000                   'COD-RET = ' RUTWTTAB-RETURN-CODE
000000           DISPLAY 'BASE  = ' RUTWTTAB-BASE ' '
000000                   'TABLA = ' RUTWTTAB-TABLA ' '
000000                   'CLAVE = ' RUTWTTAB-CLAVE
000000           MOVE 20 TO SORT-RETURN
000000     END-EVALUATE.
000000*
000000 FD9-TOTALES-VCAM.
000000* ACUMULA TOTALES POR MONEDA EN TOTALES POR OFICINA
000000     ADD ACM-VCAM-NUME      TO ACM-OFIC-NUME
000000* TOTALIZAR POR VCAM
000000     IF SALTO-PAG-DAPLCAR
000000        PERFORM FV-TITULOS.
000000     MOVE SPACES                 TO TOT-01
000000     MOVE 'TOTAL MONEDA '        TO TOT-01-TIP-TOT
000000     MOVE ACM-VCAM-NUME          TO TOT-01-TOT-OPE
000000     MOVE 'OPERACION(ES) :'      TO TOT-01-LIT-TOT
000000     MOVE ACM-VCAM-VAL-CAPT      TO TOT-01-VAL-CAPT
000000     MOVE ACM-VCAM-VAL-INTE      TO TOT-01-VAL-INTE
000000     MOVE ACM-VCAM-VAL-FINA      TO TOT-01-VAL-FINA
000000     WRITE REG-DAPLCAR FROM TOT-01     AFTER 1
000000     ADD 1                       TO CNT-LINE-DAPLCAR.
000000*
000000 FE-CTRL-X-COPD.
000000* CLASE OPERACION
000000     MOVE 99                TO CNT-LINE-DAPLCAR
000000     INITIALIZE ACM-COPD
000000     MOVE FSORT-COD-COPD    TO WR-COD-COPD
000000     PERFORM FE1-BUSCA-GLOSA-COPD
000000     MOVE WR-GLS-TOPD       TO TIT-05-GLS-TOPD
000000     MOVE WR-GLS-COPD       TO TIT-05-GLS-COPD
000000     PERFORM FF-PROCESA UNTIL
000000        FSORT-COD-OFIC NOT = WR-COD-OFIC OR
000000           FSORT-COD-VCAM NOT = WR-COD-VCAM OR
000000              FSORT-COD-COPD NOT = WR-COD-COPD OR
000000                 FS-FSORT = 1
000000     PERFORM FE9-TOTALES-COPD.
000000*
000000 FE1-BUSCA-GLOSA-COPD.
000000* BUSCA GLOSA CODIGO OPERACION EN TABLA TIO
000000     MOVE +150                TO RUTWTTAB-BASE
000000     MOVE 'TIO'               TO RUTWTTAB-TABLA
000000     MOVE WR-COD-TOPD         TO RUTWTTAB-CLAVE
000000     CALL 'RUTUTTAB' USING RUTWTTAB
000000     EVALUATE RUTWTTAB-RETURN-CODE
000000        WHEN SPACES
000000           MOVE RUTWTTAB-REGISTRO-TAB TO REG-TAB
000000           MOVE TAB-GLS-DESC          TO WR-GLS-TOPD
000000        WHEN '14'
000000           MOVE ALL '?'               TO WR-GLS-TOPD
000000        WHEN OTHER
000000           DISPLAY 'ERROR EN ACCESO TABLA TAB (RUTUTTAB). '
000000                   'COD-RET = ' RUTWTTAB-RETURN-CODE
000000           DISPLAY 'BASE  = ' RUTWTTAB-BASE ' '
000000                   'TABLA = ' RUTWTTAB-TABLA ' '
000000                   'CLAVE = ' RUTWTTAB-CLAVE
000000           MOVE 20 TO SORT-RETURN
000000     END-EVALUATE.
000000* BUSCA GLOSA CODIGO OPERACION EN TABLA AUX
000000     MOVE +371                TO RUTWTTAB-BASE
000000     MOVE 'AUX'               TO RUTWTTAB-TABLA
000000     MOVE WR-COD-COPD         TO RUTWTTAB-CLAVE
000000     CALL 'RUTUTTAB' USING RUTWTTAB
000000     EVALUATE RUTWTTAB-RETURN-CODE
000000        WHEN SPACES
000000           MOVE RUTWTTAB-REGISTRO-TAB TO REG-TAB
000000           MOVE TAB-GLS-DESC          TO WR-GLS-COPD
000000        WHEN '14'
000000           MOVE ALL '?'               TO WR-GLS-COPD
000000        WHEN OTHER
000000           DISPLAY 'ERROR EN ACCESO TABLA TAB (RUTUTTAB). '
000000                   'COD-RET = ' RUTWTTAB-RETURN-CODE
000000           DISPLAY 'BASE  = ' RUTWTTAB-BASE ' '
000000                   'TABLA = ' RUTWTTAB-TABLA ' '
000000                   'CLAVE = ' RUTWTTAB-CLAVE
000000           MOVE 20 TO SORT-RETURN
000000     END-EVALUATE.
000000*
000000 FE9-TOTALES-COPD.
000000* ACUMULA TOTALES POR CLASE EN TOTALES POR MONEDA
000000     ADD ACM-COPD-VAL-CAPT       TO ACM-VCAM-VAL-CAPT
000000     ADD ACM-COPD-VAL-INTE       TO ACM-VCAM-VAL-INTE
000000     ADD ACM-COPD-VAL-FINA       TO ACM-VCAM-VAL-FINA
000000     ADD ACM-COPD-NUME           TO ACM-VCAM-NUME
000000* TOTALIZAR POR COPD
000000     IF SALTO-PAG-DAPLCAR
000000        PERFORM FV-TITULOS.
000000     MOVE SPACES                 TO TOT-01
000000     MOVE 'TOTAL CLASE  '        TO TOT-01-TIP-TOT
000000     MOVE ACM-COPD-NUME          TO TOT-01-TOT-OPE
000000     MOVE 'OPERACION(ES) :'      TO TOT-01-LIT-TOT
000000     MOVE ACM-COPD-VAL-CAPT      TO TOT-01-VAL-CAPT
000000     MOVE ACM-COPD-VAL-INTE      TO TOT-01-VAL-INTE
000000     MOVE ACM-COPD-VAL-FINA      TO TOT-01-VAL-FINA
000000     WRITE REG-DAPLCAR FROM TOT-01     AFTER 1
000000     ADD 1                       TO CNT-LINE-DAPLCAR.
000000*
000000 FF-PROCESA.
000000     PERFORM FK-BUSCA-CLIENTE
000000     PERFORM FN-BUSCA-GLOSA-TASA
000000     PERFORM FO-BUSCA-GLOSA-PLZO
000000     PERFORM FU-IMPRIME
000000     PERFORM FA-LEER-FSORT.
000000*
000000 FK-BUSCA-CLIENTE.
000000* CON EL CODIGO DE LA OPERACION VA A LA RCC PARA OBTENER EL CIC
000000* DEL CLIENTE Y CON ESTE VA A LA DBC PARA OBTENER LOS DATOS DE
000000* CLIENTE A IMPRIMIR.
000000     MOVE 'REDKY'          TO REQ-CMD-RCC
000000     MOVE SPACES           TO REQ-RETURN-CODE-RCC
000000     MOVE FSORT-CIC-IOPD   TO REQ-KEY-RCC-CIC
000000     CALL 'DBNTRY' USING UIB REQ-AREA-RCC REG-RCC ELM-LIS-RCC
000000     EVALUATE REQ-RETURN-CODE-RCC
000000        WHEN SPACES
000000           PERFORM FK1-BUSCA-CLIENTE-DBC
000000        WHEN '14'
000000           MOVE SPACES                TO DBC-IDC-ICLI
000000           MOVE SPACES                TO DBC-VRF-ICLI
000000           MOVE 'CLIENTE INEXISTENTE' TO WR-GLS-NOMC
000000        WHEN OTHER
000000           DISPLAY '* ERROR: REDKY TABLA RCC *'
000000           DISPLAY '* TABLA          : ' REQ-TBLNM-RCC
000000           DISPLAY '* CODIGO RETORNO : ' REQ-RETURN-CODE-RCC
000000           DISPLAY '* REQ. AREA      : ' REQ-AREA-RCC
000000           MOVE 20 TO SORT-RETURN
000000     END-EVALUATE.
000000*
000000 FK1-BUSCA-CLIENTE-DBC.
000000     MOVE 'REDKY'          TO REQ-CMD-DBC
000000     MOVE SPACES           TO REQ-RETURN-CODE-DBC
000000     MOVE RCC-CIC-ICLI     TO REQ-KEY-DBC-ICLI
000000     CALL 'DBNTRY' USING UIB REQ-AREA-DBC REG-DBC ELM-LIS-DBC
000000     EVALUATE REQ-RETURN-CODE-DBC
000000        WHEN SPACES
000000           PERFORM FL-COMPACTA-NOMBRE
000000        WHEN '14'
000000           MOVE SPACES                TO DBC-IDC-ICLI
000000           MOVE SPACES                TO DBC-VRF-ICLI
000000           MOVE 'CLIENTE INEXISTENTE' TO WR-GLS-NOMC
000000        WHEN OTHER
000000           DISPLAY '* ERROR: REDKY TABLA DBC *'
000000           DISPLAY '* TABLA          : ' REQ-TBLNM-DBC
000000           DISPLAY '* CODIGO RETORNO : ' REQ-RETURN-CODE-DBC
000000           DISPLAY '* REQ. AREA      : ' REQ-AREA-DBC
000000           MOVE 20 TO SORT-RETURN
000000     END-EVALUATE.
000000*
000000 FL-COMPACTA-NOMBRE.
000000     MOVE DBC-GLS-APAT      TO RUTWCMNN-INP-APAT
000000     MOVE DBC-GLS-AMAT      TO RUTWCMNN-INP-AMAT
000000     MOVE DBC-GLS-NOMB      TO RUTWCMNN-INP-NOMB
000000     CALL 'RUTUCMNN' USING RUTWCMNN
000000     MOVE RUTWCMNN-OUT-NOMB TO WR-GLS-NOMC.
000000*
000000 FN-BUSCA-GLOSA-TASA.
000000* BUSCA GLOSA CODIGO TASA  EN TABLA INT
000000     MOVE +150                TO RUTWTTAB-BASE
000000     MOVE 'INT'               TO RUTWTTAB-TABLA
000000     MOVE FSORT-COD-TINT      TO RUTWTTAB-CLAVE
000000     CALL 'RUTUTTAB' USING RUTWTTAB
000000     EVALUATE RUTWTTAB-RETURN-CODE
000000        WHEN SPACES
000000           MOVE RUTWTTAB-REGISTRO-TAB TO REG-TAB
000000           MOVE TAB-GLS-DESC          TO WR-GLS-TINT
000000        WHEN '14'
000000           MOVE ALL '?'               TO WR-GLS-TINT
000000        WHEN OTHER
000000           DISPLAY 'ERROR EN ACCESO TABLA TAB (RUTUTTAB). '
000000                   'COD-RET = ' RUTWTTAB-RETURN-CODE
000000           DISPLAY 'BASE  = ' RUTWTTAB-BASE ' '
000000                   'TABLA = ' RUTWTTAB-TABLA ' '
000000                   'CLAVE = ' RUTWTTAB-CLAVE
000000           MOVE 20 TO SORT-RETURN
000000     END-EVALUATE.
000000*
000000 FO-BUSCA-GLOSA-PLZO.
000000* BUSCA GLOSA CODIGO PLZO  EN TABLA MSC
000000     MOVE +150                TO RUTWTTAB-BASE
000000     MOVE 'MSC'               TO RUTWTTAB-TABLA
000000     MOVE 'IPER'              TO WS-COD-IPER-V1
000000     MOVE FSORT-IND-DURV      TO WS-COD-IPER-V2
000000     MOVE WS-COD-IPER         TO RUTWTTAB-CLAVE
000000     CALL 'RUTUTTAB' USING RUTWTTAB
000000     EVALUATE RUTWTTAB-RETURN-CODE
000000        WHEN SPACES
000000           MOVE RUTWTTAB-REGISTRO-TAB TO REG-TAB
000000           MOVE TAB-GLS-ABRV          TO WR-ABR-PLZO
000000        WHEN '14'
000000           MOVE ALL '?'               TO WR-ABR-PLZO
000000        WHEN OTHER
000000           DISPLAY 'ERROR EN ACCESO TABLA TAB (RUTUTTAB). '
000000                   'COD-RET = ' RUTWTTAB-RETURN-CODE
000000           DISPLAY 'BASE  = ' RUTWTTAB-BASE ' '
000000                   'TABLA = ' RUTWTTAB-TABLA ' '
000000                   'CLAVE = ' RUTWTTAB-CLAVE
000000           MOVE 20 TO SORT-RETURN
000000     END-EVALUATE.
000000*
000000 FU-IMPRIME.
000000** GENERA LINEA DETALLE PARA OPERACION.
000000     IF SALTO-PAG-DAPLCAR
000000        PERFORM FV-TITULOS.
000000     MOVE WR-GLS-NOMC           TO DET-01-GLS-NOMC
000000     MOVE DBC-NUM-ICLI          TO DET-01-NUM-ICLI
000000     MOVE DBC-VRF-ICLI          TO DET-01-VRF-ICLI
000000     IF FSORT-IND-MCLI NOT = 'S'
000000        MOVE ' '                TO FSORT-IND-MCLI.
000000     MOVE FSORT-IND-MCLI        TO DET-01-IND-MCLI
000000     MOVE FSORT-CAI-IOPD        TO DET-01-CAI-IOPD
000000     MOVE FSORT-IIC-IOPD        TO DET-01-IIC-IOPD
000000     MOVE FSORT-NUM-DREA        TO DET-01-NUM-DREA
000000     MOVE FSORT-NUM-MREA        TO DET-01-NUM-MREA
000000     MOVE FSORT-NUM-SREA        TO DET-01-NUM-SREA
000000     MOVE FSORT-NUM-AREA        TO DET-01-NUM-AREA
000000     MOVE FSORT-NUM-DVEN        TO DET-01-NUM-DVEN
000000     MOVE FSORT-NUM-MVEN        TO DET-01-NUM-MVEN
000000     MOVE FSORT-NUM-SVEN        TO DET-01-NUM-SVEN
000000     MOVE FSORT-NUM-AVEN        TO DET-01-NUM-AVEN
000000     MOVE FSORT-SGV-TINT        TO DET-01-SGV-TINT
000000     MOVE WR-GLS-TINT           TO DET-01-GLS-TINT
000000     MOVE FSORT-VAL-SINT        TO DET-01-SGV-TPER
000000     MOVE FSORT-NUM-DURV        TO DET-01-NUM-DURV
000000     MOVE WR-ABR-PLZO           TO DET-01-ABR-PLZO
000000     MOVE FSORT-CAI-ISER        TO DET-02-CAI-ISER
000000     MOVE FSORT-IIC-ISER        TO DET-02-IIC-ISER
000000     MOVE FSORT-VAL-CAPT        TO DET-02-VAL-CAPT
000000     MOVE FSORT-VAL-INTE        TO DET-02-VAL-INTE
000000     MOVE FSORT-VAL-FINA        TO DET-02-VAL-FINA
000000     ADD FSORT-VAL-CAPT         TO ACM-COPD-VAL-CAPT
000000     ADD FSORT-VAL-INTE         TO ACM-COPD-VAL-INTE
000000     ADD FSORT-VAL-FINA         TO ACM-COPD-VAL-FINA
000000     ADD 1                      TO ACM-COPD-NUME
000000     WRITE REG-DAPLCAR FROM DET-01     AFTER 1
000000     WRITE REG-DAPLCAR FROM DET-02     AFTER 1
000000     WRITE REG-DAPLCAR FROM WR-SPACES  AFTER 1
000000     ADD 3                       TO CNT-LINE-DAPLCAR.
000000*
000000 FV-TITULOS.
000000     ADD 1                       TO CNT-PAGE-DAPLCAR
000000     MOVE CNT-PAGE-DAPLCAR       TO TIT-02-NUM-PAGE
000000     WRITE REG-DAPLCAR FROM TIT-01 AFTER CANAL-1
000000     WRITE REG-DAPLCAR FROM TIT-02 AFTER 1
000000     WRITE REG-DAPLCAR FROM TIT-03 AFTER 1
000000     WRITE REG-DAPLCAR FROM TIT-05 AFTER 2
000000     WRITE REG-DAPLCAR FROM TIT-06 AFTER 1
000000     WRITE REG-DAPLCAR FROM TIT-07 AFTER 1
000000     WRITE REG-DAPLCAR FROM TIT-08 AFTER 1
000000     WRITE REG-DAPLCAR FROM TIT-06 AFTER 1
000000     MOVE 10                     TO CNT-LINE-DAPLCAR.
000000*
000000 FZZ-FIN-SECC.
000000     EXIT.
000000     EJECT

