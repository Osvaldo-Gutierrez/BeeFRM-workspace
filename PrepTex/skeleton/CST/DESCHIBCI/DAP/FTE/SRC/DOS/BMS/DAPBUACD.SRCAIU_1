       SET-TAG-DAPBUACD SECTION.
       INI-SET-TAG-DAPBUACD.
           MOVE
           '<IDNMOD NOM=$DAPBUACD$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$18-Jun-10 11:37 AM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-DAPBUACD.
           EXIT.
           
       RET-ALL-ACD SECTION.
       INI-RET-ALL-ACD.
           PERFORM KEY-ALL-ACD.
           PERFORM RET-CAD-ACD.
       FIN-RET-ALL-ACD.
           EXIT.
       
       RET-CAD-ACD SECTION.
       INI-RET-CAD-ACD.
           MOVE CAD-GLS-FLAG IN ACD-FLD TO CAD-GLS-FLAG IN CAD.
           MOVE CAD-STP-ITRN IN ACD-FLD TO CAD-STP-ITRN IN CAD.
           MOVE CAD-COD-TACC IN ACD-FLD TO CAD-COD-TACC IN CAD.
           MOVE CAD-COD-OTRN IN ACD-FLD TO CAD-COD-OTRN IN CAD.
           MOVE CAD-COD-STAT IN ACD-FLD TO CAD-COD-STAT IN CAD.
           MOVE CAD-COD-ATRN IN ACD-FLD TO CAD-COD-ATRN IN CAD.
           MOVE CAD-IND-TCAD IN ACD-FLD TO CAD-IND-TCAD IN CAD.
           MOVE CAD-CAI-ICAD IN ACD-FLD TO CAD-CAI-ICAD IN CAD.
           MOVE CAD-IIC-ICAD IN ACD-FLD TO CAD-IIC-ICAD IN CAD.
           MOVE CAD-SGV-LIQU IN ACD-FLD TO CAD-SGV-LIQU IN CAD.
           MOVE CAD-IND-MAUT IN ACD-FLD TO CAD-IND-MAUT IN CAD.
           MOVE CAD-COD-OFIC IN ACD-FLD TO CAD-COD-OFIC IN CAD.
           MOVE CAD-GLS-DISP IN ACD-FLD TO CAD-GLS-DISP IN CAD.
       FIN-RET-CAD-ACD.
           EXIT.
       
       KEY-ALL-ACD SECTION.
       INI-KEY-ALL-ACD.
           PERFORM KEY-CAD-ACD.
       FIN-KEY-ALL-ACD.
           EXIT.
       
       KEY-CAD-ACD SECTION.
       INI-KEY-CAD-ACD.
           MOVE CAD-CAI-IOPD IN ACD-FLD TO CAD-CAI-IOPD IN CAD.
           MOVE CAD-IIC-IOPD IN ACD-FLD TO CAD-IIC-IOPD IN CAD.
           MOVE CAD-NUM-ILIQ IN ACD-FLD TO CAD-NUM-ILIQ IN CAD.
           MOVE CAD-NUM-ICAD IN ACD-FLD TO CAD-NUM-ICAD IN CAD.
       FIN-KEY-CAD-ACD.
           EXIT.
       
       SCR-CLRA-ACD SECTION.
       INI-SCR-CLRA-ACD.
           PERFORM CLR-KEY-ACD.
           PERFORM CLR-FLD-ACD.
       FIN-SCR-CLRA-ACD.
           EXIT.
       
       CLR-KEY-ACD SECTION.
       INI-CLR-KEY-ACD.
           MOVE SPACES TO CAD-CAI-IOPD IN ACD-FLD.
           MOVE ZEROES TO CAD-IIC-IOPD IN ACD-FLD.
           MOVE ZEROES TO CAD-NUM-ILIQ IN ACD-FLD.
           MOVE ZEROES TO CAD-NUM-ICAD IN ACD-FLD.
       FIN-CLR-KEY-ACD.
           EXIT.
       
       CLR-FLD-ACD SECTION.
       INI-CLR-FLD-ACD.
           PERFORM CLR-CAD-ACD.
       FIN-CLR-FLD-ACD.
           EXIT.
       
       CLR-CAD-ACD SECTION.
       INI-CLR-CAD-ACD.
           MOVE SPACES TO CAD-GLS-FLAG IN ACD-FLD.
           MOVE SPACES TO CAD-STP-ITRN IN ACD-FLD.
           MOVE SPACES TO CAD-COD-TACC IN ACD-FLD.
           MOVE SPACES TO CAD-COD-OTRN IN ACD-FLD.
           MOVE SPACES TO CAD-COD-STAT IN ACD-FLD.
           MOVE SPACES TO CAD-COD-ATRN IN ACD-FLD.
           MOVE SPACES TO CAD-IND-TCAD IN ACD-FLD.
           MOVE SPACES TO CAD-CAI-ICAD IN ACD-FLD.
           MOVE ZEROES TO CAD-IIC-ICAD IN ACD-FLD.
           MOVE ZEROES TO CAD-SGV-LIQU IN ACD-FLD.
           MOVE SPACES TO CAD-IND-MAUT IN ACD-FLD.
           MOVE SPACES TO CAD-COD-OFIC IN ACD-FLD.
           MOVE SPACES TO CAD-GLS-DISP IN ACD-FLD.
       FIN-CLR-CAD-ACD.
           EXIT.
       
       FST-KEY-ACD SECTION.
       INI-FST-KEY-ACD.
           MOVE -1 TO CAD-CAI-IOPD-LEN IN ACD-FLD.
       FIN-FST-KEY-ACD.
           EXIT.
       
       FST-FLD-ACD SECTION.
       INI-FST-FLD-ACD.
           MOVE -1 TO CAD-GLS-FLAG-LEN IN ACD-FLD.
       FIN-FST-FLD-ACD.
           EXIT.
       
       PRO-KEY-ACD SECTION.
       INI-PRO-KEY-ACD.
           PERFORM PRO-MKY-ACD.
       FIN-PRO-KEY-ACD.
           EXIT.
       
       PRO-MKY-ACD SECTION.
       INI-PRO-MKY-ACD.
       FIN-PRO-MKY-ACD.
           EXIT.
       
       PRO-FLD-ACD SECTION.
       INI-PRO-FLD-ACD.
           PERFORM PRO-CAD-ACD.
       FIN-PRO-FLD-ACD.
           EXIT.
       
       PRO-CAD-ACD SECTION.
       INI-PRO-CAD-ACD.
           MOVE FRM-CPIM-AEY TO CAD-GLS-FLAG-ATR IN ACD-FLD.
           MOVE FRM-CPIM-AEY TO CAD-STP-ITRN-ATR IN ACD-FLD.
           MOVE FRM-CPIM-AEY TO CAD-COD-TACC-ATR IN ACD-FLD.
           MOVE FRM-CPIM-AEY TO CAD-COD-OTRN-ATR IN ACD-FLD.
           MOVE FRM-CPIM-AEY TO CAD-COD-STAT-ATR IN ACD-FLD.
           MOVE FRM-CPIM-AEY TO CAD-COD-ATRN-ATR IN ACD-FLD.
           MOVE FRM-CPIM-AEY TO CAD-IND-TCAD-ATR IN ACD-FLD.
           MOVE FRM-CPIM-AEY TO CAD-CAI-ICAD-ATR IN ACD-FLD.
           MOVE FRM-CPIM-AEY TO CAD-IIC-ICAD-ATR IN ACD-FLD.
           MOVE FRM-CPIM-AEY TO CAD-SGV-LIQU-ATR IN ACD-FLD.
           MOVE FRM-CPIM-AEY TO CAD-IND-MAUT-ATR IN ACD-FLD.
           MOVE FRM-CPIM-AEY TO CAD-COD-OFIC-ATR IN ACD-FLD.
           MOVE FRM-CPIM-AEY TO CAD-GLS-DISP-ATR IN ACD-FLD.
       FIN-PRO-CAD-ACD.
           EXIT.
       
       UNP-KEY-ACD SECTION.
       INI-UNP-KEY-ACD.
           PERFORM UNP-IKY-ACD.
           PERFORM UNP-AKY-ACD.
           PERFORM UNP-MKY-ACD.
       FIN-UNP-KEY-ACD.
           EXIT.
       
       UNP-IKY-ACD SECTION.
       INI-UNP-IKY-ACD.
           MOVE FRM-CPIM-UBY TO CAD-CAI-IOPD-ATR.
           MOVE FRM-CPIM-UBY TO CAD-IIC-IOPD-ATR.
           MOVE FRM-CPIM-UBY TO CAD-NUM-ILIQ-ATR.
           MOVE FRM-CPIM-UBY TO CAD-NUM-ICAD-ATR.
       FIN-UNP-IKY-ACD.
           EXIT.
       
       UNP-MKY-ACD SECTION.
       INI-UNP-MKY-ACD.
       FIN-UNP-MKY-ACD.
           EXIT.
       
       UNP-AKY-ACD SECTION.
       INI-UNP-AKY-ACD.
       FIN-UNP-AKY-ACD.
           EXIT.
       
       UNP-FLD-ACD SECTION.
       INI-UNP-FLD-ACD.
           PERFORM UNP-CAD-ACD.
       FIN-UNP-FLD-ACD.
           EXIT.
       
       UNP-CAD-ACD SECTION.
       INI-UNP-CAD-ACD.
           MOVE FRM-CPIM-UBY TO CAD-GLS-FLAG-ATR IN ACD-FLD.
           MOVE FRM-CPIM-UBY TO CAD-STP-ITRN-ATR IN ACD-FLD.
           MOVE FRM-CPIM-UBY TO CAD-COD-TACC-ATR IN ACD-FLD.
           MOVE FRM-CPIM-UBY TO CAD-COD-OTRN-ATR IN ACD-FLD.
           MOVE FRM-CPIM-UBY TO CAD-COD-STAT-ATR IN ACD-FLD.
           MOVE FRM-CPIM-UBY TO CAD-COD-ATRN-ATR IN ACD-FLD.
           MOVE FRM-CPIM-UBY TO CAD-IND-TCAD-ATR IN ACD-FLD.
           MOVE FRM-CPIM-UBY TO CAD-CAI-ICAD-ATR IN ACD-FLD.
           MOVE FRM-CPIM-UBY TO CAD-IIC-ICAD-ATR IN ACD-FLD.
           MOVE FRM-CPIM-UBY TO CAD-SGV-LIQU-ATR IN ACD-FLD.
           MOVE FRM-CPIM-UBY TO CAD-IND-MAUT-ATR IN ACD-FLD.
           MOVE FRM-CPIM-UBY TO CAD-COD-OFIC-ATR IN ACD-FLD.
           MOVE FRM-CPIM-UBY TO CAD-GLS-DISP-ATR IN ACD-FLD.
       FIN-UNP-CAD-ACD.
           EXIT.
       
       VAL-MKY-ACD SECTION.
       INI-VAL-MKY-ACD.
       FIN-VAL-MKY-ACD.
           EXIT.
       
       VAL-FLD-ACD SECTION.
       INI-VAL-FLD-ACD.
           PERFORM VAL-CAD-GLS-FLAG.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-GLS-FLAG-LEN IN ACD-FLD
                GO TO FIN-VAL-FLD-ACD.
           PERFORM VAL-CAD-STP-ITRN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-STP-ITRN-LEN IN ACD-FLD
                GO TO FIN-VAL-FLD-ACD.
           PERFORM VAL-CAD-COD-TACC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-COD-TACC-LEN IN ACD-FLD
                GO TO FIN-VAL-FLD-ACD.
           PERFORM VAL-CAD-COD-OTRN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-COD-OTRN-LEN IN ACD-FLD
                GO TO FIN-VAL-FLD-ACD.
           PERFORM VAL-CAD-COD-STAT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-COD-STAT-LEN IN ACD-FLD
                GO TO FIN-VAL-FLD-ACD.
           PERFORM VAL-CAD-COD-ATRN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-COD-ATRN-LEN IN ACD-FLD
                GO TO FIN-VAL-FLD-ACD.
           PERFORM VAL-CAD-IND-TCAD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-IND-TCAD-LEN IN ACD-FLD
                GO TO FIN-VAL-FLD-ACD.
           PERFORM VAL-CAD-CAI-ICAD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-CAI-ICAD-LEN IN ACD-FLD
                GO TO FIN-VAL-FLD-ACD.
           PERFORM VAL-CAD-IIC-ICAD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-IIC-ICAD-LEN IN ACD-FLD
                GO TO FIN-VAL-FLD-ACD.
           PERFORM VAL-CAD-SGV-LIQU.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-SGV-LIQU-LEN IN ACD-FLD
                GO TO FIN-VAL-FLD-ACD.
           PERFORM VAL-CAD-IND-MAUT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-IND-MAUT-LEN IN ACD-FLD
                GO TO FIN-VAL-FLD-ACD.
           PERFORM VAL-CAD-COD-OFIC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-COD-OFIC-LEN IN ACD-FLD
                GO TO FIN-VAL-FLD-ACD.
           PERFORM VAL-CAD-GLS-DISP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-GLS-DISP-LEN IN ACD-FLD
                GO TO FIN-VAL-FLD-ACD.
       FIN-VAL-FLD-ACD.
           EXIT.
       
       VAL-CAD-GLS-FLAG SECTION.
       INI-VAL-CAD-GLS-FLAG.
           IF CAD-GLS-FLAG IN ACD-FLD NOT > SPACES
              MOVE SPACES TO CAD-GLS-FLAG IN ACD-FLD.
       FIN-VAL-CAD-GLS-FLAG.
           EXIT.
       
       VAL-CAD-STP-ITRN SECTION.
       INI-VAL-CAD-STP-ITRN.
           IF CAD-STP-ITRN IN ACD-FLD NOT > SPACES
              MOVE SPACES TO CAD-STP-ITRN IN ACD-FLD.
       FIN-VAL-CAD-STP-ITRN.
           EXIT.
       
       VAL-CAD-COD-TACC SECTION.
       INI-VAL-CAD-COD-TACC.
           IF CAD-COD-TACC IN ACD-FLD NOT > SPACES
              MOVE SPACES TO CAD-COD-TACC IN ACD-FLD.
       FIN-VAL-CAD-COD-TACC.
           EXIT.
       
       VAL-CAD-COD-OTRN SECTION.
       INI-VAL-CAD-COD-OTRN.
           IF CAD-COD-OTRN IN ACD-FLD NOT > SPACES
              MOVE SPACES TO CAD-COD-OTRN IN ACD-FLD.
       FIN-VAL-CAD-COD-OTRN.
           EXIT.
       
       VAL-CAD-COD-STAT SECTION.
       INI-VAL-CAD-COD-STAT.
           IF CAD-COD-STAT IN ACD-FLD NOT > SPACES
              MOVE SPACES TO CAD-COD-STAT IN ACD-FLD.
       FIN-VAL-CAD-COD-STAT.
           EXIT.
       
       VAL-CAD-COD-ATRN SECTION.
       INI-VAL-CAD-COD-ATRN.
           IF CAD-COD-ATRN IN ACD-FLD NOT > SPACES
              MOVE SPACES TO CAD-COD-ATRN IN ACD-FLD.
       FIN-VAL-CAD-COD-ATRN.
           EXIT.
       
       VAL-CAD-IND-TCAD SECTION.
       INI-VAL-CAD-IND-TCAD.
           IF CAD-IND-TCAD IN ACD-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-CAD-IND-TCAD.
      *VAL-IND Valida miscelaneo de tabla
           MOVE 'TAB' TO MSC-COD-SIST.
           MOVE 'TCYA' TO MSC-COD-TMSC IN MSC.
           MOVE CAD-IND-TCAD IN ACD-FLD TO MSC-COD-CMSC IN MSC.
           PERFORM VAL-IND.
       FIN-VAL-CAD-IND-TCAD.
           EXIT.
       
       VAL-CAD-CAI-ICAD SECTION.
       INI-VAL-CAD-CAI-ICAD.
           IF CAD-CAI-ICAD IN ACD-FLD NOT > SPACES
              MOVE SPACES TO CAD-CAI-ICAD IN ACD-FLD.
       FIN-VAL-CAD-CAI-ICAD.
           EXIT.
       
       VAL-CAD-IIC-ICAD SECTION.
       INI-VAL-CAD-IIC-ICAD.
       FIN-VAL-CAD-IIC-ICAD.
           EXIT.
       
       VAL-CAD-SGV-LIQU SECTION.
       INI-VAL-CAD-SGV-LIQU.
       FIN-VAL-CAD-SGV-LIQU.
           EXIT.
       
       VAL-CAD-IND-MAUT SECTION.
       INI-VAL-CAD-IND-MAUT.
           IF CAD-IND-MAUT IN ACD-FLD NOT > SPACES
              MOVE SPACES TO CAD-IND-MAUT IN ACD-FLD.
       FIN-VAL-CAD-IND-MAUT.
           EXIT.
       
       VAL-CAD-COD-OFIC SECTION.
       INI-VAL-CAD-COD-OFIC.
           IF CAD-COD-OFIC IN ACD-FLD NOT > SPACES
              MOVE SPACES TO CAD-COD-OFIC IN ACD-FLD.
       FIN-VAL-CAD-COD-OFIC.
           EXIT.
       
       VAL-CAD-GLS-DISP SECTION.
       INI-VAL-CAD-GLS-DISP.
           IF CAD-GLS-DISP IN ACD-FLD NOT > SPACES
              MOVE SPACES TO CAD-GLS-DISP IN ACD-FLD.
       FIN-VAL-CAD-GLS-DISP.
           EXIT.
       
       VAL-CON-FLD-ACD SECTION.
       INI-VAL-CON-FLD-ACD.
       FIN-VAL-CON-FLD-ACD.
           EXIT.
       
       VAL-CON-ACD SECTION.
       INI-VAL-CON-ACD.
       FIN-VAL-CON-ACD.
           EXIT.
       
       VAL-NUM-KEY-ACD SECTION.
       INI-VAL-NUM-KEY-ACD.
           PERFORM VAL-NUM-IKY-ACD.
           PERFORM VAL-NUM-MKY-ACD.
           PERFORM VAL-NUM-AKY-ACD.
       FIN-VAL-NUM-KEY-ACD.
           EXIT.
       
       VAL-NUM-IKY-ACD SECTION.
       INI-VAL-NUM-IKY-ACD.
       VAL-NUM-CAD-IIC-IOPD.
           MOVE CAD-IIC-IOPD-ALF IN ACD-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CAD-IIC-IOPD IN ACD-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CAD-IIC-IOPD-LEN IN ACD-FLD
                GO TO FIN-VAL-NUM-IKY-ACD.
       VAL-NUM-CAD-NUM-ILIQ.
           MOVE CAD-NUM-ILIQ-ALF IN ACD-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CAD-NUM-ILIQ IN ACD-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CAD-NUM-ILIQ-LEN IN ACD-FLD
                GO TO FIN-VAL-NUM-IKY-ACD.
       VAL-NUM-CAD-NUM-ICAD.
           MOVE CAD-NUM-ICAD-ALF IN ACD-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 1 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CAD-NUM-ICAD IN ACD-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CAD-NUM-ICAD-LEN IN ACD-FLD
                GO TO FIN-VAL-NUM-IKY-ACD.
       FIN-VAL-NUM-IKY-ACD.
           EXIT.
       
       VAL-NUM-MKY-ACD SECTION.
       INI-VAL-NUM-MKY-ACD.
       FIN-VAL-NUM-MKY-ACD.
           EXIT.
       
       VAL-NUM-AKY-ACD SECTION.
       INI-VAL-NUM-AKY-ACD.
       FIN-VAL-NUM-AKY-ACD.
           EXIT.
       
       VAL-NUM-FLD-ACD SECTION.
       INI-VAL-NUM-FLD-ACD.
       VAL-NUM-CAD-IIC-ICAD.
           MOVE CAD-IIC-ICAD-ALF IN ACD-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CAD-IIC-ICAD IN ACD-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CAD-IIC-ICAD-LEN IN ACD-FLD
                GO TO FIN-VAL-NUM-FLD-ACD.
       VAL-NUM-CAD-SGV-LIQU.
           MOVE CAD-SGV-LIQU-ALF IN ACD-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9V TO CAD-SGV-LIQU IN ACD-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CAD-SGV-LIQU-LEN IN ACD-FLD
                GO TO FIN-VAL-NUM-FLD-ACD.
       FIN-VAL-NUM-FLD-ACD.
           EXIT.
       
       EDT-KEY-ACD SECTION.
       INI-EDT-KEY-ACD.
           PERFORM EDT-IKY-ACD.
           PERFORM EDT-MKY-ACD.
           PERFORM EDT-AKY-ACD.
       FIN-EDT-KEY-ACD.
           EXIT.
       
       EDT-IKY-ACD SECTION.
       INI-EDT-IKY-ACD.
           IF CAD-IIC-IOPD-LEN IN ACD-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CAD-IIC-IOPD-LEN IN ACD-FLD
                GO TO FIN-EDT-IKY-ACD.
           MOVE CAD-IIC-IOPD IN ACD-FLD TO CAD-IIC-IOPD-EDT IN ACD-EDT.
           MOVE CAD-IIC-IOPD-EDT IN ACD-EDT TO 
                CAD-IIC-IOPD-ALF IN ACD-FLD.
           IF CAD-NUM-ILIQ-LEN IN ACD-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CAD-NUM-ILIQ-LEN IN ACD-FLD
                GO TO FIN-EDT-IKY-ACD.
           MOVE CAD-NUM-ILIQ IN ACD-FLD TO CAD-NUM-ILIQ-EDT IN ACD-EDT.
           MOVE CAD-NUM-ILIQ-EDT IN ACD-EDT TO 
                CAD-NUM-ILIQ-ALF IN ACD-FLD.
           IF CAD-NUM-ICAD-LEN IN ACD-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CAD-NUM-ICAD-LEN IN ACD-FLD
                GO TO FIN-EDT-IKY-ACD.
           MOVE CAD-NUM-ICAD IN ACD-FLD TO CAD-NUM-ICAD-EDT IN ACD-EDT.
           MOVE CAD-NUM-ICAD-EDT IN ACD-EDT TO 
                CAD-NUM-ICAD-ALF IN ACD-FLD.
       FIN-EDT-IKY-ACD.
           EXIT.
       
       EDT-MKY-ACD SECTION.
       INI-EDT-MKY-ACD.
       FIN-EDT-MKY-ACD.
           EXIT.
       
       EDT-AKY-ACD SECTION.
       INI-EDT-AKY-ACD.
       FIN-EDT-AKY-ACD.
           EXIT.
       
       EDT-FLD-ACD SECTION.
       INI-EDT-FLD-ACD.
           IF CAD-IIC-ICAD-LEN IN ACD-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CAD-IIC-ICAD-LEN IN ACD-FLD
                GO TO FIN-EDT-FLD-ACD.
           MOVE CAD-IIC-ICAD IN ACD-FLD TO CAD-IIC-ICAD-EDT IN ACD-EDT.
           MOVE CAD-IIC-ICAD-EDT IN ACD-EDT TO 
                CAD-IIC-ICAD-ALF IN ACD-FLD.
           IF CAD-SGV-LIQU-LEN IN ACD-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CAD-SGV-LIQU-LEN IN ACD-FLD
                GO TO FIN-EDT-FLD-ACD.
           MOVE CAD-SGV-LIQU IN ACD-FLD TO CAD-SGV-LIQU-EDT IN ACD-EDT.
           MOVE CAD-SGV-LIQU-EDT IN ACD-EDT TO 
                CAD-SGV-LIQU-ALF IN ACD-FLD.
       FIN-EDT-FLD-ACD.
           EXIT.
