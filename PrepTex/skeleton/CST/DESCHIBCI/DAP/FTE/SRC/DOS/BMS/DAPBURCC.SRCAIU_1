      *
      * DAPBURCC
      * Version 3.0.1 Nov-2005
      * 29-Jun-10 04:13 PM
      *
       SET-TAG-DAPBURCC SECTION.
       INI-SET-TAG-DAPBURCC.
           MOVE
           '<IDNMOD NOM=$DAPBURCC$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$29-Jun-10 04:13 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-DAPBURCC.
           EXIT.
           
       RET-ALL-RCC SECTION.
       INI-RET-ALL-RCC.
           PERFORM KEY-ALL-RCC.
           PERFORM RET-OPD-RCC.
           PERFORM RET-RCC-RCC.
           PERFORM RET-DBC-RCC.
           PERFORM RET-RCD-RCC.
       FIN-RET-ALL-RCC.
           EXIT.
       
       RET-OPD-RCC SECTION.
       INI-RET-OPD-RCC.
           MOVE OPD-COD-VCAM IN RCC-FLD TO OPD-COD-VCAM IN OPD.
           MOVE OPD-COD-TOPD IN RCC-FLD TO OPD-COD-TOPD IN OPD.
           MOVE OPD-COD-AOPD IN RCC-FLD TO OPD-COD-AOPD IN OPD.
       FIN-RET-OPD-RCC.
           EXIT.
       
       RET-RCC-RCC SECTION.
       INI-RET-RCC-RCC.
           MOVE RCC-NUM-DIRE IN RCC-FLD TO RCC-NUM-DIRE IN RCC.
       FIN-RET-RCC-RCC.
           EXIT.
       
       RET-DBC-RCC SECTION.
       INI-RET-DBC-RCC.
       FIN-RET-DBC-RCC.
           EXIT.
       
       RET-RCD-RCC SECTION.
       INI-RET-RCD-RCC.
           MOVE RCD-COD-LUGA IN RCC-FLD TO RCD-COD-LUGA IN RCD.
           MOVE RCD-GLS-DIRE IN RCC-FLD TO RCD-GLS-DIRE IN RCD.
           MOVE RCD-GLS-FONO IN RCC-FLD TO RCD-GLS-FONO IN RCD.
       FIN-RET-RCD-RCC.
           EXIT.
       
       KEY-ALL-RCC SECTION.
       INI-KEY-ALL-RCC.
           PERFORM KEY-OPD-RCC.
           PERFORM KEY-RCC-RCC.
           PERFORM KEY-DBC-RCC.
           PERFORM KEY-RCD-RCC.
       FIN-KEY-ALL-RCC.
           EXIT.
       
       KEY-OPD-RCC SECTION.
       INI-KEY-OPD-RCC.
       FIN-KEY-OPD-RCC.
           EXIT.
       
       KEY-RCC-RCC SECTION.
       INI-KEY-RCC-RCC.
           MOVE RCC-CAI-IOPD IN RCC-FLD TO RCC-CAI-IOPD IN RCC.
           MOVE RCC-IIC-IOPD IN RCC-FLD TO RCC-IIC-IOPD IN RCC.
           MOVE RCC-CAI-ICLI IN RCC-FLD TO RCC-CAI-ICLI IN RCC.
           MOVE RCC-IIC-ICLI IN RCC-FLD TO RCC-IIC-ICLI IN RCC.
       FIN-KEY-RCC-RCC.
           EXIT.
       
       KEY-DBC-RCC SECTION.
       INI-KEY-DBC-RCC.
           MOVE DBC-GLS-NOMC IN RCC-FLD TO DBC-GLS-NOMC IN DBC.
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN RCC-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN RCC-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN RCC-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN RCC-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
       FIN-KEY-DBC-RCC.
           EXIT.
       
       KEY-RCD-RCC SECTION.
       INI-KEY-RCD-RCC.
       FIN-KEY-RCD-RCC.
           EXIT.
       
       SCR-CLRA-RCC SECTION.
       INI-SCR-CLRA-RCC.
           PERFORM CLR-KEY-RCC.
           PERFORM CLR-FLD-RCC.
       FIN-SCR-CLRA-RCC.
           EXIT.
       
       CLR-KEY-RCC SECTION.
       INI-CLR-KEY-RCC.
           MOVE SPACES TO RCC-CAI-IOPD IN RCC-FLD.
           MOVE ZEROES TO RCC-IIC-IOPD IN RCC-FLD.
           MOVE SPACES TO DBC-GLS-NOMC IN RCC-FLD.
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN RCC-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN RCC-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN RCC-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN RCC-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO RCC-CAI-ICLI IN RCC-FLD.
           MOVE SPACES TO RCC-IIC-ICLI IN RCC-FLD.
       FIN-CLR-KEY-RCC.
           EXIT.
       
       CLR-FLD-RCC SECTION.
       INI-CLR-FLD-RCC.
           MOVE SPACES TO FRM-GLS-VCAM IN RCC-FLD.
           MOVE SPACES TO FRM-GLS-TOPD IN RCC-FLD.
           MOVE SPACES TO FRM-GLS-AOPD IN RCC-FLD.
           MOVE SPACES TO FRM-GLS-LUGA IN RCC-FLD.
           PERFORM CLR-OPD-RCC.
           PERFORM CLR-RCC-RCC.
           PERFORM CLR-DBC-RCC.
           PERFORM CLR-RCD-RCC.
       FIN-CLR-FLD-RCC.
           EXIT.
       
       CLR-OPD-RCC SECTION.
       INI-CLR-OPD-RCC.
           MOVE SPACES TO OPD-COD-VCAM IN RCC-FLD.
           MOVE SPACES TO OPD-COD-TOPD IN RCC-FLD.
           MOVE SPACES TO OPD-COD-AOPD IN RCC-FLD.
       FIN-CLR-OPD-RCC.
           EXIT.
       
       CLR-RCC-RCC SECTION.
       INI-CLR-RCC-RCC.
           MOVE ZEROES TO RCC-NUM-DIRE IN RCC-FLD.
       FIN-CLR-RCC-RCC.
           EXIT.
       
       CLR-DBC-RCC SECTION.
       INI-CLR-DBC-RCC.
       FIN-CLR-DBC-RCC.
           EXIT.
       
       CLR-RCD-RCC SECTION.
       INI-CLR-RCD-RCC.
           MOVE SPACES TO RCD-COD-LUGA IN RCC-FLD.
           MOVE SPACES TO RCD-GLS-DIRE IN RCC-FLD.
           MOVE SPACES TO RCD-GLS-FONO IN RCC-FLD.
       FIN-CLR-RCD-RCC.
           EXIT.
       
       FST-KEY-RCC SECTION.
       INI-FST-KEY-RCC.
      *<<<< INI_FST_KEY_RCC
           IF SCR-IND-ENG IN SCR-VEC-ENG(SCR-IPRO) = 'S'
               MOVE -1 TO DBC-NUM-ICLI-LEN IN RCC-FLD
               GO TO FIN-FST-KEY-RCC.
      *>>>>
           MOVE -1 TO RCC-CAI-IOPD-LEN IN RCC-FLD.
       FIN-FST-KEY-RCC.
           EXIT.
       
       FST-FLD-RCC SECTION.
       INI-FST-FLD-RCC.
           MOVE -1 TO RCC-NUM-DIRE-LEN IN RCC-FLD.
       FIN-FST-FLD-RCC.
           EXIT.
       
       PRO-KEY-RCC SECTION.
       INI-PRO-KEY-RCC.
           PERFORM PRO-MKY-RCC.
       FIN-PRO-KEY-RCC.
           EXIT.
       
       PRO-MKY-RCC SECTION.
       INI-PRO-MKY-RCC.
       FIN-PRO-MKY-RCC.
           EXIT.
       
       PRO-FLD-RCC SECTION.
       INI-PRO-FLD-RCC.
           PERFORM PRO-OPD-RCC.
           PERFORM PRO-RCC-RCC.
           PERFORM PRO-DBC-RCC.
           PERFORM PRO-RCD-RCC.
       FIN-PRO-FLD-RCC.
           EXIT.
       
       PRO-OPD-RCC SECTION.
       INI-PRO-OPD-RCC.
       FIN-PRO-OPD-RCC.
           EXIT.
       
       PRO-RCC-RCC SECTION.
       INI-PRO-RCC-RCC.
           MOVE FRM-CPIM-AEY TO RCC-NUM-DIRE-ATR IN RCC-FLD.
       FIN-PRO-RCC-RCC.
           EXIT.
       
       PRO-DBC-RCC SECTION.
       INI-PRO-DBC-RCC.
       FIN-PRO-DBC-RCC.
           EXIT.
       
       PRO-RCD-RCC SECTION.
       INI-PRO-RCD-RCC.
       FIN-PRO-RCD-RCC.
           EXIT.
       
       UNP-KEY-RCC SECTION.
       INI-UNP-KEY-RCC.
           PERFORM UNP-IKY-RCC.
           PERFORM UNP-AKY-RCC.
           PERFORM UNP-MKY-RCC.
       FIN-UNP-KEY-RCC.
           EXIT.
       
       UNP-IKY-RCC SECTION.
       INI-UNP-IKY-RCC.
           MOVE FRM-CPIM-UBY TO RCC-CAI-IOPD-ATR.
           MOVE FRM-CPIM-UBY TO RCC-IIC-IOPD-ATR.
      * IDC-INI --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO DBC-NUM-ICLI-ATR IN RCC-FLD.
           MOVE FRM-CPIM-UBY TO DBC-VRF-ICLI-ATR IN RCC-FLD.
           MOVE FRM-CPIM-UBY TO DBC-IND-ICLI-ATR IN RCC-FLD.
           MOVE FRM-CPIM-UBY TO DBC-GLS-ICLI-ATR IN RCC-FLD.
      * IDC-FIN --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
       FIN-UNP-IKY-RCC.
           EXIT.
       
       UNP-MKY-RCC SECTION.
       INI-UNP-MKY-RCC.
       FIN-UNP-MKY-RCC.
           EXIT.
       
       UNP-AKY-RCC SECTION.
       INI-UNP-AKY-RCC.
           MOVE FRM-CPIM-UBY TO DBC-GLS-NOMC-ATR.
       FIN-UNP-AKY-RCC.
           EXIT.
       
       UNP-FLD-RCC SECTION.
       INI-UNP-FLD-RCC.
           PERFORM UNP-OPD-RCC.
           PERFORM UNP-RCC-RCC.
           PERFORM UNP-DBC-RCC.
           PERFORM UNP-RCD-RCC.
       FIN-UNP-FLD-RCC.
           EXIT.
       
       UNP-OPD-RCC SECTION.
       INI-UNP-OPD-RCC.
       FIN-UNP-OPD-RCC.
           EXIT.
       
       UNP-RCC-RCC SECTION.
       INI-UNP-RCC-RCC.
           MOVE FRM-CPIM-UBY TO RCC-NUM-DIRE-ATR IN RCC-FLD.
       FIN-UNP-RCC-RCC.
           EXIT.
       
       UNP-DBC-RCC SECTION.
       INI-UNP-DBC-RCC.
       FIN-UNP-DBC-RCC.
           EXIT.
       
       UNP-RCD-RCC SECTION.
       INI-UNP-RCD-RCC.
       FIN-UNP-RCD-RCC.
           EXIT.
       
       VAL-MKY-RCC SECTION.
       INI-VAL-MKY-RCC.
       FIN-VAL-MKY-RCC.
           EXIT.
       
       VAL-FLD-RCC SECTION.
       INI-VAL-FLD-RCC.
           PERFORM VAL-OPD-COD-VCAM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPD-COD-VCAM-LEN IN RCC-FLD
                GO TO FIN-VAL-FLD-RCC.
           PERFORM VAL-OPD-COD-TOPD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPD-COD-TOPD-LEN IN RCC-FLD
                GO TO FIN-VAL-FLD-RCC.
           PERFORM VAL-OPD-COD-AOPD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPD-COD-AOPD-LEN IN RCC-FLD
                GO TO FIN-VAL-FLD-RCC.
           PERFORM VAL-FRM-GLS-VCAM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-VCAM-LEN IN RCC-FLD
                GO TO FIN-VAL-FLD-RCC.
           PERFORM VAL-FRM-GLS-TOPD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TOPD-LEN IN RCC-FLD
                GO TO FIN-VAL-FLD-RCC.
           PERFORM VAL-FRM-GLS-AOPD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-AOPD-LEN IN RCC-FLD
                GO TO FIN-VAL-FLD-RCC.
           PERFORM VAL-RCD-COD-LUGA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RCD-COD-LUGA-LEN IN RCC-FLD
                GO TO FIN-VAL-FLD-RCC.
           PERFORM VAL-RCC-NUM-DIRE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RCC-NUM-DIRE-LEN IN RCC-FLD
                GO TO FIN-VAL-FLD-RCC.
           PERFORM VAL-RCD-GLS-DIRE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RCD-GLS-DIRE-LEN IN RCC-FLD
                GO TO FIN-VAL-FLD-RCC.
           PERFORM VAL-RCD-GLS-FONO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RCD-GLS-FONO-LEN IN RCC-FLD
                GO TO FIN-VAL-FLD-RCC.
           PERFORM VAL-FRM-GLS-LUGA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-LUGA-LEN IN RCC-FLD
                GO TO FIN-VAL-FLD-RCC.
       FIN-VAL-FLD-RCC.
           EXIT.
       
       VAL-OPD-COD-VCAM SECTION.
       INI-VAL-OPD-COD-VCAM.
       FIN-VAL-OPD-COD-VCAM.
           EXIT.
       
       VAL-OPD-COD-TOPD SECTION.
       INI-VAL-OPD-COD-TOPD.
       FIN-VAL-OPD-COD-TOPD.
           EXIT.
       
       VAL-OPD-COD-AOPD SECTION.
       INI-VAL-OPD-COD-AOPD.
       FIN-VAL-OPD-COD-AOPD.
           EXIT.
       
       VAL-FRM-GLS-VCAM SECTION.
       INI-VAL-FRM-GLS-VCAM.
       FIN-VAL-FRM-GLS-VCAM.
           EXIT.
       
       VAL-FRM-GLS-TOPD SECTION.
       INI-VAL-FRM-GLS-TOPD.
       FIN-VAL-FRM-GLS-TOPD.
           EXIT.
       
       VAL-FRM-GLS-AOPD SECTION.
       INI-VAL-FRM-GLS-AOPD.
       FIN-VAL-FRM-GLS-AOPD.
           EXIT.
       
       VAL-RCD-COD-LUGA SECTION.
       INI-VAL-RCD-COD-LUGA.
       FIN-VAL-RCD-COD-LUGA.
           EXIT.
       
       VAL-RCC-NUM-DIRE SECTION.
       INI-VAL-RCC-NUM-DIRE.
      *<<<<
           GO TO CNT-NUM-DIRE.
      *>>>>
           IF RCC-NUM-DIRE IN RCC-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-RCC-NUM-DIRE.
      *<<<< RCC_NUM_DIRE
       CNT-NUM-DIRE.
           IF RCC-NUM-DIRE IN RCC-FLD > ZEROES
               MOVE RCC-CAI-ICLI IN RCC-FLD TO RCD-CAI-ICLI IN RCD
               MOVE RCC-IIC-ICLI IN RCC-FLD TO RCD-IIC-ICLI IN RCD
               MOVE RCC-NUM-DIRE IN RCC-FLD TO RCD-NUM-DIRE IN RCD
               MOVE 'RCD-KEY-IRCD' TO FIO-AKEY
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM SGC-FIO-RCD
               IF NOT FIO-STAT-OKS
                   MOVE 'COL' TO MSG-COD-SIST
                   MOVE 'DIR    NEX' TO MSG-COD-MENS            
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   GO TO FIN-VAL-RCC-NUM-DIRE
               ELSE
                   MOVE '00' TO PGM-STAT-RCD
                   PERFORM PUT-RCD-RCC
                   PERFORM VAL-LUGA
           ELSE
               MOVE RCC-CAI-ICLI IN RCC-FLD TO DBC-CAI-ICLI IN DBC
               MOVE RCC-IIC-ICLI IN RCC-FLD TO DBC-IIC-ICLI IN DBC
               PERFORM BUS-DOM
               IF RCD-GLS-DIRE IN RCD > SPACES
                   MOVE RCD-NUM-DIRE IN RCD TO RCC-NUM-DIRE IN RCC-FLD
                   MOVE '00' TO PGM-STAT-RCD
                   PERFORM PUT-RCD-RCC
                   PERFORM VAL-LUGA.
      *>>>>
       FIN-VAL-RCC-NUM-DIRE.
           EXIT.
       
       VAL-RCD-GLS-DIRE SECTION.
       INI-VAL-RCD-GLS-DIRE.
       FIN-VAL-RCD-GLS-DIRE.
           EXIT.
       
       VAL-RCD-GLS-FONO SECTION.
       INI-VAL-RCD-GLS-FONO.
       FIN-VAL-RCD-GLS-FONO.
           EXIT.
       
       VAL-FRM-GLS-LUGA SECTION.
       INI-VAL-FRM-GLS-LUGA.
       FIN-VAL-FRM-GLS-LUGA.
           EXIT.
       
       VAL-CON-FLD-RCC SECTION.
       INI-VAL-CON-FLD-RCC.
       FIN-VAL-CON-FLD-RCC.
           EXIT.
       
       VAL-CON-RCC SECTION.
       INI-VAL-CON-RCC.
       FIN-VAL-CON-RCC.
           EXIT.
       
       VAL-NUM-KEY-RCC SECTION.
       INI-VAL-NUM-KEY-RCC.
           PERFORM VAL-NUM-IKY-RCC.
           PERFORM VAL-NUM-MKY-RCC.
           PERFORM VAL-NUM-AKY-RCC.
       FIN-VAL-NUM-KEY-RCC.
           EXIT.
       
       VAL-NUM-IKY-RCC SECTION.
       INI-VAL-NUM-IKY-RCC.
       VAL-NUM-RCC-IIC-IOPD.
           MOVE RCC-IIC-IOPD-ALF IN RCC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO RCC-IIC-IOPD IN RCC-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO RCC-IIC-IOPD-LEN IN RCC-FLD
                GO TO FIN-VAL-NUM-IKY-RCC.
      * IDC-INI --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
       VAL-NUM-DBC-NUM-ICLI.
           MOVE DBC-NUM-ICLI-ALF IN RCC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO DBC-NUM-ICLI IN RCC-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO DBC-NUM-ICLI-LEN IN RCC-FLD
                GO TO FIN-VAL-NUM-IKY-RCC.
      * IDC-FIN --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
       FIN-VAL-NUM-IKY-RCC.
           EXIT.
       
       VAL-NUM-MKY-RCC SECTION.
       INI-VAL-NUM-MKY-RCC.
       FIN-VAL-NUM-MKY-RCC.
           EXIT.
       
       VAL-NUM-AKY-RCC SECTION.
       INI-VAL-NUM-AKY-RCC.
       FIN-VAL-NUM-AKY-RCC.
           EXIT.
       
       VAL-NUM-FLD-RCC SECTION.
       INI-VAL-NUM-FLD-RCC.
       VAL-NUM-RCC-NUM-DIRE.
           MOVE RCC-NUM-DIRE-ALF IN RCC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 4 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO RCC-NUM-DIRE IN RCC-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO RCC-NUM-DIRE-LEN IN RCC-FLD
                GO TO FIN-VAL-NUM-FLD-RCC.
       FIN-VAL-NUM-FLD-RCC.
           EXIT.
       
       EDT-KEY-RCC SECTION.
       INI-EDT-KEY-RCC.
           PERFORM EDT-IKY-RCC.
           PERFORM EDT-MKY-RCC.
           PERFORM EDT-AKY-RCC.
       FIN-EDT-KEY-RCC.
           EXIT.
       
       EDT-IKY-RCC SECTION.
       INI-EDT-IKY-RCC.
           IF RCC-IIC-IOPD-LEN IN RCC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO RCC-IIC-IOPD-LEN IN RCC-FLD
                GO TO FIN-EDT-IKY-RCC.
           MOVE RCC-IIC-IOPD IN RCC-FLD TO RCC-IIC-IOPD-EDT IN RCC-EDT.
           MOVE RCC-IIC-IOPD-EDT IN RCC-EDT TO 
                RCC-IIC-IOPD-ALF IN RCC-FLD.
      * IDC-INI --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF DBC-NUM-ICLI-LEN IN RCC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DBC-NUM-ICLI-LEN IN RCC-FLD
                GO TO FIN-EDT-IKY-RCC.
           MOVE DBC-NUM-ICLI IN RCC-FLD TO
                DBC-NUM-ICLI-EDT IN RCC-EDT.
           MOVE DBC-NUM-ICLI-EDT IN RCC-EDT TO
                DBC-NUM-ICLI-ALF IN RCC-FLD.
      * IDC-FIN --> BMCHIBCI011.SRCAIU EDICION NUMERICA
       FIN-EDT-IKY-RCC.
           EXIT.
       
       EDT-MKY-RCC SECTION.
       INI-EDT-MKY-RCC.
       FIN-EDT-MKY-RCC.
           EXIT.
       
       EDT-AKY-RCC SECTION.
       INI-EDT-AKY-RCC.
       FIN-EDT-AKY-RCC.
           EXIT.
       
       EDT-FLD-RCC SECTION.
       INI-EDT-FLD-RCC.
           IF RCC-NUM-DIRE-LEN IN RCC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO RCC-NUM-DIRE-LEN IN RCC-FLD
                GO TO FIN-EDT-FLD-RCC.
           MOVE RCC-NUM-DIRE IN RCC-FLD TO RCC-NUM-DIRE-EDT IN RCC-EDT.
           MOVE RCC-NUM-DIRE-EDT IN RCC-EDT TO 
                RCC-NUM-DIRE-ALF IN RCC-FLD.
       FIN-EDT-FLD-RCC.
           EXIT.
      *<<<< EOF

       VAL-LUGA SECTION.
       INI-VAL-LUGA.
           MOVE RCD-COD-LUGA IN RCC-FLD TO WSS-COD-LUGA IN WSS-VARI.
           IF WSS-IND-LUGA IN WSS-VARI = 'P'
               MOVE 'PAI' TO TAB-COD-TTAB
               MOVE WSS-COD-PAIS IN WSS-VARI TO TAB-COD-CTAB IN TAB
           ELSE
               MOVE 'COM' TO TAB-COD-TTAB
               MOVE WSS-COD-LUGA IN WSS-VARI TO TAB-COD-CTAB IN TAB.
           MOVE 'TAB' TO TAB-COD-SIST.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT = 'COD    NEX' AND
              RCD-COD-LUGA IN RCC-FLD > SPACES
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-LUGA IN RCC-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-LUGA IN RCC-FLD.
       FIN-VAL-LUGA.
           EXIT.
      *>>>>
