      *
      * DAPBQLIC
      * Version 3.0.1 Nov-2005
      * 22-Jun-10 12:16 PM
      *
       SET-TAG-DAPBQLIC SECTION.
       INI-SET-TAG-DAPBQLIC.
           MOVE
           '<IDNMOD NOM=$DAPBQLIC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$22-Jun-10 12:16 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-DAPBQLIC.
           EXIT.
           
       PUT-ALL-LIC SECTION.
       INI-PUT-ALL-LIC.
           PERFORM PUT-LIC-LIC.
       FIN-PUT-ALL-LIC.
           EXIT.
       
       PUT-LIC-LIC SECTION.
       INI-PUT-LIC-LIC.
           IF PGM-STAT-LIC-NUL
               GO TO FIN-PUT-LIC-LIC.
           IF NOT PGM-STAT-LIC-OKS
               PERFORM CLR-LIC-LIC
               GO TO FIN-PUT-LIC-LIC.
           MOVE LIC-GLS-FLAG IN LIC TO LIC-GLS-FLAG IN LIC-FLD.
           MOVE LIC-CAI-ISER IN LIC TO LIC-CAI-ISER IN LIC-FLD.
           MOVE LIC-IIC-ISER IN LIC TO LIC-IIC-ISER IN LIC-FLD.
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE LIC-NUM-DLIQ IN LIC TO LIC-NUM-DLIQ IN LIC-FLD.
           MOVE LIC-NUM-MLIQ IN LIC TO LIC-NUM-MLIQ IN LIC-FLD.
           MOVE LIC-NUM-SLIQ IN LIC TO LIC-NUM-SLIQ IN LIC-FLD.
           MOVE LIC-NUM-ALIQ IN LIC TO LIC-NUM-ALIQ IN LIC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE LIC-COD-STAT IN LIC TO LIC-COD-STAT IN LIC-FLD.
           MOVE LIC-COD-OFIC IN LIC TO LIC-COD-OFIC IN LIC-FLD.
           MOVE LIC-COD-VCAM IN LIC TO LIC-COD-VCAM IN LIC-FLD.
           MOVE LIC-SGV-TPAG IN LIC TO LIC-SGV-TPAG IN LIC-FLD.
           MOVE LIC-GLS-DISP IN LIC TO LIC-GLS-DISP IN LIC-FLD.
           MOVE LIC-CAI-IOPD IN LIC TO LIC-CAI-IOPD IN LIC-FLD.
           MOVE LIC-IIC-IOPD IN LIC TO LIC-IIC-IOPD IN LIC-FLD.
       FIN-PUT-LIC-LIC.
           EXIT.
       
       KEY-ALL-LIC SECTION.
       INI-KEY-ALL-LIC.
           PERFORM KEY-LIC-LIC.
       FIN-KEY-ALL-LIC.
           EXIT.
       
       KEY-LIC-LIC SECTION.
       INI-KEY-LIC-LIC.
           MOVE LIC-CAI-ISER IN LIC-FLD TO LIC-CAI-ISER IN LIC.
           MOVE LIC-IIC-ISER IN LIC-FLD TO LIC-IIC-ISER IN LIC.
       FIN-KEY-LIC-LIC.
           EXIT.
       
       SCR-CLRA-LIC SECTION.
       INI-SCR-CLRA-LIC.
           PERFORM CLR-KEY-LIC.
           PERFORM CLR-FLD-LIC.
       FIN-SCR-CLRA-LIC.
           EXIT.
       
       CLR-KEY-LIC SECTION.
       INI-CLR-KEY-LIC.
           MOVE SPACES TO LIC-CAI-ISER IN LIC-FLD.
           MOVE ZEROES TO LIC-IIC-ISER IN LIC-FLD.
       FIN-CLR-KEY-LIC.
           EXIT.
       
       CLR-FLD-LIC SECTION.
       INI-CLR-FLD-LIC.
           PERFORM CLR-LIC-LIC.
       FIN-CLR-FLD-LIC.
           EXIT.
       
       CLR-LIC-LIC SECTION.
       INI-CLR-LIC-LIC.
      *<<<<
           MOVE SPACES TO LIC-GLS-FLAG IN LIC-FLD.                      
           MOVE SPACES TO LIC-FEC-FLIQ IN LIC-FLD.                      
           MOVE SPACES TO LIC-COD-STAT IN LIC-FLD.                      
           MOVE SPACES TO LIC-COD-OFIC IN LIC-FLD.                      
           MOVE SPACES TO LIC-COD-VCAM IN LIC-FLD.                      
           MOVE ZEROES TO LIC-SGV-TPAG IN LIC-FLD.                      
           MOVE SPACES TO LIC-GLS-DISP IN LIC-FLD.                      
           MOVE SPACES TO LIC-CAI-IOPD IN LIC-FLD.                      
           MOVE ZEROES TO LIC-IIC-IOPD IN LIC-FLD.                      
           GO TO FIN-CLR-LIC-LIC.
      *>>>>
           MOVE SPACES TO LIC-GLS-FLAG IN LIC-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO LIC-FEC-FLIQ IN LIC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO LIC-COD-STAT IN LIC-FLD.
           MOVE SPACES TO LIC-COD-OFIC IN LIC-FLD.
           MOVE SPACES TO LIC-COD-VCAM IN LIC-FLD.
           MOVE ZEROES TO LIC-SGV-TPAG IN LIC-FLD.
           MOVE SPACES TO LIC-GLS-DISP IN LIC-FLD.
           MOVE SPACES TO LIC-CAI-IOPD IN LIC-FLD.
           MOVE ZEROES TO LIC-IIC-IOPD IN LIC-FLD.
       FIN-CLR-LIC-LIC.
           EXIT.
       
       FST-KEY-LIC SECTION.
       INI-FST-KEY-LIC.
           MOVE -1 TO LIC-CAI-ISER-LEN IN LIC-FLD.
       FIN-FST-KEY-LIC.
           EXIT.
       
       FST-FLD-LIC SECTION.
       INI-FST-FLD-LIC.
           MOVE -1 TO LIC-GLS-FLAG-LEN IN LIC-FLD.
       FIN-FST-FLD-LIC.
           EXIT.
       
       PRO-KEY-LIC SECTION.
       INI-PRO-KEY-LIC.
           PERFORM PRO-IKY-LIC.
           PERFORM PRO-AKY-LIC.
           PERFORM PRO-MKY-LIC.
       FIN-PRO-KEY-LIC.
           EXIT.
       
       PRO-IKY-LIC SECTION.
       INI-PRO-IKY-LIC.
           MOVE FRM-CPIM-AEY TO LIC-CAI-ISER-ATR IN LIC-FLD.
           MOVE FRM-CPIM-AEY TO LIC-IIC-ISER-ATR IN LIC-FLD.
       FIN-PRO-IKY-LIC.
           EXIT.
       
       PRO-MKY-LIC SECTION.
       INI-PRO-MKY-LIC.
       FIN-PRO-MKY-LIC.
           EXIT.
       
       PRO-AKY-LIC SECTION.
       INI-PRO-AKY-LIC.
       FIN-PRO-AKY-LIC.
           EXIT.
       
       UNP-KEY-LIC SECTION.
       INI-UNP-KEY-LIC.
           PERFORM UNP-IKY-LIC.
           PERFORM UNP-AKY-LIC.
           PERFORM UNP-MKY-LIC.
       FIN-UNP-KEY-LIC.
           EXIT.
       
       UNP-IKY-LIC SECTION.
       INI-UNP-IKY-LIC.
           MOVE FRM-CPIM-UBY TO LIC-CAI-ISER-ATR.
           MOVE FRM-CPIM-UBY TO LIC-IIC-ISER-ATR.
       FIN-UNP-IKY-LIC.
           EXIT.
       
       UNP-MKY-LIC SECTION.
       INI-UNP-MKY-LIC.
       FIN-UNP-MKY-LIC.
           EXIT.
       
       UNP-AKY-LIC SECTION.
       INI-UNP-AKY-LIC.
       FIN-UNP-AKY-LIC.
           EXIT.
       
       UNP-FLD-LIC SECTION.
       INI-UNP-FLD-LIC.
           PERFORM UNP-LIC-LIC.
       FIN-UNP-FLD-LIC.
           EXIT.
       
       UNP-LIC-LIC SECTION.
       INI-UNP-LIC-LIC.
           MOVE FRM-CPIM-UBY TO LIC-GLS-FLAG-ATR IN LIC-FLD.
           MOVE FRM-CPIM-UBY TO LIC-FEC-FLIQ-ATR IN LIC-FLD.
           MOVE FRM-CPIM-UBY TO LIC-COD-STAT-ATR IN LIC-FLD.
           MOVE FRM-CPIM-UBY TO LIC-COD-OFIC-ATR IN LIC-FLD.
           MOVE FRM-CPIM-UBY TO LIC-COD-VCAM-ATR IN LIC-FLD.
           MOVE FRM-CPIM-UBY TO LIC-SGV-TPAG-ATR IN LIC-FLD.
           MOVE FRM-CPIM-UBY TO LIC-GLS-DISP-ATR IN LIC-FLD.
           MOVE FRM-CPIM-UBY TO LIC-CAI-IOPD-ATR IN LIC-FLD.
           MOVE FRM-CPIM-UBY TO LIC-IIC-IOPD-ATR IN LIC-FLD.
       FIN-UNP-LIC-LIC.
           EXIT.
       
       VAL-KEY-LIC SECTION.
       INI-VAL-KEY-LIC.
           PERFORM VAL-IKY-LIC.
           PERFORM VAL-MKY-LIC.
           PERFORM VAL-AKY-LIC.
       FIN-VAL-KEY-LIC.
           EXIT.
       
       VAL-IKY-LIC SECTION.
       INI-VAL-IKY-LIC.
           PERFORM VAL-LIC-CAI-ISER.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO LIC-CAI-ISER-LEN IN LIC-FLD
                GO TO FIN-VAL-IKY-LIC.
           PERFORM VAL-LIC-IIC-ISER.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO LIC-IIC-ISER-LEN IN LIC-FLD
                GO TO FIN-VAL-IKY-LIC.
       FIN-VAL-IKY-LIC.
           EXIT.
       
       VAL-MKY-LIC SECTION.
       INI-VAL-MKY-LIC.
       FIN-VAL-MKY-LIC.
           EXIT.
       
       VAL-AKY-LIC SECTION.
       INI-VAL-AKY-LIC.
       FIN-VAL-AKY-LIC.
           EXIT.
       
       VAL-LIC-CAI-ISER SECTION.
       INI-VAL-LIC-CAI-ISER.
           IF LIC-CAI-ISER IN LIC-FLD NOT > SPACES
              MOVE SPACES TO LIC-CAI-ISER IN LIC-FLD.
       FIN-VAL-LIC-CAI-ISER.
           EXIT.
       
       VAL-LIC-IIC-ISER SECTION.
       INI-VAL-LIC-IIC-ISER.
       FIN-VAL-LIC-IIC-ISER.
           EXIT.
       
       VAL-CON-KEY-LIC SECTION.
       INI-VAL-CON-KEY-LIC.
           IF     LIC-CAI-ISER IN LIC-FLD NOT > SPACES 
              AND LIC-IIC-ISER IN LIC-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-LIC.
       FIN-VAL-CON-KEY-LIC.
           EXIT.
       
       VAL-CON-IKY-LIC SECTION.
       INI-VAL-CON-IKY-LIC.
           IF     LIC-CAI-ISER IN LIC-FLD NOT > SPACES 
              AND LIC-IIC-ISER IN LIC-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-LIC.
       FIN-VAL-CON-IKY-LIC.
           EXIT.
       
       VAL-NUM-KEY-LIC SECTION.
       INI-VAL-NUM-KEY-LIC.
           PERFORM VAL-NUM-IKY-LIC.
           PERFORM VAL-NUM-MKY-LIC.
           PERFORM VAL-NUM-AKY-LIC.
       FIN-VAL-NUM-KEY-LIC.
           EXIT.
       
       VAL-NUM-IKY-LIC SECTION.
       INI-VAL-NUM-IKY-LIC.
       VAL-NUM-LIC-IIC-ISER.
           MOVE LIC-IIC-ISER-ALF IN LIC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO LIC-IIC-ISER IN LIC-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO LIC-IIC-ISER-LEN IN LIC-FLD
                GO TO FIN-VAL-NUM-IKY-LIC.
       FIN-VAL-NUM-IKY-LIC.
           EXIT.
       
       VAL-NUM-MKY-LIC SECTION.
       INI-VAL-NUM-MKY-LIC.
       FIN-VAL-NUM-MKY-LIC.
           EXIT.
       
       VAL-NUM-AKY-LIC SECTION.
       INI-VAL-NUM-AKY-LIC.
       FIN-VAL-NUM-AKY-LIC.
           EXIT.
       
       VAL-NUM-FLD-LIC SECTION.
       INI-VAL-NUM-FLD-LIC.
       VAL-NUM-LIC-SGV-TPAG.
           MOVE LIC-SGV-TPAG-ALF IN LIC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NS9V TO LIC-SGV-TPAG IN LIC-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO LIC-SGV-TPAG-LEN IN LIC-FLD
                GO TO FIN-VAL-NUM-FLD-LIC.
       VAL-NUM-LIC-IIC-IOPD.
           MOVE LIC-IIC-IOPD-ALF IN LIC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO LIC-IIC-IOPD IN LIC-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO LIC-IIC-IOPD-LEN IN LIC-FLD
                GO TO FIN-VAL-NUM-FLD-LIC.
       FIN-VAL-NUM-FLD-LIC.
           EXIT.
       
       EDT-KEY-LIC SECTION.
       INI-EDT-KEY-LIC.
           PERFORM EDT-IKY-LIC.
           PERFORM EDT-MKY-LIC.
           PERFORM EDT-AKY-LIC.
       FIN-EDT-KEY-LIC.
           EXIT.
       
       EDT-IKY-LIC SECTION.
       INI-EDT-IKY-LIC.
           IF LIC-IIC-ISER-LEN IN LIC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO LIC-IIC-ISER-LEN IN LIC-FLD
                GO TO FIN-EDT-IKY-LIC.
           MOVE LIC-IIC-ISER IN LIC-FLD TO LIC-IIC-ISER-EDT IN LIC-EDT.
           MOVE LIC-IIC-ISER-EDT IN LIC-EDT TO 
                LIC-IIC-ISER-ALF IN LIC-FLD.
       FIN-EDT-IKY-LIC.
           EXIT.
       
       EDT-MKY-LIC SECTION.
       INI-EDT-MKY-LIC.
       FIN-EDT-MKY-LIC.
           EXIT.
       
       EDT-AKY-LIC SECTION.
       INI-EDT-AKY-LIC.
       FIN-EDT-AKY-LIC.
           EXIT.
       
       EDT-FLD-LIC SECTION.
       INI-EDT-FLD-LIC.
           IF LIC-SGV-TPAG-LEN IN LIC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO LIC-SGV-TPAG-LEN IN LIC-FLD
                GO TO FIN-EDT-FLD-LIC.
           MOVE LIC-SGV-TPAG IN LIC-FLD TO LIC-SGV-TPAG-EDT IN LIC-EDT.
           MOVE LIC-SGV-TPAG-EDT IN LIC-EDT TO 
                LIC-SGV-TPAG-ALF IN LIC-FLD.
           IF LIC-IIC-IOPD-LEN IN LIC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO LIC-IIC-IOPD-LEN IN LIC-FLD
                GO TO FIN-EDT-FLD-LIC.
           MOVE LIC-IIC-IOPD IN LIC-FLD TO LIC-IIC-IOPD-EDT IN LIC-EDT.
           MOVE LIC-IIC-IOPD-EDT IN LIC-EDT TO 
                LIC-IIC-IOPD-ALF IN LIC-FLD.
       FIN-EDT-FLD-LIC.
           EXIT.
