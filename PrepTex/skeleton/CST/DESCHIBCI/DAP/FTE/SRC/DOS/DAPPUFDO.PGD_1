       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSG-REQA==.
       COPY GNSBRMSG.
       COPY GNSWCFIO.
       COPY GNSWVFIO.
       COPY GNSWGFRM.
       COPY GNSWGSYS.

   *<<<<WSS
       COPY SGCBRDBC.                                                   
       COPY DAPBRRCC.                                                   
       COPY DAPBRCAD.                                                   
       COPY DAPBRODF.                                                   
       COPY DAPBRRFC.                                                   
