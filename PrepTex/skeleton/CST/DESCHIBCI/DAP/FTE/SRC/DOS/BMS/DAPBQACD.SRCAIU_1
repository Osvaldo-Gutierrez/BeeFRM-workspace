      *
      * DAPBQACD
      * Version 3.0.1 Nov-2005
      * 18-Jun-10 11:37 AM
      *
       SET-TAG-DAPBQACD SECTION.
       INI-SET-TAG-DAPBQACD.
           MOVE
           '<IDNMOD NOM=$DAPBQACD$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$18-Jun-10 11:37 AM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-DAPBQACD.
           EXIT.
           
       PUT-ALL-ACD SECTION.
       INI-PUT-ALL-ACD.
           PERFORM PUT-CAD-ACD.
       FIN-PUT-ALL-ACD.
           EXIT.
       
       PUT-CAD-ACD SECTION.
       INI-PUT-CAD-ACD.
           IF PGM-STAT-CAD-NUL
               GO TO FIN-PUT-CAD-ACD.
           IF NOT PGM-STAT-CAD-OKS
               PERFORM CLR-CAD-ACD
               GO TO FIN-PUT-CAD-ACD.
           MOVE CAD-CAI-IOPD IN CAD TO CAD-CAI-IOPD IN ACD-FLD.
           MOVE CAD-IIC-IOPD IN CAD TO CAD-IIC-IOPD IN ACD-FLD.
           MOVE CAD-NUM-ILIQ IN CAD TO CAD-NUM-ILIQ IN ACD-FLD.
           MOVE CAD-NUM-ICAD IN CAD TO CAD-NUM-ICAD IN ACD-FLD.
           MOVE CAD-GLS-FLAG IN CAD TO CAD-GLS-FLAG IN ACD-FLD.
           MOVE CAD-STP-ITRN IN CAD TO CAD-STP-ITRN IN ACD-FLD.
           MOVE CAD-COD-TACC IN CAD TO CAD-COD-TACC IN ACD-FLD.
           MOVE CAD-COD-OTRN IN CAD TO CAD-COD-OTRN IN ACD-FLD.
           MOVE CAD-COD-STAT IN CAD TO CAD-COD-STAT IN ACD-FLD.
           MOVE CAD-COD-ATRN IN CAD TO CAD-COD-ATRN IN ACD-FLD.
           MOVE CAD-IND-TCAD IN CAD TO CAD-IND-TCAD IN ACD-FLD.
           MOVE CAD-CAI-ICAD IN CAD TO CAD-CAI-ICAD IN ACD-FLD.
           MOVE CAD-IIC-ICAD IN CAD TO CAD-IIC-ICAD IN ACD-FLD.
           MOVE CAD-SGV-LIQU IN CAD TO CAD-SGV-LIQU IN ACD-FLD.
           MOVE CAD-IND-MAUT IN CAD TO CAD-IND-MAUT IN ACD-FLD.
           MOVE CAD-COD-OFIC IN CAD TO CAD-COD-OFIC IN ACD-FLD.
           MOVE CAD-GLS-DISP IN CAD TO CAD-GLS-DISP IN ACD-FLD.
       FIN-PUT-CAD-ACD.
           EXIT.
       
       KEY-ALL-ACD SECTION.
       INI-KEY-ALL-ACD.
           PERFORM KEY-CAD-ACD.
       FIN-KEY-ALL-ACD.
           EXIT.
       
       KEY-CAD-ACD SECTION.
       INI-KEY-CAD-ACD.
           MOVE CAD-CAI-IOPD IN ACD-FLD TO CAD-CAI-IOPD IN CAD.
           MOVE CAD-IIC-IOPD IN ACD-FLD TO CAD-IIC-IOPD IN CAD.
           MOVE CAD-NUM-ILIQ IN ACD-FLD TO CAD-NUM-ILIQ IN CAD.
           MOVE CAD-NUM-ICAD IN ACD-FLD TO CAD-NUM-ICAD IN CAD.
       FIN-KEY-CAD-ACD.
           EXIT.
       
       SCR-CLRA-ACD SECTION.
       INI-SCR-CLRA-ACD.
           PERFORM CLR-KEY-ACD.
           PERFORM CLR-FLD-ACD.
       FIN-SCR-CLRA-ACD.
           EXIT.
       
       CLR-KEY-ACD SECTION.
       INI-CLR-KEY-ACD.
           MOVE SPACES TO CAD-CAI-IOPD IN ACD-FLD.
           MOVE ZEROES TO CAD-IIC-IOPD IN ACD-FLD.
           MOVE ZEROES TO CAD-NUM-ILIQ IN ACD-FLD.
           MOVE ZEROES TO CAD-NUM-ICAD IN ACD-FLD.
       FIN-CLR-KEY-ACD.
           EXIT.
       
       CLR-FLD-ACD SECTION.
       INI-CLR-FLD-ACD.
           PERFORM CLR-CAD-ACD.
       FIN-CLR-FLD-ACD.
           EXIT.
       
       CLR-CAD-ACD SECTION.
       INI-CLR-CAD-ACD.
           MOVE SPACES TO CAD-GLS-FLAG IN ACD-FLD.
           MOVE SPACES TO CAD-STP-ITRN IN ACD-FLD.
           MOVE SPACES TO CAD-COD-TACC IN ACD-FLD.
           MOVE SPACES TO CAD-COD-OTRN IN ACD-FLD.
           MOVE SPACES TO CAD-COD-STAT IN ACD-FLD.
           MOVE SPACES TO CAD-COD-ATRN IN ACD-FLD.
           MOVE SPACES TO CAD-IND-TCAD IN ACD-FLD.
           MOVE SPACES TO CAD-CAI-ICAD IN ACD-FLD.
           MOVE ZEROES TO CAD-IIC-ICAD IN ACD-FLD.
           MOVE ZEROES TO CAD-SGV-LIQU IN ACD-FLD.
           MOVE SPACES TO CAD-IND-MAUT IN ACD-FLD.
           MOVE SPACES TO CAD-COD-OFIC IN ACD-FLD.
           MOVE SPACES TO CAD-GLS-DISP IN ACD-FLD.
       FIN-CLR-CAD-ACD.
           EXIT.
       
       FST-KEY-ACD SECTION.
       INI-FST-KEY-ACD.
           MOVE -1 TO CAD-CAI-IOPD-LEN IN ACD-FLD.
       FIN-FST-KEY-ACD.
           EXIT.
       
       FST-FLD-ACD SECTION.
       INI-FST-FLD-ACD.
           MOVE -1 TO CAD-GLS-FLAG-LEN IN ACD-FLD.
       FIN-FST-FLD-ACD.
           EXIT.
       
       PRO-KEY-ACD SECTION.
       INI-PRO-KEY-ACD.
           PERFORM PRO-IKY-ACD.
           PERFORM PRO-AKY-ACD.
           PERFORM PRO-MKY-ACD.
       FIN-PRO-KEY-ACD.
           EXIT.
       
       PRO-IKY-ACD SECTION.
       INI-PRO-IKY-ACD.
           MOVE FRM-CPIM-AEY TO CAD-CAI-IOPD-ATR IN ACD-FLD.
           MOVE FRM-CPIM-AEY TO CAD-IIC-IOPD-ATR IN ACD-FLD.
           MOVE FRM-CPIM-AEY TO CAD-NUM-ILIQ-ATR IN ACD-FLD.
           MOVE FRM-CPIM-AEY TO CAD-NUM-ICAD-ATR IN ACD-FLD.
       FIN-PRO-IKY-ACD.
           EXIT.
       
       PRO-MKY-ACD SECTION.
       INI-PRO-MKY-ACD.
       FIN-PRO-MKY-ACD.
           EXIT.
       
       PRO-AKY-ACD SECTION.
       INI-PRO-AKY-ACD.
       FIN-PRO-AKY-ACD.
           EXIT.
       
       UNP-KEY-ACD SECTION.
       INI-UNP-KEY-ACD.
           PERFORM UNP-IKY-ACD.
           PERFORM UNP-AKY-ACD.
           PERFORM UNP-MKY-ACD.
       FIN-UNP-KEY-ACD.
           EXIT.
       
       UNP-IKY-ACD SECTION.
       INI-UNP-IKY-ACD.
           MOVE FRM-CPIM-UBY TO CAD-CAI-IOPD-ATR.
           MOVE FRM-CPIM-UBY TO CAD-IIC-IOPD-ATR.
           MOVE FRM-CPIM-UBY TO CAD-NUM-ILIQ-ATR.
           MOVE FRM-CPIM-UBY TO CAD-NUM-ICAD-ATR.
       FIN-UNP-IKY-ACD.
           EXIT.
       
       UNP-MKY-ACD SECTION.
       INI-UNP-MKY-ACD.
       FIN-UNP-MKY-ACD.
           EXIT.
       
       UNP-AKY-ACD SECTION.
       INI-UNP-AKY-ACD.
       FIN-UNP-AKY-ACD.
           EXIT.
       
       UNP-FLD-ACD SECTION.
       INI-UNP-FLD-ACD.
           PERFORM UNP-CAD-ACD.
       FIN-UNP-FLD-ACD.
           EXIT.
       
       UNP-CAD-ACD SECTION.
       INI-UNP-CAD-ACD.
           MOVE FRM-CPIM-UBY TO CAD-GLS-FLAG-ATR IN ACD-FLD.
           MOVE FRM-CPIM-UBY TO CAD-STP-ITRN-ATR IN ACD-FLD.
           MOVE FRM-CPIM-UBY TO CAD-COD-TACC-ATR IN ACD-FLD.
           MOVE FRM-CPIM-UBY TO CAD-COD-OTRN-ATR IN ACD-FLD.
           MOVE FRM-CPIM-UBY TO CAD-COD-STAT-ATR IN ACD-FLD.
           MOVE FRM-CPIM-UBY TO CAD-COD-ATRN-ATR IN ACD-FLD.
           MOVE FRM-CPIM-UBY TO CAD-IND-TCAD-ATR IN ACD-FLD.
           MOVE FRM-CPIM-UBY TO CAD-CAI-ICAD-ATR IN ACD-FLD.
           MOVE FRM-CPIM-UBY TO CAD-IIC-ICAD-ATR IN ACD-FLD.
           MOVE FRM-CPIM-UBY TO CAD-SGV-LIQU-ATR IN ACD-FLD.
           MOVE FRM-CPIM-UBY TO CAD-IND-MAUT-ATR IN ACD-FLD.
           MOVE FRM-CPIM-UBY TO CAD-COD-OFIC-ATR IN ACD-FLD.
           MOVE FRM-CPIM-UBY TO CAD-GLS-DISP-ATR IN ACD-FLD.
       FIN-UNP-CAD-ACD.
           EXIT.
       
       VAL-KEY-ACD SECTION.
       INI-VAL-KEY-ACD.
           PERFORM VAL-IKY-ACD.
           PERFORM VAL-MKY-ACD.
           PERFORM VAL-AKY-ACD.
       FIN-VAL-KEY-ACD.
           EXIT.
       
       VAL-IKY-ACD SECTION.
       INI-VAL-IKY-ACD.
           PERFORM VAL-CAD-CAI-IOPD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-CAI-IOPD-LEN IN ACD-FLD
                GO TO FIN-VAL-IKY-ACD.
           PERFORM VAL-CAD-IIC-IOPD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-IIC-IOPD-LEN IN ACD-FLD
                GO TO FIN-VAL-IKY-ACD.
           PERFORM VAL-CAD-NUM-ILIQ.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-NUM-ILIQ-LEN IN ACD-FLD
                GO TO FIN-VAL-IKY-ACD.
           PERFORM VAL-CAD-NUM-ICAD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-NUM-ICAD-LEN IN ACD-FLD
                GO TO FIN-VAL-IKY-ACD.
       FIN-VAL-IKY-ACD.
           EXIT.
       
       VAL-MKY-ACD SECTION.
       INI-VAL-MKY-ACD.
       FIN-VAL-MKY-ACD.
           EXIT.
       
       VAL-AKY-ACD SECTION.
       INI-VAL-AKY-ACD.
       FIN-VAL-AKY-ACD.
           EXIT.
       
       VAL-CAD-CAI-IOPD SECTION.
       INI-VAL-CAD-CAI-IOPD.
           IF CAD-CAI-IOPD IN ACD-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-CAD-CAI-IOPD.
       FIN-VAL-CAD-CAI-IOPD.
           EXIT.
       
       VAL-CAD-IIC-IOPD SECTION.
       INI-VAL-CAD-IIC-IOPD.
           IF CAD-IIC-IOPD IN ACD-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-CAD-IIC-IOPD.
       FIN-VAL-CAD-IIC-IOPD.
           EXIT.
       
       VAL-CAD-NUM-ILIQ SECTION.
       INI-VAL-CAD-NUM-ILIQ.
       FIN-VAL-CAD-NUM-ILIQ.
           EXIT.
       
       VAL-CAD-NUM-ICAD SECTION.
       INI-VAL-CAD-NUM-ICAD.
       FIN-VAL-CAD-NUM-ICAD.
           EXIT.
       
       VAL-CON-KEY-ACD SECTION.
       INI-VAL-CON-KEY-ACD.
           IF     CAD-CAI-IOPD IN ACD-FLD NOT > SPACES 
              AND CAD-IIC-IOPD IN ACD-FLD NOT > ZEROES 
              AND CAD-NUM-ILIQ IN ACD-FLD NOT > ZEROES 
              AND CAD-NUM-ICAD IN ACD-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-ACD.
       FIN-VAL-CON-KEY-ACD.
           EXIT.
       
       VAL-CON-IKY-ACD SECTION.
       INI-VAL-CON-IKY-ACD.
           IF     CAD-CAI-IOPD IN ACD-FLD NOT > SPACES 
              AND CAD-IIC-IOPD IN ACD-FLD NOT > ZEROES 
              AND CAD-NUM-ILIQ IN ACD-FLD NOT > ZEROES 
              AND CAD-NUM-ICAD IN ACD-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-ACD.
       FIN-VAL-CON-IKY-ACD.
           EXIT.
       
       VAL-NUM-KEY-ACD SECTION.
       INI-VAL-NUM-KEY-ACD.
           PERFORM VAL-NUM-IKY-ACD.
           PERFORM VAL-NUM-MKY-ACD.
           PERFORM VAL-NUM-AKY-ACD.
       FIN-VAL-NUM-KEY-ACD.
           EXIT.
       
       VAL-NUM-IKY-ACD SECTION.
       INI-VAL-NUM-IKY-ACD.
       VAL-NUM-CAD-IIC-IOPD.
           MOVE CAD-IIC-IOPD-ALF IN ACD-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CAD-IIC-IOPD IN ACD-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CAD-IIC-IOPD-LEN IN ACD-FLD
                GO TO FIN-VAL-NUM-IKY-ACD.
       VAL-NUM-CAD-NUM-ILIQ.
           MOVE CAD-NUM-ILIQ-ALF IN ACD-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CAD-NUM-ILIQ IN ACD-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CAD-NUM-ILIQ-LEN IN ACD-FLD
                GO TO FIN-VAL-NUM-IKY-ACD.
       VAL-NUM-CAD-NUM-ICAD.
           MOVE CAD-NUM-ICAD-ALF IN ACD-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 1 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CAD-NUM-ICAD IN ACD-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CAD-NUM-ICAD-LEN IN ACD-FLD
                GO TO FIN-VAL-NUM-IKY-ACD.
       FIN-VAL-NUM-IKY-ACD.
           EXIT.
       
       VAL-NUM-MKY-ACD SECTION.
       INI-VAL-NUM-MKY-ACD.
       FIN-VAL-NUM-MKY-ACD.
           EXIT.
       
       VAL-NUM-AKY-ACD SECTION.
       INI-VAL-NUM-AKY-ACD.
       FIN-VAL-NUM-AKY-ACD.
           EXIT.
       
       VAL-NUM-FLD-ACD SECTION.
       INI-VAL-NUM-FLD-ACD.
       VAL-NUM-CAD-IIC-ICAD.
           MOVE CAD-IIC-ICAD-ALF IN ACD-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CAD-IIC-ICAD IN ACD-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CAD-IIC-ICAD-LEN IN ACD-FLD
                GO TO FIN-VAL-NUM-FLD-ACD.
       VAL-NUM-CAD-SGV-LIQU.
           MOVE CAD-SGV-LIQU-ALF IN ACD-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9V TO CAD-SGV-LIQU IN ACD-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CAD-SGV-LIQU-LEN IN ACD-FLD
                GO TO FIN-VAL-NUM-FLD-ACD.
       FIN-VAL-NUM-FLD-ACD.
           EXIT.
       
       EDT-KEY-ACD SECTION.
       INI-EDT-KEY-ACD.
           PERFORM EDT-IKY-ACD.
           PERFORM EDT-MKY-ACD.
           PERFORM EDT-AKY-ACD.
       FIN-EDT-KEY-ACD.
           EXIT.
       
       EDT-IKY-ACD SECTION.
       INI-EDT-IKY-ACD.
           IF CAD-IIC-IOPD-LEN IN ACD-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CAD-IIC-IOPD-LEN IN ACD-FLD
                GO TO FIN-EDT-IKY-ACD.
           MOVE CAD-IIC-IOPD IN ACD-FLD TO CAD-IIC-IOPD-EDT IN ACD-EDT.
           MOVE CAD-IIC-IOPD-EDT IN ACD-EDT TO 
                CAD-IIC-IOPD-ALF IN ACD-FLD.
           IF CAD-NUM-ILIQ-LEN IN ACD-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CAD-NUM-ILIQ-LEN IN ACD-FLD
                GO TO FIN-EDT-IKY-ACD.
           MOVE CAD-NUM-ILIQ IN ACD-FLD TO CAD-NUM-ILIQ-EDT IN ACD-EDT.
           MOVE CAD-NUM-ILIQ-EDT IN ACD-EDT TO 
                CAD-NUM-ILIQ-ALF IN ACD-FLD.
           IF CAD-NUM-ICAD-LEN IN ACD-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CAD-NUM-ICAD-LEN IN ACD-FLD
                GO TO FIN-EDT-IKY-ACD.
           MOVE CAD-NUM-ICAD IN ACD-FLD TO CAD-NUM-ICAD-EDT IN ACD-EDT.
           MOVE CAD-NUM-ICAD-EDT IN ACD-EDT TO 
                CAD-NUM-ICAD-ALF IN ACD-FLD.
       FIN-EDT-IKY-ACD.
           EXIT.
       
       EDT-MKY-ACD SECTION.
       INI-EDT-MKY-ACD.
       FIN-EDT-MKY-ACD.
           EXIT.
       
       EDT-AKY-ACD SECTION.
       INI-EDT-AKY-ACD.
       FIN-EDT-AKY-ACD.
           EXIT.
       
       EDT-FLD-ACD SECTION.
       INI-EDT-FLD-ACD.
           IF CAD-IIC-ICAD-LEN IN ACD-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CAD-IIC-ICAD-LEN IN ACD-FLD
                GO TO FIN-EDT-FLD-ACD.
           MOVE CAD-IIC-ICAD IN ACD-FLD TO CAD-IIC-ICAD-EDT IN ACD-EDT.
           MOVE CAD-IIC-ICAD-EDT IN ACD-EDT TO 
                CAD-IIC-ICAD-ALF IN ACD-FLD.
           IF CAD-SGV-LIQU-LEN IN ACD-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CAD-SGV-LIQU-LEN IN ACD-FLD
                GO TO FIN-EDT-FLD-ACD.
           MOVE CAD-SGV-LIQU IN ACD-FLD TO CAD-SGV-LIQU-EDT IN ACD-EDT.
           MOVE CAD-SGV-LIQU-EDT IN ACD-EDT TO 
                CAD-SGV-LIQU-ALF IN ACD-FLD.
       FIN-EDT-FLD-ACD.
           EXIT.
