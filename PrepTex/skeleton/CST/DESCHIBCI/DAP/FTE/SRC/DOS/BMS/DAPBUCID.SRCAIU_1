               GO TO FIN-VAL-CID-FEC-FVEN.
       FIN-VAL-CID-FEC-FVEN.
           EXIT.

       VAL-CID-GLS-FLAG SECTION.
       INI-VAL-CID-GLS-FLAG.
           IF CID-GLS-FLAG IN CID-FLD NOT > SPACES
              MOVE SPACES TO CID-GLS-FLAG IN CID-FLD.
       FIN-VAL-CID-GLS-FLAG.
           EXIT.

       VAL-CID-GLS-DISP SECTION.
       INI-VAL-CID-GLS-DISP.
           IF CID-GLS-DISP IN CID-FLD NOT > SPACES
              MOVE SPACES TO CID-GLS-DISP IN CID-FLD.
       FIN-VAL-CID-GLS-DISP.
           EXIT.

       VAL-CON-FLD-CID SECTION.
       INI-VAL-CON-FLD-CID.
       FIN-VAL-CON-FLD-CID.
           EXIT.

       VAL-CON-CID SECTION.
       INI-VAL-CON-CID.
       FIN-VAL-CON-CID.
           EXIT.

       VAL-NUM-KEY-CID SECTION.
       INI-VAL-NUM-KEY-CID.
           PERFORM VAL-NUM-IKY-CID.
           PERFORM VAL-NUM-MKY-CID.
           PERFORM VAL-NUM-AKY-CID.
       FIN-VAL-NUM-KEY-CID.
           EXIT.

       VAL-NUM-IKY-CID SECTION.
       INI-VAL-NUM-IKY-CID.
       VAL-NUM-CID-IIC-IOPD.
           MOVE CID-IIC-IOPD-ALF IN CID-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CID-IIC-IOPD IN CID-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CID-IIC-IOPD-LEN IN CID-FLD
                GO TO FIN-VAL-NUM-IKY-CID.
       FIN-VAL-NUM-IKY-CID.
           EXIT.

       VAL-NUM-MKY-CID SECTION.
       INI-VAL-NUM-MKY-CID.
       FIN-VAL-NUM-MKY-CID.
           EXIT.

       VAL-NUM-AKY-CID SECTION.
       INI-VAL-NUM-AKY-CID.
       FIN-VAL-NUM-AKY-CID.
           EXIT.

       VAL-NUM-FLD-CID SECTION.
       INI-VAL-NUM-FLD-CID.
       VAL-NUM-CID-VAL-SCAP.
           MOVE CID-VAL-SCAP-ALF IN CID-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9V TO CID-VAL-SCAP IN CID-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CID-VAL-SCAP-LEN IN CID-FLD
                GO TO FIN-VAL-NUM-FLD-CID.
       VAL-NUM-CID-VAL-INTD.
           MOVE CID-VAL-INTD-ALF IN CID-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9V TO CID-VAL-INTD IN CID-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CID-VAL-INTD-LEN IN CID-FLD
                GO TO FIN-VAL-NUM-FLD-CID.
       VAL-NUM-CID-VAL-READ.
           MOVE CID-VAL-READ-ALF IN CID-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9V TO CID-VAL-READ IN CID-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CID-VAL-READ-LEN IN CID-FLD
                GO TO FIN-VAL-NUM-FLD-CID.
       VAL-NUM-CID-SGV-TINT.
           MOVE CID-SGV-TINT-ALF IN CID-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9V TO CID-SGV-TINT IN CID-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CID-SGV-TINT-LEN IN CID-FLD
                GO TO FIN-VAL-NUM-FLD-CID.
       VAL-NUM-CID-SGV-TINP.
           MOVE CID-SGV-TINP-ALF IN CID-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9V TO CID-SGV-TINP IN CID-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CID-SGV-TINP-LEN IN CID-FLD
                GO TO FIN-VAL-NUM-FLD-CID.
       VAL-NUM-CID-NUM-DIAS.
           MOVE CID-NUM-DIAS-ALF IN CID-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 5 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CID-NUM-DIAS IN CID-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CID-NUM-DIAS-LEN IN CID-FLD
                GO TO FIN-VAL-NUM-FLD-CID.
       VAL-NUM-CID-SGV-VCAM.
           MOVE CID-SGV-VCAM-ALF IN CID-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9V TO CID-SGV-VCAM IN CID-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CID-SGV-VCAM-LEN IN CID-FLD
                GO TO FIN-VAL-NUM-FLD-CID.
       FIN-VAL-NUM-FLD-CID.
           EXIT.

       EDT-KEY-CID SECTION.
       INI-EDT-KEY-CID.
           PERFORM EDT-IKY-CID.
           PERFORM EDT-MKY-CID.
           PERFORM EDT-AKY-CID.
       FIN-EDT-KEY-CID.
           EXIT.

       EDT-IKY-CID SECTION.
       INI-EDT-IKY-CID.
           IF CID-IIC-IOPD-LEN IN CID-FLD = -9
                MOVE -1 TO CID-IIC-IOPD-LEN IN CID-FLD
                GO TO FIN-EDT-IKY-CID.
           MOVE CID-IIC-IOPD IN CID-FLD TO CID-IIC-IOPD-EDT IN CID-EDT.
           MOVE CID-IIC-IOPD-EDT IN CID-EDT TO
                CID-IIC-IOPD-ALF IN CID-FLD.
       FIN-EDT-IKY-CID.
           EXIT.

       EDT-MKY-CID SECTION.
       INI-EDT-MKY-CID.
       FIN-EDT-MKY-CID.
           EXIT.

       EDT-AKY-CID SECTION.
       INI-EDT-AKY-CID.
       FIN-EDT-AKY-CID.
           EXIT.

       EDT-FLD-CID SECTION.
       INI-EDT-FLD-CID.
           IF CID-VAL-SCAP-LEN IN CID-FLD = -9
                MOVE -1 TO CID-VAL-SCAP-LEN IN CID-FLD
                GO TO FIN-EDT-FLD-CID.
           MOVE CID-VAL-SCAP IN CID-FLD TO CID-VAL-SCAP-EDT IN CID-EDT.
           MOVE CID-VAL-SCAP-EDT IN CID-EDT TO
                CID-VAL-SCAP-ALF IN CID-FLD.
           IF CID-VAL-INTD-LEN IN CID-FLD = -9
                MOVE -1 TO CID-VAL-INTD-LEN IN CID-FLD
                GO TO FIN-EDT-FLD-CID.
           MOVE CID-VAL-INTD IN CID-FLD TO CID-VAL-INTD-EDT IN CID-EDT.
           MOVE CID-VAL-INTD-EDT IN CID-EDT TO
                CID-VAL-INTD-ALF IN CID-FLD.
           IF CID-VAL-READ-LEN IN CID-FLD = -9
                MOVE -1 TO CID-VAL-READ-LEN IN CID-FLD
                GO TO FIN-EDT-FLD-CID.
           MOVE CID-VAL-READ IN CID-FLD TO CID-VAL-READ-EDT IN CID-EDT.
           MOVE CID-VAL-READ-EDT IN CID-EDT TO
                CID-VAL-READ-ALF IN CID-FLD.
           IF CID-SGV-TINT-LEN IN CID-FLD = -9
                MOVE -1 TO CID-SGV-TINT-LEN IN CID-FLD
                GO TO FIN-EDT-FLD-CID.
           MOVE CID-SGV-TINT IN CID-FLD TO CID-SGV-TINT-EDT IN CID-EDT.
           MOVE CID-SGV-TINT-EDT IN CID-EDT TO
                CID-SGV-TINT-ALF IN CID-FLD.
           IF CID-SGV-TINP-LEN IN CID-FLD = -9
                MOVE -1 TO CID-SGV-TINP-LEN IN CID-FLD
                GO TO FIN-EDT-FLD-CID.
           MOVE CID-SGV-TINP IN CID-FLD TO CID-SGV-TINP-EDT IN CID-EDT.
           MOVE CID-SGV-TINP-EDT IN CID-EDT TO
                CID-SGV-TINP-ALF IN CID-FLD.
           IF CID-NUM-DIAS-LEN IN CID-FLD = -9
                MOVE -1 TO CID-NUM-DIAS-LEN IN CID-FLD
                GO TO FIN-EDT-FLD-CID.
           MOVE CID-NUM-DIAS IN CID-FLD TO CID-NUM-DIAS-EDT IN CID-EDT.
           MOVE CID-NUM-DIAS-EDT IN CID-EDT TO
                CID-NUM-DIAS-ALF IN CID-FLD.
           IF CID-SGV-VCAM-LEN IN CID-FLD = -9
                MOVE -1 TO CID-SGV-VCAM-LEN IN CID-FLD
                GO TO FIN-EDT-FLD-CID.
           MOVE CID-SGV-VCAM IN CID-FLD TO CID-SGV-VCAM-EDT IN CID-EDT.
           MOVE CID-SGV-VCAM-EDT IN CID-EDT TO
                CID-SGV-VCAM-ALF IN CID-FLD.
       FIN-EDT-FLD-CID.
           EXIT.
