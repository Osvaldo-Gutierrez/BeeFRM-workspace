      *
      * DAPBUCCO
      * Version 3.0.1 Nov-2005
      * 12-Jul-10 02:42 PM
      *
       SET-TAG-DAPBUCCO SECTION.
       INI-SET-TAG-DAPBUCCO.
           MOVE
           '<IDNMOD NOM=$DAPBUCCO$ VSN=$08.10.20$ VGM=$3.0.1$
      -    'GEN=$12-Jul-10 02:42 PM$ FNC=$080905$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-DAPBUCCO.
           EXIT.
           
       RET-ALL-CCO SECTION.
       INI-RET-ALL-CCO.
           PERFORM KEY-ALL-CCO.
           PERFORM RET-CCO-CCO.
       FIN-RET-ALL-CCO.
           EXIT.
       
       RET-CCO-CCO SECTION.
       INI-RET-CCO-CCO.
           MOVE CCO-GLS-NMBR IN CCO-FLD TO CCO-GLS-NMBR IN CCO.
           MOVE CCO-GLS-DIRE IN CCO-FLD TO CCO-GLS-DIRE IN CCO.
           MOVE CCO-NUM-CDAD IN CCO-FLD TO CCO-NUM-CDAD IN CCO.
           MOVE CCO-GLS-CDAD IN CCO-FLD TO CCO-GLS-CDAD IN CCO.
           MOVE CCO-IND-EMIS IN CCO-FLD TO CCO-IND-EMIS IN CCO.
           MOVE CCO-NUM-FOLI IN CCO-FLD TO CCO-NUM-FOLI IN CCO.
           MOVE CCO-GLS-DISP IN CCO-FLD TO CCO-GLS-DISP IN CCO.
       FIN-RET-CCO-CCO.
           EXIT.
       
       KEY-ALL-CCO SECTION.
       INI-KEY-ALL-CCO.
           PERFORM KEY-CCO-CCO.
       FIN-KEY-ALL-CCO.
           EXIT.
       
       KEY-CCO-CCO SECTION.
       INI-KEY-CCO-CCO.
           MOVE CCO-NUM-OFIC IN CCO-FLD TO CCO-NUM-OFIC IN CCO.
           MOVE CCO-NUM-NRUT IN CCO-FLD TO CCO-NUM-NRUT IN CCO.
           MOVE CCO-COD-NRUT IN CCO-FLD TO CCO-COD-NRUT IN CCO.
       FIN-KEY-CCO-CCO.
           EXIT.
       
       SCR-CLRA-CCO SECTION.
       INI-SCR-CLRA-CCO.
           PERFORM CLR-KEY-CCO.
           PERFORM CLR-FLD-CCO.
       FIN-SCR-CLRA-CCO.
           EXIT.
       
       CLR-KEY-CCO SECTION.
       INI-CLR-KEY-CCO.
           MOVE SPACES TO FRM-COD-VOID IN CCO-FLD.
           MOVE ZEROES TO CCO-NUM-OFIC IN CCO-FLD.
           MOVE SPACES TO FRM-IND-NEXT IN CCO-FLD.
           MOVE ZEROES TO CCO-NUM-NRUT IN CCO-FLD.
           MOVE SPACES TO CCO-COD-NRUT IN CCO-FLD.
       FIN-CLR-KEY-CCO.
           EXIT.
       
       CLR-FLD-CCO SECTION.
       INI-CLR-FLD-CCO.
           PERFORM CLR-CCO-CCO.
       FIN-CLR-FLD-CCO.
           EXIT.
       
       CLR-CCO-CCO SECTION.
       INI-CLR-CCO-CCO.
           MOVE SPACES TO CCO-GLS-NMBR IN CCO-FLD.
           MOVE SPACES TO CCO-GLS-DIRE IN CCO-FLD.
           MOVE ZEROES TO CCO-NUM-CDAD IN CCO-FLD.
           MOVE SPACES TO CCO-GLS-CDAD IN CCO-FLD.
           MOVE ZEROES TO CCO-IND-EMIS IN CCO-FLD.
           MOVE ZEROES TO CCO-NUM-FOLI IN CCO-FLD.
           MOVE SPACES TO CCO-GLS-DISP IN CCO-FLD.
       FIN-CLR-CCO-CCO.
           EXIT.
       
       FST-KEY-CCO SECTION.
       INI-FST-KEY-CCO.
           MOVE -1 TO FRM-COD-VOID-LEN IN CCO-FLD.
       FIN-FST-KEY-CCO.
           EXIT.
       
       FST-FLD-CCO SECTION.
       INI-FST-FLD-CCO.
           MOVE -1 TO CCO-GLS-NMBR-LEN IN CCO-FLD.
       FIN-FST-FLD-CCO.
           EXIT.
       
       PRO-KEY-CCO SECTION.
       INI-PRO-KEY-CCO.
           MOVE FRM-CPIM-AEY TO FRM-COD-VOID-ATR IN CCO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-NEXT-ATR IN CCO-FLD.
           PERFORM PRO-MKY-CCO.
       FIN-PRO-KEY-CCO.
           EXIT.
       
       PRO-MKY-CCO SECTION.
       INI-PRO-MKY-CCO.
       FIN-PRO-MKY-CCO.
           EXIT.
       
       PRO-FLD-CCO SECTION.
       INI-PRO-FLD-CCO.
           PERFORM PRO-CCO-CCO.
       FIN-PRO-FLD-CCO.
           EXIT.
       
       PRO-CCO-CCO SECTION.
       INI-PRO-CCO-CCO.
           MOVE FRM-CPIM-AEY TO CCO-GLS-NMBR-ATR IN CCO-FLD.
           MOVE FRM-CPIM-AEY TO CCO-GLS-DIRE-ATR IN CCO-FLD.
           MOVE FRM-CPIM-AEY TO CCO-NUM-CDAD-ATR IN CCO-FLD.
           MOVE FRM-CPIM-AEY TO CCO-GLS-CDAD-ATR IN CCO-FLD.
           MOVE FRM-CPIM-AEY TO CCO-IND-EMIS-ATR IN CCO-FLD.
           MOVE FRM-CPIM-AEY TO CCO-NUM-FOLI-ATR IN CCO-FLD.
           MOVE FRM-CPIM-AEY TO CCO-GLS-DISP-ATR IN CCO-FLD.
       FIN-PRO-CCO-CCO.
           EXIT.
       
       UNP-KEY-CCO SECTION.
       INI-UNP-KEY-CCO.
           MOVE FRM-CPIM-UBY TO FRM-COD-VOID-ATR IN CCO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IND-NEXT-ATR IN CCO-FLD.
           PERFORM UNP-IKY-CCO.
           PERFORM UNP-AKY-CCO.
           PERFORM UNP-MKY-CCO.
       FIN-UNP-KEY-CCO.
           EXIT.
       
       UNP-IKY-CCO SECTION.
       INI-UNP-IKY-CCO.
           MOVE FRM-CPIM-UBY TO FRM-COD-VOID-ATR.
           MOVE FRM-CPIM-UBY TO CCO-NUM-OFIC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-IND-NEXT-ATR.
           MOVE FRM-CPIM-UBY TO CCO-NUM-NRUT-ATR.
           MOVE FRM-CPIM-UBY TO CCO-COD-NRUT-ATR.
       FIN-UNP-IKY-CCO.
           EXIT.
       
       UNP-MKY-CCO SECTION.
       INI-UNP-MKY-CCO.
       FIN-UNP-MKY-CCO.
           EXIT.
       
       UNP-AKY-CCO SECTION.
       INI-UNP-AKY-CCO.
       FIN-UNP-AKY-CCO.
           EXIT.
       
       UNP-FLD-CCO SECTION.
       INI-UNP-FLD-CCO.
           PERFORM UNP-CCO-CCO.
       FIN-UNP-FLD-CCO.
           EXIT.
       
       UNP-CCO-CCO SECTION.
       INI-UNP-CCO-CCO.
           MOVE FRM-CPIM-UBY TO CCO-GLS-NMBR-ATR IN CCO-FLD.
           MOVE FRM-CPIM-UBY TO CCO-GLS-DIRE-ATR IN CCO-FLD.
           MOVE FRM-CPIM-UBY TO CCO-NUM-CDAD-ATR IN CCO-FLD.
           MOVE FRM-CPIM-UBY TO CCO-GLS-CDAD-ATR IN CCO-FLD.
           MOVE FRM-CPIM-UBY TO CCO-IND-EMIS-ATR IN CCO-FLD.
           MOVE FRM-CPIM-UBY TO CCO-NUM-FOLI-ATR IN CCO-FLD.
           MOVE FRM-CPIM-UBY TO CCO-GLS-DISP-ATR IN CCO-FLD.
       FIN-UNP-CCO-CCO.
           EXIT.
       
       VAL-MKY-CCO SECTION.
       INI-VAL-MKY-CCO.
       FIN-VAL-MKY-CCO.
           EXIT.
       
       VAL-FLD-CCO SECTION.
       INI-VAL-FLD-CCO.
           PERFORM VAL-CCO-GLS-NMBR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CCO-GLS-NMBR-LEN IN CCO-FLD
                GO TO FIN-VAL-FLD-CCO.
           PERFORM VAL-CCO-GLS-DIRE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CCO-GLS-DIRE-LEN IN CCO-FLD
                GO TO FIN-VAL-FLD-CCO.
           PERFORM VAL-CCO-NUM-CDAD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CCO-NUM-CDAD-LEN IN CCO-FLD
                GO TO FIN-VAL-FLD-CCO.
           PERFORM VAL-CCO-GLS-CDAD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CCO-GLS-CDAD-LEN IN CCO-FLD
                GO TO FIN-VAL-FLD-CCO.
           PERFORM VAL-CCO-IND-EMIS.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CCO-IND-EMIS-LEN IN CCO-FLD
                GO TO FIN-VAL-FLD-CCO.
           PERFORM VAL-CCO-NUM-FOLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CCO-NUM-FOLI-LEN IN CCO-FLD
                GO TO FIN-VAL-FLD-CCO.
           PERFORM VAL-CCO-GLS-DISP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CCO-GLS-DISP-LEN IN CCO-FLD
                GO TO FIN-VAL-FLD-CCO.
       FIN-VAL-FLD-CCO.
           EXIT.
       
       VAL-CCO-GLS-NMBR SECTION.
       INI-VAL-CCO-GLS-NMBR.
           IF CCO-GLS-NMBR IN CCO-FLD NOT > SPACES
              MOVE SPACES TO CCO-GLS-NMBR IN CCO-FLD.
       FIN-VAL-CCO-GLS-NMBR.
           EXIT.
       
       VAL-CCO-GLS-DIRE SECTION.
       INI-VAL-CCO-GLS-DIRE.
           IF CCO-GLS-DIRE IN CCO-FLD NOT > SPACES
              MOVE SPACES TO CCO-GLS-DIRE IN CCO-FLD.
       FIN-VAL-CCO-GLS-DIRE.
           EXIT.
       
       VAL-CCO-NUM-CDAD SECTION.
       INI-VAL-CCO-NUM-CDAD.
       FIN-VAL-CCO-NUM-CDAD.
           EXIT.
       
       VAL-CCO-GLS-CDAD SECTION.
       INI-VAL-CCO-GLS-CDAD.
           IF CCO-GLS-CDAD IN CCO-FLD NOT > SPACES
              MOVE SPACES TO CCO-GLS-CDAD IN CCO-FLD.
       FIN-VAL-CCO-GLS-CDAD.
           EXIT.
       
       VAL-CCO-IND-EMIS SECTION.
       INI-VAL-CCO-IND-EMIS.
       FIN-VAL-CCO-IND-EMIS.
           EXIT.
       
       VAL-CCO-NUM-FOLI SECTION.
       INI-VAL-CCO-NUM-FOLI.
       FIN-VAL-CCO-NUM-FOLI.
           EXIT.
       
       VAL-CCO-GLS-DISP SECTION.
       INI-VAL-CCO-GLS-DISP.
           IF CCO-GLS-DISP IN CCO-FLD NOT > SPACES
              MOVE SPACES TO CCO-GLS-DISP IN CCO-FLD.
       FIN-VAL-CCO-GLS-DISP.
           EXIT.
       
       VAL-CON-FLD-CCO SECTION.
       INI-VAL-CON-FLD-CCO.
       FIN-VAL-CON-FLD-CCO.
           EXIT.
       
       VAL-CON-CCO SECTION.
       INI-VAL-CON-CCO.
       FIN-VAL-CON-CCO.
           EXIT.
       
       VAL-NUM-KEY-CCO SECTION.
       INI-VAL-NUM-KEY-CCO.
           PERFORM VAL-NUM-IKY-CCO.
           PERFORM VAL-NUM-MKY-CCO.
           PERFORM VAL-NUM-AKY-CCO.
       FIN-VAL-NUM-KEY-CCO.
           EXIT.
       
       VAL-NUM-IKY-CCO SECTION.
       INI-VAL-NUM-IKY-CCO.
       VAL-NUM-CCO-NUM-OFIC.
           MOVE CCO-NUM-OFIC-ALF IN CCO-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CCO-NUM-OFIC IN CCO-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CCO-NUM-OFIC-LEN IN CCO-FLD
                GO TO FIN-VAL-NUM-IKY-CCO.
       VAL-NUM-CCO-NUM-NRUT.
           MOVE CCO-NUM-NRUT-ALF IN CCO-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 9 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CCO-NUM-NRUT IN CCO-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CCO-NUM-NRUT-LEN IN CCO-FLD
                GO TO FIN-VAL-NUM-IKY-CCO.
       FIN-VAL-NUM-IKY-CCO.
           EXIT.
       
       VAL-NUM-MKY-CCO SECTION.
       INI-VAL-NUM-MKY-CCO.
       FIN-VAL-NUM-MKY-CCO.
           EXIT.
       
       VAL-NUM-AKY-CCO SECTION.
       INI-VAL-NUM-AKY-CCO.
       FIN-VAL-NUM-AKY-CCO.
           EXIT.
       
       VAL-NUM-FLD-CCO SECTION.
       INI-VAL-NUM-FLD-CCO.
       VAL-NUM-CCO-NUM-CDAD.
           MOVE CCO-NUM-CDAD-ALF IN CCO-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 4 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CCO-NUM-CDAD IN CCO-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CCO-NUM-CDAD-LEN IN CCO-FLD
                GO TO FIN-VAL-NUM-FLD-CCO.
       VAL-NUM-CCO-IND-EMIS.
           MOVE CCO-IND-EMIS-ALF IN CCO-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 1 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CCO-IND-EMIS IN CCO-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CCO-IND-EMIS-LEN IN CCO-FLD
                GO TO FIN-VAL-NUM-FLD-CCO.
       VAL-NUM-CCO-NUM-FOLI.
           MOVE CCO-NUM-FOLI-ALF IN CCO-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 6 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CCO-NUM-FOLI IN CCO-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CCO-NUM-FOLI-LEN IN CCO-FLD
                GO TO FIN-VAL-NUM-FLD-CCO.
       FIN-VAL-NUM-FLD-CCO.
           EXIT.
       
       EDT-KEY-CCO SECTION.
       INI-EDT-KEY-CCO.
           PERFORM EDT-IKY-CCO.
           PERFORM EDT-MKY-CCO.
           PERFORM EDT-AKY-CCO.
       FIN-EDT-KEY-CCO.
           EXIT.
       
       EDT-IKY-CCO SECTION.
       INI-EDT-IKY-CCO.
           IF CCO-NUM-OFIC-LEN IN CCO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CCO-NUM-OFIC-LEN IN CCO-FLD
                GO TO FIN-EDT-IKY-CCO.
           MOVE CCO-NUM-OFIC IN CCO-FLD TO CCO-NUM-OFIC-EDT IN CCO-EDT.
           MOVE CCO-NUM-OFIC-EDT IN CCO-EDT TO 
                CCO-NUM-OFIC-ALF IN CCO-FLD.
           IF CCO-NUM-NRUT-LEN IN CCO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CCO-NUM-NRUT-LEN IN CCO-FLD
                GO TO FIN-EDT-IKY-CCO.
           MOVE CCO-NUM-NRUT IN CCO-FLD TO CCO-NUM-NRUT-EDT IN CCO-EDT.
           MOVE CCO-NUM-NRUT-EDT IN CCO-EDT TO 
                CCO-NUM-NRUT-ALF IN CCO-FLD.
       FIN-EDT-IKY-CCO.
           EXIT.
       
       EDT-MKY-CCO SECTION.
       INI-EDT-MKY-CCO.
       FIN-EDT-MKY-CCO.
           EXIT.
       
       EDT-AKY-CCO SECTION.
       INI-EDT-AKY-CCO.
       FIN-EDT-AKY-CCO.
           EXIT.
       
       EDT-FLD-CCO SECTION.
       INI-EDT-FLD-CCO.
           IF CCO-NUM-CDAD-LEN IN CCO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CCO-NUM-CDAD-LEN IN CCO-FLD
                GO TO FIN-EDT-FLD-CCO.
           MOVE CCO-NUM-CDAD IN CCO-FLD TO CCO-NUM-CDAD-EDT IN CCO-EDT.
           MOVE CCO-NUM-CDAD-EDT IN CCO-EDT TO 
                CCO-NUM-CDAD-ALF IN CCO-FLD.
           IF CCO-IND-EMIS-LEN IN CCO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CCO-IND-EMIS-LEN IN CCO-FLD
                GO TO FIN-EDT-FLD-CCO.
           MOVE CCO-IND-EMIS IN CCO-FLD TO CCO-IND-EMIS-EDT IN CCO-EDT.
           MOVE CCO-IND-EMIS-EDT IN CCO-EDT TO 
                CCO-IND-EMIS-ALF IN CCO-FLD.
           IF CCO-NUM-FOLI-LEN IN CCO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CCO-NUM-FOLI-LEN IN CCO-FLD
                GO TO FIN-EDT-FLD-CCO.
           MOVE CCO-NUM-FOLI IN CCO-FLD TO CCO-NUM-FOLI-EDT IN CCO-EDT.
           MOVE CCO-NUM-FOLI-EDT IN CCO-EDT TO 
                CCO-NUM-FOLI-ALF IN CCO-FLD.
       FIN-EDT-FLD-CCO.
           EXIT.
