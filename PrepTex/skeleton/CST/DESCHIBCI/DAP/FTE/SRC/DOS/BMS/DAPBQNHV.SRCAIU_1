       FIN-VAL-MKY-NHV.
           EXIT.
       
       VAL-AKY-NHV SECTION.
       INI-VAL-AKY-NHV.
       FIN-VAL-AKY-NHV.
           EXIT.
       
       VAL-NHV-NUM-AVEN SECTION.
       INI-VAL-NHV-NUM-AVEN.
       FIN-VAL-NHV-NUM-AVEN.
           EXIT.
       
       VAL-NHV-NUM-MVEN SECTION.
       INI-VAL-NHV-NUM-MVEN.
       FIN-VAL-NHV-NUM-MVEN.
           EXIT.
       
       VAL-NHV-NUM-DVEN SECTION.
       INI-VAL-NHV-NUM-DVEN.
       FIN-VAL-NHV-NUM-DVEN.
           EXIT.
       
       VAL-NHV-COD-MONE SECTION.
       INI-VAL-NHV-COD-MONE.
           IF NHV-COD-MONE IN NHV-FLD NOT > SPACES
              MOVE SPACES TO NHV-COD-MONE IN NHV-FLD.
       FIN-VAL-NHV-COD-MONE.
           EXIT.
       
       VAL-NHV-COD-PROD SECTION.
