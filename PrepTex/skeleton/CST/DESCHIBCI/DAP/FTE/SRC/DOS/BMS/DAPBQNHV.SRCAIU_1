       SET-TAG-DAPBQNHV SECTION.
       INI-SET-TAG-DAPBQNHV.
           MOVE
           '<IDNMOD NOM=$DAPBQNHV$ VSN=$06.08.30$ VGM=$3.0.1$
      -    'GEN=$26-Jul-10 06:34 PM$ FNC=$DAPNVA$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-DAPBQNHV.
           EXIT.
           
       PUT-ALL-NHV SECTION.
       INI-PUT-ALL-NHV.
           PERFORM PUT-NHV-NHV.
       FIN-PUT-ALL-NHV.
           EXIT.
       
       PUT-NHV-NHV SECTION.
       INI-PUT-NHV-NHV.
           IF PGM-STAT-NHV-NUL
               GO TO FIN-PUT-NHV-NHV.
           IF NOT PGM-STAT-NHV-OKS
               PERFORM CLR-NHV-NHV
               GO TO FIN-PUT-NHV-NHV.
           MOVE NHV-NUM-ICLI IN NHV TO NHV-NUM-ICLI IN NHV-FLD.
           MOVE NHV-COD-VCLI IN NHV TO NHV-COD-VCLI IN NHV-FLD.
           MOVE NHV-NUM-AVEN IN NHV TO NHV-NUM-AVEN IN NHV-FLD.
           MOVE NHV-NUM-MVEN IN NHV TO NHV-NUM-MVEN IN NHV-FLD.
           MOVE NHV-NUM-DVEN IN NHV TO NHV-NUM-DVEN IN NHV-FLD.
           MOVE NHV-COD-MONE IN NHV TO NHV-COD-MONE IN NHV-FLD.
           MOVE NHV-COD-PROD IN NHV TO NHV-COD-PROD IN NHV-FLD.
           MOVE NHV-COD-OFIC IN NHV TO NHV-COD-OFIC IN NHV-FLD.
           MOVE NHV-COD-CORR IN NHV TO NHV-COD-CORR IN NHV-FLD.
           MOVE NHV-COD-VPRO IN NHV TO NHV-COD-VPRO IN NHV-FLD.
           MOVE NHV-NUM-NCUO IN NHV TO NHV-NUM-NCUO IN NHV-FLD.
           MOVE NHV-NUM-AEMI IN NHV TO NHV-NUM-AEMI IN NHV-FLD.
           MOVE NHV-NUM-MEMI IN NHV TO NHV-NUM-MEMI IN NHV-FLD.
           MOVE NHV-NUM-DEMI IN NHV TO NHV-NUM-DEMI IN NHV-FLD.
           MOVE NHV-NUM-VIPE IN NHV TO NHV-NUM-VIPE IN NHV-FLD.
           MOVE NHV-NUM-INTP IN NHV TO NHV-NUM-INTP IN NHV-FLD.
           MOVE NHV-NUM-REAP IN NHV TO NHV-NUM-REAP IN NHV-FLD.
           MOVE NHV-COD-SUCN IN NHV TO NHV-COD-SUCN IN NHV-FLD.
       FIN-PUT-NHV-NHV.
           EXIT.
       
       KEY-ALL-NHV SECTION.
       INI-KEY-ALL-NHV.
           PERFORM KEY-NHV-NHV.
       FIN-KEY-ALL-NHV.
           EXIT.
       
       KEY-NHV-NHV SECTION.
       INI-KEY-NHV-NHV.
           MOVE NHV-NUM-AVEN IN NHV-FLD TO NHV-NUM-AVEN IN NHV.
           MOVE NHV-NUM-MVEN IN NHV-FLD TO NHV-NUM-MVEN IN NHV.
           MOVE NHV-NUM-DVEN IN NHV-FLD TO NHV-NUM-DVEN IN NHV.
           MOVE NHV-COD-MONE IN NHV-FLD TO NHV-COD-MONE IN NHV.
           MOVE NHV-COD-PROD IN NHV-FLD TO NHV-COD-PROD IN NHV.
           MOVE NHV-COD-OFIC IN NHV-FLD TO NHV-COD-OFIC IN NHV.
           MOVE NHV-COD-CORR IN NHV-FLD TO NHV-COD-CORR IN NHV.
           MOVE NHV-COD-VPRO IN NHV-FLD TO NHV-COD-VPRO IN NHV.
           MOVE NHV-NUM-NCUO IN NHV-FLD TO NHV-NUM-NCUO IN NHV.
       FIN-KEY-NHV-NHV.
           EXIT.
       
       SCR-CLRA-NHV SECTION.
       INI-SCR-CLRA-NHV.
           PERFORM CLR-KEY-NHV.
           PERFORM CLR-FLD-NHV.
       FIN-SCR-CLRA-NHV.
           EXIT.
       
       CLR-KEY-NHV SECTION.
       INI-CLR-KEY-NHV.
           MOVE ZEROES TO NHV-NUM-AVEN IN NHV-FLD.
           MOVE ZEROES TO NHV-NUM-MVEN IN NHV-FLD.
           MOVE ZEROES TO NHV-NUM-DVEN IN NHV-FLD.
           MOVE SPACES TO NHV-COD-MONE IN NHV-FLD.
           MOVE SPACES TO NHV-COD-PROD IN NHV-FLD.
           MOVE SPACES TO NHV-COD-OFIC IN NHV-FLD.
           MOVE SPACES TO NHV-COD-CORR IN NHV-FLD.
           MOVE SPACES TO NHV-COD-VPRO IN NHV-FLD.
           MOVE ZEROES TO NHV-NUM-NCUO IN NHV-FLD.
       FIN-CLR-KEY-NHV.
           EXIT.
       
       CLR-FLD-NHV SECTION.
       INI-CLR-FLD-NHV.
           PERFORM CLR-NHV-NHV.
       FIN-CLR-FLD-NHV.
           EXIT.
       
       CLR-NHV-NHV SECTION.
       INI-CLR-NHV-NHV.
           MOVE SPACES TO NHV-NUM-ICLI IN NHV-FLD.
           MOVE SPACES TO NHV-COD-VCLI IN NHV-FLD.
           MOVE ZEROES TO NHV-NUM-AEMI IN NHV-FLD.
           MOVE ZEROES TO NHV-NUM-MEMI IN NHV-FLD.
           MOVE ZEROES TO NHV-NUM-DEMI IN NHV-FLD.
           MOVE ZEROES TO NHV-NUM-VIPE IN NHV-FLD.
           MOVE ZEROES TO NHV-NUM-INTP IN NHV-FLD.
           MOVE ZEROES TO NHV-NUM-REAP IN NHV-FLD.
           MOVE ZEROES TO NHV-COD-SUCN IN NHV-FLD.
       FIN-CLR-NHV-NHV.
           EXIT.
       
       FST-KEY-NHV SECTION.
       INI-FST-KEY-NHV.
           MOVE -1 TO NHV-NUM-AVEN-LEN IN NHV-FLD.
       FIN-FST-KEY-NHV.
           EXIT.
       
       FST-FLD-NHV SECTION.
       INI-FST-FLD-NHV.
      *<<<< INI_FST_FLD_NHV
      *>>>> INI_FST_KEY_OPD 
           MOVE -1 TO NHV-NUM-ICLI-LEN IN NHV-FLD.
       FIN-FST-FLD-NHV.
           EXIT.
       
       PRO-KEY-NHV SECTION.
       INI-PRO-KEY-NHV.
           PERFORM PRO-IKY-NHV.
           PERFORM PRO-AKY-NHV.
           PERFORM PRO-MKY-NHV.
       FIN-PRO-KEY-NHV.
           EXIT.
       
