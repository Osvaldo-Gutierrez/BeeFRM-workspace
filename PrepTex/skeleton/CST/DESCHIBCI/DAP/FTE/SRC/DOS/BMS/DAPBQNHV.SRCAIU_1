           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NS9V TO NHV-NUM-REAP IN NHV-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO NHV-NUM-REAP-LEN IN NHV-FLD
                GO TO FIN-VAL-NUM-FLD-NHV.
       VAL-NUM-NHV-COD-SUCN.
           MOVE NHV-COD-SUCN-ALF IN NHV-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO NHV-COD-SUCN IN NHV-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO NHV-COD-SUCN-LEN IN NHV-FLD
                GO TO FIN-VAL-NUM-FLD-NHV.
       FIN-VAL-NUM-FLD-NHV.
           EXIT.
       
       EDT-KEY-NHV SECTION.
       INI-EDT-KEY-NHV.
           PERFORM EDT-IKY-NHV.
           PERFORM EDT-MKY-NHV.
           PERFORM EDT-AKY-NHV.
       FIN-EDT-KEY-NHV.
           EXIT.
       
       EDT-IKY-NHV SECTION.
       INI-EDT-IKY-NHV.
           IF NHV-NUM-AVEN-LEN IN NHV-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO NHV-NUM-AVEN-LEN IN NHV-FLD
                GO TO FIN-EDT-IKY-NHV.
           MOVE NHV-NUM-AVEN IN NHV-FLD TO NHV-NUM-AVEN-EDT IN NHV-EDT.
           MOVE NHV-NUM-AVEN-EDT IN NHV-EDT TO 
                NHV-NUM-AVEN-ALF IN NHV-FLD.
           IF NHV-NUM-MVEN-LEN IN NHV-FLD = FRM-ERR-NUME
