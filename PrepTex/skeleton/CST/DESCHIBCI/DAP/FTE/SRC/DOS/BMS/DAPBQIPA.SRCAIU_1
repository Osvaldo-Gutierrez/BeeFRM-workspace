      *
      * DAPBQIPA
      * Version 3.0.1 Nov-2005
      * 07-Jun-10 03:36 PM
      *
       SET-TAG-DAPBQIPA SECTION.
       INI-SET-TAG-DAPBQIPA.
           MOVE
           '<IDNMOD NOM=$DAPBQIPA$ VSN=$10.01.05$ VGM=$3.0.1$
      -    'GEN=$07-Jun-10 03:36 PM$ FNC=$091208$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-DAPBQIPA.
           EXIT.
           
       PUT-ALL-IPA SECTION.
       INI-PUT-ALL-IPA.
           PERFORM PUT-IPA-IPA.
       FIN-PUT-ALL-IPA.
           EXIT.
       
       PUT-IPA-IPA SECTION.
       INI-PUT-IPA-IPA.
           IF PGM-STAT-IPA-NUL
               GO TO FIN-PUT-IPA-IPA.
           IF NOT PGM-STAT-IPA-OKS
               PERFORM CLR-IPA-IPA
               GO TO FIN-PUT-IPA-IPA.
           MOVE IPA-CAI-ISER IN IPA TO IPA-CAI-ISER IN IPA-FLD.
           MOVE IPA-IIC-ISER IN IPA TO IPA-IIC-ISER IN IPA-FLD.
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE IPA-NUM-DVEN IN IPA TO IPA-NUM-DVEN IN IPA-FLD.
           MOVE IPA-NUM-MVEN IN IPA TO IPA-NUM-MVEN IN IPA-FLD.
           MOVE IPA-NUM-SVEN IN IPA TO IPA-NUM-SVEN IN IPA-FLD.
           MOVE IPA-NUM-AVEN IN IPA TO IPA-NUM-AVEN IN IPA-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE IPA-COD-COPD IN IPA TO IPA-COD-COPD IN IPA-FLD.
           MOVE IPA-VAL-CAPT IN IPA TO IPA-VAL-CAPT IN IPA-FLD.
           MOVE IPA-SGV-INTS IN IPA TO IPA-SGV-INTS IN IPA-FLD.
           MOVE IPA-VAL-CAIN IN IPA TO IPA-VAL-CAIN IN IPA-FLD.
           MOVE IPA-VAL-MLIQ IN IPA TO IPA-VAL-MLIQ IN IPA-FLD.
           MOVE IPA-COD-OFLI IN IPA TO IPA-COD-OFLI IN IPA-FLD.
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE IPA-NUM-DLIQ IN IPA TO IPA-NUM-DLIQ IN IPA-FLD.
           MOVE IPA-NUM-MLIQ IN IPA TO IPA-NUM-MLIQ IN IPA-FLD.
           MOVE IPA-NUM-SLIQ IN IPA TO IPA-NUM-SLIQ IN IPA-FLD.
           MOVE IPA-NUM-ALIQ IN IPA TO IPA-NUM-ALIQ IN IPA-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE IPA-NUM-PLZO IN IPA TO IPA-NUM-PLZO IN IPA-FLD.
           MOVE IPA-NUM-TVAL IN IPA TO IPA-NUM-TVAL IN IPA-FLD.
           MOVE IPA-SGV-TINT IN IPA TO IPA-SGV-TINT IN IPA-FLD.
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE IPA-NUM-DCAP IN IPA TO IPA-NUM-DCAP IN IPA-FLD.
           MOVE IPA-NUM-MCAP IN IPA TO IPA-NUM-MCAP IN IPA-FLD.
           MOVE IPA-NUM-SCAP IN IPA TO IPA-NUM-SCAP IN IPA-FLD.
           MOVE IPA-NUM-ACAP IN IPA TO IPA-NUM-ACAP IN IPA-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
      * VRF-INI --> BMCHIBCI001210.SRCAIU 
           MOVE IPA-NUM-CLI1 IN IPA TO IPA-NUM-CLI1 IN IPA-FLD.
           MOVE IPA-VRF-CLI1 IN IPA TO IPA-VRF-CLI1 IN IPA-FLD.
      * VRF-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE IPA-GLS-NOM1 IN IPA TO IPA-GLS-NOM1 IN IPA-FLD.
           MOVE IPA-COD-CIU1 IN IPA TO IPA-COD-CIU1 IN IPA-FLD.
           MOVE IPA-GLS-DIR1 IN IPA TO IPA-GLS-DIR1 IN IPA-FLD.
      * VRF-INI --> BMCHIBCI001210.SRCAIU 
           MOVE IPA-NUM-CLI2 IN IPA TO IPA-NUM-CLI2 IN IPA-FLD.
           MOVE IPA-VRF-CLI2 IN IPA TO IPA-VRF-CLI2 IN IPA-FLD.
      * VRF-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE IPA-GLS-NOM2 IN IPA TO IPA-GLS-NOM2 IN IPA-FLD.
           MOVE IPA-COD-CIU2 IN IPA TO IPA-COD-CIU2 IN IPA-FLD.
           MOVE IPA-GLS-DIR2 IN IPA TO IPA-GLS-DIR2 IN IPA-FLD.
           MOVE IPA-COD-OFIC IN IPA TO IPA-COD-OFIC IN IPA-FLD.
           MOVE IPA-GLS-DISP IN IPA TO IPA-GLS-DISP IN IPA-FLD.
       FIN-PUT-IPA-IPA.
           EXIT.
       
       KEY-ALL-IPA SECTION.
       INI-KEY-ALL-IPA.
           PERFORM KEY-IPA-IPA.
       FIN-KEY-ALL-IPA.
           EXIT.
       
       KEY-IPA-IPA SECTION.
       INI-KEY-IPA-IPA.
           MOVE IPA-CAI-ISER IN IPA-FLD TO IPA-CAI-ISER IN IPA.
           MOVE IPA-IIC-ISER IN IPA-FLD TO IPA-IIC-ISER IN IPA.
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE IPA-NUM-DVEN IN IPA-FLD TO IPA-NUM-DVEN IN IPA.
           MOVE IPA-NUM-MVEN IN IPA-FLD TO IPA-NUM-MVEN IN IPA.
           MOVE IPA-NUM-SVEN IN IPA-FLD TO IPA-NUM-SVEN IN IPA.
           MOVE IPA-NUM-AVEN IN IPA-FLD TO IPA-NUM-AVEN IN IPA.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
       FIN-KEY-IPA-IPA.
           EXIT.
       
       SCR-CLRA-IPA SECTION.
       INI-SCR-CLRA-IPA.
           PERFORM CLR-KEY-IPA.
           PERFORM CLR-FLD-IPA.
       FIN-SCR-CLRA-IPA.
           EXIT.
       
       CLR-KEY-IPA SECTION.
       INI-CLR-KEY-IPA.
           MOVE SPACES TO FRM-COD-VOID IN IPA-FLD.
           MOVE SPACES TO IPA-CAI-ISER IN IPA-FLD.
           MOVE ZEROES TO IPA-IIC-ISER IN IPA-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO IPA-FEC-FVEN IN IPA-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
       FIN-CLR-KEY-IPA.
           EXIT.
       
       CLR-FLD-IPA SECTION.
       INI-CLR-FLD-IPA.
           PERFORM CLR-IPA-IPA.
       FIN-CLR-FLD-IPA.
           EXIT.
       
       CLR-IPA-IPA SECTION.
       INI-CLR-IPA-IPA.
           MOVE SPACES TO IPA-COD-COPD IN IPA-FLD.
           MOVE ZEROES TO IPA-VAL-CAPT IN IPA-FLD.
           MOVE ZEROES TO IPA-SGV-INTS IN IPA-FLD.
           MOVE ZEROES TO IPA-VAL-CAIN IN IPA-FLD.
           MOVE ZEROES TO IPA-VAL-MLIQ IN IPA-FLD.
           MOVE SPACES TO IPA-COD-OFLI IN IPA-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO IPA-FEC-FLIQ IN IPA-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO IPA-NUM-PLZO IN IPA-FLD.
           MOVE ZEROES TO IPA-NUM-TVAL IN IPA-FLD.
           MOVE ZEROES TO IPA-SGV-TINT IN IPA-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO IPA-FEC-FCAP IN IPA-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
      * VRF-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO IPA-NUM-CLI1 IN IPA-FLD.
           MOVE SPACES TO IPA-VRF-CLI1 IN IPA-FLD.
      * VRF-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO IPA-GLS-NOM1 IN IPA-FLD.
           MOVE ZEROES TO IPA-COD-CIU1 IN IPA-FLD.
           MOVE SPACES TO IPA-GLS-DIR1 IN IPA-FLD.
      * VRF-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO IPA-NUM-CLI2 IN IPA-FLD.
           MOVE SPACES TO IPA-VRF-CLI2 IN IPA-FLD.
      * VRF-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO IPA-GLS-NOM2 IN IPA-FLD.
           MOVE ZEROES TO IPA-COD-CIU2 IN IPA-FLD.
           MOVE SPACES TO IPA-GLS-DIR2 IN IPA-FLD.
           MOVE SPACES TO IPA-COD-OFIC IN IPA-FLD.
           MOVE SPACES TO IPA-GLS-DISP IN IPA-FLD.
       FIN-CLR-IPA-IPA.
           EXIT.
       
       FST-KEY-IPA SECTION.
       INI-FST-KEY-IPA.
           MOVE -1 TO FRM-COD-VOID-LEN IN IPA-FLD.
       FIN-FST-KEY-IPA.
           EXIT.
       
       FST-FLD-IPA SECTION.
       INI-FST-FLD-IPA.
           MOVE -1 TO IPA-COD-COPD-LEN IN IPA-FLD.
       FIN-FST-FLD-IPA.
           EXIT.
       
       PRO-KEY-IPA SECTION.
       INI-PRO-KEY-IPA.
           MOVE FRM-CPIM-AEY TO FRM-COD-VOID-ATR IN IPA-FLD.
           PERFORM PRO-IKY-IPA.
           PERFORM PRO-AKY-IPA.
           PERFORM PRO-MKY-IPA.
       FIN-PRO-KEY-IPA.
           EXIT.
       
       PRO-IKY-IPA SECTION.
       INI-PRO-IKY-IPA.
           MOVE FRM-CPIM-AEY TO IPA-CAI-ISER-ATR IN IPA-FLD.
           MOVE FRM-CPIM-AEY TO IPA-IIC-ISER-ATR IN IPA-FLD.
           MOVE FRM-CPIM-AEY TO IPA-FEC-FVEN-ATR IN IPA-FLD.
       FIN-PRO-IKY-IPA.
           EXIT.
       
       PRO-MKY-IPA SECTION.
       INI-PRO-MKY-IPA.
       FIN-PRO-MKY-IPA.
           EXIT.
       
       PRO-AKY-IPA SECTION.
       INI-PRO-AKY-IPA.
       FIN-PRO-AKY-IPA.
           EXIT.
       
       UNP-KEY-IPA SECTION.
       INI-UNP-KEY-IPA.
           MOVE FRM-CPIM-UBY TO FRM-COD-VOID-ATR IN IPA-FLD.
           PERFORM UNP-IKY-IPA.
           PERFORM UNP-AKY-IPA.
           PERFORM UNP-MKY-IPA.
       FIN-UNP-KEY-IPA.
           EXIT.
       
       UNP-IKY-IPA SECTION.
       INI-UNP-IKY-IPA.
           MOVE FRM-CPIM-UBY TO FRM-COD-VOID-ATR.
           MOVE FRM-CPIM-UBY TO IPA-CAI-ISER-ATR.
           MOVE FRM-CPIM-UBY TO IPA-IIC-ISER-ATR.
           MOVE FRM-CPIM-UBY TO IPA-FEC-FVEN-ATR IN IPA-FLD.
       FIN-UNP-IKY-IPA.
           EXIT.
       
       UNP-MKY-IPA SECTION.
       INI-UNP-MKY-IPA.
       FIN-UNP-MKY-IPA.
           EXIT.
       
       UNP-AKY-IPA SECTION.
       INI-UNP-AKY-IPA.
       FIN-UNP-AKY-IPA.
           EXIT.
       
       UNP-FLD-IPA SECTION.
       INI-UNP-FLD-IPA.
           PERFORM UNP-IPA-IPA.
       FIN-UNP-FLD-IPA.
           EXIT.
       
       UNP-IPA-IPA SECTION.
       INI-UNP-IPA-IPA.
           MOVE FRM-CPIM-UBY TO IPA-COD-COPD-ATR IN IPA-FLD.
           MOVE FRM-CPIM-UBY TO IPA-VAL-CAPT-ATR IN IPA-FLD.
           MOVE FRM-CPIM-UBY TO IPA-SGV-INTS-ATR IN IPA-FLD.
           MOVE FRM-CPIM-UBY TO IPA-VAL-CAIN-ATR IN IPA-FLD.
           MOVE FRM-CPIM-UBY TO IPA-VAL-MLIQ-ATR IN IPA-FLD.
           MOVE FRM-CPIM-UBY TO IPA-COD-OFLI-ATR IN IPA-FLD.
           MOVE FRM-CPIM-UBY TO IPA-FEC-FLIQ-ATR IN IPA-FLD.
           MOVE FRM-CPIM-UBY TO IPA-NUM-PLZO-ATR IN IPA-FLD.
           MOVE FRM-CPIM-UBY TO IPA-NUM-TVAL-ATR IN IPA-FLD.
           MOVE FRM-CPIM-UBY TO IPA-SGV-TINT-ATR IN IPA-FLD.
           MOVE FRM-CPIM-UBY TO IPA-FEC-FCAP-ATR IN IPA-FLD.
           MOVE FRM-CPIM-UBY TO IPA-NUM-CLI1-ATR IN IPA-FLD.
           MOVE FRM-CPIM-UBY TO IPA-VRF-CLI1-ATR IN IPA-FLD.
           MOVE FRM-CPIM-UBY TO IPA-GLS-NOM1-ATR IN IPA-FLD.
           MOVE FRM-CPIM-UBY TO IPA-COD-CIU1-ATR IN IPA-FLD.
           MOVE FRM-CPIM-UBY TO IPA-GLS-DIR1-ATR IN IPA-FLD.
           MOVE FRM-CPIM-UBY TO IPA-NUM-CLI2-ATR IN IPA-FLD.
           MOVE FRM-CPIM-UBY TO IPA-VRF-CLI2-ATR IN IPA-FLD.
           MOVE FRM-CPIM-UBY TO IPA-GLS-NOM2-ATR IN IPA-FLD.
           MOVE FRM-CPIM-UBY TO IPA-COD-CIU2-ATR IN IPA-FLD.
           MOVE FRM-CPIM-UBY TO IPA-GLS-DIR2-ATR IN IPA-FLD.
           MOVE FRM-CPIM-UBY TO IPA-COD-OFIC-ATR IN IPA-FLD.
           MOVE FRM-CPIM-UBY TO IPA-GLS-DISP-ATR IN IPA-FLD.
       FIN-UNP-IPA-IPA.
           EXIT.
       
       VAL-KEY-IPA SECTION.
       INI-VAL-KEY-IPA.
           PERFORM VAL-IKY-IPA.
           PERFORM VAL-MKY-IPA.
           PERFORM VAL-AKY-IPA.
       FIN-VAL-KEY-IPA.
           EXIT.
       
       VAL-IKY-IPA SECTION.
       INI-VAL-IKY-IPA.
           PERFORM VAL-FRM-COD-VOID.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-VOID-LEN IN IPA-FLD
                GO TO FIN-VAL-IKY-IPA.
           PERFORM VAL-IPA-CAI-ISER.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO IPA-CAI-ISER-LEN IN IPA-FLD
                GO TO FIN-VAL-IKY-IPA.
           PERFORM VAL-IPA-IIC-ISER.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO IPA-IIC-ISER-LEN IN IPA-FLD
                GO TO FIN-VAL-IKY-IPA.
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-IPA-FEC-FVEN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                IPA-FEC-FVEN-LEN IN IPA-FLD
                GO TO FIN-VAL-IKY-IPA.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
       FIN-VAL-IKY-IPA.
           EXIT.
       
       VAL-MKY-IPA SECTION.
       INI-VAL-MKY-IPA.
       FIN-VAL-MKY-IPA.
           EXIT.
       
       VAL-AKY-IPA SECTION.
       INI-VAL-AKY-IPA.
       FIN-VAL-AKY-IPA.
           EXIT.
       
       VAL-FRM-COD-VOID SECTION.
       INI-VAL-FRM-COD-VOID.
           IF FRM-COD-VOID IN IPA-FLD NOT > SPACES
              MOVE SPACES TO FRM-COD-VOID IN IPA-FLD.
       FIN-VAL-FRM-COD-VOID.
           EXIT.
       
       VAL-IPA-CAI-ISER SECTION.
       INI-VAL-IPA-CAI-ISER.
           IF IPA-CAI-ISER IN IPA-FLD NOT > SPACES
              MOVE SPACES TO IPA-CAI-ISER IN IPA-FLD.
       FIN-VAL-IPA-CAI-ISER.
           EXIT.
       
       VAL-IPA-IIC-ISER SECTION.
       INI-VAL-IPA-IIC-ISER.
       FIN-VAL-IPA-IIC-ISER.
           EXIT.
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-IPA-FEC-FVEN SECTION.
       INI-VAL-IPA-FEC-FVEN.
           IF IPA-FEC-FVEN IN IPA-FLD  NOT > ZEROES
              MOVE ZEROES TO IPA-FEC-FVEN IN IPA-FLD.
      *VAL-FEC Validar Fecha
           MOVE IPA-FEC-FVEN IN IPA-FLD TO FEC-FECH.
           MOVE FEC-FORM-FEC TO FEC-FORM.
           MOVE FEC-FBLK-S  TO FEC-FBLK.
           MOVE FEC-CHOY-LE TO FEC-CHOY.
           MOVE FEC-VHBL-S TO FEC-VHBL.
      *%<<<< INI_IPA_FEC_FVEN
           MOVE FEC-FBLK-N TO FEC-FBLK.
           MOVE FEC-CHOY-N TO FEC-CHOY.
           MOVE FEC-VHBL-N TO FEC-VHBL.
      *%>>>> INI_IPA_FEC_FVEN
           MOVE FEC-VAL-FEC TO FEC-CMND.
           PERFORM PRO-FEC.
           IF FEC-STAT-OKS
               MOVE FEC-FECH TO IPA-FEC-FVEN IN IPA-FLD
           ELSE
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE FEC-MENS     TO FRM-MENS
               GO TO FIN-VAL-IPA-FEC-FVEN.
       FIN-VAL-IPA-FEC-FVEN.
           EXIT.
      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-CON-KEY-IPA SECTION.
       INI-VAL-CON-KEY-IPA.
           IF     FRM-COD-VOID IN IPA-FLD NOT > SPACES 
              AND IPA-CAI-ISER IN IPA-FLD NOT > SPACES 
              AND IPA-IIC-ISER IN IPA-FLD NOT > ZEROES 
              AND IPA-FEC-FVEN IN IPA-FLD NOT > ZEROES
                PERFORM ERR-KEY
                PERFORM FST-KEY-IPA.
       FIN-VAL-CON-KEY-IPA.
           EXIT.
       
       VAL-CON-IKY-IPA SECTION.
       INI-VAL-CON-IKY-IPA.
           IF     FRM-COD-VOID IN IPA-FLD NOT > SPACES 
              AND IPA-CAI-ISER IN IPA-FLD NOT > SPACES 
              AND IPA-IIC-ISER IN IPA-FLD NOT > ZEROES 
              AND IPA-FEC-FVEN IN IPA-FLD NOT > ZEROES
                PERFORM ERR-KEY
                PERFORM FST-KEY-IPA.
       FIN-VAL-CON-IKY-IPA.
           EXIT.
       
       VAL-NUM-KEY-IPA SECTION.
       INI-VAL-NUM-KEY-IPA.
           PERFORM VAL-NUM-IKY-IPA.
           PERFORM VAL-NUM-MKY-IPA.
           PERFORM VAL-NUM-AKY-IPA.
       FIN-VAL-NUM-KEY-IPA.
           EXIT.
       
       VAL-NUM-IKY-IPA SECTION.
       INI-VAL-NUM-IKY-IPA.
       VAL-NUM-IPA-IIC-ISER.
           MOVE IPA-IIC-ISER-ALF IN IPA-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO IPA-IIC-ISER IN IPA-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO IPA-IIC-ISER-LEN IN IPA-FLD
                GO TO FIN-VAL-NUM-IKY-IPA.
       FIN-VAL-NUM-IKY-IPA.
           EXIT.
       
       VAL-NUM-MKY-IPA SECTION.
       INI-VAL-NUM-MKY-IPA.
       FIN-VAL-NUM-MKY-IPA.
           EXIT.
       
       VAL-NUM-AKY-IPA SECTION.
       INI-VAL-NUM-AKY-IPA.
       FIN-VAL-NUM-AKY-IPA.
           EXIT.
       
       VAL-NUM-FLD-IPA SECTION.
       INI-VAL-NUM-FLD-IPA.
       VAL-NUM-IPA-VAL-CAPT.
           MOVE IPA-VAL-CAPT-ALF IN IPA-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NS9V TO IPA-VAL-CAPT IN IPA-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO IPA-VAL-CAPT-LEN IN IPA-FLD
                GO TO FIN-VAL-NUM-FLD-IPA.
       VAL-NUM-IPA-SGV-INTS.
           MOVE IPA-SGV-INTS-ALF IN IPA-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NS9V TO IPA-SGV-INTS IN IPA-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO IPA-SGV-INTS-LEN IN IPA-FLD
                GO TO FIN-VAL-NUM-FLD-IPA.
       VAL-NUM-IPA-VAL-CAIN.
           MOVE IPA-VAL-CAIN-ALF IN IPA-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NS9V TO IPA-VAL-CAIN IN IPA-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO IPA-VAL-CAIN-LEN IN IPA-FLD
                GO TO FIN-VAL-NUM-FLD-IPA.
       VAL-NUM-IPA-VAL-MLIQ.
           MOVE IPA-VAL-MLIQ-ALF IN IPA-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NS9V TO IPA-VAL-MLIQ IN IPA-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO IPA-VAL-MLIQ-LEN IN IPA-FLD
                GO TO FIN-VAL-NUM-FLD-IPA.
       VAL-NUM-IPA-NUM-PLZO.
           MOVE IPA-NUM-PLZO-ALF IN IPA-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 4 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO IPA-NUM-PLZO IN IPA-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO IPA-NUM-PLZO-LEN IN IPA-FLD
                GO TO FIN-VAL-NUM-FLD-IPA.
       VAL-NUM-IPA-NUM-TVAL.
           MOVE IPA-NUM-TVAL-ALF IN IPA-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 4 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO IPA-NUM-TVAL IN IPA-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO IPA-NUM-TVAL-LEN IN IPA-FLD
                GO TO FIN-VAL-NUM-FLD-IPA.
       VAL-NUM-IPA-SGV-TINT.
           MOVE IPA-SGV-TINT-ALF IN IPA-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NS9V TO IPA-SGV-TINT IN IPA-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
