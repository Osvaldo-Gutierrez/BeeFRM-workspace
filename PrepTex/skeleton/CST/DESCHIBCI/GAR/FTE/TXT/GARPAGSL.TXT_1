*%! PARA MANEJAT DATACOM DESDE AMBIENTE VSAM INTERACTIVO
*%! INCLUDE 'GARSRC:GNSVSIDTC.SRC'
*% FRM = 'GSL'
*% REG = 'GSL'
*% PGM_PTC = TRUE
*% IF WSS
      *<<<< WSS
       COPY GARWXGSL.
       COPY SGCBRRCD.
       COPY TABBRCOM.
       COPY SGCBRDBC.
       COPY TABBRUSR.
       COPY GNSWGCPT.
       COPY GNSWGSTR.
       COPY GNSWGPES.
       COPY GARBRGSO.
       COPY GARBRGDG.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GDG-REQA==. 
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GSO-REQA==. 
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-USR-REQA==. 
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==. 
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RCD-REQA==. 

       01  WSS-VARI.
           03 WSS-TIPO-DIV.
              05 WSS-TIPO                          PIC X(03).
              05 WSS-DIV                           PIC X(03).
           03 WSS-TSG-OPE.                               
              05 WSS-COD-TOPE                   PIC X(03).
              05 WSS-COD-SOPE                   PIC X(03).
      *
           03 WSS-COD-PESO.
              05 WSS-IND-TMON                  PIC X(01).
              05 WSS-COD-REST                  PIC X(05).
           03 WSS-FEC-EDIT.
              05  WSS-NUM-DEDIT                        PIC 9(02).
              05  WSS-NUM-MEDIT                        PIC 9(02).
              05  WSS-NUM-SEDIT                        PIC 9(02).
              05  WSS-NUM-AEDIT                        PIC 9(02).
           03 WSS-GSL-DESC.
              05  WSS-NUM-DESC                         PIC 9(03).
           03 WSS-IND-MGDG                             PIC X(01).
           03  GSO-KEY-IGSO.                                        
      *        Solicitud BEE                                        
               05 GSO-NUM-SBGL                         PIC 9(15).   
      *        Numero de garantia                                   
               05  GSO-KEY-IGDG.                                    
                   07 GSO-CIC-GTIA.                                 
                      09 GSO-CAI-GTIA                  PIC X(04).   
                      09 GSO-IIC-GTIA                  PIC X(08).   
           03 WSS-CIC-GTIA.                                 
              05 WSS-CAI-GTIA                  PIC X(04).   
              05 WSS-IIC-GTIA                  PIC X(08).   

           03  WSS-FRM-MENS.
               05 WSS-FRM-MENS1                         PIC X(21).
               05 WSS-FRM-MENS2                         PIC X(12).
      *>>>>
*% END
*% IF FIN_INI
      *<<<< FIN_INI
            MOVE TSK-TERM-ALF TO WSX-COD-TERM IN WSX-TSGSL.               
      *>>>>
*% END
*% IF FIN_HBL_KEY
      *<<<< FIN_HBL_KEY
           IF SCR-CCPP = 'ING'
               MOVE FRM-CPIM-AEY TO GSL-NUM-SBGL-ATR IN GSL-FLD
               MOVE FRM-CPIM-AEY TO GSL-NUM-SBGL-STP
           ELSE
               MOVE FRM-CPIM-AEY TO DBC-NUM-ICLI-ATR IN GSL-FLD
               MOVE FRM-CPIM-AEY TO DBC-NUM-ICLI-STP
               MOVE FRM-CPIM-AEY TO DBC-IND-ICLI-ATR IN GSL-FLD
               MOVE FRM-CPIM-AEY TO DBC-IND-ICLI-STP
               MOVE FRM-CPIM-AEY TO DBC-VRF-ICLI-ATR IN GSL-FLD
               MOVE FRM-CPIM-AEY TO DBC-VRF-ICLI-STP
               MOVE FRM-CPIM-AEY TO DBC-GLS-ICLI-ATR IN GSL-FLD
               MOVE FRM-CPIM-AEY TO DBC-GLS-ICLI-STP
               MOVE 0  TO DBC-NUM-ICLI-LEN IN GSL-FLD
               MOVE -1 TO GSL-NUM-SBGL-LEN IN GSL-FLD.
      *>>>>
*% END
*% IF FIN_HBL_FLD
      *>>>> FIN_HBL_FLD
           IF SCR-CCPP = 'ING'
               MOVE FRM-CPIM-AEY TO DBC-NUM-ICLI-ATR IN GSL-FLD
               MOVE FRM-CPIM-AEY TO DBC-NUM-ICLI-STP
               MOVE FRM-CPIM-AEY TO DBC-VRF-ICLI-ATR IN GSL-FLD
               MOVE FRM-CPIM-AEY TO DBC-VRF-ICLI-STP
               MOVE FRM-CPIM-AEY TO DBC-IND-ICLI-ATR IN GSL-FLD
               MOVE FRM-CPIM-AEY TO DBC-IND-ICLI-STP
               MOVE FRM-CPIM-AEY TO DBC-GLS-ICLI-ATR IN GSL-FLD
               MOVE FRM-CPIM-AEY TO DBC-GLS-ICLI-STP.

            IF SCR-CMND NOT = 'CAN'
               MOVE FRM-CPIM-AEY       TO GSL-GLS-MOTC-ATR IN GSL-FLD
               MOVE FRM-CPIM-AEY       TO GSL-GLS-MOTC-STP.

            IF SCR-CMND = 'ING' OR 'MOD'
               GO TO FIN-HBL-FLD.

            MOVE FRM-CPIM-AEY       TO GSL-COD-ORIG-ATR IN GSL-FLD.
            MOVE FRM-CPIM-AEY       TO GSL-COD-ORIG-STP.
      *CGL-INI  20-AUG-2003 20:28:44   
      *      MOVE FRM-CPIM-AEY       TO GSL-FEC-FAUT-ATR IN GSL-FLD.
      *      MOVE FRM-CPIM-AEY       TO GSL-FEC-FAUT-STP.
      *CGL-FIN  20-AUG-2003 20:28:54 
                           
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
           IF SCR-CCPP NOT = 'ING'
              GO TO NOT-ING.

           MOVE DBC-NUM-ICLI IN GSL-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN GSL-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN GSL-FLD TO DBC-GLS-ICLI IN DBC.
           MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GET-CLI.
           IF NOT FIO-STAT-OKS
              MOVE 'CLI    NEX' TO MSG-COD-MENS
              GO TO ERR-FND-KEY.
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.
           PERFORM CPT-BLKS.
           MOVE CPT-STRN            TO FRM-GLS-NOMC IN GSL-FLD.
      *CGL-INI  6-SEP-2003 12:15:30  

           MOVE DBC-CAI-ICLI IN DBC TO RCD-CAI-ICLI IN RCD.
           MOVE DBC-IIC-ICLI IN DBC TO RCD-IIC-ICLI IN RCD.
           MOVE ZEROES TO RCD-NUM-DIRE IN RCD.
           MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM SGC-FIO-RCD.
           IF NOT ( FIO-STAT-OKS AND
              RCD-CAI-ICLI IN RCD = DBC-CAI-ICLI IN DBC AND
              RCD-IIC-ICLI IN RCD = DBC-IIC-ICLI IN DBC )
               GO TO ERR-FND-KEY.

           MOVE RCD-GLS-DIRE IN RCD TO FRM-GLS-DIRC IN GSL-FLD.
           MOVE RCD-NUM-FONO IN RCD TO FRM-GLS-TELF IN GSL-FLD.

           MOVE 'TAB' TO FIO-SIST.
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'COM' TO TAB-COD-TTAB IN TAB.
           MOVE RCD-COD-LUGA IN RCD TO TAB-COD-CTAB IN TAB.
           PERFORM BUS-TAB.
           MOVE TAB TO COM.
           MOVE COM-GLS-COMU IN COM TO FRM-GLS-COMU IN GSL-FLD.
           MOVE COM-GLS-CIUD IN COM TO FRM-GLS-CIUD IN GSL-FLD.
           
           MOVE 'TAB' TO FIO-SIST.
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'REG' TO TAB-COD-TTAB IN TAB.
           MOVE COM-COD-REGI IN COM TO TAB-COD-CTAB IN TAB.
           PERFORM BUS-TAB.
           MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-REGI IN GSL-FLD.
      *CGL-FIN  6-SEP-2003 12:15:16 
           PERFORM OBT-CORR-SBGL.

       NOT-ING.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
       CONT-FND-KEY.
      * Se realiza consulta.
           IF SCR-CMND = 'CON'
               GO TO FIN-FND-KEY.
      
      * Se Intenta Ingreso .
           IF SCR-CMND NOT = 'ING'
               GO TO OTRO-CMN.

      *Codigo estado, Fecha estado y ejecutivo.
           MOVE '01'     TO GSL-COD-ESOL IN GSL-FLD.
           PERFORM GET-FHOY.
           MOVE HOY-DHOY TO WSS-NUM-DEDIT.
           MOVE HOY-MHOY TO WSS-NUM-MEDIT.
           MOVE HOY-SHOY TO WSS-NUM-SEDIT.
           MOVE HOY-AHOY TO WSS-NUM-AEDIT.
           MOVE WSS-FEC-EDIT TO GSL-FEC-ESOL IN GSL-FLD.

      *Usuario
           IF SCR-USER NOT > SPACES
              MOVE SPACES TO SCR-USER.
           MOVE SCR-USER TO GSL-COD-USER IN GSL-FLD.
           GO TO FIN-FND-KEY.
         
       OTRO-CMN.
      * Se intenta CANCELAR
      *PVG INI 14-OCT-2003
           IF SCR-CMND = 'CAN' AND GSL-COD-ESOL IN GSL = '02'
              MOVE 'GSL    YACAN' TO MSG-COD-MENS
              GO TO ERR-FND-KEY.
      *PVG FIN 14-OCT-2003
           IF SCR-CMND = 'CAN'
               IF GSL-COD-ESOL IN GSL NOT = '10'
      *            MOVE '02' TO GSL-COD-ESOL IN GSL
                   GO TO MOV-COMUNES
               ELSE
                   MOVE 'GSL  CONSTIT' TO MSG-COD-MENS
                   GO TO ERR-FND-KEY.
      
      * Se intenta ACTIVAR SOLICITUD.
           IF SCR-CMND NOT = 'ASO'
               GO TO SIGUE-CMN.
      *CGL-INI  7-SEP-2003 11:49:57 
      *     IF GSL-COD-ESOL IN GSL = '01' OR '05' 
            IF GSL-COD-ESOL IN GSL = '01' 
      *CGL-FIN  7-SEP-2003 11:50:28 
               MOVE GSL-KEY-IGSL IN GSL TO GSO-NUM-SBGL IN GSO
               MOVE SPACES              TO GSO-KEY-IGDG IN GSO
               MOVE FIO-GET-NLS TO FIO-CMND
               PERFORM GAR-FIO-GSO
               IF NOT FIO-STAT-OKS OR
                   GSO-NUM-SBGL IN GSO NOT = GSL-KEY-IGSL IN GSL
                   MOVE 'SIN GSO ASOC' TO MSG-COD-MENS
                   GO TO ERR-FND-KEY
               ELSE
      *            MOVE '10' TO GSL-COD-ESOL IN GSL
                   MOVE PGM-STAT-GSL TO FIO-STAT
                   PERFORM ANALIZA-GSO
                   IF MSG-COD-MENS NOT > SPACES
                      MOVE PGM-STAT-GSL TO FIO-STAT
                      GO TO MOV-COMUNES
                   ELSE
                      GO TO ERR-FND-KEY
           ELSE
               MOVE 'GSLESOL NO01' TO MSG-COD-MENS
               GO TO ERR-FND-KEY.

       SIGUE-CMN.
      * Se intenta MODIFICAR o ELIMINAR
           IF GSL-COD-ESOL IN GSL NOT = '01'
               MOVE 'GSLESOL NO01' TO MSG-COD-MENS
               GO TO ERR-FND-KEY.
      
       MOV-COMUNES.
           IF SCR-CMND = 'ASO' OR
              SCR-CMND = 'CAN'
              GO TO REALIZA-MOV
           ELSE
              GO TO FIN-FND-KEY.
              
       REALIZA-MOV.
           MOVE SCR-USER TO GSL-COD-USER IN GSL-FLD.
           PERFORM GET-FHOY.
           MOVE HOY-DHOY TO WSS-NUM-DEDIT.
           MOVE HOY-MHOY TO WSS-NUM-MEDIT.
           MOVE HOY-SHOY TO WSS-NUM-SEDIT.
           MOVE HOY-AHOY TO WSS-NUM-AEDIT.
           MOVE WSS-FEC-EDIT TO GSL-FEC-ESOL IN GSL-FLD.
           GO TO FIN-FND-KEY.

       ERR-FND-KEY.
           MOVE 'GAR' TO MSG-COD-SIST.

           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE SCR-STAT-CON TO SCR-STAT.
           MOVE '00' TO FIO-STAT.
           GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF INI_CMN_ACC
      *<<<< INI_CMN_ACC
      *>>>>
*% END
*% IF INI_CMN_ING
      *<<<< INI_CMN_ING
           MOVE DBC-CIC-ICLI IN DBC TO GSL-CIC-DCLI IN GSL.
      *>>>>
*% END
*% IF CMN_ING
      *<<<< CMN_ING
      *>>>>
*% END
*% IF FIN_CMN_ING
      *<<<< FIN_CMN_ING
            IF NOT FIO-STAT-OKS
      *         Error inconsistencia en el ingreso : informar
                MOVE FIO-MENS TO FRM-MENS
                GO TO FIN-CMN-ING. 

            IF SCR-OFRM-SRV AND FIO-STAT-OKS AND SCR-IERR = 'N'
              MOVE WSX-NOM-TSGSL  TO QUE-COLA
              MOVE QUE-DEL        TO QUE-CMND
              PERFORM GNS-PRO-QUE
              MOVE GSL-NUM-SBGL IN GSL TO WSX-GSL-NUM-SBGL
              MOVE WSX-NOM-TSGSL  TO QUE-COLA
              MOVE WSX-LEN-TSGSL  TO QUE-LITM
              MOVE WSX-TAB-GSL    TO QUE-ITEM
              MOVE QUE-PUT        TO QUE-CMND
              PERFORM GNS-PRO-QUE.

      *>>>>
*% END
*% IF CMN_MOD
      *<<<< CMN_MOD
           IF SCR-CMND = 'MOD'
               GO TO SIGUE-CMN-MOD. 

           IF SCR-CMND  = 'CAN' OR 'ASO'
               PERFORM UPD-PUT-ELI-GSO.

           IF NOT FIO-STAT-OKS 
               GO TO FIN-CMN-MOD. 
                  
       SIGUE-CMN-MOD. 
      *>>>>
*% END
*% IF FIN_CMN_MOD
      *<<<< FIN_CMN_MOD
           IF FIO-STAT-OKS
              IF SCR-OFRM-SRV AND
                 SCR-CMND = 'MOD'
                 PERFORM UPD-PUT-ELI-GSO
                 MOVE '00' TO FIO-STAT.

            IF NOT FIO-STAT-OKS
      *        Error inconsistencia en la modificacion : INFORMAR               
                MOVE FIO-MENS TO FRM-MENS
                GO TO FIN-CMN-MOD.
      *PVG INI 14-OCT-2003
           IF SCR-CMND = 'CAN'
              MOVE 'GSL    CANOK' TO MSG-COD-MENS
           ELSE
           IF SCR-CMND = 'ASO'
              MOVE 'GSL    ASOOK' TO MSG-COD-MENS
           ELSE
              GO TO FIN-CMN-MOD.
           MOVE 'GAR' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
      *PVG FIN 14-OCT-2003
      *>>>>
*% END
*% IF CMN_ELI
      *<<<< CMN_ELI
           PERFORM UPD-PUT-ELI-GSO.

           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-MOD. 
      *>>>>
*% END
*% IF FIN_CMN_ELI
      *<<<< FIN_CMN_ELI
            IF NOT FIO-STAT-OKS
      *        Error inconsistencia en la modificacion : INFORMAR               
                MOVE FIO-MENS TO FRM-MENS
                GO TO FIN-CMN-MOD.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY SGCBFRCD.
       COPY SGCBFDBC.
       COPY SGCBGCLI.
       COPY TABBBUSR.
       COPY TABBFUSR.
       COPY GARBFGSO.
       COPY GARBFGDG.
       COPY GNSBGPES.
       COPY GNSBBTAB.
       COPY GNSBGCPT.
       COPY GNSBGSTR.
       COPY GNSBGVSM.
       COPY GNSBPFEC.
       COPY GNSBVCOD.
       COPY GNSBVTAB.


       OBT-CORR-SBGL SECTION.
       INI-OBT-CORR-SBGL.
           MOVE '999999999999' TO GSL-KEY-IGSL IN GSL.
           MOVE FIO-GET-LEQ TO FIO-CMND.
           PERFORM GAR-FIO-GSL.
           IF NOT  FIO-STAT-OKS
               MOVE 1 TO GSL-NUM-SBGL IN GSL-FLD
               MOVE SPACES TO GSL-COD-ORIG IN GSL
               GO TO FIN-OBT-CORR-SBGL.           
           ADD 1 TO GSL-NUM-SBGL IN GSL. 
           MOVE GSL-NUM-SBGL IN GSL TO GSL-NUM-SBGL IN GSL-FLD.
           MOVE SPACES TO GSL-COD-ORIG IN GSL.
       FIN-OBT-CORR-SBGL.
           EXIT.

       ANALIZA-GSO SECTION.
       INI-ANALIZA-GSO.
      *Primera GSO asociada ya viene leida.
           MOVE SPACES TO MSG-COD-MENS.
           IF GSL-COD-ESOL IN GSL = '01' AND
              GSO-COD-ESGA IN GSO NOT = '01'
               MOVE 'GSO ASC NO 01' TO MSG-COD-MENS
               GO TO FIN-ANALIZA-GSO.
           IF GSL-COD-ESOL IN GSL = '05' AND
              GSO-COD-ESGA IN GSO NOT = '02'
               MOVE 'GSO ASC NO 02' TO MSG-COD-MENS
               GO TO FIN-ANALIZA-GSO.

           MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM GAR-FIO-GSO.
           IF NOT FIO-STAT-OKS OR 
              GSO-NUM-SBGL IN GSO NOT = GSL-KEY-IGSL IN GSL
               GO TO FIN-ANALIZA-GSO
           ELSE
               GO TO INI-ANALIZA-GSO.
       FIN-ANALIZA-GSO.
           EXIT. 

       ANALIZA-GDG SECTION.
       INI-ANALIZA-GDG.
           MOVE 'S' TO WSS-IND-MGDG.
           IF SCR-CMND NOT = 'CAN'
               GO TO FIN-ANALIZA-GDG.
      *Llaves originales de GSO:
           MOVE GSO-KEY-IGSO IN GSO TO GSO-KEY-IGSO IN WSS-VARI.
      *GSO viene recien leida, por lo tanto: 
      *llaves GSO-NUM-SBGL + GSO-KEY-IGDG completas.
           MOVE ZEROES TO GSO-NUM-SBGL IN GSO.
           MOVE FIO-GET-NLS TO FIO-CMND.
       LOOP-ANALIZA-GDG.
           MOVE 'GSO-KEY-IGDG' TO FIO-AKEY.
           PERFORM GAR-FIO-GSO.
           IF NOT FIO-STAT-OKS OR
              GSO-KEY-IGDG IN GSO NOT = GSO-KEY-IGDG IN WSS-VARI
               MOVE 'S' TO WSS-IND-MGDG
               GO TO FIN-ANALIZA-GDG.
           IF GSO-NUM-SBGL IN GSO NOT = GSO-NUM-SBGL IN WSS-VARI
               MOVE 'N' TO WSS-IND-MGDG
               GO TO FIN-ANALIZA-GDG
           ELSE
               MOVE FIO-GET-NXT TO FIO-CMND
               GO TO LOOP-ANALIZA-GDG.
  
       FIN-ANALIZA-GDG.
      *Restauro GSO
           MOVE GSO-KEY-IGSO IN WSS-VARI TO GSO-KEY-IGSO IN GSO.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GAR-FIO-GSO.
       END-ANALIZA-GDG.
           EXIT.

       MODIFICA-GDG SECTION.
       INI-MODIFICA-GDG.
           MOVE SPACES TO MSG-COD-MENS.
           MOVE GSO-KEY-IGDG IN GSO TO GDG-KEY-IGDG IN GDG.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GAR-FIO-GDG.
           IF NOT FIO-STAT-OKS
               MOVE 'GDG   NEX' TO MSG-COD-MENS
               GO TO FIN-MODIFICA-GDG.

           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM GAR-FIO-GDG.
           IF NOT FIO-STAT-OKS
               MOVE 'GDG NOUPD' TO MSG-COD-MENS
               GO TO FIN-MODIFICA-GDG.
           MOVE '20' TO GDG-IND-EST1 IN GDG.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM GAR-FIO-GDG.
       FIN-MODIFICA-GDG.
           EXIT.
      *>>>>
*% END
*% IF EOF_PU
      *<<<< EOF_PU
       UPD-PUT-ELI-GSO SECTION.
       INI-UPD-PUT-ELI-GSO.
           MOVE 'S' TO WSS-IND-MGDG.
           MOVE GSL-KEY-IGSL IN GSL TO GSO-NUM-SBGL IN GSO.
           MOVE SPACES              TO GSO-KEY-IGDG IN GSO.
           MOVE FIO-GET-NLS TO FIO-CMND.

       LOOP-GSO.
           PERFORM GAR-FIO-GSO.
           IF NOT FIO-STAT-OKS OR
               GSO-NUM-SBGL IN GSO NOT = GSL-KEY-IGSL IN GSL
               MOVE ZEROES TO FIO-STAT 
               GO TO FIN-UPD-PUT-ELI-GSO.
           
           IF WSS-IND-MGDG = 'S' AND SCR-CMND  = 'CAN'
               PERFORM ANALIZA-GDG
               IF WSS-IND-MGDG = 'S'
                   PERFORM MODIFICA-GDG
                   IF NOT FIO-STAT-OKS
                       MOVE 'ERROR, NO MODIF GTIA ' TO WSS-FRM-MENS1
                       MOVE  GDG-CIC-GTIA IN GDG    TO WSS-FRM-MENS2
                       MOVE  WSS-FRM-MENS           TO FRM-MENS
                       GO TO FIN-UPD-PUT-ELI-GSO.

           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM GAR-FIO-GSO.
           IF NOT FIO-STAT-OKS
               MOVE 'GSO NOUPD' TO MSG-COD-MENS
               GO TO FIN-UPD-PUT-ELI-GSO.

           IF ( SCR-OFRM-SRV  AND SCR-CMND = 'MOD' ) OR       
               SCR-CMND  = 'ELI'
               MOVE FIO-DEL TO FIO-CMND
               PERFORM GAR-FIO-GSO
               IF FIO-STAT-OKS
                   GO TO INI-UPD-PUT-ELI-GSO
               ELSE
                   GO TO FIN-UPD-PUT-ELI-GSO.

           PERFORM GET-FHOY.
           IF SCR-CMND  = 'CAN'
               MOVE '12'     TO GSO-COD-ESGA IN GSO.
           IF SCR-CMND  = 'ASO'
      *CGL-INI  7-SEP-2003 12:03:37 
              IF GSL-COD-TPRO IN GSL = 'S'
                  MOVE '02'     TO GSO-COD-ESGA IN GSO
              ELSE
                  MOVE '02'     TO GSO-COD-ESGA IN GSO
                  MOVE '02'     TO GSO-COD-EST2 IN GSO.  
      *CGL-FIN  7-SEP-2003 12:03:44 
           MOVE HOY-FHOY TO GSO-FEC-ESOL IN GSO.
           MOVE SCR-USER TO GSO-COD-USER IN GSO.

           IF SCR-CMND  = 'CAN' OR 'ASO'
               MOVE FIO-MOD TO FIO-CMND
               PERFORM GAR-FIO-GSO
      *PVG INI 14-OCT-2003 Archivo VSAM
               MOVE FIO-GET-NLS TO FIO-CMND
               PERFORM GAR-FIO-GSO
      *PVG FIN 14-OCT-2003
               MOVE FIO-GET-NXT TO FIO-CMND
               GO TO LOOP-GSO.

       FIN-UPD-PUT-ELI-GSO.
           EXIT.

      *>>>>
*% END
