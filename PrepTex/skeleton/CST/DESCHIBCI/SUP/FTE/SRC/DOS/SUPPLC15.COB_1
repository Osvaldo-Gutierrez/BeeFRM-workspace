       PROCEDURE DIVISION.
      *===================
       A-CONTROL SECTION.
      *=================
       AA-INICIALIZACION.
           PERFORM B-INICIALIZAR.
           SKIP1
       AB-PROCESAMIENTO.
           IF WS-PROCESO-OK
              PERFORM C-PROCESAR
           END-IF.
           SKIP1
       AC-TERMINACION.
           PERFORM D-TERMINAR.
           SKIP1
       AZ-FIN-DE-SECCION.
           STOP RUN.
           EJECT
       B-INICIALIZAR SECTION.
      *======================
       BA-PROC-PPAL.
      *-------------
           PERFORM BB-ABRIR-ARCHIVOS.
           GO BZ-FIN-DE-SECCION.
       BB-ABRIR-ARCHIVOS.
      *------------------
           OPEN OUTPUT SUPLIS1
                INPUT  SUPVP15.

           IF WR-FS-SUPVP15 NOT = 0 AND NOT = 97
              MOVE 'B-INICIALIZAR'          TO WR-SECCION
              MOVE 'BB-ABRIR-ARCHIVOS'      TO WR-PARRAFO
              MOVE 'SUPVP15    '            TO WR-ARCHIVO
              MOVE 'OPEN       '            TO WR-OPERACI
              MOVE WR-FS-SUPVP15            TO WR-FILE-ST
              PERFORM BC-DISPLAY-ERROR
           END-IF.
           ACCEPT WR-FECHA-HOY FROM DATE.
           SKIP1
       BC-DISPLAY-ERROR.
      *-----------------
           SET  WS-PROCESO-ERROR            TO TRUE
           DISPLAY ' '
           DISPLAY 'ERROR EN SECCION  : ' WR-SECCION
           DISPLAY ' PARRAFO          : ' WR-PARRAFO
           DISPLAY ' ARCHIVO          : ' WR-ARCHIVO
           DISPLAY ' OPERACION        : ' WR-OPERACI
           DISPLAY ' FILE STATUS      : ' WR-FILE-ST
           DISPLAY ' MENSAJE DE ERROR : ' WR-MENSAJE
           DISPLAY ' '.
           SKIP1
       BZ-FIN-DE-SECCION.
      *------------------
           EXIT.
           EJECT
       C-PROCESAR SECTION.
      *===================
       CA-PROC-PPAL.
      *-------------
           PERFORM CB-LEER-SUPVP15
           PERFORM CC-ACUMULA-CARATULA
              UNTIL (WR-FS-SUPVP15 NOT = 0 OR WS-PROCESO-ERROR)
           IF (NOT WS-PROCESO-ERROR)
               PERFORM CC-LISTA-CARATULA
           END-IF.
           GO CZ-FIN-DE-SECCION.
           SKIP1
       CB-LEER-SUPVP15 .
      *----------------
           READ SUPVP15 END-READ.
           IF WR-FS-SUPVP15 = 0
              ADD 1                         TO WA-L-P15
           ELSE
           IF WR-FS-SUPVP15  NOT = 10
              MOVE 'C-PROCESAR'             TO WR-SECCION
              MOVE 'CB-LEER-SUPVP15 '       TO WR-PARRAFO
              MOVE 'SUPVP15     '           TO WR-ARCHIVO
              MOVE 'READ       '            TO WR-OPERACI
              MOVE WR-FS-SUPVP15            TO WR-FILE-ST
              PERFORM BC-DISPLAY-ERROR
           END-IF.
           SKIP1
       XX-MUEVE-REG-CABECERA.
      *----------------------
           MOVE P15H-COD-INS-FIN             TO WR-LA02-COD-INS-FIN
           MOVE P15H-IDE-ARC                 TO WR-LA03-IDE-ARC
           MOVE WR-MES(P15H-MES-PER)         TO WR-LA03-MES-PER
           MOVE P15H-ANO-PER                 TO WR-LA03-ANO-PER
      *****IF (P15H-ANO-PER > '90')
           IF (P15H-ANO-PER < ROLL-YY)
               MOVE '20'                     TO WR-LA03-SIG-PER
           ELSE
               MOVE '19'                     TO WR-LA03-SIG-PER
           END-IF.
           SKIP1
       CC-ACUMULA-CARATULA.
      *--------------------
           IF (P15H-IDE-ARC = 'P15')
               PERFORM XX-MUEVE-REG-CABECERA
           ELSE
               ADD 1                        TO WA-NUM-REG
               IF (WR-REGI-ANT NOT = P15S-COD-REGI)
                   ADD 1                    TO WA-NUM-REGI
                   MOVE P15S-COD-REGI       TO WR-REGI-ANT
               END-IF
               ADD P15S-VAL-SCON            TO WA-VAL-SCON
           END-IF.
           PERFORM CB-LEER-SUPVP15.
           SKIP1
       CC-LISTA-CARATULA.
      *------------------
           MOVE WA-NUM-REG                  TO WR-LA05-NUM-REG
           MOVE WA-NUM-REGI                 TO WR-LA07-NUM-REGI
           MOVE WA-VAL-SCON                 TO WR-LA08-VAL-SCON

           WRITE SUPRLIS FROM WR-LINEA01 AFTER PAGE
           WRITE SUPRLIS FROM WR-LINEA02 AFTER 3
           WRITE SUPRLIS FROM WR-LINEA03 AFTER 2
           WRITE SUPRLIS FROM WR-LINEA04 AFTER 3
           WRITE SUPRLIS FROM WR-LINEA05
           WRITE SUPRLIS FROM WR-LINEA06
           WRITE SUPRLIS FROM WR-LINEA07
           WRITE SUPRLIS FROM WR-LINEA06
           WRITE SUPRLIS FROM WR-LINEA08
           WRITE SUPRLIS FROM WR-LINEA09
           WRITE SUPRLIS FROM WR-LINEA10 AFTER 4
           WRITE SUPRLIS FROM WR-LINEA11 AFTER 2
           WRITE SUPRLIS FROM WR-LINEA12 AFTER 1.
           SKIP1
       CZ-FIN-DE-SECCION.
      *------------------
           EXIT.
           EJECT
       D-TERMINAR SECTION.
