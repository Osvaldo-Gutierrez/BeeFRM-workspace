       PUT-ALL-P16 SECTION.
       INI-PUT-ALL-P16.
           PERFORM PUT-P16-P16.
       FIN-PUT-ALL-P16.
           EXIT.

       PUT-P16-P16 SECTION.
       INI-PUT-P16-P16.
           IF PGM-STAT-P16-NUL
               GO TO FIN-PUT-P16-P16.
           IF NOT PGM-STAT-P16-OKS
               PERFORM CLR-P16-P16
               GO TO FIN-PUT-P16-P16.
           MOVE P16-COD-PROD IN P16 TO P16-COD-PROD IN P16-FLD.
           MOVE P16-COD-REGI IN P16 TO P16-COD-REGI IN P16-FLD.
           MOVE P16-COD-MONE IN P16 TO P16-COD-MONE IN P16-FLD.
           MOVE P16-COD-ACTI IN P16 TO P16-COD-ACTI IN P16-FLD.
           MOVE P16-VAL-SLDO IN P16 TO P16-VAL-SLDO IN P16-FLD.
       FIN-PUT-P16-P16.
           EXIT.
      * 03-Oct-95 12:37 PM
       RET-ALL-P16 SECTION.
       INI-RET-ALL-P16.
           PERFORM KEY-ALL-P16.
           PERFORM RET-P16-P16.
       FIN-RET-ALL-P16.
           EXIT.

       RET-P16-P16 SECTION.
       INI-RET-P16-P16.
           MOVE P16-VAL-SLDO IN P16-FLD TO P16-VAL-SLDO IN P16.
       FIN-RET-P16-P16.
           EXIT.

       KEY-ALL-P16 SECTION.
       INI-KEY-ALL-P16.
           PERFORM KEY-P16-P16.
       FIN-KEY-ALL-P16.
           EXIT.

       KEY-P16-P16 SECTION.
       INI-KEY-P16-P16.
           MOVE P16-COD-PROD IN P16-FLD TO P16-COD-PROD IN P16.
           MOVE P16-COD-REGI IN P16-FLD TO P16-COD-REGI IN P16.
           MOVE P16-COD-MONE IN P16-FLD TO P16-COD-MONE IN P16.
           MOVE P16-COD-ACTI IN P16-FLD TO P16-COD-ACTI IN P16.
       FIN-KEY-P16-P16.
           EXIT.

       SCR-CLRA-P16 SECTION.
       INI-SCR-CLRA-P16.
           PERFORM CLR-KEY-P16.
           PERFORM CLR-FLD-P16.
       FIN-SCR-CLRA-P16.
           EXIT.

       CLR-KEY-P16 SECTION.
       INI-CLR-KEY-P16.
           MOVE ZEROES TO P16-COD-PROD IN P16-FLD.
           MOVE ZEROES TO P16-COD-REGI IN P16-FLD.
           MOVE SPACES TO P16-COD-MONE IN P16-FLD.
           MOVE ZEROES TO P16-COD-ACTI IN P16-FLD.
       FIN-CLR-KEY-P16.
           EXIT.

       CLR-FLD-P16 SECTION.
       INI-CLR-FLD-P16.
           PERFORM CLR-P16-P16.
       FIN-CLR-FLD-P16.
           EXIT.

       CLR-P16-P16 SECTION.
       INI-CLR-P16-P16.
           MOVE ZEROES TO P16-VAL-SLDO IN P16-FLD.
       FIN-CLR-P16-P16.
           EXIT.

       FST-KEY-P16 SECTION.
       INI-FST-KEY-P16.
           MOVE -1 TO P16-COD-PROD-LEN IN P16-FLD.
       FIN-FST-KEY-P16.
           EXIT.

       FST-FLD-P16 SECTION.
       INI-FST-FLD-P16.
           MOVE -1 TO P16-VAL-SLDO-LEN IN P16-FLD.
       FIN-FST-FLD-P16.
           EXIT.

       PRO-KEY-P16 SECTION.
       INI-PRO-KEY-P16.
           PERFORM PRO-IKY-P16.
           PERFORM PRO-AKY-P16.
           PERFORM PRO-MKY-P16.
       FIN-PRO-KEY-P16.
           EXIT.

       PRO-IKY-P16 SECTION.
       INI-PRO-IKY-P16.
           MOVE FRM-CPIM-AEY TO P16-COD-PROD-ATR IN P16-FLD.
           MOVE FRM-CPIM-AEY TO P16-COD-REGI-ATR IN P16-FLD.
           MOVE FRM-CPIM-AEY TO P16-COD-MONE-ATR IN P16-FLD.
           MOVE FRM-CPIM-AEY TO P16-COD-ACTI-ATR IN P16-FLD.
       FIN-PRO-IKY-P16.
           EXIT.

       PRO-MKY-P16 SECTION.
       INI-PRO-MKY-P16.
       FIN-PRO-MKY-P16.
           EXIT.

       PRO-AKY-P16 SECTION.
       INI-PRO-AKY-P16.
       FIN-PRO-AKY-P16.
           EXIT.

       PRO-FLD-P16 SECTION.
       INI-PRO-FLD-P16.
           PERFORM PRO-P16-P16.
       FIN-PRO-FLD-P16.
           EXIT.

       PRO-P16-P16 SECTION.
       INI-PRO-P16-P16.
           MOVE FRM-CPIM-AEY TO P16-VAL-SLDO-ATR IN P16-FLD.
       FIN-PRO-P16-P16.
           EXIT.

       UNP-KEY-P16 SECTION.
       INI-UNP-KEY-P16.
           PERFORM UNP-IKY-P16.
           PERFORM UNP-AKY-P16.
           PERFORM UNP-MKY-P16.
       FIN-UNP-KEY-P16.
           EXIT.

       UNP-IKY-P16 SECTION.
       INI-UNP-IKY-P16.
           MOVE FRM-CPIM-UBY TO P16-COD-PROD-ATR.
           MOVE FRM-CPIM-UBY TO P16-COD-REGI-ATR.
           MOVE FRM-CPIM-UBY TO P16-COD-MONE-ATR.
           MOVE FRM-CPIM-UBY TO P16-COD-ACTI-ATR.
       FIN-UNP-IKY-P16.
           EXIT.

       UNP-MKY-P16 SECTION.
       INI-UNP-MKY-P16.
       FIN-UNP-MKY-P16.
           EXIT.

       UNP-AKY-P16 SECTION.
       INI-UNP-AKY-P16.
       FIN-UNP-AKY-P16.
           EXIT.

       UNP-FLD-P16 SECTION.
       INI-UNP-FLD-P16.
           PERFORM UNP-P16-P16.
       FIN-UNP-FLD-P16.
           EXIT.

       UNP-P16-P16 SECTION.
       INI-UNP-P16-P16.
           MOVE FRM-CPIM-UBY TO P16-VAL-SLDO-ATR IN P16-FLD.
       FIN-UNP-P16-P16.
           EXIT.

       VAL-KEY-P16 SECTION.
       INI-VAL-KEY-P16.
           PERFORM VAL-IKY-P16.
           PERFORM VAL-MKY-P16.
           PERFORM VAL-AKY-P16.
       FIN-VAL-KEY-P16.
           EXIT.

       VAL-IKY-P16 SECTION.
       INI-VAL-IKY-P16.
           PERFORM VAL-P16-COD-PROD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO P16-COD-PROD-LEN IN P16-FLD
                GO TO FIN-VAL-IKY-P16.
           PERFORM VAL-P16-COD-REGI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO P16-COD-REGI-LEN IN P16-FLD
                GO TO FIN-VAL-IKY-P16.
           PERFORM VAL-P16-COD-MONE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO P16-COD-MONE-LEN IN P16-FLD
                GO TO FIN-VAL-IKY-P16.
           PERFORM VAL-P16-COD-ACTI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO P16-COD-ACTI-LEN IN P16-FLD
                GO TO FIN-VAL-IKY-P16.
       FIN-VAL-IKY-P16.
           EXIT.

       VAL-MKY-P16 SECTION.
       INI-VAL-MKY-P16.
       FIN-VAL-MKY-P16.
           EXIT.

       VAL-AKY-P16 SECTION.
       INI-VAL-AKY-P16.
       FIN-VAL-AKY-P16.
           EXIT.

       VAL-FLD-P16 SECTION.
       INI-VAL-FLD-P16.
           PERFORM VAL-P16-VAL-SLDO.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO P16-VAL-SLDO-LEN IN P16-FLD
                GO TO FIN-VAL-FLD-P16.
       FIN-VAL-FLD-P16.
           EXIT.

       VAL-P16-COD-PROD SECTION.
       INI-VAL-P16-COD-PROD.
           IF P16-COD-PROD IN P16-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-P16-COD-PROD.
       FIN-VAL-P16-COD-PROD.
           EXIT.

       VAL-P16-COD-REGI SECTION.
       INI-VAL-P16-COD-REGI.
           IF P16-COD-REGI IN P16-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-P16-COD-REGI.
       FIN-VAL-P16-COD-REGI.
           EXIT.

       VAL-P16-COD-MONE SECTION.
       INI-VAL-P16-COD-MONE.
           IF P16-COD-MONE IN P16-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-P16-COD-MONE.
       FIN-VAL-P16-COD-MONE.
           EXIT.

       VAL-P16-COD-ACTI SECTION.
       INI-VAL-P16-COD-ACTI.
           IF P16-COD-ACTI IN P16-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-P16-COD-ACTI.
       FIN-VAL-P16-COD-ACTI.
           EXIT.

       VAL-P16-VAL-SLDO SECTION.
       INI-VAL-P16-VAL-SLDO.
       FIN-VAL-P16-VAL-SLDO.
           EXIT.

       VAL-CON-KEY-P16 SECTION.
       INI-VAL-CON-KEY-P16.
           IF     P16-COD-PROD IN P16-FLD NOT > ZEROES
              AND P16-COD-REGI IN P16-FLD NOT > ZEROES
              AND P16-COD-MONE IN P16-FLD NOT > SPACES
              AND P16-COD-ACTI IN P16-FLD NOT > ZEROES
                PERFORM ERR-KEY
                PERFORM FST-KEY-P16.
       FIN-VAL-CON-KEY-P16.
           EXIT.

       VAL-CON-IKY-P16 SECTION.
       INI-VAL-CON-IKY-P16.
           IF     P16-COD-PROD IN P16-FLD NOT > ZEROES
              AND P16-COD-REGI IN P16-FLD NOT > ZEROES
              AND P16-COD-MONE IN P16-FLD NOT > SPACES
              AND P16-COD-ACTI IN P16-FLD NOT > ZEROES
                PERFORM ERR-KEY
                PERFORM FST-KEY-P16.
       FIN-VAL-CON-IKY-P16.
           EXIT.

       VAL-CON-FLD-P16 SECTION.
       INI-VAL-CON-FLD-P16.
       FIN-VAL-CON-FLD-P16.
           EXIT.

       VAL-CON-P16 SECTION.
       INI-VAL-CON-P16.
       FIN-VAL-CON-P16.
           EXIT.

       VAL-NUM-KEY-P16 SECTION.
       INI-VAL-NUM-KEY-P16.
           PERFORM VAL-NUM-IKY-P16.
           PERFORM VAL-NUM-MKY-P16.
           PERFORM VAL-NUM-AKY-P16.
       FIN-VAL-NUM-KEY-P16.
           EXIT.

       VAL-NUM-IKY-P16 SECTION.
       INI-VAL-NUM-IKY-P16.
       VAL-NUM-P16-COD-PROD.
           MOVE P16-COD-PROD-ALF IN P16-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO P16-COD-PROD IN P16-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO P16-COD-PROD-LEN IN P16-FLD
                GO TO FIN-VAL-NUM-IKY-P16.
       VAL-NUM-P16-COD-REGI.
           MOVE P16-COD-REGI-ALF IN P16-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 2 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO P16-COD-REGI IN P16-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO P16-COD-REGI-LEN IN P16-FLD
                GO TO FIN-VAL-NUM-IKY-P16.
       VAL-NUM-P16-COD-ACTI.
           MOVE P16-COD-ACTI-ALF IN P16-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 2 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO P16-COD-ACTI IN P16-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO P16-COD-ACTI-LEN IN P16-FLD
                GO TO FIN-VAL-NUM-IKY-P16.
       FIN-VAL-NUM-IKY-P16.
           EXIT.

       VAL-NUM-MKY-P16 SECTION.
       INI-VAL-NUM-MKY-P16.
       FIN-VAL-NUM-MKY-P16.
           EXIT.

       VAL-NUM-AKY-P16 SECTION.
       INI-VAL-NUM-AKY-P16.
       FIN-VAL-NUM-AKY-P16.
           EXIT.

       VAL-NUM-FLD-P16 SECTION.
       INI-VAL-NUM-FLD-P16.
       VAL-NUM-P16-VAL-SLDO.
           MOVE P16-VAL-SLDO-ALF IN P16-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 13 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO P16-VAL-SLDO IN P16-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO P16-VAL-SLDO-LEN IN P16-FLD
                GO TO FIN-VAL-NUM-FLD-P16.
       FIN-VAL-NUM-FLD-P16.
           EXIT.

       EDT-KEY-P16 SECTION.
       INI-EDT-KEY-P16.
           PERFORM EDT-IKY-P16.
           PERFORM EDT-MKY-P16.
           PERFORM EDT-AKY-P16.
       FIN-EDT-KEY-P16.
           EXIT.

       EDT-IKY-P16 SECTION.
       INI-EDT-IKY-P16.
           IF P16-COD-PROD-LEN IN P16-FLD = -9
                MOVE -1 TO P16-COD-PROD-LEN IN P16-FLD
                GO TO FIN-EDT-IKY-P16.
           MOVE P16-COD-PROD IN P16-FLD TO P16-COD-PROD-EDT IN P16-EDT.
           MOVE P16-COD-PROD-EDT IN P16-EDT TO
                P16-COD-PROD-ALF IN P16-FLD.
           IF P16-COD-REGI-LEN IN P16-FLD = -9
                MOVE -1 TO P16-COD-REGI-LEN IN P16-FLD
                GO TO FIN-EDT-IKY-P16.
           MOVE P16-COD-REGI IN P16-FLD TO P16-COD-REGI-EDT IN P16-EDT.
           MOVE P16-COD-REGI-EDT IN P16-EDT TO
                P16-COD-REGI-ALF IN P16-FLD.
           IF P16-COD-ACTI-LEN IN P16-FLD = -9
                MOVE -1 TO P16-COD-ACTI-LEN IN P16-FLD
                GO TO FIN-EDT-IKY-P16.
           MOVE P16-COD-ACTI IN P16-FLD TO P16-COD-ACTI-EDT IN P16-EDT.
           MOVE P16-COD-ACTI-EDT IN P16-EDT TO
                P16-COD-ACTI-ALF IN P16-FLD.
       FIN-EDT-IKY-P16.
           EXIT.

       EDT-MKY-P16 SECTION.
       INI-EDT-MKY-P16.
       FIN-EDT-MKY-P16.
           EXIT.

       EDT-AKY-P16 SECTION.
       INI-EDT-AKY-P16.
       FIN-EDT-AKY-P16.
           EXIT.

       EDT-FLD-P16 SECTION.
       INI-EDT-FLD-P16.
           IF P16-VAL-SLDO-LEN IN P16-FLD = -9
                MOVE -1 TO P16-VAL-SLDO-LEN IN P16-FLD
                GO TO FIN-EDT-FLD-P16.
           MOVE P16-VAL-SLDO IN P16-FLD TO P16-VAL-SLDO-EDT IN P16-EDT.
           MOVE P16-VAL-SLDO-EDT IN P16-EDT TO
                P16-VAL-SLDO-ALF IN P16-FLD.
       FIN-EDT-FLD-P16.
           EXIT.
