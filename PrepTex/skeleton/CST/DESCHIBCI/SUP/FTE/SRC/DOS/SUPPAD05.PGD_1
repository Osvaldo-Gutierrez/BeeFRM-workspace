       COPY GNSBPVRF.
       COPY SGCBFDBC.
       COPY TABBBOFI.
       COPY TABBFOFI.
       COPY TABBVOFI.
       COPY TABBBUSR.
       COPY TABBFUSR.
       COPY TABBVUSR.
       COPY GNSBGCPT.
       COPY GNSBGFEC.
       COPY GNSBGHOY.
       COPY GNSBFTAB.
       COPY GNSBEGLS.
       COPY GNSBVIND.
       COPY GNSBVMSC.
      *>>>>
