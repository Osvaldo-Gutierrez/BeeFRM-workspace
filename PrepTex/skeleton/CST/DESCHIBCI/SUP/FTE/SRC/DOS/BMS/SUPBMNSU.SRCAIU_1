      * 22-May-97 11:37 AM
       PUT-ALL-NSU SECTION.
       INI-PUT-ALL-NSU.
           PERFORM PUT-NSU-NSU.
       FIN-PUT-ALL-NSU.
           EXIT.

       PUT-NSU-NSU SECTION.
       INI-PUT-NSU-NSU.
           IF PGM-STAT-NSU-NUL
               GO TO FIN-PUT-NSU-NSU.
           IF NOT PGM-STAT-NSU-OKS
               PERFORM CLR-NSU-NSU
               GO TO FIN-PUT-NSU-NSU.
           MOVE NSU-NUM-RUT IN NSU TO NSU-NUM-RUT IN NSU-FLD.
           MOVE NSU-VRF-RUT IN NSU TO NSU-VRF-RUT IN NSU-FLD.
           MOVE NSU-GLS-NOMC IN NSU TO NSU-GLS-NOMC IN NSU-FLD.
       FIN-PUT-NSU-NSU.
           EXIT.
      * 22-May-97 11:37 AM
       RET-ALL-NSU SECTION.
       INI-RET-ALL-NSU.
           PERFORM KEY-ALL-NSU.
           PERFORM RET-NSU-NSU.
       FIN-RET-ALL-NSU.
           EXIT.

       RET-NSU-NSU SECTION.
       INI-RET-NSU-NSU.
           MOVE NSU-GLS-NOMC IN NSU-FLD TO NSU-GLS-NOMC IN NSU.
           PERFORM GET-FHOY
           MOVE SCR-USER            TO NSU-USU-ULT-MOD IN NSU
           MOVE HOY-DHOY            TO NSU-NUM-DMOD    IN NSU
           MOVE HOY-MHOY            TO NSU-NUM-MMOD    IN NSU
           MOVE HOY-AHOY            TO NSU-NUM-AMOD    IN NSU
           MOVE HOY-SHOY            TO NSU-NUM-SMOD    IN NSU.
       FIN-RET-NSU-NSU.
           EXIT.

       KEY-ALL-NSU SECTION.
       INI-KEY-ALL-NSU.
           PERFORM KEY-NSU-NSU.
       FIN-KEY-ALL-NSU.
           EXIT.

       KEY-NSU-NSU SECTION.
       INI-KEY-NSU-NSU.
           MOVE NSU-NUM-RUT IN NSU-FLD TO NSU-NUM-RUT IN NSU.
           MOVE NSU-VRF-RUT IN NSU-FLD TO NSU-VRF-RUT IN NSU.
       FIN-KEY-NSU-NSU.
           EXIT.

       SCR-CLRA-NSU SECTION.
       INI-SCR-CLRA-NSU.
           PERFORM CLR-KEY-NSU.
           PERFORM CLR-FLD-NSU.
       FIN-SCR-CLRA-NSU.
           EXIT.

       CLR-KEY-NSU SECTION.
       INI-CLR-KEY-NSU.
           MOVE ZEROES TO NSU-NUM-RUT IN NSU-FLD.
           MOVE SPACES TO NSU-VRF-RUT IN NSU-FLD.
       FIN-CLR-KEY-NSU.
           EXIT.

       CLR-FLD-NSU SECTION.
       INI-CLR-FLD-NSU.
           PERFORM CLR-NSU-NSU.
       FIN-CLR-FLD-NSU.
           EXIT.

       CLR-NSU-NSU SECTION.
       INI-CLR-NSU-NSU.
           MOVE SPACES TO NSU-GLS-NOMC IN NSU-FLD.
       FIN-CLR-NSU-NSU.
           EXIT.

       FST-KEY-NSU SECTION.
       INI-FST-KEY-NSU.
           MOVE -1 TO NSU-NUM-RUT-LEN IN NSU-FLD.
       FIN-FST-KEY-NSU.
           EXIT.

       FST-FLD-NSU SECTION.
       INI-FST-FLD-NSU.
           MOVE -1 TO NSU-GLS-NOMC-LEN IN NSU-FLD.
       FIN-FST-FLD-NSU.
           EXIT.

       PRO-KEY-NSU SECTION.
       INI-PRO-KEY-NSU.
           PERFORM PRO-IKY-NSU.
           PERFORM PRO-AKY-NSU.
           PERFORM PRO-MKY-NSU.
       FIN-PRO-KEY-NSU.
           EXIT.

       PRO-IKY-NSU SECTION.
       INI-PRO-IKY-NSU.
           MOVE FRM-CPIM-AEY TO NSU-NUM-RUT-ATR IN NSU-FLD.
           MOVE FRM-CPIM-AEY TO NSU-VRF-RUT-ATR IN NSU-FLD.
       FIN-PRO-IKY-NSU.
           EXIT.

       PRO-MKY-NSU SECTION.
       INI-PRO-MKY-NSU.
       FIN-PRO-MKY-NSU.
           EXIT.

       PRO-AKY-NSU SECTION.
       INI-PRO-AKY-NSU.
       FIN-PRO-AKY-NSU.
           EXIT.

       PRO-FLD-NSU SECTION.
       INI-PRO-FLD-NSU.
           PERFORM PRO-NSU-NSU.
       FIN-PRO-FLD-NSU.
           EXIT.

       PRO-NSU-NSU SECTION.
       INI-PRO-NSU-NSU.
           MOVE FRM-CPIM-AEY TO NSU-GLS-NOMC-ATR IN NSU-FLD.
       FIN-PRO-NSU-NSU.
           EXIT.

       UNP-KEY-NSU SECTION.
       INI-UNP-KEY-NSU.
           PERFORM UNP-IKY-NSU.
           PERFORM UNP-AKY-NSU.
           PERFORM UNP-MKY-NSU.
       FIN-UNP-KEY-NSU.
           EXIT.

       UNP-IKY-NSU SECTION.
       INI-UNP-IKY-NSU.
           MOVE FRM-CPIM-UBY TO NSU-NUM-RUT-ATR.
           MOVE FRM-CPIM-UBY TO NSU-VRF-RUT-ATR.
       FIN-UNP-IKY-NSU.
           EXIT.

       UNP-MKY-NSU SECTION.
       INI-UNP-MKY-NSU.
       FIN-UNP-MKY-NSU.
           EXIT.

       UNP-AKY-NSU SECTION.
       INI-UNP-AKY-NSU.
       FIN-UNP-AKY-NSU.
           EXIT.

       UNP-FLD-NSU SECTION.
       INI-UNP-FLD-NSU.
           PERFORM UNP-NSU-NSU.
       FIN-UNP-FLD-NSU.
           EXIT.

       UNP-NSU-NSU SECTION.
       INI-UNP-NSU-NSU.
           MOVE FRM-CPIM-UBY TO NSU-GLS-NOMC-ATR IN NSU-FLD.
       FIN-UNP-NSU-NSU.
           EXIT.

       VAL-KEY-NSU SECTION.
       INI-VAL-KEY-NSU.
           PERFORM VAL-IKY-NSU.
           PERFORM VAL-MKY-NSU.
           PERFORM VAL-AKY-NSU.
       FIN-VAL-KEY-NSU.
           EXIT.

       VAL-IKY-NSU SECTION.
       INI-VAL-IKY-NSU.
           PERFORM VAL-NSU-NUM-RUT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO NSU-NUM-RUT-LEN IN NSU-FLD
                GO TO FIN-VAL-IKY-NSU.
           PERFORM VAL-NSU-VRF-RUT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO NSU-VRF-RUT-LEN IN NSU-FLD
                GO TO FIN-VAL-IKY-NSU.
       FIN-VAL-IKY-NSU.
           EXIT.

       VAL-MKY-NSU SECTION.
       INI-VAL-MKY-NSU.
       FIN-VAL-MKY-NSU.
           EXIT.

       VAL-AKY-NSU SECTION.
       INI-VAL-AKY-NSU.
       FIN-VAL-AKY-NSU.
           EXIT.

       VAL-FLD-NSU SECTION.
       INI-VAL-FLD-NSU.
           PERFORM VAL-NSU-GLS-NOMC.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO NSU-GLS-NOMC-LEN IN NSU-FLD
                GO TO FIN-VAL-FLD-NSU.
       FIN-VAL-FLD-NSU.
           EXIT.

       VAL-NSU-NUM-RUT SECTION.
       INI-VAL-NSU-NUM-RUT.
           IF NSU-NUM-RUT IN NSU-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-NSU-NUM-RUT.
       FIN-VAL-NSU-NUM-RUT.
           EXIT.

       VAL-NSU-VRF-RUT SECTION.
       INI-VAL-NSU-VRF-RUT.
           IF NSU-VRF-RUT IN NSU-FLD NOT > SPACES
              MOVE SPACES TO NSU-VRF-RUT IN NSU-FLD.
      *VAL-VRF Valida Verificador
           MOVE VRF-VAL TO VRF-CMND.
           MOVE VRF-SVRF-K TO VRF-SVRF.
           MOVE VRF-ZERO-S TO VRF-ZERO.
           MOVE NSU-NUM-RUT IN NSU-FLD TO VRF-NUME.
           MOVE NSU-VRF-RUT IN NSU-FLD TO VRF-DVRF.
           MOVE 7 TO VRF-LMAX.
           PERFORM PRO-VRF.
       FIN-VAL-NSU-VRF-RUT.
           EXIT.

       VAL-NSU-GLS-NOMC SECTION.
       INI-VAL-NSU-GLS-NOMC.
           IF NSU-GLS-NOMC IN NSU-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-NSU-GLS-NOMC.
       FIN-VAL-NSU-GLS-NOMC.
           EXIT.

       VAL-CON-KEY-NSU SECTION.
       INI-VAL-CON-KEY-NSU.
           IF     NSU-NUM-RUT IN NSU-FLD NOT > ZEROES
              AND NSU-VRF-RUT IN NSU-FLD NOT > SPACES
                PERFORM ERR-KEY
                PERFORM FST-KEY-NSU.
       FIN-VAL-CON-KEY-NSU.
           EXIT.

       VAL-CON-IKY-NSU SECTION.
       INI-VAL-CON-IKY-NSU.
           IF     NSU-NUM-RUT IN NSU-FLD NOT > ZEROES
              AND NSU-VRF-RUT IN NSU-FLD NOT > SPACES
                PERFORM ERR-KEY
                PERFORM FST-KEY-NSU.
       FIN-VAL-CON-IKY-NSU.
           EXIT.

       VAL-CON-FLD-NSU SECTION.
       INI-VAL-CON-FLD-NSU.
       FIN-VAL-CON-FLD-NSU.
           EXIT.

       VAL-CON-NSU SECTION.
       INI-VAL-CON-NSU.
       FIN-VAL-CON-NSU.
           EXIT.

       VAL-NUM-KEY-NSU SECTION.
       INI-VAL-NUM-KEY-NSU.
           PERFORM VAL-NUM-IKY-NSU.
           PERFORM VAL-NUM-MKY-NSU.
           PERFORM VAL-NUM-AKY-NSU.
       FIN-VAL-NUM-KEY-NSU.
           EXIT.

       VAL-NUM-IKY-NSU SECTION.
       INI-VAL-NUM-IKY-NSU.
       VAL-NUM-NSU-NUM-RUT.
           MOVE NSU-NUM-RUT-ALF IN NSU-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 9 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO NSU-NUM-RUT IN NSU-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO NSU-NUM-RUT-LEN IN NSU-FLD
                GO TO FIN-VAL-NUM-IKY-NSU.
       FIN-VAL-NUM-IKY-NSU.
           EXIT.

       VAL-NUM-MKY-NSU SECTION.
       INI-VAL-NUM-MKY-NSU.
       FIN-VAL-NUM-MKY-NSU.
           EXIT.

       VAL-NUM-AKY-NSU SECTION.
       INI-VAL-NUM-AKY-NSU.
       FIN-VAL-NUM-AKY-NSU.
           EXIT.

       VAL-NUM-FLD-NSU SECTION.
       INI-VAL-NUM-FLD-NSU.
       FIN-VAL-NUM-FLD-NSU.
           EXIT.

       EDT-KEY-NSU SECTION.
       INI-EDT-KEY-NSU.
           PERFORM EDT-IKY-NSU.
           PERFORM EDT-MKY-NSU.
           PERFORM EDT-AKY-NSU.
       FIN-EDT-KEY-NSU.
           EXIT.

       EDT-IKY-NSU SECTION.
       INI-EDT-IKY-NSU.
           IF NSU-NUM-RUT-LEN IN NSU-FLD = -9
                MOVE -1 TO NSU-NUM-RUT-LEN IN NSU-FLD
                GO TO FIN-EDT-IKY-NSU.
           MOVE NSU-NUM-RUT IN NSU-FLD TO NSU-NUM-RUT-EDT IN NSU-EDT.
           MOVE NSU-NUM-RUT-EDT IN NSU-EDT TO
                NSU-NUM-RUT-ALF IN NSU-FLD.
       FIN-EDT-IKY-NSU.
           EXIT.

       EDT-MKY-NSU SECTION.
       INI-EDT-MKY-NSU.
       FIN-EDT-MKY-NSU.
           EXIT.

       EDT-AKY-NSU SECTION.
       INI-EDT-AKY-NSU.
       FIN-EDT-AKY-NSU.
           EXIT.

       EDT-FLD-NSU SECTION.
       INI-EDT-FLD-NSU.
       FIN-EDT-FLD-NSU.
           EXIT.
