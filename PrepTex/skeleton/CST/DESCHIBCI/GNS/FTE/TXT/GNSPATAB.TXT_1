*%! Codigo adicional para programa
*%! LA SIGUIENTE INSTRUCCION PERMITE NO INCLUIR LOS
*%! COPYS GNSBRTAB Y GNSBFTAB DEL ESQUELETO,
*%! PUES EL REGISTRO TAB ES EL ARCHIVO BASE
*% NO_TIENE_TIMESTAMP = TRUE
*%! HDR_CST = FALSE
*% NO_HDR_CST = TRUE
*% NO_INC_TAB = TRUE
*% FRM = "TAB"
*% PADRE = "TAB"
*% KEY_PADRE = "TAB-COD-TABL"
*% LARGO_KEY_PADRE = 15
*% FIO_PADRE = "SIS-FIO-TAB"
*% SIST_PADRE = "GNS"
*% KEY_BROWSE_PADRE = "TAB-COD-TABL"
*% ACEPTE_PF7 = TRUE
*% ACEPTE_PF8 = TRUE
*% ORG_PADRE_VSI = TRUE
*% INCLUDE 'GNSSRC:PFSCONENT.SRC'
*% PGM_PTC = TRUE
*% LARGO_TOTAL_COMMAREA = 1369 
*% IF WSS
      *<<<< WSS
       COPY GNSWGTSK.
       COPY GNSMXW01.
      *>>>>
*% END
*% IF INI_FIO
      *<<<< INI_FIO
*% IF PGM_PQ
           MOVE TSK-TERM-ALF TO PTC-TT-TAB-TERM.                        
           MOVE TSK-TERM-ALF TO PTC-IT-TAB-TERM.                        
           MOVE TSK-TERM-ALF TO PTC-OT-TAB-TERM.                        
           MOVE TSK-TERM-ALF TO PTC-CT-TAB-TERM. 

           MOVE PTC-IT-TAB-COLA TO QUE-COLA.                            
           MOVE PTC-IT-TAB-LENG TO QUE-LITM.                            
           MOVE +1              TO QUE-NITM.                            
           MOVE QUE-GET         TO QUE-CMND.                            
           PERFORM GNS-PRO-QUE.                                         
           IF QUE-STAT = QUE-STAT-OKS                                   
              MOVE QUE-ITEM     TO IT-001                               
              MOVE IT-001-02-APT-COD-SIST TO SCR-SIST                   
              MOVE QUE-DEL TO QUE-CMND                                  
              PERFORM GNS-PRO-QUE.                                      
           IF SCR-SISG NOT > SPACES                                     
              MOVE SCR-SIST TO SCR-SISG.                                
                                                                        
           MOVE SPACES          TO QUE-ITEM.                            
           MOVE PTC-TT-TAB-COLA TO QUE-COLA.                            
           MOVE PTC-TT-TAB-LENG TO QUE-LITM.                            
           MOVE +1              TO QUE-NITM.                            
           MOVE QUE-GET         TO QUE-CMND.                            
           PERFORM GNS-PRO-QUE.                                         
           IF QUE-STAT = QUE-STAT-OKS                                   
              MOVE QUE-ITEM TO PTC-TT-TAB-CMND                          
              MOVE QUE-DEL  TO QUE-CMND                                 
              PERFORM GNS-PRO-QUE. 
*% END

*% IF PGM_PU
            IF SCR-DESDE-CMN                                             
               MOVE SPACES TO SCR-TAB-REQA.    
            IF SCR-SISG NOT > SPACES
               MOVE SCR-SIST TO SCR-SISG.
*% END
      *>>>>
*% END
*% IF FND_KEY
      *<<<< FND_KEY
       CON-FND-KEY.  
           MOVE FIO-GET-KEY TO FIO-CMND.     
           MOVE SCR-SISG TO FIO-SIST. 
       GO TO FND-TAB-NLS.
      *>>>>
*% END
*% IF FND_KEY OR CMN_ING OR INI_CMN_MOD
      *<<<< FND_KEY OR CMN_ING OR INI_CMN_MOD
           MOVE SCR-SISG TO FIO-SIST.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
       FND-TAB-NLS.
           IF SCR-OFRM-SRV                                              
              IF NOT PTC-TT-TAB-CMND = 'CUN'                            
                 MOVE FIO-GET-NLS TO FIO-CMND                           
              ELSE                                                      
                 NEXT SENTENCE                                          
           ELSE                                                         
              PERFORM GNS-FIO-TAB                                       
              MOVE FIO-STAT TO PGM-STAT-TAB.   
      *>>>>
*% END
*% IF PFS_SCR_GFLD
      *<<<< PFS_SCR_GFLD
           IF NOT ( ( SCR-CCPP = 'MOD' OR  SCR-CCPP = 'ACC' OR          
                      SCR-CCPP = 'ELI'                      OR          
                    ( SCR-CCPP = 'ACT' AND SCR-ACTU = SCR-ACTU-MOD ) )  
                      AND ( FRM-FFLD = FRM-FFLD-PF8 OR                  
                            FRM-FFLD = FRM-FFLD-PF7 ) )   
      *>>>>
*% END
*% IF PFS_GET_CON OR PFS_GET_ACC OR PFS_SCR_GKEY   
      *<<<< PFS_GET_CON OR PFS_GET_ACC OR PFS_SCR_GKEY   
*% IF PGM_PU
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 OR FRM-FFLD = FRM-FFLD-PF7 )
*% END
      *>>>>
*% END
*% IF PUT_SCR
      *<<<< PUT_SCR
           IF TAB-FEC-FTRN IN TAB IS NOT NUMERIC
               MOVE ZEROES TO TAB-FEC-FTRN IN TAB.
      *>>>>
*% END
*% IF FIN_PUT_SCR
      *<<<< FIN_PUT_SCR
      *PTC-INI:                                                         
           IF SCR-CMND = 'CON' AND                                      
              SCR-OFRM-SRV                                              
              MOVE PTC-OT-TAB-COLA TO QUE-COLA                          
              MOVE QUE-DEL TO QUE-CMND                                  
              PERFORM GNS-PRO-QUE                                       
                                                                        
              MOVE PTC-CT-TAB-COLA TO QUE-COLA                          
              MOVE QUE-DEL TO QUE-CMND                                  
              PERFORM GNS-PRO-QUE                                       
                                                                        
              MOVE ZEROES TO QUE-NITM                                   
              MOVE ZEROES TO FRM-IFLD                                   
              IF PTC-TT-TAB-CMND = 'CUN'                                
                 MOVE FIO-GET-KEY TO FIO-CMND                           
              ELSE                                                      
                 MOVE FIO-GET-NLS TO FIO-CMND                           
              END-IF                                                    
              IF TAB-COD-CTAB IN TAB-FLD > SPACES AND                   
                 TAB-COD-TTAB IN TAB NOT = 'MSC'                        
                 PERFORM GNS-FIO-TAB                                    
                 PERFORM VRF-FST-CON                                    
                 IF FIO-STAT-OKS                                        
                    IF NOT PTC-TT-TAB-CMND = 'CUN'                      
                       MOVE TAB TO MSC  
                       MOVE FIO-GET-NXT TO FIO-CMND                     
      * ISP-INI 30-09-2004 ERROR CUN-GNS-SON-S INFORMADO WIO            
                    ELSE                                                
                       NEXT SENTENCE                                    
      * ISP-FIN 30-09-2004 ERROR CUN-GNS-SON-S INFORMADO WIO            
                 ELSE                                                   
                    MOVE 'S' TO SCR-IERR                                
                    GO TO FIN-PUT-SCR.                                  
       CIC-PTC-TAB.                                                     
           IF SCR-CMND = 'CON' AND                                      
              SCR-OFRM-SRV                                              
              PERFORM GNS-FIO-TAB                                       
      *CAF-INI                                                          
              IF NOT FIO-STAT-OKS                                       
                 IF FIO-STAT-EOF                                        
                    MOVE '00' TO FIO-STAT                               
                    MOVE 15   TO FRM-IFLD                               
                    PERFORM PUT-QUE-TAB                                 
                 ELSE                                                   
                    MOVE 'S'  TO SCR-IERR                               
                 END-IF                                                 
                 GO TO FIN-PUT-SCR                                      
              ELSE                                                      
      *CAF-FIN                                                          
              MOVE 'N' TO WSS-VRF-TAB                                   
              PERFORM VRF-TAB                                           
              IF WSS-VRF-TAB NOT = 'N'                                  
                 IF WSS-VRF-TAB  = 'V'                                  
                    MOVE FIO-GET-NXT TO FIO-CMND                        
                    GO TO CIC-PTC-TAB                                   
                 ELSE                                                   
                    ADD 1 TO WSS-001-TOT-OCC                            
                    IF WSS-001-TOT-OCC NOT > IT-001-TOT-OCC             
                       MOVE FIO-STAT TO PGM-STAT-TAB                    
                       ADD 1 TO FRM-IFLD                                
                                                                        
                       MOVE TAB-COD-TTAB IN TAB TO                      
                            OT-001-02-TAB-COD-TTAB IN OT-001(FRM-IFLD)  
                       MOVE TAB-COD-CTAB IN TAB TO                      
                            OT-001-02-TAB-COD-CTAB IN OT-001(FRM-IFLD)  
                       MOVE SCR-SIST            TO                      
                            OT-001-03-APT-COD-SIST IN OT-001(FRM-IFLD)  
                       MOVE TAB-CIC-CTAB IN TAB TO                      
                            OT-001-03-APT-CIC-CTAB IN OT-001(FRM-IFLD)  
                       MOVE TAB-EXT-CTAB IN TAB TO                      
                            OT-001-03-APT-EXT-CTAB IN OT-001(FRM-IFLD)  
                       MOVE TAB-GLS-DESC IN TAB TO                      
                            OT-001-03-APT-GLS-DESC IN OT-001(FRM-IFLD)  
                       MOVE TAB-GLS-ABRV IN TAB TO                      
                            OT-001-03-APT-GLS-ABRV IN OT-001(FRM-IFLD)  
                       MOVE TAB-GLS-DCOR IN TAB TO                      
                            OT-001-03-APT-GLS-DCOR IN OT-001(FRM-IFLD)  
                       MOVE TAB-IND-VIGE IN TAB TO                      
                            OT-001-03-APT-IND-VIGE IN OT-001(FRM-IFLD)  
                                                                        
                       MOVE TAB-NUM-DTRN IN TAB TO                      
                            TAB-NUM-DTRN IN TAB-FLD                     
                       MOVE TAB-NUM-MTRN IN TAB TO                      
                            TAB-NUM-MTRN IN TAB-FLD                     
                       MOVE TAB-NUM-STRN IN TAB TO                      
                            TAB-NUM-STRN IN TAB-FLD                     
                       MOVE TAB-NUM-ATRN IN TAB TO                      
                            TAB-NUM-ATRN IN TAB-FLD                     
                                                                        
                       MOVE TAB-FEC-FTRN IN TAB-FLD TO                  
                            OT-001-03-APT-FEC-FTRN IN OT-001(FRM-IFLD)  
                       MOVE TAB-GLS-DATA IN TAB TO                      
                            OT-001-03-APT-GLS-DATA IN OT-001(FRM-IFLD)  
                                                                        
                       PERFORM PUT-QUE-TAB                              
                       IF NOT PTC-TT-TAB-CMND = 'CUN'                   
                          MOVE FIO-GET-NXT TO FIO-CMND                  
                          GO TO CIC-PTC-TAB                             
                       ELSE                                             
                          MOVE 15 TO FRM-IFLD                           
                          PERFORM PUT-QUE-TAB                           
                    ELSE                                                
                       IF FRM-IFLD > ZEROES                             
                          MOVE 15 TO FRM-IFLD                           
                          PERFORM PUT-QUE-TAB                           
                       ELSE                                             
                          NEXT SENTENCE                                 
              ELSE                                                      
                 IF FRM-IFLD > ZEROES                                   
                    MOVE 15 TO FRM-IFLD                                 
                    PERFORM PUT-QUE-TAB.  
      *>>>>
*% END
*% IF FIN_HBL_FLD
      *<<<< FIN_HBL_FLD
           IF SCR-CMND = 'COP'
               PERFORM UNP-IKY-TAB
               PERFORM FST-KEY-TAB.
      *>>>>
*% END
*% IF ABT_CMN_ACC
      *<<<< ABT_CMN_ACC
               IF SCR-CMND = 'COP'
                   MOVE 'GNS'          TO MSG-COD-SIST
                   MOVE 'REG    NOCOP' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC   TO FRM-MENS
                   GO TO FIN-CMN-ACC
               ELSE
      *>>>>
*% END
*% IF INI_CMN_ACC
      *<<<< INI_CMN_ACC
           IF SCR-CMND = 'COP'
               PERFORM CMN-ING
               IF FIO-STAT-OKS
                   MOVE 'GNS'          TO MSG-COD-SIST
                   MOVE 'REG    SICOP' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC   TO FRM-MENS
                   GO TO FIN-CMN-ACC
               ELSE
                   MOVE 'GNS'          TO MSG-COD-SIST
                   MOVE 'REG    ERCOP' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC   TO FRM-MENS
                   GO TO FIN-CMN-ACC.
      *>>>>
*% END
*% IF INI_CMN_ELI OR INI_CMN_ACC OR CMN_PGM
      *<<<< INI_CMN_ELI OR INI_CMN_ACC OR CMN_PGM
           MOVE SCR-SISG TO FIO-SIST.
      *>>>>
*% END
*% IF CMN_ING OR CMN_MOD
      *<<<< CMN_ING OR CMN_MOD
           IF TAB-COD-CTAB IN TAB-FLD > SPACES
               PERFORM GET-FHOY
               MOVE HOY-STP-ITRN TO TAB-STP-ITRN IN TAB.
      *>>>>
*% END
*% IF CMN_ING
      *<<<< CMN_ING
           MOVE SPACES TO TAB-GLS-FLAG IN TAB.
           MOVE ZEROES TO TAB-HRA-HRTR IN TAB.
           MOVE SPACES TO TAB-MSC-TACC IN TAB.
           MOVE SPACES TO TAB-COD-OTRN IN TAB.
           MOVE SPACES TO TAB-COD-ATRN IN TAB.
           MOVE SPACES TO TAB-SNX-TABL IN TAB.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
*% IF PGM_PQ
       COPY GNSBGVSM.
       SIS-FIO-TAB SECTION.                                             
       INI-SIS-FIO-TAB.                                                 
           MOVE SCR-SISG TO FIO-SIST.                                   
           PERFORM GNS-FIO-TAB.                                         
       FIN-SIS-FIO-TAB.                                                 
           EXIT.                                                        
*% END
                                                                        
       RST-STAT SECTION.                                                
       INI-RST-STAT.                                                    
           MOVE SCR-STPP     TO SCR-STAT.                               
           MOVE SCR-MFLD-NOT TO SCR-MFLD.                               
       FIN-RST-STAT.                                                    
           EXIT. 

       COPY GNSBVCOD.                                                   
       COPY GNSBVTAB.                                                   
       COPY GNSBGHOY.                                                   
       COPY GNSBEGLS.
*% IF PQM_PU
       COPY GNSBGFRM.
*% END
      *>>>>
*% END
*% IF EOF_IBM
      *<<<< IF EOF_IBM
*% IF PGM_PQ
       PUT-QUE-TAB SECTION.                                             
       INI-PUT-QUE-TAB.                                                 
           IF FRM-IFLD = 15                                             
              MOVE PTC-OT-TAB-COLA TO QUE-COLA                          
              MOVE OT-001          TO QUE-ITEM                          
              MOVE PTC-OT-TAB-LENG TO QUE-LITM                          
              MOVE QUE-PUT  TO QUE-CMND                                 
              PERFORM GNS-PRO-QUE                                       
              MOVE SPACES TO OT-001                                     
              MOVE ZEROES TO FRM-IFLD.                                  
       FIN-PUT-QUE-TAB.                                                 
           EXIT.                                                        
                                                                        
       VER-TAB SECTION.                                                 
       INI-VER-TAB.                                                     
      *     IF FIO-CMND = FIO-GET-NLS                                   
      *         DISPLAY 'REGISTRO RECUPERADO NLS ' TAB.                 
       FIN-VER-TAB.                                                     
           EXIT.                                                        
                                                                        
       VRF-TAB SECTION.                                                 
       INI-VRF-TAB.                                                     
           IF FIO-STAT-OKS                                              
              IF IT-001-02-APT-IND-VIGE > SPACES AND                    
                 IT-001-02-APT-IND-VIGE NOT = TAB-IND-VIGE IN TAB       
                 MOVE 'V' TO WSS-VRF-TAB                                
              ELSE                                                      
                 MOVE TAB TO MSC                                        
                 IF IT-001-02-APT-COD-TMSC > SPACES                     
                    IF IT-001-02-APT-COD-TMSC = MSC-COD-TMSC IN MSC     
                       MOVE 'S' TO WSS-VRF-TAB                          
                    ELSE                                                
                       NEXT SENTENCE                                    
                 ELSE                                                   
                 IF IT-001-01-TAB-COD-TTAB = TAB-COD-TTAB IN TAB        
                    MOVE 'S' TO WSS-VRF-TAB.                            
       FIN-VRF-TAB.                                                     
           EXIT.                                                        
                                                                        
       VRF-FST-CON SECTION.                                             
       INI-VRF-FST-CON.                                                 
           IF PGM-FST-LEC-TAB = 'S'                                     
              MOVE 'N' TO PGM-FST-LEC-TAB                               
              IF TAB-COD-CTAB IN TAB NOT = TAB-COD-CTAB IN TAB-FLD      
                 MOVE '23' TO FIO-STAT.                                 
       FIN-VRF-FST-CON.                                                 
           EXIT.                                                        
*% END
      *>>>>
*% END
*% IF EOF_PU
      *<<<< IF EOF_PU
       LEE-NXT-PRV SECTION.                                             
       INI-LEE-NXT-PRV.                                                 
           IF SCR-TAB-REQA > SPACES                                     
               PERFORM LEE-NXT-PRV-TAB                                  
           ELSE                                                         
               PERFORM LEE-NXT-PRV-FST.                                 
       FIN-LEE-NXT-PRV.                                                 
           EXIT.                                                        
                                                                        
       LEE-NXT-PRV-TAB SECTION.                                         
       INI-LEE-NXT-PRV-TAB.                                             
           MOVE SCR-TAB-REQA TO ADR-TAB-REQA.                           
           MOVE FIO-KDEF-S TO FIO-KDEF.                                 
           IF FRM-FFLD = FRM-FFLD-PF7                                   
               MOVE FIO-GET-PRV TO FIO-CMND                             
           ELSE                                                         
               MOVE FIO-GET-NXT TO FIO-CMND.                            
           MOVE SCR-SISG TO FIO-SIST.                                   
           PERFORM GNS-FIO-TAB.                                         
           IF FIO-STAT-OKS                                              
               MOVE '00' TO PGM-STAT-TAB                                
               MOVE ADR-TAB-REQA TO SCR-TAB-REQA.                       
       FIN-LEE-NXT-PRV-TAB.                                             
           EXIT.                                                        
                                                                        
                                                                        
       LEE-NXT-PRV-FST SECTION.                                         
       INI-LEE-NXT-PRV-FST.                                             
           IF TAB-COD-TTAB IN TAB > SPACES OR                           
              TAB-COD-CTAB IN TAB > SPACES                              
               MOVE FIO-GET-KEY    TO FIO-CMND                          
               MOVE SCR-SISG TO FIO-SIST                                
               PERFORM GNS-FIO-TAB                                      
               IF FIO-STAT-OKS                                          
                   IF FRM-FFLD = FRM-FFLD-PF7                           
                       MOVE FIO-GET-PRV TO FIO-CMND                     
                   ELSE                                                 
                       MOVE FIO-GET-NXT TO FIO-CMND                     
               ELSE                                                     
                   IF FRM-FFLD = FRM-FFLD-PF7                           
                       MOVE FIO-GET-LEQ TO FIO-CMND                     
                   ELSE                                                 
                       MOVE FIO-GET-NLS TO FIO-CMND                     
           ELSE                                                         
               IF FRM-FFLD = FRM-FFLD-PF7                               
                   MOVE FIO-GET-LEQ TO FIO-CMND                         
               ELSE                                                     
                   MOVE FIO-GET-NLS TO FIO-CMND.                        
           MOVE SCR-SISG TO FIO-SIST.                                   
           PERFORM GNS-FIO-TAB.                                         
           IF FIO-STAT-OKS                                              
               MOVE '00' TO PGM-STAT-TAB                                
               MOVE ADR-TAB-REQA TO SCR-TAB-REQA.                       
       FIN-LEE-NXT-PRV-FST.                                             
           EXIT.                                                        
                                                                        
                                                                        
       STS-NXT-PRV SECTION.                                             
       INI-STS-NXT-PRV.                                                 
           MOVE SPACE TO MSG-COD-MENS.                                  
           IF FIO-STAT-OKS                                              
                PERFORM SCR-CLRA-TAB                                    
                GO TO FIN-STS-NXT-PRV                                   
           ELSE                                                         
                MOVE SPACES TO PGM-STAT                                 
                                                                        
                MOVE 'GNS' TO MSG-COD-SIST                              
                PERFORM MSG-NXT-PRV                                     
                PERFORM GET-MSG                                         
                MOVE MSG-GLS-DESC TO FRM-MENS                           
                MOVE '00' TO FIO-STAT                                   
                MOVE SCR-STAT-CON TO SCR-STAT.                          
       FIN-STS-NXT-PRV.                                                 
           EXIT.                                                        
                                                                        
       MSG-NXT-PRV SECTION.                                             
       INI-MSG-NXT-PRV.                                                 
           IF FRM-FFLD = FRM-FFLD-PF7                                   
               MOVE 'TAB    NOPRV' TO MSG-COD-MENS                      
           ELSE                                                         
               MOVE 'TAB    NONXT' TO MSG-COD-MENS.                     
       FIN-MSG-NXT-PRV.                                                 
           EXIT. 

       VAL-IKY-TAB SECTION.                                             
       INI-VAL-IKY-TAB.                                                 
           PERFORM VAL-TAB-COD-TTAB.                                    
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR        
                MOVE -1 TO TAB-COD-TTAB-LEN IN TAB-FLD                  
                GO TO FIN-VAL-IKY-TAB.                                  
           PERFORM VAL-TAB-COD-CTAB.                                    
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR        
                MOVE -1 TO TAB-COD-CTAB-LEN IN TAB-FLD                  
                GO TO FIN-VAL-IKY-TAB.                                  
       FIN-VAL-IKY-TAB.                                                 
           EXIT.                                                        
                                                                        
       VAL-TAB-COD-TTAB SECTION.                                        
       INI-VAL-TAB-COD-TTAB.                                            
           IF TAB-COD-TTAB IN TAB-FLD NOT > SPACES                      
              MOVE SPACES TO TAB-COD-TTAB IN TAB-FLD.                   
      *<<<< TAB_COD_TTAB                                                
      *VAL-COD Valida codigo de tabla                                   
           IF TAB-COD-TTAB IN TAB-FLD NOT > SPACES AND                  
              NOT ( FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8 )           
                PERFORM ERR-GLS                                         
                GO TO FIN-VAL-TAB-COD-TTAB.                             
                                                                        
           IF TAB-COD-TTAB IN TAB-FLD NOT = 'TBL' AND                   
              NOT ( FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8 )           
               MOVE 'TBL' TO TAB-COD-TTAB IN TAB                        
               MOVE TAB-COD-TTAB IN TAB-FLD TO TAB-COD-CTAB IN TAB      
               MOVE SCR-SISG TO FIO-SIST                                
               MOVE FIO-GET-KEY TO FIO-CMND                             
               PERFORM GNS-FIO-TAB                                      
               IF NOT FIO-STAT-OKS                                      
                   MOVE 'GNS'        TO MSG-COD-SIST                    
                   MOVE 'TBL    NEX' TO MSG-COD-MENS                    
                   PERFORM GET-MSG                                      
                   MOVE MSG-GLS-DESC TO FRM-MENS                        
                   MOVE FRM-SUAR-MAL TO FRM-SUAR.                       
      *>>>>                                                             
       FIN-VAL-TAB-COD-TTAB.                                            
           EXIT.                                                        
                                                                        
       VAL-TAB-COD-CTAB SECTION.                                        
       INI-VAL-TAB-COD-CTAB.                                            
           IF TAB-COD-CTAB IN TAB-FLD NOT > SPACES                      
              MOVE SPACES TO TAB-COD-CTAB IN TAB-FLD.                   
      *<<<< TAB_COD_CTAB                                                
           IF TAB-COD-CTAB IN TAB-FLD NOT > SPACES                      
               MOVE SPACES TO TAB-COD-CTAB IN TAB-FLD                   
               GO TO FIN-VAL-TAB-COD-CTAB.                              
           MOVE TAB-COD-TTAB IN TAB-FLD TO TAB-COD-TTAB IN TAB.         
           MOVE SCR-SISG TO FIO-SIST.                                   
           MOVE SPACES TO TAB-COD-CTAB IN TAB.                          
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM GNS-FIO-TAB.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE 'TAB    NEX' TO MSG-COD-MENS                        
               MOVE 'GNS' TO MSG-COD-SIST                               
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE FRM-SUAR-MAL TO FRM-SUAR.                           
      *>>>>                                                             
       FIN-VAL-TAB-COD-CTAB.                                            
           EXIT.                                                        

      *>>>>
*% END
