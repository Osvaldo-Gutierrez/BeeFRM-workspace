      *
      * GNSBUPTK
      * Version 3.0.1 Nov-2005
      * 26-Sep-14 02:02 PM
      *
       SET-TAG-GNSBUPTK SECTION.
       INI-SET-TAG-GNSBUPTK.
           MOVE
           '<IDNMOD NOM=$GNSBUPTK$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$26-Sep-14 02:02 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-GNSBUPTK.
           EXIT.
           
       RET-ALL-PTK SECTION.
       INI-RET-ALL-PTK.
           PERFORM KEY-ALL-PTK.
           PERFORM RET-PTK-PTK.
           PERFORM RET-ITK-PTK.
       FIN-RET-ALL-PTK.
           EXIT.
       
       RET-PTK-PTK SECTION.
       INI-RET-PTK-PTK.
           MOVE PTK-MSC-STAT IN PTK-FLD TO PTK-MSC-STAT IN PTK.
           MOVE PTK-GLS-PARA IN PTK-FLD TO PTK-GLS-PARA IN PTK.
           MOVE PTK-GLS-DISP IN PTK-FLD TO PTK-GLS-DISP IN PTK.
       FIN-RET-PTK-PTK.
           EXIT.
       
       RET-ITK-PTK SECTION.
       INI-RET-ITK-PTK.
       FIN-RET-ITK-PTK.
           EXIT.
       
       KEY-ALL-PTK SECTION.
       INI-KEY-ALL-PTK.
           PERFORM KEY-PTK-PTK.
           PERFORM KEY-ITK-PTK.
       FIN-KEY-ALL-PTK.
           EXIT.
       
       KEY-PTK-PTK SECTION.
       INI-KEY-PTK-PTK.
           MOVE PTK-CAI-IITK IN PTK-FLD TO PTK-CAI-IITK IN PTK.
           MOVE PTK-IIC-IITK IN PTK-FLD TO PTK-IIC-IITK IN PTK.
           MOVE PTK-NUM-SECU IN PTK-FLD TO PTK-NUM-SECU IN PTK.
           MOVE PTK-NUM-CORP IN PTK-FLD TO PTK-NUM-CORP IN PTK.
       FIN-KEY-PTK-PTK.
           EXIT.
       
       KEY-ITK-PTK SECTION.
       INI-KEY-ITK-PTK.
           MOVE ITK-COD-SIST IN PTK-FLD TO ITK-COD-SIST IN ITK.
           MOVE ITK-COD-TPRO IN PTK-FLD TO ITK-COD-TPRO IN ITK.
           MOVE ITK-COD-ENTI IN PTK-FLD TO ITK-COD-ENTI IN ITK.
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE ITK-NUM-DPRO IN PTK-FLD TO ITK-NUM-DPRO IN ITK.
           MOVE ITK-NUM-MPRO IN PTK-FLD TO ITK-NUM-MPRO IN ITK.
           MOVE ITK-NUM-SPRO IN PTK-FLD TO ITK-NUM-SPRO IN ITK.
           MOVE ITK-NUM-APRO IN PTK-FLD TO ITK-NUM-APRO IN ITK.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE ITK-HRA-HRPR IN PTK-FLD TO ITK-HRA-HRPR IN ITK.
           MOVE ITK-NUM-UNIC IN PTK-FLD TO ITK-NUM-UNIC IN ITK.
       FIN-KEY-ITK-PTK.
           EXIT.
       
       SCR-CLRA-PTK SECTION.
       INI-SCR-CLRA-PTK.
           PERFORM CLR-KEY-PTK.
           PERFORM CLR-FLD-PTK.
       FIN-SCR-CLRA-PTK.
           EXIT.
       
       CLR-KEY-PTK SECTION.
       INI-CLR-KEY-PTK.
           MOVE SPACES TO PTK-CAI-IITK IN PTK-FLD.
           MOVE SPACES TO PTK-IIC-IITK IN PTK-FLD.
           MOVE ZEROES TO PTK-NUM-SECU IN PTK-FLD.
           MOVE ZEROES TO PTK-NUM-CORP IN PTK-FLD.
           MOVE SPACES TO ITK-COD-SIST IN PTK-FLD.
           MOVE SPACES TO ITK-COD-TPRO IN PTK-FLD.
           MOVE SPACES TO ITK-COD-ENTI IN PTK-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO ITK-FEC-FPRO IN PTK-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO ITK-HRA-HRPR IN PTK-FLD.
           MOVE SPACES TO ITK-NUM-UNIC IN PTK-FLD.
      *<<<< FIN_CLR_KEY_PTK                                             
           MOVE ZEROES TO ITK-HRA-HRPR IN PTK-FLD.                      
           PERFORM GET-FHOY.                                            
           MOVE HOY-DHOY TO ITK-NUM-DPRO IN PTK-FLD.                    
           MOVE HOY-MHOY TO ITK-NUM-MPRO IN PTK-FLD.                    
           MOVE HOY-SHOY TO ITK-NUM-SPRO IN PTK-FLD.                    
           MOVE HOY-AHOY TO ITK-NUM-APRO IN PTK-FLD.                    
      *>>>>                                         
       FIN-CLR-KEY-PTK.
           EXIT.
       
       CLR-FLD-PTK SECTION.
       INI-CLR-FLD-PTK.
           MOVE SPACES TO FRM-GLS-SIST IN PTK-FLD.
           MOVE SPACES TO FRM-GLS-TPRO IN PTK-FLD.
           PERFORM CLR-PTK-PTK.
           PERFORM CLR-ITK-PTK.
       FIN-CLR-FLD-PTK.
           EXIT.
       
       CLR-PTK-PTK SECTION.
       INI-CLR-PTK-PTK.
           MOVE SPACES TO PTK-MSC-STAT IN PTK-FLD.
           MOVE SPACES TO PTK-GLS-PARA IN PTK-FLD.
           MOVE SPACES TO PTK-GLS-DISP IN PTK-FLD.
       FIN-CLR-PTK-PTK.
           EXIT.
       
       CLR-ITK-PTK SECTION.
       INI-CLR-ITK-PTK.
       FIN-CLR-ITK-PTK.
           EXIT.
       
       FST-KEY-PTK SECTION.
       INI-FST-KEY-PTK.
           MOVE -1 TO PTK-CAI-IITK-LEN IN PTK-FLD.
       FIN-FST-KEY-PTK.
           EXIT.
       
       FST-FLD-PTK SECTION.
       INI-FST-FLD-PTK.
           MOVE -1 TO PTK-MSC-STAT-LEN IN PTK-FLD.
       FIN-FST-FLD-PTK.
           EXIT.
       
       PRO-KEY-PTK SECTION.
       INI-PRO-KEY-PTK.
           PERFORM PRO-MKY-PTK.
       FIN-PRO-KEY-PTK.
           EXIT.
       
       PRO-MKY-PTK SECTION.
       INI-PRO-MKY-PTK.
       FIN-PRO-MKY-PTK.
           EXIT.
           MOVE FRM-CPIM-AEY TO ITK-FEC-FPRO-ATR IN PTK-FLD.
       
       PRO-FLD-PTK SECTION.
       INI-PRO-FLD-PTK.
           PERFORM PRO-PTK-PTK.
           PERFORM PRO-ITK-PTK.
       FIN-PRO-FLD-PTK.
           EXIT.
       
       PRO-PTK-PTK SECTION.
       INI-PRO-PTK-PTK.
           MOVE FRM-CPIM-AEY TO PTK-MSC-STAT-ATR IN PTK-FLD.
           MOVE FRM-CPIM-AEY TO PTK-GLS-PARA-ATR IN PTK-FLD.
           MOVE FRM-CPIM-AEY TO PTK-GLS-DISP-ATR IN PTK-FLD.
       FIN-PRO-PTK-PTK.
           EXIT.
       
       PRO-ITK-PTK SECTION.
       INI-PRO-ITK-PTK.
       FIN-PRO-ITK-PTK.
           EXIT.
       
       UNP-KEY-PTK SECTION.
       INI-UNP-KEY-PTK.
           PERFORM UNP-IKY-PTK.
           PERFORM UNP-AKY-PTK.
           PERFORM UNP-MKY-PTK.
       FIN-UNP-KEY-PTK.
           EXIT.
       
       UNP-IKY-PTK SECTION.
       INI-UNP-IKY-PTK.
           MOVE FRM-CPIM-UBY TO PTK-CAI-IITK-ATR.
           MOVE FRM-CPIM-UBY TO PTK-IIC-IITK-ATR.
           MOVE FRM-CPIM-UBY TO PTK-NUM-SECU-ATR.
           MOVE FRM-CPIM-UBY TO PTK-NUM-CORP-ATR.
       FIN-UNP-IKY-PTK.
           EXIT.
       
       UNP-MKY-PTK SECTION.
       INI-UNP-MKY-PTK.
       FIN-UNP-MKY-PTK.
           EXIT.
       
       UNP-AKY-PTK SECTION.
       INI-UNP-AKY-PTK.
           MOVE FRM-CPIM-UBY TO ITK-COD-SIST-ATR.
           MOVE FRM-CPIM-UBY TO ITK-COD-TPRO-ATR.
           MOVE FRM-CPIM-UBY TO ITK-COD-ENTI-ATR.
           MOVE FRM-CPIM-UBY TO ITK-FEC-FPRO-ATR IN PTK-FLD.
           MOVE FRM-CPIM-UBY TO ITK-HRA-HRPR-ATR.
           MOVE FRM-CPIM-UBY TO ITK-NUM-UNIC-ATR.
       FIN-UNP-AKY-PTK.
           EXIT.
       
       UNP-FLD-PTK SECTION.
       INI-UNP-FLD-PTK.
           PERFORM UNP-PTK-PTK.
           PERFORM UNP-ITK-PTK.
       FIN-UNP-FLD-PTK.
           EXIT.
       
       UNP-PTK-PTK SECTION.
       INI-UNP-PTK-PTK.
           MOVE FRM-CPIM-UBY TO PTK-MSC-STAT-ATR IN PTK-FLD.
           MOVE FRM-CPIM-UBY TO PTK-GLS-PARA-ATR IN PTK-FLD.
           MOVE FRM-CPIM-UBY TO PTK-GLS-DISP-ATR IN PTK-FLD.
       FIN-UNP-PTK-PTK.
           EXIT.
       
       UNP-ITK-PTK SECTION.
       INI-UNP-ITK-PTK.
       FIN-UNP-ITK-PTK.
           EXIT.
       
       VAL-MKY-PTK SECTION.
       INI-VAL-MKY-PTK.
       FIN-VAL-MKY-PTK.
           EXIT.
       
       VAL-FLD-PTK SECTION.
       INI-VAL-FLD-PTK.
           PERFORM VAL-FRM-GLS-SIST.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-SIST-LEN IN PTK-FLD
                GO TO FIN-VAL-FLD-PTK.
           PERFORM VAL-FRM-GLS-TPRO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TPRO-LEN IN PTK-FLD
                GO TO FIN-VAL-FLD-PTK.
           PERFORM VAL-PTK-MSC-STAT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PTK-MSC-STAT-LEN IN PTK-FLD
                GO TO FIN-VAL-FLD-PTK.
           PERFORM VAL-PTK-GLS-PARA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PTK-GLS-PARA-LEN IN PTK-FLD
                GO TO FIN-VAL-FLD-PTK.
           PERFORM VAL-PTK-GLS-DISP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PTK-GLS-DISP-LEN IN PTK-FLD
                GO TO FIN-VAL-FLD-PTK.
       FIN-VAL-FLD-PTK.
           EXIT.
       
       VAL-FRM-GLS-SIST SECTION.
       INI-VAL-FRM-GLS-SIST.
       FIN-VAL-FRM-GLS-SIST.
           EXIT.
       
       VAL-FRM-GLS-TPRO SECTION.
       INI-VAL-FRM-GLS-TPRO.
       FIN-VAL-FRM-GLS-TPRO.
           EXIT.
       
       VAL-PTK-MSC-STAT SECTION.
       INI-VAL-PTK-MSC-STAT.
           IF PTK-MSC-STAT IN PTK-FLD NOT > SPACES
              MOVE SPACES TO PTK-MSC-STAT IN PTK-FLD.
       FIN-VAL-PTK-MSC-STAT.
           EXIT.
       
       VAL-PTK-GLS-PARA SECTION.
       INI-VAL-PTK-GLS-PARA.
           IF PTK-GLS-PARA IN PTK-FLD NOT > SPACES
              MOVE SPACES TO PTK-GLS-PARA IN PTK-FLD.
       FIN-VAL-PTK-GLS-PARA.
           EXIT.
       
       VAL-PTK-GLS-DISP SECTION.
       INI-VAL-PTK-GLS-DISP.
           IF PTK-GLS-DISP IN PTK-FLD NOT > SPACES
              MOVE SPACES TO PTK-GLS-DISP IN PTK-FLD.
       FIN-VAL-PTK-GLS-DISP.
           EXIT.
       
       VAL-CON-FLD-PTK SECTION.
       INI-VAL-CON-FLD-PTK.
       FIN-VAL-CON-FLD-PTK.
           EXIT.
       
       VAL-CON-PTK SECTION.
       INI-VAL-CON-PTK.
       FIN-VAL-CON-PTK.
           EXIT.
       
       VAL-NUM-KEY-PTK SECTION.
       INI-VAL-NUM-KEY-PTK.
           PERFORM VAL-NUM-IKY-PTK.
           PERFORM VAL-NUM-MKY-PTK.
           PERFORM VAL-NUM-AKY-PTK.
       FIN-VAL-NUM-KEY-PTK.
           EXIT.
       
       VAL-NUM-IKY-PTK SECTION.
       INI-VAL-NUM-IKY-PTK.
       VAL-NUM-PTK-NUM-SECU.
           MOVE PTK-NUM-SECU-ALF IN PTK-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 4 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO PTK-NUM-SECU IN PTK-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO PTK-NUM-SECU-LEN IN PTK-FLD
                GO TO FIN-VAL-NUM-IKY-PTK.
       VAL-NUM-PTK-NUM-CORP.
           MOVE PTK-NUM-CORP-ALF IN PTK-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 4 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO PTK-NUM-CORP IN PTK-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO PTK-NUM-CORP-LEN IN PTK-FLD
                GO TO FIN-VAL-NUM-IKY-PTK.
       FIN-VAL-NUM-IKY-PTK.
           EXIT.
       
       VAL-NUM-MKY-PTK SECTION.
       INI-VAL-NUM-MKY-PTK.
       FIN-VAL-NUM-MKY-PTK.
           EXIT.
       
       VAL-NUM-AKY-PTK SECTION.
       INI-VAL-NUM-AKY-PTK.
       FIN-VAL-NUM-AKY-PTK.
           EXIT.
       
       VAL-NUM-FLD-PTK SECTION.
       INI-VAL-NUM-FLD-PTK.
       FIN-VAL-NUM-FLD-PTK.
           EXIT.
       
       EDT-KEY-PTK SECTION.
       INI-EDT-KEY-PTK.
           PERFORM EDT-IKY-PTK.
           PERFORM EDT-MKY-PTK.
           PERFORM EDT-AKY-PTK.
       FIN-EDT-KEY-PTK.
           EXIT.
       
       EDT-IKY-PTK SECTION.
       INI-EDT-IKY-PTK.
           IF PTK-NUM-SECU-LEN IN PTK-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PTK-NUM-SECU-LEN IN PTK-FLD
                GO TO FIN-EDT-IKY-PTK.
           MOVE PTK-NUM-SECU IN PTK-FLD TO PTK-NUM-SECU-EDT IN PTK-EDT.
           MOVE PTK-NUM-SECU-EDT IN PTK-EDT TO 
                PTK-NUM-SECU-ALF IN PTK-FLD.
           IF PTK-NUM-CORP-LEN IN PTK-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PTK-NUM-CORP-LEN IN PTK-FLD
                GO TO FIN-EDT-IKY-PTK.
           MOVE PTK-NUM-CORP IN PTK-FLD TO PTK-NUM-CORP-EDT IN PTK-EDT.
           MOVE PTK-NUM-CORP-EDT IN PTK-EDT TO 
                PTK-NUM-CORP-ALF IN PTK-FLD.
       FIN-EDT-IKY-PTK.
           EXIT.
       
       EDT-MKY-PTK SECTION.
       INI-EDT-MKY-PTK.
       FIN-EDT-MKY-PTK.
           EXIT.
       
       EDT-AKY-PTK SECTION.
       INI-EDT-AKY-PTK.
       FIN-EDT-AKY-PTK.
           EXIT.
       
       EDT-FLD-PTK SECTION.
       INI-EDT-FLD-PTK.
       FIN-EDT-FLD-PTK.
           EXIT.
