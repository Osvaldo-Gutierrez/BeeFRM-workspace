      *
      * GNSBUTAB
      * Version 3.0.1 Nov-2005
      * 01-Oct-14 06:29 PM
      *
       SET-TAG-GNSBUTAB SECTION.
       INI-SET-TAG-GNSBUTAB.
           MOVE
           '<IDNMOD NOM=$GNSBUTAB$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$01-Oct-14 06:29 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-GNSBUTAB.
           EXIT.
           
       RET-ALL-TAB SECTION.
       INI-RET-ALL-TAB.
           PERFORM KEY-ALL-TAB.
           PERFORM RET-TAB-TAB.
       FIN-RET-ALL-TAB.
           EXIT.
       
       RET-TAB-TAB SECTION.
       INI-RET-TAB-TAB.
           MOVE TAB-CIC-CTAB IN TAB-FLD TO TAB-CIC-CTAB IN TAB.
           MOVE TAB-EXT-CTAB IN TAB-FLD TO TAB-EXT-CTAB IN TAB.
           MOVE TAB-GLS-DESC IN TAB-FLD TO TAB-GLS-DESC IN TAB.
           MOVE TAB-GLS-ABRV IN TAB-FLD TO TAB-GLS-ABRV IN TAB.
           MOVE TAB-GLS-DCOR IN TAB-FLD TO TAB-GLS-DCOR IN TAB.
           MOVE TAB-IND-VIGE IN TAB-FLD TO TAB-IND-VIGE IN TAB.
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE TAB-NUM-DTRN IN TAB-FLD TO TAB-NUM-DTRN IN TAB.
           MOVE TAB-NUM-MTRN IN TAB-FLD TO TAB-NUM-MTRN IN TAB.
           MOVE TAB-NUM-STRN IN TAB-FLD TO TAB-NUM-STRN IN TAB.
           MOVE TAB-NUM-ATRN IN TAB-FLD TO TAB-NUM-ATRN IN TAB.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE TAB-GLS-DATA IN TAB-FLD TO TAB-GLS-DATA IN TAB.
       FIN-RET-TAB-TAB.
           EXIT.
       
       KEY-ALL-TAB SECTION.
       INI-KEY-ALL-TAB.
           PERFORM KEY-TAB-TAB.
       FIN-KEY-ALL-TAB.
           EXIT.
       
       KEY-TAB-TAB SECTION.
       INI-KEY-TAB-TAB.
           MOVE TAB-COD-TTAB IN TAB-FLD TO TAB-COD-TTAB IN TAB.
           MOVE TAB-COD-CTAB IN TAB-FLD TO TAB-COD-CTAB IN TAB.
       FIN-KEY-TAB-TAB.
           EXIT.
       
       SCR-CLRA-TAB SECTION.
       INI-SCR-CLRA-TAB.
           PERFORM CLR-KEY-TAB.
           PERFORM CLR-FLD-TAB.
       FIN-SCR-CLRA-TAB.
           EXIT.
       
       CLR-KEY-TAB SECTION.
       INI-CLR-KEY-TAB.
           MOVE SPACES TO TAB-COD-TTAB IN TAB-FLD.
           MOVE SPACES TO TAB-COD-CTAB IN TAB-FLD.
       FIN-CLR-KEY-TAB.
           EXIT.
       
       CLR-FLD-TAB SECTION.
       INI-CLR-FLD-TAB.
           PERFORM CLR-TAB-TAB.
       FIN-CLR-FLD-TAB.
           EXIT.
       
       CLR-TAB-TAB SECTION.
       INI-CLR-TAB-TAB.
           MOVE SPACES TO TAB-CIC-CTAB IN TAB-FLD.
           MOVE SPACES TO TAB-EXT-CTAB IN TAB-FLD.
           MOVE SPACES TO TAB-GLS-DESC IN TAB-FLD.
           MOVE SPACES TO TAB-GLS-ABRV IN TAB-FLD.
           MOVE SPACES TO TAB-GLS-DCOR IN TAB-FLD.
           MOVE SPACES TO TAB-IND-VIGE IN TAB-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO TAB-FEC-FTRN IN TAB-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO TAB-GLS-DATA IN TAB-FLD.
       FIN-CLR-TAB-TAB.
           EXIT.
       
       FST-KEY-TAB SECTION.
       INI-FST-KEY-TAB.
           MOVE -1 TO TAB-COD-TTAB-LEN IN TAB-FLD.
       FIN-FST-KEY-TAB.
           EXIT.
       
       FST-FLD-TAB SECTION.
       INI-FST-FLD-TAB.
           MOVE -1 TO TAB-CIC-CTAB-LEN IN TAB-FLD.
       FIN-FST-FLD-TAB.
           EXIT.
       
       PRO-KEY-TAB SECTION.
       INI-PRO-KEY-TAB.
           PERFORM PRO-MKY-TAB.
       FIN-PRO-KEY-TAB.
           EXIT.
       
       PRO-MKY-TAB SECTION.
       INI-PRO-MKY-TAB.
       FIN-PRO-MKY-TAB.
           EXIT.
       
       PRO-FLD-TAB SECTION.
       INI-PRO-FLD-TAB.
           PERFORM PRO-TAB-TAB.
       FIN-PRO-FLD-TAB.
           EXIT.
       
       PRO-TAB-TAB SECTION.
       INI-PRO-TAB-TAB.
           MOVE FRM-CPIM-AEY TO TAB-CIC-CTAB-ATR IN TAB-FLD.
           MOVE FRM-CPIM-AEY TO TAB-EXT-CTAB-ATR IN TAB-FLD.
           MOVE FRM-CPIM-AEY TO TAB-GLS-DESC-ATR IN TAB-FLD.
           MOVE FRM-CPIM-AEY TO TAB-GLS-ABRV-ATR IN TAB-FLD.
           MOVE FRM-CPIM-AEY TO TAB-GLS-DCOR-ATR IN TAB-FLD.
           MOVE FRM-CPIM-AEY TO TAB-IND-VIGE-ATR IN TAB-FLD.
           MOVE FRM-CPIM-AEY TO TAB-GLS-DATA-ATR IN TAB-FLD.
       FIN-PRO-TAB-TAB.
           EXIT.
       
       UNP-KEY-TAB SECTION.
       INI-UNP-KEY-TAB.
           PERFORM UNP-IKY-TAB.
           PERFORM UNP-AKY-TAB.
           PERFORM UNP-MKY-TAB.
       FIN-UNP-KEY-TAB.
           EXIT.
       
       UNP-IKY-TAB SECTION.
       INI-UNP-IKY-TAB.
           MOVE FRM-CPIM-UBY TO TAB-COD-TTAB-ATR.
           MOVE FRM-CPIM-UBY TO TAB-COD-CTAB-ATR.
       FIN-UNP-IKY-TAB.
           EXIT.
       
       UNP-MKY-TAB SECTION.
       INI-UNP-MKY-TAB.
       FIN-UNP-MKY-TAB.
           EXIT.
       
       UNP-AKY-TAB SECTION.
       INI-UNP-AKY-TAB.
       FIN-UNP-AKY-TAB.
           EXIT.
       
       UNP-FLD-TAB SECTION.
       INI-UNP-FLD-TAB.
           PERFORM UNP-TAB-TAB.
       FIN-UNP-FLD-TAB.
           EXIT.
       
       UNP-TAB-TAB SECTION.
       INI-UNP-TAB-TAB.
           MOVE FRM-CPIM-UBY TO TAB-CIC-CTAB-ATR IN TAB-FLD.
           MOVE FRM-CPIM-UBY TO TAB-EXT-CTAB-ATR IN TAB-FLD.
           MOVE FRM-CPIM-UBY TO TAB-GLS-DESC-ATR IN TAB-FLD.
           MOVE FRM-CPIM-UBY TO TAB-GLS-ABRV-ATR IN TAB-FLD.
           MOVE FRM-CPIM-UBY TO TAB-GLS-DCOR-ATR IN TAB-FLD.
           MOVE FRM-CPIM-UBY TO TAB-IND-VIGE-ATR IN TAB-FLD.
           MOVE FRM-CPIM-UBY TO TAB-GLS-DATA-ATR IN TAB-FLD.
       FIN-UNP-TAB-TAB.
           EXIT.
       
       VAL-MKY-TAB SECTION.
       INI-VAL-MKY-TAB.
       FIN-VAL-MKY-TAB.
           EXIT.
       
       VAL-FLD-TAB SECTION.
       INI-VAL-FLD-TAB.
           PERFORM VAL-TAB-CIC-CTAB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAB-CIC-CTAB-LEN IN TAB-FLD
                GO TO FIN-VAL-FLD-TAB.
           PERFORM VAL-TAB-EXT-CTAB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAB-EXT-CTAB-LEN IN TAB-FLD
                GO TO FIN-VAL-FLD-TAB.
           PERFORM VAL-TAB-GLS-DESC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAB-GLS-DESC-LEN IN TAB-FLD
                GO TO FIN-VAL-FLD-TAB.
           PERFORM VAL-TAB-GLS-ABRV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAB-GLS-ABRV-LEN IN TAB-FLD
                GO TO FIN-VAL-FLD-TAB.
           PERFORM VAL-TAB-GLS-DCOR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAB-GLS-DCOR-LEN IN TAB-FLD
                GO TO FIN-VAL-FLD-TAB.
           PERFORM VAL-TAB-IND-VIGE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAB-IND-VIGE-LEN IN TAB-FLD
                GO TO FIN-VAL-FLD-TAB.
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-TAB-FEC-FTRN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                TAB-FEC-FTRN-LEN IN TAB-FLD
                GO TO FIN-VAL-FLD-TAB.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-TAB-GLS-DATA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAB-GLS-DATA-LEN IN TAB-FLD
                GO TO FIN-VAL-FLD-TAB.
       FIN-VAL-FLD-TAB.
           EXIT.
       
       VAL-TAB-CIC-CTAB SECTION.
       INI-VAL-TAB-CIC-CTAB.
           IF TAB-CIC-CTAB IN TAB-FLD NOT > SPACES
              MOVE SPACES TO TAB-CIC-CTAB IN TAB-FLD.
      *<<<< TAB_CIC_CTAB
           IF TAB-CIC-CTAB IN TAB-FLD NOT > SPACES
               MOVE TAB-COD-CTAB IN TAB-FLD TO TAB-CIC-CTAB IN TAB-FLD.
      *>>>>
       FIN-VAL-TAB-CIC-CTAB.
           EXIT.
       
       VAL-TAB-EXT-CTAB SECTION.
       INI-VAL-TAB-EXT-CTAB.
           IF TAB-EXT-CTAB IN TAB-FLD NOT > SPACES
              MOVE SPACES TO TAB-EXT-CTAB IN TAB-FLD.
       FIN-VAL-TAB-EXT-CTAB.
           EXIT.
       
       VAL-TAB-GLS-DESC SECTION.
       INI-VAL-TAB-GLS-DESC.
           IF TAB-GLS-DESC IN TAB-FLD NOT > SPACES
              MOVE SPACES TO TAB-GLS-DESC IN TAB-FLD.
       FIN-VAL-TAB-GLS-DESC.
           EXIT.
       
       VAL-TAB-GLS-ABRV SECTION.
       INI-VAL-TAB-GLS-ABRV.
           IF TAB-GLS-ABRV IN TAB-FLD NOT > SPACES
              MOVE SPACES TO TAB-GLS-ABRV IN TAB-FLD.
       FIN-VAL-TAB-GLS-ABRV.
           EXIT.
       
       VAL-TAB-GLS-DCOR SECTION.
       INI-VAL-TAB-GLS-DCOR.
           IF TAB-GLS-DCOR IN TAB-FLD NOT > SPACES
              MOVE SPACES TO TAB-GLS-DCOR IN TAB-FLD.
       FIN-VAL-TAB-GLS-DCOR.
           EXIT.
       
       VAL-TAB-IND-VIGE SECTION.
       INI-VAL-TAB-IND-VIGE.
      *<<<< INI_TAB_IND_VIGE
      *    IF ( SCR-CCPP = 'ING' OR
      *       ( SCR-CCPP = 'ACT' AND SCR-ACTU = SCR-ACTU-ING )) AND
      *         TAB-COD-TTAB IN TAB-FLD = 'MSC' AND
      *       ( TAB-COD-CTAB IN TAB-FLD = 'VIGE' OR
      *         TAB-COD-CTAB IN TAB-FLD NOT > SPACES )
      *             GO TO FIN-VAL-TAB-IND-VIGE.
      *>>>>
           IF TAB-IND-VIGE IN TAB-FLD NOT > SPACES
              MOVE SPACES TO TAB-IND-VIGE IN TAB-FLD.
      *VAL-IND Valida miscelaneo de tabla
           MOVE 'GNS' TO MSC-COD-SIST.
           MOVE 'VIGE' TO MSC-COD-TMSC IN MSC.
           MOVE TAB-IND-VIGE IN TAB-FLD TO MSC-COD-CMSC IN MSC.
           PERFORM VAL-IND.
       FIN-VAL-TAB-IND-VIGE.
           EXIT.
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-TAB-FEC-FTRN SECTION.
       INI-VAL-TAB-FEC-FTRN.
           IF TAB-FEC-FTRN IN TAB-FLD  NOT > ZEROES
              MOVE ZEROES TO TAB-FEC-FTRN IN TAB-FLD.
       FIN-VAL-TAB-FEC-FTRN.
           EXIT.
      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-TAB-GLS-DATA SECTION.
       INI-VAL-TAB-GLS-DATA.
           IF TAB-GLS-DATA IN TAB-FLD NOT > SPACES
              MOVE SPACES TO TAB-GLS-DATA IN TAB-FLD.
       FIN-VAL-TAB-GLS-DATA.
           EXIT.
       
       VAL-CON-FLD-TAB SECTION.
       INI-VAL-CON-FLD-TAB.
      *<<<< VAL_CON_FLD_TAB
           PERFORM VAL-IKY-TAB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                GO TO FIN-VAL-CON-FLD-TAB.
           IF SCR-CMND = 'COP'
               PERFORM KEY-TAB-TAB
               MOVE SCR-SISG                TO FIO-SIST
               MOVE FIO-GET-KEY             TO FIO-CMND
               PERFORM GNS-FIO-TAB
               IF FIO-STAT-OKS
                   MOVE 'GNS'          TO MSG-COD-SIST
                   MOVE 'KEYPRIMDUPLI' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC   TO FRM-MENS
                   MOVE FRM-SUAR-MAL   TO FRM-SUAR.
      *>>>>
       FIN-VAL-CON-FLD-TAB.
           EXIT.
       
       VAL-CON-TAB SECTION.
       INI-VAL-CON-TAB.
       FIN-VAL-CON-TAB.
           EXIT.
       
       VAL-NUM-KEY-TAB SECTION.
       INI-VAL-NUM-KEY-TAB.
           PERFORM VAL-NUM-IKY-TAB.
           PERFORM VAL-NUM-MKY-TAB.
           PERFORM VAL-NUM-AKY-TAB.
       FIN-VAL-NUM-KEY-TAB.
           EXIT.
       
       VAL-NUM-IKY-TAB SECTION.
       INI-VAL-NUM-IKY-TAB.
       FIN-VAL-NUM-IKY-TAB.
           EXIT.
       
       VAL-NUM-MKY-TAB SECTION.
       INI-VAL-NUM-MKY-TAB.
       FIN-VAL-NUM-MKY-TAB.
           EXIT.
       
       VAL-NUM-AKY-TAB SECTION.
       INI-VAL-NUM-AKY-TAB.
       FIN-VAL-NUM-AKY-TAB.
           EXIT.
       
       VAL-NUM-FLD-TAB SECTION.
       INI-VAL-NUM-FLD-TAB.
       FIN-VAL-NUM-FLD-TAB.
           EXIT.
       
       EDT-KEY-TAB SECTION.
       INI-EDT-KEY-TAB.
           PERFORM EDT-IKY-TAB.
           PERFORM EDT-MKY-TAB.
           PERFORM EDT-AKY-TAB.
       FIN-EDT-KEY-TAB.
           EXIT.
       
       EDT-IKY-TAB SECTION.
       INI-EDT-IKY-TAB.
       FIN-EDT-IKY-TAB.
           EXIT.
       
       EDT-MKY-TAB SECTION.
       INI-EDT-MKY-TAB.
       FIN-EDT-MKY-TAB.
           EXIT.
       
       EDT-AKY-TAB SECTION.
       INI-EDT-AKY-TAB.
       FIN-EDT-AKY-TAB.
           EXIT.
       
       EDT-FLD-TAB SECTION.
       INI-EDT-FLD-TAB.
       FIN-EDT-FLD-TAB.
           EXIT.
