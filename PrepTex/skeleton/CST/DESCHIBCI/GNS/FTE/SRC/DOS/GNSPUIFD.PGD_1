               PERFORM SCR-EXIT.                                        
           PERFORM VAL-CON-KEY-IFD.                                     
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR               
               PERFORM FST-KEY-IFD                                      
               GO TO ERR-SCR-GKEY.                                      
      *JSS                                                              
           PERFORM CLR-FLD-IFD.                                         
           PERFORM VAL-IKY-IFD.                                         
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR               
               GO TO ERR-SCR-GKEY.                                      
           PERFORM VAL-MKY-IFD.                                         
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR               
               GO TO ERR-SCR-GKEY.                                      
           PERFORM VAL-AKY-IFD.                                         
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )       
               GO TO FIN-SCR-GKEY.                                      
       ERR-SCR-GKEY.                                                    
           PERFORM SCR-MSSG.                                            
           PERFORM EDT-KEY-IFD.                                         
           PERFORM SCR-PUTA.                                            
           PERFORM SCR-EXIT.                                            
       FIN-SCR-GKEY.                                                    
           PERFORM SCR-MSSG.                                            
       SCR-GIKY SECTION.                                                
       INI-SCR-GIKY.                                                    
           MOVE SCR-NMAP TO FRM-NAME.                                   
           MOVE SCR-NLIB TO FRM-NLIB.                                   
           MOVE FRM-GET-ALL TO FRM-CMND.                                
           PERFORM GNS-PRO-FRM.                                         
           MOVE SPACES TO FRM-MENS.                                     
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR        
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )          
               MOVE 'PFSINV    ' TO MSG-COD-MENS                        
               MOVE 'GNS' TO MSG-COD-SIST                               
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               PERFORM FST-KEY-IFD                                      
               PERFORM SCR-MSSG                                         
               PERFORM SCR-PUTA                                         
               PERFORM SCR-EXIT.                                        
           MOVE FRM-DFLD TO IFD-FLD.                                    
           IF FRM-STAT-ABT                                              
               GO TO FIN-SCR-GIKY.                                      
           MOVE FRM-SUAR-OKS TO FRM-SUAR.                               
           PERFORM VAL-NUM-IKY-IFD.                                     
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR               
               PERFORM SCR-MSSG                                         
               PERFORM EDT-IKY-IFD                                      
               PERFORM SCR-PUTA                                         
               PERFORM SCR-EXIT.                                        
           PERFORM VAL-CON-IKY-IFD.                                     
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR               
               PERFORM FST-KEY-IFD                                      
               GO TO ERR-SCR-GIKY.                                      
      *JSS                                                              
           PERFORM CLR-FLD-IFD.                                         
           PERFORM VAL-IKY-IFD.                                         
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )       
               GO TO FIN-SCR-GIKY.                                      
       ERR-SCR-GIKY.                                                    
           PERFORM SCR-MSSG.                                            
           PERFORM EDT-IKY-IFD.                                         
           PERFORM SCR-PUTA.                                            
           PERFORM SCR-EXIT.                                            
       FIN-SCR-GIKY.                                                    
           PERFORM SCR-MSSG.                                            
       FND-KEY SECTION.                                                 
       INI-FND-KEY.                                                     
           MOVE 'GNS' TO TAB-COD-SIST FIO-SIST.                         
           MOVE 'FRM' TO TAB-COD-TTAB IN TAB.                           
           MOVE 'IFD' TO TAB-COD-CTAB IN TAB.                           
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-INI-FND-KEY.                                                 
           PERFORM GNS-FIO-TAB.                                         
           IF FIO-STAT-OKS                                              
               MOVE TAB-COD-TABL IN TAB TO WSS-COD-TABL IN WSS-VARI.    
           IF NOT ( FIO-STAT-OKS AND                                    
                    WSS-COD-TTAB = 'FRM' AND WSS-COD-PFJO = 'IFD' )     
               GO TO CON-INI-FND-KEY.                                   
           IF WSS-COD-CORR = '01'                                       
               MOVE TAB-GLS-ABRV IN TAB TO FRM-COD-SS01 IN IFD-FLD      
               MOVE TAB-MSC-STAT IN TAB TO FRM-MSC-ST01 IN IFD-FLD      
           ELSE                                                         
           IF WSS-COD-CORR = '02'                                       
               MOVE TAB-GLS-ABRV IN TAB TO FRM-COD-SS02 IN IFD-FLD      
               MOVE TAB-MSC-STAT IN TAB TO FRM-MSC-ST02 IN IFD-FLD      
           ELSE                                                         
           IF WSS-COD-CORR = '03'                                       
               MOVE TAB-GLS-ABRV IN TAB TO FRM-COD-SS03 IN IFD-FLD      
               MOVE TAB-MSC-STAT IN TAB TO FRM-MSC-ST03 IN IFD-FLD      
           ELSE                                                         
           IF WSS-COD-CORR = '04'                                       
               MOVE TAB-GLS-ABRV IN TAB TO FRM-COD-SS04 IN IFD-FLD      
               MOVE TAB-MSC-STAT IN TAB TO FRM-MSC-ST04 IN IFD-FLD      
           ELSE                                                         
           IF WSS-COD-CORR = '05'                                       
               MOVE TAB-GLS-ABRV IN TAB TO FRM-COD-SS05 IN IFD-FLD      
               MOVE TAB-MSC-STAT IN TAB TO FRM-MSC-ST05 IN IFD-FLD      
           ELSE                                                         
           IF WSS-COD-CORR = '06'                                       
               MOVE TAB-GLS-ABRV IN TAB TO FRM-COD-SS06 IN IFD-FLD      
               MOVE TAB-MSC-STAT IN TAB TO FRM-MSC-ST06 IN IFD-FLD      
           ELSE                                                         
           IF WSS-COD-CORR = '07'                                       
               MOVE TAB-GLS-ABRV IN TAB TO FRM-COD-SS07 IN IFD-FLD      
               MOVE TAB-MSC-STAT IN TAB TO FRM-MSC-ST07 IN IFD-FLD      
           ELSE                                                         
           IF WSS-COD-CORR = '08'                                       
               MOVE TAB-GLS-ABRV IN TAB TO FRM-COD-SS08 IN IFD-FLD      
               MOVE TAB-MSC-STAT IN TAB TO FRM-MSC-ST08 IN IFD-FLD      
           ELSE                                                         
           IF WSS-COD-CORR = '09'                                       
               MOVE TAB-GLS-ABRV IN TAB TO FRM-COD-SS09 IN IFD-FLD      
               MOVE TAB-MSC-STAT IN TAB TO FRM-MSC-ST09 IN IFD-FLD      
           ELSE                                                         
           IF WSS-COD-CORR = '10'                                       
               MOVE TAB-GLS-ABRV IN TAB TO FRM-COD-SS10 IN IFD-FLD      
               MOVE TAB-MSC-STAT IN TAB TO FRM-MSC-ST10 IN IFD-FLD      
           ELSE                                                         
           IF WSS-COD-CORR = '11'                                       
               MOVE TAB-GLS-ABRV IN TAB TO FRM-COD-SS11 IN IFD-FLD      
               MOVE TAB-MSC-STAT IN TAB TO FRM-MSC-ST11 IN IFD-FLD      
           ELSE                                                         
           IF WSS-COD-CORR = '12'                                       
               MOVE TAB-GLS-ABRV IN TAB TO FRM-COD-SS12 IN IFD-FLD      
               MOVE TAB-MSC-STAT IN TAB TO FRM-MSC-ST12 IN IFD-FLD.     
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-INI-FND-KEY.                                       
       CON-INI-FND-KEY.                                                 
           PERFORM KEY-ALL-IFD.                                         
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM GNS-FIO-IFD.                                         
           MOVE FIO-STAT TO PGM-STAT-IFD.                               
       FIN-FND-KEY.                                                     
           EXIT.                                                        
   *>>>
