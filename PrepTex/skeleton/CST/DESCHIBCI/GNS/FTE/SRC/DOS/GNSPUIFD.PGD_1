           ELSE                                                         
           IF SCR-INST = SCR-INST-GAB                                   
                PERFORM GET-CON.                                        
           PERFORM DESSCRTRL.                                           
           IF SCR-ACTU = SCR-ACTU-MOD                                   
                  PERFORM MSG-SCR-MOD                                   
           ELSE                                                         
                  PERFORM MSG-SCR-ING.                                  
           GO TO FIN-SCR-ACT.
    *>>>>
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF ( FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8  OR
                           FRM-FFLD-PF5 OR FRM-FFLD-PF6 ) AND
              SCR-ACTU = SCR-ACTU-MOD
               PERFORM XCT-PRGP.
           IF SCR-ACTU = SCR-ACTU-MOD
                  PERFORM MSG-SCR-MOD
           ELSE
                  PERFORM MSG-SCR-ING.
       FIN-SCR-ACT.
           EXIT.

       SCR-PGM SECTION.
       INI-SCR-PGM.
   *<<<<INI_SCR_PGM
           IF SCR-INST = SCR-INST-HKY                                   
               MOVE SCR-INST-GKY TO SCR-INST                            
               PERFORM HBL-KEY                                          
               IF PGM-LIMPIAR-LLAVES                                    
                   PERFORM SCR-CLRA-IFD                                 
                   PERFORM EDT-KEY-IFD                                  
                   PERFORM EDT-FLD-IFD                                  
                   PERFORM SCR-PUTA                                     
                   PERFORM SCR-EXIT                                     
               ELSE                                                     
                   PERFORM CLR-FLD-IFD                                  
                   PERFORM EDT-FLD-IFD                                  
                   PERFORM SCR-PUTA                                     
                   PERFORM SCR-EXIT                                     
           ELSE                                                         
           IF SCR-INST = SCR-INST-GKY                                   
                PERFORM SCR-RST                                         
                PERFORM GET-KEY                                         
                IF FRM-STAT-ABT                                         
                        MOVE SPACES TO FRM-MENS                         
                        PERFORM SCR-MSSG                                
                        PERFORM DESSCRTRL                               
                        GO TO FIN-SCR-PGM.                              
           MOVE SPACES TO FRM-MENS.                                     
           PERFORM FND-KEY.                                             
      *JSS IF NOT FIO-STAT-OKS                                          
      *        PERFORM CLR-FLD-IFD.                                     
           IF FIO-STAT = FIO-STAT-NEX                                   
               IF FRM-MENS NOT > SPACES                                 
                   MOVE 'REGNEX    ' TO MSG-COD-MENS                    
                   MOVE 'GNS' TO MSG-COD-SIST                           
                   PERFORM GET-MSG                                      
                   MOVE MSG-GLS-DESC TO FRM-MENS                        
                   PERFORM SCR-MSSG                                     
                   MOVE SCR-STAT-ABT TO SCR-STAT                        
                   PERFORM DESSCRTRL                                    
                   GO TO FIN-SCR-PGM                                    
               ELSE                                                     
                   PERFORM SCR-MSSG                                     
                   MOVE SCR-STAT-ABT TO SCR-STAT                        
                   PERFORM DESSCRTRL                                    
                   GO TO FIN-SCR-PGM.                                   
       TRL-SCR-PGM.                                                     
           PERFORM DESSCRTRL.                                           
           MOVE SPACES TO FRM-MENS.                                     
           IF SCR-STAT NOT = SCR-STAT-CON                               
               PERFORM CMN-PGM.                                         
           PERFORM SCR-MSSG.                                            
           GO TO FIN-SCR-PGM.
   *>>>>INI_SCR_PGM
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
       TRL-SCR-PGM.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8 OR
                         FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM XCT-PRGP.
           MOVE SPACES TO FRM-MENS.
           IF SCR-STAT NOT = SCR-STAT-CON
               PERFORM CMN-PGM.
           PERFORM SCR-MSSG.
       FIN-SCR-PGM.
           EXIT.

       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               PERFORM SCR-CLRA-IFD
               PERFORM EDT-KEY-IFD
               PERFORM EDT-FLD-IFD
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-IFD
               PERFORM EDT-KEY-IFD
           ELSE
               PERFORM CLR-FLD-IFD
               PERFORM EDT-FLD-IFD.
           PERFORM STP-EXIT.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

       XCT-PRGP SECTION.
       INI-XCT-PRGP.
           MOVE SCR-INST-BRW TO SCR-INST.
           PERFORM STP-EXIT.
           MOVE FRM-FFLD   TO SCR-FFLD.
      *
           MOVE SCR-PROG     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
           PERFORM GNS-PRO-SYS.
           PERFORM XCT-ERR.
       FIN-XCT-PRGP.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO IFD-FLD.
       FIN-SCR-RST.
           EXIT.

       GET-FLD SECTION.
       INI-GET-FLD.
           PERFORM SCR-GFLD.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-FLD.
           EXIT.

       CMN-MOD SECTION.
       INI-CMN-MOD.
           PERFORM KEY-ALL-IFD.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM GNS-FIO-IFD.
           IF FIO-STAT-OKS AND
              IFD-STP-ITRN IN IFD NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM GNS-FIO-IFD
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM GNS-FIO-IFD
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-MOD.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-MOD.
           PERFORM RET-STT-IFD.
           PERFORM RET-ALL-IFD.
           PERFORM RET-HDR-IFD.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM GNS-FIO-IFD.
       FIN-CMN-MOD.
           EXIT.

       CMN-ING SECTION.
       INI-CMN-ING.
           MOVE SPACES TO IFD.
           PERFORM RET-STT-IFD.
           PERFORM RET-ALL-IFD.
           PERFORM RET-HDR-IFD.
           MOVE FIO-PUT TO FIO-CMND.
           PERFORM GNS-FIO-IFD.
       FIN-CMN-ING.
           EXIT.

       CMN-ACC SECTION.
       INI-CMN-ACC.
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'ACCION NO REALIZADA' TO FRM-MENS
      *<<<< ABT_CMN_ACC
               IF SCR-CMND = 'COP'
                   MOVE 'GNS' TO MSG-COD-SIST
                   MOVE 'REG    NOCOP' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   GO TO FIN-CMN-ACC
               ELSE
      *>>>>
               GO TO FIN-CMN-ACC.
      *<<<< INI_CMN_ACC
           IF SCR-CMND = 'COP'
               PERFORM CMN-ING
               IF FIO-STAT-OKS
                   MOVE 'GNS' TO MSG-COD-SIST
                   MOVE 'REG    SICOP' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   GO TO FIN-CMN-ACC
               ELSE
                   MOVE 'GNS' TO MSG-COD-SIST
                   MOVE 'REG    ERCOP' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   GO TO FIN-CMN-ACC.
      *>>>>
           PERFORM KEY-ALL-IFD.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM GNS-FIO-IFD.
           IF FIO-STAT-OKS AND
              IFD-STP-ITRN IN IFD NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM GNS-FIO-IFD
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM GNS-FIO-IFD
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-ACC.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ACC.
           PERFORM RET-ALL-IFD.
       FIN-CMN-ACC.
           EXIT.

       CMN-ELI SECTION.
       INI-CMN-ELI.
           PERFORM KEY-ALL-IFD.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM GNS-FIO-IFD.
           IF FIO-STAT-OKS AND
              IFD-STP-ITRN IN IFD NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM GNS-FIO-IFD
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM GNS-FIO-IFD
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-ELI.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ELI.
           MOVE FIO-DEL TO FIO-CMND.
           PERFORM GNS-FIO-IFD.
       FIN-CMN-ELI.
           EXIT.

       CMN-PGM SECTION.
       INI-CMN-PGM.
           PERFORM RET-ALL-IFD.
       FIN-CMN-PGM.
           EXIT.

       STP-EXIT SECTION.
       INI-STP-EXIT.
           MOVE SCR-PROG TO PGM-PROG.
           MOVE SCR-NTRN TO PGM-NTRN.

           MOVE SCR-PROV TO SCR-PROG.
           MOVE SCR-NTRV TO SCR-NTRN.

           MOVE PGM-PROG TO SCR-PROV
           MOVE PGM-NTRN TO SCR-NTRV.
       FIN-STP-EXIT.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
   *<<<<INI_SCR_EXIT
           MOVE 'P'      TO SCR-ICMN.                                   
           MOVE SCR-NTRN TO FRM-NTRN.                                   
           MOVE SCR-VARI TO FRM-CMMA.                                   
           MOVE SCR-TCMA TO FRM-TCMA.                                   
           MOVE FRM-EXT  TO FRM-CMND.                                   
           PERFORM GNS-PRO-FRM.                                         
           GO TO FIN_SCR_EXIT.           
   *>>>>
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO SYS-NTRN.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE IFD-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO IFD-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GFLD SECTION.
       INI-SCR-GFLD.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-FLD-IFD
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO IFD-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GFLD.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-IFD.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-IFD
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           PERFORM VAL-NUM-FLD-IFD.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-IFD
               PERFORM EDT-FLD-IFD
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           PERFORM VAL-MKY-IFD.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GFLD.
           PERFORM VAL-FLD-IFD.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GFLD.
           PERFORM VAL-CON-FLD-IFD.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GFLD.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-FLD-IFD.
       ERR-SCR-GFLD.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-IFD.
           PERFORM EDT-FLD-IFD.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GFLD.
           MOVE SCR-STAT-MOD TO SCR-STAT.

       GET-ACC SECTION.
       INI-GET-ACC.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO IFD-GLS-MEN2
               MOVE SCR-PRMT     TO IFD-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO IFD-FLD.
           IF NOT ( ( FRM-FFLD = FRM-FFLD-PF12 OR FRM-FFLD-PF3 ) OR
                      SCR-CONF > SPACES )
               PERFORM VAL-NUM-KEY-IFD
               PERFORM VAL-NUM-FLD-IFD.
       FIN-GET-ACC.
           MOVE SCR-STAT-SON TO SCR-STAT.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO IFD-GLS-MEN2
               MOVE SCR-PRMT     TO IFD-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO IFD-FLD.
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO IFD-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
           IF SCR-DESDE-CMN
               MOVE IFD-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO IFD-FLD
      * FLB  7-NOV-1994
               PERFORM CLR-KEY-IFD

               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO IFD-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO IFD-GLS-MENS.
           PERFORM PRO-FLD-IFD.
           PERFORM PRO-MKY-IFD.
           PERFORM UNP-IKY-IFD.
           PERFORM FST-KEY-IFD.
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
           IF SCR-DESDE-CMN
               MOVE IFD-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO IFD-FLD
               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO IFD-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO IFD-GLS-MENS.
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-FLD-IFD.
           MOVE '1' TO FRM-CPIM-AEY.
           PERFORM UNP-KEY-IFD.
           PERFORM FST-KEY-IFD.
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
   *<<<<INI_HBL_FLD
           PERFORM PRO-IKY-IFD.                                         
           PERFORM PRO-AKY-IFD.                                         
   *>>>>
           PERFORM UNP-MKY-IFD.
           PERFORM UNP-FLD-IFD.
           PERFORM FST-FLD-IFD.
      *<<<< FIN_HBL_FLD
           MOVE 0 TO FRM-COD-GS01-LEN IN IFD-FLD.
           IF FRM-COD-SS01 IN IFD-FLD > SPACES
               MOVE ':'          TO FRM-GLS-2P01     IN IFD-FLD
               MOVE 'C'          TO FRM-COD-QS01     IN IFD-FLD
               IF FRM-MSC-ST01 IN IFD-FLD = 'S'
                   MOVE FRM-CPIM-UBY TO FRM-COD-GS01-ATR IN IFD-FLD
                   MOVE -1           TO FRM-COD-GS01-LEN IN IFD-FLD
               ELSE
                   MOVE FRM-CPIM-AEY TO FRM-COD-GS01-ATR IN IFD-FLD
           ELSE
               MOVE SPACES       TO FRM-GLS-2P01     IN IFD-FLD
               MOVE ' '          TO FRM-COD-QS01     IN IFD-FLD
               MOVE FRM-CPIM-AEY TO FRM-COD-GS01-ATR IN IFD-FLD.

           IF FRM-COD-SS02 IN IFD-FLD > SPACES
               MOVE ':'          TO FRM-GLS-2P02     IN IFD-FLD
               MOVE 'C'          TO FRM-COD-QS02     IN IFD-FLD
               IF FRM-MSC-ST02 IN IFD-FLD = 'S'
                   MOVE FRM-CPIM-UBY TO FRM-COD-GS02-ATR IN IFD-FLD
                   MOVE -1           TO FRM-COD-GS02-LEN IN IFD-FLD
               ELSE
                   MOVE FRM-CPIM-AEY TO FRM-COD-GS02-ATR IN IFD-FLD
           ELSE
               MOVE SPACES       TO FRM-GLS-2P02     IN IFD-FLD
               MOVE ' '          TO FRM-COD-QS02     IN IFD-FLD
               MOVE FRM-CPIM-AEY TO FRM-COD-GS02-ATR IN IFD-FLD.

           IF FRM-COD-SS03 IN IFD-FLD > SPACES
               MOVE ':'          TO FRM-GLS-2P03     IN IFD-FLD
               MOVE 'C'          TO FRM-COD-QS03     IN IFD-FLD
               IF FRM-MSC-ST03 IN IFD-FLD = 'S'
                   MOVE FRM-CPIM-UBY TO FRM-COD-GS03-ATR IN IFD-FLD
                   MOVE -1           TO FRM-COD-GS03-LEN IN IFD-FLD
               ELSE
                   MOVE FRM-CPIM-AEY TO FRM-COD-GS03-ATR IN IFD-FLD
           ELSE
               MOVE SPACES       TO FRM-GLS-2P03     IN IFD-FLD
               MOVE ' '          TO FRM-COD-QS03     IN IFD-FLD
               MOVE FRM-CPIM-AEY TO FRM-COD-GS03-ATR IN IFD-FLD.

           IF FRM-COD-SS04 IN IFD-FLD > SPACES
               MOVE ':'          TO FRM-GLS-2P04     IN IFD-FLD
               MOVE 'M'          TO FRM-COD-QS04     IN IFD-FLD
               MOVE 'T'          TO FRM-COD-QS05     IN IFD-FLD
               MOVE 'C'          TO FRM-COD-QS06     IN IFD-FLD
               IF FRM-MSC-ST04 IN IFD-FLD = 'S'
                   MOVE FRM-CPIM-UBY TO FRM-COD-GS04-ATR IN IFD-FLD
                   MOVE FRM-CPIM-UBY TO FRM-COD-GS05-ATR IN IFD-FLD
                   MOVE FRM-CPIM-UBY TO FRM-COD-GS06-ATR IN IFD-FLD
                   MOVE -1           TO FRM-COD-GS04-LEN IN IFD-FLD
               ELSE
                   MOVE FRM-CPIM-AEY TO FRM-COD-GS04-ATR IN IFD-FLD
                   MOVE FRM-CPIM-AEY TO FRM-COD-GS05-ATR IN IFD-FLD
                   MOVE FRM-CPIM-AEY TO FRM-COD-GS06-ATR IN IFD-FLD
           ELSE
               MOVE SPACES       TO FRM-GLS-2P04     IN IFD-FLD
               MOVE ' '          TO FRM-COD-QS04     IN IFD-FLD
               MOVE ' '          TO FRM-COD-QS05     IN IFD-FLD
               MOVE ' '          TO FRM-COD-QS06     IN IFD-FLD
               MOVE FRM-CPIM-AEY TO FRM-COD-GS04-ATR IN IFD-FLD
               MOVE FRM-CPIM-AEY TO FRM-COD-GS05-ATR IN IFD-FLD
               MOVE FRM-CPIM-AEY TO FRM-COD-GS06-ATR IN IFD-FLD.

           IF FRM-COD-SS05 IN IFD-FLD > SPACES
               MOVE ':'          TO FRM-GLS-2P05     IN IFD-FLD
               MOVE 'M'          TO FRM-COD-QS07     IN IFD-FLD
               MOVE 'T'          TO FRM-COD-QS08     IN IFD-FLD
               MOVE 'C'          TO FRM-COD-QS09     IN IFD-FLD
               IF FRM-MSC-ST05 IN IFD-FLD = 'S'
                   MOVE FRM-CPIM-UBY TO FRM-COD-GS07-ATR IN IFD-FLD
                   MOVE FRM-CPIM-UBY TO FRM-COD-GS08-ATR IN IFD-FLD
                   MOVE FRM-CPIM-UBY TO FRM-COD-GS09-ATR IN IFD-FLD
                   MOVE -1           TO FRM-COD-GS07-LEN IN IFD-FLD
               ELSE
                   MOVE FRM-CPIM-AEY TO FRM-COD-GS07-ATR IN IFD-FLD
                   MOVE FRM-CPIM-AEY TO FRM-COD-GS08-ATR IN IFD-FLD
                   MOVE FRM-CPIM-AEY TO FRM-COD-GS09-ATR IN IFD-FLD
           ELSE
               MOVE SPACES       TO FRM-GLS-2P05     IN IFD-FLD
               MOVE ' '          TO FRM-COD-QS07     IN IFD-FLD
               MOVE ' '          TO FRM-COD-QS08     IN IFD-FLD
               MOVE ' '          TO FRM-COD-QS09     IN IFD-FLD
               MOVE FRM-CPIM-AEY TO FRM-COD-GS07-ATR IN IFD-FLD
               MOVE FRM-CPIM-AEY TO FRM-COD-GS08-ATR IN IFD-FLD
               MOVE FRM-CPIM-AEY TO FRM-COD-GS09-ATR IN IFD-FLD.

