              ITK-NUM-MMPR IN ITK > 59
               MOVE 'TAB' TO MSG-COD-SIST
               MOVE 'NUMMMTRNOVA' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-ITK-HRA-HRPR.
           IF ITK-NUM-SSPR IN ITK < ZEROES OR
              ITK-NUM-SSPR IN ITK > 59
               MOVE 'TAB' TO MSG-COD-SIST
               MOVE 'NUMSSTRNOVA' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-ITK-HRA-HRPR.
      *>>>>
       FIN-VAL-ITK-HRA-HRPR.
           EXIT.
       
       VAL-ITK-NUM-UNIC SECTION.
       INI-VAL-ITK-NUM-UNIC.
       FIN-VAL-ITK-NUM-UNIC.
           EXIT.
       
       VAL-CON-KEY-ITK SECTION.
       INI-VAL-CON-KEY-ITK.
           IF     ITK-CAI-IITK IN ITK-FLD NOT > SPACES                  
              AND ITK-IIC-IITK IN ITK-FLD NOT > SPACES                  
              AND ITK-COD-SIST IN ITK-FLD NOT > SPACES                  
              AND ITK-COD-TPRO IN ITK-FLD NOT > SPACES                  
              AND ITK-COD-ENTI IN ITK-FLD NOT > SPACES                  
              AND ITK-FEC-FPRO IN ITK-FLD NOT > ZEROES                  
              AND ITK-HRA-HRPR IN ITK-FLD NOT > ZEROES                  
              AND ITK-NUM-UNIC IN ITK-FLD NOT > ZEROES                  
                PERFORM ERR-KEY                                         
                PERFORM FST-KEY-ITK.                                  
           GO TO FIN-VAL-CON-KEY-ITK.  
           IF     ITK-CAI-IITK IN ITK-FLD NOT > SPACES 
              AND ITK-IIC-IITK IN ITK-FLD NOT > SPACES 
              AND ITK-COD-SIST IN ITK-FLD NOT > SPACES 
              AND ITK-COD-TPRO IN ITK-FLD NOT > SPACES 
              AND ITK-COD-ENTI IN ITK-FLD NOT > SPACES 
              AND ITK-FEC-FPRO IN ITK-FLD NOT > ZEROES
              AND ITK-HRA-HRPR IN ITK-FLD NOT > SPACES 
              AND ITK-NUM-UNIC IN ITK-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-ITK.
       FIN-VAL-CON-KEY-ITK.
           EXIT.
       
       VAL-CON-IKY-ITK SECTION.
       INI-VAL-CON-IKY-ITK.
           IF     ITK-CAI-IITK IN ITK-FLD NOT > SPACES 
              AND ITK-IIC-IITK IN ITK-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-ITK.
       FIN-VAL-CON-IKY-ITK.
           EXIT.
       
       VAL-NUM-KEY-ITK SECTION.
       INI-VAL-NUM-KEY-ITK.
           PERFORM VAL-NUM-IKY-ITK.
           PERFORM VAL-NUM-MKY-ITK.
           PERFORM VAL-NUM-AKY-ITK.
       FIN-VAL-NUM-KEY-ITK.
           EXIT.
       
       VAL-NUM-IKY-ITK SECTION.
       INI-VAL-NUM-IKY-ITK.
       FIN-VAL-NUM-IKY-ITK.
           EXIT.
       
       VAL-NUM-MKY-ITK SECTION.
       INI-VAL-NUM-MKY-ITK.
       VAL-NUM-ITK-NUM-UNIC.
           MOVE ITK-NUM-UNIC-ALF IN ITK-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO ITK-NUM-UNIC IN ITK-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO ITK-NUM-UNIC-LEN IN ITK-FLD
                GO TO FIN-VAL-NUM-MKY-ITK.
       FIN-VAL-NUM-MKY-ITK.
           EXIT.
       
       VAL-NUM-AKY-ITK SECTION.
       INI-VAL-NUM-AKY-ITK.
       FIN-VAL-NUM-AKY-ITK.
           EXIT.
       
       VAL-NUM-FLD-ITK SECTION.
       INI-VAL-NUM-FLD-ITK.
       VAL-NUM-ITK-VAL-OITE.
           MOVE ITK-VAL-OITE-ALF IN ITK-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO ITK-VAL-OITE IN ITK-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO ITK-VAL-OITE-LEN IN ITK-FLD
                GO TO FIN-VAL-NUM-FLD-ITK.
       VAL-NUM-ITK-VAL-TITE.
           MOVE ITK-VAL-TITE-ALF IN ITK-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO ITK-VAL-TITE IN ITK-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO ITK-VAL-TITE-LEN IN ITK-FLD
                GO TO FIN-VAL-NUM-FLD-ITK.
       VAL-NUM-ITK-VAL-TPRO.
           MOVE ITK-VAL-TPRO-ALF IN ITK-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO ITK-VAL-TPRO IN ITK-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO ITK-VAL-TPRO-LEN IN ITK-FLD
                GO TO FIN-VAL-NUM-FLD-ITK.
       FIN-VAL-NUM-FLD-ITK.
           EXIT.
       
       EDT-KEY-ITK SECTION.
       INI-EDT-KEY-ITK.
           PERFORM EDT-IKY-ITK.
           PERFORM EDT-MKY-ITK.
           PERFORM EDT-AKY-ITK.
       FIN-EDT-KEY-ITK.
           EXIT.
       
       EDT-IKY-ITK SECTION.
       INI-EDT-IKY-ITK.
       FIN-EDT-IKY-ITK.
           EXIT.
       
       EDT-MKY-ITK SECTION.
       INI-EDT-MKY-ITK.
           IF ITK-NUM-UNIC-LEN IN ITK-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO ITK-NUM-UNIC-LEN IN ITK-FLD
                GO TO FIN-EDT-MKY-ITK.
           MOVE ITK-NUM-UNIC IN ITK-FLD TO ITK-NUM-UNIC-EDT IN ITK-EDT.
           MOVE ITK-NUM-UNIC-EDT IN ITK-EDT TO 
                ITK-NUM-UNIC-ALF IN ITK-FLD.
       FIN-EDT-MKY-ITK.
           EXIT.
       
       EDT-AKY-ITK SECTION.
       INI-EDT-AKY-ITK.
       FIN-EDT-AKY-ITK.
           EXIT.
       
       EDT-FLD-ITK SECTION.
       INI-EDT-FLD-ITK.
           IF ITK-VAL-OITE-LEN IN ITK-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO ITK-VAL-OITE-LEN IN ITK-FLD
                GO TO FIN-EDT-FLD-ITK.
           MOVE ITK-VAL-OITE IN ITK-FLD TO ITK-VAL-OITE-EDT IN ITK-EDT.
           MOVE ITK-VAL-OITE-EDT IN ITK-EDT TO 
                ITK-VAL-OITE-ALF IN ITK-FLD.
           IF ITK-VAL-TITE-LEN IN ITK-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO ITK-VAL-TITE-LEN IN ITK-FLD
                GO TO FIN-EDT-FLD-ITK.
           MOVE ITK-VAL-TITE IN ITK-FLD TO ITK-VAL-TITE-EDT IN ITK-EDT.
           MOVE ITK-VAL-TITE-EDT IN ITK-EDT TO 
                ITK-VAL-TITE-ALF IN ITK-FLD.
           IF ITK-VAL-TPRO-LEN IN ITK-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO ITK-VAL-TPRO-LEN IN ITK-FLD
                GO TO FIN-EDT-FLD-ITK.
           MOVE ITK-VAL-TPRO IN ITK-FLD TO ITK-VAL-TPRO-EDT IN ITK-EDT.
           MOVE ITK-VAL-TPRO-EDT IN ITK-EDT TO 
                ITK-VAL-TPRO-ALF IN ITK-FLD.
       FIN-EDT-FLD-ITK.
           EXIT.
