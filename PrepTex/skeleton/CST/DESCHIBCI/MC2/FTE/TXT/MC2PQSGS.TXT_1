              MOVE SGS-MSC-UTPR IN SGS     TO MSC-COD-CMSC IN MSC       
              PERFORM BUS-MSC                                           
              MOVE MSC-GLS-DCOR IN MSC     TO WSS-GLS-UTPR-V(WSS-J)     
           ELSE                                                         
              MOVE SPACES                  TO WSS-GLS-UTPR-V(WSS-J).    
                                                                        
           IF SGS-IND-UTPZ IN SGS > SPACES                              
              MOVE 'TAB'                   TO MSC-COD-SIST              
              MOVE 'IPER'                  TO MSC-COD-TMSC IN MSC       
              MOVE SGS-IND-UTPZ IN SGS     TO MSC-COD-CMSC IN MSC       
              PERFORM BUS-MSC                                           
               MOVE MSC-GLS-DCOR IN MSC    TO WSS-GLS-UTPZ-V(WSS-J)     
           ELSE                                                         
               MOVE SPACES                 TO WSS-GLS-UTPZ-V(WSS-J).    
                                                                        
           IF SGS-IND-EDRA IN SGS > SPACES                              
              MOVE 'TAB'                   TO TAB-COD-SIST              
              MOVE 'ERA'                   TO TAB-COD-TTAB IN TAB       
              MOVE SGS-IND-EDRA IN SGS     TO TAB-COD-CTAB IN TAB       
              PERFORM BUS-TAB                                           
               MOVE TAB-GLS-DCOR IN TAB    TO WSS-GLS-EDRA-V(WSS-J)     
           ELSE                                                         
               MOVE SPACES                 TO WSS-GLS-EDRA-V(WSS-J).    
       FIN-CRG-VAL-VEC.                                                 
           EXIT.                                                        
                                                                        
                                                                        
       SCRL-TAB-SGS SECTION.                                            
       INI-SCRL-TAB-SGS.                                                
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF5                   
               PERFORM SCRL-TAB-SGS-PF7                                 
               GO TO FIN-SCRL-TAB-SGS.                                  
           IF FRM-FFLD = FRM-FFLD-PF8  OR FRM-FFLD-PF6                  
               PERFORM SCRL-TAB-SGS-PF8                                 
               GO TO FIN-SCRL-TAB-SGS.                                  
       FIN-SCRL-TAB-SGS.                                                
           EXIT.                                                        
                                                                        
       SCRL-TAB-SGS-PF7 SECTION.                                        
       INI-SCRL-TAB-SGS-PF7.                                            
           MOVE SCR-SGS-KEY-ISGS TO SGS-KEY-ISGS IN SGS.                
           MOVE FIO-GET-LEQ TO FIO-CMND.                                
           PERFORM MC2-FIO-SGS.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    SGS-KEY-ISGS IN SGS = SCR-SGS-KEY-ISGS)             
      *        SGS-KEY-ISGS IN SGS = SCR-SGS-KEY-ISGS )                 
               MOVE 'TABCURRNEX' TO MSG-COD-MENS                        
               GO TO MSG-SCRL-TAB-SGS-PF7.                              
       PRV-SCRL-TAB-SGS-PF8.                                            
           MOVE FIO-GET-PRV TO FIO-CMND.                                
           PERFORM MC2-FIO-SGS.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    SGS-CIC-PLAN IN SGS = SCR-SGS-CIC-PLAN AND          
                    SGS-COD-SIST IN SGS = SCR-SGS-COD-SIST AND          
                    SGS-COD-EVEN IN SGS = SCR-SGS-COD-EVEN AND          
                    SGS-IND-TPLT IN SGS = SCR-SGS-IND-TPLT AND          
                    SGS-FEC-PLAN IN SGS = SCR-SGS-FEC-PLAN AND          
                    SGS-HRA-PLAN IN SGS = SCR-SGS-HRA-PLAN )            
               MOVE 'TAB    NOPRV' TO MSG-COD-MENS                      
               GO TO MSG-SCRL-TAB-SGS-PF7.                              
           MOVE FIO-STAT TO PGM-STAT-SGS.                               
           PERFORM SCR-CLRA-SGS.                                        
           PERFORM PUT-SCR.                                             
           GO TO FIN-SCRL-TAB-SGS-PF7.                                  
       MSG-SCRL-TAB-SGS-PF7.                                            
           MOVE 'TAB' TO MSG-COD-SIST.                                  
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MEN2.                               
       FIN-SCRL-TAB-SGS-PF7.                                            
           EXIT.                                                        
                                                                        
       SCRL-TAB-SGS-PF8 SECTION.                                        
       INI-SCRL-TAB-SGS-PF8.                                            
           MOVE SCR-SGS-KEY-ISGS TO SGS-KEY-ISGS IN SGS.                
           MOVE FIO-GET-NLS TO FIO-CMND.                                
           PERFORM MC2-FIO-SGS.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    SGS-KEY-ISGS IN SGS = SCR-SGS-KEY-ISGS)             
               MOVE 'TABCURRNEX' TO MSG-COD-MENS                        
               GO TO MSG-SCRL-TAB-SGS-PF8.                              
       NXT-SCRL-TAB-SGS-PF8.                                            
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           PERFORM MC2-FIO-SGS.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    SGS-CIC-PLAN IN SGS = SCR-SGS-CIC-PLAN AND          
                    SGS-COD-SIST IN SGS = SCR-SGS-COD-SIST AND          
                    SGS-COD-EVEN IN SGS = SCR-SGS-COD-EVEN AND          
                    SGS-IND-TPLT IN SGS = SCR-SGS-IND-TPLT AND          
                    SGS-FEC-PLAN IN SGS = SCR-SGS-FEC-PLAN AND          
                    SGS-HRA-PLAN IN SGS = SCR-SGS-HRA-PLAN )            
               MOVE 'TAB    NONXT' TO MSG-COD-MENS                      
               GO TO MSG-SCRL-TAB-SGS-PF8.                              
           MOVE FIO-STAT TO PGM-STAT-SGS.                               
           PERFORM SCR-CLRA-SGS.                                        
           PERFORM PUT-SCR.                                             
           GO TO FIN-SCRL-TAB-SGS-PF8.                                  
       MSG-SCRL-TAB-SGS-PF8.                                            
           MOVE 'TAB' TO MSG-COD-SIST.                                  
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MEN2.                               
       FIN-SCRL-TAB-SGS-PF8.                                            
           EXIT.                                                        
      *>>>>                                                             
*% END
*% IF EOF_PU
      *<<<< EOF_PU                                                      
       PGM-EBR-ALL SECTION.                                             
       INI-PGM-EBR-ALL.                                                 
           MOVE 'GNS' TO FIO-SIST.                                      
           MOVE FIO-EBR-ALL TO FIO-CMND.                                
           PERFORM GNS-FIO-TAB.                                         
       FIN-PGM-EBR-ALL.                                                 
           EXIT.                                                        
       PRO-CMN-VRF SECTION.                                             
       INI-PRO-CMN-VRF.                                                 
           PERFORM OBT-COD-PESO.                                        
           MOVE SPACES TO MSG-COD-MENS.                                 
           MOVE ZEROES TO WSS-K.                                        
           MOVE SGS-CAI-PLAN IN SGS-FLD TO SGS-CAI-PLAN IN SGS.         
           MOVE SGS-IIC-PLAN IN SGS-FLD TO SGS-IIC-PLAN IN SGS.         
           MOVE SGS-COD-SIST IN SGS-FLD TO SGS-COD-SIST IN SGS.         
           MOVE SGS-COD-EVEN IN SGS-FLD TO SGS-COD-EVEN IN SGS.         
           MOVE SGS-IND-TPLT IN SGS-FLD TO SGS-IND-TPLT IN SGS.         
           MOVE SGS-NUM-DLAN IN SGS-FLD TO SGS-NUM-DLAN IN SGS.         
           MOVE SGS-NUM-MLAN IN SGS-FLD TO SGS-NUM-MLAN IN SGS.         
           MOVE SGS-NUM-SLAN IN SGS-FLD TO SGS-NUM-SLAN IN SGS.         
           MOVE SGS-NUM-ALAN IN SGS-FLD TO SGS-NUM-ALAN IN SGS.         
           MOVE SGS-HRA-PLAN IN SGS-FLD TO SGS-HRA-PLAN IN SGS.         
           MOVE ZEROES TO SGS-NUM-ISGS IN SGS.                          
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-PRO-CMN-VRF.                                                 
           PERFORM MC2-FIO-SGS.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
              SGS-CAI-PLAN IN SGS-FLD = SGS-CAI-PLAN IN SGS AND         
              SGS-IIC-PLAN IN SGS-FLD = SGS-IIC-PLAN IN SGS AND         
              SGS-COD-SIST IN SGS-FLD = SGS-COD-SIST IN SGS AND         
              SGS-COD-EVEN IN SGS-FLD = SGS-COD-EVEN IN SGS AND         
              SGS-IND-TPLT IN SGS-FLD = SGS-IND-TPLT IN SGS AND         
              SGS-NUM-DLAN IN SGS-FLD = SGS-NUM-DLAN IN SGS AND         
              SGS-NUM-MLAN IN SGS-FLD = SGS-NUM-MLAN IN SGS AND         
              SGS-NUM-SLAN IN SGS-FLD = SGS-NUM-SLAN IN SGS AND         
              SGS-NUM-ALAN IN SGS-FLD = SGS-NUM-ALAN IN SGS AND         
              SGS-HRA-PLAN IN SGS-FLD = SGS-HRA-PLAN IN SGS )           
               GO TO CNT-PRO-CMN-VRF.                                   
                                                                        
           IF SGS-IND-VIGE IN SGS = 'A' OR 'D'                          
               MOVE 'SGSVRF NOVRF' TO MSG-COD-MENS                      
               GO TO MSG-PRO-CMN-VRF.                                   
                                                                        
           ADD 1 TO WSS-K.                                              
           MOVE SGS-STP-ITRN IN SGS TO WSS-SGS-STP-ITRN(WSS-K).         
           MOVE SGS-MSC-TACC IN SGS TO WSS-SGS-MSC-TACC(WSS-K).         
           MOVE SGS-MSC-STAT IN SGS TO WSS-SGS-MSC-STAT(WSS-K).         
           MOVE SGS-COD-OTRN IN SGS TO WSS-SGS-COD-OTRN(WSS-K).         
           MOVE SGS-COD-ATRN IN SGS TO WSS-SGS-COD-ATRN(WSS-K).         
           MOVE SGS-IND-DCTO IN SGS TO WSS-SGS-IND-DCTO(WSS-K).         
           MOVE SGS-IND-CAAB IN SGS TO WSS-SGS-IND-CAAB(WSS-K).         
           MOVE SGS-COD-TSEG IN SGS TO WSS-SGS-COD-TSEG(WSS-K).         
           MOVE SGS-COD-SEGU IN SGS TO WSS-SGS-COD-SEGU(WSS-K).         
           MOVE SGS-COD-OCOB IN SGS TO WSS-SGS-COD-OCOB(WSS-K).         
           MOVE SGS-SGV-TAMO IN SGS TO WSS-SGS-SGV-TAMO(WSS-K).         
           MOVE SGS-SGV-TMEX IN SGS TO WSS-SGS-SGV-TMEX(WSS-K).         
           MOVE SGS-COD-VCOC IN SGS TO WSS-SGS-COD-VCOC(WSS-K).         
           MOVE SGS-COD-CPTO IN SGS TO WSS-SGS-COD-CPTO(WSS-K).         
           MOVE SGS-COD-TINT IN SGS TO WSS-SGS-COD-TINT(WSS-K).         
           MOVE SGS-NUM-PLZO IN SGS TO WSS-SGS-NUM-PLZO(WSS-K).         
           MOVE SGS-IND-UTPZ IN SGS TO WSS-SGS-IND-UTPZ(WSS-K).         
           MOVE SGS-NUM-EDAT IN SGS TO WSS-SGS-NUM-EDAT(WSS-K).         
           MOVE SGS-IND-EDRA IN SGS TO WSS-SGS-IND-EDRA(WSS-K).         
           MOVE SGS-VAL-TOPE IN SGS TO WSS-SGS-VAL-TOPE(WSS-K).         
           MOVE SGS-COD-VTOP IN SGS TO WSS-SGS-COD-VTOP(WSS-K).         
           MOVE SGS-COD-CPTT IN SGS TO WSS-SGS-COD-CPTT(WSS-K).         
           MOVE SGS-VAL-VMIN IN SGS TO WSS-SGS-VAL-VMIN(WSS-K).         
           MOVE SGS-VAL-VMAX IN SGS TO WSS-SGS-VAL-VMAX(WSS-K).         
           MOVE SGS-COD-VMYM IN SGS TO WSS-SGS-COD-VMYM(WSS-K).         
           MOVE SGS-IND-SOBL IN SGS TO WSS-SGS-IND-SOBL(WSS-K).         
           MOVE SGS-COD-IPVC IN SGS TO WSS-SGS-COD-IPVC(WSS-K).         
                                                                        
           IF SGS-IND-UTPZ IN SGS = 'D'                                 
               MOVE SGS-NUM-PLZO IN SGS TO WSS-CRT-ORD-UNO(WSS-K)       
           ELSE                                                         
           IF SGS-IND-UTPZ IN SGS = 'M' OR 'C'                          
               MULTIPLY SGS-NUM-PLZO IN SGS BY 30 GIVING                
                                            WSS-CRT-ORD-UNO(WSS-K)      
           ELSE                                                         
           IF SGS-IND-UTPZ IN SGS = 'A'                                 
               MULTIPLY SGS-NUM-PLZO IN SGS BY 365 GIVING               
                                            WSS-CRT-ORD-UNO(WSS-K)      
           ELSE                                                         
               MOVE 999999 TO WSS-CRT-ORD-UNO(WSS-K).                   
                                                                        
           IF SGS-NUM-EDAT IN SGS NOT > ZEROES                          
              MOVE 999 TO WSS-CRT-ORD-DOS(WSS-K)                        
           ELSE                                                         
              MOVE SGS-NUM-EDAT IN SGS TO WSS-CRT-ORD-DOS(WSS-K).       
                                                                        
           IF SGS-VAL-TOPE IN SGS NOT > ZEROES                          
              MOVE 99999999999 TO WSS-CRT-ORD-TRES(WSS-K)               
           ELSE                                                         
              PERFORM CONV-MON-NAC                                      
              IF MSG-COD-MENS > SPACES                                  
                 GO TO MSG-PRO-CMN-VRF                                  
              ELSE                                                      
                 MOVE WSS-VAL-PESO IN WSS-VARI TO                       
                                           WSS-CRT-ORD-TRES(WSS-K).     
                                                                        
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-PRO-CMN-VRF.                                       
                                                                        
       CNT-PRO-CMN-VRF.                                                 
           IF MSG-COD-MENS > SPACES                                     
               GO TO MSG-PRO-CMN-VRF.                                   
                                                                        
      *     PERFORM DISPL-VEC.                                          
           PERFORM SORT-WSS-SGS.                                        
      *     PERFORM DISPL-VEC.                                          
                                                                        
           PERFORM ELI-SGS.                                             
           IF MSG-COD-MENS > SPACES                                     
               GO TO MSG-PRO-CMN-VRF.                                   
                                                                        
           MOVE ZEROES TO WSS-NUM-RDPL.                                 
           PERFORM GRB-SGS.                                             
           IF MSG-COD-MENS > SPACES                                     
               GO TO MSG-PRO-CMN-VRF.                                   
                                                                        
           IF WSS-NUM-RDPL > ZEROES                                     
               MOVE WSS-NUM-RDPL TO WSS-NUM-GRDP                        
               MOVE WSS-GLS-RDPL TO FRM-MEN2.                           
                                                                        
           GO TO FIN-PRO-CMN-VRF.                                       
                                                                        
       MSG-PRO-CMN-VRF.                                                 
           IF MSG-COD-SIST NOT > SPACES                                 
              MOVE 'TAB' TO MSG-COD-SIST.                               
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MENS.                               
           MOVE 'S' TO SCR-IERR.                                        
           MOVE '00' TO FIO-STAT.                                       
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           GO TO FIN-PRO-CMN-VRF.                                       
                                                                        
       FIN-PRO-CMN-VRF.                                                 
           EXIT.                                                        
                                                                        
       SORT-WSS-SGS SECTION.                                            
       INI-SORT-WSS-SGS.                                                
           PERFORM ORD-UNO VARYING WSS-B FROM WSS-K BY -1               
                           UNTIL WSS-B < 1.                             
                                                                        
                                                                        
           PERFORM ORD-DOS.                                             
                                                                        
           PERFORM ORD-TRES.                                            
                                                                        
           PERFORM ORD-CUATRO.                                          
                                                                        
       FIN-SORT-WSS-SGS.                                                
           EXIT.                                                        
                                                                        
       ORD-UNO SECTION.                                                 
       INI-ORD-UNO.                                                     
           COMPUTE WSS-C = WSS-B - 1.                                   
           PERFORM ORD-UNO-A VARYING WSS-D FROM 1 BY 1                  
                           UNTIL WSS-D > WSS-C.                         
       FIN-ORD-UNO.                                                     
           EXIT.                                                        
                                                                        
       ORD-UNO-A SECTION.                                               
       INI-ORD-UNO-A.                                                   
           COMPUTE WSS-E = WSS-D + 1.                                   
            IF WSS-SGS-GLS-CRIT(WSS-D) > WSS-SGS-GLS-CRIT(WSS-E)        
                PERFORM SWP-WSS-SGS.                                    
       FIN-ORD-UNO-A.                                                   
           EXIT.                                                        
                                                                        
       ORD-DOS SECTION.                                                 
       INI-ORD-DOS.                                                     
           MOVE 1 TO WSS-A.                                             
           MOVE 1 TO WSS-INI.                                           
           MOVE WSS-SGS-GLS-CRIT(WSS-A) TO WSS-TMP-GLS-CRIT.            
       LUP-ORD-DOS.                                                     
           ADD 1 TO WSS-A.                                              
           IF WSS-A > WSS-K                                             
               COMPUTE WSS-FIN = WSS-A - 1                              
               COMPUTE WSS-LIM = WSS-FIN - WSS-INI                      
               IF WSS-LIM > ZEROES                                      
                   PERFORM ORD-DOS-A                                    
                   GO TO FIN-ORD-DOS                                    
               ELSE                                                     
                   GO TO FIN-ORD-DOS.                                   
                                                                        
           IF NOT WSS-SGS-GLS-CRIT(WSS-A) = WSS-TMP-GLS-CRIT            
               MOVE WSS-SGS-GLS-CRIT(WSS-A) TO WSS-TMP-GLS-CRIT         
               COMPUTE WSS-FIN = WSS-A - 1                              
               COMPUTE WSS-LIM = WSS-FIN - WSS-INI                      
               IF WSS-LIM > ZEROES                                      
                   PERFORM ORD-DOS-A                                    
                   MOVE WSS-A TO WSS-INI                                
               ELSE                                                     
                   MOVE WSS-A TO WSS-INI.                               
                                                                        
           GO TO LUP-ORD-DOS.                                           
                                                                        
       FIN-ORD-DOS.                                                     
           EXIT.                                                        
                                                                        
       ORD-DOS-A SECTION.                                               
       INI-ORD-DOS-A.                                                   
           PERFORM ORD-DOS-B VARYING WSS-B FROM WSS-FIN BY -1           
                           UNTIL WSS-B < 1.                             
       FIN-ORD-DOS-A.                                                   
           EXIT.                                                        
                                                                        
       ORD-DOS-B SECTION.                                               
       INI-ORD-DOS-B.                                                   
           COMPUTE WSS-C = WSS-B - 1.                                   
           PERFORM ORD-DOS-C VARYING WSS-D FROM WSS-INI BY 1            
                           UNTIL WSS-D > WSS-C.                         
       FIN-ORD-DOS-B.                                                   
           EXIT.                                                        
                                                                        
       ORD-DOS-C SECTION.                                               
       INI-ORD-DOS-C.                                                   
           COMPUTE WSS-E = WSS-D + 1.                                   
            IF WSS-CRT-ORD-UNO(WSS-D) > WSS-CRT-ORD-UNO(WSS-E)          
                PERFORM SWP-WSS-SGS.                                    
       FIN-ORD-DOS-C.                                                   
           EXIT.                                                        
                                                                        
       ORD-TRES SECTION.                                                
       INI-ORD-TRES.                                                    
           MOVE 1 TO WSS-A.                                             
           MOVE 1 TO WSS-INI.                                           
           MOVE WSS-SGS-GLS-CRIT(WSS-A) TO WSS-TMP-GLS-CRIT.            
           MOVE WSS-CRT-ORD-UNO(WSS-A)  TO WSS-TMP-CRIT-UNO.            
       LUP-ORD-TRES.                                                    
           ADD 1 TO WSS-A.                                              
           IF WSS-A > WSS-K                                             
               COMPUTE WSS-FIN = WSS-A - 1                              
               COMPUTE WSS-LIM = WSS-FIN - WSS-INI                      
               IF WSS-LIM > ZEROES                                      
                   PERFORM ORD-TRES-A                                   
                   GO TO FIN-ORD-TRES                                   
               ELSE                                                     
                   GO TO FIN-ORD-TRES.                                  
                                                                        
           MOVE WSS-SGS-GLS-CRIT(WSS-A) TO WSS-TMP-GLS-CRIT-V.          
           MOVE WSS-CRT-ORD-UNO(WSS-A)  TO WSS-TMP-CRIT-UNO-V.          
           IF NOT WSS-GLS-CDOS-V = WSS-GLS-CDOS                         
               MOVE WSS-SGS-GLS-CRIT(WSS-A) TO WSS-TMP-GLS-CRIT         
               MOVE WSS-CRT-ORD-UNO(WSS-A)  TO WSS-TMP-CRIT-UNO         
               COMPUTE WSS-FIN = WSS-A - 1                              
               COMPUTE WSS-LIM = WSS-FIN - WSS-INI                      
               IF WSS-LIM > ZEROES                                      
                   PERFORM ORD-TRES-A                                   
                   MOVE WSS-A TO WSS-INI                                
               ELSE                                                     
                   MOVE WSS-A TO WSS-INI.                               
                                                                        
           GO TO LUP-ORD-TRES.                                          
                                                                        
       FIN-ORD-TRES.                                                    
           EXIT.                                                        
                                                                        
       ORD-TRES-A SECTION.                                              
       INI-ORD-TRES-A.                                                  
           PERFORM ORD-TRES-B VARYING WSS-B FROM WSS-FIN BY -1          
                           UNTIL WSS-B < 1.                             
       FIN-ORD-TRES-A.                                                  
           EXIT.                                                        
                                                                        
       ORD-TRES-B SECTION.                                              
       INI-ORD-TRES-B.                                                  
           COMPUTE WSS-C = WSS-B - 1.                                   
           PERFORM ORD-TRES-C VARYING WSS-D FROM WSS-INI BY 1           
                           UNTIL WSS-D > WSS-C.                         
       FIN-ORD-TRES-B.                                                  
           EXIT.                                                        
                                                                        
       ORD-TRES-C SECTION.                                              
       INI-ORD-TRES-C.                                                  
           COMPUTE WSS-E = WSS-D + 1.                                   
            IF WSS-CRT-ORD-DOS(WSS-D) > WSS-CRT-ORD-DOS(WSS-E)          
                PERFORM SWP-WSS-SGS.                                    
       FIN-ORD-TRES-C.                                                  
           EXIT.                                                        
                                                                        
                                                                        
       ORD-CUATRO SECTION.                                              
       INI-ORD-CUATRO.                                                  
           MOVE 1 TO WSS-A.                                             
           MOVE 1 TO WSS-INI.                                           
           MOVE WSS-SGS-GLS-CRIT(WSS-A) TO WSS-TMP-GLS-CRIT.            
           MOVE WSS-CRT-ORD-UNO(WSS-A)  TO WSS-TMP-CRIT-UNO.            
           MOVE WSS-CRT-ORD-DOS(WSS-A)  TO WSS-TMP-CRIT-DOS.            
       LUP-ORD-CUATRO.                                                  
           ADD 1 TO WSS-A.                                              
           IF WSS-A > WSS-K                                             
               COMPUTE WSS-FIN = WSS-A - 1                              
               COMPUTE WSS-LIM = WSS-FIN - WSS-INI                      
               IF WSS-LIM > ZEROES                                      
                   PERFORM ORD-CUATRO-A                                 
                   GO TO FIN-ORD-CUATRO                                 
               ELSE                                                     
                   GO TO FIN-ORD-CUATRO.                                
                                                                        
           MOVE WSS-SGS-GLS-CRIT(WSS-A) TO WSS-TMP-GLS-CRIT-V.          
           MOVE WSS-CRT-ORD-UNO(WSS-A)  TO WSS-TMP-CRIT-UNO-V.          
           MOVE WSS-CRT-ORD-DOS(WSS-A)  TO WSS-TMP-CRIT-DOS-V.          
           IF NOT WSS-GLS-CTRES-V = WSS-GLS-CTRES                       
               MOVE WSS-SGS-GLS-CRIT(WSS-A) TO WSS-TMP-GLS-CRIT         
               MOVE WSS-CRT-ORD-UNO(WSS-A)  TO WSS-TMP-CRIT-UNO         
               MOVE WSS-CRT-ORD-DOS(WSS-A)  TO WSS-TMP-CRIT-DOS         
               COMPUTE WSS-FIN = WSS-A - 1                              
               COMPUTE WSS-LIM = WSS-FIN - WSS-INI                      
               IF WSS-LIM > ZEROES                                      
                   PERFORM ORD-CUATRO-A                                 
                   MOVE WSS-A TO WSS-INI                                
               ELSE                                                     
                   MOVE WSS-A TO WSS-INI.                               
                                                                        
           GO TO LUP-ORD-CUATRO.                                        
                                                                        
       FIN-ORD-CUATRO.                                                  
           EXIT.                                                        
                                                                        
       ORD-CUATRO-A SECTION.                                            
       INI-ORD-CUATRO-A.                                                
           PERFORM ORD-CUATRO-B VARYING WSS-B FROM WSS-FIN BY -1        
                           UNTIL WSS-B < 1.                             
       FIN-ORD-CUATRO-A.                                                
           EXIT.                                                        
                                                                        
       ORD-CUATRO-B SECTION.                                            
       INI-ORD-CUATRO-B.                                                
           COMPUTE WSS-C = WSS-B - 1.                                   
           PERFORM ORD-CUATRO-C VARYING WSS-D FROM WSS-INI BY 1         
                           UNTIL WSS-D > WSS-C.                         
       FIN-ORD-CUATRO-B.                                                
           EXIT.                                                        
                                                                        
       ORD-CUATRO-C SECTION.                                            
       INI-ORD-CUATRO-C.                                                
           COMPUTE WSS-E = WSS-D + 1.                                   
            IF WSS-CRT-ORD-TRES(WSS-D) > WSS-CRT-ORD-TRES(WSS-E)        
                PERFORM SWP-WSS-SGS.                                    
       FIN-ORD-CUATRO-C.                                                
           EXIT.                                                        
                                                                        
       ELI-SGS SECTION.                                                 
       INI-ELI-SGS.                                                     
           MOVE SGS-CAI-PLAN IN SGS-FLD TO SGS-CAI-PLAN IN SGS.         
           MOVE SGS-IIC-PLAN IN SGS-FLD TO SGS-IIC-PLAN IN SGS.         
           MOVE SGS-COD-SIST IN SGS-FLD TO SGS-COD-SIST IN SGS.         
           MOVE SGS-COD-EVEN IN SGS-FLD TO SGS-COD-EVEN IN SGS.         
           MOVE SGS-IND-TPLT IN SGS-FLD TO SGS-IND-TPLT IN SGS.         
           MOVE SGS-NUM-DLAN IN SGS-FLD TO SGS-NUM-DLAN IN SGS.         
           MOVE SGS-NUM-MLAN IN SGS-FLD TO SGS-NUM-MLAN IN SGS.         
           MOVE SGS-NUM-SLAN IN SGS-FLD TO SGS-NUM-SLAN IN SGS.         
           MOVE SGS-NUM-ALAN IN SGS-FLD TO SGS-NUM-ALAN IN SGS.         
           MOVE SGS-HRA-PLAN IN SGS-FLD TO SGS-HRA-PLAN IN SGS.         
           MOVE ZEROES TO SGS-NUM-ISGS IN SGS.                          
           MOVE FIO-GET-NLS TO FIO-CMND.                                
           PERFORM MC2-FIO-SGS.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
              SGS-CAI-PLAN IN SGS-FLD = SGS-CAI-PLAN IN SGS AND         
              SGS-IIC-PLAN IN SGS-FLD = SGS-IIC-PLAN IN SGS AND         
              SGS-COD-SIST IN SGS-FLD = SGS-COD-SIST IN SGS AND         
              SGS-COD-EVEN IN SGS-FLD = SGS-COD-EVEN IN SGS AND         
              SGS-IND-TPLT IN SGS-FLD = SGS-IND-TPLT IN SGS AND         
              SGS-NUM-DLAN IN SGS-FLD = SGS-NUM-DLAN IN SGS AND         
              SGS-NUM-MLAN IN SGS-FLD = SGS-NUM-MLAN IN SGS AND         
              SGS-NUM-SLAN IN SGS-FLD = SGS-NUM-SLAN IN SGS AND         
              SGS-NUM-ALAN IN SGS-FLD = SGS-NUM-ALAN IN SGS AND         
              SGS-HRA-PLAN IN SGS-FLD = SGS-HRA-PLAN IN SGS )           
               GO TO FIN-ELI-SGS.                                       
                                                                        
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.                            
           PERFORM MC2-FIO-SGS.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE 'SGSVRF NOUPD' TO MSG-COD-MENS                      
               MOVE FIO-BCK-OUT TO FIO-CMND                             
               PERFORM MC2-FIO-SGS                                      
               GO TO FIN-ELI-SGS.                                       
                                                                        
           MOVE FIO-DEL TO FIO-CMND.                                    
           PERFORM MC2-FIO-SGS.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE 'SGSVRF NODEL' TO MSG-COD-MENS                      
               MOVE FIO-BCK-OUT TO FIO-CMND                             
               PERFORM MC2-FIO-SGS                                      
               GO TO FIN-ELI-SGS.                                       
                                                                        
           GO TO INI-ELI-SGS.                                           
       FIN-ELI-SGS.                                                     
           EXIT.                                                        
                                                                        
       GRB-SGS SECTION.                                                 
       INI-GRB-SGS.                                                     
           MOVE ZEROES TO WSS-L.                                        
           MOVE ZEROES TO WSS-NDPL.                                     
           PERFORM CLR-SGS-ANT.                                         
       LUP-GRB-SGS.                                                     
           ADD 1 TO WSS-L.                                              
           IF WSS-L > WSS-K                                             
               GO TO FIN-GRB-SGS.                                       
                                                                        
           PERFORM CHK-DUPL-SGS.                                        
           IF WSS-IND-DUPL = 'S'                                        
               ADD 1 TO WSS-NUM-RDPL                                    
               GO TO LUP-GRB-SGS                                        
           ELSE                                                         
               ADD 1 TO WSS-NDPL.                                       
                                                                        
           MOVE SGS-CAI-PLAN IN SGS-FLD TO SGS-CAI-PLAN IN SGS.         
           MOVE SGS-IIC-PLAN IN SGS-FLD TO SGS-IIC-PLAN IN SGS.         
           MOVE SGS-COD-SIST IN SGS-FLD TO SGS-COD-SIST IN SGS.         
           MOVE SGS-COD-EVEN IN SGS-FLD TO SGS-COD-EVEN IN SGS.         
           MOVE SGS-IND-TPLT IN SGS-FLD TO SGS-IND-TPLT IN SGS.         
           MOVE SGS-NUM-DLAN IN SGS-FLD TO SGS-NUM-DLAN IN SGS.         
           MOVE SGS-NUM-MLAN IN SGS-FLD TO SGS-NUM-MLAN IN SGS.         
           MOVE SGS-NUM-SLAN IN SGS-FLD TO SGS-NUM-SLAN IN SGS.         
           MOVE SGS-NUM-ALAN IN SGS-FLD TO SGS-NUM-ALAN IN SGS.         
           MOVE SGS-HRA-PLAN IN SGS-FLD TO SGS-HRA-PLAN IN SGS.         
           MOVE WSS-NDPL                TO SGS-NUM-ISGS IN SGS.         
                                                                        
           MOVE WSS-SGS-STP-ITRN(WSS-L) TO SGS-STP-ITRN IN SGS.         
           MOVE WSS-SGS-MSC-TACC(WSS-L) TO SGS-MSC-TACC IN SGS.         
      *     MOVE WSS-SGS-MSC-STAT(WSS-L) TO SGS-MSC-STAT IN SGS.        
           MOVE 'V'                     TO SGS-MSC-STAT IN SGS.         
      *PVG INI 04-JUN-2009                                              
           IF SCR-CMND = 'COP'                                          
              MOVE 'I'                  TO SGS-MSC-STAT IN SGS.         
      *PVG FIN 04-JUN-2009                                              
           MOVE WSS-SGS-COD-OTRN(WSS-L) TO SGS-COD-OTRN IN SGS.         
           MOVE WSS-SGS-COD-ATRN(WSS-L) TO SGS-COD-ATRN IN SGS.         
           MOVE WSS-SGS-IND-DCTO(WSS-L) TO SGS-IND-DCTO IN SGS.         
           MOVE WSS-SGS-IND-CAAB(WSS-L) TO SGS-IND-CAAB IN SGS.         
           MOVE WSS-SGS-COD-TSEG(WSS-L) TO SGS-COD-TSEG IN SGS.         
           MOVE WSS-SGS-COD-SEGU(WSS-L) TO SGS-COD-SEGU IN SGS.         
           MOVE WSS-SGS-COD-OCOB(WSS-L) TO SGS-COD-OCOB IN SGS.         
           MOVE WSS-SGS-SGV-TAMO(WSS-L) TO SGS-SGV-TAMO IN SGS.         
           MOVE WSS-SGS-SGV-TMEX(WSS-L) TO SGS-SGV-TMEX IN SGS.         
           MOVE WSS-SGS-COD-VCOC(WSS-L) TO SGS-COD-VCOC IN SGS.         
           MOVE WSS-SGS-COD-CPTO(WSS-L) TO SGS-COD-CPTO IN SGS.         
           MOVE WSS-SGS-COD-TINT(WSS-L) TO SGS-COD-TINT IN SGS.         
           MOVE WSS-SGS-NUM-PLZO(WSS-L) TO SGS-NUM-PLZO IN SGS.         
           MOVE WSS-SGS-IND-UTPZ(WSS-L) TO SGS-IND-UTPZ IN SGS.         
           MOVE WSS-SGS-NUM-EDAT(WSS-L) TO SGS-NUM-EDAT IN SGS.         
           MOVE WSS-SGS-IND-EDRA(WSS-L) TO SGS-IND-EDRA IN SGS.         
           MOVE WSS-SGS-VAL-TOPE(WSS-L) TO SGS-VAL-TOPE IN SGS.         
           MOVE WSS-SGS-COD-VTOP(WSS-L) TO SGS-COD-VTOP IN SGS.         
           MOVE WSS-SGS-COD-CPTT(WSS-L) TO SGS-COD-CPTT IN SGS.         
           MOVE WSS-SGS-VAL-VMIN(WSS-L) TO SGS-VAL-VMIN IN SGS.         
           MOVE WSS-SGS-VAL-VMAX(WSS-L) TO SGS-VAL-VMAX IN SGS.         
           MOVE WSS-SGS-COD-VMYM(WSS-L) TO SGS-COD-VMYM IN SGS.         
           MOVE WSS-SGS-IND-SOBL(WSS-L) TO SGS-IND-SOBL IN SGS.         
           MOVE WSS-SGS-COD-IPVC(WSS-L) TO SGS-COD-IPVC IN SGS.         
           MOVE SPACES                  TO SGS-GLS-DISP IN SGS.         
                                                                        
           MOVE FIO-PUT TO FIO-CMND.                                    
           PERFORM MC2-FIO-SGS.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE FIO-BCK-OUT TO FIO-CMND                             
               PERFORM MC2-FIO-SGS                                      
               MOVE 'SGSVRF NOPUT' TO MSG-COD-MENS                      
               GO TO FIN-GRB-SGS.                                       
                                                                        
           GO TO LUP-GRB-SGS.                                           
       FIN-GRB-SGS.                                                     
           EXIT.                                                        
                                                                        
       CHK-DUPL-SGS SECTION.                                            
       INI-CHK-DUPL-SGS.                                                
           MOVE SPACES TO WSS-IND-DUPL.                                 
      *PVG INI 04-JUN-2009                                              
           IF SCR-CMND = 'COP'                                          
              GO TO FIN-CHK-DUPL-SGS.                                   
      *PVG FIN 04-JUN-2009                                              
                                                                        
           MOVE 'TAB' TO TAB-COD-SIST FIO-SIST.                         
           MOVE 'SGS' TO TAB-COD-TTAB IN TAB.                           
           MOVE WSS-SGS-COD-SEGU(WSS-L) TO TAB-COD-CTAB IN TAB.         
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM GNS-FIO-TAB                                          
           MOVE TAB-CIC-CTAB IN TAB TO WSS-CIC-CSEG IN WSS-VARI.        
                                                                        
           IF WSS-CIC-SEGU IN WSS-VARI = 'INF'                          
           IF WSS-SGS-COD-TSEG(WSS-L) = WSS-COD-TSEG-ANT AND            
                 WSS-SGS-COD-OCOB(WSS-L) = WSS-COD-OCOB-ANT AND         
                 WSS-SGS-NUM-PLZO(WSS-L) = WSS-NUM-PLZO-ANT AND         
                 WSS-SGS-IND-UTPZ(WSS-L) = WSS-IND-UTPZ-ANT AND         
                 WSS-SGS-NUM-EDAT(WSS-L) = WSS-NUM-EDAT-ANT AND         
                 WSS-SGS-IND-EDRA(WSS-L) = WSS-IND-EDRA-ANT AND         
                 WSS-SGS-VAL-TOPE(WSS-L) = WSS-VAL-TOPE-ANT AND         
                 WSS-SGS-COD-VTOP(WSS-L) = WSS-COD-VTOP-ANT AND         
                 WSS-SGS-COD-CPTT(WSS-L) = WSS-COD-CPTT-ANT             
                  MOVE 'S' TO WSS-IND-DUPL                              
                  GO TO CNT-CHK-DUPL-SGS.                               
                                                                        
      *PVG INI 04-JUN-2009                                              
      *    IF WSS-CIC-SEGU IN WSS-VARI = 'CSP' OR 'MFS'                 
           IF (WSS-CIC-SEGU IN WSS-VARI = 'CSP' OR 'MFS' OR             
                                 'MXD' OR 'MXM' OR 'MXA' OR             
                                 'FXD' OR 'FXM' OR 'FXA')               
      *PVG FIN 04-JUN-2009                                              
              IF WSS-SGS-COD-TSEG(WSS-L) = WSS-COD-TSEG-ANT AND         
                 WSS-SGS-COD-OCOB(WSS-L) = WSS-COD-OCOB-ANT AND         
      *           WSS-SGS-SGV-TAMO(WSS-L) = WSS-SGV-TAMO-ANT AND        
      *           WSS-SGS-COD-VCOC(WSS-L) = WSS-COD-VCOC-ANT AND        
      *           WSS-SGS-COD-CPTO(WSS-L) = WSS-COD-CPTO-ANT AND        
                 WSS-SGS-COD-IPVC(WSS-L) = WSS-COD-IPVC-ANT AND         
                 WSS-SGS-NUM-PLZO(WSS-L) = WSS-NUM-PLZO-ANT AND         
                 WSS-SGS-IND-UTPZ(WSS-L) = WSS-IND-UTPZ-ANT AND         
                 WSS-SGS-NUM-EDAT(WSS-L) = WSS-NUM-EDAT-ANT AND         
                 WSS-SGS-IND-EDRA(WSS-L) = WSS-IND-EDRA-ANT AND         
                 WSS-SGS-VAL-TOPE(WSS-L) = WSS-VAL-TOPE-ANT AND         
                 WSS-SGS-COD-VTOP(WSS-L) = WSS-COD-VTOP-ANT AND         
                 WSS-SGS-COD-CPTT(WSS-L) = WSS-COD-CPTT-ANT             
                  MOVE 'S' TO WSS-IND-DUPL                              
                  GO TO CNT-CHK-DUPL-SGS.                               
                                                                        
       CNT-CHK-DUPL-SGS.                                                
           MOVE WSS-SGS-COD-TSEG(WSS-L) TO WSS-COD-TSEG-ANT.            
           MOVE WSS-SGS-COD-SEGU(WSS-L) TO WSS-COD-SEGU-ANT.            
           MOVE WSS-SGS-COD-OCOB(WSS-L) TO WSS-COD-OCOB-ANT.            
           MOVE WSS-SGS-SGV-TAMO(WSS-L) TO WSS-SGV-TAMO-ANT.            
           MOVE WSS-SGS-SGV-TMEX(WSS-L) TO WSS-SGV-TMEX-ANT.            
           MOVE WSS-SGS-COD-VCOC(WSS-L) TO WSS-COD-VCOC-ANT.            
           MOVE WSS-SGS-COD-CPTO(WSS-L) TO WSS-COD-CPTO-ANT.            
           MOVE WSS-SGS-COD-IPVC(WSS-L) TO WSS-COD-IPVC-ANT.            
           MOVE WSS-SGS-NUM-PLZO(WSS-L) TO WSS-NUM-PLZO-ANT.            
           MOVE WSS-SGS-IND-UTPZ(WSS-L) TO WSS-IND-UTPZ-ANT.            
           MOVE WSS-SGS-NUM-EDAT(WSS-L) TO WSS-NUM-EDAT-ANT.            
           MOVE WSS-SGS-IND-EDRA(WSS-L) TO WSS-IND-EDRA-ANT.            
           MOVE WSS-SGS-VAL-TOPE(WSS-L) TO WSS-VAL-TOPE-ANT.            
           MOVE WSS-SGS-COD-VTOP(WSS-L) TO WSS-COD-VTOP-ANT.            
           MOVE WSS-SGS-COD-CPTT(WSS-L) TO WSS-COD-CPTT-ANT.            
       FIN-CHK-DUPL-SGS.                                                
           EXIT.                                                        
                                                                        
       CLR-SGS-ANT SECTION.                                             
       INI-CLR-SGS-ANT.                                                 
           MOVE SPACES TO WSS-COD-TSEG-ANT.                             
           MOVE SPACES TO WSS-COD-SEGU-ANT.                             
           MOVE SPACES TO WSS-COD-OCOB-ANT.                             
           MOVE ZEROES TO WSS-SGV-TAMO-ANT.                             
           MOVE ZEROES TO WSS-SGV-TMEX-ANT.                             
           MOVE SPACES TO WSS-COD-VCOC-ANT.                             
           MOVE SPACES TO WSS-COD-CPTO-ANT.                             
           MOVE SPACES TO WSS-COD-IPVC-ANT.                             
           MOVE ZEROES TO WSS-NUM-PLZO-ANT.                             
           MOVE SPACES TO WSS-IND-UTPZ-ANT.                             
           MOVE ZEROES TO WSS-NUM-EDAT-ANT.                             
           MOVE SPACES TO WSS-IND-EDRA-ANT.                             
           MOVE ZEROES TO WSS-VAL-TOPE-ANT.                             
           MOVE SPACES TO WSS-COD-VTOP-ANT.                             
           MOVE SPACES TO WSS-COD-CPTT-ANT.                             
       INI-CLR-SGS-ANT.                                                 
           EXIT.                                                        
                                                                        
       OBT-COD-PESO SECTION.                                            
       INI-OBT-COD-PESO.                                                
      * OBTIENE CODIGO VALOR DE CAMBIO DE PESOS MEDIANTE CIC DE LA MONED
           MOVE 'TAB'          TO FIO-SIST.                             
           MOVE 'VLR'          TO TAB-CIC-TTAB IN TAB.                  
           MOVE '0999  '       TO TAB-CIC-CTAB IN TAB.                  
           MOVE 'TAB-CIC-TABL' TO FIO-AKEY.                             
           MOVE FIO-GET-KEY    TO FIO-CMND.                             
           PERFORM GNS-FIO-TAB.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE 'TAB'          TO MSG-COD-SIST                      
               MOVE 'CIC0999  NEX' TO MSG-COD-MENS.                     
                                                                        
           MOVE TAB-COD-CTAB IN TAB TO WSS-COD-PESO IN WSS-VARI.        
       FIN-OBT-COD-PESO.                                                
           EXIT.                                                        
                                                                        
       CONV-MON-NAC SECTION.                                            
       INI-CONV-MON-NAC.                                                
           MOVE ZEROES TO WSS-VAL-PESO IN WSS-VARI.                     
           PERFORM OBT-COD-PESO.                                        
           IF MSG-COD-MENS > SPACES                                     
               GO TO FIN-CONV-MON-NAC.                                  
                                                                        
           IF SGS-COD-VTOP IN SGS = WSS-COD-PESO IN WSS-VARI            
               MOVE SGS-VAL-TOPE IN SGS TO WSS-VAL-PESO IN WSS-VARI     
               GO TO FIN-CONV-MON-NAC.                                  
                                                                        
           PERFORM GET-FHOY.                                            
                                                                        
           MOVE SGS-COD-VTOP IN SGS      TO CAM-COD-VCAM IN CAM(1).     
           MOVE WSS-COD-PESO IN WSS-VARI TO CAM-COD-VCAM IN CAM(2).     
           MOVE HOY-FHOY                 TO CAM-FEC-FCAM IN CAM(1)      
                                            CAM-FEC-FCAM IN CAM(2).     
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM TAB-FIO-CAM.                                         
           IF NOT FIO-STAT-OKS OR                                       
              CAM-MSC-STAT IN CAM NOT = 'S'                             
               MOVE 'COL'        TO MSG-COD-SIST                        
               MOVE 'CAM    NEX' TO MSG-COD-MENS                        
               GO TO FIN-CONV-MON-NAC.                                  
                                                                        
           MULTIPLY CAM-SGV-VCAM IN CAM BY SGS-VAL-TOPE IN SGS          
                   GIVING WSS-VAL-PESO IN WSS-VARI.                     
       FIN-CONV-MON-NAC.                                                
           EXIT.                                                        
                                                                        
       SWP-WSS-SGS SECTION.                                             
       INI-SWP-WSS-SGS.                                                 
           MOVE WSS-VEC-SGS(WSS-D) TO WSS-VEC-SGS-TMP.                  
           MOVE WSS-VEC-SGS(WSS-E) TO WSS-VEC-SGS(WSS-D).               
           MOVE WSS-VEC-SGS-TMP    TO WSS-VEC-SGS(WSS-E).               
       FIN-SWP-WSS-SGS.                                                 
           EXIT.                                                        
                                                                        
       DISPL-VEC SECTION.                                               
       INI-DISPL-VEC.                                                   
           MOVE ZEROES TO WSS-M.                                        
       LUP-DISPL-VEC.                                                   
           ADD 1 TO WSS-M.                                              
           IF WSS-M > WSS-K                                             
               GO TO FIN-DISPL-VEC.                                     
           GO TO LUP-DISPL-VEC.                                         
       FIN-DISPL-VEC.                                                   
           EXIT.                                                        
                                                                        
       PRO-CMN-ACA SECTION.                                             
       INI-PRO-CMN-ACA.                                                 
           MOVE SGS-CAI-PLAN IN SGS-FLD TO SGS-CAI-PLAN IN SGS.         
           MOVE SGS-IIC-PLAN IN SGS-FLD TO SGS-IIC-PLAN IN SGS.         
           MOVE SGS-COD-SIST IN SGS-FLD TO SGS-COD-SIST IN SGS.         
           MOVE SGS-COD-EVEN IN SGS-FLD TO SGS-COD-EVEN IN SGS.         
           MOVE SGS-IND-TPLT IN SGS-FLD TO SGS-IND-TPLT IN SGS.         
           MOVE SGS-NUM-DLAN IN SGS-FLD TO SGS-NUM-DLAN IN SGS.         
           MOVE SGS-NUM-MLAN IN SGS-FLD TO SGS-NUM-MLAN IN SGS.         
           MOVE SGS-NUM-SLAN IN SGS-FLD TO SGS-NUM-SLAN IN SGS.         
           MOVE SGS-NUM-ALAN IN SGS-FLD TO SGS-NUM-ALAN IN SGS.         
           MOVE SGS-HRA-PLAN IN SGS-FLD TO SGS-HRA-PLAN IN SGS.         
           MOVE ZEROES TO SGS-NUM-ISGS IN SGS.                          
       LUP-PRO-CMN-ACA.                                                 
           MOVE FIO-GET-NLS TO FIO-CMND.                                
           PERFORM MC2-FIO-SGS.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
              SGS-CAI-PLAN IN SGS-FLD = SGS-CAI-PLAN IN SGS AND         
              SGS-IIC-PLAN IN SGS-FLD = SGS-IIC-PLAN IN SGS AND         
              SGS-COD-SIST IN SGS-FLD = SGS-COD-SIST IN SGS AND         
              SGS-COD-EVEN IN SGS-FLD = SGS-COD-EVEN IN SGS AND         
              SGS-IND-TPLT IN SGS-FLD = SGS-IND-TPLT IN SGS AND         
              SGS-NUM-DLAN IN SGS-FLD = SGS-NUM-DLAN IN SGS AND         
              SGS-NUM-MLAN IN SGS-FLD = SGS-NUM-MLAN IN SGS AND         
              SGS-NUM-SLAN IN SGS-FLD = SGS-NUM-SLAN IN SGS AND         
              SGS-NUM-ALAN IN SGS-FLD = SGS-NUM-ALAN IN SGS AND         
              SGS-HRA-PLAN IN SGS-FLD = SGS-HRA-PLAN IN SGS )           
               GO TO FIN-PRO-CMN-ACA.                                   
                                                                        
           IF SGS-IND-VIGE IN SGS NOT = 'V'                             
               MOVE 'SGS    NOVRF' TO MSG-COD-MENS                      
               MOVE FIO-BCK-OUT TO FIO-CMND                             
               PERFORM MC2-FIO-SGS                                      
               GO TO MSG-PRO-CMN-ACA.                                   
                                                                        
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.                            
           PERFORM MC2-FIO-SGS.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE 'SGSACA NOUPD' TO MSG-COD-MENS                      
               MOVE FIO-BCK-OUT TO FIO-CMND                             
               PERFORM MC2-FIO-SGS                                      
               GO TO MSG-PRO-CMN-ACA.                                   
                                                                        
           MOVE 'A' TO SGS-IND-VIGE IN SGS.                             
           MOVE FIO-MOD TO FIO-CMND.                                    
           PERFORM MC2-FIO-SGS.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE 'SGSACA NOMOD' TO MSG-COD-MENS                      
               MOVE FIO-BCK-OUT TO FIO-CMND                             
               PERFORM MC2-FIO-SGS                                      
               GO TO MSG-PRO-CMN-ACA.                                   
                                                                        
       NXT-PRO-CMN-ACA.                                                 
           ADD 1 TO SGS-NUM-ISGS IN SGS.                                
           GO TO LUP-PRO-CMN-ACA.                                       
                                                                        
       MSG-PRO-CMN-ACA.                                                 
           IF MSG-COD-SIST NOT > SPACES                                 
              MOVE 'TAB' TO MSG-COD-SIST.                               
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MENS.                               
           MOVE '00' TO FIO-STAT.                                       
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           GO TO FIN-PRO-CMN-ACA.                                       
                                                                        
       FIN-PRO-CMN-ACA.                                                 
           EXIT.                                                        
                                                                        
       PRO-CMN-ANL SECTION.                                             
       INI-PRO-CMN-ANL.                                                 
           MOVE SGS-CAI-PLAN IN SGS-FLD TO SGS-CAI-PLAN IN SGS.         
           MOVE SGS-IIC-PLAN IN SGS-FLD TO SGS-IIC-PLAN IN SGS.         
           MOVE SGS-COD-SIST IN SGS-FLD TO SGS-COD-SIST IN SGS.         
           MOVE SGS-COD-EVEN IN SGS-FLD TO SGS-COD-EVEN IN SGS.         
           MOVE SGS-IND-TPLT IN SGS-FLD TO SGS-IND-TPLT IN SGS.         
           MOVE SGS-NUM-DLAN IN SGS-FLD TO SGS-NUM-DLAN IN SGS.         
           MOVE SGS-NUM-MLAN IN SGS-FLD TO SGS-NUM-MLAN IN SGS.         
           MOVE SGS-NUM-SLAN IN SGS-FLD TO SGS-NUM-SLAN IN SGS.         
           MOVE SGS-NUM-ALAN IN SGS-FLD TO SGS-NUM-ALAN IN SGS.         
           MOVE SGS-HRA-PLAN IN SGS-FLD TO SGS-HRA-PLAN IN SGS.         
           MOVE ZEROES TO SGS-NUM-ISGS IN SGS.                          
       LUP-PRO-CMN-ANL.                                                 
           MOVE FIO-GET-NLS TO FIO-CMND.                                
           PERFORM MC2-FIO-SGS.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
              SGS-CAI-PLAN IN SGS-FLD = SGS-CAI-PLAN IN SGS AND         
              SGS-IIC-PLAN IN SGS-FLD = SGS-IIC-PLAN IN SGS AND         
              SGS-COD-SIST IN SGS-FLD = SGS-COD-SIST IN SGS AND         
