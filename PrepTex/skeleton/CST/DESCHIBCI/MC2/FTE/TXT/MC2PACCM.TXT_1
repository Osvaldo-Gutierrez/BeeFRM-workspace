*%! Codigo adicional para archivo
*% PGM_PTC = TRUE
*% LARGO_TOTAL_COMMAREA = 3000
*% IF FIN_SCR_VARI
      *<<<< FIN_SCR_VARI
           03  SCR-VARI-PFS.
               05  SCR-DCM-KEY-IDCM.
                   07 SCR-DCM-CIC-IDCM              PIC X(12).
                   07 SCR-DCM-NUM-IDCM              PIC 9(03).
           03  SCR-IND-ELI                          PIC X(01).
           03  SCR-UMOD-CCM            VALUE SPACES PIC X(14).
      *>>>>
*% END
*% IF WSS
      *<<<< WSS
       COPY GNSWCVRF.
       COPY GNSWVVRF.
       COPY GNSBRCIC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CIC-REQA==.
       COPY GNSWGCIC.
       COPY MC2BRDCM.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DCM-REQA==.
       COPY GNSBRMSC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSC-REQA==.
       COPY TABBRCAM.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CAM-REQA==.

       COPY MC2WGDCM.
       COPY MC2WXDCM.
       01  WSS-VARI.
           03 WSS-COD-MENY        VALUE SPACES          PIC X(12).
           03 WSS-COD-MENN        VALUE SPACES          PIC X(12).
           03 WSS-COD-EVEN.
              05 WSS-COD-SIST                           PIC X(03).
              05 WSS-COD-EVTO                           PIC X(06).
           03 WSS-COD-VCAM.
              05 WSS-COD-TVTI                           PIC X(01).
              05 WSS-COD-CVTI                           PIC X(03).
              05 WSS-COD-TCTI                           PIC X(02).
      *
           03 WSS-COD-PESO        VALUE SPACES          PIC X(06).
           03 WSS-K               VALUE ZEROES          PIC 9(03).
           03 WSS-R               VALUE ZEROES          PIC 9(03).
           03 WSS-L               VALUE ZEROES          PIC 9(03).
           03 WSS-V               VALUE ZEROES          PIC 9(03).
           03 WSS-V1              VALUE ZEROES          PIC 9(03).
           03 WSS-V2              VALUE ZEROES          PIC 9(03).
           03 WSS-V3              VALUE ZEROES          PIC 9(03).
      *     03 WSS-GLS-TPLT.                                             
      *        05 WSS-CIC-TPLT                           PIC X(01).      
      *        05 WSS-GLS-DPLT                           PIC X(11).      
      *     03 WSS-RDCM            VALUE ZEROES          PIC 9(03).      
      *>>>>
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 OR
                         FRM-FFLD-PF7 OR FRM-FFLD-PF8
               PERFORM VAL-NUM-KEY-CCM
               PERFORM VAL-NUM-FLD-CCM
               PERFORM SCRL-TAB-CCM
               PERFORM SCR-TRL.
      *>>>>
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-OFRM-SRV
                MOVE TSK-TERM-ALF TO DCM-COD-TERM
                MOVE TSK-TERM-ALF TO DCM-COD-TERM-MAS
           ELSE
                MOVE EIBTRMID     TO DCM-COD-TERM
                MOVE EIBTRMID     TO DCM-COD-TERM-MAS.
      *>>>>
*% END
*% IF FND_KEY
      *<<<< FND_KEY
           IF SCR-CCPP = 'ING'
              MOVE 'I' TO CCM-IND-VIGE IN CCM-FLD.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
           MOVE CCM-STP-ITRN IN CCM TO SCR-UMOD-CCM.
           IF NOT FIO-STAT-OKS
              GO TO DCM-FND-KEY.
           IF SCR-CMND = 'CON'
              GO TO DCM-FND-KEY.
      *
           IF CCM-MSC-STAT IN CCM = 'D' AND SCR-CMND NOT = 'CON'
              MOVE 'S' TO SCR-IERR
              MOVE 'CCM    ANLYA' TO MSG-COD-MENS
              GO TO MSG-FND-KEY.
           IF CCM-MSC-STAT IN CCM = 'A' AND SCR-CMND NOT = 'ANL'
              MOVE 'S' TO SCR-IERR
              MOVE 'CCM    ACAYA' TO MSG-COD-MENS
              GO TO MSG-FND-KEY.
           IF CCM-MSC-STAT IN CCM = 'V' AND SCR-CMND = 'ANL'
              MOVE 'S' TO SCR-IERR
              MOVE 'CCM    ACANO' TO MSG-COD-MENS
              GO TO MSG-FND-KEY.
           IF SCR-CMND = 'VRF'
              IF CCM-MSC-STAT IN CCM = 'I' OR 'M'
                 GO TO DCM-FND-KEY
              ELSE
                 MOVE 'S' TO SCR-IERR
                 MOVE 'CMN      INV' TO MSG-COD-MENS
                 GO TO MSG-FND-KEY.
           IF SCR-CMND = 'ACA'
              IF CCM-MSC-STAT IN CCM = 'V'
                 GO TO DCM-FND-KEY
              ELSE
                 MOVE 'S' TO SCR-IERR
                 MOVE 'CMN      INV' TO MSG-COD-MENS
                 GO TO MSG-FND-KEY.
           IF SCR-CMND = 'ANL'
              IF CCM-MSC-STAT IN CCM = 'A'
                 GO TO DCM-FND-KEY
              ELSE
                 MOVE 'S' TO SCR-IERR
                 MOVE 'CMN      INV' TO MSG-COD-MENS
                 GO TO MSG-FND-KEY.
           IF SCR-CMND = 'ELI'
              IF CCM-MSC-STAT IN CCM = 'I' OR 'M' OR 'V'
                 GO TO DCM-FND-KEY
              ELSE
                 MOVE 'S' TO SCR-IERR
                 MOVE 'CMN      INV' TO MSG-COD-MENS
                 GO TO MSG-FND-KEY.
           GO TO DCM-FND-KEY.
       MSG-FND-KEY.
           MOVE 'TAB'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE '00'         TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
       DCM-FND-KEY.
           IF SCR-CCPP = 'ING'
               PERFORM BUS-COR-DCM
               GO TO CON-FND-KEY
           ELSE
              IF NOT FIO-STAT-OKS
                 GO TO FIN-FND-KEY.
      *PVG INI 25-OCT-2007                                              
            IF SCR-OFRM-SRV AND SCR-CMND = 'CON' AND                    
               DCM-NUM-IDCM IN CCM-FLD NOT > ZEROES                     
               MOVE 1 TO DCM-NUM-IDCM IN CCM-FLD.                       
      *PVG FIN 25-OCT-2007                                              
           IF DCM-NUM-IDCM IN CCM-FLD NOT > ZEROES
               PERFORM BUS-COR-DCM
           ELSE
              MOVE CCM-IND-DSCT IN CCM TO CCM-IND-DSCT IN CCM-FLD
              MOVE CCM-IND-CAAB IN CCM TO CCM-IND-CAAB IN CCM-FLD

              MOVE CCM-CIC-ICCM IN CCM     TO DCM-CIC-IDCM IN DCM
              MOVE DCM-NUM-IDCM IN CCM-FLD TO DCM-NUM-IDCM IN DCM
              MOVE FIO-GET-KEY TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE FIO-STAT TO PGM-STAT-DCM
              IF FIO-STAT-OKS AND SCR-CMND = 'ELI'
                 MOVE DCM-STP-ITRN IN DCM TO SCR-UMOD.

            IF PGM-STAT-DCM-OKS AND SCR-OFRM-SRV AND SCR-CMND = 'CON'
               PERFORM CRG-VEC-DCM.
            MOVE PGM-STAT-DCM TO FIO-STAT.
       CON-FND-KEY.
           IF MSG-COD-MENS > SPACES
              MOVE '23' TO FIO-STAT
              GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF PUT_SCR
      *<<<< PUT_SCR
           IF PGM-STAT-CCM-OKS
              MOVE DCM-KEY-IDCM IN DCM TO SCR-DCM-KEY-IDCM
              IF SCR-CMND = 'ELI'
               MOVE DCM-STP-ITRN IN DCM TO SCR-UMOD.
      *>>>>
*% END
*% IF SCR_GIKY_VAL_IKY
      *<<<< SCR_GIKY_VAL_IKY
      *>>>>
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF7 )
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 )
      *>>>>
*% END
*% IF INI_CMN_MOD
      *<<<<
           GO TO NXT-CMN-MOD.
      *>>>>
*% END
*% IF CMN_MOD
      *<<<< CMN_MOD
       NXT-CMN-MOD.
           PERFORM KEY-ALL-CCM.                                         
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.                            
           PERFORM MC2-FIO-CCM.                                         
           IF FIO-STAT-OKS AND                                          
              CCM-STP-ITRN IN CCM NOT = SCR-UMOD-CCM                    
               MOVE FIO-ULK-REC TO FIO-CMND                             
               PERFORM MC2-FIO-CCM                                      
               MOVE FIO-BCK-OUT TO FIO-CMND                             
               PERFORM MC2-FIO-CCM                                      
               MOVE 'GNS' TO MSG-COD-SIST                               
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS                      
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               GO TO FIN-CMN-MOD.                                       
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-CMN-MOD.                                       
           PERFORM RET-STT-CCM.                                         
           PERFORM RET-ALL-CCM.                                         
           PERFORM RET-HDR-CCM.                                         
           MOVE 'M' TO CCM-MSC-STAT IN CCM.
      *>>>>
*% END
*% IF FIN_CMN_MOD
      *<<<< FIN_CMN_MOD
           IF NOT FIO-STAT-OKS
              GO TO FIN-CMN-MOD.
           MOVE CCM-CIC-ICCM IN CCM     TO DCM-CIC-IDCM IN DCM.
           MOVE DCM-NUM-IDCM IN CCM-FLD TO DCM-NUM-IDCM IN DCM.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-DCM.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-CCM
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-CCM
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE 'TAB' TO MSG-COD-SIST
              MOVE 'ERRUPD   DCM' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE '23' TO FIO-STAT PGM-STAT-DCM PGM-STAT-DCM
              GO TO FIN-CMN-MOD.

           PERFORM RET-STT-DCM.
           PERFORM RET-DCM-CCM.
           PERFORM RET-HDR-DCM.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM MC2-FIO-DCM.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-CCM
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-CCM
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE '23' TO FIO-STAT PGM-STAT-DCM PGM-STAT-DCM
              GO TO FIN-CMN-MOD.

           MOVE 'TAB'               TO FIO-SIST.
           MOVE 'DPL'               TO TAB-COD-TTAB IN TAB.
           MOVE DCM-CIC-IDCM IN DCM TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-MOD.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
              MOVE '23' TO FIO-STAT PGM-STAT-DCM PGM-STAT-DCM
              GO TO FIN-CMN-MOD.

           MOVE FRM-GLS-DES1 IN CCM-FLD TO TAB-GLS-DESC IN TAB.
           MOVE FRM-GLS-DES2 IN CCM-FLD TO TAB-GLS-DATA IN TAB.
           PERFORM GET-FHOY.
           MOVE HOY-FHOY                 TO TAB-FEC-FTRN IN TAB.
           MOVE HOY-HHOY                 TO TAB-HRA-HRTR IN TAB.
           MOVE SCR-OTRN                 TO TAB-COD-OTRN IN TAB.
           MOVE SCR-USER                 TO TAB-COD-ATRN IN TAB.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
              MOVE '23' TO FIO-STAT PGM-STAT-DCM PGM-STAT-DCM
              GO TO FIN-CMN-MOD.
      *>>>>
*% END
*% IF INI_CMN_ING
      *<<<< INI_CMN_ING
           IF DCM-NUM-IDCM IN CCM-FLD > 1
              MOVE '00' TO FIO-STAT
              GO TO CNT-CMN-ING.
           PERFORM OBT-CIC-CCM.
           IF CIC-STAT NOT = CIC-STAT-OKS
              GO TO FIN-CMN-ING.
           MOVE CIC-CAI-ICIC IN CIC TO CCM-CAI-ICCM IN CCM.
           MOVE CIC-IIC-ICIC IN CIC TO CCM-IIC-ICCM IN CCM.
           MOVE CIC-CIC-ICIC IN CIC TO CCM-CIC-ICCM IN CCM-FLD.
      *>>>>
*% END
*% IF FIN_CMN_ING
      *<<<< FIN_CMN_ING
       CNT-CMN-ING.
           IF NOT FIO-STAT-OKS
              GO TO FIN-CMN-ING.
           PERFORM RET-STT-DCM.
           PERFORM KEY-DCM-CCM.
           MOVE CCM-CIC-ICCM IN CCM-FLD TO DCM-CIC-IDCM IN DCM.
           PERFORM RET-DCM-CCM.
           PERFORM RET-HDR-DCM.
           MOVE FIO-PUT TO FIO-CMND.
           PERFORM MC2-FIO-DCM.
           IF NOT FIO-STAT-OKS
              MOVE FIO-STAT TO PGM-STAT-CCM PGM-STAT-DCM
              GO TO FIN-CMN-ING.

           IF DCM-NUM-IDCM IN CCM-FLD = 1
              MOVE 'TAB'                   TO FIO-SIST
              MOVE 'DPL'                   TO TAB-COD-TTAB IN TAB
              MOVE DCM-CIC-IDCM IN DCM     TO TAB-COD-CTAB IN TAB
              MOVE FRM-GLS-DES1 IN CCM-FLD TO TAB-GLS-DESC IN TAB
              MOVE FRM-GLS-DES2 IN CCM-FLD TO TAB-GLS-DATA IN TAB
              MOVE 'I'                     TO TAB-MSC-TACC IN TAB
              MOVE 'I'                     TO TAB-MSC-STAT IN TAB
              MOVE 'S'                     TO TAB-IND-VIGE IN TAB
              MOVE SPACES                  TO TAB-COD-OTRN IN TAB
              MOVE 'MC2PUCCM'              TO TAB-COD-ATRN IN TAB
              MOVE DCM-CAI-IDCM IN DCM     TO TAB-GLS-DCOR IN TAB
              MOVE DCM-IIC-IDCM IN DCM     TO TAB-GLS-ABRV IN TAB
              PERFORM GET-FHOY
              MOVE HOY-FHOY                 TO TAB-FEC-FTRN IN TAB
              MOVE HOY-HHOY                 TO TAB-HRA-HRTR IN TAB
              MOVE SCR-OTRN                 TO TAB-COD-OTRN IN TAB
              MOVE SCR-USER                 TO TAB-COD-ATRN IN TAB
              MOVE FIO-PUT TO FIO-CMND
              PERFORM GNS-FIO-TAB
              IF NOT FIO-STAT-OKS
                 MOVE 'TAB'               TO MSG-COD-SIST
                 MOVE 'DPL    NOGRB'      TO MSG-COD-MENS
                 PERFORM GET-MSG
                 MOVE MSG-GLS-DESC        TO FRM-MENS
                 GO TO FIN-CMN-ING.

      *MODIFICA ESTADO CCM
           PERFORM KEY-ALL-CCM.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-CCM.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-CCM
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-CCM
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE 'TAB' TO MSG-COD-SIST
              MOVE 'ERRUPDSTACCM' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              GO TO FIN-CMN-ING.
           PERFORM RET-STT-CCM.
           PERFORM RET-HDR-CCM.
           PERFORM RET-ALL-CCM.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM MC2-FIO-CCM.
      *>>>>
*% END
*% IF INI_CMN_ACC
      *<<<< INI_CMN_ACC
           IF SCR-CMND = 'VRF'
               PERFORM PRO-CMN-VRF
               IF MSG-COD-MENS > SPACES
                  GO TO FIN-CMN-ACC.
           GO TO NXT-CMN-ACC.
      *>>>>
*% END
*% IF CMN_ACC
      *<<<< CMN_ACC
       NXT-CMN-ACC.
           PERFORM KEY-ALL-CCM.                                         
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.                            
           PERFORM MC2-FIO-CCM.                                         
           IF FIO-STAT-OKS AND                                          
              CCM-STP-ITRN IN CCM NOT = SCR-UMOD-CCM                    
               MOVE FIO-ULK-REC TO FIO-CMND                             
               PERFORM MC2-FIO-CCM                                      
               MOVE FIO-BCK-OUT TO FIO-CMND                             
               PERFORM MC2-FIO-CCM                                      
               MOVE 'GNS' TO MSG-COD-SIST                               
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS                      
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               GO TO FIN-CMN-ACC.                                       
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-CMN-ACC.                                       
      *    PERFORM RET-ALL-CCM.                                         
           IF SCR-CMND = 'VRF'
               MOVE 'V'          TO CCM-IND-VIGE IN CCM
               MOVE 'VRFYES    ' TO WSS-COD-MENY
               MOVE 'VRFNOT    ' TO WSS-COD-MENN
           ELSE
           IF SCR-CMND = 'ACA'
               MOVE 'A'          TO CCM-IND-VIGE IN CCM
               MOVE 'ACAYES    ' TO WSS-COD-MENY
               MOVE 'ACANOT    ' TO WSS-COD-MENN
           ELSE
           IF SCR-CMND = 'ANL'
               MOVE 'D'          TO CCM-IND-VIGE IN CCM
               MOVE 'ANLYES    ' TO WSS-COD-MENY
               MOVE 'ANLNOT    ' TO WSS-COD-MENN.

           MOVE FIO-MOD TO FIO-CMND.
           PERFORM MC2-FIO-CCM.
           IF NOT FIO-STAT-OKS
               MOVE 'S' TO SCR-IERR
               MOVE 'TAB' TO MSG-COD-SIST
               MOVE WSS-COD-MENN TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE WSS-COD-MENY  TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN1.

           IF SCR-CMND NOT = 'ANL'                                      
              GO TO FIN-CMN-ACC.                                        
           MOVE 'TAB' TO FIO-SIST.                                      
           MOVE 'DPL' TO TAB-COD-TTAB IN TAB.                           
           MOVE CCM-CIC-ICCM IN CCM TO TAB-COD-CTAB IN TAB.             
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM GNS-FIO-TAB.                                         
           IF FIO-STAT-OKS                                              
              MOVE FIO-GET-KEY-UPD TO FIO-CMND                          
              PERFORM GNS-FIO-TAB                                       
              IF FIO-STAT-OKS                                           
                 PERFORM GET-FHOY                                       
                 MOVE HOY-FHOY            TO TAB-FEC-FTRN IN TAB        
                 MOVE HOY-HHOY            TO TAB-HRA-HRTR IN TAB        
                 MOVE SCR-OTRN            TO TAB-COD-OTRN IN TAB        
                 MOVE SCR-USER            TO TAB-COD-ATRN IN TAB        
                 MOVE 'N'      TO TAB-IND-VIGE IN TAB                   
                 MOVE FIO-MOD TO FIO-CMND                               
                 PERFORM GNS-FIO-TAB                                    
                 IF NOT FIO-STAT-OKS                                    
                    MOVE 'ERROR UPD DPL' TO FRM-MEN2                    
                 ELSE                                                   
                    NEXT SENTENCE                                       
              ELSE                                                      
                 MOVE 'ERROR UPD DPL' TO FRM-MEN2                       
           ELSE                                                         
              MOVE 'ERROR UPD DPL' TO FRM-MEN2.                         
      *>>>>
*% END
*% IF INI_CMN_ELI
      *<<<< INI_CMN_ELI
           PERFORM KEY-ALL-CCM.
           GO TO CNT-CMN-ELI.
      *>>>>
*% END
*% IF FIN_CMN_ELI
      *<<<< FIN_CMN_ELI
           MOVE FIO-STAT TO PGM-STAT-CCM.
       CNT-CMN-ELI.
           IF FRM-IND-SGTE IN CCM-FLD = 'S'
              PERFORM ELI-ALL-CCM
              GO TO FIN-CMN-ELI.
           MOVE CCM-CIC-ICCM IN CCM-FLD TO DCM-CIC-IDCM IN DCM.
           MOVE DCM-NUM-IDCM IN CCM-FLD TO DCM-NUM-IDCM IN DCM.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-DCM.
           IF FIO-STAT-OKS AND
              DCM-STP-ITRN IN DCM NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM MC2-FIO-CCM
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-CCM
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE '23' TO FIO-STAT PGM-STAT-DCM PGM-STAT-DCM
               GO TO FIN-CMN-ELI.
           IF NOT FIO-STAT-OKS
              GO TO FIN-CMN-ELI.

           MOVE FIO-DEL TO FIO-CMND.
           PERFORM MC2-FIO-DCM.
           MOVE FIO-STAT TO PGM-STAT-DCM.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE '23' TO FIO-STAT PGM-STAT-DCM PGM-STAT-DCM
              GO TO FIN-CMN-ELI.
           MOVE CCM-CIC-ICCM IN CCM-FLD TO DCM-CIC-IDCM IN DCM.
           MOVE ZEROES                  TO DCM-NUM-IDCM IN DCM.
           MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM MC2-FIO-DCM.
           IF (FIO-STAT-OKS AND
               DCM-CIC-IDCM IN DCM = CCM-CIC-ICCM IN CCM-FLD)
              MOVE PGM-STAT-DCM TO FIO-STAT
              PERFORM MOD-STAT-CCM
              GO TO FIN-CMN-ELI.

           PERFORM KEY-ALL-CCM.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-CCM.
           IF FIO-STAT-OKS AND
              CCM-STP-ITRN IN CCM NOT = SCR-UMOD-CCM
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM MC2-FIO-CCM
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-CCM
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM MC2-FIO-DCM
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-DCM
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-ELI.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ELI.
           MOVE FIO-DEL TO FIO-CMND.
           PERFORM MC2-FIO-CCM.
           MOVE FIO-STAT TO PGM-STAT-CCM.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-CCM
