*% IF EOF
      *<<<< EOF
       COPY GNSBBTAB.
       COPY GNSBVTAB.
       COPY GNSBVCOD.
       COPY TABBFOFI.
       COPY TABBVOFI.
       COPY SGCBFDBC.
       COPY MC2BFDEC.
       COPY TABBFUSR.
       COPY TABBVTDU.
       COPY TABBBUSR.

       PUT-TAB SECTION.
       INI-PUT-TAB.
           MOVE QCN-NOM-TSQCN     TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE QCN-NOM-TSQCN-MAS TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE 1 TO SCR-NUM-PANT.
           MOVE LOW-VALUES TO WSS-GLS-NQCN-IDX-MTI.
           MOVE LOW-VALUES TO WSS-GLS-NQCN-IDX-FRM.
      *
           MOVE 1 TO FRM-IFLD.
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.

           MOVE 1 TO WSS-F IN WSS-VARI.
      *PVG INI 24-JUN-2009
           IF FRM-IND-SGTE IN QCN-FLD = 'A'
              MOVE WSS-J TO WSS-F IN WSS-VARI
              IF WSS-F IN WSS-VARI NOT > ZEROES
                 MOVE 'S'   TO SCR-IERR
                 MOVE 'TAB' TO MSG-COD-SIST
                 MOVE 'QCN    NEX' TO MSG-COD-MENS
                 PERFORM GET-MSG
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 MOVE '00' TO FIO-STAT
                 GO TO FIN-PUT-TAB.
      *PVG FIN 24-JUN-2009
       LUP-PUT-TAB.
      *PVG INI 24-JUN-2009
      *    IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI
           IF ((WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI AND
                FRM-IND-SGTE IN QCN-FLD NOT = 'A')       OR
               (WSS-F IN WSS-VARI < 1                    AND
                FRM-IND-SGTE IN QCN-FLD = 'A'))
      *PVG FIN 24-JUN-2009
              GO TO FIN-PUT-TAB.
           IF WSS-NUM-ICLI-V(WSS-F) > ZEROES
              PERFORM PUT-TAB-QCN
              PERFORM PUT-QCN-QUE
              PERFORM SET-FRM-IFLD
           ELSE
           IF FRM-IFLD = 1 AND WSS-F = 1
              MOVE 'MC2' TO MSG-COD-SIST
              MOVE 'QCN    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE '00' TO FIO-STAT
              MOVE 'S' TO SCR-IERR
              GO TO FIN-PUT-TAB
           ELSE
              MOVE '00' TO FIO-STAT
              GO TO FIN-PUT-TAB.

      *PVG INI 24-JUN-2009
           IF FRM-IND-SGTE IN QCN-FLD = 'A'
               SUBTRACT 1 FROM WSS-F IN WSS-VARI
           ELSE
      *PVG FIN 24-JUN-2009
              ADD 1         TO WSS-F IN WSS-VARI.
           GO TO LUP-PUT-TAB.
       FIN-PUT-TAB.
           MOVE 1                  TO QUE-NITM.
           MOVE QCN-LEN-TSQCN-MAS  TO QUE-LITM.
           MOVE QCN-NOM-TSQCN-MAS  TO QUE-COLA.

           IF WSS-NITM > WSS-NITMMAX
              MOVE 'S'           TO QCN-MAS-IND-HMAS
           ELSE
              MOVE 'N'           TO QCN-MAS-IND-HMAS.

           MOVE WSS-NITM         TO WSS-NUM-ICUR.
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.
           MOVE WSS-NUM-ICUR-ALF TO QCN-MAS-NUM-TOCC.
           MOVE QCN-MAS-ITEM     TO QUE-ITEM.
           MOVE QUE-PUT          TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF WSS-NITM > WSS-IMAX
               MOVE 'MC2' TO MSG-COD-SIST
               MOVE 'QCNTABDMAS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       EXT-PUT-TAB.
           EXIT.

       SET-FRM-IFLD SECTION.
       INI-SET-FRM-IFLD.
           IF SCR-OFRM-SRV
                MOVE 1 TO FRM-IFLD
           ELSE
                ADD 1 TO FRM-IFLD.
       FIN-SET-FRM-IFLD.
           EXIT.
      *--------------------------------------------
       SCRL-TAB-QCN SECTION.
       INI-SCRL-TAB-QCN.
           MOVE SPACES TO MSG-COD-MENS.

           IF FRM-FFLD = FRM-FFLD-PF6
               PERFORM PUT-TAB-ASC
           ELSE
               PERFORM PUT-TAB-DSC.
           IF MSG-COD-MENS > SPACES
               MOVE 'MC2' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-QCN.
           EXIT.

      *--------------------------------------------
       PUT-TAB-QCN SECTION.
       INI-PUT-TAB-QCN.
           PERFORM CLR-WSS.
           MOVE WSS-NUM-ICLI-V(WSS-F) TO WSS-NUM-ICLI-QCN IN WSS-VARI.
           MOVE WSS-VRF-ICLI-V(WSS-F) TO WSS-VRF-ICLI-QCN IN WSS-VARI.
           MOVE WSS-IND-ICLI-V(WSS-F) TO WSS-IND-ICLI-QCN IN WSS-VARI.
           MOVE WSS-GLS-ICLI-V(WSS-F) TO WSS-GLS-ICLI-QCN IN WSS-VARI.
           MOVE WSS-GLS-RSOC-V(WSS-F) TO WSS-GLS-RSOC-QCN IN WSS-VARI.
           MOVE WSS-COD-OFIC-V(WSS-F) TO WSS-COD-OFIC-QCN IN WSS-VARI.
           MOVE WSS-FEC-FAPE-V(WSS-F) TO WSS-FEC-FAPE-QCN IN WSS-VARI.
           MOVE WSS-FEC-FCTR-V(WSS-F) TO WSS-FEC-FCTR-QCN IN WSS-VARI.
           MOVE WSS-CAI-ICVN-V(WSS-F) TO WSS-CAI-ICVN-QCN IN WSS-VARI.
           MOVE WSS-IIC-ICVN-V(WSS-F) TO WSS-IIC-ICVN-QCN IN WSS-VARI.
           MOVE WSS-GLS-DESC-V(WSS-F) TO WSS-GLS-DESC-QCN IN WSS-VARI.
           MOVE WSS-IND-SCVN-V(WSS-F) TO WSS-IND-SCVN-QCN IN WSS-VARI.
           MOVE WSS-COD-TIPO-V(WSS-F) TO WSS-COD-TIPO-QCN IN WSS-VARI.
           MOVE WSS-COD-STIP-V(WSS-F) TO WSS-COD-STIP-QCN IN WSS-VARI.
           MOVE WSS-FEC-FCVN-V(WSS-F) TO WSS-FEC-FCVN-QCN IN WSS-VARI.
           MOVE WSS-HRA-HRCV-V(WSS-F) TO WSS-HRA-HRCV-QCN IN WSS-VARI.


           MOVE WSS-NUM-ICLI-QCN IN WSS-VARI TO WSS-NUM-ICLI-FRM.
           MOVE WSS-VRF-ICLI-QCN IN WSS-VARI TO WSS-VRF-ICLI-FRM.
           MOVE WSS-IND-ICLI-QCN IN WSS-VARI TO WSS-IND-ICLI-FRM.
           MOVE WSS-GLS-ICLI-QCN IN WSS-VARI TO WSS-GLS-ICLI-FRM.
           MOVE WSS-GLS-RSOC-QCN IN WSS-VARI TO WSS-GLS-RSOC-FRM.
           MOVE WSS-COD-OFIC-QCN IN WSS-VARI TO WSS-COD-OFIC-FRM.
           MOVE WSS-FEC-FAPE-QCN IN WSS-VARI TO WSS-FEC-FAPE-FRM.
           MOVE WSS-FEC-FCTR-QCN IN WSS-VARI TO WSS-FEC-FCTR-FRM.
           MOVE WSS-CAI-ICVN-QCN IN WSS-VARI TO WSS-CAI-ICVN-FRM.
           MOVE WSS-IIC-ICVN-QCN IN WSS-VARI TO WSS-IIC-ICVN-FRM.
           MOVE WSS-GLS-DESC-QCN IN WSS-VARI TO WSS-GLS-DESC-FRM.
           MOVE WSS-IND-SCVN-QCN IN WSS-VARI TO WSS-IND-SCVN-FRM.
           MOVE WSS-COD-TIPO-QCN IN WSS-VARI TO WSS-COD-TIPO-FRM.
           MOVE WSS-COD-STIP-QCN IN WSS-VARI TO WSS-COD-STIP-FRM.
           MOVE WSS-FEC-FCVN-QCN IN WSS-VARI TO WSS-FEC-FCVN-FRM.
           MOVE WSS-HRA-HRCV-QCN IN WSS-VARI TO WSS-HRA-HRCV-FRM.

      *PVG INI 24-JUN-2009
           IF FRM-IND-SGTE IN QCN-FLD = 'A' AND
              WSS-F IN WSS-VARI = WSS-IMAX
              MOVE 1 TO FRM-IFLD.
      *PVG FIN 24-JUN-2009
           IF FRM-IFLD > WSS-IMAX
              GO TO FIN-PUT-TAB-QCN.

           MOVE WSS-NUM-ICLI-QCN IN WSS-VARI TO 
               FRM-NUM-ICLI IN QCN-FLD(FRM-IFLD).
           MOVE WSS-VRF-ICLI-QCN IN WSS-VARI TO 
               FRM-VRF-ICLI IN QCN-FLD(FRM-IFLD).
           MOVE WSS-IND-ICLI-QCN IN WSS-VARI TO 
               FRM-IND-ICLI IN QCN-FLD(FRM-IFLD).
           MOVE WSS-GLS-ICLI-QCN IN WSS-VARI TO 
               FRM-GLS-ICLI IN QCN-FLD(FRM-IFLD).
           MOVE WSS-GLS-RSOC-QCN IN WSS-VARI TO 
