*%! Codigo adicional
*% NO_TIENE_TIMESTAMP = TRUE
*% NO_HDR_CST = TRUE
*% LARGO_TOTAL_COMMAREA = 3000
*% FMT_CHI = TRUE
*% GEN_BCI = TRUE
*% PGM_PTC = TRUE
*% IF SETTAG
*% VSNPQ = "05.12.20.1"
*% FNCPQ = "MC2-CONVENIO"
*% END
*% IF CMT_ID
      *<<<<
      *                                                                 
      *-----------------------------------------------------------------
      * OBJETIVO PROGRAMA   : CONSULTA MASIVA DE RELACION ENTRE PERSONAS
      *                       (CLIENTE(S) ASOCIADO(S) A UNA EMPRESA)    
      * ENTIDAD DE CONSULTA      : REC DE SGC                           
      *                          : DBC DE SGC                           
      * ENTIDAD DE ACTUALIZACION : NO HAY                               
      *-----------------------------------------------------------------
      * MANTENCION                                                      
      * ==========                                                      
      * PROYECTO   : PRC; DESCUENTOS POR PLANILLA; REL.ENTRE CLIENTES.  
      * OBJETIVO   : 1.- MODIFICAR LA PANTALLA MC2 QRE PARA QUE HAGA LA 
      *                  LECTURA DE REGISTROS ANTERIORES A PARTIR DE UNO
      *                  INFORMADO.                                     
      *              2.- INVERTIR EL ORDEN A LISTAR REGISTROS           
      *                  ANTERIORES.                                    
      * FECHA      : 20-ABRIL-2009.                                     
      * AUTOR      : PVC.                                               
      *-----------------------------------------------------------------
      *>>>>
*% END      
*% IF WSS
      *<<<< WSS
       COPY MC2WXQRE.
       COPY GNSWGCPT.
       COPY GNSWGSTR.
*% IF NOT PGM_MCP AND NOT PGM_PTC
       COPY GNSWGQUE.
*% END
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
       01 WSS-NUM-CCUR.
           03 WSS-NUM-ICUR-EDT  PIC  ZZ9.
           03 WSS-NUM-ICUR-ALF       PIC X(03).
           03 WSS-NUM-ICUR      REDEFINES
              WSS-NUM-ICUR-ALF       PIC 9(03).

       COPY SGCBRDBC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
      *>>>>
*% END
*% IF FIN_SCR_VARI
           03 SCR-NUM-PANT               PIC 9(0005).
           03 SCR-QRE-FLD                PIC X(1100).
           03 SCR-CIC-ICLI.
              05 SCR-CAI-ICLI            PIC X(04).
              05 SCR-IIC-ICLI            PIC X(08).
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-OFRM-SRV
                MOVE TSK-TERM-ALF TO QRE-COD-TERM
                MOVE TSK-TERM-ALF TO QRE-COD-TERM-MAS
           ELSE
                MOVE EIBTRMID     TO QRE-COD-TERM
                MOVE EIBTRMID     TO QRE-COD-TERM-MAS.
           MOVE +3859 TO SCR-TCMA.
           PERFORM GNS-ERR-QUE.
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )
               MOVE SCR-QRE-FLD TO QRE-FLD
               GO TO FIN-INI.

           IF ( FRM-FFLD = FRM-FFLD-PF6 )
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM
               ADD 1              TO QUE-NITM
               MOVE QRE-NOM-TSQRE TO QUE-COLA
               MOVE QRE-LEN-TSQRE TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT NOT = QUE-STAT-OKS
                  MOVE SCR-QRE-FLD TO QRE-FLD
                  GO TO FIN-INI.


      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
           MOVE SCR-CAI-ICLI IN SCR-VARI TO REC-CAI-ICLI IN REC.       
           MOVE SCR-IIC-ICLI IN SCR-VARI TO REC-IIC-ICLI IN REC.       
      *PVC-INI 20-ABR-2009 - LECTURA REGISTROS ANTERIORES               
           MOVE SPACES     TO WSS-CAI-IREL IN WSS-VARI                  
                              WSS-IIC-IREL IN WSS-VARI.                 
      *    IF FRM-IND-SGTE IN QRE-FLD = 'S'                             
           IF FRM-IND-SGTE IN QRE-FLD = 'S' OR 'A'                      
               MOVE FRM-COD-TREK IN QRE-FLD TO REC-COD-TREC IN REC
               PERFORM OBT-CIC-ICLI
               MOVE WSS-CAI-ICLI IN WSS-VARI TO REC-CAI-IREL IN REC
               MOVE WSS-IIC-ICLI IN WSS-VARI TO REC-IIC-IREL IN REC
               MOVE REC-CAI-IREL IN REC TO WSS-CAI-IREL IN WSS-VARI     
               MOVE REC-IIC-IREL IN REC TO WSS-IIC-IREL IN WSS-VARI.    
           IF FRM-IND-SGTE IN QRE-FLD = 'A'                             
               MOVE FIO-GET-LEQ TO FIO-CMND                             
               GO TO LUP-LEE-REC.                                       
      *PVC-FIN 20-ABR-2009 - LECTURA REGISTROS ANTERIORES               

           MOVE FIO-GET-NLS TO FIO-CMND.
       LUP-LEE-REC.
           PERFORM SGC-FIO-REC.
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF
               GO TO CON-FND-KEY.

           IF NOT (SCR-CAI-ICLI IN SCR-VARI = REC-CAI-ICLI IN REC AND
              SCR-IIC-ICLI IN SCR-VARI = REC-IIC-ICLI IN REC)
               GO TO CON-FND-KEY.

           IF (FRM-IND-SGTE IN QRE-FLD = 'S' OR 'A') AND                
              REC-CAI-IREL IN REC = WSS-CAI-IREL IN WSS-VARI AND        
              REC-IIC-IREL IN REC = WSS-IIC-IREL IN WSS-VARI            
               GO TO NXT-LEE-REC.                                       
                                                                        
           MOVE SPACES              TO WSS-COD-SGMT IN WSS-VARI.
           MOVE REC-COD-TREC IN REC TO WSS-COD-TREC IN WSS-VARI.
           MOVE REC-COD-AREC IN REC TO WSS-COD-AREC IN WSS-VARI.
           MOVE REC-COD-SARC IN REC TO WSS-COD-SARC IN WSS-VARI.

           IF REC-COD-TREC IN QRE-FLD > SPACES AND
              REC-COD-TREC IN QRE-FLD NOT = WSS-COD-SGMT IN WSS-VARI
               GO TO NXT-LEE-REC.

           IF FRM-FEC-FIND IN QRE-FLD > ZEROES           
               MOVE FRM-NUM-DIND IN QRE-FLD TO WSS-NUM-DINI IN WSS-VARI
               MOVE FRM-NUM-MIND IN QRE-FLD TO WSS-NUM-MINI IN WSS-VARI
               MOVE FRM-NUM-SIND IN QRE-FLD TO WSS-NUM-SINI IN WSS-VARI
               MOVE FRM-NUM-AIND IN QRE-FLD TO WSS-NUM-AINI IN WSS-VARI
               IF WSS-FEC-FINI IN WSS-VARI > REC-FEC-FINI IN REC
                   GO TO NXT-LEE-REC.

           IF FRM-FEC-FINH IN QRE-FLD > ZEROES           
               MOVE FRM-NUM-DINH IN QRE-FLD TO WSS-NUM-DINI IN WSS-VARI
               MOVE FRM-NUM-MINH IN QRE-FLD TO WSS-NUM-MINI IN WSS-VARI
               MOVE FRM-NUM-SINH IN QRE-FLD TO WSS-NUM-SINI IN WSS-VARI
               MOVE FRM-NUM-AINH IN QRE-FLD TO WSS-NUM-AINI IN WSS-VARI
               IF WSS-FEC-FINI IN WSS-VARI < REC-FEC-FINI IN REC
                   GO TO NXT-LEE-REC.

           IF FRM-FEC-FTED IN QRE-FLD > ZEROES           
               MOVE FRM-NUM-DTED IN QRE-FLD TO WSS-NUM-DTER IN WSS-VARI
               MOVE FRM-NUM-MTED IN QRE-FLD TO WSS-NUM-MTER IN WSS-VARI
               MOVE FRM-NUM-STED IN QRE-FLD TO WSS-NUM-STER IN WSS-VARI
               MOVE FRM-NUM-ATED IN QRE-FLD TO WSS-NUM-ATER IN WSS-VARI
               IF WSS-FEC-FTER IN WSS-VARI > REC-FEC-FTER IN REC
                   GO TO NXT-LEE-REC.

           IF FRM-FEC-FTEH IN QRE-FLD > ZEROES           
               MOVE FRM-NUM-DTEH IN QRE-FLD TO WSS-NUM-DTER IN WSS-VARI
               MOVE FRM-NUM-MTEH IN QRE-FLD TO WSS-NUM-MTER IN WSS-VARI
               MOVE FRM-NUM-STEH IN QRE-FLD TO WSS-NUM-STER IN WSS-VARI
               MOVE FRM-NUM-ATEH IN QRE-FLD TO WSS-NUM-ATER IN WSS-VARI
               IF WSS-FEC-FTER IN WSS-VARI < REC-FEC-FTER IN REC
                   GO TO NXT-LEE-REC.

           PERFORM CRG-VEC-QRE.

           IF (SCR-OFRM-SRV AND WSS-J NOT < WSS-NITMMAX)
              GO TO CON-FND-KEY.

           IF ((NOT SCR-OFRM-SRV) AND WSS-J > WSS-JMAX)
                 GO TO CON-FND-KEY.

       NXT-LEE-REC.
      *PVC-INI 20-ABR-2009 - LECTURA REGISTROS ANTERIORES               
      *    MOVE FIO-GET-NXT TO FIO-CMND.                                
           IF FRM-IND-SGTE IN QRE-FLD = 'A'                             
               MOVE FIO-GET-PRV TO FIO-CMND                             
           ELSE                                                         
               MOVE FIO-GET-NXT TO FIO-CMND.                            
      *PVC-FIN 20-ABR-2009 - LECTURA REGISTROS ANTERIORES               
           GO TO LUP-LEE-REC.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
       CON-FND-KEY.
           PERFORM PUT-TAB.

           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *----------------------------------------------------------
       MSG-FND-KEY.
           MOVE 'TAB' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM VAL-NUM-KEY-QRE
               PERFORM VAL-NUM-FLD-QRE
               PERFORM SCRL-TAB-QRE
               PERFORM SCR-TRL.
      *>>>>
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBBTAB.
       COPY GNSBVTAB.
       COPY GNSBVCOD.
       COPY GNSBEGLS.
       COPY SGCBFDBC.

       PUT-TAB SECTION.
       INI-PUT-TAB.
           MOVE QRE-NOM-TSQRE     TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE QRE-NOM-TSQRE-MAS TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE 1 TO SCR-NUM-PANT.
           MOVE LOW-VALUES TO WSS-GLS-NQRE-IDX-MTI.
           MOVE LOW-VALUES TO WSS-GLS-NQRE-IDX-FRM.
      *
           MOVE 1 TO FRM-IFLD.
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.

           MOVE 1 TO WSS-F IN WSS-VARI.
      *PVC-INI 21-ABR-2009 - INVERTIR ORDEN A LISTAR REG ANTERIORES     
           IF FRM-IND-SGTE IN QRE-FLD = 'A'                             
              MOVE WSS-J    TO WSS-F IN WSS-VARI                        
              IF WSS-F IN WSS-VARI NOT > ZEROES                         
                 MOVE 'S'   TO SCR-IERR                                 
                 MOVE 'TAB' TO MSG-COD-SIST                             
                 MOVE 'QRE    NEX' TO MSG-COD-MENS                      
                 PERFORM GET-MSG                                        
                 MOVE MSG-GLS-DESC TO FRM-MENS                          
                 MOVE '00' TO FIO-STAT                                  
                 GO TO FIN-PUT-TAB.                                     
      *PVC-FIN 21-ABR-2009 - INVERTIR ORDEN A LISTAR REG ANTERIORES     
       LUP-PUT-TAB.
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI
      *PVC-INI 21-ABR-2009 - INVERTIR ORDEN A LISTAR REG ANTERIORES     
              AND FRM-IND-SGTE IN QRE-FLD NOT = 'A'                     
      *PVC-FIN 21-ABR-2009 - INVERTIR ORDEN A LISTAR REG ANTERIORES     
              GO TO FIN-PUT-TAB.                                        
      *PVC-INI 21-ABR-2009 - INVERTIR ORDEN A LISTAR REG ANTERIORES     
           IF WSS-F IN WSS-VARI < 1 AND FRM-IND-SGTE IN QRE-FLD = 'A'   
              GO TO FIN-PUT-TAB.
      *PVC-FIN 21-ABR-2009 - INVERTIR ORDEN A LISTAR REG ANTERIORES     
           IF WSS-COD-TREC-V(WSS-F) > SPACES
              PERFORM PUT-TAB-QRE
              PERFORM PUT-QRE-QUE
              PERFORM SET-FRM-IFLD
           ELSE
           IF FRM-IFLD = 1 AND WSS-F = 1
              MOVE 'TAB' TO MSG-COD-SIST
              MOVE 'QRE    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE '00' TO FIO-STAT
              MOVE 'S' TO SCR-IERR
              GO TO FIN-PUT-TAB
           ELSE
              MOVE '00' TO FIO-STAT
              GO TO FIN-PUT-TAB.

      *PVC-INI 21-ABR-2009 - INVERTIR ORDEN A LISTAR REG ANTERIORES     
           IF FRM-IND-SGTE IN QRE-FLD = 'A'                             
               SUBTRACT 1 FROM WSS-F IN WSS-VARI                        
           ELSE                                                         
      *PVC-FIN 21-ABR-2009 - INVERTIR ORDEN A LISTAR REG ANTERIORES     
               ADD 1 TO WSS-F IN WSS-VARI.
           GO TO LUP-PUT-TAB.
       FIN-PUT-TAB.
           MOVE 1                  TO QUE-NITM.
           MOVE QRE-LEN-TSQRE-MAS  TO QUE-LITM.
           MOVE QRE-NOM-TSQRE-MAS  TO QUE-COLA.

           IF WSS-NITM > WSS-NITMMAX
              MOVE 'S'           TO QRE-MAS-IND-HMAS
           ELSE
              MOVE 'N'           TO QRE-MAS-IND-HMAS.

           MOVE WSS-NITM         TO WSS-NUM-ICUR.
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.
           MOVE WSS-NUM-ICUR-ALF TO QRE-MAS-NUM-TOCC.
           MOVE QRE-MAS-ITEM     TO QUE-ITEM.
           MOVE QUE-PUT          TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF WSS-NITM > WSS-IMAX
               MOVE 'TAB' TO MSG-COD-SIST
               MOVE 'QRETABDMAS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       EXT-PUT-TAB.
           EXIT.

       SET-FRM-IFLD SECTION.
       INI-SET-FRM-IFLD.
           IF SCR-OFRM-SRV
                MOVE 1 TO FRM-IFLD
           ELSE
                ADD 1 TO FRM-IFLD.
       FIN-SET-FRM-IFLD.
           EXIT.
      *--------------------------------------------
       SCRL-TAB-QRE SECTION.
       INI-SCRL-TAB-QRE.
           MOVE SPACES TO MSG-COD-MENS.

           IF FRM-FFLD = FRM-FFLD-PF6
               PERFORM PUT-TAB-ASC
           ELSE
               PERFORM PUT-TAB-DSC.
           IF MSG-COD-MENS > SPACES
               MOVE 'TAB' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-QRE.
           EXIT.

      *--------------------------------------------
       PUT-TAB-QRE SECTION.
       INI-PUT-TAB-QRE.
           PERFORM CLR-WSS.
           MOVE WSS-COD-TREC-V(WSS-F) TO WSS-COD-TREC-QRE IN WSS-VARI.
           MOVE WSS-NUM-ICLI-V(WSS-F) TO WSS-NUM-ICLI-QRE IN WSS-VARI.
           MOVE WSS-VRF-ICLI-V(WSS-F) TO WSS-VRF-ICLI-QRE IN WSS-VARI.
           MOVE WSS-IND-ICLI-V(WSS-F) TO WSS-IND-ICLI-QRE IN WSS-VARI.
           MOVE WSS-GLS-ICLI-V(WSS-F) TO WSS-GLS-ICLI-QRE IN WSS-VARI.
           MOVE WSS-GLS-NOMC-V(WSS-F) TO WSS-GLS-NOMC-QRE IN WSS-VARI.
           MOVE WSS-IND-VIGE-V(WSS-F) TO WSS-IND-VIGE-QRE IN WSS-VARI.
           MOVE WSS-FEC-FINI-V(WSS-F) TO WSS-FEC-FINI-QRE IN WSS-VARI.
           MOVE WSS-FEC-FTER-V(WSS-F) TO WSS-FEC-FTER-QRE IN WSS-VARI.

           MOVE WSS-COD-TREC-QRE IN WSS-VARI TO WSS-COD-TREC-FRM.
           MOVE WSS-NUM-ICLI-QRE IN WSS-VARI TO WSS-NUM-ICLI-FRM.
           MOVE WSS-VRF-ICLI-QRE IN WSS-VARI TO WSS-VRF-ICLI-FRM.
           MOVE WSS-IND-ICLI-QRE IN WSS-VARI TO WSS-IND-ICLI-FRM.
           MOVE WSS-GLS-ICLI-QRE IN WSS-VARI TO WSS-GLS-ICLI-FRM.
           MOVE WSS-GLS-NOMC-QRE IN WSS-VARI TO WSS-GLS-NOMC-FRM.
           MOVE WSS-IND-VIGE-QRE IN WSS-VARI TO WSS-IND-VIGE-FRM.
           MOVE WSS-FEC-FINI-QRE IN WSS-VARI TO WSS-FEC-FINI-FRM.
           MOVE WSS-FEC-FTER-QRE IN WSS-VARI TO WSS-FEC-FTER-FRM.

      *PVC-INI 21-ABR-2009 - INVERTIR ORDEN A LISTAR REG ANTERIORES     
           IF FRM-IND-SGTE IN QRE-FLD = 'A' AND                         
              WSS-F IN WSS-VARI = WSS-IMAX                              
               MOVE 1 TO FRM-IFLD.                                      
      *PVC-FIN 21-ABR-2009 - INVERTIR ORDEN A LISTAR REG ANTERIORES     
           IF FRM-IFLD > WSS-IMAX
              GO TO FIN-PUT-TAB-QRE.

           MOVE WSS-COD-TREC-QRE IN WSS-VARI TO 
               FRM-COD-TREC IN QRE-FLD(FRM-IFLD).
           MOVE WSS-NUM-ICLI-QRE IN WSS-VARI TO 
               FRM-NUM-ICLI IN QRE-FLD(FRM-IFLD).
           MOVE WSS-VRF-ICLI-QRE IN WSS-VARI TO 
               FRM-VRF-ICLI IN QRE-FLD(FRM-IFLD).
           MOVE WSS-IND-ICLI-QRE IN WSS-VARI TO 
               FRM-IND-ICLI IN QRE-FLD(FRM-IFLD).
           MOVE WSS-GLS-ICLI-QRE IN WSS-VARI TO 
               FRM-GLS-ICLI IN QRE-FLD(FRM-IFLD).
           MOVE WSS-GLS-NOMC-QRE IN WSS-VARI TO 
               FRM-GLS-NOMC IN QRE-FLD(FRM-IFLD).
           MOVE WSS-IND-VIGE-QRE IN WSS-VARI TO 
               FRM-IND-VIGE IN QRE-FLD(FRM-IFLD).
           MOVE WSS-FEC-FINI-QRE IN WSS-VARI TO 
               FRM-FEC-FINI IN QRE-FLD(FRM-IFLD).
           MOVE WSS-FEC-FTER-QRE IN WSS-VARI TO 
               FRM-FEC-FTER IN QRE-FLD(FRM-IFLD).
       FIN-PUT-TAB-QRE.
           EXIT.
      *----------------------------------------------------
       PUT-QRE-QUE SECTION.
       INI-PUT-QRE-QUE.
           PERFORM PRO-FLD-QUE.
           ADD 1 TO WSS-NITM.
           MOVE QRE-NOM-TSQRE    TO QUE-COLA.
           MOVE QRE-LEN-TSQRE    TO QUE-LITM.
           MOVE WSS-NITM         TO QUE-NITM.
           MOVE SPACES           TO QUE-ITEM.
           IF SCR-OFRM-SRV
              MOVE WSS-GLS-NQRE-IDX-MTI  TO QUE-ITEM
           ELSE
              MOVE WSS-GLS-NQRE-IDX-FRM  TO QUE-ITEM.
           MOVE QUE-PUT  TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
       FIN-PUT-QRE-QUE.
           EXIT.
      *----------------------------------------------------

       PUT-TAB-ASC SECTION.
       INI-PUT-TAB-ASC.
      *PVG INI 24-JUN-2009                                              
           IF FRM-IND-SGTE IN QRE-FLD = 'A'                             
              MOVE 'FPF    INV' TO MSG-COD-MENS                         
              GO TO EXT-PUT-TAB-ASC.                                    
      *PVG FIN 24-JUN-2009                                              
           MOVE SPACES TO MSG-COD-MENS.
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.
           MOVE 0 TO FRM-IFLD.
       LUP-PUT-TAB-ASC.
           ADD 1              TO QUE-NITM.
           MOVE QRE-NOM-TSQRE TO QUE-COLA.
           MOVE QRE-LEN-TSQRE TO QUE-LITM.
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD = 0
                   MOVE 'QRETABNONXT' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-ASC
               ELSE
                   GO TO FIN-PUT-TAB-ASC.
           ADD 1 TO FRM-IFLD.
           MOVE QUE-ITEM TO FRM-COD-TREC-IDX(FRM-IFLD).
           IF FRM-IFLD < WSS-IMAX
               GO TO LUP-PUT-TAB-ASC.
       FIN-PUT-TAB-ASC.
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX
               ADD 1 FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1
                                 UNTIL WSS-I > WSS-IMAX.
           IF FRM-IFLD > 0
               ADD 1 TO SCR-NUM-PANT.
           IF FRM-IFLD = WSS-IMAX
               ADD 1              TO QUE-NITM
               MOVE QRE-NOM-TSQRE TO QUE-COLA
               MOVE QRE-LEN-TSQRE TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT = QUE-STAT-OKS
                   MOVE 'TAB'        TO MSG-COD-SIST
                   MOVE 'QRETABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-ASC.
           EXIT.

       PUT-TAB-DSC SECTION.
       INI-PUT-TAB-DSC.
      *PVG INI 24-JUN-2009                                              
           IF FRM-IND-SGTE IN QRE-FLD = 'A'                             
              MOVE 'FPF    INV' TO MSG-COD-MENS                         
              GO TO EXT-PUT-TAB-DSC.                                    
      *PVG FIN 24-JUN-2009                                              
           MOVE SPACES TO MSG-COD-MENS.
           IF SCR-NUM-PANT = 1
               MOVE 'QRETABNOPRV' TO MSG-COD-MENS
               GO TO EXT-PUT-TAB-DSC.

           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.
           ADD 1 TO QUE-NITM.
           ADD 1 WSS-IMAX GIVING FRM-IFLD.
       LUP-PUT-TAB-DSC.
           SUBTRACT  1 FROM QUE-NITM.
           MOVE QRE-NOM-TSQRE TO QUE-COLA
           MOVE QRE-LEN-TSQRE TO QUE-LITM
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD > WSS-IMAX
                   MOVE 'QRETABNOPRV' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-DSC
               ELSE
                   GO TO FIN-PUT-TAB-DSC.
           SUBTRACT 1 FROM FRM-IFLD.
           MOVE QUE-ITEM TO FRM-COD-TREC-IDX(FRM-IFLD).
           IF FRM-IFLD > 1
               GO TO LUP-PUT-TAB-DSC.
       FIN-PUT-TAB-DSC.

           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1
                                 UNTIL WSS-I < 1.

           IF FRM-IFLD NOT > WSS-IMAX
               SUBTRACT 1 FROM SCR-NUM-PANT
               MOVE 'TAB'        TO MSG-COD-SIST
               MOVE 'QRETABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-DSC.
           EXIT.

       COPY GNSBGCPT.
       COPY GNSBGSTR.

       CLR-FRM SECTION.
       INI-CLR-FRM.
           MOVE SPACES TO FRM-COD-TREC IN QRE-FLD(WSS-I).
           MOVE ZEROES TO FRM-NUM-ICLI IN QRE-FLD(WSS-I).
           MOVE SPACES TO FRM-VRF-ICLI IN QRE-FLD(WSS-I).
           MOVE SPACES TO FRM-IND-ICLI IN QRE-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-ICLI IN QRE-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-NOMC IN QRE-FLD(WSS-I).
           MOVE SPACES TO FRM-IND-VIGE IN QRE-FLD(WSS-I).
           MOVE SPACES TO FRM-FEC-FINI IN QRE-FLD(WSS-I).
           MOVE SPACES TO FRM-FEC-FTER IN QRE-FLD(WSS-I).
       FIN-CLR-FRM.
           EXIT.

       CLR-WSS SECTION.
       INI-CLR-WSS.
           MOVE SPACES TO WSS-COD-TREC-QRE IN WSS-VARI.
           MOVE ZEROES TO WSS-NUM-ICLI-QRE IN WSS-VARI.
           MOVE SPACES TO WSS-VRF-ICLI-QRE IN WSS-VARI.
           MOVE SPACES TO WSS-IND-ICLI-QRE IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-ICLI-QRE IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-NOMC-QRE IN WSS-VARI.
           MOVE SPACES TO WSS-IND-VIGE-QRE IN WSS-VARI.
           MOVE SPACES TO WSS-FEC-FINI-QRE IN WSS-VARI.
           MOVE SPACES TO WSS-FEC-FTER-QRE IN WSS-VARI.

           IF SCR-OFRM-SRV
               PERFORM CLR-FRM-MTX.
       FIN-CLR-WSS.
           EXIT.

       CLR-FRM-MTX SECTION.
       INI-CLR-FRM-MTX.
           MOVE SPACES TO FRM-COD-TREC IN QRE-FLD(1).
           MOVE ZEROES TO FRM-NUM-ICLI IN QRE-FLD(1).
           MOVE SPACES TO FRM-VRF-ICLI IN QRE-FLD(1).
           MOVE SPACES TO FRM-IND-ICLI IN QRE-FLD(1).
           MOVE SPACES TO FRM-GLS-ICLI IN QRE-FLD(1).
           MOVE SPACES TO FRM-GLS-NOMC IN QRE-FLD(1).
           MOVE SPACES TO FRM-IND-VIGE IN QRE-FLD(1).
           MOVE SPACES TO FRM-FEC-FINI IN QRE-FLD(1).
           MOVE SPACES TO FRM-FEC-FTER IN QRE-FLD(1).
       FIN-CLR-FRM-MTX.
           EXIT.

       CRG-VEC-QRE SECTION.
       INI-CRG-VEC-QRE.
           ADD 1 TO WSS-J.
           IF WSS-J IN WSS-VARI > WSS-JMAX IN WSS-VARI
               GO TO FIN-CRG-VEC-QRE.

           PERFORM CLR-QRE-VEC.
           MOVE WSS-COD-SGMT IN WSS-VARI TO
                             WSS-COD-TREC-V IN WSS-VARI(WSS-J).
           MOVE REC-CAI-IREL IN REC TO DBC-CAI-ICLI IN DBC.
           MOVE REC-IIC-IREL IN REC TO DBC-IIC-ICLI IN DBC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           IF FIO-STAT-OKS
               MOVE DBC-NUM-ICLI IN DBC TO 
                             WSS-NUM-ICLI-V IN WSS-VARI(WSS-J)
               MOVE DBC-GLS-ICLI IN DBC TO 
                             WSS-GLS-ICLI-V IN WSS-VARI(WSS-J)
               MOVE DBC-IND-ICLI IN DBC TO 
                             WSS-IND-ICLI-V IN WSS-VARI(WSS-J)
               MOVE DBC-VRF-ICLI IN DBC TO 
                             WSS-VRF-ICLI-V IN WSS-VARI(WSS-J)
               MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
               PERFORM CPT-BLKS
               MOVE CPT-STRN TO 
                             WSS-GLS-NOMC-V IN WSS-VARI(WSS-J).
           MOVE REC-NUM-SINI IN REC TO WSS-DMA-SINI IN WSS-VARI.
           MOVE REC-NUM-AINI IN REC TO WSS-DMA-AINI IN WSS-VARI.
           MOVE REC-NUM-MINI IN REC TO WSS-DMA-MINI IN WSS-VARI.
           MOVE REC-NUM-DINI IN REC TO WSS-DMA-DINI IN WSS-VARI.
           MOVE WSS-DMA-FINI IN WSS-VARI TO
                             WSS-FEC-FINI-V IN WSS-VARI(WSS-J).

           MOVE REC-NUM-STER IN REC TO WSS-DMA-STER IN WSS-VARI.
           MOVE REC-NUM-ATER IN REC TO WSS-DMA-ATER IN WSS-VARI.
           MOVE REC-NUM-MTER IN REC TO WSS-DMA-MTER IN WSS-VARI.
           MOVE REC-NUM-DTER IN REC TO WSS-DMA-DTER IN WSS-VARI.
           MOVE WSS-DMA-FTER IN WSS-VARI TO
                             WSS-FEC-FTER-V IN WSS-VARI(WSS-J).
           MOVE REC-IND-VIGE IN REC TO
                             WSS-IND-VIGE-V IN WSS-VARI(WSS-J).
       FIN-CRG-VEC-QRE.
           EXIT.

       CLR-QRE-VEC SECTION.
       INI-CLR-QRE-VEC.
           MOVE SPACES TO WSS-COD-TREC-V(WSS-I).
           MOVE ZEROES TO WSS-NUM-ICLI-V(WSS-I).
           MOVE SPACES TO WSS-VRF-ICLI-V(WSS-I).
           MOVE SPACES TO WSS-IND-ICLI-V(WSS-I).
           MOVE SPACES TO WSS-GLS-ICLI-V(WSS-I).
           MOVE SPACES TO WSS-GLS-NOMC-V(WSS-I).
           MOVE SPACES TO WSS-IND-VIGE-V(WSS-I).
           MOVE SPACES TO WSS-FEC-FINI-V(WSS-I).
           MOVE SPACES TO WSS-FEC-FTER-V(WSS-I).
       FIN-CLR-QRE-VEC.
           EXIT.

       PRO-FLD-QUE SECTION.
       INI-PRO-FLD-QUE.
           MOVE 'P' TO WSS-COD-TREC-ATR.
           MOVE 'P' TO WSS-NUM-ICLI-ATR.
           MOVE 'P' TO WSS-VRF-ICLI-ATR.
           MOVE 'P' TO WSS-IND-ICLI-ATR.
           MOVE 'P' TO WSS-GLS-ICLI-ATR.
           MOVE 'P' TO WSS-GLS-NOMC-ATR.
           MOVE 'P' TO WSS-IND-VIGE-ATR.
           MOVE 'P' TO WSS-FEC-FINI-ATR.
           MOVE 'P' TO WSS-FEC-FTER-ATR.
       FIN-PRO-FLD-QUE.
           EXIT.

       OBT-CIC-ICLI SECTION.
       INI-OBT-CIC-ICLI.
           MOVE FRM-NUM-ICLY IN QRE-FLD TO DBC-NUM-ICLI IN DBC
           MOVE FRM-IND-ICLY IN QRE-FLD TO DBC-IND-ICLI IN DBC
           MOVE FRM-GLS-ICLY IN QRE-FLD TO DBC-GLS-ICLI IN DBC
           MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY
           MOVE FIO-GET-KEY TO FIO-CMND
           PERFORM SGC-FIO-DBC
           IF FIO-STAT-OKS
               MOVE DBC-CAI-ICLI IN DBC TO WSS-CAI-ICLI IN WSS-VARI
               MOVE DBC-IIC-ICLI IN DBC TO WSS-IIC-ICLI IN WSS-VARI.
       FIN-OBT-CIC-ICLI.
           EXIT.
      *>>>> EOF
*% END
