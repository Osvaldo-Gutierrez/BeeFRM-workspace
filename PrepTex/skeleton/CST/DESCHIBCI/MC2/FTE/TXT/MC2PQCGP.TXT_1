*%! Codigo adicional para archivo
*% NO_TIENE_TIMESTAMP = FALSE
*% NO_HDR_CST = FALSE
*% LARGO_TOTAL_COMMAREA = 3000
*% FMT_CHI = TRUE
*% GEN_BCI = TRUE
*% PGM_PTC = TRUE
*% LARGO_MI_COMMAREA = 0
*% IF SETTAG
*% VSNPQ = "05.11.21.1"
*% VSNPU = "05.11.21.1"
*% FNCPQ = "MC2-CONVENIO"
*% FNCPU = "MC2-CONVENIO"
*% END
*%! Codigo adicional para archivo
*% IF MI_COMMAREA
      *<<<<
           03 SCR-NUM-PANT               PIC 9(0002).                   
           03 SCR-CGP-FLD                PIC X(1100).                   
      *>>>>
*% END
*% IF WSS
      *<<<< WSS                                                         
      *PVG INI 18-MAR-2009                                              
       COPY GNSWGCVT.                                                   
      *PVG FIN 18-MAR-2009                                              
       COPY GNSWGCPT.                                                   
       COPY GNSWGSTR.                                                   
       COPY MC2WXCGP.                                                   
       COPY MC2BRPRD.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-PRD-REQA==.        
      *>>>>                                                             
*% END
*% IF INI_FIO
      *<<<< INI_FIO                                                     
           IF SCR-OFRM-SRV                                              
                MOVE TSK-TERM-ALF TO CGP-COD-TERM                       
                MOVE TSK-TERM-ALF TO CGP-COD-TERM-MAS                   
           ELSE                                                         
                MOVE EIBTRMID     TO CGP-COD-TERM                       
                MOVE EIBTRMID     TO CGP-COD-TERM-MAS.                  
           MOVE +3859 TO SCR-TCMA.                                      
           PERFORM GNS-ERR-QUE.                                         
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )      
               MOVE SCR-CGP-FLD TO CGP-FLD                              
               GO TO FIN-INI.                                           
                                                                        
           IF ( FRM-FFLD = FRM-FFLD-PF6 )                               
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM        
               ADD 1              TO QUE-NITM                           
               MOVE CGP-NOM-TSCGP TO QUE-COLA                           
               MOVE CGP-LEN-TSCGP TO QUE-LITM                           
               MOVE QUE-GET       TO QUE-CMND                           
               PERFORM GNS-PRO-QUE                                      
               IF QUE-STAT NOT = QUE-STAT-OKS                           
                  MOVE SCR-CGP-FLD TO CGP-FLD                           
                  GO TO FIN-INI.                                        
           PERFORM GET-FHOY.                                            
      *>>>>                                                             
*% END
*% IF INI_SCR_COM
      *<<<< INI_SCR_COM                                                 
           MOVE CGP-NOM-TSCGP TO QUE-COLA.                              
           MOVE QUE-DEL       TO QUE-CMND.                              
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE CGP-NOM-TSCGP-MAS TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
      *>>>>                                                             
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC                                                 
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6                   
               PERFORM VAL-NUM-KEY-CGP                                  
               PERFORM VAL-NUM-FLD-CGP                                  
               PERFORM SCRL-TAB-CGP                                     
               PERFORM SCR-TRL.                                         
      *>>>>                                                             
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY                                                 
           PERFORM KEY-ALL-CGP.                                         
           MOVE FIO-GET-LEQ TO FIO-CMND.                                
      *PVG INI 18-MAR-2009 CONSULTA REFUNDE MONEDAS                     
       LUP-LEE-PLN.                                                     
      *PVG FIN 18-MAR-2009                                              
           PERFORM MC2-FIO-PLN.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    PLN-CAI-PLAN IN PLN = PLN-CAI-PLAN IN CGP-FLD AND   
                    PLN-IIC-PLAN IN PLN = PLN-IIC-PLAN IN CGP-FLD )     
              MOVE '23' TO FIO-STAT PGM-STAT-PLN                        
              GO TO FIN-FND-KEY.                                        
                                                                        
      *PVG INI 18-MAR-2009 CONSULTA REFUNDE MONEDAS                     
           IF FRM-IND-TCON IN CGP-FLD = 'R' AND                         
              PLN-MSC-STAT IN PLN NOT = 'A'                             
              MOVE FIO-GET-PRV TO FIO-CMND                              
              GO TO LUP-LEE-PLN.                                        
      *PVG FIN 18-MAR-2009                                              
                                                                        
           MOVE FIO-STAT TO PGM-STAT-PLN.                               
           PERFORM PUT-PLN-CGP.                                         
                                                                        
           IF FRM-IND-TCON IN CGP-FLD > SPACES                          
              MOVE FRM-NUM-IREG IN CGP-FLD TO PRD-NUM-IPRD IN PRD       
           ELSE                                                         
              MOVE ZEROES                  TO PRD-NUM-IPRD IN PRD.      
                                                                        
           PERFORM CLR-VEC-PLAN.                                        
           MOVE ZEROES      TO WSS-J.                                   
           MOVE PLN-CIC-IPRD IN PLN TO PRD-CIC-IPRD IN PRD.             
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-LEE-PRD.                                                     
           PERFORM MC2-FIO-PRD.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    PRD-CIC-IPRD IN PRD = PLN-CIC-IPRD IN PLN )         
              GO TO CON-FND-KEY.                                        
                                                                        
           PERFORM CRG-INF-PROD.                                        
                                                                        
           IF (SCR-OFRM-SRV       AND WSS-J > WSS-NITMMAX)              
              GO TO CON-FND-KEY.                                        
           IF ((NOT SCR-OFRM-SRV) AND WSS-J > WSS-JMAX)                 
              GO TO CON-FND-KEY.                                        
       NXT-LEE-PLN.                                                     
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-LEE-PRD.                                           
      *>>>>                                                             
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY                                                 
       CON-FND-KEY.                                                     
           PERFORM PUT-TAB.                                             
                                                                        
           MOVE '00' TO FIO-STAT.                                       
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           GO TO FIN-FND-KEY.                                           
      *----------------------------------------------------------       
       MSG-FND-KEY.                                                     
           MOVE 'TAB'        TO MSG-COD-SIST.                           
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MENS.                               
           MOVE '00'         TO FIO-STAT.                               
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           GO TO FIN-FND-KEY.                                           
      *>>>>                                                             
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON                                                 
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )           
      *>>>>                                                             
*% END
*% IF EOF
      *<<<< EOF                                                         
       PGM-EBR-ALL SECTION.                                             
       INI-PGM-EBR-ALL.                                                 
           MOVE 'GNS' TO FIO-SIST.                                      
           MOVE FIO-EBR-ALL TO FIO-CMND.                                
           PERFORM GNS-FIO-TAB.                                         
       FIN-PGM-EBR-ALL.                                                 
           EXIT.                                                        
       COPY GNSBBTAB.                                                   
       COPY GNSBVTAB.                                                   
       COPY GNSBVCOD.                                                   
       COPY GNSBPFEC.                                                   
       COPY MC2BFPRD.                                                   
                                                                        
       PUT-TAB SECTION.                                                 
       INI-PUT-TAB.                                                     
           MOVE CGP-NOM-TSCGP     TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
           MOVE CGP-NOM-TSCGP-MAS TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE 1 TO SCR-NUM-PANT.                                      
           MOVE LOW-VALUES TO FRM-NUM-PLAN-IDX-MTI FRM-NUM-PLAN-IDX-FRM.
      *                                                                 
           MOVE 1 TO FRM-IFLD.                                          
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.                        
                                                                        
           MOVE 1       TO WSS-F IN WSS-VARI.                           
       LUP-PUT-TAB.                                                     
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI                  
              GO TO FIN-PUT-TAB.                                        
           IF WSS-NUM-PLAN-V(WSS-F) > ZEROES                            
              PERFORM PUT-TAB-CGP                                       
              PERFORM PUT-CGP-QUE                                       
              PERFORM SET-FRM-IFLD                                      
           ELSE                                                         
           IF FRM-IFLD = 1 AND WSS-F = 1                                
              MOVE 'TAB'        TO MSG-COD-SIST                         
              MOVE 'CGP    NEX' TO MSG-COD-MENS                         
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              MOVE '00' TO FIO-STAT                                     
              GO TO FIN-PUT-TAB                                         
           ELSE                                                         
              MOVE '00' TO FIO-STAT                                     
              GO TO FIN-PUT-TAB.                                        
                                                                        
           ADD 1 TO WSS-F IN WSS-VARI.                                  
           GO TO LUP-PUT-TAB.                                           
       FIN-PUT-TAB.                                                     
           MOVE 1                  TO QUE-NITM.                         
           MOVE CGP-LEN-TSCGP-MAS  TO QUE-LITM.                         
           MOVE CGP-NOM-TSCGP-MAS  TO QUE-COLA.                         
                                                                        
           IF WSS-NITM > WSS-NITMMAX                                    
              MOVE 'S'           TO CGP-MAS-IND-HMAS                    
           ELSE                                                         
              MOVE 'N'           TO CGP-MAS-IND-HMAS.                   
                                                                        
           MOVE WSS-NITM         TO WSS-NUM-ICUR.                       
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.                   
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.                   
           MOVE WSS-NUM-ICUR-ALF TO CGP-MAS-NUM-TOCC.                   
           MOVE CGP-MAS-ITEM     TO QUE-ITEM.                           
           MOVE QUE-PUT          TO QUE-CMND.                           
           PERFORM GNS-PRO-QUE.                                         
           IF WSS-NITM > WSS-IMAX                                       
               MOVE 'TAB' TO MSG-COD-SIST                               
               MOVE 'CGPTABDMAS' TO MSG-COD-MENS                        
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN2.                           
       EXT-PUT-TAB.                                                     
           EXIT.                                                        
                                                                        
       SET-FRM-IFLD SECTION.                                            
       INI-SET-FRM-IFLD.                                                
           IF SCR-OFRM-SRV                                              
                MOVE 1 TO FRM-IFLD                                      
           ELSE                                                         
                ADD 1 TO FRM-IFLD.                                      
       FIN-SET-FRM-IFLD.                                                
           EXIT.                                                        
      *--------------------------------------------                     
       SCRL-TAB-CGP SECTION.                                            
       INI-SCRL-TAB-CGP.                                                
           MOVE SPACES TO MSG-COD-MENS.                                 
                                                                        
           IF FRM-FFLD = FRM-FFLD-PF6                                   
               PERFORM PUT-TAB-ASC                                      
           ELSE                                                         
               PERFORM PUT-TAB-DSC.                                     
           IF MSG-COD-MENS > SPACES                                     
               MOVE 'TAB' TO MSG-COD-SIST                               
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN2.                           
       FIN-SCRL-TAB-CGP.                                                
           EXIT.                                                        
                                                                        
      *--------------------------------------------                     
       PUT-TAB-CGP SECTION.                                             
       INI-PUT-TAB-CGP.                                                 
           PERFORM CLR-WSS.                                             
           IF SCR-OFRM-SRV                                              
              MOVE WSS-NUM-PLAN-V(WSS-F) TO MTI-NUM-PLAN IN WSS-VARI    
              MOVE WSS-GLS-SIST-V(WSS-F) TO MTI-GLS-SIST IN WSS-VARI    
              MOVE WSS-GLS-VCOC-V(WSS-F) TO MTI-GLS-VCOC IN WSS-VARI    
              MOVE WSS-GLS-TOOC-V(WSS-F) TO MTI-GLS-TOOC IN WSS-VARI    
              MOVE WSS-GLS-AOOC-V(WSS-F) TO MTI-GLS-AOOC IN WSS-VARI    
              MOVE WSS-GLS-PROD-V(WSS-F) TO MTI-GLS-PROD IN WSS-VARI    
           ELSE                                                         
              MOVE WSS-NUM-PLAN-V(WSS-F) TO WSS-NUM-PLAN IN WSS-VARI    
              MOVE WSS-GLS-SIST-V(WSS-F) TO WSS-GLS-SIST IN WSS-VARI    
              MOVE WSS-GLS-VCOC-V(WSS-F) TO WSS-GLS-VCOC IN WSS-VARI    
              MOVE WSS-GLS-TOOC-V(WSS-F) TO WSS-GLS-TOOC IN WSS-VARI    
              MOVE WSS-GLS-AOOC-V(WSS-F) TO WSS-GLS-AOOC IN WSS-VARI    
              MOVE WSS-GLS-PROD-V(WSS-F) TO WSS-GLS-PROD IN WSS-VARI.   
                                                                        
           IF FRM-IFLD > WSS-IMAX                                       
              GO TO FIN-PUT-TAB-CGP.                                    
                                                                        
           MOVE WSS-NUM-PLAN-V(WSS-F) TO                                
                FRM-NUM-PLAN IN CGP-FLD(FRM-IFLD).                      
           MOVE WSS-GLS-SIST-V(WSS-F) TO                                
                FRM-GLS-SIST IN CGP-FLD(FRM-IFLD).                      
           MOVE WSS-GLS-VCOC-V(WSS-F) TO                                
                FRM-GLS-VCOC IN CGP-FLD(FRM-IFLD).                      
           MOVE WSS-GLS-TOOC-V(WSS-F) TO                                
                FRM-GLS-TOOC IN CGP-FLD(FRM-IFLD).                      
           MOVE WSS-GLS-AOOC-V(WSS-F) TO                                
                FRM-GLS-AOOC IN CGP-FLD(FRM-IFLD).                      
           MOVE WSS-GLS-PROD-V(WSS-F) TO                                
                FRM-GLS-PROD IN CGP-FLD(FRM-IFLD).                      
       FIN-PUT-TAB-CGP.                                                 
           EXIT.                                                        
      *----------------------------------------------------             
       PUT-CGP-QUE SECTION.                                             
       INI-PUT-CGP-QUE.                                                 
           PERFORM PRO-FLD-QUE.                                         
           ADD 1 TO WSS-NITM.                                           
           MOVE CGP-NOM-TSCGP    TO QUE-COLA.                           
           MOVE CGP-LEN-TSCGP    TO QUE-LITM.                           
           MOVE WSS-NITM         TO QUE-NITM.                           
           MOVE SPACES           TO QUE-ITEM.                           
           IF SCR-OFRM-SRV                                              
              MOVE FRM-NUM-PLAN-IDX-MTI TO QUE-ITEM                     
           ELSE                                                         
              MOVE FRM-NUM-PLAN-IDX-FRM TO QUE-ITEM.                    
           MOVE QUE-PUT  TO QUE-CMND.                                   
           PERFORM GNS-PRO-QUE.                                         
       FIN-PUT-CGP-QUE.                                                 
           EXIT.                                                        
      *----------------------------------------------------             
                                                                        
       PUT-TAB-ASC SECTION.                                             
       INI-PUT-TAB-ASC.                                                 
           MOVE SPACES TO MSG-COD-MENS.                                 
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.           
           MOVE 0 TO FRM-IFLD.                                          
       LUP-PUT-TAB-ASC.                                                 
           ADD 1              TO QUE-NITM.                              
           MOVE CGP-NOM-TSCGP TO QUE-COLA.                              
           MOVE CGP-LEN-TSCGP TO QUE-LITM.                              
           MOVE QUE-GET       TO QUE-CMND.                              
           PERFORM GNS-PRO-QUE.                                         
           IF QUE-STAT NOT = QUE-STAT-OKS                               
               IF FRM-IFLD = 0                                          
                   MOVE 'CGPTABNONXT' TO MSG-COD-MENS                   
                   GO TO FIN-PUT-TAB-ASC                                
               ELSE                                                     
                   GO TO FIN-PUT-TAB-ASC.                               
           ADD 1 TO FRM-IFLD.                                           
           MOVE QUE-ITEM TO FRM-NUM-PLAN-IDX(FRM-IFLD).                 
           IF FRM-IFLD < WSS-IMAX                                       
               GO TO LUP-PUT-TAB-ASC.                                   
       FIN-PUT-TAB-ASC.                                                 
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX                      
               ADD 1 FRM-IFLD GIVING WSS-I                              
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1            
                                 UNTIL WSS-I > WSS-IMAX.                
           IF FRM-IFLD > 0                                              
               ADD 1 TO SCR-NUM-PANT.                                   
           IF FRM-IFLD = WSS-IMAX                                       
               ADD 1              TO QUE-NITM                           
               MOVE CGP-NOM-TSCGP TO QUE-COLA                           
               MOVE CGP-LEN-TSCGP TO QUE-LITM                           
               MOVE QUE-GET       TO QUE-CMND                           
               PERFORM GNS-PRO-QUE                                      
               IF QUE-STAT = QUE-STAT-OKS                               
                   MOVE 'TAB'        TO MSG-COD-SIST                    
                   MOVE 'CGPTABDMAS' TO MSG-COD-MENS.                   
       EXT-PUT-TAB-ASC.                                                 
           EXIT.                                                        
                                                                        
       PUT-TAB-DSC SECTION.                                             
       INI-PUT-TAB-DSC.                                                 
           MOVE SPACES TO MSG-COD-MENS.                                 
           IF SCR-NUM-PANT = 1                                          
               MOVE 'CGPTABNOPRV' TO MSG-COD-MENS                       
               GO TO EXT-PUT-TAB-DSC.                                   
                                                                        
           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.                
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.               
           ADD 1 TO QUE-NITM.                                           
           ADD 1 WSS-IMAX GIVING FRM-IFLD.                              
       LUP-PUT-TAB-DSC.                                                 
           SUBTRACT  1 FROM QUE-NITM.                                   
           MOVE CGP-NOM-TSCGP TO QUE-COLA                               
           MOVE CGP-LEN-TSCGP TO QUE-LITM                               
           MOVE QUE-GET       TO QUE-CMND.                              
           PERFORM GNS-PRO-QUE.                                         
           IF QUE-STAT NOT = QUE-STAT-OKS                               
               IF FRM-IFLD > WSS-IMAX                                   
                   MOVE 'CGPTABNOPRV' TO MSG-COD-MENS                   
                   GO TO FIN-PUT-TAB-DSC                                
               ELSE                                                     
                   GO TO FIN-PUT-TAB-DSC.                               
           SUBTRACT 1 FROM FRM-IFLD.                                    
           MOVE QUE-ITEM TO FRM-NUM-PLAN-IDX(FRM-IFLD).                 
           IF FRM-IFLD > 1                                              
               GO TO LUP-PUT-TAB-DSC.                                   
       FIN-PUT-TAB-DSC.                                                 
                                                                        
           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1                  
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I                    
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1           
                                 UNTIL WSS-I < 1.                       
                                                                        
           IF FRM-IFLD NOT > WSS-IMAX                                   
               SUBTRACT 1 FROM SCR-NUM-PANT                             
               MOVE 'TAB'        TO MSG-COD-SIST                        
               MOVE 'CGPTABDMAS' TO MSG-COD-MENS.                       
       EXT-PUT-TAB-DSC.                                                 
           EXIT.                                                        
                                                                        
       COPY GNSBGCPT.                                                   
       COPY GNSBGSTR.                                                   
                                                                        
       CLR-FRM SECTION.                                                 
       INI-CLR-FRM.                                                     
           MOVE ZEROES TO FRM-NUM-PLAN IN CGP-FLD(WSS-I).               
           MOVE SPACES TO FRM-GLS-SIST IN CGP-FLD(WSS-I).               
           MOVE SPACES TO FRM-GLS-VCOC IN CGP-FLD(WSS-I).               
           MOVE SPACES TO FRM-GLS-TOOC IN CGP-FLD(WSS-I).               
           MOVE SPACES TO FRM-GLS-AOOC IN CGP-FLD(WSS-I).               
           MOVE SPACES TO FRM-GLS-PROD IN CGP-FLD(WSS-I).               
       FIN-CLR-FRM.                                                     
           EXIT.                                                        
                                                                        
       CLR-WSS SECTION.                                                 
       INI-CLR-WSS.                                                     
           MOVE ZEROES TO WSS-NUM-PLAN IN WSS-VARI.                     
           MOVE SPACES TO WSS-GLS-SIST IN WSS-VARI.                     
           MOVE SPACES TO WSS-GLS-VCOC IN WSS-VARI.                     
           MOVE SPACES TO WSS-GLS-TOOC IN WSS-VARI.                     
           MOVE SPACES TO WSS-GLS-AOOC IN WSS-VARI.                     
           MOVE SPACES TO WSS-GLS-PROD IN WSS-VARI.                     
                                                                        
           MOVE ZEROES TO MTI-NUM-PLAN IN WSS-VARI.                     
           MOVE SPACES TO MTI-GLS-SIST IN WSS-VARI.                     
           MOVE SPACES TO MTI-GLS-VCOC IN WSS-VARI.                     
           MOVE SPACES TO MTI-GLS-TOOC IN WSS-VARI.                     
           MOVE SPACES TO MTI-GLS-AOOC IN WSS-VARI.                     
           MOVE SPACES TO MTI-GLS-PROD IN WSS-VARI.                     
                                                                        
           IF SCR-OFRM-SRV                                              
               PERFORM CLR-FRM-MTX.                                     
       FIN-CLR-WSS.                                                     
           EXIT.                                                        
                                                                        
       CLR-FRM-MTX SECTION.                                             
       INI-CLR-FRM-MTX.                                                 
           MOVE ZEROES TO FRM-NUM-PLAN IN CGP-FLD(1).                   
           MOVE SPACES TO FRM-GLS-SIST IN CGP-FLD(1).                   
           MOVE SPACES TO FRM-GLS-VCOC IN CGP-FLD(1).                   
           MOVE SPACES TO FRM-GLS-TOOC IN CGP-FLD(1).                   
           MOVE SPACES TO FRM-GLS-AOOC IN CGP-FLD(1).                   
           MOVE SPACES TO FRM-GLS-PROD IN CGP-FLD(1).                   
       FIN-CLR-FRM-MTX.                                                 
           EXIT.                                                        
                                                                        
       PRO-FLD-QUE SECTION.                                             
       INI-PRO-FLD-QUE.                                                 
           MOVE 'P' TO MTI-NUM-PLAN-ATR IN WSS-VARI..                   
           MOVE 'P' TO MTI-GLS-SIST-ATR IN WSS-VARI..                   
           MOVE 'P' TO MTI-GLS-VCOC-ATR IN WSS-VARI..                   
           MOVE 'P' TO MTI-GLS-TOOC-ATR IN WSS-VARI..                   
           MOVE 'P' TO MTI-GLS-AOOC-ATR IN WSS-VARI..                   
           MOVE 'P' TO MTI-GLS-PROD-ATR IN WSS-VARI..                   
                                                                        
           MOVE 'P' TO WSS-NUM-PLAN-ATR IN WSS-VARI..                   
           MOVE 'P' TO WSS-GLS-SIST-ATR IN WSS-VARI..                   
           MOVE 'P' TO WSS-GLS-VCOC-ATR IN WSS-VARI..                   
           MOVE 'P' TO WSS-GLS-TOOC-ATR IN WSS-VARI..                   
           MOVE 'P' TO WSS-GLS-AOOC-ATR IN WSS-VARI..                   
           MOVE 'P' TO WSS-GLS-PROD-ATR IN WSS-VARI..                   
       FIN-PRO-FLD-QUE.                                                 
           EXIT.                                                        
                                                                        
       CLR-VEC-PLAN SECTION.                                            
       INI-CLR-VEC-PLAN.                                                
           MOVE ZEROES TO WSS-J.                                        
       LUP-CLR-VEC-PLAN.                                                
           ADD 1 TO WSS-J.                                              
           IF WSS-J > WSS-JMAX                                          
              GO TO FIN-CLR-VEC-PLAN.                                   
                                                                        
           MOVE ZEROES TO WSS-NUM-PLAN-V(WSS-J).                        
           MOVE SPACES TO WSS-GLS-SIST-V(WSS-J).                        
           MOVE SPACES TO WSS-GLS-VCOC-V(WSS-J).                        
           MOVE SPACES TO WSS-GLS-TOOC-V(WSS-J).                        
           MOVE SPACES TO WSS-GLS-AOOC-V(WSS-J).                        
           MOVE SPACES TO WSS-GLS-PROD-V(WSS-J).                        
           GO TO LUP-CLR-VEC-PLAN.                                      
       FIN-CLR-VEC-PLAN.                                                
           EXIT.                                                        
                                                                        
       CRG-INF-PROD SECTION.                                            
       INI-CRG-INF-PROD.                                                
      *PVG INI 18-MAR-2009 CONSULTA REFUNDE MONEDAS                     
           IF FRM-IND-TCON IN CGP-FLD = 'R'                             
              IF PRD-COD-VCOC IN PRD NOT = WSS-COD-VCOC-R IN WSS-VARI   
                 MOVE PRD-COD-VCOC IN PRD TO WSS-COD-VCOC-R IN WSS-VARI 
              ELSE                                                      
                 GO FIN-CRG-INF-PROD.                                   
      *PVG FIN 18-MAR-2009                                              
                                                                        
           ADD 1 TO WSS-J.                                              
           MOVE PRD-NUM-IPRD IN PRD TO WSS-NUM-PLAN-V(WSS-J).           
           IF PRD-COD-SIST IN PRD > SPACES                              
              MOVE PRD-COD-SIST IN PRD TO WSS-GLS-SIST-V(WSS-J)         
              MOVE 'TAB'               TO TAB-COD-SIST                  
