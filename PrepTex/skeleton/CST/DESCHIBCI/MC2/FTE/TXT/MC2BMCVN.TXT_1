*% IF CVN_FEC_FVEN
      *<<<< CVN_FEC_FVEN

           IF CVN-FEC-FVEN IN CVN-FLD NOT > ZEROES
               GO TO FIN-VAL-CVN-FEC-FVEN.
               
           MOVE CVN-NUM-DAPE IN CVN-FLD TO FEC-DEC1.
           MOVE CVN-NUM-MAPE IN CVN-FLD TO FEC-MEC1.
           MOVE CVN-NUM-SAPE IN CVN-FLD TO FEC-SEC1.
           MOVE CVN-NUM-AAPE IN CVN-FLD TO FEC-AEC1.
           MOVE CVN-NUM-DVEN IN CVN-FLD TO FEC-DEC2.
           MOVE CVN-NUM-MVEN IN CVN-FLD TO FEC-MEC2.
           MOVE CVN-NUM-SVEN IN CVN-FLD TO FEC-SEC2.
           MOVE CVN-NUM-AVEN IN CVN-FLD TO FEC-AEC2.
           MOVE FEC-FORM-FEC TO FEC-FORM.
           IF CVN-IND-DURA IN CVN-FLD = 'A'
               MOVE FEC-DIF-ANO TO FEC-CMND
           ELSE
           IF CVN-IND-DURA IN CVN-FLD = 'M'
               MOVE FEC-DIF-MES TO FEC-CMND
           ELSE
           IF CVN-IND-DURA IN CVN-FLD = 'D'
               MOVE FEC-DIF-DIA TO FEC-CMND
           ELSE
               MOVE FEC-DIF-DIA TO FEC-CMND.
           PERFORM CAL-FEC.
           IF NOT FEC-STAT-OKS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE FEC-MENS     TO FRM-MENS
               GO TO FIN-VAL-CVN-FEC-FVEN
           ELSE
           IF CVN-IND-DURA IN CVN-FLD = 'A'
               MOVE FEC-NANO TO CVN-NUM-DURA IN CVN-FLD
           ELSE
           IF CVN-IND-DURA IN CVN-FLD = 'M'
               MOVE FEC-NMES TO CVN-NUM-DURA IN CVN-FLD
           ELSE
           IF CVN-IND-DURA IN CVN-FLD = 'D'
               MOVE FEC-NDIA TO CVN-NUM-DURA IN CVN-FLD
           ELSE
               MOVE FEC-NDIA TO CVN-NUM-DURA IN CVN-FLD.
               
      *>>>>
*% END
*% IF INI_VAL_CON_KEY_CVN
      *<<<< INI_VAL_CON_KEY_CVN
           IF SCR-CMND = 'ING'
               GO TO FIN-VAL-CON-KEY-CVN.
      *>>>>
*% END
