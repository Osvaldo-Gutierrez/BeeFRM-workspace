*%! Codigo adicional
*% NO_TIENE_TIMESTAMP = TRUE
*% NO_HDR_CST = TRUE
*% LARGO_TOTAL_COMMAREA = 3000
*% FMT_CHI = TRUE
*% GEN_BCI = TRUE
*% PGM_PTC = TRUE
*% IF SETTAG
*% VSNPQ = "05.12.12.1"
*% FNCPQ = "MC2-CONVENIO"
*% END
*% IF WSS
      *<<<< WSS
       COPY MC2WXQEC.
       COPY GNSWGCPT.
       COPY GNSWGSTR.
*% IF NOT PGM_MCP AND NOT PGM_PTC
       COPY GNSWGQUE.
*% END
      *>>>>
*% END
*% IF CMT_ID
      *<<<<
      *              24-JUN-2009 CONSULTA REGISTROS ANTERIORES          
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
       01 WSS-NUM-CCUR.
           03 WSS-NUM-ICUR-EDT  PIC  ZZ9.
           03 WSS-NUM-ICUR-ALF       PIC X(03).
           03 WSS-NUM-ICUR      REDEFINES
              WSS-NUM-ICUR-ALF       PIC 9(03).

       COPY SGCBRDBC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
       COPY MC2BRCVN.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CVN-REQA==.
      *>>>>
*% END
*% IF FIN_SCR_VARI
           03 SCR-NUM-PANT               PIC 9(0010).
           03 SCR-QEC-FLD                PIC X(1100).
           03 SCR-CIC-ICLI.
              05 SCR-CAI-ICLI            PIC X(04).
              05 SCR-IIC-ICLI            PIC X(08).
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-OFRM-SRV
                MOVE TSK-TERM-ALF TO QEC-COD-TERM
                MOVE TSK-TERM-ALF TO QEC-COD-TERM-MAS
           ELSE
                MOVE EIBTRMID     TO QEC-COD-TERM
                MOVE EIBTRMID     TO QEC-COD-TERM-MAS.
           MOVE +3859 TO SCR-TCMA.
           PERFORM GNS-ERR-QUE.
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )
               MOVE SCR-QEC-FLD TO QEC-FLD
               GO TO FIN-INI.

           IF ( FRM-FFLD = FRM-FFLD-PF6 )
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM
               ADD 1              TO QUE-NITM
               MOVE QEC-NOM-TSQEC TO QUE-COLA
               MOVE QEC-LEN-TSQEC TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT NOT = QUE-STAT-OKS
                  MOVE SCR-QEC-FLD TO QEC-FLD
                  GO TO FIN-INI.


      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
           MOVE SCR-CAI-ICLI IN SCR-VARI TO DEC-CAI-ICLI IN DEC.       
           MOVE SCR-IIC-ICLI IN SCR-VARI TO DEC-IIC-ICLI IN DEC.       
      *PVG INI 24-JUN-2009
      *    IF FRM-IND-SGTE IN QEC-FLD = 'S'
      *MHM INI 17-NOV-2010 CANALES INDIRECTOS EVEREST
      *    IF FRM-IND-SGTE IN QEC-FLD = 'S' OR 'A'
      *MHM FIN 17-NOV-2010
      *PVG FIN 24-JUN-2009
           MOVE FRM-CAI-ICNV IN QEC-FLD TO DEC-CAI-ICVN IN DEC.
           MOVE FRM-IIC-ICNV IN QEC-FLD TO DEC-IIC-ICVN IN DEC.
           MOVE FRM-COD-TREK IN QEC-FLD TO DEC-COD-TREC IN DEC.

      *PVG INI 24-JUN-2009                                              
           IF FRM-IND-SGTE IN QEC-FLD = 'A'                             
              MOVE FIO-GET-LEQ TO FIO-CMND                              
           ELSE                                                         
      *PVG FIN 24-JUN-2009                                              
           MOVE FIO-GET-NLS TO FIO-CMND.
       LUP-LEE-DEC.
           PERFORM MC2-FIO-DEC.
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF
               GO TO CON-FND-KEY.
      *PVG INI 24-JUN-2009                                              
           IF ((FRM-IND-SGTE IN QEC-FLD = 'S' OR 'A')         AND       
                DEC-CAI-ICVN IN DEC = FRM-CAI-ICNV IN QEC-FLD AND       
                DEC-IIC-ICVN IN DEC = FRM-IIC-ICNV IN QEC-FLD AND       
                DEC-COD-TREC IN DEC = FRM-COD-TREK IN QEC-FLD)          
                GO TO NXT-LEE-DEC.                                      
      *PVG FIN 24-JUN-2009                                              

           IF SCR-CAI-ICLI IN SCR-VARI > SPACES AND
              SCR-CAI-ICLI IN SCR-VARI NOT = DEC-CAI-ICLI IN DEC
               GO TO NXT-LEE-DEC.

           IF SCR-IIC-ICLI IN SCR-VARI > SPACES AND
              SCR-IIC-ICLI IN SCR-VARI NOT = DEC-IIC-ICLI IN DEC
               GO TO NXT-LEE-DEC.

           IF DEC-CAI-ICVN IN QEC-FLD > SPACES AND
              DEC-CAI-ICVN IN QEC-FLD NOT = DEC-CAI-ICVN IN DEC
               GO TO NXT-LEE-DEC.

           IF DEC-IIC-ICVN IN QEC-FLD > ZEROES AND
              DEC-IIC-ICVN IN QEC-FLD NOT = DEC-IIC-ICVN IN DEC
               GO TO NXT-LEE-DEC.

           IF DEC-COD-TREC IN QEC-FLD > SPACES AND
              DEC-COD-TREC IN QEC-FLD NOT = DEC-COD-TREC IN DEC
               GO TO NXT-LEE-DEC.

           PERFORM CRG-VEC-QEC.

           IF (SCR-OFRM-SRV AND WSS-J > WSS-NITMMAX)
              GO TO CON-FND-KEY.

           IF ((NOT SCR-OFRM-SRV) AND WSS-J > WSS-JMAX)
                 GO TO CON-FND-KEY.

       NXT-LEE-DEC.
      *PVG INI 24-JUN-2009                                              
           IF FRM-IND-SGTE IN QEC-FLD = 'A'                             
              MOVE FIO-GET-PRV TO FIO-CMND                              
           ELSE                                                         
      *PVG FIN 24-JUN-2009                                              
              MOVE FIO-GET-NXT TO FIO-CMND.
           GO TO LUP-LEE-DEC.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
       CON-FND-KEY.
           PERFORM PUT-TAB.

           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *----------------------------------------------------------
       MSG-FND-KEY.
           MOVE 'TAB' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM VAL-NUM-KEY-QEC
               PERFORM VAL-NUM-FLD-QEC
               PERFORM SCRL-TAB-QEC
               PERFORM SCR-TRL.
      *>>>>
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBBTAB.
       COPY GNSBVTAB.
       COPY GNSBVCOD.
       COPY GNSBEGLS.
       COPY SGCBFDBC.
       COPY MC2BFCVN.

       PUT-TAB SECTION.
       INI-PUT-TAB.
           MOVE QEC-NOM-TSQEC     TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE QEC-NOM-TSQEC-MAS TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE 1 TO SCR-NUM-PANT.
           MOVE LOW-VALUES TO WSS-GLS-NQEC-IDX-MTI.
           MOVE LOW-VALUES TO WSS-GLS-NQEC-IDX-FRM.
      *
           MOVE 1 TO FRM-IFLD.
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.

           MOVE 1 TO WSS-F IN WSS-VARI.
      *PVG INI 24-JUN-2009                                              
           IF FRM-IND-SGTE IN QEC-FLD = 'A'                             
              MOVE WSS-J TO WSS-F IN WSS-VARI                           
              IF WSS-F IN WSS-VARI NOT > ZEROES                         
                 MOVE 'S'   TO SCR-IERR                                 
                 MOVE 'TAB' TO MSG-COD-SIST                             
                 MOVE 'QEC    NEX' TO MSG-COD-MENS                      
                 PERFORM GET-MSG                                        
                 MOVE MSG-GLS-DESC TO FRM-MENS                          
                 MOVE '00' TO FIO-STAT                                  
                 GO TO FIN-PUT-TAB.                                     
      *PVG FIN 24-JUN-2009                                              
       LUP-PUT-TAB.
      *PVG INI 24-JUN-2009                                              
      *    IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI                  
           IF ((WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI AND            
                FRM-IND-SGTE IN QEC-FLD NOT = 'A')       OR             
               (WSS-F IN WSS-VARI < 1                    AND            
                FRM-IND-SGTE IN QEC-FLD = 'A'))                         
      *PVG FIN 24-JUN-2009                                              
              GO TO FIN-PUT-TAB.
           IF WSS-CAI-ICVN-V(WSS-F) > SPACES
              PERFORM PUT-TAB-QEC
              PERFORM PUT-QEC-QUE
              PERFORM SET-FRM-IFLD
           ELSE
           IF FRM-IFLD = 1 AND WSS-F = 1
              MOVE 'MC2' TO MSG-COD-SIST
              MOVE 'QEC    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE '00' TO FIO-STAT
              MOVE 'S' TO SCR-IERR
              GO TO FIN-PUT-TAB
           ELSE
              MOVE '00' TO FIO-STAT
              GO TO FIN-PUT-TAB.

      *PVG INI 24-JUN-2009                                              
           IF FRM-IND-SGTE IN QEC-FLD = 'A'                             
               SUBTRACT 1 FROM WSS-F IN WSS-VARI                        
           ELSE                                                         
      *PVG FIN 24-JUN-2009                                              
               ADD 1 TO WSS-F IN WSS-VARI.
           GO TO LUP-PUT-TAB.
       FIN-PUT-TAB.
           MOVE 1                  TO QUE-NITM.
           MOVE QEC-LEN-TSQEC-MAS  TO QUE-LITM.
           MOVE QEC-NOM-TSQEC-MAS  TO QUE-COLA.

           IF WSS-NITM > WSS-NITMMAX
              MOVE 'S'           TO QEC-MAS-IND-HMAS
           ELSE
              MOVE 'N'           TO QEC-MAS-IND-HMAS.

           MOVE WSS-NITM         TO WSS-NUM-ICUR.
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.
           MOVE WSS-NUM-ICUR-ALF TO QEC-MAS-NUM-TOCC.
           MOVE QEC-MAS-ITEM     TO QUE-ITEM.
           MOVE QUE-PUT          TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF WSS-NITM > WSS-IMAX
               MOVE 'MC2' TO MSG-COD-SIST
               MOVE 'QECTABDMAS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       EXT-PUT-TAB.
           EXIT.

       SET-FRM-IFLD SECTION.
       INI-SET-FRM-IFLD.
           IF SCR-OFRM-SRV
                MOVE 1 TO FRM-IFLD
           ELSE
                ADD 1 TO FRM-IFLD.
       FIN-SET-FRM-IFLD.
           EXIT.
      *--------------------------------------------
       SCRL-TAB-QEC SECTION.
       INI-SCRL-TAB-QEC.
           MOVE SPACES TO MSG-COD-MENS.

           IF FRM-FFLD = FRM-FFLD-PF6
               PERFORM PUT-TAB-ASC
           ELSE
               PERFORM PUT-TAB-DSC.
           IF MSG-COD-MENS > SPACES
               MOVE 'TAB' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-QEC.
           EXIT.

      *--------------------------------------------
       PUT-TAB-QEC SECTION.
       INI-PUT-TAB-QEC.
           PERFORM CLR-WSS.
           MOVE WSS-COD-TREC-V(WSS-F) TO WSS-COD-TREC-QEC IN WSS-VARI.
           MOVE WSS-CAI-ICVN-V(WSS-F) TO WSS-CAI-ICVN-QEC IN WSS-VARI.
           MOVE WSS-IIC-ICVN-V(WSS-F) TO WSS-IIC-ICVN-QEC IN WSS-VARI.
           MOVE WSS-IND-SEMP-V(WSS-F) TO WSS-IND-SEMP-QEC IN WSS-VARI.
           MOVE WSS-IND-APRB-V(WSS-F) TO WSS-IND-APRB-QEC IN WSS-VARI.
           MOVE WSS-IND-CMEM-V(WSS-F) TO WSS-IND-CMEM-QEC IN WSS-VARI.
           MOVE WSS-COD-FRTE-V(WSS-F) TO WSS-COD-FRTE-QEC IN WSS-VARI.
           MOVE WSS-SGV-CMEJ-V(WSS-F) TO WSS-SGV-CMEJ-QEC IN WSS-VARI.

           MOVE WSS-COD-TREC-QEC IN WSS-VARI TO WSS-COD-TREC-FRM.
           MOVE WSS-CAI-ICVN-QEC IN WSS-VARI TO WSS-CAI-ICVN-FRM.
           MOVE WSS-IIC-ICVN-QEC IN WSS-VARI TO WSS-IIC-ICVN-FRM.
           MOVE WSS-IND-SEMP-QEC IN WSS-VARI TO WSS-IND-SEMP-FRM.
           MOVE WSS-IND-APRB-QEC IN WSS-VARI TO WSS-IND-APRB-FRM.
           MOVE WSS-IND-CMEM-QEC IN WSS-VARI TO WSS-IND-CMEM-FRM.
           MOVE WSS-COD-FRTE-QEC IN WSS-VARI TO WSS-COD-FRTE-FRM.
           MOVE WSS-SGV-CMEJ-QEC IN WSS-VARI TO WSS-SGV-CMEJ-FRM.

      *PVG INI 24-JUN-2009                                              
           IF FRM-IND-SGTE IN QEC-FLD = 'A' AND                         
              WSS-F IN WSS-VARI = WSS-IMAX                              
              MOVE 1 TO FRM-IFLD.                                       
      *PVG FIN 24-JUN-2009                                              
           IF FRM-IFLD > WSS-IMAX
              GO TO FIN-PUT-TAB-QEC.

           MOVE WSS-COD-TREC-QEC IN WSS-VARI TO
               FRM-COD-TREC IN QEC-FLD(FRM-IFLD).
           MOVE WSS-CAI-ICVN-QEC IN WSS-VARI TO
               FRM-CAI-ICVN IN QEC-FLD(FRM-IFLD).
           MOVE WSS-IIC-ICVN-QEC IN WSS-VARI TO
               FRM-IIC-ICVN IN QEC-FLD(FRM-IFLD).
           MOVE WSS-IND-SEMP-QEC IN WSS-VARI TO
               FRM-IND-SEMP IN QEC-FLD(FRM-IFLD).
           MOVE WSS-IND-APRB-QEC IN WSS-VARI TO
               FRM-IND-APRB IN QEC-FLD(FRM-IFLD).
           MOVE WSS-IND-CMEM-QEC IN WSS-VARI TO
               FRM-IND-CMEM IN QEC-FLD(FRM-IFLD).
           MOVE WSS-COD-FRTE-QEC IN WSS-VARI TO
               FRM-COD-FRTE IN QEC-FLD(FRM-IFLD).
           MOVE WSS-SGV-CMEJ-QEC IN WSS-VARI TO
               FRM-SGV-CMEJ IN QEC-FLD(FRM-IFLD).
       FIN-PUT-TAB-QEC.
           EXIT.
      *----------------------------------------------------
       PUT-QEC-QUE SECTION.
       INI-PUT-QEC-QUE.
           PERFORM PRO-FLD-QUE.
           ADD 1 TO WSS-NITM.
           MOVE QEC-NOM-TSQEC    TO QUE-COLA.
           MOVE QEC-LEN-TSQEC    TO QUE-LITM.
           MOVE WSS-NITM         TO QUE-NITM.
           MOVE SPACES           TO QUE-ITEM.
           IF SCR-OFRM-SRV
              MOVE WSS-GLS-NQEC-IDX-MTI  TO QUE-ITEM
           ELSE
              MOVE WSS-GLS-NQEC-IDX-FRM  TO QUE-ITEM.
           MOVE QUE-PUT  TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
       FIN-PUT-QEC-QUE.
           EXIT.
      *----------------------------------------------------

       PUT-TAB-ASC SECTION.
       INI-PUT-TAB-ASC.
      *PVG INI 24-JUN-2009                                              
           IF FRM-IND-SGTE IN QEC-FLD = 'A'                             
              MOVE 'FPF    INV' TO MSG-COD-MENS                         
              GO TO EXT-PUT-TAB-ASC.                                    
      *PVG FIN 24-JUN-2009                                              
           MOVE SPACES TO MSG-COD-MENS.
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.
           MOVE 0 TO FRM-IFLD.
       LUP-PUT-TAB-ASC.
           ADD 1              TO QUE-NITM.
           MOVE QEC-NOM-TSQEC TO QUE-COLA.
           MOVE QEC-LEN-TSQEC TO QUE-LITM.
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD = 0
                   MOVE 'QECTABNONXT' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-ASC
               ELSE
                   GO TO FIN-PUT-TAB-ASC.
           ADD 1 TO FRM-IFLD.
           MOVE QUE-ITEM TO FRM-COD-TREC-IDX(FRM-IFLD).
           IF FRM-IFLD < WSS-IMAX
               GO TO LUP-PUT-TAB-ASC.
       FIN-PUT-TAB-ASC.
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX
               ADD 1 FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1
                                 UNTIL WSS-I > WSS-IMAX.
           IF FRM-IFLD > 0
               ADD 1 TO SCR-NUM-PANT.
           IF FRM-IFLD = WSS-IMAX
               ADD 1              TO QUE-NITM
               MOVE QEC-NOM-TSQEC TO QUE-COLA
               MOVE QEC-LEN-TSQEC TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT = QUE-STAT-OKS
                   MOVE 'MC2'        TO MSG-COD-SIST
                   MOVE 'QECTABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-ASC.
           EXIT.

       PUT-TAB-DSC SECTION.
       INI-PUT-TAB-DSC.
      *PVG INI 24-JUN-2009                                              
           IF FRM-IND-SGTE IN QEC-FLD = 'A'                             
              MOVE 'FPF    INV' TO MSG-COD-MENS                         
              GO TO EXT-PUT-TAB-DSC.                                    
      *PVG FIN 24-JUN-2009                                              
           MOVE SPACES TO MSG-COD-MENS.
           IF SCR-NUM-PANT = 1
               MOVE 'QECTABNOPRV' TO MSG-COD-MENS
               GO TO EXT-PUT-TAB-DSC.

           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.
           ADD 1 TO QUE-NITM.
           ADD 1 WSS-IMAX GIVING FRM-IFLD.
       LUP-PUT-TAB-DSC.
           SUBTRACT  1 FROM QUE-NITM.
           MOVE QEC-NOM-TSQEC TO QUE-COLA
           MOVE QEC-LEN-TSQEC TO QUE-LITM
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD > WSS-IMAX
                   MOVE 'QECTABNOPRV' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-DSC
               ELSE
                   GO TO FIN-PUT-TAB-DSC.
           SUBTRACT 1 FROM FRM-IFLD.
           MOVE QUE-ITEM TO FRM-COD-TREC-IDX(FRM-IFLD).
           IF FRM-IFLD > 1
               GO TO LUP-PUT-TAB-DSC.
       FIN-PUT-TAB-DSC.

           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1
                                 UNTIL WSS-I < 1.

           IF FRM-IFLD NOT > WSS-IMAX
               SUBTRACT 1 FROM SCR-NUM-PANT
               MOVE 'MC2'        TO MSG-COD-SIST
               MOVE 'QECTABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-DSC.
           EXIT.

       COPY GNSBGCPT.
       COPY GNSBGSTR.

       CLR-FRM SECTION.
       INI-CLR-FRM.
           MOVE SPACES TO FRM-COD-TREC IN QEC-FLD(WSS-I).
           MOVE SPACES TO FRM-CAI-ICVN IN QEC-FLD(WSS-I).
           MOVE ZEROES TO FRM-IIC-ICVN IN QEC-FLD(WSS-I).
           MOVE SPACES TO FRM-IND-SEMP IN QEC-FLD(WSS-I).
           MOVE SPACES TO FRM-IND-APRB IN QEC-FLD(WSS-I).
           MOVE SPACES TO FRM-IND-CMEM IN QEC-FLD(WSS-I).
           MOVE SPACES TO FRM-COD-FRTE IN QEC-FLD(WSS-I).
           MOVE ZEROES TO FRM-SGV-CMEJ IN QEC-FLD(WSS-I).
       FIN-CLR-FRM.
           EXIT.

       CLR-WSS SECTION.
       INI-CLR-WSS.
           MOVE SPACES TO WSS-COD-TREC-QEC IN WSS-VARI.
           MOVE SPACES TO WSS-CAI-ICVN-QEC IN WSS-VARI.
           MOVE ZEROES TO WSS-IIC-ICVN-QEC IN WSS-VARI.
           MOVE SPACES TO WSS-IND-SEMP-QEC IN WSS-VARI.
           MOVE SPACES TO WSS-IND-APRB-QEC IN WSS-VARI.
           MOVE SPACES TO WSS-IND-CMEM-QEC IN WSS-VARI.
           MOVE SPACES TO WSS-COD-FRTE-QEC IN WSS-VARI.
           MOVE ZEROES TO WSS-SGV-CMEJ-QEC IN WSS-VARI.

           IF SCR-OFRM-SRV
               PERFORM CLR-FRM-MTX.
       FIN-CLR-WSS.
           EXIT.

       CLR-FRM-MTX SECTION.
       INI-CLR-FRM-MTX.
           MOVE SPACES TO FRM-COD-TREC IN QEC-FLD(1).
           MOVE SPACES TO FRM-CAI-ICVN IN QEC-FLD(1).
           MOVE ZEROES TO FRM-IIC-ICVN IN QEC-FLD(1).
           MOVE SPACES TO FRM-IND-SEMP IN QEC-FLD(1).
           MOVE SPACES TO FRM-IND-APRB IN QEC-FLD(1).
           MOVE SPACES TO FRM-IND-CMEM IN QEC-FLD(1).
           MOVE SPACES TO FRM-COD-FRTE IN QEC-FLD(1).
           MOVE ZEROES TO FRM-SGV-CMEJ IN QEC-FLD(1).
       FIN-CLR-FRM-MTX.
           EXIT.

       CRG-VEC-QEC SECTION.
       INI-CRG-VEC-QEC.
           ADD 1 TO WSS-J.
           IF WSS-J IN WSS-VARI > WSS-JMAX IN WSS-VARI
               GO TO FIN-CRG-VEC-QEC.

           PERFORM CLR-QEC-VEC.
           MOVE DEC-COD-TREC IN DEC TO
                             WSS-COD-TREC-V IN WSS-VARI(WSS-J).
           MOVE DEC-CAI-ICVN IN DEC TO
                             WSS-CAI-ICVN-V IN WSS-VARI(WSS-J).
           MOVE DEC-IIC-ICVN IN DEC TO
                             WSS-IIC-ICVN-V IN WSS-VARI(WSS-J).
           MOVE DEC-IND-SEMP IN DEC TO
                             WSS-IND-SEMP-V IN WSS-VARI(WSS-J).
           MOVE DEC-IND-APRB IN DEC TO
                             WSS-IND-APRB-V IN WSS-VARI(WSS-J).
           MOVE DEC-IND-CMEM IN DEC TO
                             WSS-IND-CMEM-V IN WSS-VARI(WSS-J).
           MOVE DEC-COD-FRTE IN DEC TO
                             WSS-COD-FRTE-V IN WSS-VARI(WSS-J).
           IF SCR-OFRM-SRV
              MOVE DEC-SGV-CMEJ IN DEC TO FRM-SGV-CMEJ-EDT
