*%! Codigo adicional
*% NO_TIENE_TIMESTAMP = TRUE
*% NO_HDR_CST = TRUE
*% LARGO_TOTAL_COMMAREA = 3000
*% FMT_CHI = TRUE
*% GEN_BCI = TRUE
*% PGM_PTC = TRUE
*% IF SETTAG
*% VSNPQ = "06.03.20.1"
*% FNCPQ = "MC2-CONVENIO"
*% END
*% IF CMT_ID
      *<<<<
      *              24-JUN-2009 CONSULTA REGISTROS ANTERIORES          
      *>>>>
*% END
*% IF WSS
      *<<<< WSS
       COPY MC2WXQCC.
       COPY GNSWGCPT.
       COPY GNSWGSTR.
       COPY GNSWGHOY.
*% IF NOT PGM_MCP AND NOT PGM_PTC
       COPY GNSWGQUE.
*% END
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
       01 WSS-NUM-CCUR.
           03 WSS-NUM-ICUR-EDT  PIC  ZZ9.
           03 WSS-NUM-ICUR-ALF       PIC X(03).
           03 WSS-NUM-ICUR      REDEFINES
              WSS-NUM-ICUR-ALF       PIC 9(03).

       COPY SGCBRDBC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
       COPY MC2BRDEC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DEC-REQA==.
       COPY MC2BRDPC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DPC-REQA==.
       COPY SGCBRREC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-REC-REQA==.
       COPY TABBRPLN.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-PLN-REQA==.
       COPY TABBRPRD.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-PRD-REQA==.
      *>>>>
*% END
*% IF FIN_PGM_STAT
      *<<<< FIN_PGM_STAT
              05 PGM-STAT-PLN.
                 88 PGM-STAT-PLN-NUL    VALUE '  '.
                 07 PGM-STT1-PLN        VALUE '2' PIC X(01).
                 88 PGM-STAT-PLN-OKS VALUE '0'.
                 07 PGM-STT2-PLN        VALUE '3' PIC X(01).
              05 PGM-STAT-PRD.
                 88 PGM-STAT-PRD-NUL    VALUE '  '.
                 07 PGM-STT1-PRD        VALUE '2' PIC X(01).
                 88 PGM-STAT-PRD-OKS VALUE '0'.
                 07 PGM-STT2-PRD        VALUE '3' PIC X(01).
      *>>>>
*% END
*% IF FIN_SCR_VARI
      *<<<< FIN_SCR_VARI
           03 SCR-NUM-PANT               PIC 9(0010).
           03 SCR-QCC-FLD                PIC X(1100).
           03 SCR-CIC-ICLI.
              05 SCR-CAI-ICLI            PIC X(04).
              05 SCR-IIC-ICLI            PIC X(08).
      *>>>>
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-OFRM-SRV
                MOVE TSK-TERM-ALF TO QCC-COD-TERM
                MOVE TSK-TERM-ALF TO QCC-COD-TERM-MAS
           ELSE
                MOVE EIBTRMID     TO QCC-COD-TERM
                MOVE EIBTRMID     TO QCC-COD-TERM-MAS.
           MOVE +3859 TO SCR-TCMA.
           PERFORM GNS-ERR-QUE.
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )
               MOVE SCR-QCC-FLD TO QCC-FLD
               GO TO FIN-INI.

           IF ( FRM-FFLD = FRM-FFLD-PF6 )
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM
               ADD 1              TO QUE-NITM
               MOVE QCC-NOM-TSQCC TO QUE-COLA
               MOVE QCC-LEN-TSQCC TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT NOT = QUE-STAT-OKS
                  MOVE SCR-QCC-FLD TO QCC-FLD
                  GO TO FIN-INI.
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
           PERFORM GET-FHOY.					
           MOVE ZEROES TO WSS-J IN WSS-VARI.                            

           MOVE SCR-CAI-ICLI IN SCR-VARI TO REC-CAI-IREL IN REC.       
           MOVE SCR-IIC-ICLI IN SCR-VARI TO REC-IIC-IREL IN REC.       
           MOVE 'REC-CIC-IREL' TO FIO-AKEY.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-REC.
           IF NOT FIO-STAT-OKS
               GO TO CON-FND-KEY.

      *PVG INI 24-JUN-2009                                              
      *    IF FRM-IND-SGTE IN QCC-FLD = 'S'                             
           IF FRM-IND-SGTE IN QCC-FLD = 'S' OR 'A'                      
      *PVG FIN 24-JUN-2009                                              
               MOVE FRM-CAI-CONV IN QCC-FLD TO CVN-CAI-ICVN IN CVN
               MOVE FRM-IIC-CONV IN QCC-FLD TO CVN-IIC-ICVN IN CVN
               MOVE SPACES TO CVN-STP-ICVN IN CVN
           ELSE
               MOVE SPACES TO CVN-CIC-ICVN IN CVN
               MOVE SPACES TO CVN-STP-ICVN IN CVN.

      *PVG INI 24-JUN-2009                                              
           IF FRM-IND-SGTE IN QCC-FLD = 'A'                             
              MOVE FIO-GET-LEQ TO FIO-CMND                              
           ELSE                                                         
      *PVG FIN 24-JUN-2009                                              
              MOVE FIO-GET-NLS TO FIO-CMND.

       LUP-LEE-CVN.
           PERFORM MC2-FIO-CVN.
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF
               GO TO CON-FND-KEY.
      *PVG INI 24-JUN-2009                                              
           IF ((FRM-IND-SGTE IN QCC-FLD = 'S' OR 'A')         AND       
                CVN-CAI-ICVN IN CVN = FRM-CAI-CONV IN QCC-FLD AND       
                CVN-IIC-ICVN IN CVN = FRM-IIC-CONV IN QCC-FLD)          
                GO TO NXT-LEE-CVN.                                      
      *PVG FIN 24-JUN-2009                                              

           IF CVN-IND-SCVN IN QCC-FLD > SPACES AND
              CVN-IND-SCVN IN QCC-FLD NOT = CVN-IND-SCVN IN CVN
               GO TO NXT-LEE-CVN.

      *PVG INI 18-OCT-2010
           IF (CVN-COD-TIPO IN QCC-FLD > SPACES AND
               CVN-COD-TIPO IN CVN NOT = CVN-COD-TIPO IN QCC-FLD)
               GO TO NXT-LEE-CVN.

           IF (CVN-COD-STIP IN QCC-FLD > SPACES AND
               CVN-COD-STIP IN CVN NOT = CVN-COD-STIP IN QCC-FLD)
               GO TO NXT-LEE-CVN.

           IF (FRM-IND-VIGE IN QCC-FLD = 'S' AND
               CVN-FEC-FVEN IN CVN < HOY-FHOY IN HOY-VARI)
               GO TO NXT-LEE-CVN.
      *PVG FIN 18-OCT-2010

           IF CVN-CIC-ICVN IN CVN = WSS-CIC-ICVN IN WSS-VARI            
               GO TO NXT-LEE-CVN.                                       
                                                                        
           MOVE CVN-CIC-ICVN IN CVN TO WSS-CIC-ICVN IN WSS-VARI.        
           MOVE CVN-CAI-ICVN IN CVN      TO DEC-CAI-ICVN IN DEC.       
           MOVE CVN-IIC-ICVN IN CVN      TO DEC-IIC-ICVN IN DEC.       
           MOVE 'DEC-KEY-ICVN' TO FIO-AKEY
           MOVE FIO-GET-NLS TO FIO-CMND.
       LUP-LEE-DEC.
           PERFORM MC2-FIO-DEC.
           IF NOT ( FIO-STAT-OKS AND 
                    DEC-CAI-ICVN IN DEC = CVN-CAI-ICVN IN CVN AND
                    DEC-IIC-ICVN IN DEC = CVN-IIC-ICVN IN CVN )
               GO TO NXT-LEE-CVN.

      *PVG INI 18-OCT-2010
           IF (FRM-COD-TSEG IN QCC-FLD > SPACES AND
               DEC-COD-TREC IN DEC NOT = FRM-COD-TSEG IN QCC-FLD)
               GO TO NXT-LEE-CVN.
      *PVG FIN 18-OCT-2010

           MOVE DEC-CAI-ICLI IN DEC      TO REC-CAI-ICLI IN REC.       
           MOVE DEC-IIC-ICLI IN DEC      TO REC-IIC-ICLI IN REC.
           MOVE SCR-CAI-ICLI IN SCR-VARI TO REC-CAI-IREL IN REC.
           MOVE SCR-IIC-ICLI IN SCR-VARI TO REC-IIC-IREL IN REC.
           MOVE DEC-COD-TREC IN DEC      TO REC-COD-TREC IN REC.      
           MOVE ZEROES                   TO REC-NUM-TREC IN REC.      
           MOVE FIO-GET-NLS TO FIO-CMND.
       LUP-LEE-REC.
           PERFORM SGC-FIO-REC.
           IF NOT ( FIO-STAT-OKS AND
                    REC-CAI-ICLI IN REC = DEC-CAI-ICLI IN DEC AND
                    REC-IIC-ICLI IN REC = DEC-IIC-ICLI IN DEC AND
                    REC-CAI-IREL IN REC = SCR-CAI-ICLI IN SCR-VARI AND
                    REC-IIC-IREL IN REC = SCR-IIC-ICLI IN SCR-VARI AND
                    REC-COD-TREC IN REC = DEC-COD-TREC IN DEC )
               GO TO NXT-LEE-DEC.

           IF REC-IND-VIGE IN REC NOT = 'S'
               MOVE FIO-GET-NXT TO FIO-CMND
               GO TO LUP-LEE-REC.

           IF REC-FEC-FTER IN REC > ZEROES AND
              REC-FEC-FTER IN REC < HOY-FHOY
               MOVE FIO-GET-NXT TO FIO-CMND
               GO TO LUP-LEE-REC.

           MOVE CVN-CAI-ICVN IN CVN      TO DPC-CAI-ICVN IN DPC.
           MOVE CVN-IIC-ICVN IN CVN      TO DPC-IIC-ICVN IN DPC.
           MOVE DEC-CAI-ICLI IN DEC      TO DPC-CAI-ICLI IN DPC.       
           MOVE DEC-IIC-ICLI IN DEC      TO DPC-IIC-ICLI IN DPC.
           MOVE DEC-COD-TREC IN DEC      TO DPC-COD-TREC IN DPC.
           MOVE SPACES                   TO DPC-COD-SIST IN DPC.
           MOVE SPACES                   TO DPC-COD-GRPO IN DPC.
           MOVE ZEROES                   TO DPC-NUM-IDPC IN DPC.
           MOVE FIO-GET-NLS TO FIO-CMND.

       LUP-LEE-DPC.
           PERFORM MC2-FIO-DPC.
           IF NOT ( FIO-STAT-OKS AND
                    CVN-CAI-ICVN IN CVN = DPC-CAI-ICVN IN DPC AND
                    CVN-IIC-ICVN IN CVN = DPC-IIC-ICVN IN DPC AND
                    DEC-CAI-ICLI IN DEC = DPC-CAI-ICLI IN DPC AND    
                    DEC-IIC-ICLI IN DEC = DPC-IIC-ICLI IN DPC AND
                    DEC-COD-TREC IN DEC = DPC-COD-TREC IN DPC )
               GO TO NXT-LEE-DEC.

           MOVE SPACES TO WSS-IND-SELE IN WSS-VARI.
           MOVE SPACES TO WSS-IND-SELG IN WSS-VARI.

           IF DPC-COD-SIST IN DPC > SPACES
               PERFORM EVAL-PROD
           ELSE
               PERFORM ANL-PLN-PRD.

           IF DPC-COD-SIST IN DPC > SPACES AND
              WSS-IND-SELE IN WSS-VARI = SPACES
               GO TO CRG-VEC
           ELSE
               IF WSS-IND-SELG IN WSS-VARI = 'S'
                   GO TO CRG-VEC.

           GO TO NXT-LEE-DPC.

       CRG-VEC.
           PERFORM CRG-VEC-QCC.

           IF (SCR-OFRM-SRV AND WSS-J > WSS-NITMMAX)
               GO TO CON-FND-KEY.

           IF ((NOT SCR-OFRM-SRV) AND WSS-J > WSS-JMAX)
               GO TO CON-FND-KEY.

           GO TO NXT-LEE-DEC.

       NXT-LEE-DPC.
           MOVE FIO-GET-NXT TO FIO-CMND.
           GO TO LUP-LEE-DPC.

       NXT-LEE-DEC.
           MOVE 'DEC-KEY-ICVN' TO FIO-AKEY.
           MOVE FIO-GET-NXT TO FIO-CMND.
           GO TO LUP-LEE-DEC.

       NXT-LEE-CVN.
      *PVG INI 24-JUN-2009                                              
           IF FRM-IND-SGTE IN QCC-FLD = 'A'                             
              MOVE FIO-GET-PRV TO FIO-CMND                              
           ELSE                                                         
      *PVG FIN 24-JUN-2009                                              
              MOVE FIO-GET-NXT TO FIO-CMND.
           GO TO LUP-LEE-CVN.
      *>>>>
*% END

*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
       CON-FND-KEY.
           PERFORM PUT-TAB.

           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *----------------------------------------------------------
       MSG-FND-KEY.
           MOVE 'TAB' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM VAL-NUM-KEY-QCC
               PERFORM VAL-NUM-FLD-QCC
               PERFORM SCRL-TAB-QCC
               PERFORM SCR-TRL.
      *>>>>
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBBTAB.
       COPY GNSBVTAB.
       COPY GNSBVCOD.
       COPY GNSBEGLS.
       COPY SGCBFDBC.
       COPY MC2BFDEC.
       COPY MC2BFDPC.
       COPY SGCBFREC.
       COPY TABBFPLN.
       COPY TABBFPRD.

       PUT-TAB SECTION.
       INI-PUT-TAB.
           MOVE QCC-NOM-TSQCC     TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE QCC-NOM-TSQCC-MAS TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE 1 TO SCR-NUM-PANT.
           MOVE LOW-VALUES TO WSS-GLS-NQCC-IDX-MTI.
           MOVE LOW-VALUES TO WSS-GLS-NQCC-IDX-FRM.
      *
           MOVE 1 TO FRM-IFLD.
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.

           MOVE 1 TO WSS-F IN WSS-VARI.
      *PVG INI 24-JUN-2009                                              
           IF FRM-IND-SGTE IN QCC-FLD = 'A'                             
              MOVE WSS-J TO WSS-F IN WSS-VARI                           
              IF WSS-F IN WSS-VARI NOT > ZEROES                         
                 MOVE 'S'   TO SCR-IERR                                 
                 MOVE 'TAB' TO MSG-COD-SIST                             
                 MOVE 'QCC    NEX' TO MSG-COD-MENS                      
                 PERFORM GET-MSG                                        
                 MOVE MSG-GLS-DESC TO FRM-MENS                          
                 MOVE '00' TO FIO-STAT                                  
                 GO TO FIN-PUT-TAB.                                     
      *PVG FIN 24-JUN-2009                                              
       LUP-PUT-TAB.
      *PVG INI 24-JUN-2009                                              
      *    IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI                  
           IF ((WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI AND            
                FRM-IND-SGTE IN QCC-FLD NOT = 'A')       OR             
               (WSS-F IN WSS-VARI < 1                    AND            
                FRM-IND-SGTE IN QCC-FLD = 'A'))                         
      *PVG FIN 24-JUN-2009                                              
              GO TO FIN-PUT-TAB.
           IF WSS-CAI-ICVN-V(WSS-F) > SPACES
              PERFORM PUT-TAB-QCC
              PERFORM PUT-QCC-QUE
              PERFORM SET-FRM-IFLD
           ELSE
           IF FRM-IFLD = 1 AND WSS-F = 1
              MOVE 'MC2' TO MSG-COD-SIST
              MOVE 'QCC    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE '00' TO FIO-STAT
              MOVE 'S' TO SCR-IERR
              GO TO FIN-PUT-TAB
           ELSE
              MOVE '00' TO FIO-STAT
              GO TO FIN-PUT-TAB.

      *PVG INI 24-JUN-2009                                              
           IF FRM-IND-SGTE IN QCC-FLD = 'A'                             
               SUBTRACT 1 FROM WSS-F IN WSS-VARI                        
           ELSE                                                         
      *PVG FIN 24-JUN-2009                                              
               ADD 1 TO WSS-F IN WSS-VARI.
           GO TO LUP-PUT-TAB.
       FIN-PUT-TAB.
           MOVE 1                  TO QUE-NITM.
           MOVE QCC-LEN-TSQCC-MAS  TO QUE-LITM.
           MOVE QCC-NOM-TSQCC-MAS  TO QUE-COLA.

           IF WSS-NITM > WSS-NITMMAX
              MOVE 'S'           TO QCC-MAS-IND-HMAS
           ELSE
              MOVE 'N'           TO QCC-MAS-IND-HMAS.

           MOVE WSS-NITM         TO WSS-NUM-ICUR.
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.
           MOVE WSS-NUM-ICUR-ALF TO QCC-MAS-NUM-TOCC.
           MOVE QCC-MAS-ITEM     TO QUE-ITEM.
           MOVE QUE-PUT          TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF WSS-NITM > WSS-IMAX
               MOVE 'MC2' TO MSG-COD-SIST
               MOVE 'QCCTABDMAS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       EXT-PUT-TAB.
           EXIT.

       SET-FRM-IFLD SECTION.
       INI-SET-FRM-IFLD.
           IF SCR-OFRM-SRV
                MOVE 1 TO FRM-IFLD
           ELSE
                ADD 1 TO FRM-IFLD.
       FIN-SET-FRM-IFLD.
           EXIT.
      *--------------------------------------------
       SCRL-TAB-QCC SECTION.
       INI-SCRL-TAB-QCC.
           MOVE SPACES TO MSG-COD-MENS.

           IF FRM-FFLD = FRM-FFLD-PF6
               PERFORM PUT-TAB-ASC
           ELSE
               PERFORM PUT-TAB-DSC.
           IF MSG-COD-MENS > SPACES
               MOVE 'MC2' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-QCC.
           EXIT.

      *--------------------------------------------
       PUT-TAB-QCC SECTION.
       INI-PUT-TAB-QCC.
           PERFORM CLR-WSS.
           MOVE WSS-CAI-ICVN-V(WSS-F) TO WSS-CAI-ICVN-QCC IN WSS-VARI.
           MOVE WSS-IIC-ICVN-V(WSS-F) TO WSS-IIC-ICVN-QCC IN WSS-VARI.
           MOVE WSS-COD-TREC-V(WSS-F) TO WSS-COD-TREC-QCC IN WSS-VARI.
           MOVE WSS-IND-SCVN-V(WSS-F) TO WSS-IND-SCVN-QCC IN WSS-VARI.
           MOVE WSS-COD-TIPO-V(WSS-F) TO WSS-COD-TIPO-QCC IN WSS-VARI.
           MOVE WSS-COD-STIP-V(WSS-F) TO WSS-COD-STIP-QCC IN WSS-VARI.
           MOVE WSS-NUM-ICLI-V(WSS-F) TO WSS-NUM-ICLI-QCC IN WSS-VARI.
           MOVE WSS-VRF-ICLI-V(WSS-F) TO WSS-VRF-ICLI-QCC IN WSS-VARI.
           MOVE WSS-IND-ICLI-V(WSS-F) TO WSS-IND-ICLI-QCC IN WSS-VARI.
           MOVE WSS-GLS-ICLI-V(WSS-F) TO WSS-GLS-ICLI-QCC IN WSS-VARI.
           MOVE WSS-GLS-DESC-V(WSS-F) TO WSS-GLS-DESC-QCC IN WSS-VARI.
           MOVE WSS-GLS-RZNS-V(WSS-F) TO WSS-GLS-RZNS-QCC IN WSS-VARI.

           MOVE WSS-CAI-ICVN-QCC IN WSS-VARI TO WSS-CAI-ICVN-FRM.
           MOVE WSS-IIC-ICVN-QCC IN WSS-VARI TO WSS-IIC-ICVN-FRM.
           MOVE WSS-COD-TREC-QCC IN WSS-VARI TO WSS-COD-TREC-FRM.
           MOVE WSS-IND-SCVN-QCC IN WSS-VARI TO WSS-IND-SCVN-FRM.
           MOVE WSS-COD-TIPO-QCC IN WSS-VARI TO WSS-COD-TIPO-FRM.
           MOVE WSS-COD-STIP-QCC IN WSS-VARI TO WSS-COD-STIP-FRM.
           MOVE WSS-NUM-ICLI-QCC IN WSS-VARI TO WSS-NUM-ICLI-FRM.
           MOVE WSS-VRF-ICLI-QCC IN WSS-VARI TO WSS-VRF-ICLI-FRM.
           MOVE WSS-IND-ICLI-QCC IN WSS-VARI TO WSS-IND-ICLI-FRM.
           MOVE WSS-GLS-ICLI-QCC IN WSS-VARI TO WSS-GLS-ICLI-FRM.
           MOVE WSS-GLS-DESC-QCC IN WSS-VARI TO WSS-GLS-DESC-FRM.
           MOVE WSS-GLS-RZNS-QCC IN WSS-VARI TO WSS-GLS-RZNS-FRM.

      *PVG INI 24-JUN-2009                                              
           IF FRM-IND-SGTE IN QCC-FLD = 'A' AND                         
              WSS-F IN WSS-VARI = WSS-IMAX                              
              MOVE 1 TO FRM-IFLD.                                       
      *PVG FIN 24-JUN-2009                                              
           IF FRM-IFLD > WSS-IMAX
              GO TO FIN-PUT-TAB-QCC.

           MOVE WSS-CAI-ICVN-QCC IN WSS-VARI TO
               FRM-CAI-ICVN IN QCC-FLD(FRM-IFLD).
           MOVE WSS-IIC-ICVN-QCC IN WSS-VARI TO
               FRM-IIC-ICVN IN QCC-FLD(FRM-IFLD).
           MOVE WSS-COD-TREC-QCC IN WSS-VARI TO
               FRM-COD-TREC IN QCC-FLD(FRM-IFLD).
           MOVE WSS-IND-SCVN-QCC IN WSS-VARI TO
               FRM-IND-SCVN IN QCC-FLD(FRM-IFLD).
           MOVE WSS-COD-TIPO-QCC IN WSS-VARI TO
               FRM-COD-TIPO IN QCC-FLD(FRM-IFLD).
           MOVE WSS-COD-STIP-QCC IN WSS-VARI TO
               FRM-COD-STIP IN QCC-FLD(FRM-IFLD).
           MOVE WSS-NUM-ICLI-QCC IN WSS-VARI TO
               FRM-NUM-ICLI IN QCC-FLD(FRM-IFLD).
           MOVE WSS-VRF-ICLI-QCC IN WSS-VARI TO
               FRM-VRF-ICLI IN QCC-FLD(FRM-IFLD).
           MOVE WSS-IND-ICLI-QCC IN WSS-VARI TO
               FRM-IND-ICLI IN QCC-FLD(FRM-IFLD).
           MOVE WSS-GLS-ICLI-QCC IN WSS-VARI TO
               FRM-GLS-ICLI IN QCC-FLD(FRM-IFLD).
           MOVE WSS-GLS-DESC-QCC IN WSS-VARI TO
               FRM-GLS-DESC IN QCC-FLD(FRM-IFLD). 
           MOVE WSS-GLS-RZNS-QCC IN WSS-VARI TO
               FRM-GLS-RZNS IN QCC-FLD(FRM-IFLD). 
       FIN-PUT-TAB-QCC.
           EXIT.
      *----------------------------------------------------
       PUT-QCC-QUE SECTION.
       INI-PUT-QCC-QUE.
           PERFORM PRO-FLD-QUE.
           ADD 1 TO WSS-NITM.
           MOVE QCC-NOM-TSQCC    TO QUE-COLA.
           MOVE QCC-LEN-TSQCC    TO QUE-LITM.
           MOVE WSS-NITM         TO QUE-NITM.
           MOVE SPACES           TO QUE-ITEM.
           IF SCR-OFRM-SRV
              MOVE WSS-GLS-NQCC-IDX-MTI  TO QUE-ITEM
           ELSE
              MOVE WSS-GLS-NQCC-IDX-FRM  TO QUE-ITEM.
           MOVE QUE-PUT  TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
       FIN-PUT-QCC-QUE.
           EXIT.
      *----------------------------------------------------

       PUT-TAB-ASC SECTION.
       INI-PUT-TAB-ASC.
      *PVG INI 24-JUN-2009                                              
           IF FRM-IND-SGTE IN QCC-FLD = 'A'                             
              MOVE 'FPF    INV' TO MSG-COD-MENS                         
              GO TO EXT-PUT-TAB-ASC.                                    
      *PVG FIN 24-JUN-2009                                              
           MOVE SPACES TO MSG-COD-MENS.
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.
           MOVE 0 TO FRM-IFLD.
       LUP-PUT-TAB-ASC.
           ADD 1              TO QUE-NITM.
           MOVE QCC-NOM-TSQCC TO QUE-COLA.
           MOVE QCC-LEN-TSQCC TO QUE-LITM.
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD = 0
                   MOVE 'QCCTABNONXT' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-ASC
               ELSE
                   GO TO FIN-PUT-TAB-ASC.
           ADD 1 TO FRM-IFLD.
           MOVE QUE-ITEM TO FRM-CAI-ICVN-IDX(FRM-IFLD).
           IF FRM-IFLD < WSS-IMAX
               GO TO LUP-PUT-TAB-ASC.
       FIN-PUT-TAB-ASC.
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX
               ADD 1 FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1
                                 UNTIL WSS-I > WSS-IMAX.
           IF FRM-IFLD > 0
               ADD 1 TO SCR-NUM-PANT.
           IF FRM-IFLD = WSS-IMAX
               ADD 1              TO QUE-NITM
               MOVE QCC-NOM-TSQCC TO QUE-COLA
               MOVE QCC-LEN-TSQCC TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT = QUE-STAT-OKS
                   MOVE 'MC2'        TO MSG-COD-SIST
                   MOVE 'QCCTABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-ASC.
           EXIT.

       PUT-TAB-DSC SECTION.
       INI-PUT-TAB-DSC.
      *PVG INI 24-JUN-2009                                              
           IF FRM-IND-SGTE IN QCC-FLD = 'A'                             
              MOVE 'FPF    INV' TO MSG-COD-MENS                         
              GO TO EXT-PUT-TAB-DSC.                                    
      *PVG FIN 24-JUN-2009                                              
           MOVE SPACES TO MSG-COD-MENS.
           IF SCR-NUM-PANT = 1
               MOVE 'QCCTABNOPRV' TO MSG-COD-MENS
               GO TO EXT-PUT-TAB-DSC.

           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.
           ADD 1 TO QUE-NITM.
           ADD 1 WSS-IMAX GIVING FRM-IFLD.
       LUP-PUT-TAB-DSC.
           SUBTRACT  1 FROM QUE-NITM.
           MOVE QCC-NOM-TSQCC TO QUE-COLA
           MOVE QCC-LEN-TSQCC TO QUE-LITM
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD > WSS-IMAX
                   MOVE 'QCCTABNOPRV' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-DSC
               ELSE
                   GO TO FIN-PUT-TAB-DSC.
           SUBTRACT 1 FROM FRM-IFLD.
           MOVE QUE-ITEM TO FRM-CAI-ICVN-IDX(FRM-IFLD).
           IF FRM-IFLD > 1
               GO TO LUP-PUT-TAB-DSC.
       FIN-PUT-TAB-DSC.

           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1
                                 UNTIL WSS-I < 1.

           IF FRM-IFLD NOT > WSS-IMAX
               SUBTRACT 1 FROM SCR-NUM-PANT
               MOVE 'MC2'        TO MSG-COD-SIST
               MOVE 'QCCTABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-DSC.
           EXIT.

       COPY GNSBGCPT.
       COPY GNSBGSTR.
       COPY GNSBGHOY.

       CLR-FRM SECTION.
       INI-CLR-FRM.
           MOVE SPACES TO FRM-CAI-ICVN IN QCC-FLD(WSS-I).
           MOVE ZEROES TO FRM-IIC-ICVN IN QCC-FLD(WSS-I).
           MOVE SPACES TO FRM-COD-TREC IN QCC-FLD(WSS-I).
           MOVE SPACES TO FRM-IND-SCVN IN QCC-FLD(WSS-I).
           MOVE SPACES TO FRM-COD-TIPO IN QCC-FLD(WSS-I).
           MOVE SPACES TO FRM-COD-STIP IN QCC-FLD(WSS-I).
           MOVE ZEROES TO FRM-NUM-ICLI IN QCC-FLD(WSS-I).
           MOVE SPACES TO FRM-VRF-ICLI IN QCC-FLD(WSS-I).
           MOVE SPACES TO FRM-IND-ICLI IN QCC-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-ICLI IN QCC-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-DESC IN QCC-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-RZNS IN QCC-FLD(WSS-I).
       FIN-CLR-FRM.
           EXIT.

       CLR-WSS SECTION.
       INI-CLR-WSS.
           MOVE SPACES TO WSS-CAI-ICVN-QCC IN WSS-VARI.
           MOVE ZEROES TO WSS-IIC-ICVN-QCC IN WSS-VARI.
           MOVE SPACES TO WSS-COD-TREC-QCC IN WSS-VARI.
           MOVE SPACES TO WSS-IND-SCVN-QCC IN WSS-VARI.
           MOVE SPACES TO WSS-COD-TIPO-QCC IN WSS-VARI.
           MOVE SPACES TO WSS-COD-STIP-QCC IN WSS-VARI.
           MOVE ZEROES TO WSS-NUM-ICLI-QCC IN WSS-VARI.
           MOVE SPACES TO WSS-VRF-ICLI-QCC IN WSS-VARI.
           MOVE SPACES TO WSS-IND-ICLI-QCC IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-ICLI-QCC IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-DESC-QCC IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-RZNS-QCC IN WSS-VARI.
           IF SCR-OFRM-SRV
               PERFORM CLR-FRM-MTX.
       FIN-CLR-WSS.
           EXIT.

       CLR-FRM-MTX SECTION.
       INI-CLR-FRM-MTX.
           MOVE SPACES TO FRM-CAI-ICVN IN QCC-FLD(1).
           MOVE ZEROES TO FRM-IIC-ICVN IN QCC-FLD(1).
           MOVE SPACES TO FRM-COD-TREC IN QCC-FLD(1).
           MOVE SPACES TO FRM-IND-SCVN IN QCC-FLD(1).
           MOVE SPACES TO FRM-COD-TIPO IN QCC-FLD(1).
           MOVE SPACES TO FRM-COD-STIP IN QCC-FLD(1).
           MOVE ZEROES TO FRM-NUM-ICLI IN QCC-FLD(1).
           MOVE SPACES TO FRM-VRF-ICLI IN QCC-FLD(1).
           MOVE SPACES TO FRM-IND-ICLI IN QCC-FLD(1).
           MOVE SPACES TO FRM-GLS-ICLI IN QCC-FLD(1).
           MOVE SPACES TO FRM-GLS-DESC IN QCC-FLD(1).
           MOVE SPACES TO FRM-GLS-RZNS IN QCC-FLD(1).
       FIN-CLR-FRM-MTX.
           EXIT.

       CRG-VEC-QCC SECTION.
       INI-CRG-VEC-QCC.
           ADD 1 TO WSS-J.
           IF WSS-J IN WSS-VARI > WSS-JMAX IN WSS-VARI
               GO TO FIN-CRG-VEC-QCC.

           PERFORM CLR-QCC-VEC.

           MOVE CVN-CAI-ICVN IN CVN TO 
                             WSS-CAI-ICVN-V IN WSS-VARI(WSS-J).
           MOVE CVN-IIC-ICVN IN CVN TO 
                             WSS-IIC-ICVN-V IN WSS-VARI(WSS-J).

           MOVE DEC-COD-TREC IN DEC TO 
                             WSS-COD-TREC-V IN WSS-VARI(WSS-J).
           MOVE CVN-IND-SCVN IN CVN TO 
                             WSS-IND-SCVN-V IN WSS-VARI(WSS-J).
           MOVE DPC-COD-TOOC IN DPC TO 
                             WSS-COD-TIPO-V IN WSS-VARI(WSS-J).
           MOVE DPC-COD-AOOC IN DPC TO 
                             WSS-COD-STIP-V IN WSS-VARI(WSS-J).

           MOVE DEC-CAI-ICLI IN DEC TO DBC-CAI-ICLI IN DBC.
           MOVE DEC-IIC-ICLI IN DEC TO DBC-IIC-ICLI IN DBC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           IF NOT FIO-STAT-OKS
              MOVE 'SGC'          TO MSG-COD-SIST
              MOVE 'CLI    NEX'   TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC IN MSG TO 
                             WSS-CAI-ICVN-V IN WSS-VARI(WSS-J)
              MOVE ZEROES TO WSS-NUM-ICLI-V IN WSS-VARI(WSS-J)
              MOVE SPACES TO WSS-VRF-ICLI-V IN WSS-VARI(WSS-J)
              MOVE SPACES TO WSS-IND-ICLI-V IN WSS-VARI(WSS-J)
              MOVE SPACES TO WSS-GLS-ICLI-V IN WSS-VARI(WSS-J).

           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
           PERFORM CPT-BLKS
           MOVE CPT-STRN  TO WSS-GLS-RZNS-V IN WSS-VARI(WSS-J).

           MOVE DBC-NUM-ICLI IN DBC TO
                             WSS-NUM-ICLI-V IN WSS-VARI(WSS-J).
           MOVE DBC-VRF-ICLI IN DBC TO
                             WSS-VRF-ICLI-V IN WSS-VARI(WSS-J).
           MOVE DBC-IND-ICLI IN DBC TO
                             WSS-IND-ICLI-V IN WSS-VARI(WSS-J).
           MOVE DBC-GLS-ICLI IN DBC TO
                             WSS-GLS-ICLI-V IN WSS-VARI(WSS-J).

      *    MOVE 'TAB'               TO TAB-COD-SIST.                    
           MOVE 'MC2'               TO TAB-COD-SIST.                    
      *    MOVE 'DPL'               TO TAB-COD-TTAB IN TAB.             
           MOVE 'DCV'               TO TAB-COD-TTAB IN TAB.             
           MOVE CVN-CIC-ICVN IN CVN TO TAB-COD-CTAB IN TAB.
           PERFORM BUS-TAB.
           IF MSG-COD-MENS NOT > SPACES
              MOVE TAB-GLS-DESC TO WSS-GLS-DESC-V IN WSS-VARI(WSS-J)
           ELSE
              MOVE 'SIN DESCRIPCION' TO 
                                   WSS-GLS-DESC-V IN WSS-VARI(WSS-J).

       FIN-CRG-VEC-QCC.
           EXIT.

       CLR-QCC-VEC SECTION.
       INI-CLR-QCC-VEC.
           MOVE SPACES TO WSS-CAI-ICVN-V(WSS-I).
           MOVE ZEROES TO WSS-IIC-ICVN-V(WSS-I).
           MOVE SPACES TO WSS-COD-TREC-V(WSS-I).
           MOVE SPACES TO WSS-IND-SCVN-V(WSS-I).
           MOVE SPACES TO WSS-COD-TIPO-V(WSS-I).
           MOVE SPACES TO WSS-COD-STIP-V(WSS-I).
           MOVE ZEROES TO WSS-NUM-ICLI-V(WSS-I).
           MOVE SPACES TO WSS-VRF-ICLI-V(WSS-I).
           MOVE SPACES TO WSS-IND-ICLI-V(WSS-I).
           MOVE SPACES TO WSS-GLS-ICLI-V(WSS-I).
           MOVE SPACES TO WSS-GLS-DESC-V(WSS-I).
           MOVE SPACES TO WSS-GLS-RZNS-V(WSS-I).
       FIN-CLR-QCC-VEC.
           EXIT.

       PRO-FLD-QUE SECTION.
       INI-PRO-FLD-QUE.
           MOVE 'P' TO WSS-CAI-ICVN-ATR.
           MOVE 'P' TO WSS-IIC-ICVN-ATR.
           MOVE 'P' TO WSS-COD-TREC-ATR.
           MOVE 'P' TO WSS-IND-SCVN-ATR.
           MOVE 'P' TO WSS-COD-TIPO-ATR.
           MOVE 'P' TO WSS-COD-STIP-ATR.
           MOVE 'P' TO WSS-NUM-ICLI-ATR.
           MOVE 'P' TO WSS-VRF-ICLI-ATR.
           MOVE 'P' TO WSS-IND-ICLI-ATR.
           MOVE 'P' TO WSS-GLS-ICLI-ATR.
           MOVE 'P' TO WSS-GLS-DESC-ATR.
           MOVE 'P' TO WSS-GLS-RZNS-ATR.
       FIN-PRO-FLD-QUE.
           EXIT.

       EVAL-PROD SECTION.
       INI-EVAL-PROD.
           IF DPC-COD-SIST IN QCC-FLD > SPACES AND
              DPC-COD-SIST IN QCC-FLD NOT = DPC-COD-SIST IN DPC
               MOVE 'N' TO WSS-IND-SELE IN WSS-VARI
               GO TO FIN-EVAL-PROD.

           IF DPC-COD-VCOC IN QCC-FLD > SPACES AND
              DPC-COD-VCOC IN QCC-FLD NOT = DPC-COD-VCOC IN DPC
               MOVE 'N' TO WSS-IND-SELE IN WSS-VARI
               GO TO FIN-EVAL-PROD.

           IF DPC-COD-TOOC IN QCC-FLD > SPACES AND
              DPC-COD-TOOC IN QCC-FLD NOT = DPC-COD-TOOC IN DPC
               MOVE 'N' TO WSS-IND-SELE IN WSS-VARI
               GO TO FIN-EVAL-PROD.

      *PVG INI 18-OCT-2010
           IF DPC-IND-TPLN IN QCC-FLD > SPACES AND
              DPC-IND-TPLN IN DPC NOT = DPC-IND-TPLN IN QCC-FLD
              MOVE 'N' TO WSS-IND-SELE IN WSS-VARI
              GO TO FIN-EVAL-PROD.
      *PVG FIN 18-OCT-2010

           IF DPC-COD-AOOC IN QCC-FLD > SPACES AND
              DPC-COD-AOOC IN DPC > SPACES AND                          
              DPC-COD-AOOC IN QCC-FLD NOT = DPC-COD-AOOC IN DPC
               MOVE 'N' TO WSS-IND-SELE IN WSS-VARI
               GO TO FIN-EVAL-PROD.
       FIN-EVAL-PROD.
           EXIT.

       ANL-PLN-PRD SECTION.
       INI-ANL-PLN-PRD.
           MOVE DPC-COD-GRPO IN DPC TO PLN-CIC-PLAN IN PLN.
           MOVE HOY-STP-ITRN        TO PLN-STP-FINI IN PLN.
           MOVE FIO-GET-LEQ TO FIO-CMND.
       LUP-LEE-PLN.
           PERFORM TAB-FIO-PLN.
           MOVE FIO-STAT TO PGM-STAT-PLN.
           IF NOT ( FIO-STAT-OKS AND
                    PLN-CIC-PLAN IN PLN = DPC-COD-GRPO IN DPC )
               GO TO FIN-ANL-PLN-PRD.
           IF PLN-MSC-STAT IN PLN NOT = 'A'
               MOVE FIO-GET-PRV TO FIO-CMND
               GO TO LUP-LEE-PLN.

           MOVE PLN-CIC-IPRD IN PLN TO PRD-CIC-IPRD IN PRD.
           MOVE ZEROES              TO PRD-NUM-IPRD IN PRD.
           MOVE FIO-GET-NLS TO FIO-CMND.
       LUP-LEE-PRD.
           PERFORM TAB-FIO-PRD.
           MOVE FIO-STAT TO PGM-STAT-PRD.
           IF NOT ( FIO-STAT-OKS AND
                    PRD-CIC-IPRD IN PRD = PLN-CIC-IPRD IN PLN )
               GO TO FIN-ANL-PLN-PRD.

           IF DPC-COD-SIST IN QCC-FLD > SPACES AND
              DPC-COD-SIST IN QCC-FLD NOT = PRD-COD-SIST IN PRD
               GO TO NXT-LEE-PRD.

           IF DPC-COD-VCOC IN QCC-FLD > SPACES AND
              DPC-COD-VCOC IN QCC-FLD NOT = PRD-COD-VCOC IN PRD
               GO TO NXT-LEE-PRD.

           IF DPC-COD-TOOC IN QCC-FLD > SPACES AND
              DPC-COD-TOOC IN QCC-FLD NOT = PRD-COD-TOOC IN PRD
               GO TO NXT-LEE-PRD.

           IF DPC-COD-AOOC IN QCC-FLD > SPACES AND
              DPC-COD-AOOC IN QCC-FLD NOT = PRD-COD-AOOC IN PRD
               GO TO NXT-LEE-PRD.

      *PVG INI 18-OCT-2010
           IF DPC-IND-TPLN IN QCC-FLD > SPACES AND
              DPC-IND-TPLN IN DPC NOT = DPC-IND-TPLN IN QCC-FLD
              GO TO NXT-LEE-PRD.
      *PVG FIN 18-OCT-2010

           MOVE 'S' TO WSS-IND-SELE IN WSS-VARI.
           GO TO FIN-ANL-PLN-PRD.

       NXT-LEE-PRD.
           MOVE FIO-GET-NXT TO FIO-CMND.
           GO TO LUP-LEE-PRD.
       FIN-ANL-PLN-PRD.
           IF PGM-STAT-PLN-OKS
              MOVE FIO-END-BRW TO FIO-CMND
              PERFORM TAB-FIO-PLN.
           IF PGM-STAT-PRD-OKS
              MOVE FIO-END-BRW TO FIO-CMND
              PERFORM TAB-FIO-PRD.
       EXI-ANL-PLN-PRD.
           EXIT.

      *>>>> EOF
*% END
