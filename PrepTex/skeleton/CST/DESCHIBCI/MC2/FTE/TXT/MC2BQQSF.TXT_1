*%! Codigo adicional
*% IF FIN_KEY_SFC_QSF
      *<<<< FIN_KEY_SFC_QSF
           MOVE 240000                  TO SFC-HRA-HINI IN SFC.
      *>>>> FIN_KEY_SFC_QSF
*% END
*% IF INI_FST_KEY_QSF
      *<<<< INI_FST_KEY_QSF
           MOVE FRM-POS-CURS TO FRM-COD-IROL-LEN IN QSF-FLD.
           GO TO FIN-FST-KEY-QSF.
      *>>>> INI_FST_KEY_QSF
*% END
*% IF INI_FRM_COD_IROL
      *<<<< INI_FRM_COD_IROL
           IF FRM-COD-IROL IN QSF-FLD NOT > SPACES AND
              FRM-COD-USER IN QSF-FLD > SPACES
              GO TO FIN-VAL-FRM-COD-IROL.

      *>>>> INI_FRM_COD_IROL
*% END
*% IF FRM_COD_IROL
      *<<<< FRM_COD_IROL
           MOVE 'MC2'                   TO TAB-COD-SIST.
           MOVE 'RDF'                   TO TAB-COD-TTAB IN TAB.
           MOVE FRM-COD-IROL IN QSF-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE FRM-SUAR-MAL TO FRM-SUAR
              GO TO FIN-VAL-FRM-COD-IROL.
      *>>>> FRM_COD_IROL
*% END
*% IF INI_SFC_FEC_FINI
      *<<<< INI_SFC_FEC_FINI
           IF SFC-FEC-FINI IN QSF-FLD  NOT > ZEROES
              MOVE HOY-DHOY IN HOY-VARI TO SFC-NUM-DINI IN QSF-FLD
              MOVE HOY-MHOY IN HOY-VARI TO SFC-NUM-MINI IN QSF-FLD
              MOVE HOY-SHOY IN HOY-VARI TO SFC-NUM-SINI IN QSF-FLD
              MOVE HOY-AHOY IN HOY-VARI TO SFC-NUM-AINI IN QSF-FLD
              GO TO FIN-VAL-SFC-FEC-FINI.
           MOVE FEC-CHOY-N  TO FEC-CHOY.
           MOVE FEC-VHBL-N TO FEC-VHBL.
      *>>>> INI_SFC_FEC_FINI
*% END
*% IF SFC_COD_SIST
      *<<<< SFC_COD_SIST
           IF SFC-COD-SIST IN QSF-FLD NOT > SPACES
              MOVE WSS-IND-SIST IN WSS-VARI TO SFC-COD-SIST IN QSF-FLD
              GO TO FIN-VAL-SFC-COD-SIST.

      *VAL-COD Valida codigo de tabla
           MOVE 'TAB'                   TO TAB-COD-SIST.
           MOVE 'SIS'                   TO TAB-COD-TTAB IN TAB.
           MOVE SFC-COD-SIST IN QSF-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>> SFC_COD_SIST
*% END
*% IF INI_SFC_COD_EVEN
      *<<<< INI_SFC_COD_EVEN
           IF SFC-COD-EVEN IN QSF-FLD NOT > SPACES AND
              SFC-COD-SIST IN QSF-FLD = WSS-IND-SIST IN WSS-VARI
              MOVE WSS-IND-EVTO IN WSS-VARI TO SFC-COD-EVEN IN QSF-FLD
              GO TO FIN-VAL-SFC-COD-EVEN.
      *>>>> INI_SFC_COD_EVEN
*% END
*% IF FIN_SFC_COD_EVEN
      *<<<< FIN_SFC_COD_EVEN
      *VAL-COD Valida codigo de tabla
           IF SFC-COD-EVEN IN QSF-FLD > SPACES
              MOVE 'TAB'                    TO TAB-COD-SIST
              MOVE 'EVT'                    TO TAB-COD-TTAB IN TAB
              MOVE SFC-COD-SIST IN QSF-FLD  TO WSS-COD-SIST IN WSS-VARI
              MOVE SFC-COD-EVEN IN QSF-FLD  TO WSS-COD-EVTO IN WSS-VARI
              MOVE WSS-COD-EVEN IN WSS-VARI TO TAB-COD-CTAB IN TAB
              PERFORM VAL-COD
              IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>> FIN_SFC_COD_EVEN
*% END
*% IF FIN_FRM_COD_BNCO
      *<<<< FIN_FRM_COD_BNCO
      *VAL-COD Valida codigo de tabla
           IF FRM-COD-BNCO IN QSF-FLD > SPACES
              MOVE 'MC2'                   TO TAB-COD-SIST
              MOVE 'FXB'                   TO TAB-COD-TTAB IN TAB
              MOVE FRM-COD-BNCO IN QSF-FLD TO TAB-COD-CTAB IN TAB
              PERFORM VAL-COD
              IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>> FIN_FRM_COD_BNCO
*% END
*% IF FIN_FRM_COD_CEPL
      *<<<< FIN_FRM_COD_CEPL
      *VAL-COD Valida codigo de tabla
           IF FRM-COD-CEPL IN QSF-FLD > SPACES
              MOVE 'MC2'                   TO TAB-COD-SIST
              MOVE 'JPD'                   TO TAB-COD-TTAB IN TAB
              MOVE FRM-COD-CEPL IN QSF-FLD TO TAB-COD-CTAB IN TAB
              PERFORM VAL-COD
              IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>> FIN_FRM_COD_CEPL
*% END
*% IF FIN_SFC_COD_BANC
      *<<<< FIN_SFC_COD_BANC
      *VAL-COD Valida codigo de tabla
           IF SFC-COD-BANC IN QSF-FLD > SPACES
              MOVE 'MC2'                   TO TAB-COD-SIST
              MOVE 'AGB'                   TO TAB-COD-TTAB IN TAB
              MOVE SFC-COD-BANC IN QSF-FLD TO TAB-COD-CTAB IN TAB
              PERFORM VAL-COD
              IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>> FIN_SFC_COD_BANC
*% END
*% IF FIN_SFC_COD_OFIC
      *<<<< FIN_SFC_COD_OFIC
      *VAL-COD Valida codigo de tabla
           IF SFC-COD-OFIC IN QSF-FLD > SPACES
              MOVE SFC-COD-OFIC IN QSF-FLD TO OFI-COD-OFIC IN OFI
              PERFORM VAL-OFI
              IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>> FIN_SFC_COD_OFIC
*% END
*% IF FIN_SFC_COD_REGN
      *<<<< FIN_SFC_COD_REGN
      *VAL-COD Valida codigo de tabla
           IF SFC-COD-REGN IN QSF-FLD > SPACES
              MOVE 'TAB'                   TO TAB-COD-SIST
              MOVE 'RGL'                   TO TAB-COD-TTAB IN TAB
              MOVE SFC-COD-REGN IN QSF-FLD TO TAB-COD-CTAB IN TAB
              PERFORM VAL-COD
              IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>> FIN_SFC_COD_REGN
*% END
*% IF INI_FRM_FEC_FINI
      *<<<< INI_FRM_FEC_FINI
           MOVE FEC-CHOY-N  TO FEC-CHOY.
           MOVE FEC-VHBL-N TO FEC-VHBL.
      *>>>> INI_FRM_FEC_FINI
*% END
