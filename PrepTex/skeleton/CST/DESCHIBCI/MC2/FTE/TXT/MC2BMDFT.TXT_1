*% IF FIN_PUT_CFT_DFT
      *<<<< FIN_PUT_CFT_DFT                                             
           IF CFT-COD-SIST IN DFT-FLD > SPACES                          
              MOVE 'TAB'                    TO TAB-COD-SIST             
              MOVE 'SIS'                    TO TAB-COD-TTAB IN TAB      
              MOVE CFT-COD-SIST IN DFT-FLD  TO TAB-COD-CTAB IN TAB      
              PERFORM BUS-TAB                                           
              MOVE TAB-GLS-DCOR IN TAB      TO FRM-GLS-SIST IN DFT-FLD  
           ELSE                                                         
              MOVE SPACES                   TO FRM-GLS-SIST IN DFT-FLD. 
           IF CFT-COD-EVEN IN DFT-FLD > SPACES                          
              MOVE 'TAB'                    TO TAB-COD-SIST             
              MOVE 'EVT'                    TO TAB-COD-TTAB IN TAB      
              MOVE CFT-COD-SIST IN DFT-FLD  TO WSS-COD-SIST IN WSS-VARI 
              MOVE CFT-COD-EVEN IN DFT-FLD  TO WSS-COD-EVTO IN WSS-VARI 
              MOVE WSS-COD-EVEN IN WSS-VARI TO TAB-COD-CTAB IN TAB      
              PERFORM BUS-TAB                                           
              MOVE TAB-GLS-DCOR IN TAB      TO FRM-GLS-EVEN IN DFT-FLD  
           ELSE                                                         
              MOVE SPACES                   TO FRM-GLS-EVEN IN DFT-FLD. 
           IF CFT-IND-TPLT IN DFT-FLD > SPACES                          
              MOVE 'TAB'                    TO TAB-COD-SIST             
              MOVE 'TDP'                    TO TAB-COD-TTAB IN TAB      
              MOVE CFT-IND-TPLT IN DFT-FLD  TO TAB-COD-CTAB IN TAB      
              PERFORM BUS-TAB                                           
              MOVE TAB-GLS-DCOR IN TAB      TO FRM-GLS-TPLT IN DFT-FLD  
           ELSE                                                         
              MOVE SPACES                   TO FRM-GLS-TPLT IN DFT-FLD. 
           IF CFT-COD-VCOC IN DFT-FLD > SPACES                          
              MOVE 'TAB'                    TO TAB-COD-SIST             
              MOVE 'VLR'                    TO TAB-COD-TTAB IN TAB      
              MOVE CFT-COD-VCOC IN DFT-FLD  TO TAB-COD-CTAB IN TAB      
              PERFORM BUS-TAB                                           
              MOVE TAB-GLS-DCOR IN TAB      TO FRM-GLS-VCOC IN DFT-FLD  
           ELSE                                                         
              MOVE SPACES                   TO FRM-GLS-VCOC IN DFT-FLD. 
           IF CFT-IND-VIGE IN DFT-FLD > SPACES                          
              MOVE 'GNS'                    TO TAB-COD-SIST             
              MOVE 'STT'                    TO TAB-COD-TTAB IN TAB      
              MOVE CFT-IND-VIGE IN DFT-FLD  TO TAB-COD-CTAB IN TAB      
              PERFORM BUS-TAB                                           
              MOVE TAB-GLS-DCOR IN TAB      TO FRM-GLS-VIGE IN DFT-FLD  
           ELSE                                                         
              MOVE SPACES                   TO FRM-GLS-VIGE IN DFT-FLD. 
           IF CFT-MSC-UTPT IN DFT-FLD > SPACES                          
              MOVE 'TAB'                   TO MSC-COD-SIST
              MOVE 'IPER'                  TO MSC-COD-TMSC IN MSC
              MOVE CFT-MSC-UTPT IN DFT-FLD TO MSC-COD-CMSC IN MSC
              PERFORM BUS-MSC
              MOVE MSC-GLS-DCOR IN MSC     TO FRM-GLS-UTPT IN DFT-FLD
           ELSE                                                         
              MOVE SPACES                  TO FRM-GLS-UTPT IN DFT-FLD. 
      *>>>>                                                             
*% END
*% IF FIN_PUT_DFT_DFT
      *<<<< FIN_PUT_DFT_CFT
           IF DFT-COD-ROLF IN DFT-FLD > SPACES                          
              MOVE 'MC2'                    TO TAB-COD-SIST             
              MOVE 'RXF'                    TO TAB-COD-TTAB IN TAB      
              MOVE DFT-COD-ROLF IN DFT-FLD  TO TAB-COD-CTAB IN TAB      
              PERFORM BUS-TAB                                           
              MOVE TAB-GLS-DESC IN TAB      TO FRM-GLS-ROLF IN DFT-FLD  
           ELSE                                                         
              MOVE SPACES                   TO FRM-GLS-ROLF IN DFT-FLD. 
      *>>>>
*% END
*% IF DFT_COD_ROLF
      *<<<< DFT_COD_ROLF
           MOVE 'MC2'                   TO TAB-COD-SIST.                
           MOVE 'RXF'                   TO TAB-COD-TTAB IN TAB.         
           MOVE DFT-COD-ROLF IN DFT-FLD TO TAB-COD-CTAB IN TAB.         
           PERFORM VAL-COD.                                             
           IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'               
              MOVE SPACES       TO FRM-GLS-ROLF IN DFT-FLD              
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              MOVE FRM-SUAR-MAL TO FRM-SUAR                             
           ELSE                                                         
              MOVE TAB-GLS-DCOR IN TAB TO FRM-GLS-ROLF IN DFT-FLD.
      *>>>>
*% END
*% IF FIN_CLR_KEY_CFT
      *<<<<
      *>>>>
*% END
*% IF DFT_COD_MDFT
      *<<<< DFT_COD_MDFT
           IF DFT-COD-MDFT IN DFT-FLD NOT > SPACES AND
              DFT-SGV-MDFT IN DFT-FLD > ZEROES
              PERFORM ERR-GLS
              GO TO FIN-VAL-DFT-COD-MDFT.
           IF DFT-COD-MDFT IN DFT-FLD > SPACES
              MOVE 'TAB'                   TO TAB-COD-SIST
              MOVE 'VLR'                   TO TAB-COD-TTAB IN TAB
              MOVE DFT-COD-MDFT IN DFT-FLD TO TAB-COD-CTAB IN TAB
              PERFORM VAL-COD
              IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>> DFT_COD_MDFT
*% END
*% IF DFT_CIC_IDFT
      *<<<< DFT_CIC_IDFT 
           MOVE DFT-CIC-IDFT IN DFT-FLD TO CFT-CIC-IDFT IN CFT.
           MOVE 'CFT-CIC-IDFT' TO FIO-AKEY.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM MC2-FIO-CFT.
           MOVE FIO-STAT TO PGM-STAT-CFT.
           IF NOT FIO-STAT-OKS
              MOVE 'MC2'       TO MSG-COD-SIST
              MOVE 'CFT   NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE FRM-SUAR-MAL TO FRM-SUAR
              GO TO FIN-VAL-DFT-CIC-IDFT.

           IF SCR-CMND = 'CON'
              GO TO FIN-VAL-DFT-CIC-IDFT.

           IF CFT-MSC-STAT IN CFT = 'A'
              MOVE 'MC2'       TO MSG-COD-SIST
              MOVE 'CFT   ACA' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE FRM-SUAR-MAL TO FRM-SUAR
              GO TO FIN-VAL-DFT-CIC-IDFT.
           IF CFT-MSC-STAT IN CFT = 'D'
              MOVE 'MC2'       TO MSG-COD-SIST
              MOVE 'CFT   ANL' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE FRM-SUAR-MAL TO FRM-SUAR
              GO TO FIN-VAL-DFT-CIC-IDFT.
      *>>>> DFT_CIC_IDFT
*% END
