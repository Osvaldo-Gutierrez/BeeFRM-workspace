              MOVE 'P'    TO PPC-IND-TPLT IN PPC-FLD                    
              GO TO FIN-VAL-PPC-IND-TPLT.                               
           MOVE 'TAB'                    TO TAB-COD-SIST.               
           MOVE 'TDP'                    TO TAB-COD-TTAB IN TAB.        
           MOVE PPC-IND-TPLT IN PPC-FLD  TO TAB-COD-CTAB IN TAB.        
           PERFORM VAL-TAB.                                             
           IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'               
              MOVE 'GNS'        TO MSG-COD-SIST                         
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC TO FRM-MEN2                             
              MOVE 'TAB'        TO MSG-COD-SIST                         
              MOVE 'ERR    TDP' TO MSG-COD-MENS                         
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC IN MSG TO FRM-MEN1                      
              MOVE FRM-SUAR-MAL TO FRM-SUAR.                            
                                                                        
           IF PPC-IND-TPLT IN PPC-FLD NOT = 'P'                         
              MOVE 'TAB'        TO MSG-COD-SIST                         
              MOVE 'ERR    TDP' TO MSG-COD-MENS                         
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC TO FRM-MEN1                             
              MOVE FRM-SUAR-MAL TO FRM-SUAR.                            
      *>>>>                                                             
*% END
*% IF INI_PPC_FEC_FINI
      *<<<< INI_PPC_FEC_FINI                                            
           MOVE FEC-VHBL-N TO FEC-VHBL.                                 
           MOVE FEC-CHOY-N TO FEC-CHOY.                                 
           IF SCR-CMND = 'ING' OR 'MOD' OR 'VRF' OR 'ACA'               
              MOVE FEC-CHOY-GE TO FEC-CHOY.                             
                                                                        
           IF PPC-FEC-FINI IN PPC-FLD NOT > ZEROES                      
              PERFORM GET-FHOY                                          
              MOVE HOY-DHOY IN HOY-VARI TO PPC-NUM-DINI IN PPC-FLD      
              MOVE HOY-MHOY IN HOY-VARI TO PPC-NUM-MINI IN PPC-FLD      
              MOVE HOY-SHOY IN HOY-VARI TO PPC-NUM-SINI IN PPC-FLD      
              MOVE HOY-AHOY IN HOY-VARI TO PPC-NUM-AINI IN PPC-FLD      
              IF PPC-HRA-HINI IN PPC-FLD NOT > ZEROES                   
                 MOVE HOY-HHOY IN HOY-VARI TO PPC-HRA-HINI IN PPC-FLD   
                 GO TO FIN-VAL-PPC-FEC-FINI                             
              ELSE                                                      
                 GO TO FIN-VAL-PPC-FEC-FINI.                            
      *>>>>                                                             
*% END
