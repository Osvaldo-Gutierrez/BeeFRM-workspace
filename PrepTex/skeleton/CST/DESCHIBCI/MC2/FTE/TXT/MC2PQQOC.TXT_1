*% FMT_CHI = TRUE
*% GEN_BCI = TRUE
*% PGM_PTC = TRUE
*% PGM_STAT_ROC = TRUE
*% IF CMT_ID
      *<<<<
      *              24-JUN-2009 CONSULTA REGISTROS ANTERIORES          
      *>>>>
*% END
*% IF WSS
      *<<<< WSS
       COPY MC2WXQOC.
       COPY GNSWGCPT.
       COPY GNSWGSTR.
       COPY MC2BRCVN.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CVN-REQA==.
       COPY COLBROPC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OPC-REQA==.
       COPY COLBRRDC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RDC-REQA==.
       COPY COLBRVEN.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-VEN-REQA==.
       COPY SGCBRDBC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
       01  WSS-NUM-CCUR.
           03 WSS-NUM-ICUR-EDT  PIC  ZZ9.
           03 WSS-NUM-ICUR-ALF       PIC X(03).
           03 WSS-NUM-ICUR      REDEFINES
              WSS-NUM-ICUR-ALF       PIC 9(03).

*% END
*% IF FIN_SCR_VARI
           03 SCR-NUM-PANT               PIC 9(0006).
           03 SCR-QOC-FLD                PIC X(1100).
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-OFRM-SRV
                MOVE TSK-TERM-ALF TO QOC-COD-TERM
                MOVE TSK-TERM-ALF TO QOC-COD-TERM-MAS
           ELSE
                MOVE EIBTRMID     TO QOC-COD-TERM
                MOVE EIBTRMID     TO QOC-COD-TERM-MAS.
           MOVE +3859 TO SCR-TCMA.
           PERFORM GNS-ERR-QUE.
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )
               MOVE SCR-QOC-FLD TO QOC-FLD
               GO TO FIN-INI.

           IF ( FRM-FFLD = FRM-FFLD-PF6 )
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM
               ADD 1              TO QUE-NITM
               MOVE QOC-NOM-TSQOC TO QUE-COLA
               MOVE QOC-LEN-TSQOC TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT NOT = QUE-STAT-OKS
                  MOVE SCR-QOC-FLD TO QOC-FLD
                  GO TO FIN-INI.

           PERFORM GET-FHOY.
      *>>>>
*% END
*% IF INI_SCR_COM
      *<<<< INI_SCR_COM
      *    MOVE QOC-NOM-TSQOC TO QUE-COLA.                              
      *    MOVE QUE-DEL       TO QUE-CMND.                              
      *    PERFORM GNS-PRO-QUE.                                         
      *                                                                 
      *    MOVE QOC-NOM-TSQOC-MAS TO QUE-COLA.                          
      *    MOVE QUE-DEL           TO QUE-CMND.                          
      *    PERFORM GNS-PRO-QUE.                                         
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
      *PVG INI 24-JUN-2009                                              
      *    MOVE CVN-CAI-ICVN IN QOC-FLD TO ROC-CAI-PLAN IN ROC.         
      *    MOVE CVN-IIC-ICVN IN QOC-FLD TO ROC-IIC-PLAN IN ROC.         
      *    MOVE FIO-GET-NLS TO FIO-CMND.                                
           MOVE CVN-CAI-ICVN IN QOC-FLD TO CVN-CAI-ICVN IN CVN.         
           MOVE CVN-IIC-ICVN IN QOC-FLD TO CVN-IIC-ICVN IN CVN.         
           MOVE 99999999 TO CVN-FEC-FCVN IN CVN.                        
           MOVE 999999   TO CVN-HRA-HRCV IN CVN.                        
           MOVE FIO-GET-LEQ TO FIO-CMND.                                
           PERFORM MC2-FIO-CVN.                                         
           IF NOT (FIO-STAT-OKS AND                                     
                   CVN-CAI-ICVN IN QOC-FLD = CVN-CAI-ICVN IN CVN AND    
                   CVN-IIC-ICVN IN QOC-FLD = CVN-IIC-ICVN IN CVN)       
              MOVE 'CVN    NEX' TO MSG-COD-MENS                         
              GO TO MSG-FND-KEY.                                        
           MOVE FIO-END-BRW TO FIO-CMND.                                
           PERFORM MC2-FIO-CVN.                                         
           MOVE CVN-CAI-ICVN IN QOC-FLD TO ROC-CAI-PLAN IN ROC.         
           MOVE CVN-IIC-ICVN IN QOC-FLD TO ROC-IIC-PLAN IN ROC.         
           IF FRM-IND-SGTE IN QOC-FLD = 'S' OR 'A'                      
              PERFORM BUS-ROC-KALT                                      
              IF MSG-COD-MENS IN MSG > SPACES                           
                 GO TO MSG-FND-KEY                                      
              ELSE                                                      
              IF FRM-IND-SGTE IN QOC-FLD = 'S'                          
                 MOVE FIO-GET-NXT TO FIO-CMND                           
              ELSE                                                      
                 MOVE FIO-GET-PRV TO FIO-CMND.                          
                                                                        
           IF NOT (FRM-IND-SGTE IN QOC-FLD = 'S' OR 'A')                
              MOVE FIO-GET-NLS TO FIO-CMND.
      *PVG FIN 24-JUN-2009                                              
       LUP-LEE-ROC.
           MOVE 'ROC-CIC-PLAN' TO FIO-AKEY.                             
           PERFORM TAB-FIO-ROC.
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF
               GO TO CON-FND-KEY.

           IF NOT (ROC-CAI-PLAN IN ROC = CVN-CAI-ICVN IN QOC-FLD AND        
                   ROC-IIC-PLAN IN ROC = CVN-IIC-ICVN IN QOC-FLD)           
               GO TO CON-FND-KEY.

           IF ROC-COD-CPTO IN ROC NOT = 'CVN'                           
              GO TO NXT-LEE-ROC.                                        
                                                                        
      *PVG INI 24-JUN-2009                                              
      *    IF FRM-IND-SGTE IN QOC-FLD = 'S' AND                         
      *       FRM-CAI-IOPK IN QOC-FLD NOT = ROC-CAI-IOPC IN ROC AND     
      *       FRM-IIC-IOPK IN QOC-FLD NOT = ROC-IIC-IOPC IN ROC AND     
      *       FRM-EXT-IOPK IN QOC-FLD NOT = ROC-EXT-IOPC IN ROC         
      *        GO TO CON-FND-KEY.                                       
      *PVG FIN 24-JUN-2009                                              

      * Busca La OPC en Vector
           PERFORM CHK-ROC VARYING ROC-I FROM 1 BY 1 UNTIL
             ROC-I > ROC-IMAX OR
            (ROC-CAI-IOPC IN ROC = QOC-CAI-IOPC IN WSS-VARI(ROC-I) AND
             ROC-IIC-IOPC IN ROC = QOC-IIC-IOPC IN WSS-VARI(ROC-I)). 

      * Encuentra la OPC
           IF ROC-I NOT > ROC-IMAX
               GO TO NXT-LEE-ROC. 

      * No encuentra la OPC
           ADD 1 TO ROC-IMAX.
           IF ROC-IMAX > ROC-MAX-IMAX
               MOVE 'VECROC OVRFL'  TO MSG-COD-MENS
               GO TO MSG-FND-KEY.

      * Carga OPC en Vector
           MOVE ROC-CAI-IOPC IN ROC TO QOC-CAI-IOPC 
                                           IN WSS-VARI(ROC-IMAX).
           MOVE ROC-IIC-IOPC IN ROC TO QOC-IIC-IOPC 
                                           IN WSS-VARI(ROC-IMAX).
         *PVG INI 24-JUN-2009                                              
      *    MOVE CVN-CAI-ICVN IN QOC-FLD TO CVN-CAI-ICVN IN CVN.         
      *    MOVE CVN-IIC-ICVN IN QOC-FLD TO CVN-IIC-ICVN IN CVN.         
      *    MOVE 99999999 TO CVN-FEC-FCVN IN CVN.                        
      *    MOVE 999999   TO CVN-HRA-HRCV IN CVN.                        
      *    MOVE FIO-GET-LEQ TO FIO-CMND.                                
      *    PERFORM MC2-FIO-CVN.                                         
      *    IF NOT (FIO-STAT-OKS AND                                     
      *       CVN-CAI-ICVN IN QOC-FLD = CVN-CAI-ICVN IN CVN AND         
      *       CVN-IIC-ICVN IN QOC-FLD = CVN-IIC-ICVN IN CVN)            
      *        GO TO NXT-LEE-ROC.                                       
      *PVG FIN 24-JUN-2009                                              
           MOVE FRM-NUM-DLIN IN QOC-FLD TO WSS-NUM-DINI IN WSS-VARI.
           MOVE FRM-NUM-MLIN IN QOC-FLD TO WSS-NUM-MINI IN WSS-VARI.
           MOVE FRM-NUM-SLIN IN QOC-FLD TO WSS-NUM-SINI IN WSS-VARI.
           MOVE FRM-NUM-ALIN IN QOC-FLD TO WSS-NUM-AINI IN WSS-VARI.
           MOVE FRM-NUM-DLFN IN QOC-FLD TO WSS-NUM-DTER IN WSS-VARI.
           MOVE FRM-NUM-MLFN IN QOC-FLD TO WSS-NUM-MTER IN WSS-VARI.
           MOVE FRM-NUM-SLFN IN QOC-FLD TO WSS-NUM-STER IN WSS-VARI.
           MOVE FRM-NUM-ALFN IN QOC-FLD TO WSS-NUM-ATER IN WSS-VARI.

           MOVE ROC-CAI-IOPC IN ROC TO OPC-CAI-IOPC IN OPC.
           MOVE ROC-IIC-IOPC IN ROC TO OPC-IIC-IOPC IN OPC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-OPC.
           IF NOT FIO-STAT-OKS
               GO TO NXT-LEE-ROC.
           
           IF OPC-MSC-STAT IN QOC-FLD > SPACES AND
              OPC-MSC-STAT IN QOC-FLD NOT = OPC-MSC-STAT IN OPC
               GO TO NXT-LEE-ROC.

           IF OPC-COD-OFOC IN QOC-FLD > SPACES AND
              OPC-COD-OFOC IN QOC-FLD NOT = OPC-COD-OFOC IN OPC
               GO TO NXT-LEE-ROC.

           IF WSS-FEC-FINI IN WSS-VARI > ZEROES AND
              WSS-FEC-FINI IN WSS-VARI > OPC-FEC-FCOL IN OPC
               GO TO NXT-LEE-ROC.

           IF WSS-FEC-FTER IN WSS-VARI > ZEROES AND
              WSS-FEC-FTER IN WSS-VARI < OPC-FEC-FCOL IN OPC
               GO TO NXT-LEE-ROC.

           MOVE OPC-CAI-IOPC IN OPC TO VEN-CAI-IOPC IN VEN.
           MOVE OPC-IIC-IOPC IN OPC TO VEN-IIC-IOPC IN VEN.
           MOVE OPC-NUM-IVEN IN OPC TO VEN-NUM-IVEN IN VEN.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-VEN.
           IF NOT FIO-STAT-OKS
               GO TO NXT-LEE-ROC.

           MOVE OPC-CAI-IOPC IN OPC TO RDC-CAI-IOPC IN RDC.
           MOVE OPC-IIC-IOPC IN OPC TO RDC-IIC-IOPC IN RDC.
           MOVE 0                   TO RDC-NUM-IDLC IN RDC.
           MOVE 'RDC-KEY-IDLC' TO FIO-AKEY.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-RDC.
           IF NOT FIO-STAT-OKS
               GO TO NXT-LEE-ROC.

           MOVE RDC-CAI-ICLI IN RDC TO DBC-CAI-ICLI IN DBC.
           MOVE RDC-IIC-ICLI IN RDC TO DBC-IIC-ICLI IN DBC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           IF NOT FIO-STAT-OKS
               GO TO NXT-LEE-ROC.

           PERFORM CRG-VEC-QOC.

           IF (SCR-OFRM-SRV       AND WSS-J > WSS-NITMMAX)
              GO TO CON-FND-KEY.

           IF ((NOT SCR-OFRM-SRV) AND WSS-J > WSS-JMAX)
              GO TO CON-FND-KEY.

       NXT-LEE-ROC.
      *PVG INI 24-JUN-2009                                              
           IF FRM-IND-SGTE IN QOC-FLD = 'A'                             
              MOVE FIO-GET-PRV TO FIO-CMND                              
           ELSE                                                         
      *PVG FIN 24-JUN-2009                                              
              MOVE FIO-GET-NXT TO FIO-CMND.
           GO TO LUP-LEE-ROC.
      *>>>>
*% END

*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
       CON-FND-KEY.
           PERFORM PUT-TAB.

           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *----------------------------------------------------------
       MSG-FND-KEY.
           MOVE 'TAB' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM VAL-NUM-KEY-QOC
               PERFORM VAL-NUM-FLD-QOC
               PERFORM SCRL-TAB-QOC
               PERFORM SCR-TRL.
      *>>>>
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBBTAB.
       COPY GNSBVTAB.
       COPY GNSBVCOD.

       PUT-TAB SECTION.
       INI-PUT-TAB.
           MOVE QOC-NOM-TSQOC     TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE QOC-NOM-TSQOC-MAS TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE 1 TO SCR-NUM-PANT.
           MOVE LOW-VALUES TO WSS-GLS-NQOC-IDX-MTI
           MOVE LOW-VALUES TO WSS-GLS-NQOC-IDX-FRM.
      *
           MOVE 1 TO FRM-IFLD.
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.

           MOVE 1 TO WSS-F IN WSS-VARI.
      *PVG INI 24-JUN-2009                                              
           IF FRM-IND-SGTE IN QOC-FLD = 'A'                             
              MOVE WSS-J TO WSS-F IN WSS-VARI                           
              IF WSS-F IN WSS-VARI NOT > ZEROES                         
                 MOVE 'S'   TO SCR-IERR                                 
                 MOVE 'TAB' TO MSG-COD-SIST                             
                 MOVE 'QOC    NEX' TO MSG-COD-MENS                      
                 PERFORM GET-MSG                                        
                 MOVE MSG-GLS-DESC TO FRM-MENS                          
                 MOVE '00' TO FIO-STAT                                  
                 GO TO FIN-PUT-TAB.                                     
      *PVG FIN 24-JUN-2009                                              

       LUP-PUT-TAB.
      *PVG INI 24-JUN-2009                                              
      *    IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI                  
           IF ((WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI AND            
                FRM-IND-SGTE IN QOC-FLD NOT = 'A')       OR             
               (WSS-F IN WSS-VARI < 1                    AND            
                FRM-IND-SGTE IN QOC-FLD = 'A'))                         
      *PVG FIN 24-JUN-2009                                              
              GO TO FIN-PUT-TAB.
           IF WSS-IIC-IOPC-V(WSS-F) > ZEROES
              PERFORM PUT-TAB-QOC
              PERFORM PUT-QOC-QUE
              PERFORM SET-FRM-IFLD
           ELSE
           IF FRM-IFLD = 1 AND WSS-F = 1
              MOVE 'S' TO SCR-IERR                                      
              MOVE 'TAB' TO MSG-COD-SIST
              MOVE 'QOC    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE '00' TO FIO-STAT
              GO TO FIN-PUT-TAB
           ELSE
              MOVE '00' TO FIO-STAT
              GO TO FIN-PUT-TAB.

      *PVG INI 24-JUN-2009                                              
           IF FRM-IND-SGTE IN QOC-FLD = 'A'                             
               SUBTRACT 1 FROM WSS-F IN WSS-VARI                        
           ELSE                                                         
      *PVG FIN 24-JUN-2009                                              
               ADD 1 TO WSS-F IN WSS-VARI.
           GO TO LUP-PUT-TAB.
       FIN-PUT-TAB.
           MOVE 1                  TO QUE-NITM.
           MOVE QOC-LEN-TSQOC-MAS  TO QUE-LITM.
           MOVE QOC-NOM-TSQOC-MAS  TO QUE-COLA.

           IF WSS-NITM > WSS-NITMMAX
              MOVE 'S'           TO QOC-MAS-IND-HMAS
           ELSE
              MOVE 'N'           TO QOC-MAS-IND-HMAS.

           MOVE WSS-NITM         TO WSS-NUM-ICUR.
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.
           MOVE WSS-NUM-ICUR-ALF TO QOC-MAS-NUM-TOCC.
           MOVE QOC-MAS-ITEM     TO QUE-ITEM.
           MOVE QUE-PUT          TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF WSS-NITM > WSS-IMAX
               MOVE 'TAB' TO MSG-COD-SIST
               MOVE 'QOCTABDMAS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       EXT-PUT-TAB.
           EXIT.

       SET-FRM-IFLD SECTION.
       INI-SET-FRM-IFLD.
           IF SCR-OFRM-SRV
                MOVE 1 TO FRM-IFLD
           ELSE
                ADD 1 TO FRM-IFLD.
       FIN-SET-FRM-IFLD.
           EXIT.
      *--------------------------------------------
       SCRL-TAB-QOC SECTION.
       INI-SCRL-TAB-QOC.
           MOVE SPACES TO MSG-COD-MENS.

           IF FRM-FFLD = FRM-FFLD-PF6
               PERFORM PUT-TAB-ASC
           ELSE
               PERFORM PUT-TAB-DSC.
           IF MSG-COD-MENS > SPACES
               MOVE 'TAB' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-QOC.
           EXIT.

      *--------------------------------------------
       PUT-TAB-QOC SECTION.
       INI-PUT-TAB-QOC.
           PERFORM CLR-WSS.
           MOVE WSS-COD-SIST-V(WSS-F) TO WSS-COD-SIST-QOC IN WSS-VARI.
           MOVE WSS-CAI-IOPC-V(WSS-F) TO WSS-CAI-IOPC-QOC IN WSS-VARI.
           MOVE WSS-IIC-IOPC-V(WSS-F) TO WSS-IIC-IOPC-QOC IN WSS-VARI.
           MOVE WSS-EXT-IOPC-V(WSS-F) TO WSS-EXT-IOPC-QOC IN WSS-VARI.
           MOVE WSS-NUM-ICLI-V(WSS-F) TO WSS-NUM-ICLI-QOC IN WSS-VARI.
           MOVE WSS-VRF-ICLI-V(WSS-F) TO WSS-VRF-ICLI-QOC IN WSS-VARI.  
           MOVE WSS-IND-ICLI-V(WSS-F) TO WSS-IND-ICLI-QOC IN WSS-VARI.
           MOVE WSS-GLS-ICLI-V(WSS-F) TO WSS-GLS-ICLI-QOC IN WSS-VARI.  
           MOVE WSS-NUM-IVEN-V(WSS-F) TO WSS-NUM-IVEN-QOC IN WSS-VARI.
           MOVE WSS-FEC-FVES-V(WSS-F) TO WSS-FEC-FVES-QOC IN WSS-VARI.
           MOVE WSS-MSC-STAT-V(WSS-F) TO WSS-MSC-STAT-QOC IN WSS-VARI.
           MOVE WSS-COD-VCOC-V(WSS-F) TO WSS-COD-VCOC-QOC IN WSS-VARI.

           MOVE WSS-VAL-FINA-V(WSS-F) TO WSS-VAL-FINA-QOC IN WSS-VARI.  
           MOVE WSS-VAL-CRED-V(WSS-F) TO WSS-VAL-CRED-QOC IN WSS-VARI.  
           MOVE WSS-VAL-SCRE-V(WSS-F) TO WSS-VAL-SCRE-QOC IN WSS-VARI.  
                                                                        
           MOVE WSS-COD-SIST-QOC IN WSS-VARI TO WSS-COD-SIST-FRM.
           MOVE WSS-CAI-IOPC-QOC IN WSS-VARI TO WSS-CAI-IOPC-FRM.
           MOVE WSS-IIC-IOPC-QOC IN WSS-VARI TO WSS-IIC-IOPC-FRM.
           MOVE WSS-EXT-IOPC-QOC IN WSS-VARI TO WSS-EXT-IOPC-FRM.
           MOVE WSS-NUM-ICLI-QOC IN WSS-VARI TO WSS-NUM-ICLI-FRM.
           MOVE WSS-VRF-ICLI-QOC IN WSS-VARI TO WSS-VRF-ICLI-FRM.       
           MOVE WSS-IND-ICLI-QOC IN WSS-VARI TO WSS-IND-ICLI-FRM.
           MOVE WSS-GLS-ICLI-QOC IN WSS-VARI TO WSS-GLS-ICLI-FRM.       
           MOVE WSS-NUM-IVEN-QOC IN WSS-VARI TO WSS-NUM-IVEN-FRM.
           MOVE WSS-VAL-FINA-QOC IN WSS-VARI TO WSS-VAL-FINA-FRM.
           MOVE WSS-FEC-FVES-QOC IN WSS-VARI TO WSS-FEC-FVES-FRM.
           MOVE WSS-VAL-CRED-QOC IN WSS-VARI TO WSS-VAL-CRED-FRM.
           MOVE WSS-VAL-SCRE-QOC IN WSS-VARI TO WSS-VAL-SCRE-FRM.
           MOVE WSS-MSC-STAT-QOC IN WSS-VARI TO WSS-MSC-STAT-FRM.
           MOVE WSS-COD-VCOC-QOC IN WSS-VARI TO WSS-COD-VCOC-FRM.

      *PVG INI 24-JUN-2009                                              
           IF FRM-IND-SGTE IN QOC-FLD = 'A' AND                         
              WSS-F IN WSS-VARI = WSS-IMAX                              
              MOVE 1 TO FRM-IFLD.                                       
      *PVG FIN 24-JUN-2009                                              
           IF FRM-IFLD > WSS-IMAX
              GO TO FIN-PUT-TAB-QOC.

           MOVE WSS-COD-SIST-QOC IN WSS-VARI TO
               FRM-COD-SIST IN QOC-FLD(FRM-IFLD).
           MOVE WSS-CAI-IOPC-QOC IN WSS-VARI TO
               FRM-CAI-IOPC IN QOC-FLD(FRM-IFLD).
           MOVE WSS-IIC-IOPC-QOC IN WSS-VARI TO
               FRM-IIC-IOPC IN QOC-FLD(FRM-IFLD).
           MOVE WSS-EXT-IOPC-QOC IN WSS-VARI TO
               FRM-EXT-IOPC IN QOC-FLD(FRM-IFLD).
           MOVE WSS-NUM-ICLI-QOC IN WSS-VARI TO
               FRM-NUM-ICLI IN QOC-FLD(FRM-IFLD).
           MOVE WSS-VRF-ICLI-QOC IN WSS-VARI TO                         
               FRM-VRF-ICLI IN QOC-FLD(FRM-IFLD).                       
           MOVE WSS-IND-ICLI-QOC IN WSS-VARI TO
               FRM-IND-ICLI IN QOC-FLD(FRM-IFLD).
           MOVE WSS-GLS-ICLI-QOC IN WSS-VARI TO                         
               FRM-GLS-ICLI IN QOC-FLD(FRM-IFLD).                       
           MOVE WSS-NUM-IVEN-QOC IN WSS-VARI TO
               FRM-NUM-IVEN IN QOC-FLD(FRM-IFLD).
           MOVE WSS-VAL-FINA-QOC IN WSS-VARI TO
               FRM-VAL-FINA IN QOC-FLD(FRM-IFLD).
           MOVE WSS-FEC-FVES-QOC IN WSS-VARI TO
               FRM-FEC-FVES IN QOC-FLD(FRM-IFLD).
           MOVE WSS-VAL-CRED-QOC IN WSS-VARI TO
               FRM-VAL-CRED IN QOC-FLD(FRM-IFLD).
           MOVE WSS-VAL-SCRE-QOC IN WSS-VARI TO
               FRM-VAL-SCRE IN QOC-FLD(FRM-IFLD).
           MOVE WSS-MSC-STAT-QOC IN WSS-VARI TO
               FRM-MSC-STAT IN QOC-FLD(FRM-IFLD).
           MOVE WSS-COD-VCOC-QOC IN WSS-VARI TO
               FRM-COD-VCOC IN QOC-FLD(FRM-IFLD).
       FIN-PUT-TAB-QOC.
           EXIT.
      *----------------------------------------------------
       PUT-QOC-QUE SECTION.
       INI-PUT-QOC-QUE.
           PERFORM PRO-FLD-QUE.
      *PVG INI                                                          
           IF SCR-OFRM-SRV                                              
              MOVE WSS-VAL-FINA-V(WSS-F) TO FRM-VAL-FINA-EDT            
              MOVE FRM-VAL-FINA-EDT      TO                             
                             WSS-VAL-FINA-ALF IN WSS-GLS-NQOC-IDX-MTI   
              MOVE WSS-VAL-CRED-V(WSS-F) TO FRM-VAL-CRED-EDT            
              MOVE FRM-VAL-CRED-EDT      TO                             
                             WSS-VAL-CRED-ALF IN WSS-GLS-NQOC-IDX-MTI   
              MOVE WSS-VAL-SCRE-V(WSS-F) TO FRM-VAL-SCRE-EDT            
              MOVE FRM-VAL-SCRE-EDT      TO                             
                             WSS-VAL-SCRE-ALF IN WSS-GLS-NQOC-IDX-MTI.  
      *PVG FIN                                                          
           ADD 1 TO WSS-NITM.
           MOVE QOC-NOM-TSQOC    TO QUE-COLA.
           MOVE QOC-LEN-TSQOC    TO QUE-LITM.
           MOVE WSS-NITM         TO QUE-NITM.
           MOVE SPACES           TO QUE-ITEM.
           IF SCR-OFRM-SRV
              MOVE WSS-GLS-NQOC-IDX-MTI TO QUE-ITEM
           ELSE
              MOVE WSS-GLS-NQOC-IDX-FRM TO QUE-ITEM.
           MOVE QUE-PUT  TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
       FIN-PUT-QOC-QUE.
           EXIT.
