      *<<<<
       PGM-EBR-ALL SECTION.                                             
       INI-PGM-EBR-ALL.                                                 
           MOVE 'GNS' TO FIO-SIST.                                      
           MOVE FIO-EBR-ALL TO FIO-CMND.                                
           PERFORM GNS-FIO-TAB.                                         
       FIN-PGM-EBR-ALL.                                                 
           EXIT.                                                        
       COPY MC2BFDCM.
       COPY GNSBGVSM.
       COPY GNSBVTAB.
       COPY GNSBVCOD.
       COPY GNSBBTAB.
       COPY GNSBVMSC.
       COPY GNSBBMSC.
       COPY GNSBFMSC.
      *<<<< EOF_PQ
       BUS-COR-DCM SECTION.
       INI-BUS-COR-DCM.
           MOVE CCM-CIC-ICCM IN CCM TO CCM-CIC-ICCM IN CCM-FLD.
           MOVE CCM-CIC-ICCM IN CCM TO DCM-CIC-IDCM IN DCM.
           MOVE 999                 TO DCM-NUM-IDCM IN DCM.
           MOVE FIO-GET-LEQ TO FIO-CMND.
           PERFORM MC2-FIO-DCM.
           MOVE FIO-STAT TO PGM-STAT-DCM.
           IF NOT ( FIO-STAT-OKS AND
               CCM-CIC-ICCM IN CCM = DCM-CIC-IDCM IN DCM )
               PERFORM KEY-ALL-CCM
               MOVE ZEROES TO DCM-NUM-IDCM IN DCM
           ELSE
               MOVE FIO-END-BRW TO FIO-CMND
               PERFORM MC2-FIO-DCM
               IF SCR-CMND = 'ELI' AND DCM-NUM-IDCM IN CCM-FLD > ZEROES
                  MOVE DCM-STP-ITRN IN DCM TO SCR-UMOD.
      *
      *
           IF SCR-CCPP = 'ING'
              ADD 1                    TO DCM-NUM-IDCM IN DCM
              MOVE DCM-NUM-IDCM IN DCM TO DCM-NUM-IDCM IN CCM-FLD
              MOVE '23' TO FIO-STAT
              IF DCM-NUM-IDCM IN CCM-FLD > 1
                 MOVE 'TAB'               TO TAB-COD-SIST
                 MOVE 'DPL'               TO TAB-COD-TTAB IN TAB
                 MOVE DCM-CIC-IDCM IN DCM TO TAB-COD-CTAB IN TAB
                 PERFORM BUS-TAB
                 MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-DES1 IN CCM-FLD
                 MOVE TAB-GLS-DATA IN TAB TO FRM-GLS-DES2 IN CCM-FLD
                 MOVE CCM-IND-DSCT IN CCM TO CCM-IND-DSCT IN CCM-FLD
                 MOVE CCM-IND-CAAB IN CCM TO CCM-IND-CAAB IN CCM-FLD
                 MOVE '23' TO FIO-STAT.
       FIN-BUS-COR-DCM.
           EXIT.

       SCRL-TAB-CCM SECTION.
       INI-SCRL-TAB-CCM.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF5
               PERFORM SCRL-TAB-CCM-PF7
               GO TO FIN-SCRL-TAB-CCM.
           IF FRM-FFLD = FRM-FFLD-PF8  OR FRM-FFLD-PF6
               PERFORM SCRL-TAB-CCM-PF8
               GO TO FIN-SCRL-TAB-CCM.
       FIN-SCRL-TAB-CCM.
           EXIT.

       SCRL-TAB-CCM-PF7 SECTION.
       INI-SCRL-TAB-CCM-PF7.
           MOVE SCR-DCM-KEY-IDCM TO DCM-KEY-IDCM IN DCM.
           MOVE FIO-GET-LEQ TO FIO-CMND.
           PERFORM MC2-FIO-DCM.
           IF NOT ( FIO-STAT-OKS AND
                    DCM-KEY-IDCM IN DCM = SCR-DCM-KEY-IDCM)
               MOVE 'TABCURRNEX' TO MSG-COD-MENS
               GO TO MSG-SCRL-TAB-CCM-PF7.
       PRV-SCRL-TAB-CCM-PF7.
           MOVE FIO-GET-PRV TO FIO-CMND.
           PERFORM MC2-FIO-DCM.
           IF NOT ( FIO-STAT-OKS AND
                    DCM-CIC-IDCM IN DCM = SCR-DCM-CIC-IDCM )
               MOVE 'TAB    NOPRV' TO MSG-COD-MENS
               GO TO MSG-SCRL-TAB-CCM-PF7.
           MOVE FIO-STAT TO PGM-STAT-DCM.
           PERFORM KEY-CCM-CCM.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM MC2-FIO-CCM.
           MOVE FIO-STAT TO PGM-STAT-CCM.
           PERFORM SCR-CLRA-CCM.
           PERFORM PUT-SCR.
           GO TO FIN-SCRL-TAB-CCM-PF7.
       MSG-SCRL-TAB-CCM-PF7.
           MOVE 'TAB' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-CCM-PF7.
           EXIT.

       SCRL-TAB-CCM-PF8 SECTION.
       INI-SCRL-TAB-CCM-PF8.
           MOVE SCR-DCM-KEY-IDCM TO DCM-KEY-IDCM IN DCM.
           MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM MC2-FIO-DCM.
           IF NOT ( FIO-STAT-OKS AND
                    DCM-KEY-IDCM IN DCM = SCR-DCM-KEY-IDCM)
               MOVE 'TABCURRNEX' TO MSG-COD-MENS
               GO TO MSG-SCRL-TAB-CCM-PF8.
       NXT-SCRL-TAB-CCM-PF8.
           MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM MC2-FIO-DCM.
           IF NOT ( FIO-STAT-OKS AND
                    DCM-CIC-IDCM IN DCM = SCR-DCM-CIC-IDCM )
               MOVE 'TAB    NONXT' TO MSG-COD-MENS
               GO TO MSG-SCRL-TAB-CCM-PF8.
           MOVE FIO-STAT TO PGM-STAT-DCM.
           PERFORM KEY-CCM-CCM.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM MC2-FIO-CCM.
           MOVE FIO-STAT TO PGM-STAT-CCM.
           PERFORM SCR-CLRA-CCM.
           PERFORM PUT-SCR.
           GO TO FIN-SCRL-TAB-CCM-PF8.
       MSG-SCRL-TAB-CCM-PF8.
           MOVE 'TAB' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-CCM-PF8.
           EXIT.

       CRG-VEC-DCM SECTION.
       INI-CRG-VEC-DCM.
           MOVE DCM-NOM-TSDCM TO QUE-COLA.
           MOVE QUE-DEL       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           MOVE DCM-NOM-TSDCM-MAS TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE ZEROES TO DCM-J.
           MOVE ZEROES TO DCM-NITM.
           IF FRM-IND-SGTE IN CCM-FLD = 'S'
              MOVE DCM-NUM-IDCM IN CCM-FLD TO DCM-NUM-IDCM IN DCM
              ADD 1                        TO DCM-NUM-IDCM IN DCM.
      *PVG INI 25-OCT-2007                                              
      *    ELSE                                                         
      *       MOVE ZEROES                  TO DCM-NUM-IDCM IN DCM.      
      *PVG FIN 25-OCT-2007                                              
                                                                        
           MOVE CCM-CIC-ICCM IN CCM TO DCM-CIC-IDCM IN DCM.
           MOVE FIO-GET-NLS TO FIO-CMND.
       LUP-CRG-VEC-DCM.
           PERFORM MC2-FIO-DCM.
           MOVE FIO-STAT TO PGM-STAT-DCM.
           IF NOT (FIO-STAT-OKS AND
                   CCM-CIC-ICCM IN CCM = DCM-CIC-IDCM IN DCM)
              GO TO EXT-CRG-VEC-DCM.

