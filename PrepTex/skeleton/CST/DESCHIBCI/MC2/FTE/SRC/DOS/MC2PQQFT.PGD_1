       ENVIRONMENT DIVISION.
      *=====================
       CONFIGURATION SECTION.
      *----------------------
       SPECIAL-NAMES.
               DECIMAL-POINT IS COMMA.
       DATA DIVISION.
      *==============

       WORKING-STORAGE SECTION.
      *------------------------
      * COPY GNSWGSCR.
       COPY GNSWGCVT.
       77  WSS-TAG-MC2PQQFT-INI PIC X(125) VALUE                        
           '<IDNPGM NOM=$MC2PQQFT$ SKL=$07.08.21$ VSN=${vsnpq}$        
      -    'GEN=$20-Feb-19 06:05 PM$ FNC=${fncpq}$/>'.    

       COPY GNSWCSCR.
      *PTC-INI
      *PTC-SUB
      * COPY GNSWVSCR.
       COPY GNSWV3CR.
      *PTC-FIN
           03 SCR-NUM-PANT               PIC 9(0010).
           03 SCR-QFT-FLD                PIC X(1260).
       COPY MC2WIQFT.

       COPY MC2BRCFT.


       COPY GNSWVIDD.
       COPY GNSWGRQA.
       COPY GNSWGUIB.
       COPY GNSWGELS.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CFT-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-TAB-REQA==.
       COPY GNSBRTAB.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSG-REQA==.
       COPY GNSBRMSG.
       COPY GNSWCFIO.
       COPY GNSWVFIO.
      *PTC-INI
      *PTC-SUB
      * ISP  10-MAY-2001 13:46:35 
      * COPY GNSWGFRM.
       COPY GNSWG3CF.
       COPY GNSWGTSK.
       COPY GNSWGQUE.
      *PTC-FIN
       COPY GNSWVNUM.
       COPY GNSWCNUM.
       COPY GNSWGFEC.
       COPY GNSWGHOY.
       COPY GNSWGSYS.

      *<<<< WSS
       COPY GNSBRMSC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSC-REQA==.
       COPY GNSWGCPT.
       COPY GNSWGSTR.
       COPY MC2WXQFT.
       COPY MC2BRDFT.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DFT-REQA==.
      *>>>>
       01  PGM-VARI.
           03 PGM-ICLR                  VALUE ' '       PIC X(01).
           03 PGM-LIMPIAR-TODO          VALUE ' '       PIC X(01).
           03 PGM-LIMPIAR-LLAVES        VALUE 'K'       PIC X(01).
           03 PGM-LIMPIAR-CAMPOS        VALUE 'F'       PIC X(01).
           03 PGM-NO-LIMPIAR-NADA       VALUE 'N'       PIC X(01).
           03 PGM-PROG                                  PIC X(08).
           03 PGM-NTRN                                  PIC X(04).
           03 PGM-STAT.
              05 PGM-STAT-CFT.
                 88 PGM-STAT-CFT-NUL    VALUE '  '.
                 07 PGM-STT1-CFT        VALUE '2' PIC X(01).
                    88 PGM-STAT-CFT-OKS VALUE '0'.
                 07 PGM-STT2-CFT        VALUE '3' PIC X(01).
              05 PGM-STAT-DFT.
                 88 PGM-STAT-DFT-NUL    VALUE '  '.
                 07 PGM-STT1-DFT        VALUE '2' PIC X(01).
                    88 PGM-STAT-DFT-OKS VALUE '0'.
                 07 PGM-STT2-DFT        VALUE '3' PIC X(01).
           03 PGM-SRNF                  VALUE '23'      PIC X(02).
           03 PGM-SOKS                  VALUE '00'      PIC X(02).
           03 PGM-SNUL                  VALUE '  '      PIC X(02).
           03 PGM-SUAR                  VALUE SPACES    PIC X(15).
           03 PGM-CMMA.
              05 PGM-CMMA-CHR OCCURS 3000 PIC X(01).

       LINKAGE SECTION.
      *----------------
       01  DFHCOMMAREA.
           02 DFH-CMMA OCCURS 3000 PIC X(01).
       COPY GNSLGFIO.

       PROCEDURE DIVISION.
      *===================

       MAIN SECTION.
       INI-MAIN.
           PERFORM INI.
       LUP-MAIN.
           IF SCR-INST = SCR-INST-HCM
                 PERFORM SCR-COM.
           IF SCR-CCPP = 'ACC'
                 PERFORM SCR-ACC
           ELSE
                 MOVE SCR-INST-HCM TO SCR-INST.
           GO TO LUP-MAIN.
       FIN-MAIN.
           EXIT.

       INI SECTION.
       INI-INI.
       COPY GNSBGEIB.
       COPY GNSBGEDB.
           MOVE LOW-VALUES TO QFT-FLD.
           MOVE LOW-VALUES TO QFT-EDT.
           PERFORM GNS-PRO-RCK.
           PERFORM GNS-BUS-VCA.
           MOVE +3000 TO SCR-TCMA.
           PERFORM GNS-HDL-SYS.
           PERFORM GNS-HDL-FRM.
           MOVE 'MC2PQQFT' TO FIO-PROG.
           PERFORM GNS-BUS-IDD.
      *PTC-INI
           PERFORM GNS-ERR-QUE.
           PERFORM GET-TSK-TERM.
      *PTC-FIN
      *<<<< INI_FIO
           IF SCR-OFRM-SRV
                MOVE TSK-TERM-ALF TO QFT-COD-TERM
                MOVE TSK-TERM-ALF TO QFT-COD-TERM-MAS
           ELSE
                MOVE EIBTRMID     TO QFT-COD-TERM
                MOVE EIBTRMID     TO QFT-COD-TERM-MAS.
           MOVE +3859 TO SCR-TCMA.
           PERFORM GNS-ERR-QUE.
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )
               MOVE SCR-QFT-FLD TO QFT-FLD
               GO TO FIN-INI.

           IF ( FRM-FFLD = FRM-FFLD-PF6 )
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM
               ADD 1              TO QUE-NITM
               MOVE QFT-NOM-TSQFT TO QUE-COLA
               MOVE QFT-LEN-TSQFT TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT NOT = QUE-STAT-OKS
                  MOVE SCR-QFT-FLD TO QFT-FLD
                  GO TO FIN-INI.
           PERFORM GET-FHOY.
      *>>>>
       FIN-INI.
           EXIT.

       COPY GNSBHSYS.
       COPY GNSBHFRM.
       COPY GNSBPRCK.
       GNS-BUS-VCA SECTION.
       INI-GNS-BUS-VCA.
           MOVE 0 TO FRM-IFLD.
       LUP-GNS-BUS-VCA.
           ADD 1 TO FRM-IFLD.
           IF FRM-IFLD NOT > EIBCALEN AND
              FRM-IFLD NOT > 3000
                MOVE DFH-CMMA(FRM-IFLD) TO PGM-CMMA-CHR(FRM-IFLD)
                GO TO LUP-GNS-BUS-VCA.
           MOVE PGM-CMMA TO SCR-VARI.
       FIN-GNS-BUS-VCA.
           EXIT.
       COPY GNSBBIDD.

       SCR-COM SECTION.
       INI-SCR-COM.
      *<<<< INI_SCR_COM
           MOVE QFT-NOM-TSQFT TO QUE-COLA.
           MOVE QUE-DEL       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE QFT-NOM-TSQFT-MAS TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
      *>>>>
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-CLRA-QFT.
           PERFORM EDT-KEY-QFT.
           PERFORM EDT-FLD-QFT.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-HCM TO SCR-INST.
           MOVE SCR-CMNP     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       XCT-ERR.
           MOVE 'XCTERR    ' TO MSG-COD-MENS.
           MOVE 'GNS'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO SYS-MEN1.
           MOVE SYS-PROG     TO SYS-MEN2.
           MOVE SYS-ABOR     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       COPY GNSBGGBK.
       FIN-SCR-COM.
           EXIT.



       SCR-ACC SECTION.
       INI-SCR-ACC.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ACC
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-ACC.
           PERFORM DESSCRTRL.
      *<<<< PFS_SCR_ACC
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM VAL-NUM-KEY-QFT
               PERFORM VAL-NUM-FLD-QFT
               PERFORM SCRL-TAB-QFT
               PERFORM SCR-TRL.
      *>>>>
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ACC.
           EXIT.

       BRW-SCR-ACC SECTION.
       INI-BRW-SCR-ACC.
       FIN-BRW-SCR-ACC.
           EXIT.




       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               PERFORM SCR-CLRA-QFT
               PERFORM EDT-KEY-QFT
               PERFORM EDT-FLD-QFT
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-QFT
               PERFORM EDT-KEY-QFT
           ELSE
               PERFORM CLR-FLD-QFT
               PERFORM EDT-FLD-QFT.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

       SCR-KEY SECTION.
       INI-SCR-KEY.
           PERFORM SCR-RST.
           PERFORM GET-KEY.
           IF FRM-STAT-ABT
                   MOVE SPACES TO FRM-MENS
                   PERFORM SCR-MSSG
                   GO TO FIN-SCR-KEY.
       FND-SCR-KEY.
           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
           IF FIO-STAT-OKS
               PERFORM PUT-SCR
               MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
               GO TO FIN-SCR-KEY.
      *JSS ELSE
      *        PERFORM CLR-FLD-QFT.
       ERR-SCR-KEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR.
           IF FRM-MENS NOT > SPACES
               IF FIO-STAT-EOF
                   MOVE 'REGEOF    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
               IF FIO-STAT = FIO-STAT-NEX
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
           MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-SCR-KEY.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO QFT-FLD.
           MOVE LOW-VALUES TO QFT-EDT.
       FIN-SCR-RST.
           EXIT.

       GET-KEY SECTION.
       INI-GET-KEY.
           PERFORM SCR-GKEY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-KEY.
           EXIT.

       GET-IKY SECTION.
       INI-GET-IKY.
           PERFORM SCR-GIKY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-IKY.
           EXIT.

       FND-KEY SECTION.
       INI-FND-KEY.
      *<<<< INI_FND_KEY
           PERFORM KEY-ALL-QFT.

           IF CFT-CIC-IDFT IN QFT-FLD > SPACES
              MOVE 'CFT-CIC-IDFT' TO FIO-AKEY
              MOVE FIO-GET-KEY TO FIO-CMND
              PERFORM MC2-FIO-CFT
              MOVE FIO-STAT TO PGM-STAT-CFT
              GO TO CNT-FND-KEY.

           MOVE FIO-GET-LEQ TO FIO-CMND.
           PERFORM MC2-FIO-CFT.
           IF NOT ( FIO-STAT-OKS AND
                    CFT-CIC-ICFT IN CFT = CFT-CIC-ICFT IN QFT-FLD)
              MOVE '23' TO FIO-STAT PGM-STAT-CFT
              GO TO FIN-FND-KEY.

       CNT-FND-KEY.
           MOVE FIO-STAT TO PGM-STAT-CFT.
           PERFORM PUT-CFT-QFT.

           PERFORM CLR-VEC-DQFT.
           MOVE ZEROES      TO WSS-J.
           MOVE CFT-CIC-IDFT IN CFT TO DFT-CIC-IDFT IN DFT.
           MOVE FIO-GET-NLS TO FIO-CMND.

           IF FRM-IND-SGTE IN QFT-FLD > SPACES AND
              FRM-IND-SGTE IN QFT-FLD = 'S'
              MOVE FRM-NUM-KPRD IN QFT-FLD TO DFT-NUM-PRDF IN DFT
              MOVE FRM-VAL-KMNT IN QFT-FLD TO DFT-VAL-MNTO IN DFT
              MOVE FRM-NUM-KPLZ IN QFT-FLD TO DFT-NUM-PLZO IN DFT
              ADD 1                        TO DFT-NUM-PLZO IN DFT
           ELSE
              MOVE ZEROES                  TO DFT-NUM-PRDF IN DFT
              MOVE ZEROES                  TO DFT-VAL-MNTO IN DFT
              MOVE ZEROES                  TO DFT-NUM-PLZO IN DFT.
       LUP-LEE-CFT.
           PERFORM MC2-FIO-DFT.
           IF NOT ( FIO-STAT-OKS AND
                    DFT-CIC-IDFT IN DFT = CFT-CIC-IDFT IN CFT )
              GO TO CON-FND-KEY.

           PERFORM CRG-INF-RCFT.

           IF (SCR-OFRM-SRV       AND WSS-J > WSS-NITMMAX)
              GO TO CON-FND-KEY.
           IF ((NOT SCR-OFRM-SRV) AND WSS-J > WSS-JMAX)
              GO TO CON-FND-KEY.
       NXT-LEE-PLN.
           MOVE FIO-GET-NXT TO FIO-CMND.
           GO TO LUP-LEE-CFT.
      *>>>>
           PERFORM KEY-ALL-QFT.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM MC2-FIO-CFT.
           MOVE FIO-STAT TO PGM-STAT-CFT.
      *<<<< FIN_FND_KEY
       CON-FND-KEY.
           PERFORM PUT-TAB.

           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *----------------------------------------------------------
       MSG-FND-KEY.
           MOVE 'TAB'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE '00'         TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *>>>>
       FIN-FND-KEY.
           IF FIO-STAT = FIO-STAT-NEX
               MOVE 'S' TO SCR-IERR.
       EXT-FND-KEY.
           EXIT.
 
       PUT-SCR SECTION.
       INI-PUT-SCR.
           PERFORM PUT-ALL-QFT.
       FIN-PUT-SCR.
           EXIT.

       STP-EXIT SECTION.
       INI-STP-EXIT.
           MOVE SCR-PROG TO PGM-PROG.
           MOVE SCR-NTRN TO PGM-NTRN.

           MOVE SCR-PROV TO SCR-PROG.
           MOVE SCR-NTRV TO SCR-NTRN.

           MOVE PGM-PROG TO SCR-PROV
           MOVE PGM-NTRN TO SCR-NTRV.
       FIN-STP-EXIT.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO SYS-NTRN.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
      *PTC-INI
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE QFT-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO QFT-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GKEY SECTION.
       INI-SCR-GKEY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-QFT
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO QFT-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GKEY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-QFT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-QFT
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-QFT.
           PERFORM VAL-IKY-QFT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-MKY-QFT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-AKY-QFT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-CON-KEY-QFT.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GKEY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-QFT.
       ERR-SCR-GKEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-QFT.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GKEY.
           PERFORM SCR-MSSG.

       SCR-GIKY SECTION.
       INI-SCR-GIKY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-QFT
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO QFT-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GIKY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-QFT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-IKY-QFT
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-QFT.
           PERFORM VAL-IKY-QFT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GIKY.
           PERFORM VAL-CON-IKY-QFT.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GIKY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-QFT.
       ERR-SCR-GIKY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-IKY-QFT.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GIKY.
           PERFORM SCR-MSSG.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
      *>>>>
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO QFT-GLS-MEN2
               MOVE SCR-PRMT     TO QFT-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO QFT-FLD.
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO QFT-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE QFT-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO QFT-FLD
      * FLB  7-NOV-1994
               PERFORM CLR-KEY-QFT

               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO QFT-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO QFT-GLS-MENS.
           PERFORM PRO-MKY-QFT.
           PERFORM PRO-AKY-QFT.
           PERFORM UNP-IKY-QFT.
           PERFORM FST-KEY-QFT.
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE QFT-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO QFT-FLD
               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO QFT-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO QFT-GLS-MENS.
           PERFORM UNP-KEY-QFT.
           PERFORM FST-KEY-QFT.
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
           PERFORM PRO-IKY-QFT.
           PERFORM PRO-AKY-QFT.
           PERFORM UNP-MKY-QFT.
           PERFORM UNP-FLD-QFT.
           PERFORM FST-FLD-QFT.
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO QFT-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-QFT.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
      *PTC-INI
           IF SCR-STAT NOT = SCR-STPP
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-QFT
               PERFORM EDT-FLD-QFT
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-QFT
               PERFORM EDT-FLD-QFT
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-QFT.
           PERFORM EDT-FLD-QFT.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

      * PGM-EBR-ALL SECTION.
      * INI-PGM-EBR-ALL.
      *     MOVE 'GNS' TO FIO-SIST.
      *     MOVE FIO-EBR-ALL TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
      * FIN-PGM-EBR-ALL.
      *     EXIT.

       COPY MC2BQQFT.
       COPY GNSBVNUM.
       COPY MC2BFCFT.
       COPY GNSBFMSG.
       COPY GNSBGSYS.
       COPY GNSBGDTC.
      *PTC-INI
      *PTC-SUB
      * ISP  10-MAY-2001 13:46:35 
      * COPY GNSBGFRM.
       COPY GNSBG3CF.
      *PTC-FIN
       COPY GNSBGMSG.
       COPY GNSBEKEY.
       COPY GNSBIABT.
       COPY GNSBFTAB.
       COPY GNSBEGLS.
       COPY GNSBGFEC.
       COPY GNSBGHOY.
      *PTC-INI
       COPY GNSBGQUE.
       COPY GNSBGTSK.
      *PTC-FIN

      *<<<< EOF
       COPY MC2BFDFT.
       COPY GNSBBTAB.
       COPY GNSBVTAB.
       COPY GNSBVCOD.
       COPY GNSBVMSC.
       COPY GNSBBMSC.
       COPY GNSBPFEC.
       COPY GNSBFMSC.

       PUT-TAB SECTION.
       INI-PUT-TAB.
           MOVE QFT-NOM-TSQFT TO QUE-COLA.
           MOVE QUE-DEL       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           MOVE QFT-NOM-TSQFT-MAS TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE 1 TO SCR-NUM-PANT.
           MOVE LOW-VALUES TO FRM-NUM-PRDF-IDX-MTI FRM-NUM-PRDF-IDX-FRM.
      *
           MOVE 1 TO FRM-IFLD.
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.

           MOVE 1 TO WSS-F IN WSS-VARI.
       LUP-PUT-TAB.
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI
              GO TO FIN-PUT-TAB.
           IF WSS-COD-ROLF-V(WSS-F) > SPACES
              PERFORM PUT-TAB-QFT
              PERFORM PUT-QFT-QUE
              PERFORM SET-FRM-IFLD
           ELSE
           IF FRM-IFLD = 1 AND WSS-F = 1
              MOVE 'MC2' TO MSG-COD-SIST
              MOVE 'CFT    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE '00' TO FIO-STAT
              GO TO FIN-PUT-TAB
           ELSE
              MOVE '00' TO FIO-STAT
              GO TO FIN-PUT-TAB.

           ADD 1 TO WSS-F IN WSS-VARI.
           GO TO LUP-PUT-TAB.
       FIN-PUT-TAB.
           MOVE 1                  TO QUE-NITM.
           MOVE QFT-LEN-TSQFT-MAS  TO QUE-LITM.
           MOVE QFT-NOM-TSQFT-MAS  TO QUE-COLA.

           IF WSS-NITM > WSS-NITMMAX
              MOVE 'S'           TO QFT-MAS-IND-HMAS
           ELSE
              MOVE 'N'           TO QFT-MAS-IND-HMAS.

           MOVE WSS-NITM         TO WSS-NUM-ICUR.
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.
           MOVE WSS-NUM-ICUR-ALF TO QFT-MAS-NUM-TOCC.
           MOVE QFT-MAS-ITEM     TO QUE-ITEM.
           MOVE QUE-PUT          TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF WSS-NITM > WSS-IMAX
               MOVE 'TAB' TO MSG-COD-SIST
               MOVE 'CFTTABDMAS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       EXT-PUT-TAB.
           EXIT.

       SET-FRM-IFLD SECTION.
       INI-SET-FRM-IFLD.
           IF SCR-OFRM-SRV
                MOVE 1 TO FRM-IFLD
           ELSE
                ADD 1  TO FRM-IFLD.
       FIN-SET-FRM-IFLD.
           EXIT.
      *--------------------------------------------
       SCRL-TAB-QFT SECTION.
       INI-SCRL-TAB-QFT.
           MOVE SPACES TO MSG-COD-MENS.

           IF FRM-FFLD = FRM-FFLD-PF6
               PERFORM PUT-TAB-ASC
           ELSE
               PERFORM PUT-TAB-DSC.
           IF MSG-COD-MENS > SPACES
               MOVE 'TAB' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-QFT.
           EXIT.

      *--------------------------------------------
       PUT-TAB-QFT SECTION.
       INI-PUT-TAB-QFT.
           PERFORM CLR-WSS.
           IF SCR-OFRM-SRV
              MOVE WSS-NUM-PRDF-V(WSS-F) TO MTI-NUM-PRDF IN WSS-VARI
              MOVE WSS-COD-ROLF-V(WSS-F) TO MTI-COD-ROLF IN WSS-VARI
              MOVE WSS-VAL-MNTO-V(WSS-F) TO MTI-VAL-MNTO IN WSS-VARI
              MOVE WSS-NUM-PLZO-V(WSS-F) TO MTI-NUM-PLZO IN WSS-VARI
              MOVE WSS-SGV-PDFT-V(WSS-F) TO MTI-SGV-PDFT IN WSS-VARI
              MOVE WSS-SGV-MDFT-V(WSS-F) TO MTI-SGV-MDFT IN WSS-VARI
              MOVE WSS-COD-MDFT-V(WSS-F) TO MTI-COD-MDFT IN WSS-VARI
           ELSE
              MOVE WSS-NUM-PRDF-V(WSS-F) TO WSS-NUM-PRDF IN WSS-VARI
              MOVE WSS-COD-ROLF-V(WSS-F) TO WSS-COD-ROLF IN WSS-VARI
              MOVE WSS-VAL-MNTO-V(WSS-F) TO WSS-VAL-MNTO IN WSS-VARI
              MOVE WSS-NUM-PLZO-V(WSS-F) TO WSS-NUM-PLZO IN WSS-VARI
              MOVE WSS-SGV-PDFT-V(WSS-F) TO WSS-SGV-PDFT IN WSS-VARI
              MOVE WSS-SGV-MDFT-V(WSS-F) TO WSS-SGV-MDFT IN WSS-VARI
              MOVE WSS-COD-MDFT-V(WSS-F) TO WSS-COD-MDFT IN WSS-VARI.

           IF FRM-IFLD > WSS-IMAX
              GO TO FIN-PUT-TAB-QFT.

           MOVE WSS-NUM-PRDF-V(WSS-F) TO
                FRM-NUM-PRDF IN QFT-FLD(FRM-IFLD).
           MOVE WSS-COD-ROLF-V(WSS-F) TO
                FRM-COD-ROLF IN QFT-FLD(FRM-IFLD).
           MOVE WSS-VAL-MNTO-V(WSS-F) TO
                FRM-VAL-MNTO IN QFT-FLD(FRM-IFLD).
           MOVE WSS-NUM-PLZO-V(WSS-F) TO
                FRM-NUM-PLZO IN QFT-FLD(FRM-IFLD).
           MOVE WSS-SGV-PDFT-V(WSS-F) TO
                FRM-SGV-PDFT IN QFT-FLD(FRM-IFLD).
           MOVE WSS-SGV-MDFT-V(WSS-F) TO
                FRM-SGV-MDFT IN QFT-FLD(FRM-IFLD).
           MOVE WSS-COD-MDFT-V(WSS-F) TO
                FRM-COD-MDFT IN QFT-FLD(FRM-IFLD).
       FIN-PUT-TAB-QFT.
           EXIT.
      *----------------------------------------------------
       PUT-QFT-QUE SECTION.
       INI-PUT-QFT-QUE.
           PERFORM PRO-FLD-QUE.
           ADD 1 TO WSS-NITM.
           MOVE QFT-NOM-TSQFT    TO QUE-COLA.
           MOVE QFT-LEN-TSQFT    TO QUE-LITM.
           MOVE WSS-NITM         TO QUE-NITM.
           MOVE SPACES           TO QUE-ITEM.
           IF SCR-OFRM-SRV
              MOVE FRM-NUM-PRDF-IDX-MTI TO QUE-ITEM
           ELSE
              MOVE FRM-NUM-PRDF-IDX-FRM TO QUE-ITEM.
           MOVE QUE-PUT  TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
       FIN-PUT-QFT-QUE.
           EXIT.
      *----------------------------------------------------

       PUT-TAB-ASC SECTION.
       INI-PUT-TAB-ASC.
           MOVE SPACES TO MSG-COD-MENS.
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.
           MOVE 0 TO FRM-IFLD.
       LUP-PUT-TAB-ASC.
           ADD 1              TO QUE-NITM.
           MOVE QFT-NOM-TSQFT TO QUE-COLA.
           MOVE QFT-LEN-TSQFT TO QUE-LITM.
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD = 0
                   MOVE 'CFTTABNONXT' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-ASC
               ELSE
                   GO TO FIN-PUT-TAB-ASC.
           ADD 1 TO FRM-IFLD.
           MOVE QUE-ITEM TO FRM-NUM-PRDF-IDX(FRM-IFLD).
           IF FRM-IFLD < WSS-IMAX
               GO TO LUP-PUT-TAB-ASC.
       FIN-PUT-TAB-ASC.
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX
               ADD 1 FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1
                                 UNTIL WSS-I > WSS-IMAX.
           IF FRM-IFLD > 0
               ADD 1 TO SCR-NUM-PANT.
           IF FRM-IFLD = WSS-IMAX
               ADD 1              TO QUE-NITM
               MOVE QFT-NOM-TSQFT TO QUE-COLA
               MOVE QFT-LEN-TSQFT TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT = QUE-STAT-OKS
                   MOVE 'TAB'        TO MSG-COD-SIST
                   MOVE 'CFTTABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-ASC.
           EXIT.

       PUT-TAB-DSC SECTION.
       INI-PUT-TAB-DSC.
           MOVE SPACES TO MSG-COD-MENS.
           IF SCR-NUM-PANT = 1
               MOVE 'CFTTABNOPRV' TO MSG-COD-MENS
               GO TO EXT-PUT-TAB-DSC.

           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.
           ADD 1 TO QUE-NITM.
           ADD 1 WSS-IMAX GIVING FRM-IFLD.
       LUP-PUT-TAB-DSC.
           SUBTRACT  1 FROM QUE-NITM.
           MOVE QFT-NOM-TSQFT TO QUE-COLA
           MOVE QFT-LEN-TSQFT TO QUE-LITM
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD > WSS-IMAX
                   MOVE 'CFTTABNOPRV' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-DSC
               ELSE
                   GO TO FIN-PUT-TAB-DSC.
           SUBTRACT 1 FROM FRM-IFLD.
           MOVE QUE-ITEM TO FRM-NUM-PRDF-IDX(FRM-IFLD).
           IF FRM-IFLD > 1
               GO TO LUP-PUT-TAB-DSC.
       FIN-PUT-TAB-DSC.

           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1
                                 UNTIL WSS-I < 1.

           IF FRM-IFLD NOT > WSS-IMAX
               SUBTRACT 1 FROM SCR-NUM-PANT
               MOVE 'TAB'        TO MSG-COD-SIST
               MOVE 'CFTTABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-DSC.
           EXIT.

       COPY GNSBGCPT.
       COPY GNSBGSTR.

       CLR-FRM SECTION.
       INI-CLR-FRM.
           MOVE ZEROES TO FRM-NUM-PRDF IN QFT-FLD(WSS-I).
           MOVE SPACES TO FRM-COD-ROLF IN QFT-FLD(WSS-I).
           MOVE ZEROES TO FRM-VAL-MNTO IN QFT-FLD(WSS-I).
           MOVE ZEROES TO FRM-NUM-PLZO IN QFT-FLD(WSS-I).
           MOVE ZEROES TO FRM-SGV-PDFT IN QFT-FLD(WSS-I).
           MOVE ZEROES TO FRM-SGV-MDFT IN QFT-FLD(WSS-I).
           MOVE SPACES TO FRM-COD-MDFT IN QFT-FLD(WSS-I).
       FIN-CLR-FRM.
           EXIT.

       CLR-WSS SECTION.
       INI-CLR-WSS.
           MOVE ZEROES TO MTI-NUM-PRDF IN WSS-VARI.
           MOVE SPACES TO MTI-COD-ROLF IN WSS-VARI.
           MOVE ZEROES TO MTI-VAL-MNTO IN WSS-VARI.
           MOVE ZEROES TO MTI-NUM-PLZO IN WSS-VARI.
           MOVE ZEROES TO MTI-SGV-PDFT IN WSS-VARI.
           MOVE ZEROES TO MTI-SGV-MDFT IN WSS-VARI.
           MOVE SPACES TO MTI-COD-MDFT IN WSS-VARI.

           MOVE ZEROES TO WSS-NUM-PRDF IN WSS-VARI.
           MOVE SPACES TO WSS-COD-ROLF IN WSS-VARI.
           MOVE ZEROES TO WSS-VAL-MNTO IN WSS-VARI.
           MOVE ZEROES TO WSS-NUM-PLZO IN WSS-VARI.
           MOVE ZEROES TO WSS-SGV-PDFT IN WSS-VARI.
           MOVE ZEROES TO WSS-SGV-MDFT IN WSS-VARI.
           MOVE SPACES TO WSS-COD-MDFT IN WSS-VARI.

           IF SCR-OFRM-SRV
               PERFORM CLR-FRM-MTX.
       FIN-CLR-WSS.
           EXIT.

       CLR-FRM-MTX SECTION.
       INI-CLR-FRM-MTX.
           MOVE ZEROES TO FRM-NUM-PRDF IN QFT-FLD(1).
           MOVE SPACES TO FRM-COD-ROLF IN QFT-FLD(1).
           MOVE ZEROES TO FRM-VAL-MNTO IN QFT-FLD(1).
           MOVE ZEROES TO FRM-NUM-PLZO IN QFT-FLD(1).
           MOVE ZEROES TO FRM-SGV-PDFT IN QFT-FLD(1).
           MOVE ZEROES TO FRM-SGV-MDFT IN QFT-FLD(1).
           MOVE SPACES TO FRM-COD-MDFT IN QFT-FLD(1).
       FIN-CLR-FRM-MTX.
           EXIT.

       PRO-FLD-QUE SECTION.
       INI-PRO-FLD-QUE.
           MOVE 'P' TO MTI-NUM-PRDF-ATR IN WSS-VARI..
           MOVE 'P' TO MTI-COD-ROLF-ATR IN WSS-VARI..
           MOVE 'P' TO MTI-VAL-MNTO-ATR IN WSS-VARI..
           MOVE 'P' TO MTI-NUM-PLZO-ATR IN WSS-VARI..
           MOVE 'P' TO MTI-SGV-PDFT-ATR IN WSS-VARI..
           MOVE 'P' TO MTI-SGV-MDFT-ATR IN WSS-VARI..
           MOVE 'P' TO MTI-COD-MDFT-ATR IN WSS-VARI..

           MOVE 'P' TO WSS-NUM-PRDF-ATR IN WSS-VARI..
           MOVE 'P' TO WSS-COD-ROLF-ATR IN WSS-VARI..
           MOVE 'P' TO WSS-VAL-MNTO-ATR IN WSS-VARI..
           MOVE 'P' TO WSS-NUM-PLZO-ATR IN WSS-VARI..
           MOVE 'P' TO WSS-SGV-PDFT-ATR IN WSS-VARI..
           MOVE 'P' TO WSS-SGV-MDFT-ATR IN WSS-VARI..
           MOVE 'P' TO WSS-COD-MDFT-ATR IN WSS-VARI..
       FIN-PRO-FLD-QUE.
           EXIT.

       CLR-VEC-DQFT SECTION.
       INI-CLR-VEC-DQFT.
           MOVE ZEROES TO WSS-J.
       LUP-CLR-VEC-DQFT.
           ADD 1 TO WSS-J.
           IF WSS-J > WSS-JMAX
              GO TO FIN-CLR-VEC-DQFT.
           MOVE ZEROES TO WSS-NUM-PRDF-V(WSS-J).
           MOVE SPACES TO WSS-COD-ROLF-V(WSS-J).
           MOVE ZEROES TO WSS-VAL-MNTO-V(WSS-J).
           MOVE ZEROES TO WSS-NUM-PLZO-V(WSS-J).
           MOVE ZEROES TO WSS-SGV-PDFT-V(WSS-J).
           MOVE ZEROES TO WSS-SGV-MDFT-V(WSS-J).
           MOVE SPACES TO WSS-COD-MDFT-V(WSS-J).
           GO TO LUP-CLR-VEC-DQFT.
       FIN-CLR-VEC-DQFT.
           EXIT.

       CRG-INF-RCFT SECTION.
       INI-CRG-INF-RCFT.
           ADD 1 TO WSS-J.
           MOVE DFT-NUM-PRDF IN DFT TO WSS-NUM-PRDF-V(WSS-J).
           MOVE DFT-COD-ROLF IN DFT TO WSS-COD-ROLF-V(WSS-J).
           MOVE DFT-VAL-MNTO IN DFT TO WSS-VAL-MNTO-V(WSS-J).
           MOVE DFT-NUM-PLZO IN DFT TO WSS-NUM-PLZO-V(WSS-J).
           MOVE DFT-SGV-PDFT IN DFT TO WSS-SGV-PDFT-V(WSS-J).
           MOVE DFT-SGV-MDFT IN DFT TO WSS-SGV-MDFT-V(WSS-J).
           MOVE DFT-COD-MDFT IN DFT TO WSS-COD-MDFT-V(WSS-J).
       FIN-CRG-INF-RCFT.
           EXIT.
      *>>>> EOF
