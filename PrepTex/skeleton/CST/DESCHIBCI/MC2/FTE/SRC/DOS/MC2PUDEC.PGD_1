       COPY GNSBGCPT.
       COPY GNSBGLGA.
       COPY GNSBFLGA.
       COPY TABBFPLN.
       COPY MC2BFCVN.
       COPY MC2BFCNT.                                                   
       COPY MC2BFDPC.                                                   
       COPY SGCBFDBC.
      *>>>>
