      *>>>>
       FIN-VAL-CVN-COD-EEXP.
           EXIT.
       
       VAL-FRM-GLS-ABOG SECTION.
       INI-VAL-FRM-GLS-ABOG.
       FIN-VAL-FRM-GLS-ABOG.
           EXIT.
       
       VAL-FRM-GLS-EEXP SECTION.
       INI-VAL-FRM-GLS-EEXP.
       FIN-VAL-FRM-GLS-EEXP.
           EXIT.
       
       VAL-CON-FLD-CVN SECTION.
       INI-VAL-CON-FLD-CVN.
       FIN-VAL-CON-FLD-CVN.
           EXIT.
       
       VAL-CON-CVN SECTION.
       INI-VAL-CON-CVN.
       FIN-VAL-CON-CVN.
           EXIT.
       
       VAL-NUM-KEY-CVN SECTION.
       INI-VAL-NUM-KEY-CVN.
           PERFORM VAL-NUM-IKY-CVN.
           PERFORM VAL-NUM-MKY-CVN.
           PERFORM VAL-NUM-AKY-CVN.
       FIN-VAL-NUM-KEY-CVN.
           EXIT.
       
       VAL-NUM-IKY-CVN SECTION.
       INI-VAL-NUM-IKY-CVN.
       VAL-NUM-CVN-IIC-ICVN.
           MOVE CVN-IIC-ICVN-ALF IN CVN-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CVN-IIC-ICVN IN CVN-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CVN-IIC-ICVN-LEN IN CVN-FLD
                GO TO FIN-VAL-NUM-IKY-CVN.
       VAL-NUM-CVN-HRA-HRCV.
           MOVE CVN-HRA-HRCV-ALF IN CVN-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 6 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CVN-HRA-HRCV IN CVN-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CVN-HRA-HRCV-LEN IN CVN-FLD
                GO TO FIN-VAL-NUM-IKY-CVN.
       FIN-VAL-NUM-IKY-CVN.
           EXIT.
       
       VAL-NUM-MKY-CVN SECTION.
       INI-VAL-NUM-MKY-CVN.
       FIN-VAL-NUM-MKY-CVN.
           EXIT.
       
       VAL-NUM-AKY-CVN SECTION.
       INI-VAL-NUM-AKY-CVN.
       FIN-VAL-NUM-AKY-CVN.
           EXIT.
       
       VAL-NUM-FLD-CVN SECTION.
       INI-VAL-NUM-FLD-CVN.
       VAL-NUM-CVN-NUM-DURA.
           MOVE CVN-NUM-DURA-ALF IN CVN-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 5 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CVN-NUM-DURA IN CVN-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CVN-NUM-DURA-LEN IN CVN-FLD
                GO TO FIN-VAL-NUM-FLD-CVN.
       FIN-VAL-NUM-FLD-CVN.
           EXIT.
       
       EDT-KEY-CVN SECTION.
       INI-EDT-KEY-CVN.
           PERFORM EDT-IKY-CVN.
           PERFORM EDT-MKY-CVN.
           PERFORM EDT-AKY-CVN.
       FIN-EDT-KEY-CVN.
           EXIT.
       
       EDT-IKY-CVN SECTION.
       INI-EDT-IKY-CVN.
           IF CVN-IIC-ICVN-LEN IN CVN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CVN-IIC-ICVN-LEN IN CVN-FLD
                GO TO FIN-EDT-IKY-CVN.
           MOVE CVN-IIC-ICVN IN CVN-FLD TO CVN-IIC-ICVN-EDT IN CVN-EDT.
           MOVE CVN-IIC-ICVN-EDT IN CVN-EDT TO 
                CVN-IIC-ICVN-ALF IN CVN-FLD.
           IF CVN-HRA-HRCV-LEN IN CVN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CVN-HRA-HRCV-LEN IN CVN-FLD
                GO TO FIN-EDT-IKY-CVN.
           MOVE CVN-HRA-HRCV IN CVN-FLD TO CVN-HRA-HRCV-EDT IN CVN-EDT.
           MOVE CVN-HRA-HRCV-EDT IN CVN-EDT TO 
                CVN-HRA-HRCV-ALF IN CVN-FLD.
       FIN-EDT-IKY-CVN.
           EXIT.
       
       EDT-MKY-CVN SECTION.
       INI-EDT-MKY-CVN.
       FIN-EDT-MKY-CVN.
           EXIT.
       
       EDT-AKY-CVN SECTION.
       INI-EDT-AKY-CVN.
       FIN-EDT-AKY-CVN.
           EXIT.
       
       EDT-FLD-CVN SECTION.
       INI-EDT-FLD-CVN.
           IF CVN-NUM-DURA-LEN IN CVN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CVN-NUM-DURA-LEN IN CVN-FLD
                GO TO FIN-EDT-FLD-CVN.
           MOVE CVN-NUM-DURA IN CVN-FLD TO CVN-NUM-DURA-EDT IN CVN-EDT.
           MOVE CVN-NUM-DURA-EDT IN CVN-EDT TO 
                CVN-NUM-DURA-ALF IN CVN-FLD.
       FIN-EDT-FLD-CVN.
           EXIT.
