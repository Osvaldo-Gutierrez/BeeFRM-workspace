       SET-TAG-MC2BQCCM SECTION.
       INI-SET-TAG-MC2BQCCM.
           MOVE
           '<IDNMOD NOM=$MC2BQCCM$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$03-Dec-09 03:30 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-MC2BQCCM.
           EXIT.
           
       PUT-ALL-CCM SECTION.
       INI-PUT-ALL-CCM.
           PERFORM PUT-CCM-CCM.
           PERFORM PUT-DCM-CCM.
       FIN-PUT-ALL-CCM.
           EXIT.
       
       PUT-CCM-CCM SECTION.
       INI-PUT-CCM-CCM.
           IF PGM-STAT-CCM-NUL
               GO TO FIN-PUT-CCM-CCM.
           IF NOT PGM-STAT-CCM-OKS
               PERFORM CLR-CCM-CCM
               GO TO FIN-PUT-CCM-CCM.
