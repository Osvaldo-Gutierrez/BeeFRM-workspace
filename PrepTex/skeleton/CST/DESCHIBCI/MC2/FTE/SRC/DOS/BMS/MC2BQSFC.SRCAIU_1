       SET-TAG-MC2BQSFC SECTION.
       INI-SET-TAG-MC2BQSFC.
           MOVE
           '<IDNMOD NOM=$MC2BQSFC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$22-Feb-19 12:59 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-MC2BQSFC.
           EXIT.
           
       PUT-ALL-SFC SECTION.
       INI-PUT-ALL-SFC.
           PERFORM PUT-SFC-SFC.
           PERFORM PUT-DBC-SFC.
       FIN-PUT-ALL-SFC.
           EXIT.
       
       PUT-SFC-SFC SECTION.
       INI-PUT-SFC-SFC.
           IF PGM-STAT-SFC-NUL
               GO TO FIN-PUT-SFC-SFC.
           IF NOT PGM-STAT-SFC-OKS
               PERFORM CLR-SFC-SFC
               GO TO FIN-PUT-SFC-SFC.
           MOVE SFC-CAI-IOPE IN SFC TO SFC-CAI-IOPE IN SFC-FLD.
           MOVE SFC-IIC-IOPE IN SFC TO SFC-IIC-IOPE IN SFC-FLD.
           MOVE SFC-EXT-IOPE IN SFC TO SFC-EXT-IOPE IN SFC-FLD.
           MOVE SFC-COD-SIST IN SFC TO SFC-COD-SIST IN SFC-FLD.
           MOVE SFC-COD-EVEN IN SFC TO SFC-COD-EVEN IN SFC-FLD.
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE SFC-NUM-DINI IN SFC TO SFC-NUM-DINI IN SFC-FLD.
           MOVE SFC-NUM-MINI IN SFC TO SFC-NUM-MINI IN SFC-FLD.
           MOVE SFC-NUM-SINI IN SFC TO SFC-NUM-SINI IN SFC-FLD.
           MOVE SFC-NUM-AINI IN SFC TO SFC-NUM-AINI IN SFC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE SFC-HRA-HINI IN SFC TO SFC-HRA-HINI IN SFC-FLD.
           MOVE SFC-MSC-STAT IN SFC TO SFC-MSC-STAT IN SFC-FLD.
           MOVE SFC-COD-APRA IN SFC TO SFC-COD-APRA IN SFC-FLD.
           MOVE SFC-NUM-NAPR IN SFC TO SFC-NUM-NAPR IN SFC-FLD.
           MOVE SFC-COD-MAIL IN SFC TO SFC-COD-MAIL IN SFC-FLD.
           MOVE SFC-COD-RAPR IN SFC TO SFC-COD-RAPR IN SFC-FLD.
           MOVE SFC-COD-USER IN SFC TO SFC-COD-USER IN SFC-FLD.
           MOVE SFC-COD-EJEC IN SFC TO SFC-COD-EJEC IN SFC-FLD.
           MOVE SFC-COD-OFIC IN SFC TO SFC-COD-OFIC IN SFC-FLD.
           MOVE SFC-COD-REGN IN SFC TO SFC-COD-REGN IN SFC-FLD.
           MOVE SFC-COD-BANC IN SFC TO SFC-COD-BANC IN SFC-FLD.
           MOVE SFC-SGV-MNTO IN SFC TO SFC-SGV-MNTO IN SFC-FLD.
           MOVE SFC-IND-MNTO IN SFC TO SFC-IND-MNTO IN SFC-FLD.
           MOVE SFC-NUM-PLZO IN SFC TO SFC-NUM-PLZO IN SFC-FLD.
           MOVE SFC-IND-PLZO IN SFC TO SFC-IND-PLZO IN SFC-FLD.
           MOVE SFC-SGV-TMPR IN SFC TO SFC-SGV-TMPR IN SFC-FLD.
           MOVE SFC-SGV-TMSF IN SFC TO SFC-SGV-TMSF IN SFC-FLD.
           MOVE SFC-IND-TAMO IN SFC TO SFC-IND-TAMO IN SFC-FLD.
           MOVE SFC-SGV-SPPR IN SFC TO SFC-SGV-SPPR IN SFC-FLD.
           MOVE SFC-SGV-SPSF IN SFC TO SFC-SGV-SPSF IN SFC-FLD.
           MOVE SFC-IND-SPRD IN SFC TO SFC-IND-SPRD IN SFC-FLD.
           MOVE SFC-SGV-VARI IN SFC TO SFC-SGV-VARI IN SFC-FLD.
           MOVE SFC-SGV-VLRG IN SFC TO SFC-SGV-VLRG IN SFC-FLD.
           MOVE SFC-SGV-VLPI IN SFC TO SFC-SGV-VLPI IN SFC-FLD.
           MOVE SFC-SGV-VLPE IN SFC TO SFC-SGV-VLPE IN SFC-FLD.
           MOVE SFC-GLS-DES1 IN SFC TO SFC-GLS-DES1 IN SFC-FLD.
           MOVE SFC-GLS-DES2 IN SFC TO SFC-GLS-DES2 IN SFC-FLD.
      *<<<<
           IF SFC-COD-SIST IN SFC-FLD > SPACES
              MOVE 'TAB'                   TO TAB-COD-SIST
              MOVE 'SIS'                   TO TAB-COD-TTAB IN TAB
              MOVE SFC-COD-SIST IN SFC-FLD TO TAB-COD-CTAB IN TAB
              PERFORM BUS-TAB
              MOVE TAB-GLS-DCOR IN TAB TO FRM-GLS-SIST IN SFC-FLD
           ELSE
              MOVE SPACES              TO FRM-GLS-SIST IN SFC-FLD.

           IF SFC-COD-EVEN IN SFC-FLD > SPACES
              MOVE 'TAB'                    TO TAB-COD-SIST
              MOVE 'EVT'                    TO TAB-COD-TTAB IN TAB
              MOVE SFC-COD-SIST IN SFC-FLD  TO WSS-COD-SIST IN WSS-VARI
              MOVE SFC-COD-EVEN IN SFC-FLD  TO WSS-COD-EVTO IN WSS-VARI
              MOVE WSS-COD-EVEN IN WSS-VARI TO TAB-COD-CTAB IN TAB
              PERFORM BUS-TAB
              MOVE TAB-GLS-DCOR IN TAB TO FRM-GLS-EVEN IN SFC-FLD
           ELSE
              MOVE SPACES              TO FRM-GLS-EVEN IN SFC-FLD.

           IF SFC-IND-MNTO IN SFC-FLD > SPACES
              MOVE 'TAB'                   TO TAB-COD-SIST
              MOVE 'VLR'                   TO TAB-COD-TTAB IN TAB
              MOVE SFC-IND-MNTO IN SFC-FLD TO TAB-COD-CTAB IN TAB
              PERFORM BUS-TAB
              MOVE TAB-GLS-DCOR IN TAB TO FRM-GLS-MNTO IN SFC-FLD
           ELSE
              MOVE SPACES              TO FRM-GLS-MNTO IN SFC-FLD.

           IF SFC-IND-PLZO IN SFC-FLD > SPACES
              MOVE 'TAB'                   TO MSC-COD-SIST
              MOVE 'IPER'                  TO MSC-COD-TMSC IN MSC
              MOVE SFC-IND-PLZO IN SFC-FLD TO MSC-COD-CMSC IN MSC
              PERFORM BUS-MSC
              MOVE MSC-GLS-DCOR IN MSC TO FRM-GLS-PLZO IN SFC-FLD
           ELSE
              MOVE SPACES              TO FRM-GLS-PLZO IN SFC-FLD.

           IF SFC-IND-TAMO IN SFC-FLD > SPACES
              MOVE 'TAB'                   TO TAB-COD-SIST
              MOVE 'INT'                   TO TAB-COD-TTAB IN TAB
              MOVE SFC-IND-TAMO IN SFC-FLD TO TAB-COD-CTAB IN TAB
              PERFORM BUS-TAB
              MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-TAMO IN SFC-FLD
           ELSE
              MOVE SPACES              TO FRM-GLS-TAMO IN SFC-FLD.

           IF SFC-IND-SPRD IN SFC-FLD > SPACES
              MOVE 'COL'                   TO MSC-COD-SIST
              MOVE 'PBTI'                  TO MSC-COD-TMSC IN MSC
              MOVE SFC-IND-SPRD IN SFC-FLD TO MSC-COD-CMSC IN MSC
              PERFORM BUS-MSC
              MOVE MSC-GLS-DCOR IN MSC TO FRM-GLS-SPRD IN SFC-FLD
           ELSE
              MOVE SPACES              TO FRM-GLS-SPRD IN SFC-FLD.

           MOVE SFC-CIC-ICLI IN SFC TO DBC-CIC-ICLI IN DBC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           MOVE FIO-STAT TO PGM-STAT-DBC.
      *>>>>
       FIN-PUT-SFC-SFC.
           EXIT.
       
       PUT-DBC-SFC SECTION.
       INI-PUT-DBC-SFC.
