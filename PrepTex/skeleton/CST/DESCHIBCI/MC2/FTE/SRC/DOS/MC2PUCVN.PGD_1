       COPY GNSBEGLS.                                                   
       BUS-COR-CVN SECTION.
       INI-BUS-COR-CVN.
           MOVE 99999999 TO CVN-FEC-FCVN IN CVN.
           MOVE 999999   TO CVN-HRA-HRCV IN CVN.
           MOVE FIO-GET-LEQ TO FIO-CMND.
           PERFORM MC2-FIO-CVN.
           IF NOT ( FIO-STAT-OKS AND
                    CVN-CAI-ICVN IN CVN-FLD =CVN-CAI-ICVN IN CVN AND
                    CVN-IIC-ICVN IN CVN-FLD =CVN-IIC-ICVN IN CVN )
               PERFORM KEY-ALL-CVN
               PERFORM GET-FHOY
               MOVE HOY-FHOY TO CVN-FEC-FCVN IN CVN
               MOVE HOY-HHOY TO CVN-HRA-HRCV IN CVN.

           IF SCR-CCPP = 'ING'
              MOVE CVN-NUM-SCVN IN CVN TO CVN-NUM-SCVN IN CVN-FLD
              MOVE CVN-NUM-ACVN IN CVN TO CVN-NUM-ACVN IN CVN-FLD
              MOVE CVN-NUM-MCVN IN CVN TO CVN-NUM-MCVN IN CVN-FLD
              MOVE CVN-NUM-DCVN IN CVN TO CVN-NUM-DCVN IN CVN-FLD
              MOVE CVN-HRA-HRCV IN CVN TO CVN-HRA-HRCV IN CVN-FLD.
       FIN-BUS-COR-CVN.
           EXIT. 

       MOV-TACC-LGA SECTION.
       INI-MOV-TACC-LGA.
           IF SCR-CMND = 'BLQ'
              MOVE 'B'                 TO LGA-IND-TACC IN LGA-VARI
           ELSE
           IF SCR-CMND = 'DBQ'
              MOVE 'D'                 TO LGA-IND-TACC IN LGA-VARI
           ELSE
           IF SCR-CMND = 'ACA'
              MOVE 'A'                 TO LGA-IND-TACC IN LGA-VARI
           ELSE
           IF SCR-CMND = 'ELM'
              MOVE 'L'                 TO LGA-IND-TACC IN LGA-VARI.
       FIN-MOV-TACC-LGA.
           EXIT.

       ELI-ALL-DPC SECTION.                                             
       INI-ELI-ALL-DPC.                                                 
           MOVE DEC-CIC-ICVN IN DEC TO DPC-CIC-ICVN IN DPC.             
           MOVE DEC-CIC-ICLI IN DEC TO DPC-CIC-ICLI IN DPC.             
           MOVE DEC-COD-TREC IN DEC TO DPC-COD-TREC IN DPC.             
           MOVE SPACES TO DPC-COD-SIST IN DPC.                          
           MOVE SPACES TO DPC-COD-GRPO IN DPC.                          
           MOVE ZEROES TO DPC-NUM-IDPC IN DPC.                          
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-ELI-ALL-DPC.                                                 
           PERFORM MC2-FIO-DPC.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
              DPC-CIC-ICVN IN DPC = DEC-CIC-ICVN IN DEC AND             
              DPC-CIC-ICLI IN DPC = DEC-CIC-ICLI IN DEC AND             
              DPC-COD-TREC IN DPC = DEC-COD-TREC IN DEC )               
               GO TO FIN-ELI-ALL-DPC.                                   
                                                                        
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.                            
           PERFORM MC2-FIO-DPC.                                         
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-ELI-ALL-DPC.                                   
           MOVE FIO-DEL TO FIO-CMND.                                    
           PERFORM MC2-FIO-DPC.                                         
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-ELI-ALL-DPC                                    
           ELSE                                                         
               MOVE FIO-GET-NXT TO FIO-CMND                             
               GO TO LUP-ELI-ALL-DPC.                                   
       FIN-ELI-ALL-DPC.                                                 
           EXIT.                                                        
                                                                        
                                                                        
       ELI-ALL-CNT SECTION.                                             
       INI-ELI-ALL-CNT.                                                 
           MOVE DEC-CIC-ICLI IN DEC TO CNT-CIC-ICLE IN CNT.             
           MOVE DEC-CIC-ICVN IN DEC TO CNT-CIC-ICVN IN CNT.             
           MOVE SPACES TO CNT-COD-CCNT IN CNT.                          
           MOVE ZEROES TO CNT-NUM-ICNT IN CNT.                          
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-ELI-ALL-CNT.                                                 
           PERFORM MC2-FIO-CNT.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
              CNT-CIC-ICLE IN CNT = DEC-CIC-ICLI IN DEC AND             
              CNT-CIC-ICVN IN CNT = DEC-CIC-ICVN IN DEC )               
               GO TO FIN-ELI-ALL-CNT.                                   
                                                                        
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.                            
           PERFORM MC2-FIO-CNT.                                         
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-ELI-ALL-CNT.                                   
           MOVE FIO-DEL TO FIO-CMND.                                    
           PERFORM MC2-FIO-CNT.                                         
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-ELI-ALL-CNT                                    
           ELSE                                                         
               MOVE FIO-GET-NXT TO FIO-CMND                             
               GO TO LUP-ELI-ALL-CNT.                                   
       FIN-ELI-ALL-CNT.                                                 
           EXIT.                                                        
                                                                        
       ELI-DEC SECTION.                                                 
       INI-ELI-DEC.                                                     
           MOVE CVN-CIC-ICVN IN CVN TO DEC-CIC-ICVN IN DEC.             
           MOVE 'DEC-KEY-ICVN' TO FIO-AKEY.                             
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM MC2-FIO-DEC.                                         
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-ELI-DEC.                                       
                                                                        
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.                            
           PERFORM MC2-FIO-DEC.                                         
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-ELI-DEC.                                       
                                                                        
           MOVE FIO-DEL TO FIO-CMND.                                    
           PERFORM MC2-FIO-DEC.                                         
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-ELI-DEC.                                       
       FIN-ELI-DEC.                                                     
           EXIT.                                                        
                                                                        
