      *
      * MC2BUPLN
      * Version 3.0.1 Nov-2005
      * 28-Jan-10 01:14 PM
      *
       SET-TAG-MC2BUPLN SECTION.
       INI-SET-TAG-MC2BUPLN.
           MOVE
           '<IDNMOD NOM=$MC2BUPLN$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Jan-10 01:14 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-MC2BUPLN.
           EXIT.
           
       RET-ALL-PLN SECTION.
       INI-RET-ALL-PLN.
           PERFORM KEY-ALL-PLN.
           PERFORM RET-PLN-PLN.
           PERFORM RET-PRD-PLN.
       FIN-RET-ALL-PLN.
           EXIT.
       
       RET-PLN-PLN SECTION.
       INI-RET-PLN-PLN.
           MOVE PLN-MSC-STAT IN PLN-FLD TO PLN-MSC-STAT IN PLN.
       FIN-RET-PLN-PLN.
           EXIT.
       
       RET-PRD-PLN SECTION.
       INI-RET-PRD-PLN.
           MOVE PRD-COD-SIST IN PLN-FLD TO PRD-COD-SIST IN PRD.
           MOVE PRD-COD-VCOC IN PLN-FLD TO PRD-COD-VCOC IN PRD.
           MOVE PRD-COD-TOOC IN PLN-FLD TO PRD-COD-TOOC IN PRD.
           MOVE PRD-COD-AOOC IN PLN-FLD TO PRD-COD-AOOC IN PRD.
       FIN-RET-PRD-PLN.
           EXIT.
       
       KEY-ALL-PLN SECTION.
       INI-KEY-ALL-PLN.
           PERFORM KEY-PLN-PLN.
           PERFORM KEY-PRD-PLN.
       FIN-KEY-ALL-PLN.
           EXIT.
       
       KEY-PLN-PLN SECTION.
       INI-KEY-PLN-PLN.
           MOVE PLN-CAI-PLAN IN PLN-FLD TO PLN-CAI-PLAN IN PLN.
           MOVE PLN-IIC-PLAN IN PLN-FLD TO PLN-IIC-PLAN IN PLN.
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE PLN-NUM-DINI IN PLN-FLD TO PLN-NUM-DINI IN PLN.
           MOVE PLN-NUM-MINI IN PLN-FLD TO PLN-NUM-MINI IN PLN.
           MOVE PLN-NUM-SINI IN PLN-FLD TO PLN-NUM-SINI IN PLN.
           MOVE PLN-NUM-AINI IN PLN-FLD TO PLN-NUM-AINI IN PLN.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE PLN-HRA-HINI IN PLN-FLD TO PLN-HRA-HINI IN PLN.
       FIN-KEY-PLN-PLN.
           EXIT.
       
       KEY-PRD-PLN SECTION.
       INI-KEY-PRD-PLN.
           MOVE PRD-NUM-IPRD IN PLN-FLD TO PRD-NUM-IPRD IN PRD.
       FIN-KEY-PRD-PLN.
           EXIT.
       
       SCR-CLRA-PLN SECTION.
       INI-SCR-CLRA-PLN.
           PERFORM CLR-KEY-PLN.
           PERFORM CLR-FLD-PLN.
       FIN-SCR-CLRA-PLN.
           EXIT.
       
       CLR-KEY-PLN SECTION.
       INI-CLR-KEY-PLN.
           MOVE SPACES TO PLN-CAI-PLAN IN PLN-FLD.
           MOVE SPACES TO PLN-IIC-PLAN IN PLN-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO PLN-FEC-FINI IN PLN-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO PLN-HRA-HINI IN PLN-FLD.
           MOVE ZEROES TO PRD-NUM-IPRD IN PLN-FLD.
       FIN-CLR-KEY-PLN.
           EXIT.
       
       CLR-FLD-PLN SECTION.
       INI-CLR-FLD-PLN.
           MOVE SPACES TO FRM-GLS-GRP1 IN PLN-FLD.
           MOVE SPACES TO FRM-GLS-GRP2 IN PLN-FLD.
           MOVE SPACES TO FRM-GLS-STAT IN PLN-FLD.
           MOVE SPACES TO FRM-GLS-SIST IN PLN-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN PLN-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN PLN-FLD.
           MOVE SPACES TO FRM-GLS-AOOC IN PLN-FLD.
           MOVE SPACES TO FRM-GLS-PROD IN PLN-FLD.
           PERFORM CLR-PLN-PLN.
           PERFORM CLR-PRD-PLN.
       FIN-CLR-FLD-PLN.
           EXIT.
       
       CLR-PLN-PLN SECTION.
       INI-CLR-PLN-PLN.
           MOVE SPACES TO PLN-MSC-STAT IN PLN-FLD.
       FIN-CLR-PLN-PLN.
           EXIT.
       
       CLR-PRD-PLN SECTION.
       INI-CLR-PRD-PLN.
           MOVE SPACES TO PRD-COD-SIST IN PLN-FLD.
           MOVE SPACES TO PRD-COD-VCOC IN PLN-FLD.
           MOVE SPACES TO PRD-COD-TOOC IN PLN-FLD.
           MOVE SPACES TO PRD-COD-AOOC IN PLN-FLD.
       FIN-CLR-PRD-PLN.
           EXIT.
       
       FST-KEY-PLN SECTION.
       INI-FST-KEY-PLN.
           MOVE -1 TO PLN-CAI-PLAN-LEN IN PLN-FLD.
       FIN-FST-KEY-PLN.
           EXIT.
       
       FST-FLD-PLN SECTION.
       INI-FST-FLD-PLN.
      *<<<< INI_FST_FLD_PLN                                             
           IF FRM-GLS-GRP1 IN PLN-FLD > SPACES                          
              MOVE -1 TO PRD-COD-SIST-LEN IN PLN-FLD                    
              GO TO FIN-FST-FLD-PLN.                                    
      *>>>>                                                             
           MOVE -1 TO FRM-GLS-GRP1-LEN IN PLN-FLD.
       FIN-FST-FLD-PLN.
           EXIT.
       
       PRO-KEY-PLN SECTION.
       INI-PRO-KEY-PLN.
           PERFORM PRO-MKY-PLN.
       FIN-PRO-KEY-PLN.
           EXIT.
       
       PRO-MKY-PLN SECTION.
       INI-PRO-MKY-PLN.
       FIN-PRO-MKY-PLN.
           EXIT.
       
       PRO-FLD-PLN SECTION.
       INI-PRO-FLD-PLN.
           MOVE FRM-CPIM-AEY TO FRM-GLS-GRP1-ATR IN PLN-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-GRP2-ATR IN PLN-FLD.
           PERFORM PRO-PLN-PLN.
           PERFORM PRO-PRD-PLN.
       FIN-PRO-FLD-PLN.
           EXIT.
       
       PRO-PLN-PLN SECTION.
       INI-PRO-PLN-PLN.
       FIN-PRO-PLN-PLN.
           EXIT.
       
       PRO-PRD-PLN SECTION.
       INI-PRO-PRD-PLN.
           MOVE FRM-CPIM-AEY TO PRD-COD-SIST-ATR IN PLN-FLD.
           MOVE FRM-CPIM-AEY TO PRD-COD-VCOC-ATR IN PLN-FLD.
           MOVE FRM-CPIM-AEY TO PRD-COD-TOOC-ATR IN PLN-FLD.
           MOVE FRM-CPIM-AEY TO PRD-COD-AOOC-ATR IN PLN-FLD.
       FIN-PRO-PRD-PLN.
           EXIT.
       
       UNP-KEY-PLN SECTION.
       INI-UNP-KEY-PLN.
           PERFORM UNP-IKY-PLN.
           PERFORM UNP-AKY-PLN.
           PERFORM UNP-MKY-PLN.
       FIN-UNP-KEY-PLN.
           EXIT.
       
       UNP-IKY-PLN SECTION.
       INI-UNP-IKY-PLN.
           MOVE FRM-CPIM-UBY TO PLN-CAI-PLAN-ATR.
           MOVE FRM-CPIM-UBY TO PLN-IIC-PLAN-ATR.
           MOVE FRM-CPIM-UBY TO PLN-FEC-FINI-ATR IN PLN-FLD.
           MOVE FRM-CPIM-UBY TO PLN-HRA-HINI-ATR.
           MOVE FRM-CPIM-UBY TO PRD-NUM-IPRD-ATR.
      *<<<< FIN_UNP_IKY_PLN                                             
           IF  SCR-CMND = 'ING' OR 'VRF' OR 'ACA' OR 'ANL'              
               MOVE FRM-CPIM-AEY TO PRD-NUM-IPRD-ATR IN PLN-FLD.        
      *>>>>                                                             
       FIN-UNP-IKY-PLN.
           EXIT.
       
       UNP-MKY-PLN SECTION.
       INI-UNP-MKY-PLN.
       FIN-UNP-MKY-PLN.
           EXIT.
       
       UNP-AKY-PLN SECTION.
       INI-UNP-AKY-PLN.
       FIN-UNP-AKY-PLN.
           EXIT.
       
       UNP-FLD-PLN SECTION.
       INI-UNP-FLD-PLN.
           MOVE FRM-CPIM-UBY TO FRM-GLS-GRP1-ATR IN PLN-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-GRP2-ATR IN PLN-FLD.
           PERFORM UNP-PLN-PLN.
           PERFORM UNP-PRD-PLN.
      *<<<< FIN_UNP_FLD_PLN                                             
           IF FRM-GLS-GRP1 IN PLN-FLD > SPACES                          
              MOVE FRM-CPIM-AEY TO FRM-GLS-GRP1-ATR IN PLN-FLD          
              MOVE FRM-CPIM-AEY TO FRM-GLS-GRP2-ATR IN PLN-FLD.         
      *>>>>                                                             
       FIN-UNP-FLD-PLN.
           EXIT.
       
       UNP-PLN-PLN SECTION.
       INI-UNP-PLN-PLN.
       FIN-UNP-PLN-PLN.
           EXIT.
       
       UNP-PRD-PLN SECTION.
       INI-UNP-PRD-PLN.
           MOVE FRM-CPIM-UBY TO PRD-COD-SIST-ATR IN PLN-FLD.
           MOVE FRM-CPIM-UBY TO PRD-COD-VCOC-ATR IN PLN-FLD.
           MOVE FRM-CPIM-UBY TO PRD-COD-TOOC-ATR IN PLN-FLD.
           MOVE FRM-CPIM-UBY TO PRD-COD-AOOC-ATR IN PLN-FLD.
      *<<<< FIN_UNP_PRD_PLN                                             
           IF SCR-CMND = 'ANL'                                          
              MOVE FRM-CPIM-AEY TO PRD-COD-SIST-ATR IN PLN-FLD          
              MOVE FRM-CPIM-AEY TO PRD-COD-VCOC-ATR IN PLN-FLD          
              MOVE FRM-CPIM-AEY TO PRD-COD-TOOC-ATR IN PLN-FLD          
              MOVE FRM-CPIM-AEY TO PRD-COD-AOOC-ATR IN PLN-FLD.         
      *>>>>                                                             
       FIN-UNP-PRD-PLN.
           EXIT.
       
       VAL-MKY-PLN SECTION.
       INI-VAL-MKY-PLN.
       FIN-VAL-MKY-PLN.
           EXIT.
       
       VAL-FLD-PLN SECTION.
       INI-VAL-FLD-PLN.
           PERFORM VAL-FRM-GLS-GRP1.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-GRP1-LEN IN PLN-FLD
                GO TO FIN-VAL-FLD-PLN.
           PERFORM VAL-PLN-MSC-STAT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PLN-MSC-STAT-LEN IN PLN-FLD
                GO TO FIN-VAL-FLD-PLN.
           PERFORM VAL-FRM-GLS-GRP2.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-GRP2-LEN IN PLN-FLD
                GO TO FIN-VAL-FLD-PLN.
           PERFORM VAL-FRM-GLS-STAT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-STAT-LEN IN PLN-FLD
                GO TO FIN-VAL-FLD-PLN.
           PERFORM VAL-PRD-COD-SIST.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PRD-COD-SIST-LEN IN PLN-FLD
                GO TO FIN-VAL-FLD-PLN.
           PERFORM VAL-FRM-GLS-SIST.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-SIST-LEN IN PLN-FLD
                GO TO FIN-VAL-FLD-PLN.
           PERFORM VAL-PRD-COD-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PRD-COD-VCOC-LEN IN PLN-FLD
                GO TO FIN-VAL-FLD-PLN.
           PERFORM VAL-FRM-GLS-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-VCOC-LEN IN PLN-FLD
                GO TO FIN-VAL-FLD-PLN.
           PERFORM VAL-PRD-COD-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PRD-COD-TOOC-LEN IN PLN-FLD
                GO TO FIN-VAL-FLD-PLN.
           PERFORM VAL-FRM-GLS-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TOOC-LEN IN PLN-FLD
                GO TO FIN-VAL-FLD-PLN.
           PERFORM VAL-PRD-COD-AOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PRD-COD-AOOC-LEN IN PLN-FLD
                GO TO FIN-VAL-FLD-PLN.
           PERFORM VAL-FRM-GLS-AOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-AOOC-LEN IN PLN-FLD
                GO TO FIN-VAL-FLD-PLN.
           PERFORM VAL-FRM-GLS-PROD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-PROD-LEN IN PLN-FLD
                GO TO FIN-VAL-FLD-PLN.
       FIN-VAL-FLD-PLN.
           EXIT.
       
       VAL-FRM-GLS-GRP1 SECTION.
       INI-VAL-FRM-GLS-GRP1.
           IF FRM-GLS-GRP1 IN PLN-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-FRM-GLS-GRP1.
       FIN-VAL-FRM-GLS-GRP1.
           EXIT.
       
       VAL-PLN-MSC-STAT SECTION.
       INI-VAL-PLN-MSC-STAT.
       FIN-VAL-PLN-MSC-STAT.
           EXIT.
       
       VAL-FRM-GLS-GRP2 SECTION.
       INI-VAL-FRM-GLS-GRP2.
           IF FRM-GLS-GRP2 IN PLN-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-GRP2 IN PLN-FLD.
       FIN-VAL-FRM-GLS-GRP2.
           EXIT.
       
       VAL-FRM-GLS-STAT SECTION.
       INI-VAL-FRM-GLS-STAT.
       FIN-VAL-FRM-GLS-STAT.
           EXIT.
       
       VAL-PRD-COD-SIST SECTION.
       INI-VAL-PRD-COD-SIST.
           IF PRD-COD-SIST IN PLN-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-PRD-COD-SIST.
      *<<<< PRD_COD_SIST                                                
      *VAL-COD Valida codigo de tabla                                   
           MOVE 'TAB'                   TO TAB-COD-SIST.                
           MOVE 'SIS'                   TO TAB-COD-TTAB IN TAB.         
           MOVE PRD-COD-SIST IN PLN-FLD TO TAB-COD-CTAB IN TAB.         
           PERFORM VAL-COD.                                             
           IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'               
              MOVE SPACES       TO FRM-GLS-SIST IN PLN-FLD              
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              MOVE FRM-SUAR-MAL TO FRM-SUAR                             
           ELSE                                                         
              MOVE TAB-CIC-CTAB IN TAB TO WSS-CIC-SIST IN WSS-VARI      
              MOVE TAB-GLS-DCOR IN TAB TO FRM-GLS-SIST IN PLN-FLD.      
      *>>>>                                                             
       FIN-VAL-PRD-COD-SIST.
           EXIT.
       
       VAL-FRM-GLS-SIST SECTION.
       INI-VAL-FRM-GLS-SIST.
       FIN-VAL-FRM-GLS-SIST.
           EXIT.
       
       VAL-PRD-COD-VCOC SECTION.
       INI-VAL-PRD-COD-VCOC.
           IF PRD-COD-VCOC IN PLN-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-PRD-COD-VCOC.
      *<<<< PRD_COD_VCOC                                                
      *VAL-COD Valida codigo de tabla                                   
           MOVE 'TAB'                   TO TAB-COD-SIST.                
           MOVE 'VLR'                   TO TAB-COD-TTAB IN TAB.         
           MOVE PRD-COD-VCOC IN PLN-FLD TO TAB-COD-CTAB IN TAB.         
           PERFORM VAL-COD.                                             
           IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'               
              MOVE SPACES       TO FRM-GLS-VCOC IN PLN-FLD              
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              MOVE FRM-SUAR-MAL TO FRM-SUAR                             
           ELSE                                                         
              MOVE TAB-GLS-DCOR IN TAB TO FRM-GLS-VCOC IN PLN-FLD.      
      *>>>>                                                             
       FIN-VAL-PRD-COD-VCOC.
           EXIT.
       
       VAL-FRM-GLS-VCOC SECTION.
       INI-VAL-FRM-GLS-VCOC.
       FIN-VAL-FRM-GLS-VCOC.
           EXIT.
       
       VAL-PRD-COD-TOOC SECTION.
       INI-VAL-PRD-COD-TOOC.
           IF PRD-COD-TOOC IN PLN-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-PRD-COD-TOOC.
      *<<<< PRD_COD_TOOC                                                
      *VAL-COD Valida codigo de tabla                                   
           MOVE 'TAB'                   TO TAB-COD-SIST.                
           MOVE 'TIO '                  TO TAB-COD-TTAB IN TAB.         
           MOVE PRD-COD-TOOC IN PLN-FLD TO TAB-COD-CTAB IN TAB.         
           PERFORM VAL-COD.                                             
           IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'               
               MOVE SPACES       TO FRM-GLS-TOOC IN PLN-FLD             
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE FRM-SUAR-MAL TO FRM-SUAR                            
           ELSE                                                         
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-TOOC IN PLN-FLD.     
                                                                        
           MOVE TAB-GLS-DAT1 IN TAB TO WSS-GLS-DAT1 IN WSS-VARI.        
           IF WSS-TIO-SIST IN WSS-VARI NOT > SPACES                     
              GO TO FIN-VAL-PRD-COD-TOOC.                               
                                                                        
           IF ((WSS-TIO-SIST IN WSS-VARI = 'CRE' OR 'COL') AND          
               (WSS-CIC-SIST IN WSS-VARI = 'CRE' OR 'COL'))             
              GO TO FIN-VAL-PRD-COD-TOOC.                               
                                                                        
           IF WSS-TIO-SIST IN WSS-VARI NOT = WSS-CIC-SIST IN WSS-VARI   
              MOVE 'TAB'          TO MSG-COD-SIST                       
              MOVE 'SISTIO   INC' TO MSG-COD-MENS                       
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              MOVE FRM-SUAR-MAL TO FRM-SUAR                             
              GO TO FIN-VAL-PRD-COD-TOOC.                               
      *>>>>                                                             
       FIN-VAL-PRD-COD-TOOC.
           EXIT.
       
       VAL-FRM-GLS-TOOC SECTION.
       INI-VAL-FRM-GLS-TOOC.
       FIN-VAL-FRM-GLS-TOOC.
           EXIT.
       
       VAL-PRD-COD-AOOC SECTION.
       INI-VAL-PRD-COD-AOOC.
           IF PRD-COD-AOOC IN PLN-FLD NOT > SPACES
              MOVE SPACES TO PRD-COD-AOOC IN PLN-FLD.
      *<<<< PRD_COD_AOOC                                                
           IF PRD-COD-AOOC IN PLN-FLD = SPACES                          
              MOVE 'TODOS        ' TO FRM-GLS-AOOC IN PLN-FLD           
              GO TO FIN-VAL-PRD-COD-AOOC.                               
                                                                        
      *VAL-COD Valida codigo de tabla                                   
           MOVE 'TAB'                    TO TAB-COD-SIST.               
           MOVE 'AUX'                    TO TAB-COD-TTAB IN TAB.        
           MOVE PRD-COD-TOOC IN PLN-FLD  TO WSS-COD-TOOC IN WSS-VARI.   
           MOVE PRD-COD-AOOC IN PLN-FLD  TO WSS-COD-AOOC IN WSS-VARI.   
           MOVE WSS-COD-CAUX IN WSS-VARI TO TAB-COD-CTAB IN TAB.        
           PERFORM VAL-COD.                                             
           IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'               
               MOVE SPACES       TO FRM-GLS-AOOC IN PLN-FLD             
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE FRM-SUAR-MAL TO FRM-SUAR                            
           ELSE                                                         
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-AOOC IN PLN-FLD.     
      *>>>>                                                             
       FIN-VAL-PRD-COD-AOOC.
           EXIT.
       
       VAL-FRM-GLS-AOOC SECTION.
       INI-VAL-FRM-GLS-AOOC.
       FIN-VAL-FRM-GLS-AOOC.
           EXIT.
       
       VAL-FRM-GLS-PROD SECTION.
       INI-VAL-FRM-GLS-PROD.
      *<<<< FRM_GLS_PROD                                                
           MOVE PRD-COD-SIST IN PLN-FLD  TO WSS-IND-SIST IN WSS-VARI.   
           MOVE PRD-COD-VCOC IN PLN-FLD  TO WSS-IND-VCOC IN WSS-VARI.   
           MOVE PRD-COD-TOOC IN PLN-FLD  TO WSS-IND-TOOC IN WSS-VARI.   
           MOVE PRD-COD-AOOC IN PLN-FLD  TO WSS-IND-AOOC IN WSS-VARI.   
           MOVE WSS-GLS-DESC IN WSS-VARI TO FRM-GLS-PROD IN PLN-FLD.    
      *>>>>                                                             
       FIN-VAL-FRM-GLS-PROD.
           EXIT.
       
       VAL-CON-FLD-PLN SECTION.
       INI-VAL-CON-FLD-PLN.
      *<<<< VAL_CON_FLD_PLN                                             
           IF NOT (WSS-CIC-SIST IN WSS-VARI = 'CRE' OR 'COL')           
              GO TO FIN-VAL-CON-FLD-PLN.                                
           IF PRD-COD-AOOC IN PLN-FLD NOT > SPACES                      
              GO TO FIN-VAL-CON-FLD-PLN.                                
           MOVE PRD-COD-VCOC IN PLN-FLD TO POC-COD-VCOC IN POC.         
           MOVE PRD-COD-TOOC IN PLN-FLD TO POC-COD-TOOC IN POC.         
           MOVE PRD-COD-AOOC IN PLN-FLD TO POC-COD-AOOC IN POC.         
           MOVE ZEROES                  TO POC-NUM-IPOC IN POC.         
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM COL-FIO-POC.                                         
           IF NOT FIO-STAT-OKS                                          
              MOVE 'TAB'        TO MSG-COD-SIST                         
              MOVE 'POC    NEX' TO MSG-COD-MENS                         
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              MOVE FRM-SUAR-MAL TO FRM-SUAR.                            
      *>>>>                                                             
       FIN-VAL-CON-FLD-PLN.
           EXIT.
       
       VAL-CON-PLN SECTION.
       INI-VAL-CON-PLN.
       FIN-VAL-CON-PLN.
           EXIT.
       
       VAL-NUM-KEY-PLN SECTION.
       INI-VAL-NUM-KEY-PLN.
           PERFORM VAL-NUM-IKY-PLN.
           PERFORM VAL-NUM-MKY-PLN.
           PERFORM VAL-NUM-AKY-PLN.
       FIN-VAL-NUM-KEY-PLN.
           EXIT.
       
       VAL-NUM-IKY-PLN SECTION.
       INI-VAL-NUM-IKY-PLN.
       VAL-NUM-PLN-HRA-HINI.
           MOVE PLN-HRA-HINI-ALF IN PLN-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 6 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO PLN-HRA-HINI IN PLN-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO PLN-HRA-HINI-LEN IN PLN-FLD
                GO TO FIN-VAL-NUM-IKY-PLN.
       VAL-NUM-PRD-NUM-IPRD.
           MOVE PRD-NUM-IPRD-ALF IN PLN-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO PRD-NUM-IPRD IN PLN-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO PRD-NUM-IPRD-LEN IN PLN-FLD
                GO TO FIN-VAL-NUM-IKY-PLN.
       FIN-VAL-NUM-IKY-PLN.
           EXIT.
       
       VAL-NUM-MKY-PLN SECTION.
       INI-VAL-NUM-MKY-PLN.
       FIN-VAL-NUM-MKY-PLN.
           EXIT.
       
       VAL-NUM-AKY-PLN SECTION.
       INI-VAL-NUM-AKY-PLN.
       FIN-VAL-NUM-AKY-PLN.
           EXIT.
       
       VAL-NUM-FLD-PLN SECTION.
       INI-VAL-NUM-FLD-PLN.
       FIN-VAL-NUM-FLD-PLN.
           EXIT.
       
       EDT-KEY-PLN SECTION.
       INI-EDT-KEY-PLN.
           PERFORM EDT-IKY-PLN.
           PERFORM EDT-MKY-PLN.
           PERFORM EDT-AKY-PLN.
       FIN-EDT-KEY-PLN.
           EXIT.
       
       EDT-IKY-PLN SECTION.
       INI-EDT-IKY-PLN.
      *<<<< EDT_IKY_PLN                                                 
           GO TO EXT-EDT-IKY-PLN.                                       
      *>>>>                                                             
           IF PLN-HRA-HINI-LEN IN PLN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PLN-HRA-HINI-LEN IN PLN-FLD
                GO TO FIN-EDT-IKY-PLN.
           MOVE PLN-HRA-HINI IN PLN-FLD TO PLN-HRA-HINI-EDT IN PLN-EDT.
           MOVE PLN-HRA-HINI-EDT IN PLN-EDT TO 
                PLN-HRA-HINI-ALF IN PLN-FLD.
           IF PRD-NUM-IPRD-LEN IN PLN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PRD-NUM-IPRD-LEN IN PLN-FLD
                GO TO FIN-EDT-IKY-PLN.
           MOVE PRD-NUM-IPRD IN PLN-FLD TO PRD-NUM-IPRD-EDT IN PLN-EDT.
           MOVE PRD-NUM-IPRD-EDT IN PLN-EDT TO 
                PRD-NUM-IPRD-ALF IN PLN-FLD.
      *<<<< FIN_EDT_IKY_PLN                                             
       EXT-EDT-IKY-PLN.                                                 
           IF PLN-HRA-HINI-LEN IN PLN-FLD = FRM-ERR-NUME                
                MOVE FRM-IND-ERRO TO PLN-HRA-HINI-LEN IN PLN-FLD        
                GO TO FIN-EDT-IKY-PLN.                                  
           MOVE PLN-HRA-HINI IN PLN-FLD TO WSS-HRA-HINI-EDT.            
           MOVE WSS-HRA-HINI-EDT        TO PLN-HRA-HINI-ALF IN PLN-FLD. 
           IF PRD-NUM-IPRD-LEN IN PLN-FLD = FRM-ERR-NUME                
                MOVE FRM-IND-ERRO TO PRD-NUM-IPRD-LEN IN PLN-FLD        
                GO TO FIN-EDT-IKY-PLN.                                  
           MOVE PRD-NUM-IPRD IN PLN-FLD TO PRD-NUM-IPRD-EDT IN PLN-EDT. 
           MOVE PRD-NUM-IPRD-EDT IN PLN-EDT TO                          
                PRD-NUM-IPRD-ALF IN PLN-FLD.                            
      *>>>>                                                             
       FIN-EDT-IKY-PLN.
           EXIT.
       
       EDT-MKY-PLN SECTION.
       INI-EDT-MKY-PLN.
       FIN-EDT-MKY-PLN.
           EXIT.
       
       EDT-AKY-PLN SECTION.
       INI-EDT-AKY-PLN.
       FIN-EDT-AKY-PLN.
           EXIT.
       
       EDT-FLD-PLN SECTION.
       INI-EDT-FLD-PLN.
       FIN-EDT-FLD-PLN.
           EXIT.
