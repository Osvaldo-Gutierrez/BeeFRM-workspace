      *
      * MC2BUAGD
      * Version 3.0.1 Nov-2005
      * 19-Feb-19 03:59 PM
      *
       SET-TAG-MC2BUAGD SECTION.
       INI-SET-TAG-MC2BUAGD.
           MOVE
           '<IDNMOD NOM=$MC2BUAGD$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$19-Feb-19 03:59 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-MC2BUAGD.
           EXIT.
           
       RET-ALL-AGD SECTION.
       INI-RET-ALL-AGD.
           PERFORM KEY-ALL-AGD.
           PERFORM RET-AGD-AGD.
       FIN-RET-ALL-AGD.
           EXIT.
       
       RET-AGD-AGD SECTION.
       INI-RET-AGD-AGD.
           MOVE AGD-COD-TAGD IN AGD-FLD TO AGD-COD-TAGD IN AGD.
           MOVE AGD-COD-CAGD IN AGD-FLD TO AGD-COD-CAGD IN AGD.
       FIN-RET-AGD-AGD.
           EXIT.
       
       KEY-ALL-AGD SECTION.
       INI-KEY-ALL-AGD.
           PERFORM KEY-AGD-AGD.
       FIN-KEY-ALL-AGD.
           EXIT.
       
       KEY-AGD-AGD SECTION.
       INI-KEY-AGD-AGD.
           MOVE AGD-CAI-IAGD IN AGD-FLD TO AGD-CAI-IAGD IN AGD.
           MOVE AGD-IIC-IAGD IN AGD-FLD TO AGD-IIC-IAGD IN AGD.
           MOVE AGD-NUM-IAGD IN AGD-FLD TO AGD-NUM-IAGD IN AGD.
       FIN-KEY-AGD-AGD.
           EXIT.
       
       SCR-CLRA-AGD SECTION.
       INI-SCR-CLRA-AGD.
           PERFORM CLR-KEY-AGD.
           PERFORM CLR-FLD-AGD.
       FIN-SCR-CLRA-AGD.
           EXIT.
       
       CLR-KEY-AGD SECTION.
       INI-CLR-KEY-AGD.
           MOVE SPACES TO AGD-CAI-IAGD IN AGD-FLD.
           MOVE SPACES TO AGD-IIC-IAGD IN AGD-FLD.
           MOVE ZEROES TO AGD-NUM-IAGD IN AGD-FLD.
       FIN-CLR-KEY-AGD.
           EXIT.
       
       CLR-FLD-AGD SECTION.
       INI-CLR-FLD-AGD.
           MOVE SPACES TO FRM-GLS-DES1 IN AGD-FLD.
           MOVE SPACES TO FRM-GLS-DES2 IN AGD-FLD.
           MOVE SPACES TO FRM-GLS-TAGD IN AGD-FLD.
           MOVE SPACES TO FRM-GLS-CAGD IN AGD-FLD.
           PERFORM CLR-AGD-AGD.
       FIN-CLR-FLD-AGD.
           EXIT.
       
       CLR-AGD-AGD SECTION.
       INI-CLR-AGD-AGD.
           MOVE SPACES TO AGD-COD-TAGD IN AGD-FLD.
           MOVE SPACES TO AGD-COD-CAGD IN AGD-FLD.
       FIN-CLR-AGD-AGD.
           EXIT.
       
       FST-KEY-AGD SECTION.
       INI-FST-KEY-AGD.
           MOVE -1 TO AGD-CAI-IAGD-LEN IN AGD-FLD.
       FIN-FST-KEY-AGD.
           EXIT.
       
       FST-FLD-AGD SECTION.
       INI-FST-FLD-AGD.
           MOVE -1 TO AGD-COD-TAGD-LEN IN AGD-FLD.
       FIN-FST-FLD-AGD.
           EXIT.
       
       PRO-KEY-AGD SECTION.
       INI-PRO-KEY-AGD.
           PERFORM PRO-MKY-AGD.
       FIN-PRO-KEY-AGD.
           EXIT.
       
       PRO-MKY-AGD SECTION.
       INI-PRO-MKY-AGD.
       FIN-PRO-MKY-AGD.
           EXIT.
       
       PRO-FLD-AGD SECTION.
       INI-PRO-FLD-AGD.
           PERFORM PRO-AGD-AGD.
       FIN-PRO-FLD-AGD.
           EXIT.
       
       PRO-AGD-AGD SECTION.
       INI-PRO-AGD-AGD.
           MOVE FRM-CPIM-AEY TO AGD-COD-TAGD-ATR IN AGD-FLD.
           MOVE FRM-CPIM-AEY TO AGD-COD-CAGD-ATR IN AGD-FLD.
       FIN-PRO-AGD-AGD.
           EXIT.
       
       UNP-KEY-AGD SECTION.
       INI-UNP-KEY-AGD.
           PERFORM UNP-IKY-AGD.
           PERFORM UNP-AKY-AGD.
           PERFORM UNP-MKY-AGD.
       FIN-UNP-KEY-AGD.
           EXIT.
       
       UNP-IKY-AGD SECTION.
       INI-UNP-IKY-AGD.
           MOVE FRM-CPIM-UBY TO AGD-CAI-IAGD-ATR.
           MOVE FRM-CPIM-UBY TO AGD-IIC-IAGD-ATR.
           MOVE FRM-CPIM-UBY TO AGD-NUM-IAGD-ATR.
       FIN-UNP-IKY-AGD.
           EXIT.
       
       UNP-MKY-AGD SECTION.
       INI-UNP-MKY-AGD.
       FIN-UNP-MKY-AGD.
           EXIT.
       
       UNP-AKY-AGD SECTION.
       INI-UNP-AKY-AGD.
       FIN-UNP-AKY-AGD.
           EXIT.
       
       UNP-FLD-AGD SECTION.
       INI-UNP-FLD-AGD.
           PERFORM UNP-AGD-AGD.
       FIN-UNP-FLD-AGD.
           EXIT.
       
       UNP-AGD-AGD SECTION.
       INI-UNP-AGD-AGD.
           MOVE FRM-CPIM-UBY TO AGD-COD-TAGD-ATR IN AGD-FLD.
           MOVE FRM-CPIM-UBY TO AGD-COD-CAGD-ATR IN AGD-FLD.
       FIN-UNP-AGD-AGD.
           EXIT.
       
       VAL-MKY-AGD SECTION.
       INI-VAL-MKY-AGD.
       FIN-VAL-MKY-AGD.
           EXIT.
       
       VAL-FLD-AGD SECTION.
       INI-VAL-FLD-AGD.
           PERFORM VAL-FRM-GLS-DES1.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-DES1-LEN IN AGD-FLD
                GO TO FIN-VAL-FLD-AGD.
           PERFORM VAL-FRM-GLS-DES2.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-DES2-LEN IN AGD-FLD
                GO TO FIN-VAL-FLD-AGD.
           PERFORM VAL-AGD-COD-TAGD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO AGD-COD-TAGD-LEN IN AGD-FLD
                GO TO FIN-VAL-FLD-AGD.
           PERFORM VAL-FRM-GLS-TAGD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TAGD-LEN IN AGD-FLD
                GO TO FIN-VAL-FLD-AGD.
           PERFORM VAL-AGD-COD-CAGD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO AGD-COD-CAGD-LEN IN AGD-FLD
                GO TO FIN-VAL-FLD-AGD.
           PERFORM VAL-FRM-GLS-CAGD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-CAGD-LEN IN AGD-FLD
                GO TO FIN-VAL-FLD-AGD.
       FIN-VAL-FLD-AGD.
           EXIT.
       
       VAL-FRM-GLS-DES1 SECTION.
       INI-VAL-FRM-GLS-DES1.
       FIN-VAL-FRM-GLS-DES1.
           EXIT.
       
       VAL-FRM-GLS-DES2 SECTION.
       INI-VAL-FRM-GLS-DES2.
       FIN-VAL-FRM-GLS-DES2.
           EXIT.
       
       VAL-AGD-COD-TAGD SECTION.
       INI-VAL-AGD-COD-TAGD.
           IF AGD-COD-TAGD IN AGD-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-AGD-COD-TAGD.
      *<<<<                                                                  
           MOVE 'TAB'                   TO TAB-COD-SIST.                
           MOVE 'AGD'                   TO TAB-CIC-TTAB IN TAB.         
           MOVE AGD-COD-TAGD IN AGD-FLD TO TAB-CIC-CTAB IN TAB.         
           MOVE 'TAB-CIC-TABL'          TO FIO-AKEY.                    
           PERFORM VAL-TAB.                                             
           IF TAB-COD-DAT3(1) NOT > SPACES OR                           
              TAB-COD-DAT3(1) = 'SIS'                                   
              MOVE 'COD    NVG' TO MSG-COD-MENS                         
              PERFORM GET-MSG.                                          
           IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'               
              MOVE SPACES       TO FRM-GLS-TAGD IN AGD-FLD              
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              MOVE FRM-SUAR-MAL TO FRM-SUAR                             
           ELSE                                                         
              MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-TAGD IN AGD-FLD.      
      *>>>>
       FIN-VAL-AGD-COD-TAGD.
           EXIT.
       
       VAL-FRM-GLS-TAGD SECTION.
       INI-VAL-FRM-GLS-TAGD.
       FIN-VAL-FRM-GLS-TAGD.
           EXIT.
       
       VAL-AGD-COD-CAGD SECTION.
       INI-VAL-AGD-COD-CAGD.
           IF AGD-COD-CAGD IN AGD-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-AGD-COD-CAGD.
      *<<<<                                                                  
           IF TAB-COD-DAT3(10) NOT > SPACES                             
              MOVE TAB-COD-DAT3(1)         TO TAB-COD-SIST              
              MOVE TAB-GLS-DAT1 IN TAB     TO TAB-COD-TTAB IN TAB       
              MOVE AGD-COD-CAGD IN AGD-FLD TO TAB-COD-CTAB IN TAB       
              PERFORM VAL-TAB                                           
              IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'            
                 MOVE SPACES       TO FRM-GLS-CAGD IN AGD-FLD           
                 MOVE MSG-GLS-DESC TO FRM-MENS                          
                 MOVE FRM-SUAR-MAL TO FRM-SUAR                          
              ELSE                                                      
                 MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-CAGD IN AGD-FLD    
           ELSE                                                         
              MOVE TAB-COD-DAT3(1)         TO MSC-COD-SIST              
              MOVE TAB-GLS-DAT1 IN TAB     TO MSC-COD-TMSC IN MSC       
              MOVE AGD-COD-CAGD IN AGD-FLD TO MSC-COD-CMSC IN MSC       
              PERFORM VAL-MSC                                           
              IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'            
                 MOVE SPACES       TO FRM-GLS-CAGD IN AGD-FLD           
                 MOVE MSG-GLS-DESC TO FRM-MENS                          
                 MOVE FRM-SUAR-MAL TO FRM-SUAR                          
              ELSE                                                      
                 MOVE MSC-GLS-DESC IN MSC TO FRM-GLS-CAGD IN AGD-FLD.   
      *>>>>
       FIN-VAL-AGD-COD-CAGD.
           EXIT.
       
       VAL-FRM-GLS-CAGD SECTION.
       INI-VAL-FRM-GLS-CAGD.
       FIN-VAL-FRM-GLS-CAGD.
           EXIT.
       
       VAL-CON-FLD-AGD SECTION.
       INI-VAL-CON-FLD-AGD.
       FIN-VAL-CON-FLD-AGD.
           EXIT.
       
       VAL-CON-AGD SECTION.
       INI-VAL-CON-AGD.
       FIN-VAL-CON-AGD.
           EXIT.
       
       VAL-NUM-KEY-AGD SECTION.
       INI-VAL-NUM-KEY-AGD.
           PERFORM VAL-NUM-IKY-AGD.
           PERFORM VAL-NUM-MKY-AGD.
           PERFORM VAL-NUM-AKY-AGD.
       FIN-VAL-NUM-KEY-AGD.
           EXIT.
       
       VAL-NUM-IKY-AGD SECTION.
       INI-VAL-NUM-IKY-AGD.
       VAL-NUM-AGD-NUM-IAGD.
           MOVE AGD-NUM-IAGD-ALF IN AGD-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO AGD-NUM-IAGD IN AGD-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO AGD-NUM-IAGD-LEN IN AGD-FLD
                GO TO FIN-VAL-NUM-IKY-AGD.
       FIN-VAL-NUM-IKY-AGD.
           EXIT.
       
       VAL-NUM-MKY-AGD SECTION.
       INI-VAL-NUM-MKY-AGD.
       FIN-VAL-NUM-MKY-AGD.
           EXIT.
       
       VAL-NUM-AKY-AGD SECTION.
       INI-VAL-NUM-AKY-AGD.
       FIN-VAL-NUM-AKY-AGD.
           EXIT.
       
       VAL-NUM-FLD-AGD SECTION.
       INI-VAL-NUM-FLD-AGD.
       FIN-VAL-NUM-FLD-AGD.
           EXIT.
       
       EDT-KEY-AGD SECTION.
       INI-EDT-KEY-AGD.
           PERFORM EDT-IKY-AGD.
           PERFORM EDT-MKY-AGD.
           PERFORM EDT-AKY-AGD.
       FIN-EDT-KEY-AGD.
           EXIT.
       
       EDT-IKY-AGD SECTION.
       INI-EDT-IKY-AGD.
           IF AGD-NUM-IAGD-LEN IN AGD-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO AGD-NUM-IAGD-LEN IN AGD-FLD
                GO TO FIN-EDT-IKY-AGD.
           MOVE AGD-NUM-IAGD IN AGD-FLD TO AGD-NUM-IAGD-EDT IN AGD-EDT.
           MOVE AGD-NUM-IAGD-EDT IN AGD-EDT TO 
                AGD-NUM-IAGD-ALF IN AGD-FLD.
       FIN-EDT-IKY-AGD.
           EXIT.
       
       EDT-MKY-AGD SECTION.
       INI-EDT-MKY-AGD.
       FIN-EDT-MKY-AGD.
           EXIT.
       
       EDT-AKY-AGD SECTION.
       INI-EDT-AKY-AGD.
       FIN-EDT-AKY-AGD.
           EXIT.
       
       EDT-FLD-AGD SECTION.
       INI-EDT-FLD-AGD.
       FIN-EDT-FLD-AGD.
           EXIT.
