       PROCEDURE DIVISION.
      *===================

       MAIN SECTION.
       INI-MAIN.
           PERFORM INI.
       LUP-MAIN.
           IF SCR-INST = SCR-INST-HCM
                 PERFORM SCR-COM.
           IF SCR-CCPP = 'MOD'
                 PERFORM SCR-MOD
           ELSE
           IF SCR-CCPP = 'ING'
                 PERFORM SCR-ING
           ELSE
           IF SCR-CCPP = 'ACC'
                 PERFORM SCR-ACC
           ELSE
           IF SCR-CCPP = 'ELI'
                 PERFORM SCR-ELI
           ELSE
           IF SCR-CCPP = 'ACT'
                 PERFORM SCR-ACT
           ELSE
           IF SCR-CCPP = 'PGM'
                 PERFORM SCR-PGM
           ELSE
                 MOVE SCR-INST-HCM TO SCR-INST.
           GO TO LUP-MAIN.
       FIN-MAIN.
           EXIT.

       INI SECTION.
       INI-INI.
       COPY GNSBGEIB.
       COPY GNSBGEDB.
           MOVE LOW-VALUES TO CCM-FLD.
           MOVE LOW-VALUES TO CCM-EDT.
           PERFORM GNS-PRO-RCK.
           PERFORM GNS-BUS-VCA.
           MOVE +3000 TO SCR-TCMA.
           PERFORM GNS-HDL-SYS.
           PERFORM GNS-HDL-FRM.
           MOVE 'MC2PUCCM' TO FIO-PROG.
           PERFORM GNS-BUS-IDD.
      *PTC-INI
           PERFORM GNS-ERR-QUE.
           PERFORM GET-TSK-TERM.
      *PTC-FIN
      *<<<< INI_FIO
           IF SCR-OFRM-SRV
                MOVE TSK-TERM-ALF TO DCM-COD-TERM
                MOVE TSK-TERM-ALF TO DCM-COD-TERM-MAS
           ELSE
                MOVE EIBTRMID     TO DCM-COD-TERM
                MOVE EIBTRMID     TO DCM-COD-TERM-MAS.
      *>>>>
       FIN-INI.
           EXIT.

       COPY GNSBHSYS.
       COPY GNSBHFRM.
       COPY GNSBPRCK.
       COPY GNSBBVCA.
       COPY GNSBBIDD.

       SCR-COM SECTION.
       INI-SCR-COM.
           PERFORM STP-EXIT.
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-CLRA-CCM.
           PERFORM EDT-KEY-CCM.
           PERFORM EDT-FLD-CCM.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-HCM TO SCR-INST.
           MOVE SCR-CMNP     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       XCT-ERR.
           MOVE 'XCTERR    ' TO MSG-COD-MENS.
           MOVE 'GNS'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO SYS-MEN1.
           MOVE SYS-PROG     TO SYS-MEN2.
           MOVE SYS-ABOR     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       COPY GNSBGGBK.
       FIN-SCR-COM.
           EXIT.

       SCR-MOD SECTION.
       INI-SCR-MOD.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
               PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
               PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8
               PERFORM XCT-PRGP.
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
               GO TO MSG-SCR-MOD.
       MSG-SCR-MOD.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'MODNOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-MOD
               IF FIO-STAT-OKS
                   IF FRM-MEN1 NOT > SPACES
                       MOVE 'MODYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MEN1
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-MOD.
           EXIT.

       SCR-ING SECTION.
       INI-SCR-ING.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-IKY
               MOVE SCR-INST-GKY TO SCR-INST
      *PTC-INI
               IF NOT SCR-OFRM-SRV
                   PERFORM SCR-DMSG
               ELSE
                   PERFORM SCR-PUTA
      *PTC-FIN
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
       MSG-SCR-ING.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'INGNOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-ING
               IF FIO-STAT-OKS
                   IF FRM-MEN1 NOT > SPACES
                       MOVE 'INGYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MEN1
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ING.
           EXIT.

       SCR-ACC SECTION.
       INI-SCR-ACC.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8 OR
                         FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM XCT-PRGP.
           MOVE SPACES TO FRM-MENS.
           IF SCR-STAT NOT = SCR-STAT-CON
               PERFORM CMN-ACC.
           PERFORM SCR-MSSG.
       FIN-SCR-ACC.
           EXIT.

       SCR-ELI SECTION.
       INI-SCR-ELI.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8 OR
                         FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM XCT-PRGP.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'ELINOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-ELI
               IF FIO-STAT-OKS
                   IF FRM-MEN1 NOT > SPACES
                       MOVE 'ELIYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MEN1
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ELI.
           EXIT.

       SCR-ACT SECTION.
       INI-SCR-ACT.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF ( FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8  OR
                           FRM-FFLD-PF5 OR FRM-FFLD-PF6 ) AND
              SCR-ACTU = SCR-ACTU-MOD
               PERFORM XCT-PRGP.
           IF SCR-ACTU = SCR-ACTU-MOD
                  PERFORM MSG-SCR-MOD
           ELSE
                  PERFORM MSG-SCR-ING.
       FIN-SCR-ACT.
           EXIT.

       SCR-PGM SECTION.
       INI-SCR-PGM.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
       TRL-SCR-PGM.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8 OR
                         FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM XCT-PRGP.
           MOVE SPACES TO FRM-MENS.
           IF SCR-STAT NOT = SCR-STAT-CON
               PERFORM CMN-PGM.
           PERFORM SCR-MSSG.
       FIN-SCR-PGM.
           EXIT.

       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               PERFORM SCR-CLRA-CCM
               PERFORM EDT-KEY-CCM
               PERFORM EDT-FLD-CCM
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-CCM
               PERFORM EDT-KEY-CCM
           ELSE
               PERFORM CLR-FLD-CCM
               PERFORM EDT-FLD-CCM.
           PERFORM STP-EXIT.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

       XCT-PRGP SECTION.
       INI-XCT-PRGP.
           MOVE SCR-INST-BRW TO SCR-INST.
           PERFORM STP-EXIT.
           MOVE FRM-FFLD   TO SCR-FFLD.
      *
           MOVE SCR-PROG     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
           PERFORM XCT-ERR.
       FIN-XCT-PRGP.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO CCM-FLD.
           MOVE LOW-VALUES TO CCM-EDT.
       FIN-SCR-RST.
           EXIT.

       GET-FLD SECTION.
       INI-GET-FLD.
           PERFORM SCR-GFLD.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-FLD.
           EXIT.

       CMN-MOD SECTION.
       INI-CMN-MOD.
      *<<<<
           GO TO NXT-CMN-MOD.
      *>>>>
           PERFORM KEY-ALL-CCM.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-CCM.
           IF FIO-STAT-OKS AND
              CCM-STP-ITRN IN CCM NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM MC2-FIO-CCM
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-CCM
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-MOD.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-MOD.
           PERFORM RET-STT-CCM.
           PERFORM RET-ALL-CCM.
           PERFORM RET-HDR-CCM.
      *<<<< CMN_MOD
       NXT-CMN-MOD.
           PERFORM KEY-ALL-CCM.                                         
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.                            
           PERFORM MC2-FIO-CCM.                                         
           IF FIO-STAT-OKS AND                                          
              CCM-STP-ITRN IN CCM NOT = SCR-UMOD-CCM                    
               MOVE FIO-ULK-REC TO FIO-CMND                             
               PERFORM MC2-FIO-CCM                                      
               MOVE FIO-BCK-OUT TO FIO-CMND                             
               PERFORM MC2-FIO-CCM                                      
               MOVE 'GNS' TO MSG-COD-SIST                               
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS                      
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               GO TO FIN-CMN-MOD.                                       
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-CMN-MOD.                                       
           PERFORM RET-STT-CCM.                                         
           PERFORM RET-ALL-CCM.                                         
           PERFORM RET-HDR-CCM.                                         
           MOVE 'M' TO CCM-MSC-STAT IN CCM.
      *>>>>
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM MC2-FIO-CCM.
      *<<<< FIN_CMN_MOD
           IF NOT FIO-STAT-OKS
              GO TO FIN-CMN-MOD.
           MOVE CCM-CIC-ICCM IN CCM     TO DCM-CIC-IDCM IN DCM.
           MOVE DCM-NUM-IDCM IN CCM-FLD TO DCM-NUM-IDCM IN DCM.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-DCM.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-CCM
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-CCM
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE 'TAB' TO MSG-COD-SIST
              MOVE 'ERRUPD   DCM' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE '23' TO FIO-STAT PGM-STAT-DCM PGM-STAT-DCM
              GO TO FIN-CMN-MOD.

           PERFORM RET-STT-DCM.
           PERFORM RET-DCM-CCM.
           PERFORM RET-HDR-DCM.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM MC2-FIO-DCM.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-CCM
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-CCM
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE '23' TO FIO-STAT PGM-STAT-DCM PGM-STAT-DCM
              GO TO FIN-CMN-MOD.

           MOVE 'TAB'               TO FIO-SIST.
           MOVE 'DPL'               TO TAB-COD-TTAB IN TAB.
           MOVE DCM-CIC-IDCM IN DCM TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-MOD.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
              MOVE '23' TO FIO-STAT PGM-STAT-DCM PGM-STAT-DCM
              GO TO FIN-CMN-MOD.

           MOVE FRM-GLS-DES1 IN CCM-FLD TO TAB-GLS-DESC IN TAB.
           MOVE FRM-GLS-DES2 IN CCM-FLD TO TAB-GLS-DATA IN TAB.
           PERFORM GET-FHOY.
           MOVE HOY-FHOY                 TO TAB-FEC-FTRN IN TAB.
           MOVE HOY-HHOY                 TO TAB-HRA-HRTR IN TAB.
           MOVE SCR-OTRN                 TO TAB-COD-OTRN IN TAB.
           MOVE SCR-USER                 TO TAB-COD-ATRN IN TAB.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
              MOVE '23' TO FIO-STAT PGM-STAT-DCM PGM-STAT-DCM
              GO TO FIN-CMN-MOD.
      *>>>>
       FIN-CMN-MOD.
           EXIT.

       CMN-ING SECTION.
       INI-CMN-ING.
           MOVE SPACES TO CCM.
      *<<<< INI_CMN_ING
           IF DCM-NUM-IDCM IN CCM-FLD > 1
              MOVE '00' TO FIO-STAT
              GO TO CNT-CMN-ING.
           PERFORM OBT-CIC-CCM.
           IF CIC-STAT NOT = CIC-STAT-OKS
              GO TO FIN-CMN-ING.
           MOVE CIC-CAI-ICIC IN CIC TO CCM-CAI-ICCM IN CCM.
           MOVE CIC-IIC-ICIC IN CIC TO CCM-IIC-ICCM IN CCM.
           MOVE CIC-CIC-ICIC IN CIC TO CCM-CIC-ICCM IN CCM-FLD.
      *>>>>
           PERFORM RET-STT-CCM.
           PERFORM RET-ALL-CCM.
           PERFORM RET-HDR-CCM.
           MOVE FIO-PUT TO FIO-CMND.
           PERFORM MC2-FIO-CCM.
      *<<<< FIN_CMN_ING
       CNT-CMN-ING.
           IF NOT FIO-STAT-OKS
              GO TO FIN-CMN-ING.
           PERFORM RET-STT-DCM.
           PERFORM KEY-DCM-CCM.
           MOVE CCM-CIC-ICCM IN CCM-FLD TO DCM-CIC-IDCM IN DCM.
           PERFORM RET-DCM-CCM.
           PERFORM RET-HDR-DCM.
           MOVE FIO-PUT TO FIO-CMND.
           PERFORM MC2-FIO-DCM.
           IF NOT FIO-STAT-OKS
              MOVE FIO-STAT TO PGM-STAT-CCM PGM-STAT-DCM
              GO TO FIN-CMN-ING.

           IF DCM-NUM-IDCM IN CCM-FLD = 1
              MOVE 'TAB'                   TO FIO-SIST
              MOVE 'DPL'                   TO TAB-COD-TTAB IN TAB
              MOVE DCM-CIC-IDCM IN DCM     TO TAB-COD-CTAB IN TAB
              MOVE FRM-GLS-DES1 IN CCM-FLD TO TAB-GLS-DESC IN TAB
              MOVE FRM-GLS-DES2 IN CCM-FLD TO TAB-GLS-DATA IN TAB
              MOVE 'I'                     TO TAB-MSC-TACC IN TAB
              MOVE 'I'                     TO TAB-MSC-STAT IN TAB
              MOVE 'S'                     TO TAB-IND-VIGE IN TAB
              MOVE SPACES                  TO TAB-COD-OTRN IN TAB
              MOVE 'MC2PUCCM'              TO TAB-COD-ATRN IN TAB
              MOVE DCM-CAI-IDCM IN DCM     TO TAB-GLS-DCOR IN TAB
              MOVE DCM-IIC-IDCM IN DCM     TO TAB-GLS-ABRV IN TAB
              PERFORM GET-FHOY
              MOVE HOY-FHOY                 TO TAB-FEC-FTRN IN TAB
              MOVE HOY-HHOY                 TO TAB-HRA-HRTR IN TAB
              MOVE SCR-OTRN                 TO TAB-COD-OTRN IN TAB
              MOVE SCR-USER                 TO TAB-COD-ATRN IN TAB
              MOVE FIO-PUT TO FIO-CMND
              PERFORM GNS-FIO-TAB
              IF NOT FIO-STAT-OKS
                 MOVE 'TAB'               TO MSG-COD-SIST
                 MOVE 'DPL    NOGRB'      TO MSG-COD-MENS
                 PERFORM GET-MSG
                 MOVE MSG-GLS-DESC        TO FRM-MENS
                 GO TO FIN-CMN-ING.

      *MODIFICA ESTADO CCM
           PERFORM KEY-ALL-CCM.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-CCM.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-CCM
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-CCM
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE 'TAB' TO MSG-COD-SIST
              MOVE 'ERRUPDSTACCM' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              GO TO FIN-CMN-ING.
           PERFORM RET-STT-CCM.
           PERFORM RET-HDR-CCM.
           PERFORM RET-ALL-CCM.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM MC2-FIO-CCM.
      *>>>>
       FIN-CMN-ING.
           EXIT.

       CMN-ACC SECTION.
       INI-CMN-ACC.
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'ACCION NO REALIZADA' TO FRM-MENS
               GO TO FIN-CMN-ACC.
      *<<<< INI_CMN_ACC
           IF SCR-CMND = 'VRF'
               PERFORM PRO-CMN-VRF
               IF MSG-COD-MENS > SPACES
                  GO TO FIN-CMN-ACC.
           GO TO NXT-CMN-ACC.
      *>>>>
           PERFORM KEY-ALL-CCM.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-CCM.
           IF FIO-STAT-OKS AND
              CCM-STP-ITRN IN CCM NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM MC2-FIO-CCM
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-CCM
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-ACC.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ACC.
           PERFORM RET-ALL-CCM.
      *<<<< CMN_ACC
       NXT-CMN-ACC.
           PERFORM KEY-ALL-CCM.                                         
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.                            
           PERFORM MC2-FIO-CCM.                                         
           IF FIO-STAT-OKS AND                                          
              CCM-STP-ITRN IN CCM NOT = SCR-UMOD-CCM                    
               MOVE FIO-ULK-REC TO FIO-CMND                             
               PERFORM MC2-FIO-CCM                                      
               MOVE FIO-BCK-OUT TO FIO-CMND                             
               PERFORM MC2-FIO-CCM                                      
               MOVE 'GNS' TO MSG-COD-SIST                               
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS                      
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               GO TO FIN-CMN-ACC.                                       
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-CMN-ACC.                                       
      *    PERFORM RET-ALL-CCM.                                         
           IF SCR-CMND = 'VRF'
               MOVE 'V'          TO CCM-IND-VIGE IN CCM
               MOVE 'VRFYES    ' TO WSS-COD-MENY
               MOVE 'VRFNOT    ' TO WSS-COD-MENN
           ELSE
           IF SCR-CMND = 'ACA'
               MOVE 'A'          TO CCM-IND-VIGE IN CCM
               MOVE 'ACAYES    ' TO WSS-COD-MENY
               MOVE 'ACANOT    ' TO WSS-COD-MENN
           ELSE
           IF SCR-CMND = 'ANL'
               MOVE 'D'          TO CCM-IND-VIGE IN CCM
               MOVE 'ANLYES    ' TO WSS-COD-MENY
               MOVE 'ANLNOT    ' TO WSS-COD-MENN.

           MOVE FIO-MOD TO FIO-CMND.
           PERFORM MC2-FIO-CCM.
           IF NOT FIO-STAT-OKS
               MOVE 'S' TO SCR-IERR
               MOVE 'TAB' TO MSG-COD-SIST
               MOVE WSS-COD-MENN TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE WSS-COD-MENY  TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN1.

           IF SCR-CMND NOT = 'ANL'                                      
              GO TO FIN-CMN-ACC.                                        
           MOVE 'TAB' TO FIO-SIST.                                      
           MOVE 'DPL' TO TAB-COD-TTAB IN TAB.                           
           MOVE CCM-CIC-ICCM IN CCM TO TAB-COD-CTAB IN TAB.             
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM GNS-FIO-TAB.                                         
           IF FIO-STAT-OKS                                              
              MOVE FIO-GET-KEY-UPD TO FIO-CMND                          
              PERFORM GNS-FIO-TAB                                       
              IF FIO-STAT-OKS                                           
                 PERFORM GET-FHOY                                       
                 MOVE HOY-FHOY            TO TAB-FEC-FTRN IN TAB        
                 MOVE HOY-HHOY            TO TAB-HRA-HRTR IN TAB        
                 MOVE SCR-OTRN            TO TAB-COD-OTRN IN TAB        
                 MOVE SCR-USER            TO TAB-COD-ATRN IN TAB        
                 MOVE 'N'      TO TAB-IND-VIGE IN TAB                   
                 MOVE FIO-MOD TO FIO-CMND                               
                 PERFORM GNS-FIO-TAB                                    
                 IF NOT FIO-STAT-OKS                                    
                    MOVE 'ERROR UPD DPL' TO FRM-MEN2                    
                 ELSE                                                   
                    NEXT SENTENCE                                       
              ELSE                                                      
                 MOVE 'ERROR UPD DPL' TO FRM-MEN2                       
           ELSE                                                         
              MOVE 'ERROR UPD DPL' TO FRM-MEN2.                         
      *>>>>
       FIN-CMN-ACC.
           EXIT.

       CMN-ELI SECTION.
       INI-CMN-ELI.
      *<<<< INI_CMN_ELI
           PERFORM KEY-ALL-CCM.
           GO TO CNT-CMN-ELI.
      *>>>>
           PERFORM KEY-ALL-CCM.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-CCM.
           IF FIO-STAT-OKS AND
              CCM-STP-ITRN IN CCM NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM MC2-FIO-CCM
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-CCM
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-ELI.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ELI.
           MOVE FIO-DEL TO FIO-CMND.
           PERFORM MC2-FIO-CCM.
      *<<<< FIN_CMN_ELI
           MOVE FIO-STAT TO PGM-STAT-CCM.
       CNT-CMN-ELI.
           IF FRM-IND-SGTE IN CCM-FLD = 'S'
              PERFORM ELI-ALL-CCM
              GO TO FIN-CMN-ELI.
           MOVE CCM-CIC-ICCM IN CCM-FLD TO DCM-CIC-IDCM IN DCM.
           MOVE DCM-NUM-IDCM IN CCM-FLD TO DCM-NUM-IDCM IN DCM.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-DCM.
           IF FIO-STAT-OKS AND
              DCM-STP-ITRN IN DCM NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM MC2-FIO-CCM
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-CCM
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE '23' TO FIO-STAT PGM-STAT-DCM PGM-STAT-DCM
               GO TO FIN-CMN-ELI.
           IF NOT FIO-STAT-OKS
              GO TO FIN-CMN-ELI.

           MOVE FIO-DEL TO FIO-CMND.
           PERFORM MC2-FIO-DCM.
           MOVE FIO-STAT TO PGM-STAT-DCM.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE '23' TO FIO-STAT PGM-STAT-DCM PGM-STAT-DCM
              GO TO FIN-CMN-ELI.
           MOVE CCM-CIC-ICCM IN CCM-FLD TO DCM-CIC-IDCM IN DCM.
           MOVE ZEROES                  TO DCM-NUM-IDCM IN DCM.
           MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM MC2-FIO-DCM.
           IF (FIO-STAT-OKS AND
               DCM-CIC-IDCM IN DCM = CCM-CIC-ICCM IN CCM-FLD)
              MOVE PGM-STAT-DCM TO FIO-STAT
              PERFORM MOD-STAT-CCM
              GO TO FIN-CMN-ELI.

           PERFORM KEY-ALL-CCM.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-CCM.
           IF FIO-STAT-OKS AND
              CCM-STP-ITRN IN CCM NOT = SCR-UMOD-CCM
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM MC2-FIO-CCM
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-CCM
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM MC2-FIO-DCM
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-DCM
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-ELI.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ELI.
           MOVE FIO-DEL TO FIO-CMND.
           PERFORM MC2-FIO-CCM.
           MOVE FIO-STAT TO PGM-STAT-CCM.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-CCM
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-CCM
              MOVE '23' TO FIO-STAT PGM-STAT-DCM PGM-STAT-DCM
              GO TO FIN-CMN-ELI.
           MOVE 'TAB'               TO FIO-SIST.
           MOVE 'DPL'               TO TAB-COD-TTAB IN TAB.
           MOVE DCM-CIC-IDCM IN DCM TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
              MOVE PGM-STAT-DCM TO FIO-STAT
              GO TO FIN-CMN-ELI.
           MOVE FIO-DEL TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
              MOVE PGM-STAT-DCM TO FIO-STAT
              GO TO FIN-CMN-ELI.
      *>>>>
       FIN-CMN-ELI.
           EXIT.

       CMN-PGM SECTION.
       INI-CMN-PGM.
           PERFORM RET-ALL-CCM.
       FIN-CMN-PGM.
           EXIT.

       STP-EXIT SECTION.
       INI-STP-EXIT.
           MOVE SCR-PROG TO PGM-PROG.
           MOVE SCR-NTRN TO PGM-NTRN.

           MOVE SCR-PROV TO SCR-PROG.
           MOVE SCR-NTRV TO SCR-NTRN.

           MOVE PGM-PROG TO SCR-PROV
           MOVE PGM-NTRN TO SCR-NTRV.
       FIN-STP-EXIT.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO SYS-NTRN.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
      *PTC-INI
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE CCM-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO CCM-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GFLD SECTION.
       INI-SCR-GFLD.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-FLD-CCM
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO CCM-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GFLD.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-CCM.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-CCM
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           PERFORM VAL-NUM-FLD-CCM.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-CCM
               PERFORM EDT-FLD-CCM
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           PERFORM VAL-MKY-CCM.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               GO TO ERR-SCR-GFLD.
           PERFORM VAL-FLD-CCM.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GFLD.
           PERFORM VAL-CON-FLD-CCM.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GFLD.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-FLD-CCM.
       ERR-SCR-GFLD.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-CCM.
           PERFORM EDT-FLD-CCM.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GFLD.
           MOVE SCR-STAT-MOD TO SCR-STAT.

       GET-ACC SECTION.
       INI-GET-ACC.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO CCM-GLS-MEN2
               MOVE SCR-PRMT     TO CCM-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO CCM-FLD.
           IF NOT ( ( FRM-FFLD = FRM-FFLD-PF12 OR FRM-FFLD-PF3 ) OR
                      SCR-CONF > SPACES )
               PERFORM VAL-NUM-KEY-CCM
               PERFORM VAL-NUM-FLD-CCM.
       FIN-GET-ACC.
           MOVE SCR-STAT-SON TO SCR-STAT.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF7 )
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 )
      *>>>>
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO CCM-GLS-MEN2
               MOVE SCR-PRMT     TO CCM-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO CCM-FLD.
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO CCM-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE CCM-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO CCM-FLD
      * FLB  7-NOV-1994
               PERFORM CLR-KEY-CCM

               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO CCM-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO CCM-GLS-MENS.
           PERFORM PRO-FLD-CCM.
           PERFORM PRO-MKY-CCM.
           PERFORM UNP-IKY-CCM.
           PERFORM FST-KEY-CCM.
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE CCM-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO CCM-FLD
               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO CCM-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO CCM-GLS-MENS.
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-FLD-CCM.
           MOVE '1' TO FRM-CPIM-AEY.
           PERFORM UNP-KEY-CCM.
           PERFORM FST-KEY-CCM.
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
           PERFORM UNP-MKY-CCM.
           PERFORM UNP-FLD-CCM.
           PERFORM FST-FLD-CCM.
      *<<<< FIN_HBL_FLD
      *>>>>
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO CCM-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-CCM.
           PERFORM PRO-FLD-CCM.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
      * PTC-INI
           IF SCR-STAT NOT = SCR-STPP
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
      * PTC-FIN
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-CCM
               PERFORM EDT-FLD-CCM
               PERFORM SCR-PUTA
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-CCM
               PERFORM EDT-FLD-CCM
               PERFORM SCR-PUTA
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-CCM.
           PERFORM EDT-FLD-CCM.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           IF SCR-INST = SCR-INST-GCF
                  PERFORM SCR-GCNF
                  GO TO FIN-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
           IF SCR-STAT = SCR-STAT-ABT OR SCR-STAT-CON
                  GO TO FIN-DESSCRTRL.
           IF SCR-FFLD-RET OR SCR-FFLD-PF10
                  MOVE SCR-VFLD-YES TO SCR-VFLD
                  PERFORM PRO-CONF.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

       PRO-CONF SECTION.
       INI-PRO-CONF.
      *PTC-INI
           IF SCR-OFRM-SRV AND
              SCR-CONF NOT > SPACES
                MOVE SPACES TO FRM-MENS.
      *PTC-FIN
           IF FRM-MENS NOT > SPACES
               IF SCR-CONF NOT > SPACES
                   GO TO FIN-PRO-CONF
               ELSE
                   MOVE SCR-CONF TO FRM-MENS
           ELSE
               IF FRM-MEN1 NOT > SPACES
                   MOVE SCR-CONF TO FRM-MEN1.
           PERFORM PRO-KEY-CCM.
           PERFORM PRO-FLD-CCM.
           PERFORM FST-MSG.
           PERFORM SCR-MSSG.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM EDT-KEY-CCM
               PERFORM EDT-FLD-CCM.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GCF TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-PRO-CONF.
           EXIT.

       SCR-GCNF SECTION.
       INI-SCR-GCNF.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3  OR
                               FRM-FFLD-PF4 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO CCM-GLS-MEN2
               MOVE SCR-CONF     TO CCM-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO CCM-FLD.
           MOVE FRM-FFLD TO SCR-FFLD.
           IF FRM-FFLD = FRM-FFLD-PF3
               MOVE SCR-VFLD-NOT TO SCR-VFLD
           ELSE
           IF FRM-FFLD = FRM-FFLD-RET OR FRM-FFLD-PF10 OR FRM-FFLD-PF4
               MOVE SCR-VFLD-YES TO SCR-VFLD
               PERFORM VAL-NUM-KEY-CCM
               PERFORM VAL-NUM-FLD-CCM
           ELSE
           IF FRM-FFLD = FRM-FFLD-PF12
               MOVE SCR-STPP TO SCR-STAT
               IF SCR-STAT = SCR-STAT-MOD
                   PERFORM VAL-NUM-KEY-CCM
                   PERFORM VAL-NUM-FLD-CCM
                   PERFORM SCR-TRL
               ELSE
                   MOVE SCR-VFLD-NOT TO SCR-VFLD.
       FIN-SCR-GCNF.
           EXIT.

      * PGM-EBR-ALL SECTION.
      * INI-PGM-EBR-ALL.
      *     MOVE 'GNS' TO FIO-SIST.
      *     MOVE FIO-EBR-ALL TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
      * FIN-PGM-EBR-ALL.
      *     EXIT.

       RET-STT-CCM SECTION.
       INI-RET-STT-CCM.
           IF SCR-CCPP = 'ING'
               MOVE 'I' TO CCM-MSC-TACC IN CCM
               MOVE 'I' TO CCM-MSC-STAT IN CCM
           ELSE
           IF SCR-CCPP = 'MOD'
               MOVE 'M' TO CCM-MSC-TACC IN CCM
               MOVE 'M' TO CCM-MSC-STAT IN CCM.
       FIN-RET-STT-CCM.
           EXIT.

       RET-HDR-CCM SECTION.
       INI-RET-HDR-CCM.
           PERFORM GET-FHOY.
      *    Fecha Transaccion
           MOVE HOY-FHOY TO CCM-FEC-FTRN IN CCM.
      *    Hora Transaccion
           MOVE HOY-HHOY TO CCM-HRA-HRTR IN CCM.
      *    Codigo Origen transaccion
      *<<<< NOT PGM_VMS
           MOVE SCR-OTRN TO CCM-COD-OTRN IN CCM.
      *    Autor Transaccion
           MOVE SCR-USER TO CCM-COD-ATRN IN CCM.
      *>>>>
       FIN-RET-HDR-CCM.
           EXIT.

       COPY MC2BUCCM.
       COPY GNSBVNUM.
       COPY MC2BFCCM.
       COPY GNSBFMSG.
       COPY GNSBGSYS.
       COPY GNSBGDTC.
      *PTC-INI
      *PTC-SUB
      * ISP  10-MAY-2001 13:46:35 
      * COPY GNSBGFRM.
       COPY GNSBG3CF.
      *PTC-FIN
       COPY GNSBGMSG.
       COPY GNSBEKEY.
       COPY GNSBIABT.
       COPY GNSBFTAB.
       COPY GNSBEGLS.
       COPY GNSBGFEC.
       COPY GNSBGHOY.
      *PTC-INI
       COPY GNSBGQUE.
       COPY GNSBGTSK.
      *PTC-FIN

      *<<<<
       PGM-EBR-ALL SECTION.                                             
       INI-PGM-EBR-ALL.                                                 
           MOVE 'GNS' TO FIO-SIST.                                      
           MOVE FIO-EBR-ALL TO FIO-CMND.                                
           PERFORM GNS-FIO-TAB.                                         
       FIN-PGM-EBR-ALL.                                                 
           EXIT.                                                        
       COPY MC2BFDCM.
       COPY GNSBGVSM.
       COPY GNSBVTAB.
       COPY GNSBVCOD.
       COPY GNSBBTAB.
       COPY GNSBVMSC.
       COPY GNSBBMSC.
       COPY GNSBFMSC.
      *<<<< EOF_PU
       COPY GNSBGCIC.
       COPY GNSBFCIC.
       COPY GNSBPVRF.
       COPY GNSBVIND.
       COPY TABBFCAM.

       OBT-CIC-CCM SECTION.
       INI-OBT-CIC-CCM.
           MOVE 'PCCM'  TO CIC-CAI-ICIC IN CIC.
           MOVE CIC-GET TO CIC-CMND.
           PERFORM PRO-CIC.
           IF CIC-STAT NOT = CIC-STAT-OKS
               MOVE CIC-MENS     TO FRM-MENS
               MOVE FIO-STAT-NEX TO FIO-STAT.
       FIN-OBT-CIC-CCM.
           EXIT.

       RET-STT-DCM SECTION.
       INI-RET-STT-DCM.
           IF SCR-CCPP = 'ING'
               MOVE 'I' TO DCM-MSC-TACC IN DCM
               MOVE 'I' TO DCM-MSC-STAT IN DCM
           ELSE
           IF SCR-CCPP = 'MOD'
               MOVE 'M' TO DCM-MSC-TACC IN DCM
               MOVE 'M' TO DCM-MSC-STAT IN DCM.
       FIN-RET-STT-DCM.
           EXIT.

       RET-HDR-DCM SECTION.
       INI-RET-HDR-DCM.
           PERFORM GET-FHOY.
      *    Fecha Transaccion
           MOVE HOY-FHOY TO DCM-FEC-FTRN IN DCM.
      *    Hora Transaccion
           MOVE HOY-HHOY TO DCM-HRA-HRTR IN DCM.
      *    Codigo Origen transaccion
      *<<<< NOT PGM_VMS
           MOVE SCR-OTRN TO DCM-COD-OTRN IN DCM.
      *    Autor Transaccion
           MOVE SCR-USER TO DCM-COD-ATRN IN DCM.
      *>>>>
       FIN-RET-HDR-DCM.
           EXIT.

       MOD-STAT-CCM SECTION.
       INI-MOD-STAT-CCM.
      *MODIFICA ESTADO CCM
           PERFORM KEY-ALL-CCM.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-CCM.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-CCM
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-CCM
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE 'TAB'          TO MSG-COD-SIST
              MOVE 'ERRUPDSTACCM' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              GO TO FIN-MOD-STAT-CCM.
           MOVE 'M' TO CCM-IND-VIGE IN CCM.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM MC2-FIO-CCM.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-CCM
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-CCM
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE 'TAB'          TO MSG-COD-SIST
              MOVE 'ERRUPDSTACCM' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              GO TO FIN-MOD-STAT-CCM.
      *PVG INI 13-DIC-2007 ACTUALIZA CORRELATIVO POST ELI UNITARIA      
           PERFORM PRO-ACT-NCOR.                                        
      *PVG FIN 13-DIC-2007                                              
       FIN-MOD-STAT-CCM.
           EXIT.

       PRO-CMN-VRF SECTION.
       INI-PRO-CMN-VRF.
           PERFORM OBT-COD-PESO.
           IF MSG-COD-MENS > SPACES
              GO TO MSG-PRO-CMN-VRF.
           MOVE SPACES TO MSG-COD-MENS.
           MOVE ZEROES TO WSS-K.
           MOVE CCM-CIC-ICCM IN CCM-FLD TO DCM-CIC-IDCM IN DCM.
           MOVE ZEROES                  TO DCM-NUM-IDCM IN DCM.
           MOVE FIO-GET-NLS TO FIO-CMND.
       LUP-PRO-CMN-VRF.
           PERFORM MC2-FIO-DCM.
           IF NOT (FIO-STAT-OKS AND
                   CCM-CIC-ICCM IN CCM-FLD = DCM-CIC-IDCM IN DCM)
              GO TO CNT-PRO-CMN-VRF.
           ADD 1 TO WSS-K.
           MOVE DCM-VAL-MTOP IN DCM TO VEC-DCM-VAL-MTOPP(WSS-K).
           IF  DCM-VAL-MTOP IN DCM > ZEROES AND
              (DCM-COD-VCMT IN DCM NOT = WSS-COD-PESO IN WSS-VARI)
              PERFORM BUS-EQV-MNAC
              IF MSG-COD-MENS > SPACES
                 MOVE DCM-COD-VCMT IN DCM TO FRM-MEN2
                 GO TO MSG-PRO-CMN-VRF.
           PERFORM CRG-DCM-VEC.
      *MONTOS INDEFINIDOS
           IF DCM-VAL-MTOP IN DCM NOT > ZEROES
              MOVE 99999999999        TO VEC-DCM-VAL-MTOPP(WSS-K).

           IF DCM-IND-UTPT IN DCM = 'D'
              MOVE DCM-NUM-PTOP IN DCM TO VEC-DCM-NUM-PTOPD(WSS-K)
           ELSE
           IF DCM-IND-UTPT IN DCM = 'M'
              MULTIPLY DCM-NUM-PTOP IN DCM BY 30
                                   GIVING VEC-DCM-NUM-PTOPD(WSS-K)
           ELSE
           IF DCM-IND-UTPT IN DCM = 'A'
              MULTIPLY DCM-NUM-PTOP IN DCM BY 365
                                   GIVING VEC-DCM-NUM-PTOPD(WSS-K).
           IF DCM-NUM-PTOP IN DCM NOT > ZEROES
              MOVE 99999               TO VEC-DCM-NUM-PTOPD(WSS-K).

           IF DCM-NUM-ETOP IN DCM NOT > ZEROES
              MOVE 999                 TO VEC-DCM-NUM-ETOPK(WSS-K)
              MOVE 'Z'                 TO VEC-DCM-IND-UTETK(WSS-K)
           ELSE                                                         
              MOVE DCM-NUM-ETOP IN DCM TO VEC-DCM-NUM-ETOPK(WSS-K)      
              MOVE DCM-IND-UTET IN DCM TO VEC-DCM-IND-UTETK(WSS-K).     

           IF DCM-NUM-NEVT IN DCM NOT > ZEROES
              MOVE 99999               TO VEC-DCM-NUM-NEVTK(WSS-K)
           ELSE                                                         
              MOVE DCM-NUM-NEVT IN DCM TO VEC-DCM-NUM-NEVTK(WSS-K).     

           MOVE DCM-COD-SCTB IN DCM    TO VEC-DCM-COD-SCTBK(WSS-K).
           MOVE DCM-NUM-DIMO IN DCM    TO VEC-DCM-NUM-DIMOK(WSS-K).

           MOVE FIO-GET-NXT TO FIO-CMND.
           GO TO LUP-PRO-CMN-VRF.
       CNT-PRO-CMN-VRF.
           PERFORM PRO-VEC-SORT.

           PERFORM ELI-ALL-DCM.
           IF MSG-COD-MENS > SPACES
              GO TO MSG-PRO-CMN-VRF.

           PERFORM PUT-NEW-DCM.
           IF MSG-COD-MENS > SPACES
               GO TO MSG-PRO-CMN-VRF.
           GO TO FIN-PRO-CMN-VRF.
       MSG-PRO-CMN-VRF.
           IF MSG-COD-SIST NOT > SPACES
              MOVE 'TAB' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MEN1.
           MOVE 'S' TO SCR-IERR.
           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-PRO-CMN-VRF.
       FIN-PRO-CMN-VRF.
           EXIT.

       ELI-ALL-DCM SECTION.
       INI-ELI-ALL-DCM.
           MOVE CCM-CIC-ICCM IN CCM-FLD TO DCM-CIC-IDCM IN DCM.
           MOVE ZEROES                  TO DCM-NUM-IDCM IN DCM.
           MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM MC2-FIO-DCM.
           IF NOT (FIO-STAT-OKS AND
                   CCM-CIC-ICCM IN CCM-FLD = DCM-CIC-IDCM IN DCM)
              GO TO FIN-ELI-ALL-DCM.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-DCM.
           IF NOT FIO-STAT-OKS
              MOVE 'DCMVRF NOUPD' TO MSG-COD-MENS
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DCM
              GO TO FIN-ELI-ALL-DCM.
           MOVE FIO-DEL TO FIO-CMND.
           PERFORM MC2-FIO-DCM.
           IF NOT FIO-STAT-OKS
              MOVE 'DCMVRF NODEL' TO MSG-COD-MENS
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DCM
              GO TO FIN-ELI-ALL-DCM.
           GO TO INI-ELI-ALL-DCM.
       FIN-ELI-ALL-DCM.
           EXIT.

       PUT-NEW-DCM SECTION.
       INI-PUT-NEW-DCM.
           MOVE ZEROES TO WSS-R.
           MOVE ZEROES TO WSS-RDCM.                                     
           MOVE WSS-K  TO WSS-L.
       LUP-PUT-NEW-DCM.
           IF WSS-L NOT > ZEROES
               GO TO FIN-PUT-NEW-DCM.
           ADD 1 TO WSS-R.
           PERFORM CRG-VEC-DCM.
           IF VEC-DCM-GLS-FLAG(WSS-R) = 'E'                             
              GO TO CNT-PUT-NEW-DCM.                                    
           MOVE FIO-PUT TO FIO-CMND.
           PERFORM MC2-FIO-DCM.
           IF NOT FIO-STAT-OKS
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-DCM
               MOVE 'DCMVRF NOPUT' TO MSG-COD-MENS
               GO TO FIN-PUT-NEW-DCM.
       CNT-PUT-NEW-DCM.                                                 
           SUBTRACT 1 FROM WSS-L.
           GO TO LUP-PUT-NEW-DCM.
       FIN-PUT-NEW-DCM.
           EXIT.

       OBT-COD-PESO SECTION.
       INI-OBT-COD-PESO.
      * OBTIENE CODIGO VALOR DE CAMBIO DE PESOS MEDIANTE CIC DE LA MONED
           MOVE 'TAB'          TO FIO-SIST.
           MOVE 'VLR'          TO TAB-CIC-TTAB IN TAB.
           MOVE '0999  '       TO TAB-CIC-CTAB IN TAB.
           MOVE 'TAB-CIC-TABL' TO FIO-AKEY.
           MOVE FIO-GET-KEY    TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
               MOVE 'TAB'          TO MSG-COD-SIST
               MOVE 'CIC0999  NEX' TO MSG-COD-MENS.
           MOVE TAB-COD-CTAB IN TAB TO WSS-COD-PESO IN WSS-VARI.
       FIN-OBT-COD-PESO.
           EXIT.

       CRG-DCM-VEC SECTION.
       INI-CRG-DCM-VEC.
           MOVE DCM-COD-NLIC IN DCM TO VEC-DCM-COD-NLIC(WSS-K).
           MOVE DCM-COD-TCCM IN DCM TO VEC-DCM-COD-TCCM(WSS-K).
           MOVE DCM-COD-CPMT IN DCM TO VEC-DCM-COD-CPMT(WSS-K).
           MOVE DCM-NUM-ETOP IN DCM TO VEC-DCM-NUM-ETOP(WSS-K).
           MOVE DCM-IND-UTET IN DCM TO VEC-DCM-IND-UTET(WSS-K).
           MOVE DCM-NUM-NEVT IN DCM TO VEC-DCM-NUM-NEVT(WSS-K).

           MOVE DCM-GLS-FLAG IN DCM TO VEC-DCM-GLS-FLAG(WSS-K).
           MOVE DCM-CIC-IDCM IN DCM TO VEC-DCM-CIC-IDCM(WSS-K).
           MOVE DCM-NUM-IDCM IN DCM TO VEC-DCM-NUM-IDCM(WSS-K).
           MOVE DCM-STP-ITRN IN DCM TO VEC-DCM-STP-ITRN(WSS-K).
           MOVE DCM-MSC-TACC IN DCM TO VEC-DCM-MSC-TACC(WSS-K).
           MOVE DCM-MSC-STAT IN DCM TO VEC-DCM-MSC-STAT(WSS-K).
           MOVE DCM-COD-OTRN IN DCM TO VEC-DCM-COD-OTRN(WSS-K).
           MOVE DCM-COD-ATRN IN DCM TO VEC-DCM-COD-ATRN(WSS-K).
           MOVE DCM-COD-ECCM IN DCM TO VEC-DCM-COD-ECCM(WSS-K).
           MOVE DCM-COD-OCOB IN DCM TO VEC-DCM-COD-OCOB(WSS-K).
           MOVE DCM-IND-COBL IN DCM TO VEC-DCM-IND-COBL(WSS-K).
           MOVE DCM-NUM-UCOB IN DCM TO VEC-DCM-NUM-UCOB(WSS-K).
           MOVE DCM-VAL-MTOP IN DCM TO VEC-DCM-VAL-MTOP(WSS-K).
           MOVE DCM-COD-VCMT IN DCM TO VEC-DCM-COD-VCMT(WSS-K).
           MOVE DCM-IND-ACAL IN DCM TO VEC-DCM-IND-ACAL(WSS-K).         
           MOVE DCM-IND-MIVA IN DCM TO VEC-DCM-IND-MIVA(WSS-K).
           MOVE DCM-NUM-PTOP IN DCM TO VEC-DCM-NUM-PTOP(WSS-K).
           MOVE DCM-IND-UTPT IN DCM TO VEC-DCM-IND-UTPT(WSS-K).
           MOVE DCM-SGV-TAMO IN DCM TO VEC-DCM-SGV-TAMO(WSS-K).
           MOVE DCM-COD-VCOC IN DCM TO VEC-DCM-COD-VCOC(WSS-K).
           MOVE DCM-COD-CPTO IN DCM TO VEC-DCM-COD-CPTO(WSS-K).
           MOVE DCM-VAL-VMIN IN DCM TO VEC-DCM-VAL-VMIN(WSS-K).
           MOVE DCM-VAL-VMAX IN DCM TO VEC-DCM-VAL-VMAX(WSS-K).
           MOVE DCM-COD-VMYM IN DCM TO VEC-DCM-COD-VMYM(WSS-K).
           MOVE DCM-NUM-CMAX IN DCM TO VEC-DCM-NUM-CMAX(WSS-K).
           MOVE DCM-COD-TINT IN DCM TO VEC-DCM-COD-TINT(WSS-K).
           MOVE DCM-COD-ADIC IN DCM TO VEC-DCM-COD-ADIC(WSS-K).
           MOVE DCM-NUM-VIGD IN DCM TO VEC-DCM-NUM-VIGD(WSS-K).         
           MOVE DCM-MSC-UTVD IN DCM TO VEC-DCM-MSC-UTVD(WSS-K).         
           MOVE DCM-GLS-DISP IN DCM TO VEC-DCM-GLS-DISP(WSS-K).
       FIN-CRG-DCM-VEC.
           EXIT.

       CRG-VEC-DCM SECTION.
       INI-CRG-VEC-DCM.
           IF VEC-DCM-GLS-FLAG(WSS-R) = 'E'                             
              GO TO FIN-CRG-VEC-DCM.                                    
           ADD 1 TO WSS-RDCM.                                           
           MOVE VEC-DCM-COD-NLIC(WSS-R) TO DCM-COD-NLIC IN DCM.
           MOVE VEC-DCM-COD-TCCM(WSS-R) TO DCM-COD-TCCM IN DCM.
           MOVE VEC-DCM-COD-ECCM(WSS-R) TO DCM-COD-ECCM IN DCM.
           MOVE VEC-DCM-COD-OCOB(WSS-R) TO DCM-COD-OCOB IN DCM.
           MOVE VEC-DCM-COD-CPMT(WSS-R) TO DCM-COD-CPMT IN DCM.
           MOVE VEC-DCM-NUM-ETOP(WSS-R) TO DCM-NUM-ETOP IN DCM.
           MOVE VEC-DCM-IND-UTET(WSS-R) TO DCM-IND-UTET IN DCM.
           MOVE VEC-DCM-NUM-NEVT(WSS-R) TO DCM-NUM-NEVT IN DCM.
           MOVE VEC-DCM-GLS-FLAG(WSS-R) TO DCM-GLS-FLAG IN DCM.
           MOVE VEC-DCM-CIC-IDCM(WSS-R) TO DCM-CIC-IDCM IN DCM.
           MOVE WSS-RDCM                TO DCM-NUM-IDCM IN DCM.
           MOVE VEC-DCM-FEC-FTRN(WSS-R) TO DCM-FEC-FTRN IN DCM.
           MOVE VEC-DCM-HRA-HRTR(WSS-R) TO DCM-HRA-HRTR IN DCM.
           MOVE VEC-DCM-MSC-TACC(WSS-R) TO DCM-MSC-TACC IN DCM.
           MOVE 'V'                     TO DCM-MSC-STAT IN DCM.
           MOVE VEC-DCM-COD-OTRN(WSS-R) TO DCM-COD-OTRN IN DCM.
           MOVE VEC-DCM-COD-ATRN(WSS-R) TO DCM-COD-ATRN IN DCM.
           MOVE VEC-DCM-IND-COBL(WSS-R) TO DCM-IND-COBL IN DCM.
           MOVE VEC-DCM-NUM-UCOB(WSS-R) TO DCM-NUM-UCOB IN DCM.
           MOVE VEC-DCM-VAL-MTOP(WSS-R) TO DCM-VAL-MTOP IN DCM.
           MOVE VEC-DCM-COD-VCMT(WSS-R) TO DCM-COD-VCMT IN DCM.
           MOVE VEC-DCM-IND-ACAL(WSS-R) TO DCM-IND-ACAL IN DCM.         
           MOVE VEC-DCM-IND-MIVA(WSS-R) TO DCM-IND-MIVA IN DCM.
           MOVE VEC-DCM-NUM-PTOP(WSS-R) TO DCM-NUM-PTOP IN DCM.
           MOVE VEC-DCM-IND-UTPT(WSS-R) TO DCM-IND-UTPT IN DCM.
           MOVE VEC-DCM-SGV-TAMO(WSS-R) TO DCM-SGV-TAMO IN DCM.
           MOVE VEC-DCM-COD-VCOC(WSS-R) TO DCM-COD-VCOC IN DCM.
           MOVE VEC-DCM-COD-CPTO(WSS-R) TO DCM-COD-CPTO IN DCM.
           MOVE VEC-DCM-VAL-VMIN(WSS-R) TO DCM-VAL-VMIN IN DCM.
           MOVE VEC-DCM-VAL-VMAX(WSS-R) TO DCM-VAL-VMAX IN DCM.
           MOVE VEC-DCM-COD-VMYM(WSS-R) TO DCM-COD-VMYM IN DCM.
           MOVE VEC-DCM-NUM-CMAX(WSS-R) TO DCM-NUM-CMAX IN DCM.
           MOVE VEC-DCM-COD-TINT(WSS-R) TO DCM-COD-TINT IN DCM.
           MOVE VEC-DCM-COD-ADIC(WSS-R) TO DCM-COD-ADIC IN DCM.
           MOVE VEC-DCM-NUM-VIGD(WSS-R) TO DCM-NUM-VIGD IN DCM.         
           MOVE VEC-DCM-MSC-UTVD(WSS-R) TO DCM-MSC-UTVD IN DCM.         
           MOVE VEC-DCM-GLS-DISP(WSS-R) TO DCM-GLS-DISP IN DCM.
       FIN-CRG-VEC-DCM.
           EXIT.

       PRO-VEC-SORT SECTION.
       INI-PRO-VEC-SORT.
           PERFORM ORD-UNO VARYING WSS-V
                           FROM WSS-K BY -1
                           UNTIL WSS-V < 1.
       FIN-PRO-VEC-SORT.
           EXIT.

       ORD-UNO SECTION.
       INI-ORD-UNO.
           COMPUTE WSS-V1 = WSS-V - 1.
           PERFORM ORD-UNO-A VARYING WSS-V2
                             FROM 1 BY 1
                             UNTIL WSS-V2 > WSS-V1.
       FIN-ORD-UNO.
           EXIT.

       ORD-UNO-A SECTION.
       INI-ORD-UNO-A.
           COMPUTE WSS-V3 = WSS-V2 + 1.
            IF VEC-DCM-KEY1(WSS-V2) = VEC-DCM-KEY1(WSS-V3)              
               MOVE 'E' TO VEC-DCM-GLS-FLAG(WSS-V2).                    
            IF VEC-DCM-KEY1(WSS-V2) > VEC-DCM-KEY1(WSS-V3)
                PERFORM SWP-VEC-DCM.
       FIN-ORD-UNO-A.
           EXIT.

       SWP-VEC-DCM SECTION.
       INI-SWP-VEC-DCM.
           MOVE VEC-DCM-KEY1(WSS-V2) TO TMP-DCM-KEY1.
           MOVE VEC-DCM-DATA(WSS-V2) TO TMP-DCM-DATA.

           MOVE VEC-DCM-KEY1(WSS-V3) TO VEC-DCM-KEY1(WSS-V2).
           MOVE VEC-DCM-DATA(WSS-V3) TO VEC-DCM-DATA(WSS-V2).

           MOVE TMP-DCM-KEY1         TO VEC-DCM-KEY1(WSS-V3).
           MOVE TMP-DCM-DATA         TO VEC-DCM-DATA(WSS-V3).
       FIN-SWP-VEC-DCM.
           EXIT.

       BUS-EQV-MNAC SECTION.
       INI-BUS-EQV-MNAC.
           MOVE ZEROES TO WSS-CCM-E IN CCM-VARI.
       LUP-BUS-EQV-MNAC.
           ADD 1 TO WSS-CCM-E IN CCM-VARI.
           IF WSS-CCM-E IN CCM-VARI > WSS-CCM-EMAX IN CCM-VARI OR
              WSS-CCM-COD-VCOC(WSS-CCM-E) NOT > SPACES
              GO TO LEE-BUS-EQV-MNAC.
           IF WSS-CCM-COD-VCOC(WSS-CCM-E) = DCM-COD-VCMT IN DCM
              MOVE WSS-CCM-SGV-VCAM(WSS-CCM-E) TO WSS-CCM-SGV-VEQV
              GO TO CNT-BUS-EQV-MNAC
           ELSE
              GO TO LUP-BUS-EQV-MNAC.
       LEE-BUS-EQV-MNAC.
           PERFORM GET-FHOY.
           MOVE DCM-COD-VCMT IN DCM      TO CAM-COD-VCAM IN CAM(1).
           MOVE WSS-COD-PESO IN WSS-VARI TO CAM-COD-VCAM IN CAM(2).
           MOVE HOY-FHOY    TO CAM-FEC-FCAM IN CAM(1).
           MOVE HOY-FHOY    TO CAM-FEC-FCAM IN CAM(2).
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM TAB-FIO-CAM.
           IF NOT (FIO-STAT-OKS AND CAM-MSC-STAT IN CAM = 'S')
              MOVE 'TAB'        TO MSG-COD-SIST
              MOVE 'CAM    NEX' TO MSG-COD-MENS
              GO TO FIN-BUS-EQV-MNAC.
           MOVE CAM-SGV-VCAM IN CAM TO WSS-CCM-SGV-VEQV IN CCM-VARI.
           IF WSS-CCM-E IN CCM-VARI NOT > WSS-CCM-EMAX IN CCM-VARI
              MOVE DCM-COD-VCMT IN DCM          TO
                                         WSS-CCM-COD-VCOC(WSS-CCM-E)
              MOVE WSS-CCM-SGV-VEQV IN CCM-VARI TO
                                         WSS-CCM-SGV-VCAM(WSS-CCM-E).
       CNT-BUS-EQV-MNAC.
           MULTIPLY DCM-VAL-MTOP IN DCM BY WSS-CCM-SGV-VEQV IN CCM-VARI
             GIVING VEC-DCM-VAL-MTOPP(WSS-K).
       FIN-BUS-EQV-MNAC.
           EXIT.

       ELI-ALL-CCM SECTION.
       INI-ELI-ALL-CCM.
           PERFORM ELI-ALL-DCM.
           IF MSG-COD-MENS > SPACES
              GO TO MSG-ELI-ALL-CCM.
      *ELIMINA CCM
           PERFORM KEY-ALL-CCM.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-CCM.
           IF FIO-STAT-OKS AND
              CCM-STP-ITRN IN CCM NOT = SCR-UMOD-CCM
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM MC2-FIO-CCM
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-CCM
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM MC2-FIO-DCM
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-DCM
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-ELI-ALL-CCM.
           IF NOT FIO-STAT-OKS
               GO TO FIN-ELI-ALL-CCM.
           MOVE FIO-DEL TO FIO-CMND.
           PERFORM MC2-FIO-CCM.
           MOVE FIO-STAT TO PGM-STAT-CCM.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DCM
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-CCM
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-CCM
              MOVE '23' TO FIO-STAT PGM-STAT-DCM PGM-STAT-DCM
              GO TO FIN-ELI-ALL-CCM.

      *ELIMINA DPL
           MOVE 'TAB'                   TO FIO-SIST.
           MOVE 'DPL'                   TO TAB-COD-TTAB IN TAB.
           MOVE CCM-CIC-ICCM IN CCM-FLD TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
              MOVE PGM-STAT-DCM TO FIO-STAT
              GO TO FIN-ELI-ALL-CCM.
           MOVE FIO-DEL TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
              MOVE PGM-STAT-DCM TO FIO-STAT
              GO TO FIN-ELI-ALL-CCM.
           GO TO FIN-ELI-ALL-CCM.
       MSG-ELI-ALL-CCM.
           IF MSG-COD-SIST NOT > SPACES
              MOVE 'TAB' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MEN1.
           MOVE 'S' TO SCR-IERR.
       FIN-ELI-ALL-CCM.
           EXIT.
                                                                        
      *PVG INI 13-DIC-2007 ACTUALIZA CORRELATIVOS                       
       PRO-ACT-NCOR SECTION.                                            
       INI-PRO-ACT-NCOR.                                                
           MOVE SPACES TO MSG-COD-MENS.                                 
           MOVE ZEROES TO WSS-K.                                        
           MOVE CCM-CIC-ICCM IN CCM-FLD TO DCM-CIC-IDCM IN DCM.         
           MOVE ZEROES                  TO DCM-NUM-IDCM IN DCM.         
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-PRO-ACT-NCOR.                                                
           PERFORM MC2-FIO-DCM.                                         
           IF NOT FIO-STAT-OKS                                          
              GO TO CNT-PRO-ACT-NCOR.                                   
           IF NOT (CCM-CIC-ICCM IN CCM-FLD = DCM-CIC-IDCM IN DCM)       
              MOVE FIO-END-BRW TO FIO-CMND                              
              PERFORM MC2-FIO-DCM                                       
              GO TO CNT-PRO-ACT-NCOR.                                   
                                                                        
           ADD 1 TO WSS-K.                                              
           PERFORM CRG-DCM-VEC.                                         
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-PRO-ACT-NCOR.                                      
       CNT-PRO-ACT-NCOR.                                                
           PERFORM ELI-ALL-DCM.                                         
           IF MSG-COD-MENS > SPACES                                     
              GO TO MSG-PRO-ACT-NCOR.                                   
                                                                        
           PERFORM PUT-NEW-DCM.                                         
           IF MSG-COD-MENS > SPACES                                     
               GO TO MSG-PRO-ACT-NCOR.                                  
           GO TO FIN-PRO-ACT-NCOR.                                      
       MSG-PRO-ACT-NCOR.                                                
           IF MSG-COD-SIST NOT > SPACES                                 
              MOVE 'TAB' TO MSG-COD-SIST.                               
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MEN1.                               
           MOVE 'S'  TO SCR-IERR.                                       
           MOVE '00' TO FIO-STAT.                                       
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           GO TO FIN-PRO-ACT-NCOR.                                      
       FIN-PRO-ACT-NCOR.                                                
           EXIT.                                                        
      *PVG FIN 13-DIC-2007                                              
