      *
      * MC2BQAGD
      * Version 3.0.1 Nov-2005
      * 19-Feb-19 03:59 PM
      *
       SET-TAG-MC2BQAGD SECTION.
       INI-SET-TAG-MC2BQAGD.
           MOVE
           '<IDNMOD NOM=$MC2BQAGD$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$19-Feb-19 03:59 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-MC2BQAGD.
           EXIT.
           
       PUT-ALL-AGD SECTION.
       INI-PUT-ALL-AGD.
           PERFORM PUT-AGD-AGD.
       FIN-PUT-ALL-AGD.
           EXIT.
       
       PUT-AGD-AGD SECTION.
       INI-PUT-AGD-AGD.
           IF PGM-STAT-AGD-NUL
               GO TO FIN-PUT-AGD-AGD.
           IF NOT PGM-STAT-AGD-OKS
               PERFORM CLR-AGD-AGD
               GO TO FIN-PUT-AGD-AGD.
           MOVE AGD-CAI-IAGD IN AGD TO AGD-CAI-IAGD IN AGD-FLD.
           MOVE AGD-IIC-IAGD IN AGD TO AGD-IIC-IAGD IN AGD-FLD.
           MOVE AGD-NUM-IAGD IN AGD TO AGD-NUM-IAGD IN AGD-FLD.
           MOVE AGD-COD-TAGD IN AGD TO AGD-COD-TAGD IN AGD-FLD.
           MOVE AGD-COD-CAGD IN AGD TO AGD-COD-CAGD IN AGD-FLD.
      *<<<<
           MOVE 'TAB'                   TO TAB-COD-SIST.                
           MOVE 'AGD'                   TO TAB-CIC-TTAB IN TAB.         
           MOVE AGD-COD-TAGD IN AGD-FLD TO TAB-CIC-CTAB IN TAB.         
           MOVE 'TAB-CIC-TABL'          TO FIO-AKEY.                    
           PERFORM VAL-TAB.                                             
           IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'               
              MOVE AGD-COD-TAGD IN AGD-FLD TO FRM-GLS-TAGD IN AGD-FLD   
              MOVE AGD-COD-CAGD IN AGD-FLD TO FRM-GLS-CAGD IN AGD-FLD   
           ELSE                                                         
              MOVE TAB-GLS-DESC IN TAB     TO FRM-GLS-TAGD IN AGD-FLD   
              IF TAB-COD-DAT3(1) > SPACES AND                           
                 TAB-COD-DAT3(1) NOT = 'SIS'                            
                 IF TAB-COD-DAT3(10) NOT > SPACES                       
                    MOVE TAB-COD-DAT3(1)         TO TAB-COD-SIST        
                    MOVE TAB-GLS-DAT1 IN TAB     TO TAB-COD-TTAB IN TAB 
                    MOVE AGD-COD-CAGD IN AGD-FLD TO TAB-COD-CTAB IN TAB 
                    PERFORM VAL-TAB                                     
                    IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'      
                       MOVE AGD-COD-CAGD IN AGD-FLD TO                  
                            FRM-GLS-CAGD IN AGD-FLD                     
                    ELSE                                                
                       MOVE TAB-GLS-DESC IN TAB     TO                  
                            FRM-GLS-CAGD IN AGD-FLD                     
                 ELSE                                                   
                    MOVE TAB-COD-DAT3(1)         TO MSC-COD-SIST        
                    MOVE TAB-GLS-DAT1 IN TAB     TO MSC-COD-TMSC IN MSC 
                    MOVE AGD-COD-CAGD IN AGD-FLD TO MSC-COD-CMSC IN MSC 
                    PERFORM VAL-MSC                                     
                    IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'      
                       MOVE SPACES       TO FRM-GLS-CAGD IN AGD-FLD     
                       MOVE MSG-GLS-DESC TO FRM-MENS                    
                       MOVE FRM-SUAR-MAL TO FRM-SUAR                    
                    ELSE                                                
                       MOVE MSC-GLS-DESC IN MSC TO                      
                            FRM-GLS-CAGD IN AGD-FLD                     
              ELSE                                                      
                 MOVE AGD-COD-CAGD IN AGD-FLD TO                        
                      FRM-GLS-CAGD IN AGD-FLD.                          
      *>>>>                                                             
       FIN-PUT-AGD-AGD.
           EXIT.
       
       KEY-ALL-AGD SECTION.
       INI-KEY-ALL-AGD.
           PERFORM KEY-AGD-AGD.
       FIN-KEY-ALL-AGD.
           EXIT.
       
       KEY-AGD-AGD SECTION.
       INI-KEY-AGD-AGD.
           MOVE AGD-CAI-IAGD IN AGD-FLD TO AGD-CAI-IAGD IN AGD.
           MOVE AGD-IIC-IAGD IN AGD-FLD TO AGD-IIC-IAGD IN AGD.
           MOVE AGD-NUM-IAGD IN AGD-FLD TO AGD-NUM-IAGD IN AGD.
       FIN-KEY-AGD-AGD.
           EXIT.
       
       SCR-CLRA-AGD SECTION.
       INI-SCR-CLRA-AGD.
           PERFORM CLR-KEY-AGD.
           PERFORM CLR-FLD-AGD.
       FIN-SCR-CLRA-AGD.
           EXIT.
       
       CLR-KEY-AGD SECTION.
       INI-CLR-KEY-AGD.
           MOVE SPACES TO AGD-CAI-IAGD IN AGD-FLD.
           MOVE SPACES TO AGD-IIC-IAGD IN AGD-FLD.
           MOVE ZEROES TO AGD-NUM-IAGD IN AGD-FLD.
       FIN-CLR-KEY-AGD.
           EXIT.
       
       CLR-FLD-AGD SECTION.
       INI-CLR-FLD-AGD.
           MOVE SPACES TO FRM-GLS-DES1 IN AGD-FLD.
           MOVE SPACES TO FRM-GLS-DES2 IN AGD-FLD.
           MOVE SPACES TO FRM-GLS-TAGD IN AGD-FLD.
           MOVE SPACES TO FRM-GLS-CAGD IN AGD-FLD.
           PERFORM CLR-AGD-AGD.
       FIN-CLR-FLD-AGD.
           EXIT.
       
       CLR-AGD-AGD SECTION.
       INI-CLR-AGD-AGD.
           MOVE SPACES TO AGD-COD-TAGD IN AGD-FLD.
           MOVE SPACES TO AGD-COD-CAGD IN AGD-FLD.
       FIN-CLR-AGD-AGD.
           EXIT.
       
       FST-KEY-AGD SECTION.
       INI-FST-KEY-AGD.
           MOVE -1 TO AGD-CAI-IAGD-LEN IN AGD-FLD.
       FIN-FST-KEY-AGD.
           EXIT.
       
       FST-FLD-AGD SECTION.
       INI-FST-FLD-AGD.
           MOVE -1 TO AGD-COD-TAGD-LEN IN AGD-FLD.
       FIN-FST-FLD-AGD.
           EXIT.
       
       PRO-KEY-AGD SECTION.
       INI-PRO-KEY-AGD.
           PERFORM PRO-IKY-AGD.
           PERFORM PRO-AKY-AGD.
           PERFORM PRO-MKY-AGD.
       FIN-PRO-KEY-AGD.
           EXIT.
       
       PRO-IKY-AGD SECTION.
       INI-PRO-IKY-AGD.
           MOVE FRM-CPIM-AEY TO AGD-CAI-IAGD-ATR IN AGD-FLD.
           MOVE FRM-CPIM-AEY TO AGD-IIC-IAGD-ATR IN AGD-FLD.
           MOVE FRM-CPIM-AEY TO AGD-NUM-IAGD-ATR IN AGD-FLD.
       FIN-PRO-IKY-AGD.
           EXIT.
       
       PRO-MKY-AGD SECTION.
       INI-PRO-MKY-AGD.
       FIN-PRO-MKY-AGD.
           EXIT.
       
       PRO-AKY-AGD SECTION.
       INI-PRO-AKY-AGD.
       FIN-PRO-AKY-AGD.
           EXIT.
       
       UNP-KEY-AGD SECTION.
       INI-UNP-KEY-AGD.
           PERFORM UNP-IKY-AGD.
           PERFORM UNP-AKY-AGD.
           PERFORM UNP-MKY-AGD.
       FIN-UNP-KEY-AGD.
           EXIT.
       
       UNP-IKY-AGD SECTION.
       INI-UNP-IKY-AGD.
           MOVE FRM-CPIM-UBY TO AGD-CAI-IAGD-ATR.
           MOVE FRM-CPIM-UBY TO AGD-IIC-IAGD-ATR.
           MOVE FRM-CPIM-UBY TO AGD-NUM-IAGD-ATR.
       FIN-UNP-IKY-AGD.
           EXIT.
       
       UNP-MKY-AGD SECTION.
       INI-UNP-MKY-AGD.
       FIN-UNP-MKY-AGD.
           EXIT.
       
       UNP-AKY-AGD SECTION.
       INI-UNP-AKY-AGD.
       FIN-UNP-AKY-AGD.
           EXIT.
       
       UNP-FLD-AGD SECTION.
       INI-UNP-FLD-AGD.
           PERFORM UNP-AGD-AGD.
       FIN-UNP-FLD-AGD.
           EXIT.
       
       UNP-AGD-AGD SECTION.
       INI-UNP-AGD-AGD.
           MOVE FRM-CPIM-UBY TO AGD-COD-TAGD-ATR IN AGD-FLD.
           MOVE FRM-CPIM-UBY TO AGD-COD-CAGD-ATR IN AGD-FLD.
       FIN-UNP-AGD-AGD.
           EXIT.
       
       VAL-KEY-AGD SECTION.
       INI-VAL-KEY-AGD.
           PERFORM VAL-IKY-AGD.
           PERFORM VAL-MKY-AGD.
           PERFORM VAL-AKY-AGD.
       FIN-VAL-KEY-AGD.
           EXIT.
       
       VAL-IKY-AGD SECTION.
       INI-VAL-IKY-AGD.
           PERFORM VAL-AGD-CAI-IAGD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO AGD-CAI-IAGD-LEN IN AGD-FLD
                GO TO FIN-VAL-IKY-AGD.
           PERFORM VAL-AGD-IIC-IAGD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO AGD-IIC-IAGD-LEN IN AGD-FLD
                GO TO FIN-VAL-IKY-AGD.
           PERFORM VAL-AGD-NUM-IAGD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO AGD-NUM-IAGD-LEN IN AGD-FLD
                GO TO FIN-VAL-IKY-AGD.
       FIN-VAL-IKY-AGD.
           EXIT.
       
       VAL-MKY-AGD SECTION.
       INI-VAL-MKY-AGD.
       FIN-VAL-MKY-AGD.
           EXIT.
       
       VAL-AKY-AGD SECTION.
       INI-VAL-AKY-AGD.
       FIN-VAL-AKY-AGD.
           EXIT.
       
       VAL-AGD-CAI-IAGD SECTION.
       INI-VAL-AGD-CAI-IAGD.
           IF AGD-CAI-IAGD IN AGD-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-AGD-CAI-IAGD.
       FIN-VAL-AGD-CAI-IAGD.
           EXIT.
       
       VAL-AGD-IIC-IAGD SECTION.
       INI-VAL-AGD-IIC-IAGD.
      *<<<<
           IF AGD-IIC-IAGD IN AGD-FLD NOT > SPACES                      
      *MHM-INI 30-OCT-2007                                              
      *         PERFORM ERR-GLS                                         
      *         GO TO FIN-VAL-AGD-IIC-IAGD.                             
               MOVE SPACES TO AGD-IIC-IAGD IN AGD-FLD.                  
      *MHM-FIN 30-OCT-2007                                              
           GO TO FIN-VAL-AGD-IIC-IAGD.
      *>>>>
           IF AGD-IIC-IAGD IN AGD-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-AGD-IIC-IAGD.
       FIN-VAL-AGD-IIC-IAGD.
           EXIT.
       
       VAL-AGD-NUM-IAGD SECTION.
       INI-VAL-AGD-NUM-IAGD.
      *<<<<
           GO TO FIN-VAL-AGD-NUM-IAGD.
      *>>>>
           IF AGD-NUM-IAGD IN AGD-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-AGD-NUM-IAGD.
       FIN-VAL-AGD-NUM-IAGD.
           EXIT.
       
       VAL-CON-KEY-AGD SECTION.
       INI-VAL-CON-KEY-AGD.
           IF     AGD-CAI-IAGD IN AGD-FLD NOT > SPACES 
              AND AGD-IIC-IAGD IN AGD-FLD NOT > SPACES 
              AND AGD-NUM-IAGD IN AGD-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-AGD.
       FIN-VAL-CON-KEY-AGD.
           EXIT.
       
       VAL-CON-IKY-AGD SECTION.
       INI-VAL-CON-IKY-AGD.
           IF     AGD-CAI-IAGD IN AGD-FLD NOT > SPACES 
              AND AGD-IIC-IAGD IN AGD-FLD NOT > SPACES 
              AND AGD-NUM-IAGD IN AGD-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-AGD.
       FIN-VAL-CON-IKY-AGD.
           EXIT.
       
       VAL-NUM-KEY-AGD SECTION.
       INI-VAL-NUM-KEY-AGD.
           PERFORM VAL-NUM-IKY-AGD.
           PERFORM VAL-NUM-MKY-AGD.
           PERFORM VAL-NUM-AKY-AGD.
       FIN-VAL-NUM-KEY-AGD.
           EXIT.
       
       VAL-NUM-IKY-AGD SECTION.
       INI-VAL-NUM-IKY-AGD.
       VAL-NUM-AGD-NUM-IAGD.
           MOVE AGD-NUM-IAGD-ALF IN AGD-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO AGD-NUM-IAGD IN AGD-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO AGD-NUM-IAGD-LEN IN AGD-FLD
                GO TO FIN-VAL-NUM-IKY-AGD.
       FIN-VAL-NUM-IKY-AGD.
           EXIT.
       
       VAL-NUM-MKY-AGD SECTION.
       INI-VAL-NUM-MKY-AGD.
       FIN-VAL-NUM-MKY-AGD.
           EXIT.
       
       VAL-NUM-AKY-AGD SECTION.
       INI-VAL-NUM-AKY-AGD.
       FIN-VAL-NUM-AKY-AGD.
           EXIT.
       
       VAL-NUM-FLD-AGD SECTION.
       INI-VAL-NUM-FLD-AGD.
       FIN-VAL-NUM-FLD-AGD.
           EXIT.
       
       EDT-KEY-AGD SECTION.
       INI-EDT-KEY-AGD.
           PERFORM EDT-IKY-AGD.
           PERFORM EDT-MKY-AGD.
           PERFORM EDT-AKY-AGD.
       FIN-EDT-KEY-AGD.
           EXIT.
       
       EDT-IKY-AGD SECTION.
       INI-EDT-IKY-AGD.
           IF AGD-NUM-IAGD-LEN IN AGD-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO AGD-NUM-IAGD-LEN IN AGD-FLD
                GO TO FIN-EDT-IKY-AGD.
           MOVE AGD-NUM-IAGD IN AGD-FLD TO AGD-NUM-IAGD-EDT IN AGD-EDT.
           MOVE AGD-NUM-IAGD-EDT IN AGD-EDT TO 
                AGD-NUM-IAGD-ALF IN AGD-FLD.
       FIN-EDT-IKY-AGD.
           EXIT.
       
       EDT-MKY-AGD SECTION.
       INI-EDT-MKY-AGD.
       FIN-EDT-MKY-AGD.
           EXIT.
       
       EDT-AKY-AGD SECTION.
       INI-EDT-AKY-AGD.
       FIN-EDT-AKY-AGD.
           EXIT.
       
       EDT-FLD-AGD SECTION.
       INI-EDT-FLD-AGD.
       FIN-EDT-FLD-AGD.
           EXIT.
