*%! Codigo Adicional
*% PGM_PTC = TRUE
*% FRM = "PIP"
*% PADRE = "PIP"
*% KEY_PADRE = "PIP-KEY-IPIP"
*% LARGO_KEY_PADRE = 38
*% FIO_PADRE = "COL-FIO-PIP"
*% SIST_PADRE = "COL"
*% KEY_BROWSE_PADRE = "PIP-KEY-IPIP"
*% ORG_PADRE_VSI = TRUE
*% ACEPTE_PF7 = TRUE
*% ACEPTE_PF8 = TRUE
*% INCLUDE 'GNSSRC:PFSCONENT.SRC'
*% IF WSS
      *<<<< WSS
       COPY GNSWGCPT.
       COPY SGCBRDBC.

       01  WSS-VARI.
           03  WSS-COM-TIP-IMP               PIC X(03).
           03  WSS-VCOC-ANT                  PIC X(06).
           03  WSS-COM-ANT                   PIC X(03).
           03  WSS-VAL-MANT                  PIC S9(08)V9(04).
*% IF NOT PGM_MCP
           03  WSS-FRM-TOP       VALUE  06   PIC 9(02).
*% ELSE
           03  WSS-FRM-TOP       VALUE  8   PIC 9(02).
*% END          
      *>>>>
*% END
*% IF FND_KEY
      *<<<< FND_KEY
           IF SCR-CCPP = 'ACC' AND SCR-STPP = 'C'
              MOVE FIO-GET-LEQ TO FIO-CMND
              PERFORM PIP-ASUME
           ELSE
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
           IF SCR-CCPP = 'ACC' AND SCR-STPP = 'C'
              IF NOT ( PIP-COD-PLAN IN PIP = PIP-COD-PLAN IN PIP-FLD AND
                       PIP-COD-EVEN IN PIP = PIP-COD-EVEN IN PIP-FLD AND
                     ( PIP-COD-OFIC IN PIP = PIP-COD-OFIC IN PIP-FLD OR
                       PIP-COD-OFIC IN PIP = '*  ' ))
                  MOVE 23 TO FIO-STAT.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBGCPT.
       COPY SGCBFDBC.

       PIP-ASUME SECTION.
       INI-PIP-ASUME.
           IF PIP-FEC-FPLA IN PIP-FLD NOT >  ZEROES
              MOVE 99999999 TO PIP-FEC-FPLA IN PIP.
           IF PIP-COD-OFIC IN PIP-FLD NOT > SPACES
              MOVE '*  '   TO PIP-COD-OFIC IN PIP.
       FIN-PIP-ASUME.
           EXIT.
      *>>>>
*% END
