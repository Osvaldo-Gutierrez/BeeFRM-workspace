*%! Codigo adicional para archivo COLPQQCR
*% IF SETTAG
*% FNCPQ = ""
*% VSNPQ = ""
*% FNCPU = ""
*% VSNPU = ""
*% END
*% WIG_USR = TRUE
*% LARGO_TOTAL_COMMAREA = 3000
*% NO_TIENE_TIMESTAMP = TRUE
*% PGM_PTC = TRUE
*% IF WSS
      *<<<< WSS 
       COPY COLWXQCR.
       COPY GNSWGCPT.
       COPY GNSWGSTR.

       COPY COLBROPC.
       COPY SGCBRDBC.                                                   
      *>>>>                                                             
      *<<<< WSS_DTC                                                     
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OPC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSC-REQA==.        
       COPY GNSBRMSC. 
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OFI-REQA==.        
       COPY TABBROFI.  
       COPY GNSWCVRF.                                                   
       COPY GNSWVVRF.                                                   
       COPY GNSWGIDC. 

       01 WSS-I             VALUE ZEROES PIC 9(03).
       01 WSS-NUM-CCUR.
           03 WSS-NUM-ICUR-EDT  PIC  ZZ9.
           03 WSS-NUM-ICUR-ALF       PIC X(03).
           03 WSS-NUM-ICUR      REDEFINES
              WSS-NUM-ICUR-ALF       PIC 9(03).
      *>>>>
*% END
*% IF FIN_SCR_VARI
      *<<<< FIN_SCR_VARI
           03 SCR-NUM-PANT               PIC 9(0005).
           03 SCR-QCR-FLD                PIC X(1100).
       COPY COLWIQCR.  
      *>>>> FIN_SCR_VARI
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-OFRM-SRV
                MOVE TSK-TERM-ALF TO QCR-COD-TERM
                MOVE TSK-TERM-ALF TO QCR-COD-TERM-MAS
           ELSE
                MOVE EIBTRMID     TO QCR-COD-TERM
                MOVE EIBTRMID     TO QCR-COD-TERM-MAS.
           MOVE +3859 TO SCR-TCMA.
           PERFORM GNS-ERR-QUE.
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )
               MOVE SCR-QCR-FLD TO QCR-FLD
               GO TO FIN-INI.

           IF ( FRM-FFLD = FRM-FFLD-PF6 )
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM
               ADD 1              TO QUE-NITM
               MOVE QCR-NOM-TSQCR TO QUE-COLA
               MOVE QCR-LEN-TSQCR TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT NOT = QUE-STAT-OKS
                  MOVE SCR-QCR-FLD TO QCR-FLD
                  GO TO FIN-INI.
      *>>>> 
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC 
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6                   
               PERFORM VAL-NUM-KEY-QCR                                  
               PERFORM VAL-NUM-FLD-QCR                                  
               PERFORM SCRL-TAB-QCR
               PERFORM SCR-TRL.
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
           PERFORM KEY-ALL-QCR.
           MOVE CCR-CAI-IOPC IN CCR TO OPC-CAI-IOPC IN OPC.
           MOVE CCR-IIC-IOPC IN CCR TO OPC-IIC-IOPC IN OPC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-OPC.
      *BUS-TAB busca glosa de codigo en tabla
           IF OPC-COD-VCOC IN OPC     > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'VLR ' TO TAB-COD-TTAB IN TAB
               MOVE OPC-COD-VCOC IN OPC     TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-VCOC IN QCR-FLD
           ELSE                                                         
               MOVE SPACES TO FRM-GLS-VCOC IN QCR-FLD.
      *BUS-TAB busca glosa de codigo en tabla                           
           IF OPC-COD-TOOC IN OPC > SPACES                              
               MOVE 'TAB' TO TAB-COD-SIST                               
               MOVE 'TIO ' TO TAB-COD-TTAB IN TAB                       
               MOVE OPC-COD-TOOC IN OPC TO TAB-COD-CTAB IN TAB          
               PERFORM BUS-TAB                                          
               MOVE TAB-GLS-DESC IN TAB TO                              
                    FRM-GLS-COOC IN QCR-FLD                             
           ELSE                                                         
               MOVE SPACES TO FRM-GLS-COOC IN QCR-FLD.                  
      *BUS-OFI busca oficina en tablas                                  
           IF OPC-COD-OFOC IN OPC > SPACES                              
               MOVE OPC-COD-OFOC IN OPC TO OFI-COD-OFIC IN OFI          
               PERFORM BUS-OFI                                          
               MOVE OFI-GLS-DESC IN OFI TO FRM-GLS-OFOC IN QCR-FLD      
           ELSE                                                         
               MOVE SPACES TO FRM-GLS-OFOC IN QCR-FLD.                  

           MOVE ZEROES                      TO CCR-NUM-ICCR IN CCR.
           IF FRM-IND-HMAS IN QCR-FLD = 'S'
               MOVE FRM-CAI-IOPC IN QCR-FLD TO CCR-CAI-IOPC IN CCR
               MOVE FRM-IIC-IOPC IN QCR-FLD TO CCR-IIC-IOPC IN CCR
               MOVE FRM-NUM-IDLC IN QCR-FLD TO CCR-NUM-IDLC IN CCR
               MOVE FRM-VAL-ICCR IN QCR-FLD TO CCR-NUM-ICCR IN CCR.

           MOVE FIO-GET-NLS TO FIO-CMND.

       LUP-LEE-CCR.
           PERFORM COL-FIO-CCR.                                         
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF
               GO TO EXI-FND-KEY.

           IF NOT ( CCR-CAI-IOPC IN QCR-FLD = CCR-CAI-IOPC IN CCR AND
                    CCR-IIC-IOPC IN QCR-FLD = CCR-IIC-IOPC IN CCR AND
                    CCR-NUM-IDLC IN QCR-FLD = CCR-NUM-IDLC IN CCR )
               GO TO EXI-FND-KEY.

           ADD 1 TO WSS-NUM-ICCR IN WSS-VARI.

           PERFORM CRG-VEC-QCR.

           IF (SCR-OFRM-SRV AND WSS-J > WSS-NITMMAX)
              GO TO EXI-FND-KEY.

           IF ((NOT SCR-OFRM-SRV) AND WSS-J > WSS-JMAX)
                 GO TO EXI-FND-KEY.

       NXT-LEE-CCR.
           MOVE FIO-GET-NXT     TO FIO-CMND.
           GO TO LUP-LEE-CCR.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
       EXI-FND-KEY.
           PERFORM PUT-TAB.

           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *----------------------------------------------------------
       MSG-FND-KEY.
           MOVE 'COL' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )   
      *>>>>
*% END
*% IF EOF
      *<<<< 
       COPY GNSBBTAB.
       COPY GNSBVIND.                                                   
       COPY GNSBVMSC.                                                   
       COPY GNSBFMSC.                                                   
       COPY GNSBVIDC.                                                   
       COPY GNSBPVRF. 
       COPY TABBBOFI.                                                   
       COPY TABBFOFI.

       COPY GNSBVTAB.
       COPY GNSBVCOD.
       COPY COLBFOPC.
       COPY SGCBFDBC.
                                                                        
       PUT-TAB SECTION.
       INI-PUT-TAB.
           MOVE QCR-NOM-TSQCR     TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE QCR-NOM-TSQCR-MAS TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE 1 TO SCR-NUM-PANT.
           MOVE SPACES TO QCR-VEC-TS.
           MOVE LOW-VALUES TO WSS-GLS-NQCR-IDX-FRM.
      *
           MOVE 1 TO FRM-IFLD.
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.

           MOVE 1 TO WSS-F IN WSS-VARI.
       LUP-PUT-TAB.
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI
              GO TO FIN-PUT-TAB.
           IF WSS-NUM-ICCR-V(WSS-F) > ZEROES
              PERFORM PUT-TAB-QCR
              PERFORM PUT-QCR-QUE
              PERFORM SET-FRM-IFLD
           ELSE                                                         
           IF FRM-IFLD = 1 AND WSS-F = 1
              MOVE 'COL' TO MSG-COD-SIST
              MOVE 'QCR    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE '00' TO FIO-STAT
      *       MOVE 'S' TO SCR-IERR
              GO TO FIN-PUT-TAB
           ELSE
              MOVE '00' TO FIO-STAT
              GO TO FIN-PUT-TAB.
                                                                        
           ADD 1 TO WSS-F IN WSS-VARI.
           GO TO LUP-PUT-TAB.
       FIN-PUT-TAB.
           MOVE 1                  TO QUE-NITM.
           MOVE QCR-LEN-TSQCR-MAS  TO QUE-LITM.
           MOVE QCR-NOM-TSQCR-MAS  TO QUE-COLA.

           IF WSS-NITM > WSS-NITMMAX
              MOVE 'S'           TO QCR-MAS-IND-HMAS
           ELSE                                                         
              MOVE 'N'           TO QCR-MAS-IND-HMAS.

           MOVE WSS-NITM         TO WSS-NUM-ICUR.
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.
           MOVE WSS-NUM-ICUR-ALF TO QCR-MAS-NUM-TOCC.
           MOVE QCR-MAS-ITEM     TO QUE-ITEM.
           MOVE QUE-PUT          TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF WSS-NITM > WSS-IMAX
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'QCRTABDMAS' TO MSG-COD-MENS
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN2.                           
       EXT-PUT-TAB.
           EXIT.                                                        
                                                                        
       SET-FRM-IFLD SECTION.
       INI-SET-FRM-IFLD.
           IF SCR-OFRM-SRV
                MOVE 1 TO FRM-IFLD
           ELSE
                ADD 1 TO FRM-IFLD.
       FIN-SET-FRM-IFLD.
           EXIT.
      *--------------------------------------------
       SCRL-TAB-QCR SECTION.
       INI-SCRL-TAB-QCR.
           MOVE SPACES TO MSG-COD-MENS.

           IF FRM-FFLD = FRM-FFLD-PF6
               PERFORM PUT-TAB-ASC
           ELSE
               PERFORM PUT-TAB-DSC.
           IF MSG-COD-MENS > SPACES
               MOVE 'COL' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-QCR.
           EXIT.

      *--------------------------------------------
       PUT-TAB-QCR SECTION.
       INI-PUT-TAB-QCR.
           PERFORM CLR-WSS.
           MOVE WSS-NUM-ICCR-V(WSS-F) TO WSS-NUM-ICCR-QCR IN WSS-VARI.
           MOVE WSS-FEC-FCCR-V(WSS-F) TO WSS-FEC-FCCR-QCR IN WSS-VARI.
           MOVE WSS-GLS-SCRA-V(WSS-F) TO WSS-GLS-SCRA-QCR IN WSS-VARI.
           MOVE WSS-GLS-SCRT-V(WSS-F) TO WSS-GLS-SCRT-QCR IN WSS-VARI
           MOVE WSS-FEC-FING-V(WSS-F) TO WSS-FEC-FING-QCR IN WSS-VARI.
           MOVE WSS-COD-RCCR-V(WSS-F) TO WSS-COD-RCCR-QCR IN WSS-VARI.

           MOVE WSS-NUM-ICCR-QCR TO WSS-NUM-ICCR-FRM.
           MOVE WSS-FEC-FCCR-QCR TO WSS-FEC-FCCR-FRM.
           MOVE WSS-GLS-SCRA-QCR TO WSS-GLS-SCRA-FRM.
           MOVE WSS-GLS-SCRT-QCR TO WSS-GLS-SCRT-FRM.
           MOVE WSS-FEC-FING-QCR TO WSS-FEC-FING-FRM.
           MOVE WSS-FEC-FING-QCR TO WSS-FEC-FING-FRM.
           MOVE WSS-COD-RCCR-QCR TO WSS-COD-RCCR-FRM.

           IF FRM-IFLD > WSS-IMAX
              GO TO FIN-PUT-TAB-QCR.
           MOVE WSS-NUM-ICCR-V(WSS-F) TO
               FRM-NUM-ICCR IN QCR-FLD(FRM-IFLD).
           MOVE WSS-FEC-FCCR-V(WSS-F) TO
               FRM-FEC-FCCR IN QCR-FLD(FRM-IFLD).
           MOVE WSS-GLS-SCRA-V(WSS-F) TO
               FRM-GLS-SCRA IN QCR-FLD(FRM-IFLD).
           MOVE WSS-GLS-SCRT-V(WSS-F) TO
               FRM-GLS-SCRT IN QCR-FLD(FRM-IFLD).
           MOVE WSS-FEC-FING-V(WSS-F) TO
               FRM-FEC-FING IN QCR-FLD(FRM-IFLD).
           MOVE WSS-COD-RCCR-V(WSS-F) TO
               FRM-COD-RCCR IN QCR-FLD(FRM-IFLD).
       FIN-PUT-TAB-QCR.
           EXIT.
      *----------------------------------------------------
       PUT-QCR-QUE SECTION.
       INI-PUT-QCR-QUE.
           PERFORM PRO-FLD-QUE.
           ADD 1 TO WSS-NITM.
           MOVE QCR-NOM-TSQCR    TO QUE-COLA.
           MOVE QCR-LEN-TSQCR    TO QUE-LITM.
           MOVE WSS-NITM         TO QUE-NITM.
           MOVE SPACES           TO QUE-ITEM.
           IF SCR-OFRM-SRV
              MOVE QCR-VEC-TS    TO QUE-ITEM
           ELSE
              MOVE WSS-GLS-NQCR-IDX-FRM  TO QUE-ITEM.
           MOVE QUE-PUT  TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
       FIN-PUT-QCR-QUE.
           EXIT.
      *----------------------------------------------------

       PUT-TAB-ASC SECTION.                                             
       INI-PUT-TAB-ASC.                                                 
           MOVE SPACES TO MSG-COD-MENS.
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.
           MOVE 0 TO FRM-IFLD.
       LUP-PUT-TAB-ASC.                                                 
           ADD 1              TO QUE-NITM.
           MOVE QCR-NOM-TSQCR TO QUE-COLA.
           MOVE QCR-LEN-TSQCR TO QUE-LITM.
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD = 0                                          
                   MOVE 'QCRTABNONXT' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-ASC                                
               ELSE                                                     
                   GO TO FIN-PUT-TAB-ASC.                               
           ADD 1 TO FRM-IFLD.
           MOVE QUE-ITEM TO FRM-NUM-ICCR-IDX(FRM-IFLD).
           IF FRM-IFLD < WSS-IMAX                                       
               GO TO LUP-PUT-TAB-ASC.                                   
       FIN-PUT-TAB-ASC.                                                 
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX
               ADD 1 FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1
                                 UNTIL WSS-I > WSS-IMAX.
           IF FRM-IFLD > 0
               ADD 1 TO SCR-NUM-PANT.
           IF FRM-IFLD = WSS-IMAX
               ADD 1              TO QUE-NITM
               MOVE QCR-NOM-TSQCR TO QUE-COLA
               MOVE QCR-LEN-TSQCR TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT = QUE-STAT-OKS
                   MOVE 'COL'        TO MSG-COD-SIST
                   MOVE 'QCRTABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-ASC.
           EXIT.                                                        
                                                                        
       PUT-TAB-DSC SECTION.                                             
       INI-PUT-TAB-DSC.                                                 
           MOVE SPACES TO MSG-COD-MENS.
           IF SCR-NUM-PANT = 1
               MOVE 'QCRTABNOPRV' TO MSG-COD-MENS
               GO TO EXT-PUT-TAB-DSC.

           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.
           ADD 1 TO QUE-NITM.
           ADD 1 WSS-IMAX GIVING FRM-IFLD.
       LUP-PUT-TAB-DSC.                                                 
           SUBTRACT  1 FROM QUE-NITM.
           MOVE QCR-NOM-TSQCR TO QUE-COLA
           MOVE QCR-LEN-TSQCR TO QUE-LITM
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD > WSS-IMAX
                   MOVE 'QCRTABNOPRV' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-DSC                                
               ELSE                                                     
                   GO TO FIN-PUT-TAB-DSC.                               
           SUBTRACT 1 FROM FRM-IFLD.
           MOVE QUE-ITEM TO FRM-NUM-ICCR-IDX(FRM-IFLD).
           IF FRM-IFLD > 1                                              
               GO TO LUP-PUT-TAB-DSC.                                   
       FIN-PUT-TAB-DSC.                                                 

           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1
                                 UNTIL WSS-I < 1.

           IF FRM-IFLD NOT > WSS-IMAX
               SUBTRACT 1 FROM SCR-NUM-PANT
               MOVE 'COL'        TO MSG-COD-SIST
               MOVE 'QCRTABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-DSC.
           EXIT.                                                        
                                                                        
       COPY GNSBGCPT.
       COPY GNSBGSTR.

       CLR-FRM SECTION.
       INI-CLR-FRM.
           MOVE ZEROES TO FRM-NUM-ICCR IN QCR-FLD(WSS-I).
           MOVE SPACES TO FRM-FEC-FCCR IN QCR-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-SCRA IN QCR-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-SCRT IN QCR-FLD(WSS-I).
           MOVE SPACES TO FRM-FEC-FING IN QCR-FLD(WSS-I).
           MOVE SPACES TO FRM-COD-RCCR IN QCR-FLD(WSS-I).
       FIN-CLR-FRM.
           EXIT.                                                        

       CLR-WSS SECTION.
       INI-CLR-WSS.
           MOVE ZEROES TO WSS-NUM-ICCR-QCR IN WSS-VARI.
           MOVE SPACES TO WSS-FEC-FCCR-QCR IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-SCRA-QCR IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-SCRT-QCR IN WSS-VARI.
           MOVE SPACES TO WSS-FEC-FING-QCR IN WSS-VARI.
           MOVE SPACES TO WSS-COD-RCCR-QCR IN WSS-VARI.

           IF SCR-OFRM-SRV
               PERFORM CLR-FRM-MTX.
       FIN-CLR-WSS.
           EXIT.                                                        
                                                                        
       CLR-FRM-MTX SECTION.
       INI-CLR-FRM-MTX.
           MOVE ZEROES TO FRM-NUM-ICCR IN QCR-FLD(1).
           MOVE SPACES TO FRM-FEC-FCCR IN QCR-FLD(1).
           MOVE SPACES TO FRM-GLS-SCRA IN QCR-FLD(1).
           MOVE SPACES TO FRM-GLS-SCRT IN QCR-FLD(1).
           MOVE SPACES TO FRM-FEC-FING IN QCR-FLD(1).
           MOVE SPACES TO FRM-COD-RCCR IN QCR-FLD(1).
       FIN-CLR-FRM-MTX.
           EXIT.                                                        
                                                                        
       CRG-VEC-QCR SECTION.
       INI-CRG-VEC-QCR.
           ADD 1 TO WSS-J.
           IF WSS-J IN WSS-VARI > WSS-JMAX IN WSS-VARI
               GO TO FIN-CRG-VEC-QCR.

           PERFORM CLR-QCR-VEC.
           IF SCR-OFRM-SRV
               MOVE CCR-NUM-ICCR IN CCR TO
                        FRM-NUM-ICCR-EDT IN QCR-EDT
               MOVE FRM-NUM-ICCR-EDT IN QCR-EDT TO
                        WSS-NUM-ICCR-V-ALF IN WSS-VARI(WSS-J)
           ELSE
               MOVE CCR-NUM-ICCR IN CCR TO
                    WSS-NUM-ICCR-V IN WSS-VARI(WSS-J).
      *
           MOVE CCR-NUM-DCCR IN CCR TO
                WSS-NUM-DCCR-V IN WSS-VARI(WSS-J).
           MOVE CCR-NUM-MCCR IN CCR TO
                WSS-NUM-MCCR-V IN WSS-VARI(WSS-J).
           MOVE CCR-NUM-SCCR IN CCR TO
                WSS-NUM-SCCR-V IN WSS-VARI(WSS-J).
           MOVE CCR-NUM-ACCR IN CCR TO
                WSS-NUM-ACCR-V IN WSS-VARI(WSS-J).

           MOVE SPACES              TO
                WSS-GLS-SCRT-V IN WSS-VARI(WSS-J).
           IF CCR-IND-SCRT IN CCR > SPACES
               MOVE 'COL' TO MSC-COD-SIST FIO-SIST
               MOVE 'SCRT' TO MSC-COD-TMSC IN MSC
               MOVE CCR-IND-SCRT IN CCR TO MSC-COD-CMSC IN MSC
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM GNS-FIO-MSC
               IF FIO-STAT-OKS
                   MOVE MSC-GLS-DCOR IN MSC TO
                        WSS-GLS-SCRT-V IN WSS-VARI(WSS-J).
           MOVE SPACES              TO
                WSS-GLS-SCRA-V IN WSS-VARI(WSS-J).
           IF CCR-IND-SCRA IN CCR > SPACES
               MOVE 'COL' TO MSC-COD-SIST FIO-SIST
               MOVE 'SCRT' TO MSC-COD-TMSC IN MSC
               MOVE CCR-IND-SCRA IN CCR TO MSC-COD-CMSC IN MSC
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM GNS-FIO-MSC
               IF FIO-STAT-OKS
                   MOVE MSC-GLS-DCOR IN MSC TO
                        WSS-GLS-SCRA-V IN WSS-VARI(WSS-J).
      *
           MOVE CCR-NUM-DING IN CCR TO
                WSS-NUM-DING-V IN WSS-VARI(WSS-J).
           MOVE CCR-NUM-MING IN CCR TO
                WSS-NUM-MING-V IN WSS-VARI(WSS-J).
           MOVE CCR-NUM-SING IN CCR TO
                WSS-NUM-SING-V IN WSS-VARI(WSS-J).
           MOVE CCR-NUM-AING IN CCR TO
                WSS-NUM-AING-V IN WSS-VARI(WSS-J).
      *
           MOVE CCR-COD-RCCR IN CCR TO
                WSS-COD-RCCR-V IN WSS-VARI(WSS-J).
       FIN-CRG-VEC-QCR.
           EXIT.

       CLR-QCR-VEC SECTION.
       INI-CLR-QCR-VEC.
           MOVE ZEROES TO    WSS-NUM-ICCR-V(WSS-I).
           MOVE SPACES TO    WSS-FEC-FCCR-V(WSS-I).
           MOVE SPACES TO    WSS-GLS-SCRA-V(WSS-I).
           MOVE SPACES TO    WSS-GLS-SCRT-V(WSS-I).
           MOVE SPACES TO    WSS-FEC-FING-V(WSS-I).
           MOVE SPACES TO    WSS-COD-RCCR-V(WSS-I).
       FIN-CLR-QCR-VEC.
           EXIT.

       PRO-FLD-QUE SECTION.
       INI-PRO-FLD-QUE.
           MOVE 'P' TO WSS-NUM-ICCR-ATR.
           MOVE 'P' TO WSS-FEC-FCCR-ATR.
           MOVE 'P' TO WSS-GLS-SCRT-ATR.
           MOVE 'P' TO WSS-GLS-SCRA-ATR.
           MOVE 'P' TO WSS-FEC-FING-ATR.
           MOVE 'P' TO WSS-COD-RCCR-ATR.
       FIN-PRO-FLD-QUE.
           EXIT.
      *>>>>
*% END
