*%! Codigo adicional
*% NO_TIENE_TIMESTAMP = TRUE
*% NO_HDR_CST = TRUE
*% LARGO_TOTAL_COMMAREA = 3000
*% FMT_CHI = TRUE
*% GEN_BCI = TRUE
*% PGM_PTC = TRUE
*% IF SETTAG
*% VSNPQ = "08.08.20.1"
*% FNCPQ = "CNCMSV"
*% END
*% IF WSS
      *<<<< WSS
       COPY COLWXCDC.
       COPY GNSWGCPT.
       COPY GNSWGSTR.

       COPY COLBROPC.
       COPY SGCBRDBC.
*% IF NOT PGM_MCP AND NOT PGM_PTC
       COPY GNSWGQUE.
*% END
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OPC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.

       01 WSS-NUM-CCUR.
           03 WSS-NUM-ICUR-EDT  PIC  ZZ9.
           03 WSS-NUM-ICUR-ALF       PIC X(03).
           03 WSS-NUM-ICUR      REDEFINES
              WSS-NUM-ICUR-ALF       PIC 9(03).
      *>>>>
*% END
*% IF FIN_SCR_VARI
           03 SCR-NUM-PANT               PIC 9(0005).
           03 SCR-CDC-FLD                PIC X(1100).
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-OFRM-SRV
                MOVE TSK-TERM-ALF TO CDC-COD-TERM
                MOVE TSK-TERM-ALF TO CDC-COD-TERM-MAS
           ELSE
                MOVE EIBTRMID     TO CDC-COD-TERM
                MOVE EIBTRMID     TO CDC-COD-TERM-MAS.
           MOVE +3859 TO SCR-TCMA.
           PERFORM GNS-ERR-QUE.
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )
               MOVE SCR-CDC-FLD TO CDC-FLD
               GO TO FIN-INI.

           IF ( FRM-FFLD = FRM-FFLD-PF6 )
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM
               ADD 1              TO QUE-NITM
               MOVE CDC-NOM-TSCDC TO QUE-COLA
               MOVE CDC-LEN-TSCDC TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT NOT = QUE-STAT-OKS
                  MOVE SCR-CDC-FLD TO CDC-FLD
                  GO TO FIN-INI.
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
           MOVE SPACES TO WSS-IND-LEID IN WSS-VARI.                             
           IF FRM-IND-SGTE IN CDC-FLD = 'S'                                     
               MOVE FRM-NUM-ICLT IN CDC-FLD TO DBC-NUM-ICLI IN DBC              
               MOVE SPACES                  TO DBC-IND-ICLI IN DBC              
               MOVE SPACES                  TO DBC-GLS-ICLI IN DBC              
               MOVE 'DBC-IDC-ICLI'          TO FIO-AKEY                         
               MOVE FIO-GET-KEY TO FIO-CMND                                     
               PERFORM SGC-FIO-DBC                                              
               IF NOT FIO-STAT-OKS                                              
                   GO TO EXI-FND-KEY                                            
               ELSE                                                             
                  MOVE DBC-CAI-ICLI IN DBC TO WSS-CAI-ICLI IN WSS-VARI          
                  MOVE DBC-IIC-ICLI IN DBC TO WSS-IIC-ICLI IN WSS-VARI.         
                                                                                
           MOVE RDC-CAI-IOPC IN CDC-FLD TO RDC-CAI-IOPC IN RDC.                 
           MOVE RDC-IIC-IOPC IN CDC-FLD TO RDC-IIC-IOPC IN RDC.                 
           MOVE ZEROES                  TO RDC-NUM-IDLC IN RDC.                 
           MOVE 'RDC-KEY-IDLC'          TO FIO-AKEY.                            
           MOVE FIO-GET-NLS TO FIO-CMND.                                        

       LUP-LEE-RDC.                                                             
           PERFORM COL-FIO-RDC.                                                 
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF                                  
               GO TO EXI-FND-KEY.                                               
                
           IF NOT ( RDC-CAI-IOPC IN CDC-FLD = RDC-CAI-IOPC IN RDC AND        
                    RDC-IIC-IOPC IN CDC-FLD = RDC-IIC-IOPC IN RDC )        
               GO TO EXI-FND-KEY.                                              


           IF FRM-COD-TDEU IN CDC-FLD = 'DIR' AND                               
              RDC-NUM-IDLC IN RDC NOT = ZEROES                                  
               GO TO NXT-LEE-RDC.                                               
                                                                                
           IF FRM-COD-TDEU IN CDC-FLD = 'IND' AND                               
              RDC-NUM-IDLC IN RDC = ZEROES                                      
               GO TO NXT-LEE-RDC.                                               
                                                                                
           IF FRM-IND-SGTE IN CDC-FLD = 'S' AND                                 
              WSS-IND-LEID IN WSS-VARI NOT = 'S'                                
             IF NOT (WSS-CAI-ICLI IN WSS-VARI = RDC-CAI-ICLI IN RDC AND         
                     WSS-IIC-ICLI IN WSS-VARI = RDC-IIC-ICLI IN RDC )           
                GO TO NXT-LEE-RDC.                                              
                                                                                
           MOVE 'S' TO WSS-IND-LEID IN WSS-VARI.                                

           MOVE RDC-CAI-IOPC IN RDC  TO OPC-CAI-IOPC IN OPC.  
           MOVE RDC-IIC-IOPC IN RDC  TO OPC-IIC-IOPC IN OPC.  
           MOVE FIO-GET-KEY          TO FIO-CMND.             
           PERFORM COL-FIO-OPC.                               
           MOVE FIO-STAT TO PGM-STAT-OPC.
           IF NOT FIO-STAT-OKS                                
               GO TO EXI-FND-KEY.
      
           ADD 1 TO WSS-NUM-DEUD IN WSS-VARI.                                   

           PERFORM CRG-VEC-CDC.                                                 
                                                                                
           IF (SCR-OFRM-SRV AND WSS-J > WSS-NITMMAX)                            
              GO TO EXI-FND-KEY.                                                
                                                                                
           IF ((NOT SCR-OFRM-SRV) AND WSS-J > WSS-JMAX)                         
                 GO TO EXI-FND-KEY.                                             
                                                                                
       NXT-LEE-RDC.                                                             
           MOVE FIO-GET-NXT     TO FIO-CMND.  
           MOVE 'RDC-KEY-IDLC'  TO FIO-AKEY.                           
           GO TO LUP-LEE-RDC.    
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
       EXI-FND-KEY.
           PERFORM PUT-TAB.

           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *----------------------------------------------------------
       MSG-FND-KEY.
           MOVE 'COL' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM VAL-NUM-KEY-CDC
               PERFORM VAL-NUM-FLD-CDC
               PERFORM SCRL-TAB-CDC
               PERFORM SCR-TRL.
      *>>>>
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBVTAB.
       COPY GNSBVCOD.
       COPY COLBFOPC.
       COPY SGCBFDBC.

       PUT-TAB SECTION.
       INI-PUT-TAB.
           MOVE CDC-NOM-TSCDC     TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE CDC-NOM-TSCDC-MAS TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE 1 TO SCR-NUM-PANT.
           MOVE SPACES TO CDC-VEC-TS.                                      
           MOVE LOW-VALUES TO WSS-GLS-NCDC-IDX-FRM.
      *                                                                 
           MOVE 1 TO FRM-IFLD.
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.

           MOVE 1 TO WSS-F IN WSS-VARI.
       LUP-PUT-TAB.
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI
              GO TO FIN-PUT-TAB.
           IF WSS-NUM-DEUD-V(WSS-F) > ZEROES 
              PERFORM PUT-TAB-CDC
              PERFORM PUT-CDC-QUE
              PERFORM SET-FRM-IFLD
           ELSE
           IF FRM-IFLD = 1 AND WSS-F = 1
              MOVE 'COL' TO MSG-COD-SIST
              MOVE 'CDC    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE '00' TO FIO-STAT
      *       MOVE 'S' TO SCR-IERR                                      
              GO TO FIN-PUT-TAB
           ELSE
              MOVE '00' TO FIO-STAT
              GO TO FIN-PUT-TAB.

           ADD 1 TO WSS-F IN WSS-VARI.
           GO TO LUP-PUT-TAB.
       FIN-PUT-TAB.
           MOVE 1                  TO QUE-NITM.
           MOVE CDC-LEN-TSCDC-MAS  TO QUE-LITM.
           MOVE CDC-NOM-TSCDC-MAS  TO QUE-COLA.

           IF WSS-NITM > WSS-NITMMAX
              MOVE 'S'           TO CDC-MAS-IND-HMAS
           ELSE
              MOVE 'N'           TO CDC-MAS-IND-HMAS.

           MOVE WSS-NITM         TO WSS-NUM-ICUR.
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.
           MOVE WSS-NUM-ICUR-ALF TO CDC-MAS-NUM-TOCC.
           MOVE CDC-MAS-ITEM     TO QUE-ITEM.
           MOVE QUE-PUT          TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF WSS-NITM > WSS-IMAX
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'CDCTABDMAS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       EXT-PUT-TAB.
           EXIT.

       SET-FRM-IFLD SECTION.
       INI-SET-FRM-IFLD.
           IF SCR-OFRM-SRV
                MOVE 1 TO FRM-IFLD
           ELSE
                ADD 1 TO FRM-IFLD.
       FIN-SET-FRM-IFLD.
           EXIT.
      *--------------------------------------------
       SCRL-TAB-CDC SECTION.
       INI-SCRL-TAB-CDC.
           MOVE SPACES TO MSG-COD-MENS.

           IF FRM-FFLD = FRM-FFLD-PF6
               PERFORM PUT-TAB-ASC
           ELSE
               PERFORM PUT-TAB-DSC.
           IF MSG-COD-MENS > SPACES
               MOVE 'COL' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-CDC.
           EXIT.

      *--------------------------------------------
       PUT-TAB-CDC SECTION.
       INI-PUT-TAB-CDC.
           PERFORM CLR-WSS.
           MOVE WSS-NUM-DEUD-V(WSS-F) TO WSS-NUM-DEUD-CDC IN WSS-VARI.          
           MOVE WSS-NUM-ICLI-V(WSS-F) TO WSS-NUM-ICLI-CDC IN WSS-VARI.          
           MOVE WSS-VRF-ICLI-V(WSS-F) TO WSS-VRF-ICLI-CDC IN WSS-VARI.          
           MOVE WSS-IND-ICLI-V(WSS-F) TO WSS-IND-ICLI-CDC IN WSS-VARI.          
           MOVE WSS-GLS-ICLI-V(WSS-F) TO WSS-GLS-ICLI-CDC IN WSS-VARI.          
           MOVE WSS-VAL-LIMD-V(WSS-F) TO WSS-VAL-LIMD-CDC IN WSS-VARI.          

           IF SCR-OFRM-SRV           
              MOVE WSS-VAL-MONL-V-ALF(WSS-F)  
                                      TO WSS-VAL-MONL-ALF IN WSS-VARI
           ELSE
           MOVE WSS-VAL-MONL-V(WSS-F) TO WSS-VAL-MONL-CDC IN WSS-VARI.
           MOVE WSS-NUM-DIRE-V(WSS-F) TO WSS-NUM-DIRE-CDC IN WSS-VARI.          
           MOVE WSS-GLS-NOMC-V(WSS-F) TO WSS-GLS-NOMC-CDC IN WSS-VARI.          
                                                                                
           MOVE WSS-NUM-DEUD-CDC IN WSS-VARI TO WSS-NUM-DEUD-FRM.               
           MOVE WSS-NUM-ICLI-CDC IN WSS-VARI TO WSS-NUM-ICLI-FRM.               
           MOVE WSS-VRF-ICLI-CDC IN WSS-VARI TO WSS-VRF-ICLI-FRM.               
           MOVE WSS-IND-ICLI-CDC IN WSS-VARI TO WSS-IND-ICLI-FRM.               
           MOVE WSS-GLS-ICLI-CDC IN WSS-VARI TO WSS-GLS-ICLI-FRM.               
           MOVE WSS-VAL-LIMD-CDC IN WSS-VARI TO WSS-VAL-LIMD-FRM.               
           MOVE WSS-VAL-MONL-CDC IN WSS-VARI TO WSS-VAL-MONL-FRM.               
           MOVE WSS-NUM-DIRE-CDC IN WSS-VARI TO WSS-NUM-DIRE-FRM.               
           MOVE WSS-GLS-NOMC-CDC IN WSS-VARI TO WSS-GLS-NOMC-FRM.               
                                                                                
           IF FRM-IFLD > WSS-IMAX                                               
              GO TO FIN-PUT-TAB-CDC.                                            
                                                                                
           MOVE WSS-NUM-DEUD-V(WSS-F)      TO                                   
               FRM-NUM-DEUD IN CDC-FLD(FRM-IFLD).                               
           MOVE WSS-NUM-ICLI-V(WSS-F)      TO                                   
               FRM-NUM-ICLI IN CDC-FLD(FRM-IFLD).                               
           MOVE WSS-VRF-ICLI-V(WSS-F)      TO                                   
               FRM-VRF-ICLI IN CDC-FLD(FRM-IFLD).                               
           MOVE WSS-IND-ICLI-V(WSS-F)      TO                                   
               FRM-IND-ICLI IN CDC-FLD(FRM-IFLD).                               
           MOVE WSS-GLS-ICLI-V(WSS-F)      TO                                   
               FRM-GLS-ICLI IN CDC-FLD(FRM-IFLD).                               
           MOVE WSS-VAL-LIMD-V(WSS-F)      TO                                   
               FRM-VAL-LIMD IN CDC-FLD(FRM-IFLD).                               
           MOVE WSS-VAL-MONL-V(WSS-F)      TO
               FRM-VAL-MONL IN CDC-FLD(FRM-IFLD).
      *     COMPUTE FRM-VAL-MONL IN CDC-FLD(FRM-IFLD) =
      *        ( OPC-VAL-CRED IN OPC * RDC-VAL-LIMD IN RDC ) / 100.
           MOVE WSS-NUM-DIRE-V(WSS-F)      TO                                   
               FRM-NUM-DIRE IN CDC-FLD(FRM-IFLD).                               
           MOVE WSS-GLS-NOMC-V(WSS-F)      TO                                   
               FRM-GLS-NOMC IN CDC-FLD(FRM-IFLD). 
       FIN-PUT-TAB-CDC.
           EXIT.
      *----------------------------------------------------
       PUT-CDC-QUE SECTION.
       INI-PUT-CDC-QUE.
           PERFORM PRO-FLD-QUE.
           ADD 1 TO WSS-NITM.
           MOVE CDC-NOM-TSCDC    TO QUE-COLA.
           MOVE CDC-LEN-TSCDC    TO QUE-LITM.
           MOVE WSS-NITM         TO QUE-NITM.
           MOVE SPACES           TO QUE-ITEM.
           IF SCR-OFRM-SRV
              MOVE CDC-VEC-TS    TO QUE-ITEM                                   
           ELSE
              MOVE WSS-GLS-NCDC-IDX-FRM  TO QUE-ITEM.
           MOVE QUE-PUT  TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
       FIN-PUT-CDC-QUE.
           EXIT.
      *----------------------------------------------------

       PUT-TAB-ASC SECTION.
       INI-PUT-TAB-ASC.
           MOVE SPACES TO MSG-COD-MENS.
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.
           MOVE 0 TO FRM-IFLD.
       LUP-PUT-TAB-ASC.
           ADD 1              TO QUE-NITM.
           MOVE CDC-NOM-TSCDC TO QUE-COLA.
           MOVE CDC-LEN-TSCDC TO QUE-LITM.
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD = 0
                   MOVE 'CDCTABNONXT' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-ASC
               ELSE
                   GO TO FIN-PUT-TAB-ASC.
           ADD 1 TO FRM-IFLD.
           MOVE QUE-ITEM TO FRM-NUM-DEUD-IDX(FRM-IFLD).
           IF FRM-IFLD < WSS-IMAX
               GO TO LUP-PUT-TAB-ASC.
       FIN-PUT-TAB-ASC.
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX
               ADD 1 FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1
                                 UNTIL WSS-I > WSS-IMAX.
           IF FRM-IFLD > 0
               ADD 1 TO SCR-NUM-PANT.
           IF FRM-IFLD = WSS-IMAX
               ADD 1              TO QUE-NITM
               MOVE CDC-NOM-TSCDC TO QUE-COLA
               MOVE CDC-LEN-TSCDC TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT = QUE-STAT-OKS
                   MOVE 'COL'        TO MSG-COD-SIST
                   MOVE 'CDCTABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-ASC.
           EXIT.

       PUT-TAB-DSC SECTION.
       INI-PUT-TAB-DSC.
           MOVE SPACES TO MSG-COD-MENS.
           IF SCR-NUM-PANT = 1
               MOVE 'CDCTABNOPRV' TO MSG-COD-MENS
               GO TO EXT-PUT-TAB-DSC.

           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.
           ADD 1 TO QUE-NITM.
           ADD 1 WSS-IMAX GIVING FRM-IFLD.
       LUP-PUT-TAB-DSC.
           SUBTRACT  1 FROM QUE-NITM.
           MOVE CDC-NOM-TSCDC TO QUE-COLA
           MOVE CDC-LEN-TSCDC TO QUE-LITM
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD > WSS-IMAX
                   MOVE 'CDCTABNOPRV' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-DSC
               ELSE
                   GO TO FIN-PUT-TAB-DSC.
           SUBTRACT 1 FROM FRM-IFLD.
           MOVE QUE-ITEM TO FRM-NUM-DEUD-IDX(FRM-IFLD).
           IF FRM-IFLD > 1
               GO TO LUP-PUT-TAB-DSC.
       FIN-PUT-TAB-DSC.

           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1
                                 UNTIL WSS-I < 1.

           IF FRM-IFLD NOT > WSS-IMAX
               SUBTRACT 1 FROM SCR-NUM-PANT
               MOVE 'COL'        TO MSG-COD-SIST
               MOVE 'CDCTABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-DSC.
           EXIT.

       COPY GNSBGCPT.
       COPY GNSBGSTR.

       CLR-FRM SECTION.
       INI-CLR-FRM.
           MOVE ZEROES TO FRM-NUM-DEUD IN CDC-FLD(WSS-I).                       
           MOVE ZEROES TO FRM-NUM-ICLI IN CDC-FLD(WSS-I).                       
           MOVE SPACES TO FRM-VRF-ICLI IN CDC-FLD(WSS-I).                       
           MOVE SPACES TO FRM-IND-ICLI IN CDC-FLD(WSS-I).                       
           MOVE SPACES TO FRM-GLS-ICLI IN CDC-FLD(WSS-I).                       
           MOVE ZEROES TO FRM-VAL-LIMD IN CDC-FLD(WSS-I).                       
           MOVE ZEROES TO FRM-VAL-MONL IN CDC-FLD(WSS-I).                       
           MOVE ZEROES TO FRM-NUM-DIRE IN CDC-FLD(WSS-I).                       
           MOVE SPACES TO FRM-GLS-NOMC IN CDC-FLD(WSS-I).                       
       FIN-CLR-FRM.
           EXIT.

       CLR-WSS SECTION.
       INI-CLR-WSS.
           MOVE ZEROES TO WSS-NUM-DEUD-CDC IN WSS-VARI.                         
           MOVE ZEROES TO WSS-NUM-ICLI-CDC IN WSS-VARI.                         
           MOVE SPACES TO WSS-VRF-ICLI-CDC IN WSS-VARI.                         
           MOVE SPACES TO WSS-IND-ICLI-CDC IN WSS-VARI.                         
           MOVE SPACES TO WSS-GLS-ICLI-CDC IN WSS-VARI.                         
           MOVE ZEROES TO WSS-VAL-LIMD-CDC IN WSS-VARI.                         
           MOVE ZEROES TO WSS-VAL-MONL-CDC IN WSS-VARI.                         
           MOVE ZEROES TO WSS-NUM-DIRE-CDC IN WSS-VARI.                         
           MOVE SPACES TO WSS-GLS-NOMC-CDC IN WSS-VARI.                         

           IF SCR-OFRM-SRV
               PERFORM CLR-FRM-MTX.
       FIN-CLR-WSS.
           EXIT.

       CLR-FRM-MTX SECTION.
       INI-CLR-FRM-MTX.
           MOVE ZEROES TO FRM-NUM-DEUD IN CDC-FLD(1).                           
           MOVE ZEROES TO FRM-NUM-ICLI IN CDC-FLD(1).                           
           MOVE SPACES TO FRM-VRF-ICLI IN CDC-FLD(1).                           
           MOVE SPACES TO FRM-IND-ICLI IN CDC-FLD(1).                           
           MOVE SPACES TO FRM-GLS-ICLI IN CDC-FLD(1).                           
           MOVE ZEROES TO FRM-VAL-LIMD IN CDC-FLD(1).                           
           MOVE ZEROES TO FRM-VAL-MONL IN CDC-FLD(1).                           
           MOVE ZEROES TO FRM-NUM-DIRE IN CDC-FLD(1).                           
           MOVE SPACES TO FRM-GLS-NOMC IN CDC-FLD(1).                           
       FIN-CLR-FRM-MTX.
           EXIT.

       CRG-VEC-CDC SECTION.
       INI-CRG-VEC-CDC.
           ADD 1 TO WSS-J.
           IF WSS-J IN WSS-VARI > WSS-JMAX IN WSS-VARI
               GO TO FIN-CRG-VEC-CDC.

           PERFORM CLR-CDC-VEC.
           MOVE WSS-NUM-DEUD IN WSS-VARI TO                                     
                             WSS-NUM-DEUD-V IN WSS-VARI(WSS-J).                 
           IF SCR-OFRM-SRV
               MOVE RDC-VAL-LIMD IN RDC TO
                        FRM-VAL-LIMD-EDT IN CDC-EDT
               MOVE FRM-VAL-LIMD-EDT IN CDC-EDT TO
                        WSS-VAL-LIMD-V-ALF IN WSS-VARI(WSS-J)
           ELSE
               MOVE RDC-VAL-LIMD IN RDC TO
                             WSS-VAL-LIMD-V IN WSS-VARI(WSS-J).                 
           COMPUTE WSS-VAL-MONL IN WSS-VARI =
              ( OPC-VAL-CRED IN OPC * RDC-VAL-LIMD IN RDC ) / 100.
           IF SCR-OFRM-SRV
               MOVE WSS-VAL-MONL   IN WSS-VARI        TO
                         FRM-VAL-MONL-EDT IN CDC-EDT
               MOVE FRM-VAL-MONL-EDT IN CDC-EDT TO
                  WSS-VAL-MONL-V-ALF IN WSS-VARI(WSS-J)
           ELSE
               MOVE WSS-VAL-MONL IN WSS-VARI TO
                   WSS-VAL-MONL-V IN WSS-VARI(WSS-J).
           MOVE RDC-NUM-DIRE IN RDC TO                                          
                             WSS-NUM-DIRE-V IN WSS-VARI(WSS-J).                 
                                                                                
           MOVE RDC-CAI-ICLI IN RDC TO DBC-CAI-ICLI IN DBC.                     
           MOVE RDC-IIC-ICLI IN RDC TO DBC-IIC-ICLI IN DBC.                     
           MOVE FIO-GET-KEY TO FIO-CMND.                                        
           PERFORM SGC-FIO-DBC.                                                 
           IF NOT FIO-STAT-OKS                                                  
               MOVE SPACES TO WSS-NUM-ICLI-V IN WSS-VARI(WSS-J)                 
               MOVE SPACES TO WSS-VRF-ICLI-V IN WSS-VARI(WSS-J)                 
               MOVE 'NO EXISTE CLIENTE ' TO                                     
                              WSS-GLS-NOMC-V IN WSS-VARI(WSS-J)                 
           ELSE                                                                 
               MOVE DBC-NUM-ICLI IN DBC TO  
                              WSS-NUM-ICLI-V IN WSS-VARI(WSS-J) 
               MOVE DBC-VRF-ICLI IN DBC TO                      
                              WSS-VRF-ICLI-V IN WSS-VARI(WSS-J) 
               MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN             
               PERFORM CPT-BLKS                                 
