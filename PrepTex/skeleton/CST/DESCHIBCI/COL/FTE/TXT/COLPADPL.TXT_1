*%! Codigo adicional para archivo COLPQDPL
*% NO_TIENE_TIMESTAMP = FALSE
*% NO_HDR_CST = FALSE
*% LARGO_TOTAL_COMMAREA = 3000
*% FMT_CHI = TRUE
*% GEN_BCI = TRUE
*% PGM_PTC = TRUE
*% LARGO_MI_COMMAREA = 0
*% IF SETTAG
*% VSNPQ = "06.02.24.1"
*% VSNPU = "06.02.24.1"
*% FNCPQ = "CMC-CONVENIO"
*% FNCPU = "CMC-CONVENIO"
*% END
*%! Codigo adicional para archivo
*% IF MI_COMMAREA                                                 
      *<<<< MI_COMMAREA                                                 
           03 QRE-VARI.                                                 
              05 SCR-IMAX      COMP                PIC S9(04).
              05 SCR-I         COMP                PIC S9(04).       
              05 SCR-CIC-IREC.
                 07 SCR-CAI-IREC                   PIC X(04).
                 07 SCR-IIC-IREC                   PIC X(08).               
              05 SCR-CIC-ICLI.                                          
                 07 SCR-CAI-ICLI                   PIC X(04).           
                 07 SCR-IIC-ICLI                   PIC X(08).           
              05 WSS-COD-MENN     VALUE SPACES     PIC X(12).
              05 WSS-COD-MENY     VALUE SPACES     PIC X(12).
      *>>>>
*% END
*% IF WSS                                                         
      *<<<< WSS                                                         
       COPY SGCBRDBC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
       COPY COLBRVEN.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-VEN-REQA==.
       COPY COLBROPC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OPC-REQA==.
       COPY COLBRRDC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RDC-REQA==.
       COPY COLBRPLL.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-PLL-REQA==.        
       COPY GNSBRCIC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CIC-REQA==.
       COPY GNSWGCIC.
       COPY GNSWGCPT.

       01  WSS-VARI.
           03  WSS-GLS-DATA.
               05  FILLER                           PIC X(01).
               05  WSS-COD-DAT2                     PIC X(01).
               05  FILLER                           PIC X(58).
*% IF PGM_PU
      *MHM-INI 06-NOV-2008
               05 WSS-IND-EXVN    VALUE SPACES      PIC X(01).

      *MHM-FIN 06-NOV-2008
*% END
      *>>>>
*% END
*% IF CMN_MOD
      *<<<< CMN_MOD
           MOVE 'COL'               TO TAB-COD-SIST.
           MOVE 'EDP'               TO TAB-COD-TTAB IN TAB.
           MOVE DPL-COD-ESTA IN DPL TO TAB-COD-CTAB IN TAB.
           PERFORM BUS-TAB.
           MOVE TAB-GLS-DATA IN TAB TO WSS-GLS-DATA IN WSS-VARI.
           IF WSS-COD-DAT2 IN WSS-VARI = 'S'
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'DPLEST NOVLD' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC   TO FRM-MENS
               GO TO FIN-CMN-MOD.
           IF SCR-CMND = 'CDR'
              MOVE '004' TO DPL-COD-ESTA IN DPL.
      *>>>>
*% END
*% IF CMN_ACC
      *<<<< CMN_ACC
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM COL-FIO-DPL.
           IF NOT FIO-STAT-OKS
               MOVE 'S' TO SCR-IERR
               MOVE 'COL' TO MSG-COD-SIST
               MOVE WSS-COD-MENN TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE 'COL' TO MSG-COD-SIST
               MOVE WSS-COD-MENY  TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN1.
      *>>>>
*% END
*% IF FND_KEY
      *<<<< FND_KEY
           IF SCR-CCPP = 'ING'
               MOVE '000'   TO DPL-COD-NOVD IN DPL-FLD
               MOVE '001'   TO DPL-COD-ESTA IN DPL-FLD
               PERFORM BUS-COR-DPL
               GO TO CON-FND-KEY.
           IF DPL-NUM-IDPL IN DPL NOT > ZEROES
               IF NOT SCR-OFRM-SRV
                  PERFORM BUS-COR-DPL.
       CON-FND-KEY.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY                                                 
           IF FIO-STAT-OKS AND                                          
              DPL-COD-NOVD IN DPL = '003' AND                           
              SCR-CMND NOT = 'CON'                                      
               MOVE 'COL' TO MSG-COD-SIST                               
               MOVE 'DPL    CERR' TO MSG-COD-MENS                       
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE '00' TO FIO-STAT                                    
               MOVE SCR-STAT-CON TO SCR-STAT                            
               MOVE PGM-STAT-DPL TO FIO-STAT                            
               GO TO FIN-FND-KEY.                                       
      *>>>>                                                             
*% END
*% IF EOF
      *<<<< EOF
       BUS-COR-DPL SECTION. 
       INI-BUS-COR-DPL. 
           MOVE 99999 TO DPL-NUM-IDPL IN DPL. 
           MOVE FIO-GET-LEQ TO FIO-CMND. 
           PERFORM COL-FIO-DPL. 
           IF NOT ( FIO-STAT-OKS AND 
                    DPL-CAI-IPLL IN DPL-FLD = DPL-CAI-IPLL IN DPL AND 
                    DPL-IIC-IPLL IN DPL-FLD = DPL-IIC-IPLL IN DPL AND 
                    DPL-CAI-ICVN IN DPL-FLD = DPL-CAI-ICVN IN DPL AND 
                    DPL-IIC-ICVN IN DPL-FLD = DPL-IIC-ICVN IN DPL AND 
                    DPL-NUM-IPLL IN DPL-FLD = DPL-NUM-IPLL IN DPL )
               PERFORM KEY-ALL-DPL 
               MOVE ZEROES TO DPL-NUM-IDPL IN DPL. 

           IF SCR-CCPP = 'ING' 
               ADD 1 TO DPL-NUM-IDPL IN DPL 
               MOVE DPL-NUM-IDPL IN DPL TO DPL-NUM-IDPL IN DPL-FLD. 
       FIN-BUS-COR-DPL. 
           EXIT.

       COPY SGCBFDBC.
       COPY COLBFOPC.
       COPY COLBFVEN.
       COPY COLBFRDC.
       COPY COLBFPLL.                                                   
       COPY GNSBFCIC.

       COPY GNSBBTAB.
       COPY GNSBVTAB.
       COPY GNSBVCOD.
       COPY GNSBGCIC.
       COPY GNSBGCPT.

      *>>>>
*% END
