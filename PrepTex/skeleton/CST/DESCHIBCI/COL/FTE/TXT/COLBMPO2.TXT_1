*% IF VAL_CON_FLD_PO2
      *<<<< VAL_CON_FLD_PO2
      * Valida Consistencias entre Campos.
           IF ( POC-IND-MINC IN PO2-FLD NOT = SPACES AND
                POC-NUM-MINC IN PO2-FLD = ZEROES ) OR
              ( POC-IND-MINC IN PO2-FLD = SPACES AND
                POC-NUM-MINC IN PO2-FLD NOT = ZEROES )
                  MOVE 'COL' TO MSG-COD-SIST
                  MOVE 'INDMINCMINC' TO MSG-COD-MENS
                  PERFORM GET-MSG
      *           STRING MSG-GLS-DESC
      *                  MSG-GLS-DATA DELIMITED BY SIZE
      *           INTO FRM-MENS

                  MOVE MSG-GLS-DESC TO CPT-STR1
                  MOVE MSG-GLS-DATA TO CPT-STR2
                  PERFORM CPT-BLKS
                  MOVE CPT-STRN     TO FRM-MENS

                  MOVE FRM-SUAR-MAL TO FRM-SUAR
                  GO TO FIN-VAL-CON-FLD-PO2.
           IF ( POC-IND-MAXC IN PO2-FLD NOT = SPACES AND
                POC-NUM-MAXC IN PO2-FLD = ZEROES ) OR
              ( POC-IND-MAXC IN PO2-FLD = SPACES AND
                POC-NUM-MAXC IN PO2-FLD NOT = ZEROES )
                  MOVE 'COL' TO MSG-COD-SIST
                  MOVE 'INDMAXCMAXC' TO MSG-COD-MENS
                  PERFORM GET-MSG

      *           STRING MSG-GLS-DESC
      *                  MSG-GLS-DATA DELIMITED BY SIZE
      *           INTO FRM-MENS

                  MOVE MSG-GLS-DESC TO CPT-STR1
                  MOVE MSG-GLS-DATA TO CPT-STR2
                  PERFORM CPT-BLKS
                  MOVE CPT-STRN     TO FRM-MENS

                  MOVE FRM-SUAR-MAL TO FRM-SUAR
                  GO TO FIN-VAL-CON-FLD-PO2.
           IF ( POC-NUM-MINC IN PO2-FLD > ZEROES AND
                POC-NUM-MAXC IN PO2-FLD > ZEROES )
               MOVE POC-IND-MINC IN PO2-FLD TO MSC-COD-CMSC IN MSC
               MOVE POC-NUM-MINC IN PO2-FLD TO
               WSS-NUM-NFEC IN WSS-FEC-VARI
               PERFORM SUM-FEC-NFEC
               MOVE FEC-FECH TO WSS-FEC-NFE1
               MOVE POC-IND-MAXC IN PO2-FLD TO MSC-COD-CMSC IN MSC
               MOVE POC-NUM-MAXC IN PO2-FLD TO
               WSS-NUM-NFEC IN WSS-FEC-VARI
               PERFORM SUM-FEC-NFEC
               MOVE FEC-FECH TO WSS-FEC-NFE2
               PERFORM CMP-FEC-NFEC
               IF FRM-SUAR = FRM-SUAR-MAL
                   MOVE 'COL' TO MSG-COD-SIST
                   MOVE 'NUMMINCNORG' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MEN1
                   GO TO FIN-VAL-CON-FLD-PO2.
           IF ( POC-IND-MINI IN PO2-FLD NOT = SPACES AND
                POC-NUM-MINI IN PO2-FLD = ZEROES ) OR
              ( POC-IND-MINI IN PO2-FLD = SPACES AND
                POC-NUM-MINI IN PO2-FLD NOT = ZEROES )
                  MOVE 'COL' TO MSG-COD-SIST
                  MOVE 'INDMINIMINI' TO MSG-COD-MENS
                  PERFORM GET-MSG

      *           STRING MSG-GLS-DESC
      *                  MSG-GLS-DATA DELIMITED BY SIZE
      *           INTO FRM-MENS

                  MOVE MSG-GLS-DESC TO CPT-STR1
                  MOVE MSG-GLS-DATA TO CPT-STR2
                  PERFORM CPT-BLKS
                  MOVE CPT-STRN     TO FRM-MENS

                  MOVE FRM-SUAR-MAL TO FRM-SUAR
                  GO TO FIN-VAL-CON-FLD-PO2.
           IF ( POC-IND-MAXI IN PO2-FLD NOT = SPACES AND
                POC-NUM-MAXI IN PO2-FLD = ZEROES ) OR
              ( POC-IND-MAXI IN PO2-FLD = SPACES AND
                POC-NUM-MAXI IN PO2-FLD NOT = ZEROES )
                  MOVE 'COL' TO MSG-COD-SIST
                  MOVE 'INDMAXIMAXI' TO MSG-COD-MENS
                  PERFORM GET-MSG

      *           STRING MSG-GLS-DESC
      *                  MSG-GLS-DATA DELIMITED BY SIZE
      *           INTO FRM-MENS

                  MOVE MSG-GLS-DESC TO CPT-STR1
                  MOVE MSG-GLS-DATA TO CPT-STR2
                  PERFORM CPT-BLKS
                  MOVE CPT-STRN     TO FRM-MENS

                  MOVE FRM-SUAR-MAL TO FRM-SUAR
                  GO TO FIN-VAL-CON-FLD-PO2.
           IF ( POC-NUM-MINI IN PO2-FLD > ZEROES AND
                POC-NUM-MAXI IN PO2-FLD > ZEROES )
               MOVE POC-IND-MINI IN PO2-FLD TO MSC-COD-CMSC IN MSC
               MOVE POC-NUM-MINI IN PO2-FLD TO
               WSS-NUM-NFEC IN WSS-FEC-VARI
               PERFORM SUM-FEC-NFEC
               MOVE FEC-FECH TO WSS-FEC-NFE1
               MOVE POC-IND-MAXI IN PO2-FLD TO MSC-COD-CMSC IN MSC
               MOVE POC-NUM-MAXI IN PO2-FLD TO
               WSS-NUM-NFEC IN WSS-FEC-VARI
               PERFORM SUM-FEC-NFEC
               MOVE FEC-FECH TO WSS-FEC-NFE2
               PERFORM CMP-FEC-NFEC
               IF FRM-SUAR = FRM-SUAR-MAL
                   MOVE 'COL' TO MSG-COD-SIST
                   MOVE 'NUMMININORG' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MEN1
                   GO TO FIN-VAL-CON-FLD-PO2.
      *>>>>
*% END
