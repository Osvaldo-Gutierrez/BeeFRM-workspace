*%! Codigo Adicional para programa
*% HDR_CST = FALSE
*% NO_TIENE_TIMESTAMP = TRUE
*% IF WSS
      *<<<< WSS
       COPY TABBROFI.
       COPY GNSWGHOY.
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OFI-REQA==.
      *>>>>
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-INST = SCR-INST-HKY
               MOVE 'COL' TO TAB-COD-SIST
               MOVE 'EJC' TO TAB-COD-TTAB IN TAB
               PERFORM ERR-TAB.
      *>>>>
*% END
*% IF FND_KEY
      *<<<< FND_KEY
      * Busca datos en registro USR.
           MOVE 'EJC'                   TO USR-COD-TTAB IN USR.
           MOVE EJC-COD-EJEC IN EJC-FLD TO USR-COD-USER IN USR.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM TAB-FIO-USR.
           MOVE FIO-STAT TO PGM-STAT-USR.
           PERFORM PUT-USR-EJC.
      *BUS-TAB busca glosa de codigo en tabla
           IF USR-COD-UBIC IN EJC-FLD > SPACES
               MOVE USR-COD-UBIC IN EJC-FLD TO USR-COD-UBIC IN USR
               IF USR-IND-UBIC IN USR = 'O'
                   MOVE USR-COD-OFIC IN USR TO OFI-COD-OFIC IN OFI
                   PERFORM BUS-OFI
                   MOVE OFI-GLS-DESC IN OFI TO FRM-GLS-UBIC IN EJC-FLD
               ELSE
                   MOVE 'TAB' TO TAB-COD-SIST
                   MOVE 'UBI' TO TAB-COD-TTAB IN TAB
                   MOVE USR-COD-UBIC IN EJC-FLD TO TAB-COD-CTAB IN TAB
                   PERFORM BUS-TAB
                   MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-UBIC IN EJC-FLD.
      *>>>>
*% END
*% IF INI_CMN_ING
      *<<<< INI_CMN_ING
*% IF PGM_PU
           MOVE ZEROES TO EJC-FEC-FTRN IN EJC.
           MOVE ZEROES TO EJC-HRA-HRTR IN EJC.
           MOVE ZEROES TO EJC-VAL-CMAX IN EJC.
           MOVE ZEROES TO EJC-VAL-DMAX IN EJC.
*% END
      *>>>>
*% END
*% IF CMN_ING OR CMN_MOD
      *>>>>
           PERFORM GET-FHOY.
           MOVE HOY-DHOY TO EJC-NUM-DTRN IN EJC.
           MOVE HOY-MHOY TO EJC-NUM-MTRN IN EJC.
           MOVE HOY-SHOY TO EJC-NUM-STRN IN EJC.
           MOVE HOY-AHOY TO EJC-NUM-ATRN IN EJC.
      *<<<< 
*% END 
*% IF EOF
      *<<<< EOF
       COPY TABBFOFI.
       COPY TABBBOFI.
       COPY GNSBEGLS.
       COPY GNSBETAB.
       COPY GNSBBMSC.
       COPY TABBVTDU.
       COPY GNSBGHOY.
      *>>>>
*% END
*% IF FIN_HBL_KEY
           MOVE '1' TO FRM-CPIM-AEY.
*% END
*% IF FIN_HBL_IKY
           MOVE '1' TO FRM-CPIM-AEY.
*% END
