*% IF FIN_CLR_KEY_CFN
      *<<<< FIN_CLR_KEY_CFN                                             
           PERFORM GET-FHOY.                                            
           MOVE HOY-DHOY TO FRM-NUM-DPRO IN CFN-FLD.                    
           MOVE HOY-MHOY TO FRM-NUM-MPRO IN CFN-FLD.                    
           MOVE HOY-SHOY TO FRM-NUM-SPRO IN CFN-FLD.                    
           MOVE HOY-AHOY TO FRM-NUM-APRO IN CFN-FLD.                    
      *>>>>                                                             
*% END
*% IF FIN_FRM_MSC_UTPZ
      *<<<< FRM_MSC_UTPZ                                                
           IF FRM-MSC-UTPZ IN CFN-FLD NOT > SPACES                      
              GO TO FIN-VAL-FRM-MSC-UTPZ.                               
                                                                        
           MOVE 'TAB'  TO MSC-COD-SIST.                                 
           MOVE 'IPER' TO MSC-COD-TMSC IN MSC.                          
           MOVE FRM-MSC-UTPZ IN CFN-FLD TO MSC-COD-CMSC IN MSC.         
           PERFORM VAL-IND.                                             
           IF MSG-COD-MENS = 'COD    NEX' OR                            
              FRM-MSC-UTPZ IN CFN-FLD NOT > SPACES                      
               MOVE SPACES TO FRM-GLS-UTPZ IN CFN-FLD                   
           ELSE                                                         
               MOVE MSC-GLS-DCOR IN MSC TO                              
                    FRM-GLS-UTPZ IN CFN-FLD.                            
                                                                        
           IF FRM-MSC-UTPZ IN CFN-FLD = 'D'                             
              MOVE FRM-NUM-PLZO IN CFN-FLD TO OCI-NUM-PLZD.             
           IF FRM-MSC-UTPZ IN CFN-FLD = 'A'                             
              MULTIPLY FRM-NUM-PLZO IN CFN-FLD BY 360 GIVING            
                                               OCI-NUM-PLZD.            
           IF FRM-MSC-UTPZ IN CFN-FLD = 'M'                             
              MULTIPLY FRM-NUM-PLZO IN CFN-FLD BY 30 GIVING             
                                               OCI-NUM-PLZD.            
      *>>>>                                                             
*% END
*% IF FIN_FRM_COD_VCOC
      *<<<< FRM_COD_VCOC                                                
           IF FRM-COD-VCOC IN CFN-FLD NOT > SPACES                      
               GO TO FIN-VAL-FRM-COD-VCOC.                              
                                                                        
           MOVE 'TAB'                   TO TAB-COD-SIST.                
           MOVE 'VLR'                   TO TAB-COD-TTAB IN TAB.         
           MOVE FRM-COD-VCOC IN CFN-FLD TO TAB-COD-CTAB IN TAB.         
           PERFORM VAL-COD.                                             
           IF MSG-COD-MENS NOT > SPACES                                 
               MOVE TAB-GLS-DESC TO FRM-GLS-VCOC IN CFN-FLD             
           ELSE                                                         
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE FRM-SUAR-MAL TO FRM-SUAR                            
               GO TO FIN-VAL-FRM-COD-VCOC.                              
      *>>>>                                                             
*% END
*% IF FIN_FRM_COD_TOOC
      *<<<< FRM_COD_TOOC                                                
           IF FRM-COD-TOOC IN CFN-FLD NOT > SPACES                      
               GO TO FIN-VAL-FRM-COD-TOOC.                              
                                                                        
           MOVE 'TAB'                   TO TAB-COD-SIST.                
           MOVE 'TIO'                   TO TAB-COD-TTAB IN TAB.         
           MOVE FRM-COD-TOOC IN CFN-FLD TO TAB-COD-CTAB IN TAB.         
           PERFORM VAL-COD.                                             
           IF MSG-COD-MENS NOT > SPACES                                 
               MOVE TAB-GLS-DESC TO FRM-GLS-TOOC IN CFN-FLD             
           ELSE                                                         
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE FRM-SUAR-MAL TO FRM-SUAR                            
               GO TO FIN-VAL-FRM-COD-TOOC.                              
      *>>>>                                                             
*% END
*% IF FIN_FRM_COD_AOOC
      *<<<< FRM_COD_AOOC                                                
           IF FRM-COD-AOOC IN CFN-FLD NOT > SPACES                      
               GO TO FIN-VAL-FRM-COD-AOOC.                              
                                                                        
           MOVE 'TAB'                    TO TAB-COD-SIST.               
           MOVE 'AUX'                    TO TAB-COD-TTAB IN TAB.        
           MOVE FRM-COD-TOOC IN CFN-FLD  TO WSS-COD-TOOC IN WSS-VARI.   
           MOVE FRM-COD-AOOC IN CFN-FLD  TO WSS-COD-AOOC IN WSS-VARI.   
           MOVE WSS-COD-CAUX IN WSS-VARI TO TAB-COD-CTAB IN TAB.        
           PERFORM VAL-COD.                                             
           IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'               
               MOVE SPACES       TO FRM-GLS-AOOC IN CFN-FLD             
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE FRM-SUAR-MAL TO FRM-SUAR                            
           ELSE                                                         
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-AOOC IN CFN-FLD.     
      *>>>>                                                             
*% END
*% IF FIN_FRM_NUM_VCOR
      *<<<< FRM_NUM_VCOR                                                
           IF FRM-NUM-VCOR IN CFN-FLD NOT > ZEROES                      
              GO TO FIN-VAL-FRM-NUM-VCOR.                               
      *>>>>                                                             
*% END
