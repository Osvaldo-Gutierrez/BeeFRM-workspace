*%!Codigo Adicional
*% IF FIN_SCR_VARI
      *<<<<FIN_SCR_VARI
       COPY SGCBRDBC.
       COPY GNSWGCPT.
       COPY COLBRRDC.
       COPY GNSWGHOY.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RDC-REQA==.
      *>>>>
*% END
*% IF WSS
      *<<<<WSS
       01  WSS-VARI.
           03 WSS-GNS             VALUE 'GNS'       PIC X(03).
           03 WSS-GNSACC VALUE 'GNSACCSSIMUL'       PIC X(12).
           03 WSS-S                 VALUE 'S'       PIC X(01).
           03 WSS-23               VALUE '23'       PIC X(02).
           03 WSS-COL             VALUE 'COL'       PIC X(03).
           03 WSS-ING             VALUE 'ING'       PIC X(03).
           03 WSS-CON             VALUE 'CON'       PIC X(03).
           03 WSS-COO             VALUE 'COO'       PIC X(03).
           03 WSS-CIOREG VALUE 'CIOREGEX    '       PIC X(12).
           03 WSS-OPCNO  VALUE 'OPC  NORDC  '       PIC X(12).
           03 WSS-RDCNO  VALUE 'RDC  NODBC  '       PIC X(12).
           03 WSS-RDCKEY VALUE 'RDC-KEY-IDLC'       PIC X(12).
           03 WSS-PPCAUX VALUE 'PPCAUX  INEX'       PIC X(12).
      *>>>>
*% END
*% IF INI_CMN_ELI
      *<<<<INI_CMN_ELI
           PERFORM KEY-ALL-CIO.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM COL-FIO-OPC.
           IF FIO-STAT-OKS AND
              OPC-STP-ITRN IN OPC NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM COL-FIO-OPC
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM COL-FIO-OPC
               MOVE WSS-GNS TO MSG-COD-SIST
               MOVE WSS-GNSACC TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-ELI.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ELI.
           MOVE SPACES TO OPC-CAI-ICEN IN OPC.
           MOVE ZEROES TO OPC-IIC-ICEN IN OPC.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM COL-FIO-OPC.
           GO TO FIN-CMN-ELI.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<<FIN_FND_KEY
           IF SCR-CMND = WSS-ING AND OPC-CAI-ICEN IN OPC > SPACES
               MOVE WSS-COL         TO MSG-COD-SIST
               MOVE WSS-CIOREG      TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE WSS-23 TO FIO-STAT
               MOVE MSG-GLS-DESC        TO FRM-MEN1
               MOVE FRM-SUAR-MAL        TO FRM-SUAR
               GO TO FIN-FND-KEY.
           IF SCR-CMND = WSS-CON
              GO TO FIN-FND-KEY.
           MOVE WSS-COL              TO TAB-COD-SIST FIO-SIST.
           MOVE WSS-COO              TO TAB-COD-TTAB IN TAB.          
           MOVE OPC-COD-COOC IN OPC      TO TAB-COD-CTAB IN TAB.          
           MOVE FIO-GET-KEY              TO FIO-CMND.                     
           PERFORM GNS-FIO-TAB.                                           
           IF NOT (FIO-STAT-OKS AND TAB-IND-VIGE IN TAB = WSS-S)
              MOVE WSS-COL         TO MSG-COD-SIST
              MOVE WSS-PPCAUX      TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE WSS-23 TO FIO-STAT
              MOVE MSG-GLS-DESC        TO FRM-MEN1
              MOVE FRM-SUAR-MAL        TO FRM-SUAR
              GO TO FIN-FND-KEY.
      *>>>>
*% END 
*% IF EOF
      *<<<<EOF
       COPY GNSBGCPT.
       COPY SGCBFDBC.
       COPY COLBFRDC.
       COPY GNSBGHOY.
      *>>>>
*% END 
