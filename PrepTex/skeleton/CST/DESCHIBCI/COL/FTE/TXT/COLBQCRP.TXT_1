*%! Codigo adicional
*% IF SETTAG
*% VSNBQ = "06.08.22.1"
*% FNCBQ = "CNCMSV"
*% END
*% IF FIN_FST_KEY_CRP
      *<<<< FIN_FST_KEY_CRP
           MOVE -1 TO PLL-CAI-IPLL-LEN IN CRP-FLD.
      *>>>>
*% END
*% IF PLL_MSC_STAT
      *<<<< PLL_MSC_STAT
           IF PLL-MSC-STAT IN CRP-FLD NOT > SPACES
              GO TO FIN-VAL-PLL-MSC-STAT.

      *VAL-COD Valida codigo de tabla
           IF PLL-MSC-STAT IN CRP-FLD > SPACES
              MOVE 'COL'                   TO TAB-COD-SIST
              MOVE 'EPL'                   TO TAB-COD-TTAB IN TAB
              MOVE PLL-MSC-STAT IN CRP-FLD TO TAB-COD-CTAB IN TAB
              PERFORM VAL-COD
              IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'
                 MOVE SPACES       TO FRM-GLS-STAT IN CRP-FLD
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 MOVE FRM-SUAR-MAL TO FRM-SUAR
              ELSE
                 MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-STAT IN CRP-FLD.
      *>>>>
*% END
*% IF INI_VAL_CON_KEY_CRP
      *<<<< INI_VAL_CON_KEY_CRP
           GO TO FIN-VAL-CON-KEY-CRP.
      *>>>>
*% END
*% IF INI_VAL_CON_IKY_CRP
      *<<<< INI_VAL_CON_IKY_CRP
           GO TO FIN-VAL-CON-IKY-CRP.
      *>>>>
*% END
*% IF INI_FRM_FEC_FVTR
      *<<<<
           MOVE FEC-CHOY-N TO FEC-CHOY.
      *>>>>
*% END
*% IF INI_FRM_FEC_FPIN
      *<<<<
           MOVE FEC-CHOY-N TO FEC-CHOY.
      *>>>>
*% END
*% IF INI_FRM_FEC_FPTR
      *<<<<
           MOVE FEC-CHOY-N TO FEC-CHOY.
      *>>>>
*% END
*% IF FIN_PLL_COD_SITU
      *<<<<
           IF PLL-COD-SITU IN CRP-FLD NOT > SPACES                      
              GO TO FIN-VAL-PLL-COD-SITU.                               
           MOVE 'COL'                   TO TAB-COD-SIST.                
           MOVE 'SIP'                   TO TAB-COD-TTAB IN TAB.         
           MOVE PLL-COD-SITU IN CRP-FLD TO TAB-COD-CTAB IN TAB.         
           PERFORM VAL-COD.                                             
           IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'               
              MOVE SPACES       TO FRM-GLS-SITU IN CRP-FLD              
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              MOVE FRM-SUAR-MAL TO FRM-SUAR                             
           ELSE                                                         
              MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-SITU IN CRP-FLD.      
      *>>>>
*% END
*% IF INI_FRM_FEC_FVIN
      *<<<<
           MOVE FEC-CHOY-N TO FEC-CHOY.
      *>>>>
*% END
*% IF FIN_PLL_IND_FPAG
      *<<<<
      *VAL-COD Valida codigo de tabla                                   
           IF PLL-IND-FPAG IN CRP-FLD > SPACES                          
              MOVE 'COL'                   TO TAB-COD-SIST              
              MOVE 'PAC'                   TO TAB-COD-TTAB IN TAB       
              MOVE PLL-IND-FPAG IN CRP-FLD TO TAB-COD-CTAB IN TAB       
              PERFORM VAL-COD                                           
              IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'            
                 MOVE SPACES       TO FRM-GLS-STAT IN CRP-FLD           
                 MOVE MSG-GLS-DESC TO FRM-MENS                          
                 MOVE FRM-SUAR-MAL TO FRM-SUAR                          
              ELSE                                                      
                 MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-FPAG IN CRP-FLD.   
      *>>>>
*% END
*% IF FIN_PLL_IND_TCAR
      *<<<<
           IF PLL-IND-TCAR IN CRP-FLD NOT > SPACES                      
              GO TO FIN-VAL-PLL-IND-TCAR.                               
      *VAL-COD Valida codigo de tabla                                   
           IF PLL-IND-TCAR IN CRP-FLD > SPACES                          
              MOVE 'TAB'                   TO MSC-COD-SIST              
              MOVE 'TCYA'                  TO MSC-COD-TMSC IN MSC       
              MOVE PLL-IND-TCAR IN CRP-FLD TO MSC-COD-CMSC IN MSC       
              PERFORM VAL-IND                                           
              IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'            
                 MOVE MSG-GLS-DESC TO FRM-MENS                          
                 MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>>
*% END
*% IF FIN_DBC_GLS_ICLI 
      *<<<< DBC_GLS_ICLI
           IF DBC-NUM-ICLI IN CRP-FLD > ZEROES
              MOVE DBC-NUM-ICLI IN CRP-FLD TO DBC-NUM-ICLI IN DBC
              MOVE DBC-IND-ICLI IN CRP-FLD TO DBC-IND-ICLI IN DBC
              MOVE DBC-GLS-ICLI IN CRP-FLD TO DBC-GLS-ICLI IN DBC
              MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY
              MOVE FIO-GET-KEY TO FIO-CMND
              PERFORM SGC-FIO-DBC
              IF NOT FIO-STAT-OKS
                  MOVE 'SGC'        TO MSG-COD-SIST
                  MOVE 'CLI    NEX' TO MSG-COD-MENS
                  PERFORM GET-MSG
                  MOVE MSG-GLS-DESC TO FRM-MENS
                  MOVE FRM-SUAR-MAL TO FRM-SUAR
                  GO TO FIN-VAL-DBC-GLS-ICLI
              ELSE
                  MOVE DBC-CIC-ICLI IN DBC TO SCR-CIC-ICLI
           ELSE
              MOVE SPACES TO SCR-CIC-ICLI.
      *>>>>
*% END
*% IF FIN_PUT_PLL_CRP
      *<<<<
           IF PLL-MSC-STAT IN CRP-FLD > SPACES                          
              MOVE 'COL'                   TO TAB-COD-SIST              
              MOVE 'EPL'                   TO TAB-COD-TTAB IN TAB       
              MOVE PLL-MSC-STAT IN CRP-FLD TO TAB-COD-CTAB IN TAB       
              PERFORM BUS-TAB                                           
              MOVE TAB-GLS-DESC IN TAB     TO FRM-GLS-STAT IN CRP-FLD   
           ELSE                                                         
              MOVE SPACES                  TO FRM-GLS-STAT IN CRP-FLD.  
                                                                        
           IF PLL-IND-FPAG IN CRP-FLD > SPACES                          
              MOVE 'COL'                   TO TAB-COD-SIST              
              MOVE 'PAC'                   TO TAB-COD-TTAB IN TAB       
              MOVE PLL-IND-FPAG IN CRP-FLD TO TAB-COD-CTAB IN TAB       
              PERFORM BUS-TAB                                           
              MOVE TAB-GLS-DESC IN TAB     TO FRM-GLS-FPAG IN CRP-FLD   
           ELSE                                                         
              MOVE SPACES                  TO FRM-GLS-FPAG IN CRP-FLD.  
                                                                        
           IF PLL-COD-SITU IN CRP-FLD > SPACES                          
              MOVE 'COL'                   TO TAB-COD-SIST              
              MOVE 'SIP'                   TO TAB-COD-TTAB IN TAB       
              MOVE PLL-COD-SITU IN CRP-FLD TO TAB-COD-CTAB IN TAB       
              PERFORM BUS-TAB                                           
              MOVE TAB-GLS-DESC IN TAB     TO FRM-GLS-SITU IN CRP-FLD   
           ELSE                                                         
              MOVE SPACES                  TO FRM-GLS-SITU IN CRP-FLD.  
      *>>>>
*% END
