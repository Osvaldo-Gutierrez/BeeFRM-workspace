*%! Codigo adicional
*% NO_TIENE_TIMESTAMP = TRUE
*% NO_HDR_CST = TRUE
*% LARGO_TOTAL_COMMAREA = 3000
*% FMT_CHI = TRUE
*% GEN_BCI = TRUE
*% PGM_PTC = TRUE
*% PGM_STAT_DPL = TRUE
*% IF WSS
      *<<<< WSS
       COPY GNSWGCPT. 
       COPY GNSWGPES.
       COPY TABBRCAM.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CAM-REQA==.
       COPY MC2BRDPC.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DPC-REQA==.        
       COPY COLBRPLL.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-PLL-REQA==.        
       COPY SGCBRDBC.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.   
       COPY COLWGCDP.
       COPY GNSWGSTR.
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
       01 WSS-NUM-CCUR.
           03 WSS-NUM-ICUR-EDT  PIC  ZZ9.
           03 WSS-NUM-ICUR-ALF       PIC X(03).
           03 WSS-NUM-ICUR      REDEFINES
              WSS-NUM-ICUR-ALF       PIC 9(03).
      *>>>>
*% END
*% IF FIN_SCR_VARI
           03 SCR-NUM-PANT               PIC 9(0005).
           03 SCR-CDP-FLD                PIC X(1100).
           03 SCR-CIC-ICLI.
              05 SCR-CAI-ICLI            PIC X(04).
              05 SCR-IIC-ICLI            PIC 9(08).
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-OFRM-SRV
                MOVE TSK-TERM-ALF TO CDP-COD-TERM
                MOVE TSK-TERM-ALF TO CDP-COD-TERM-MAS
           ELSE
                MOVE EIBTRMID     TO CDP-COD-TERM
                MOVE EIBTRMID     TO CDP-COD-TERM-MAS.
           MOVE +3859 TO SCR-TCMA.
           PERFORM GNS-ERR-QUE.
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )
               MOVE SCR-CDP-FLD TO CDP-FLD
               GO TO FIN-INI.

           IF ( FRM-FFLD = FRM-FFLD-PF6 )
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM
               ADD 1              TO QUE-NITM
               MOVE CDP-NOM-TSCDP TO QUE-COLA
               MOVE CDP-LEN-TSCDP TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT NOT = QUE-STAT-OKS
                  MOVE SCR-CDP-FLD TO CDP-FLD
                  GO TO FIN-INI.
           PERFORM GET-FHOY.
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
           MOVE 'N' TO WSS-IND-EXIS.                                    
           MOVE PLL-CAI-IPLL IN CDP-FLD TO PLL-CAI-IPLL IN PLL.         
           MOVE PLL-IIC-IPLL IN CDP-FLD TO PLL-IIC-IPLL IN PLL.         
           MOVE PLL-CAI-ICVN IN CDP-FLD TO PLL-CAI-ICVN IN PLL.         
           MOVE PLL-IIC-ICVN IN CDP-FLD TO PLL-IIC-ICVN IN PLL.         
           MOVE PLL-NUM-IPLL IN CDP-FLD TO PLL-NUM-IPLL IN PLL.         
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM COL-FIO-PLL.                                         
           IF NOT FIO-STAT-OKS                                          
              MOVE 'PLL NOT EXI' TO MSG-COD-MENS                        
              MOVE 'S' TO SCR-IERR
              GO TO CON-FND-KEY                                     
           ELSE                                                         
              MOVE FIO-STAT TO PGM-STAT-PLL.                            
           MOVE PLL-CAI-ICVN IN CDP-FLD TO DPC-CAI-ICVN IN DPC.         
           MOVE PLL-IIC-ICVN IN CDP-FLD TO DPC-IIC-ICVN IN DPC.         
           MOVE SPACES              TO DPC-CIC-ICLI IN DPC.             
           MOVE SPACES              TO DPC-COD-TREC IN DPC.             
           MOVE SPACES              TO DPC-COD-SIST IN DPC.             
           MOVE SPACES              TO DPC-COD-GRPO IN DPC.             
           MOVE ZEROES              TO DPC-NUM-IDPC IN DPC.             
           MOVE FIO-GET-NLS TO FIO-CMND.                                
           PERFORM MC2-FIO-DPC.                                         
           IF NOT (FIO-STAT-OKS AND                                     
                    PLL-CAI-ICVN IN CDP-FLD = DPC-CAI-ICVN IN DPC AND   
                    PLL-IIC-ICVN IN CDP-FLD = DPC-IIC-ICVN IN DPC)      
                    MOVE 'CVN NO EXI' TO MSG-COD-MENS                   
                    MOVE 'S' TO SCR-IERR
                    GO TO CON-FND-KEY.                              
           MOVE DPC-CIC-ICLI IN DPC TO DBC-CIC-ICLI IN DBC.             
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM SGC-FIO-DBC.                                         
           IF NOT FIO-STAT-OKS                                          
               GO TO CON-FND-KEY.                                   
           MOVE DBC-NUM-ICLI IN DBC TO FRM-NUM-ICLI IN CDP-FLD.         
           MOVE DBC-VRF-ICLI IN DBC TO FRM-VRF-ICLI IN CDP-FLD.         
           MOVE DBC-IND-ICLI IN DBC TO FRM-IND-ICLI IN CDP-FLD.         
           MOVE DBC-GLS-ICLI IN DBC TO FRM-GLS-ICLI IN CDP-FLD.         
           MOVE DBC-GLS-NOMC IN DBC TO FRM-GLS-NOMC IN CDP-FLD.         
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.                        
           PERFORM CPT-BLKS.                                            
           MOVE CPT-STRN            TO FRM-GLS-NOMC IN CDP-FLD.         

           MOVE PLL-CAI-IPLL IN CDP-FLD TO DPL-CAI-IPLL IN DPL.         
           MOVE PLL-IIC-IPLL IN CDP-FLD TO DPL-IIC-IPLL IN DPL.         
           MOVE PLL-CAI-ICVN IN CDP-FLD TO DPL-CAI-ICVN IN DPL.         
           MOVE PLL-IIC-ICVN IN CDP-FLD TO DPL-IIC-ICVN IN DPL.         
           MOVE PLL-NUM-IPLL IN CDP-FLD TO DPL-NUM-IPLL IN DPL.         
           MOVE ZEROES                  TO DPL-NUM-IDPL IN DPL.         
           IF FRM-IND-SGTE IN CDP-FLD = 'A'
              MOVE 99999999 TO DPL-NUM-IDPL IN DPL                             
              MOVE FIO-GET-LEQ TO FIO-CMND                              
           ELSE                                                         
              MOVE FIO-GET-NLS TO FIO-CMND.                             
       LUP-LEE-DPL.                                                 
           PERFORM COL-FIO-DPL.                                         
           IF NOT (  FIO-STAT-OKS AND                                   
               PLL-CAI-IPLL IN CDP-FLD = DPL-CAI-IPLL IN DPL AND        
               PLL-IIC-IPLL IN CDP-FLD = DPL-IIC-IPLL IN DPL AND        
               PLL-CAI-ICVN IN CDP-FLD = DPL-CAI-ICVN IN DPL AND        
               PLL-IIC-ICVN IN CDP-FLD = DPL-IIC-ICVN IN DPL AND        
               PLL-NUM-IPLL IN CDP-FLD = DPL-NUM-IPLL IN DPL)           
                 GO TO CON-FND-KEY.                                 

           IF WSS-IND-EXIS = 'N'                                        
              IF FRM-CAI-OPER IN CDP-FLD > SPACES OR                    
                 FRM-IIC-OPER IN CDP-FLD > ZEROES                       
              IF NOT (FRM-CAI-OPER IN CDP-FLD = DPL-CAI-IOPC IN DPL AND 
                      FRM-IIC-OPER IN CDP-FLD = DPL-IIC-IOPC IN DPL)    
                 GO TO NXT-LEE-DPL                                  
              ELSE                                                      
                 MOVE 'S' TO WSS-IND-EXIS
                 GO TO NXT-LEE-DPL.
                                                                        
           PERFORM CRG-VEC-CDP.

           IF (SCR-OFRM-SRV AND WSS-J > WSS-NITMMAX)
              GO TO CON-FND-KEY.

           IF ((NOT SCR-OFRM-SRV) AND WSS-J > WSS-JMAX)
                 GO TO CON-FND-KEY.

       NXT-LEE-DPL.
           IF FRM-IND-SGTE IN CDP-FLD = 'A'                             
               MOVE FIO-GET-PRV TO FIO-CMND
           ELSE
               MOVE FIO-GET-NXT TO FIO-CMND.
           GO TO LUP-LEE-DPL.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
       CON-FND-KEY.
           PERFORM PUT-TAB.

           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *----------------------------------------------------------
       MSG-FND-KEY.
           MOVE 'COL' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM VAL-NUM-KEY-CDP
               PERFORM VAL-NUM-FLD-CDP
               PERFORM SCRL-TAB-CDP
               PERFORM SCR-TRL.
      *>>>>
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY TABBFCAM.
       COPY COLBFPLL.
       COPY MC2BFDPC.
       COPY SGCBFDBC.                                                   
       COPY GNSBBTAB.
       COPY GNSBBMSC.                                                   
       COPY GNSBVTAB.
       COPY GNSBVCOD.
       COPY GNSBPFEC.

       PUT-TAB SECTION.
       INI-PUT-TAB.
           MOVE CDP-NOM-TSCDP     TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE CDP-NOM-TSCDP-MAS TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE 1 TO SCR-NUM-PANT.
           MOVE SPACES TO WSS-NUM-IDPL-IDX-MTI.                                       
           MOVE SPACES TO WSS-NUM-IDPL-IDX-FRM.
      *                                                                 
           MOVE 1 TO FRM-IFLD.
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.

           IF FRM-IND-SGTE IN CDP-FLD = 'A'
              MOVE WSS-J TO WSS-F IN WSS-VARI
           ELSE
              MOVE 1 TO WSS-F IN WSS-VARI.
       LUP-PUT-TAB.
           IF ((WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI AND
                FRM-IND-SGTE IN CDP-FLD NOT = 'A')       OR
               (WSS-F IN WSS-VARI < 1 AND WSS-J > 0      AND
                FRM-IND-SGTE IN CDP-FLD = 'A'))
              GO TO FIN-PUT-TAB.
           IF WSS-NUM-IDPL-V(WSS-F) > ZEROES
              PERFORM PUT-TAB-CDP
              PERFORM PUT-CDP-QUE
              PERFORM SET-FRM-IFLD
           ELSE
           IF (FRM-IFLD = 1 AND WSS-F = 1) OR
              (FRM-IFLD = 1 AND WSS-F = WSS-J AND
              FRM-IND-SGTE IN CDP-FLD = 'A')
              MOVE 'COL' TO MSG-COD-SIST
              MOVE 'CDP    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE '00' TO FIO-STAT
              MOVE 'S' TO SCR-IERR                                      
              GO TO FIN-PUT-TAB
           ELSE
              MOVE '00' TO FIO-STAT
              GO TO FIN-PUT-TAB.

           IF FRM-IND-SGTE IN CDP-FLD = 'A'
               SUBTRACT 1 FROM WSS-F IN WSS-VARI
           ELSE
               ADD 1 TO WSS-F IN WSS-VARI.
           GO TO LUP-PUT-TAB.
       FIN-PUT-TAB.
           MOVE 1                  TO QUE-NITM.
           MOVE CDP-LEN-TSCDP-MAS  TO QUE-LITM.
           MOVE CDP-NOM-TSCDP-MAS  TO QUE-COLA.

           IF WSS-NITM > WSS-NITMMAX
              MOVE 'S'           TO CDP-MAS-IND-HMAS
           ELSE
              MOVE 'N'           TO CDP-MAS-IND-HMAS.

           MOVE WSS-NITM         TO WSS-NUM-ICUR.
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.
           MOVE WSS-NUM-ICUR-ALF TO CDP-MAS-NUM-TOCC.
           MOVE CDP-MAS-ITEM     TO QUE-ITEM.
           MOVE QUE-PUT          TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF WSS-NITM > WSS-IMAX
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'CDPTABDMAS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       EXT-PUT-TAB.
           EXIT.

       SET-FRM-IFLD SECTION.
       INI-SET-FRM-IFLD.
           IF SCR-OFRM-SRV
                MOVE 1 TO FRM-IFLD
           ELSE
                ADD 1 TO FRM-IFLD.
       FIN-SET-FRM-IFLD.
           EXIT.
      *--------------------------------------------
       SCRL-TAB-CDP SECTION.
       INI-SCRL-TAB-CDP.
           MOVE SPACES TO MSG-COD-MENS.

           IF FRM-FFLD = FRM-FFLD-PF6
               PERFORM PUT-TAB-ASC
           ELSE
               PERFORM PUT-TAB-DSC.
           IF MSG-COD-MENS > SPACES
               MOVE 'COL' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-CDP.
           EXIT.

      *--------------------------------------------
       PUT-TAB-CDP SECTION.
       INI-PUT-TAB-CDP.
           PERFORM CLR-WSS.

           MOVE WSS-NUM-IDPL-V(WSS-F) TO WSS-NUM-IDPL-CDP IN WSS-VARI.
           MOVE WSS-GLS-NOMS-V(WSS-F) TO WSS-GLS-NOMS-CDP IN WSS-VARI.
           MOVE WSS-NUM-ICLS-V(WSS-F) TO WSS-NUM-ICLS-CDP IN WSS-VARI.
           MOVE WSS-VRF-ICLS-V(WSS-F) TO WSS-VRF-ICLS-CDP IN WSS-VARI.
           MOVE WSS-IND-ICLS-V(WSS-F) TO WSS-IND-ICLS-CDP IN WSS-VARI.
           MOVE WSS-GLS-ICLS-V(WSS-F) TO WSS-GLS-ICLS-CDP IN WSS-VARI.
           MOVE WSS-IND-NOVD-V(WSS-F) TO WSS-IND-NOVD-CDP IN WSS-VARI.
           MOVE WSS-FEC-FVEN-V(WSS-F) TO WSS-FEC-FVEN-CDP IN WSS-VARI.
           MOVE WSS-CAI-IOPC-V(WSS-F) TO WSS-CAI-IOPC-CDP IN WSS-VARI.
           MOVE WSS-IIC-IOPC-V(WSS-F) TO WSS-IIC-IOPC-CDP IN WSS-VARI.
           MOVE WSS-NUM-IVEN-V(WSS-F) TO WSS-NUM-IVEN-CDP IN WSS-VARI.
           MOVE WSS-NUM-VCOR-V(WSS-F) TO WSS-NUM-VCOR-CDP IN WSS-VARI.
           MOVE WSS-COD-VCOC-V(WSS-F) TO WSS-COD-VCOC-CDP IN WSS-VARI.
           MOVE WSS-VAL-CUOT-V(WSS-F) TO WSS-VAL-CUOT-CDP IN WSS-VARI.
           MOVE WSS-VAL-MPAG-V(WSS-F) TO WSS-VAL-MPAG-CDP IN WSS-VARI.

           MOVE WSS-NUM-IDPL-CDP IN WSS-VARI TO WSS-NUM-IDPL-FRM.
           MOVE WSS-GLS-NOMS-CDP IN WSS-VARI TO WSS-GLS-NOMS-FRM.
           MOVE WSS-NUM-ICLS-CDP IN WSS-VARI TO WSS-NUM-ICLS-FRM.
           MOVE WSS-VRF-ICLS-CDP IN WSS-VARI TO WSS-VRF-ICLS-FRM.
           MOVE WSS-IND-ICLS-CDP IN WSS-VARI TO WSS-IND-ICLS-FRM.
           MOVE WSS-GLS-ICLS-CDP IN WSS-VARI TO WSS-GLS-ICLS-FRM.
           MOVE WSS-IND-NOVD-CDP IN WSS-VARI TO WSS-IND-NOVD-FRM.
           MOVE WSS-FEC-FVEN-CDP IN WSS-VARI TO WSS-FEC-FVEN-FRM.
           MOVE WSS-CAI-IOPC-CDP IN WSS-VARI TO WSS-CAI-IOPC-FRM.
           MOVE WSS-IIC-IOPC-CDP IN WSS-VARI TO WSS-IIC-IOPC-FRM.
           MOVE WSS-NUM-IVEN-CDP IN WSS-VARI TO WSS-NUM-IVEN-FRM.
           MOVE WSS-NUM-VCOR-CDP IN WSS-VARI TO WSS-NUM-VCOR-FRM.
           MOVE WSS-COD-VCOC-CDP IN WSS-VARI TO WSS-COD-VCOC-FRM.
           MOVE WSS-VAL-CUOT-CDP IN WSS-VARI TO WSS-VAL-CUOT-FRM.
           MOVE WSS-VAL-MPAG-CDP IN WSS-VARI TO WSS-VAL-MPAG-FRM.

           IF FRM-IND-SGTE IN CDP-FLD = 'A' AND
              WSS-F IN WSS-VARI = WSS-IMAX
              MOVE 1 TO FRM-IFLD.

           IF FRM-IFLD > WSS-IMAX
              GO TO FIN-PUT-TAB-CDP.

           MOVE WSS-NUM-IDPL-CDP IN WSS-VARI TO 
               FRM-NUM-IDPL IN CDP-FLD(FRM-IFLD).
           MOVE WSS-GLS-NOMS-CDP IN WSS-VARI TO 
               FRM-GLS-NOMS IN CDP-FLD(FRM-IFLD).
           MOVE WSS-NUM-ICLS-CDP IN WSS-VARI TO 
               FRM-NUM-ICLS IN CDP-FLD(FRM-IFLD).
           MOVE WSS-VRF-ICLS-CDP IN WSS-VARI TO 
               FRM-VRF-ICLS IN CDP-FLD(FRM-IFLD).
           MOVE WSS-IND-ICLS-CDP IN WSS-VARI TO 
               FRM-IND-ICLS IN CDP-FLD(FRM-IFLD).
           MOVE WSS-GLS-ICLS-CDP IN WSS-VARI TO 
               FRM-GLS-ICLS IN CDP-FLD(FRM-IFLD).
           MOVE WSS-IND-NOVD-CDP IN WSS-VARI TO 
               FRM-IND-NOVD IN CDP-FLD(FRM-IFLD).
           MOVE WSS-FEC-FVEN-CDP IN WSS-VARI TO 
               FRM-FEC-FVEN IN CDP-FLD(FRM-IFLD).
           MOVE WSS-CAI-IOPC-CDP IN WSS-VARI TO 
               FRM-CAI-IOPC IN CDP-FLD(FRM-IFLD).
           MOVE WSS-IIC-IOPC-CDP IN WSS-VARI TO 
               FRM-IIC-IOPC IN CDP-FLD(FRM-IFLD).
           MOVE WSS-NUM-IVEN-CDP IN WSS-VARI TO 
               FRM-NUM-IVEN IN CDP-FLD(FRM-IFLD).
           MOVE WSS-NUM-VCOR-CDP IN WSS-VARI TO 
               FRM-NUM-VCOR IN CDP-FLD(FRM-IFLD).
           MOVE WSS-COD-VCOC-CDP IN WSS-VARI TO 
               FRM-COD-VCOC IN CDP-FLD(FRM-IFLD).
           MOVE WSS-VAL-CUOT-CDP IN WSS-VARI TO 
               FRM-VAL-CUOT IN CDP-FLD(FRM-IFLD).
           MOVE WSS-VAL-MPAG-CDP IN WSS-VARI TO 
               FRM-VAL-MPAG IN CDP-FLD(FRM-IFLD).
       FIN-PUT-TAB-CDP.
           EXIT.
      *----------------------------------------------------
       PUT-CDP-QUE SECTION.
       INI-PUT-CDP-QUE.
           PERFORM PRO-FLD-QUE.
           ADD 1 TO WSS-NITM.
           MOVE CDP-NOM-TSCDP    TO QUE-COLA.
           MOVE CDP-LEN-TSCDP    TO QUE-LITM.
           MOVE WSS-NITM         TO QUE-NITM.
           MOVE SPACES           TO QUE-ITEM.
           IF SCR-OFRM-SRV
              MOVE WSS-NUM-IDPL-IDX-MTI TO QUE-ITEM                     
           ELSE
              MOVE WSS-NUM-IDPL-IDX-FRM  TO QUE-ITEM.
           MOVE QUE-PUT  TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
       FIN-PUT-CDP-QUE.
           EXIT.
      *----------------------------------------------------

       PUT-TAB-ASC SECTION.
       INI-PUT-TAB-ASC.
           IF FRM-IND-SGTE IN CDP-FLD = 'A'
              MOVE 'FPF    INV' TO MSG-COD-MENS
              GO TO EXT-PUT-TAB-ASC.
           MOVE SPACES TO MSG-COD-MENS.
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.
           MOVE 0 TO FRM-IFLD.
       LUP-PUT-TAB-ASC.
           ADD 1              TO QUE-NITM.
           MOVE CDP-NOM-TSCDP TO QUE-COLA.
           MOVE CDP-LEN-TSCDP TO QUE-LITM.
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD = 0
                   MOVE 'CDP PLL NXT' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-ASC
               ELSE
                   GO TO FIN-PUT-TAB-ASC.
           ADD 1 TO FRM-IFLD.
           MOVE QUE-ITEM TO FRM-NUM-IDPL-IDX(FRM-IFLD).
           PERFORM PRO-FLD-FRM.
           IF FRM-IFLD < WSS-IMAX
               GO TO LUP-PUT-TAB-ASC.
       FIN-PUT-TAB-ASC.
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX
               ADD 1 FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1
                                 UNTIL WSS-I > WSS-IMAX.
           IF FRM-IFLD > 0
               ADD 1 TO SCR-NUM-PANT.
           IF FRM-IFLD = WSS-IMAX
               ADD 1              TO QUE-NITM
               MOVE CDP-NOM-TSCDP TO QUE-COLA
               MOVE CDP-LEN-TSCDP TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT = QUE-STAT-OKS
                   MOVE 'COL'        TO MSG-COD-SIST
                   MOVE 'CDPTABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-ASC.
           EXIT.

       PUT-TAB-DSC SECTION.
       INI-PUT-TAB-DSC.
           IF FRM-IND-SGTE IN CDP-FLD = 'A'
              MOVE 'FPF    INV' TO MSG-COD-MENS
              GO TO EXT-PUT-TAB-DSC.
           MOVE SPACES TO MSG-COD-MENS.
           IF SCR-NUM-PANT = 1
               MOVE 'CDP PLL PRV' TO MSG-COD-MENS
               GO TO EXT-PUT-TAB-DSC.

           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.
           ADD 1 TO QUE-NITM.
           ADD 1 WSS-IMAX GIVING FRM-IFLD.
       LUP-PUT-TAB-DSC.
           SUBTRACT  1 FROM QUE-NITM.
           MOVE CDP-NOM-TSCDP TO QUE-COLA
           MOVE CDP-LEN-TSCDP TO QUE-LITM
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD > WSS-IMAX
                   MOVE 'CDP PLL PRV' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-DSC
               ELSE
                   GO TO FIN-PUT-TAB-DSC.
           SUBTRACT 1 FROM FRM-IFLD.
           MOVE QUE-ITEM TO FRM-NUM-IDPL-IDX(FRM-IFLD).
           PERFORM PRO-FLD-FRM.
           IF FRM-IFLD > 1
               GO TO LUP-PUT-TAB-DSC.
       FIN-PUT-TAB-DSC.

           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1
                                 UNTIL WSS-I < 1.

           IF FRM-IFLD NOT > WSS-IMAX
               SUBTRACT 1 FROM SCR-NUM-PANT
               MOVE 'COL'        TO MSG-COD-SIST
               MOVE 'CDPTABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-DSC.
           EXIT.

       COPY GNSBGCPT.
       COPY GNSBGSTR.

       CLR-FRM SECTION.
       INI-CLR-FRM.
           MOVE ZEROES TO FRM-NUM-IDPL IN CDP-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-NOMS IN CDP-FLD(WSS-I).
           MOVE ZEROES TO FRM-NUM-ICLS IN CDP-FLD(WSS-I).
           MOVE SPACES TO FRM-VRF-ICLS IN CDP-FLD(WSS-I).
           MOVE SPACES TO FRM-IND-ICLS IN CDP-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-ICLS IN CDP-FLD(WSS-I).
           MOVE SPACES TO FRM-IND-NOVD IN CDP-FLD(WSS-I).
           MOVE SPACES TO FRM-FEC-FVEN IN CDP-FLD(WSS-I).
           MOVE SPACES TO FRM-CAI-IOPC IN CDP-FLD(WSS-I).
           MOVE ZEROES TO FRM-IIC-IOPC IN CDP-FLD(WSS-I).
           MOVE ZEROES TO FRM-NUM-IVEN IN CDP-FLD(WSS-I).
           MOVE ZEROES TO FRM-NUM-VCOR IN CDP-FLD(WSS-I).
           MOVE SPACES TO FRM-COD-VCOC IN CDP-FLD(WSS-I).
           MOVE ZEROES TO FRM-VAL-CUOT IN CDP-FLD(WSS-I).
           MOVE ZEROES TO FRM-VAL-MPAG IN CDP-FLD(WSS-I).
       FIN-CLR-FRM.
           EXIT.

       CLR-WSS SECTION.
       INI-CLR-WSS.
           MOVE ZEROES TO WSS-NUM-IDPL-CDP IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-NOMS-CDP IN WSS-VARI.
           MOVE ZEROES TO WSS-NUM-ICLS-CDP IN WSS-VARI.
           MOVE SPACES TO WSS-VRF-ICLS-CDP IN WSS-VARI.
           MOVE SPACES TO WSS-IND-ICLS-CDP IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-ICLS-CDP IN WSS-VARI.
           MOVE SPACES TO WSS-IND-NOVD-CDP IN WSS-VARI.
           MOVE SPACES TO WSS-FEC-FVEN-CDP IN WSS-VARI.
           MOVE SPACES TO WSS-CAI-IOPC-CDP IN WSS-VARI.
           MOVE ZEROES TO WSS-IIC-IOPC-CDP IN WSS-VARI.
           MOVE ZEROES TO WSS-NUM-IVEN-CDP IN WSS-VARI.
           MOVE ZEROES TO WSS-NUM-VCOR-CDP IN WSS-VARI.
           MOVE SPACES TO WSS-COD-VCOC-CDP IN WSS-VARI.
           MOVE ZEROES TO WSS-VAL-CUOT-CDP IN WSS-VARI.
           MOVE ZEROES TO WSS-VAL-MPAG-CDP IN WSS-VARI.

           IF SCR-OFRM-SRV
               PERFORM CLR-FRM-MTX.
       FIN-CLR-WSS.
           EXIT.

       CLR-FRM-MTX SECTION.
       INI-CLR-FRM-MTX.
           MOVE ZEROES TO  FRM-NUM-IDPL IN CDP-FLD(1).
           MOVE SPACES TO  FRM-GLS-NOMS IN CDP-FLD(1).
           MOVE ZEROES TO  FRM-NUM-ICLS IN CDP-FLD(1).
           MOVE SPACES TO  FRM-VRF-ICLS IN CDP-FLD(1).
           MOVE SPACES TO  FRM-IND-ICLS IN CDP-FLD(1).
           MOVE SPACES TO  FRM-GLS-ICLS IN CDP-FLD(1).
           MOVE SPACES TO  FRM-IND-NOVD IN CDP-FLD(1).
           MOVE SPACES TO  FRM-FEC-FVEN IN CDP-FLD(1).
           MOVE SPACES TO  FRM-CAI-IOPC IN CDP-FLD(1).
           MOVE ZEROES TO  FRM-IIC-IOPC IN CDP-FLD(1).
           MOVE ZEROES TO  FRM-NUM-IVEN IN CDP-FLD(1).
           MOVE ZEROES TO  FRM-NUM-VCOR IN CDP-FLD(1).
           MOVE SPACES TO  FRM-COD-VCOC IN CDP-FLD(1).
           MOVE ZEROES TO  FRM-VAL-CUOT IN CDP-FLD(1).
           MOVE ZEROES TO  FRM-VAL-MPAG IN CDP-FLD(1).
       FIN-CLR-FRM-MTX.
           EXIT.

       CRG-VEC-CDP SECTION.
       INI-CRG-VEC-CDP.
           ADD 1 TO WSS-J.
           IF WSS-J IN WSS-VARI > WSS-JMAX IN WSS-VARI
               GO TO FIN-CRG-VEC-CDP.

           PERFORM CLR-CDP-VEC.

           MOVE DPL-NUM-IDPL IN DPL TO
                             WSS-NUM-IDPL-V IN WSS-VARI(WSS-J).
           MOVE DPL-CIC-ICLI IN DPL TO DBC-CIC-ICLI IN DBC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           MOVE DBC-NUM-ICLI IN DBC TO 
                             WSS-NUM-ICLS-V IN WSS-VARI(WSS-J). 
           MOVE DBC-VRF-ICLI IN DBC TO 
                             WSS-VRF-ICLS-V IN WSS-VARI(WSS-J). 
           MOVE DBC-IND-ICLI IN DBC TO 
                             WSS-IND-ICLS-V IN WSS-VARI(WSS-J). 
           MOVE DBC-GLS-ICLI IN DBC TO 
                             WSS-GLS-ICLS-V IN WSS-VARI(WSS-J). 
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.                        
           PERFORM CPT-BLKS.                                            
           MOVE CPT-STRN TO  WSS-GLS-NOMS-V(WSS-J).
           MOVE DPL-COD-ESTA IN DPL TO
                             WSS-IND-NOVD-V IN WSS-VARI(WSS-J).
           MOVE DPL-NUM-SVEN IN DPL TO WSS-NUM-STRA IN WSS-VARI.
           MOVE DPL-NUM-AVEN IN DPL TO WSS-NUM-ATRA IN WSS-VARI.
           MOVE DPL-NUM-MVEN IN DPL TO WSS-NUM-MTRA IN WSS-VARI.
           MOVE DPL-NUM-DVEN IN DPL TO WSS-NUM-DTRA IN WSS-VARI.
           MOVE WSS-FEC-FTRA IN WSS-VARI TO
                             WSS-FEC-FVEN-V IN WSS-VARI(WSS-J).
           MOVE DPL-CAI-IOPC IN DPL TO                                  
                             WSS-CAI-IOPC-V IN WSS-VARI(WSS-J).         
           MOVE DPL-IIC-IOPC IN DPL TO                                  
                             WSS-IIC-IOPC-V IN WSS-VARI(WSS-J).         
           MOVE DPL-NUM-IVEN IN DPL TO                                  
                             WSS-NUM-IVEN-V IN WSS-VARI(WSS-J).         
           MOVE DPL-NUM-VCOR IN DPL TO                                  
                             WSS-NUM-VCOR-V IN WSS-VARI(WSS-J).         
           MOVE DPL-COD-VCOC IN DPL TO                                  
                             WSS-COD-VCOC-V IN WSS-VARI(WSS-J).         
           IF SCR-OFRM-SRV                                              
              MOVE DPL-VAL-CUOT IN DPL TO 
                                FRM-VAL-CUOT-EDT IN CDP-EDT             
              MOVE FRM-VAL-CUOT-EDT IN CDP-EDT TO 
                                WSS-VAL-CUOT-V-ALF IN WSS-VARI(WSS-J)
              MOVE DPL-VAL-MPAG IN DPL TO 
                                FRM-VAL-MPAG-EDT IN CDP-EDT             
              MOVE FRM-VAL-MPAG-EDT IN CDP-EDT TO 
                                WSS-VAL-MPAG-V-ALF IN WSS-VARI(WSS-J)
           ELSE                                                         
              MOVE DPL-VAL-CUOT IN DPL TO 
                                WSS-VAL-CUOT-V IN WSS-VARI(WSS-J)       
              MOVE DPL-VAL-MPAG IN DPL TO 
                                WSS-VAL-MPAG-V IN WSS-VARI(WSS-J).      
       FIN-CRG-VEC-CDP.
           EXIT.

       CLR-CDP-VEC SECTION.
       INI-CLR-CDP-VEC.
           MOVE ZEROES TO  WSS-NUM-IDPL-V(WSS-J).
           MOVE SPACES TO  WSS-GLS-NOMS-V(WSS-J).
           MOVE ZEROES TO  WSS-NUM-ICLS-V(WSS-J).
           MOVE SPACES TO  WSS-VRF-ICLS-V(WSS-J).
           MOVE SPACES TO  WSS-IND-ICLS-V(WSS-J).
           MOVE SPACES TO  WSS-GLS-ICLS-V(WSS-J).
           MOVE SPACES TO  WSS-IND-NOVD-V(WSS-J).
           MOVE SPACES TO  WSS-FEC-FVEN-V(WSS-J).
           MOVE SPACES TO  WSS-CAI-IOPC-V(WSS-J).
           MOVE ZEROES TO  WSS-IIC-IOPC-V(WSS-J).
           MOVE ZEROES TO  WSS-NUM-IVEN-V(WSS-J).
           MOVE ZEROES TO  WSS-NUM-VCOR-V(WSS-J).
           MOVE SPACES TO  WSS-COD-VCOC-V(WSS-J).
           MOVE ZEROES TO  WSS-VAL-CUOT-V(WSS-J).
           MOVE ZEROES TO  WSS-VAL-MPAG-V(WSS-J).
       FIN-CLR-CDP-VEC.
           EXIT.

       PRO-FLD-QUE SECTION.
       INI-PRO-FLD-QUE.
           MOVE 'P' TO  WSS-NUM-IDPL-ATR.
           MOVE 'P' TO  WSS-GLS-NOMS-ATR.
           MOVE 'P' TO  WSS-NUM-ICLS-ATR.
           MOVE 'P' TO  WSS-VRF-ICLS-ATR.
           MOVE 'P' TO  WSS-IND-ICLS-ATR.
           MOVE 'P' TO  WSS-GLS-ICLS-ATR.
           MOVE 'P' TO  WSS-IND-NOVD-ATR.
           MOVE 'P' TO  WSS-FEC-FVEN-ATR.
           MOVE 'P' TO  WSS-CAI-IOPC-ATR.
           MOVE 'P' TO  WSS-IIC-IOPC-ATR.
           MOVE 'P' TO  WSS-NUM-IVEN-ATR.
           MOVE 'P' TO  WSS-NUM-VCOR-ATR.
           MOVE 'P' TO  WSS-COD-VCOC-ATR.
           MOVE 'P' TO  WSS-VAL-CUOT-ATR.
           MOVE 'P' TO  WSS-VAL-MPAG-ATR.
       FIN-PRO-FLD-QUE.
           EXIT.

       PRO-FLD-FRM SECTION.
       INI-PRO-FLD-FRM.
           MOVE FRM-CPIM-AEY TO FRM-NUM-IDPL-ATR IN CDP-FLD(FRM-IFLD).
           MOVE FRM-CPIM-AEY TO FRM-GLS-NOMS-ATR IN CDP-FLD(FRM-IFLD).
           MOVE FRM-CPIM-AEY TO FRM-NUM-ICLS-ATR IN CDP-FLD(FRM-IFLD).
           MOVE FRM-CPIM-AEY TO FRM-VRF-ICLS-ATR IN CDP-FLD(FRM-IFLD).
           MOVE FRM-CPIM-AEY TO FRM-IND-ICLS-ATR IN CDP-FLD(FRM-IFLD).
           MOVE FRM-CPIM-AEY TO FRM-GLS-ICLS-ATR IN CDP-FLD(FRM-IFLD).
           MOVE FRM-CPIM-AEY TO FRM-IND-NOVD-ATR IN CDP-FLD(FRM-IFLD).
           MOVE FRM-CPIM-AEY TO FRM-FEC-FVEN-ATR IN CDP-FLD(FRM-IFLD).
           MOVE FRM-CPIM-AEY TO FRM-CAI-IOPC-ATR IN CDP-FLD(FRM-IFLD).
           MOVE FRM-CPIM-AEY TO FRM-IIC-IOPC-ATR IN CDP-FLD(FRM-IFLD).
           MOVE FRM-CPIM-AEY TO FRM-NUM-IVEN-ATR IN CDP-FLD(FRM-IFLD).
           MOVE FRM-CPIM-AEY TO FRM-NUM-VCOR-ATR IN CDP-FLD(FRM-IFLD).
           MOVE FRM-CPIM-AEY TO FRM-COD-VCOC-ATR IN CDP-FLD(FRM-IFLD).
           MOVE FRM-CPIM-AEY TO FRM-VAL-CUOT-ATR IN CDP-FLD(FRM-IFLD).
           MOVE FRM-CPIM-AEY TO FRM-VAL-MPAG-ATR IN CDP-FLD(FRM-IFLD).
       FIN-PRO-FLD-FRM.
           EXIT.
      *>>>> EOF
*% END
