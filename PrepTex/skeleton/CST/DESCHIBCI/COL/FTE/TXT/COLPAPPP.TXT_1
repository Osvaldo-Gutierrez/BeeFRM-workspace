*%! Codigo Adicional
*% PGM_PTC = TRUE
*% FRM = "PPP"
*% PADRE = "PPP"
*% KEY_PADRE = "PPP-KEY-IPPP"
*% LARGO_KEY_PADRE = 24
*% FIO_PADRE = "COL-FIO-PPP"
*% SIST_PADRE = "COL"
*% KEY_BROWSE_PADRE = "PPP-KEY-IPPP"
*% ORG_PADRE_VSI = TRUE
*%! ACEPTE_PF7 = TRUE
*%! ACEPTE_PF8 = TRUE
*% INCLUDE 'GNSSRC:PFSCONENT.SRC'
*% IF WSS
      *<<<< WSS
       COPY COLBRPOC.
       COPY GNSBRMSC.
 
       01  WSS-VARI.
           03  WSS-COD-CAUX.
               05  WSS-COD-TOOC              PIC 9(03).
               05  WSS-COD-AOOC              PIC 9(03).
           03  WSS-HRA-HORA.
               05  WSS-HRA-PLAN              PIC X(04).
               05  WSS-HRA-SEGU              PIC X(02).
           03  WSS-IFLD                      PIC 9(03).
*% IF NOT PGM_MCP
           03  WSS-FRM-TOP     VALUE  12     PIC 9(02).
*% ELSE
           03  WSS-FRM-TOP     VALUE   7     PIC 9(02).
*% END
      *>>>>
*% END
*% IF FND_KEY
      *<<<< FND_KEY
              MOVE FIO-GET-LEQ TO FIO-CMND
              PERFORM PPP-ASUME
              GO TO CON-FND-KEY.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
       CON-FND-KEY.
           PERFORM COL-FIO-PPP.
           IF FIO-STAT-OKS
               MOVE PPP-STP-ITRN IN PPP TO SCR-UMOD.
           MOVE FIO-STAT TO PGM-STAT-PPP.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY COLBFPOC.
       COPY GNSBFMSC.
       COPY GNSBVCOD.
       COPY GNSBVTAB.
       COPY GNSBVIND.
       COPY GNSBVMSC.


       PPP-ASUME SECTION.
       INI-PPP-ASUME.
           IF PPP-FEC-PLAN IN PPP-FLD = ZEROES
              MOVE 99999999 TO PPP-FEC-PLAN IN PPP.
           IF PPP-HRA-PLAN IN PPP-FLD = ZEROES
              MOVE 9999    TO PPP-HRA-PLAN IN PPP.
       FIN-PPP-ASUME.
           EXIT.
      *>>>>
*% END
