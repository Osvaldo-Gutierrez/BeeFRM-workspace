*%! Codigo adicional
*% NO_TIENE_TIMESTAMP = TRUE
*% NO_HDR_CST = TRUE
*% LARGO_TOTAL_COMMAREA = 3000
*% FMT_CHI = TRUE
*% GEN_BCI = TRUE
*% PGM_PTC = TRUE
*% IF SETTAG
*% VSNPQ = "06.08.22.1"
*% FNCPQ = "CNCMSV"
*% END
*% IF WSS
      *<<<< WSS
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DEC-REQA==.        
       COPY MC2BRDEC.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.        
       COPY SGCBRDBC.                                                   
       COPY COLWXCRP.
       COPY GNSWGCPT.
       COPY GNSWGSTR.
*% IF NOT PGM_MCP AND NOT PGM_PTC
       COPY GNSWGQUE.
*% END
       01 WSS-VARI.                                                     
           03 WSS-I             VALUE ZEROES        PIC 9(02).        
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
       01 WSS-NUM-CCUR.
           03 WSS-NUM-ICUR-EDT  PIC  ZZ9.
           03 WSS-NUM-ICUR-ALF       PIC X(03).
           03 WSS-NUM-ICUR      REDEFINES
              WSS-NUM-ICUR-ALF       PIC 9(03).
      *>>>>
*% END
*% IF FIN_SCR_VARI
           03 SCR-NUM-PANT               PIC 9(0005).
           03 SCR-CRP-FLD                PIC X(1100).
           03 SCR-CIC-ICLI.
              05 SCR-CAI-ICLI            PIC X(04).
              05 SCR-IIC-ICLI            PIC 9(08).
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-OFRM-SRV
                MOVE TSK-TERM-ALF TO CRP-COD-TERM
                MOVE TSK-TERM-ALF TO CRP-COD-TERM-MAS
           ELSE
                MOVE EIBTRMID     TO CRP-COD-TERM
                MOVE EIBTRMID     TO CRP-COD-TERM-MAS.
           MOVE +3859 TO SCR-TCMA.
           PERFORM GNS-ERR-QUE.
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )
               MOVE SCR-CRP-FLD TO CRP-FLD
               GO TO FIN-INI.

           IF ( FRM-FFLD = FRM-FFLD-PF6 )
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM
               ADD 1              TO QUE-NITM
               MOVE CRP-NOM-TSCRP TO QUE-COLA
               MOVE CRP-LEN-TSCRP TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT NOT = QUE-STAT-OKS
                  MOVE SCR-CRP-FLD TO CRP-FLD
                  GO TO FIN-INI.
           PERFORM GET-FHOY.
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
           MOVE PLL-CAI-IPLL IN CRP-FLD TO PLL-CAI-IPLL IN PLL.       
           MOVE PLL-IIC-IPLL IN CRP-FLD TO PLL-IIC-IPLL IN PLL.       
      *HRS-INI 25-JUN-2009 SE ADAPTA LECTURA HACIA ATRAS
      *    IF FRM-IND-SGTE IN CRP-FLD = 'S'
           IF FRM-IND-SGTE IN CRP-FLD = 'S' OR 'A'
      *HRS-FIN 25-JUN-2009
               MOVE FRM-CAI-YPLL IN CRP-FLD TO PLL-CAI-IPLL IN PLL
               MOVE FRM-IIC-YPLL IN CRP-FLD TO PLL-IIC-IPLL IN PLL
               MOVE FRM-CAI-YCVN IN CRP-FLD TO PLL-CAI-ICVN IN PLL
               MOVE FRM-IIC-YCVN IN CRP-FLD TO PLL-IIC-ICVN IN PLL
               MOVE FRM-NUM-YPLL IN CRP-FLD TO PLL-NUM-IPLL IN PLL.

      *HRS-INI 25-JUN-2009 SE ADAPTA LECTURA HACIA ATRAS
           IF FRM-IND-SGTE IN CRP-FLD = 'A'
              MOVE FIO-GET-LEQ TO FIO-CMND
           ELSE
      *HRS-FIN 25-JUN-2009
           MOVE FIO-GET-NLS TO FIO-CMND.
       LUP-LEE-PLL.
           PERFORM COL-FIO-PLL.
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF
               GO TO CON-FND-KEY.

      *HRS-INI 25-JUN-2009 SE ADAPTA LECTURA HACIA ATRAS
           IF FRM-IND-SGTE IN CRP-FLD = 'A'
              IF PLL-CIC-IPLL IN PLL    =  SPACES OR
                 PLL-CIC-IPLL IN PLL    =  ZEROES OR
                 PLL-CIC-IPLL IN PLL    =  LOW-VALUE
                 GO TO NXT-LEE-PLL.

      *HRS-INI 02-JUL-2009 SE ADAPTA LECTURA
           IF PLL-CAI-IPLL IN PLL = FRM-CAI-YPLL IN CRP-FLD AND
              PLL-IIC-IPLL IN PLL = FRM-IIC-YPLL IN CRP-FLD AND
              PLL-CAI-ICVN IN PLL = FRM-CAI-YCVN IN CRP-FLD AND
              PLL-IIC-ICVN IN PLL = FRM-IIC-YCVN IN CRP-FLD AND
              PLL-NUM-IPLL IN PLL = FRM-NUM-YPLL IN CRP-FLD
                 GO TO NXT-LEE-PLL.
      *HRS-FIN 02-JUL-2009
      *HRS-FIN 25-JUN-2009

           IF PLL-CAI-IPLL IN CRP-FLD > SPACES AND
              PLL-CAI-IPLL IN CRP-FLD NOT = PLL-CAI-IPLL IN PLL
               GO TO NXT-LEE-PLL.

           IF PLL-IIC-IPLL IN CRP-FLD > ZEROES AND
              PLL-IIC-IPLL IN CRP-FLD NOT = PLL-IIC-IPLL IN PLL
               GO TO NXT-LEE-PLL.
      *MHM-INI 06-NOV-2008
           IF PLL-CAI-ICVN IN CRP-FLD > SPACES AND
              PLL-CAI-ICVN IN CRP-FLD NOT = PLL-CAI-ICVN IN PLL
               GO TO NXT-LEE-PLL.

           IF PLL-IIC-ICVN IN CRP-FLD > ZEROES AND
              PLL-IIC-ICVN IN CRP-FLD NOT = PLL-IIC-ICVN IN PLL
               GO TO NXT-LEE-PLL.
      *MHM-FIN 06-NOV-2008

           IF PLL-NUM-IPLL IN CRP-FLD > ZEROES AND                      
              PLL-NUM-IPLL IN CRP-FLD NOT = PLL-NUM-IPLL IN PLL         
               GO TO NXT-LEE-PLL.                                       
                                                                        
           MOVE FRM-NUM-DPIN IN CRP-FLD TO WSS-NUM-DINI IN WSS-VARI.
           MOVE FRM-NUM-MPIN IN CRP-FLD TO WSS-NUM-MINI IN WSS-VARI.
           MOVE FRM-NUM-SPIN IN CRP-FLD TO WSS-NUM-SINI IN WSS-VARI.
           MOVE FRM-NUM-APIN IN CRP-FLD TO WSS-NUM-AINI IN WSS-VARI.
           MOVE FRM-NUM-DPTR IN CRP-FLD TO WSS-NUM-DTER IN WSS-VARI.
           MOVE FRM-NUM-MPTR IN CRP-FLD TO WSS-NUM-MTER IN WSS-VARI.
           MOVE FRM-NUM-SPTR IN CRP-FLD TO WSS-NUM-STER IN WSS-VARI.
           MOVE FRM-NUM-APTR IN CRP-FLD TO WSS-NUM-ATER IN WSS-VARI.
 
           IF FRM-FEC-FPIN IN CRP-FLD > ZEROES AND
              WSS-FEC-FINI IN WSS-VARI > PLL-FEC-FPGO IN PLL
               GO TO NXT-LEE-PLL.

           IF FRM-FEC-FPTR IN CRP-FLD > ZEROES AND
              WSS-FEC-FTER IN WSS-VARI < PLL-FEC-FPGO IN PLL
               GO TO NXT-LEE-PLL.

           MOVE FRM-NUM-DVIN IN CRP-FLD TO WSS-NUM-DINI IN WSS-VARI.
           MOVE FRM-NUM-MVIN IN CRP-FLD TO WSS-NUM-MINI IN WSS-VARI.
           MOVE FRM-NUM-SVIN IN CRP-FLD TO WSS-NUM-SINI IN WSS-VARI.
           MOVE FRM-NUM-AVIN IN CRP-FLD TO WSS-NUM-AINI IN WSS-VARI.
           MOVE FRM-NUM-DVTR IN CRP-FLD TO WSS-NUM-DTER IN WSS-VARI.
           MOVE FRM-NUM-MVTR IN CRP-FLD TO WSS-NUM-MTER IN WSS-VARI.
           MOVE FRM-NUM-SVTR IN CRP-FLD TO WSS-NUM-STER IN WSS-VARI.
           MOVE FRM-NUM-AVTR IN CRP-FLD TO WSS-NUM-ATER IN WSS-VARI.

           IF FRM-FEC-FVIN IN CRP-FLD > ZEROES AND
              WSS-FEC-FINI IN WSS-VARI > PLL-FEC-FVEN IN PLL
               GO TO NXT-LEE-PLL.

           IF FRM-FEC-FVTR IN CRP-FLD > ZEROES AND
              WSS-FEC-FTER IN WSS-VARI < PLL-FEC-FVEN IN PLL
               GO TO NXT-LEE-PLL.

           IF PLL-MSC-STAT IN CRP-FLD > SPACES AND
              PLL-MSC-STAT IN CRP-FLD NOT = PLL-MSC-STAT IN PLL
               GO TO NXT-LEE-PLL.

           IF PLL-COD-SITU IN CRP-FLD > SPACES AND                      
              PLL-COD-SITU IN CRP-FLD NOT = PLL-COD-SITU IN PLL         
               GO TO NXT-LEE-PLL.                                       
                                                                        
           IF PLL-IND-FPAG IN CRP-FLD > SPACES AND
              PLL-IND-FPAG IN CRP-FLD NOT = PLL-IND-FPAG IN PLL
               GO TO NXT-LEE-PLL.

           IF PLL-IND-TCAR IN CRP-FLD > SPACES AND                      
              PLL-IND-TCAR IN CRP-FLD NOT = PLL-IND-TCAR IN PLL         
               GO TO NXT-LEE-PLL.                                       
                                                                        
           IF PLL-CAI-ICYA IN CRP-FLD > SPACES AND                      
              PLL-CAI-ICYA IN CRP-FLD NOT = PLL-CAI-ICYA IN PLL         
               GO TO NXT-LEE-PLL.                                       
                                                                        
           IF PLL-IIC-ICYA IN CRP-FLD > SPACES AND                      
              PLL-IIC-ICYA IN CRP-FLD NOT = PLL-IIC-ICYA IN PLL         
               GO TO NXT-LEE-PLL.                                       

      *MHM-INI 06-NOV-2008
           IF SCR-IIC-ICLI NOT > ZEROES
               GO TO CNT-SIN-EMP.

           MOVE PLL-CAI-ICVN IN PLL TO DEC-CAI-ICVN IN DEC.
           MOVE PLL-IIC-ICVN IN PLL TO DEC-IIC-ICVN IN DEC.
           MOVE 'DEC-KEY-ICVN' TO FIO-AKEY.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM MC2-FIO-DEC.
           IF NOT FIO-STAT-OKS
               GO TO NXT-LEE-PLL.

           IF DEC-CAI-ICLI IN DEC NOT = SCR-CAI-ICLI OR
              DEC-IIC-ICLI IN DEC NOT = SCR-IIC-ICLI
               GO TO NXT-LEE-PLL.

       CNT-SIN-EMP.
      *MHM-FIN 06-NOV-2008
                                                                        
           PERFORM CRG-VEC-CRP.

           IF (SCR-OFRM-SRV AND WSS-J > WSS-NITMMAX)
              GO TO CON-FND-KEY.

           IF ((NOT SCR-OFRM-SRV) AND WSS-J > WSS-JMAX)
                 GO TO CON-FND-KEY.

      *HRS-INI 25-JUN-2009 SE ADAPTA LECTURA HACIA ATRAS
     ***   IF FRM-IND-SGTE IN CRP-FLD = 'A'
     ***      IF (WSS-J > WSS-IMAX)
     ***          GO TO CON-FND-KEY.
      *HRS-FIN 25-JUN-2009

       NXT-LEE-PLL.
      *HRS-INI 25-JUN-2009 SE ADAPTA LECTURA HACIA ATRAS
           IF FRM-IND-SGTE IN CRP-FLD = 'A'
              MOVE FIO-GET-PRV TO FIO-CMND
           ELSE
      *HRS-FIN 25-JUN-2009
           MOVE FIO-GET-NXT TO FIO-CMND.
           GO TO LUP-LEE-PLL.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
       CON-FND-KEY.
           PERFORM PUT-TAB.

           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *----------------------------------------------------------
       MSG-FND-KEY.
           MOVE 'COL' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM VAL-NUM-KEY-CRP
               PERFORM VAL-NUM-FLD-CRP
               PERFORM SCRL-TAB-CRP
               PERFORM SCR-TRL.
      *>>>>
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY SGCBFDBC.                                                   
       COPY MC2BFDEC.                                                   
       COPY GNSBBTAB.
       COPY GNSBBMSC.                                                   
       COPY GNSBVTAB.
       COPY GNSBVCOD.
       COPY GNSBPFEC.

       PUT-TAB SECTION.
       INI-PUT-TAB.
           MOVE CRP-NOM-TSCRP     TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE CRP-NOM-TSCRP-MAS TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE 1 TO SCR-NUM-PANT.
           MOVE SPACES TO CRP-TS.                                       
           MOVE LOW-VALUES TO WSS-CAI-IPLL-IDX-FRM.
      *                                                                 
           MOVE 1 TO FRM-IFLD.
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.

      *HRS-INI 25-JUN-2009 SE ADAPTA LECTURA HACIA ATRAS
           IF FRM-IND-SGTE IN CRP-FLD = 'A'
              MOVE WSS-J TO WSS-F IN WSS-VARI
           ELSE
      *HRS-FIN 25-JUN-2009
           MOVE 1 TO WSS-F IN WSS-VARI.
       LUP-PUT-TAB.
      *HRS-INI 25-JUN-2009 SE ADAPTA LECTURA HACIA ATRAS
      *    IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI
           IF ((WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI AND
                FRM-IND-SGTE IN CRP-FLD NOT = 'A')       OR
               (WSS-F IN WSS-VARI < 1  AND WSS-J > 0     AND
                FRM-IND-SGTE IN CRP-FLD = 'A'))
      *HRS-FIN 25-JUN-2009
              GO TO FIN-PUT-TAB.
           IF WSS-IIC-IPLL-V(WSS-F) > ZEROES OR
              WSS-CAI-IPLL-V(WSS-F) > SPACES 
              PERFORM PUT-TAB-CRP
              PERFORM PUT-CRP-QUE
              PERFORM SET-FRM-IFLD
           ELSE
           IF (FRM-IFLD = 1 AND WSS-F = 1) OR
              (FRM-IFLD = 1 AND WSS-F = WSS-J AND
              FRM-IND-SGTE IN CRP-FLD = 'A')
              MOVE 'COL' TO MSG-COD-SIST
              MOVE 'CRP    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE '00' TO FIO-STAT
              MOVE 'S' TO SCR-IERR                                      
              GO TO FIN-PUT-TAB
           ELSE
              MOVE '00' TO FIO-STAT
              GO TO FIN-PUT-TAB.

      *HRS-INI 25-JUN-2009 SE ADAPTA LECTURA HACIA ATRAS
           IF FRM-IND-SGTE IN CRP-FLD = 'A'
               SUBTRACT 1 FROM WSS-F IN WSS-VARI
           ELSE
      *HRS-FIN 25-JUN-2009
           ADD 1 TO WSS-F IN WSS-VARI.
           GO TO LUP-PUT-TAB.
       FIN-PUT-TAB.
           MOVE 1                  TO QUE-NITM.
           MOVE CRP-LEN-TSCRP-MAS  TO QUE-LITM.
           MOVE CRP-NOM-TSCRP-MAS  TO QUE-COLA.

           IF WSS-NITM > WSS-NITMMAX
              MOVE 'S'           TO CRP-MAS-IND-HMAS
           ELSE
              MOVE 'N'           TO CRP-MAS-IND-HMAS.

           MOVE WSS-NITM         TO WSS-NUM-ICUR.
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.
           MOVE WSS-NUM-ICUR-ALF TO CRP-MAS-NUM-TOCC.
           MOVE CRP-MAS-ITEM     TO QUE-ITEM.
           MOVE QUE-PUT          TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF WSS-NITM > WSS-IMAX
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'CRPTABDMAS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       EXT-PUT-TAB.
           EXIT.

       SET-FRM-IFLD SECTION.
       INI-SET-FRM-IFLD.
           IF SCR-OFRM-SRV
                MOVE 1 TO FRM-IFLD
           ELSE
                ADD 1 TO FRM-IFLD.
       FIN-SET-FRM-IFLD.
           EXIT.
      *--------------------------------------------
       SCRL-TAB-CRP SECTION.
       INI-SCRL-TAB-CRP.
           MOVE SPACES TO MSG-COD-MENS.

           IF FRM-FFLD = FRM-FFLD-PF6
               PERFORM PUT-TAB-ASC
           ELSE
               PERFORM PUT-TAB-DSC.
           IF MSG-COD-MENS > SPACES
               MOVE 'COL' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-CRP.
           EXIT.

      *--------------------------------------------
       PUT-TAB-CRP SECTION.
       INI-PUT-TAB-CRP.
           PERFORM CLR-WSS.
           MOVE WSS-CAI-IPLL-V(WSS-F) TO WSS-CAI-IPLL-CRP IN CRP-TS.
           MOVE WSS-IIC-IPLL-V(WSS-F) TO WSS-IIC-IPLL-CRP IN CRP-TS.
           MOVE WSS-NUM-IPLL-V(WSS-F) TO WSS-NUM-IPLL-CRP IN CRP-TS.    
           MOVE WSS-NUM-NREG-V(WSS-F) TO WSS-NUM-NREG-CRP IN CRP-TS.
           MOVE WSS-FEC-FPAG-V(WSS-F) TO WSS-FEC-FPAG-CRP IN CRP-TS.
           MOVE WSS-FEC-FVEN-V(WSS-F) TO WSS-FEC-FVEN-CRP IN CRP-TS.
           MOVE WSS-VAL-TOTA-V(WSS-F) TO WSS-VAL-TOTA-CRP IN CRP-TS.
           MOVE WSS-MSC-STAT-V(WSS-F) TO WSS-MSC-STAT-CRP IN CRP-TS.
           MOVE WSS-IND-FPAG-V(WSS-F) TO WSS-IND-FPAG-CRP IN CRP-TS.
           MOVE WSS-IND-TCAR-V(WSS-F) TO WSS-IND-TCAR-CRP IN CRP-TS.
           MOVE WSS-CAI-ICYA-V(WSS-F) TO WSS-CAI-ICYA-CRP IN CRP-TS.
           MOVE WSS-IIC-ICYA-V(WSS-F) TO WSS-IIC-ICYA-CRP IN CRP-TS.
           MOVE WSS-CAI-ICVN-V(WSS-F) TO WSS-CAI-ICVN-CRP IN CRP-TS.
           MOVE WSS-IIC-ICVN-V(WSS-F) TO WSS-IIC-ICVN-CRP IN CRP-TS.
           MOVE WSS-COD-SITU-V(WSS-F) TO WSS-COD-SITU-CRP IN CRP-TS.    
           MOVE WSS-GLS-REFE-V(WSS-F) TO WSS-GLS-REFE-CRP IN CRP-TS.    
           MOVE WSS-NUM-ICLI-V(WSS-F) TO WSS-NUM-ICLI-CRP IN CRP-TS.
           MOVE WSS-VRF-ICLI-V(WSS-F) TO WSS-VRF-ICLI-CRP IN CRP-TS.
           MOVE WSS-IND-ICLI-V(WSS-F) TO WSS-IND-ICLI-CRP IN CRP-TS.
           MOVE WSS-GLS-ICLI-V(WSS-F) TO WSS-GLS-ICLI-CRP IN CRP-TS.

           MOVE WSS-CAI-IPLL-CRP IN CRP-TS TO WSS-CAI-IPLL-FRM.
           MOVE WSS-IIC-IPLL-CRP IN CRP-TS TO WSS-IIC-IPLL-FRM.
           MOVE WSS-NUM-IPLL-CRP IN CRP-TS TO WSS-NUM-IPLL-FRM.         
           MOVE WSS-NUM-NREG-CRP IN CRP-TS TO WSS-NUM-NREG-FRM.
           MOVE WSS-FEC-FPAG-CRP IN CRP-TS TO WSS-FEC-FPAG-FRM.
           MOVE WSS-FEC-FVEN-CRP IN CRP-TS TO WSS-FEC-FVEN-FRM.
           MOVE WSS-VAL-TOTA-CRP IN CRP-TS TO WSS-VAL-TOTA-FRM.
           MOVE WSS-MSC-STAT-CRP IN CRP-TS TO WSS-MSC-STAT-FRM.
           MOVE WSS-IND-FPAG-CRP IN CRP-TS TO WSS-IND-FPAG-FRM.
           MOVE WSS-IND-TCAR-CRP IN CRP-TS TO WSS-IND-TCAR-FRM.
           MOVE WSS-CAI-ICYA-CRP IN CRP-TS TO WSS-CAI-ICYA-FRM.
           MOVE WSS-IIC-ICYA-CRP IN CRP-TS TO WSS-IIC-ICYA-FRM.
           MOVE WSS-CAI-ICVN-CRP IN CRP-TS TO WSS-CAI-ICVN-FRM.
           MOVE WSS-IIC-ICVN-CRP IN CRP-TS TO WSS-IIC-ICVN-FRM.
           MOVE WSS-COD-SITU-CRP IN CRP-TS TO WSS-COD-SITU-FRM.         
           MOVE WSS-GLS-REFE-CRP IN CRP-TS TO WSS-GLS-REFE-FRM.         
           MOVE WSS-NUM-ICLI-CRP IN CRP-TS TO WSS-NUM-ICLI-FRM.
           MOVE WSS-VRF-ICLI-CRP IN CRP-TS TO WSS-VRF-ICLI-FRM.
           MOVE WSS-IND-ICLI-CRP IN CRP-TS TO WSS-IND-ICLI-FRM.
           MOVE WSS-GLS-ICLI-CRP IN CRP-TS TO WSS-GLS-ICLI-FRM.

      *HRS-INI 25-JUN-2009 SE ADAPTA LECTURA HACIA ATRAS
           IF FRM-IND-SGTE IN CRP-FLD = 'A' AND
              WSS-F IN WSS-VARI = WSS-IMAX
              MOVE 1 TO FRM-IFLD.
      *HRS-FIN 25-JUN-2009
           IF FRM-IFLD > WSS-IMAX
              GO TO FIN-PUT-TAB-CRP.

           MOVE WSS-CAI-IPLL-CRP IN CRP-TS TO 
               FRM-CAI-IPLL IN CRP-FLD(FRM-IFLD).
           MOVE WSS-IIC-IPLL-CRP IN CRP-TS TO 
               FRM-IIC-IPLL IN CRP-FLD(FRM-IFLD).
           MOVE WSS-NUM-IPLL-CRP IN CRP-TS TO                           
                FRM-NUM-IPLL IN CRP-FLD(FRM-IFLD).                      
           MOVE WSS-NUM-NREG-CRP IN CRP-TS TO 
               FRM-NUM-NREG IN CRP-FLD(FRM-IFLD).
           MOVE WSS-FEC-FPAG-CRP IN CRP-TS TO 
               FRM-FEC-FPAG IN CRP-FLD(FRM-IFLD).
           MOVE WSS-FEC-FVEN-CRP IN CRP-TS TO 
               FRM-FEC-FVEN IN CRP-FLD(FRM-IFLD).
           MOVE WSS-VAL-TOTA-CRP IN CRP-TS TO 
               FRM-VAL-TOTA IN CRP-FLD(FRM-IFLD).
           MOVE WSS-MSC-STAT-CRP IN CRP-TS TO 
               FRM-MSC-STAT IN CRP-FLD(FRM-IFLD).
           MOVE WSS-IND-FPAG-CRP IN CRP-TS TO 
               FRM-IND-FPAG IN CRP-FLD(FRM-IFLD).
           MOVE WSS-IND-TCAR-CRP IN CRP-TS TO 
               FRM-IND-TCAR IN CRP-FLD(FRM-IFLD).
           MOVE WSS-CAI-ICYA-CRP IN CRP-TS TO 
               FRM-CAI-ICYA IN CRP-FLD(FRM-IFLD).
           MOVE WSS-IIC-ICYA-CRP IN CRP-TS TO 
               FRM-IIC-ICYA IN CRP-FLD(FRM-IFLD).
           MOVE WSS-CAI-ICVN-CRP IN CRP-TS TO 
               FRM-CAI-ICVN IN CRP-FLD(FRM-IFLD).
           MOVE WSS-IIC-ICVN-CRP IN CRP-TS TO 
               FRM-IIC-ICVN IN CRP-FLD(FRM-IFLD).
           MOVE WSS-COD-SITU-CRP IN CRP-TS TO                           
                FRM-COD-SITU IN CRP-FLD(FRM-IFLD).                      
           MOVE WSS-GLS-REFE-CRP IN CRP-TS TO                           
                FRM-GLS-REFE IN CRP-FLD(FRM-IFLD).                      
           MOVE WSS-NUM-ICLI-CRP IN CRP-TS TO
                FRM-NUM-ICLI IN CRP-FLD(FRM-IFLD).
           MOVE WSS-VRF-ICLI-CRP IN CRP-TS TO
                FRM-VRF-ICLI IN CRP-FLD(FRM-IFLD).
           MOVE WSS-IND-ICLI-CRP IN CRP-TS TO
                FRM-IND-ICLI IN CRP-FLD(FRM-IFLD).
           MOVE WSS-GLS-ICLI-CRP IN CRP-TS TO
                FRM-GLS-ICLI IN CRP-FLD(FRM-IFLD).
       FIN-PUT-TAB-CRP.
           EXIT.
      *----------------------------------------------------
       PUT-CRP-QUE SECTION.
       INI-PUT-CRP-QUE.
           PERFORM PRO-FLD-QUE.
           ADD 1 TO WSS-NITM.
           MOVE CRP-NOM-TSCRP    TO QUE-COLA.
           MOVE CRP-LEN-TSCRP    TO QUE-LITM.
           MOVE WSS-NITM         TO QUE-NITM.
           MOVE SPACES           TO QUE-ITEM.
           IF SCR-OFRM-SRV
              MOVE CRP-TS TO QUE-ITEM                                   
           ELSE
              MOVE WSS-CAI-IPLL-IDX-FRM  TO QUE-ITEM.
           MOVE QUE-PUT  TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
       FIN-PUT-CRP-QUE.
           EXIT.
      *----------------------------------------------------

       PUT-TAB-ASC SECTION.
       INI-PUT-TAB-ASC.
      *HRS-INI 25-JUN-2009 SE ADAPTA LECTURA HACIA ATRAS
           IF FRM-IND-SGTE IN CRP-FLD = 'A'
              MOVE 'FPF    INV' TO MSG-COD-MENS
              GO TO EXT-PUT-TAB-ASC.
      *HRS-FIN 25-JUN-2009
           MOVE SPACES TO MSG-COD-MENS.
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.
           MOVE 0 TO FRM-IFLD.
       LUP-PUT-TAB-ASC.
           ADD 1              TO QUE-NITM.
           MOVE CRP-NOM-TSCRP TO QUE-COLA.
           MOVE CRP-LEN-TSCRP TO QUE-LITM.
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD = 0
                   MOVE 'CRPTABNONXT' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-ASC
               ELSE
                   GO TO FIN-PUT-TAB-ASC.
           ADD 1 TO FRM-IFLD.
           MOVE QUE-ITEM TO FRM-CAI-IPLL-IDX(FRM-IFLD).
           IF FRM-IFLD < WSS-IMAX
               GO TO LUP-PUT-TAB-ASC.
       FIN-PUT-TAB-ASC.
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX
               ADD 1 FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1
                                 UNTIL WSS-I > WSS-IMAX.
           IF FRM-IFLD > 0
               ADD 1 TO SCR-NUM-PANT.
           IF FRM-IFLD = WSS-IMAX
               ADD 1              TO QUE-NITM
               MOVE CRP-NOM-TSCRP TO QUE-COLA
               MOVE CRP-LEN-TSCRP TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT = QUE-STAT-OKS
                   MOVE 'COL'        TO MSG-COD-SIST
                   MOVE 'CRPTABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-ASC.
           EXIT.

       PUT-TAB-DSC SECTION.
       INI-PUT-TAB-DSC.
      *HRS-INI 25-JUN-2009 SE ADAPTA LECTURA HACIA ATRAS
           IF FRM-IND-SGTE IN CRP-FLD = 'A'
              MOVE 'FPF    INV' TO MSG-COD-MENS
              GO TO EXT-PUT-TAB-DSC.
      *HRS-FIN 25-JUN-2009
           MOVE SPACES TO MSG-COD-MENS.
           IF SCR-NUM-PANT = 1
               MOVE 'CRPTABNOPRV' TO MSG-COD-MENS
               GO TO EXT-PUT-TAB-DSC.

           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.
           ADD 1 TO QUE-NITM.
           ADD 1 WSS-IMAX GIVING FRM-IFLD.
       LUP-PUT-TAB-DSC.
           SUBTRACT  1 FROM QUE-NITM.
           MOVE CRP-NOM-TSCRP TO QUE-COLA
           MOVE CRP-LEN-TSCRP TO QUE-LITM
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD > WSS-IMAX
                   MOVE 'CRPTABNOPRV' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-DSC
               ELSE
                   GO TO FIN-PUT-TAB-DSC.
           SUBTRACT 1 FROM FRM-IFLD.
           MOVE QUE-ITEM TO FRM-CAI-IPLL-IDX(FRM-IFLD).
           IF FRM-IFLD > 1
               GO TO LUP-PUT-TAB-DSC.
       FIN-PUT-TAB-DSC.

           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1
                                 UNTIL WSS-I < 1.

           IF FRM-IFLD NOT > WSS-IMAX
               SUBTRACT 1 FROM SCR-NUM-PANT
               MOVE 'COL'        TO MSG-COD-SIST
               MOVE 'CRPTABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-DSC.
           EXIT.

       COPY GNSBGCPT.
       COPY GNSBGSTR.

       CLR-FRM SECTION.
       INI-CLR-FRM.
           MOVE SPACES TO FRM-CAI-IPLL IN CRP-FLD(WSS-I).
           MOVE ZEROES TO FRM-IIC-IPLL IN CRP-FLD(WSS-I).
           MOVE ZEROES TO FRM-NUM-IPLL IN CRP-FLD(WSS-I).               
           MOVE ZEROES TO FRM-NUM-NREG IN CRP-FLD(WSS-I).
           MOVE SPACES TO FRM-FEC-FPAG IN CRP-FLD(WSS-I).
           MOVE SPACES TO FRM-FEC-FVEN IN CRP-FLD(WSS-I).
           MOVE ZEROES TO FRM-VAL-TOTA IN CRP-FLD(WSS-I).
           MOVE SPACES TO FRM-MSC-STAT IN CRP-FLD(WSS-I).
           MOVE SPACES TO FRM-IND-FPAG IN CRP-FLD(WSS-I).
           MOVE SPACES TO FRM-IND-TCAR IN CRP-FLD(WSS-I).
           MOVE SPACES TO FRM-CAI-ICYA IN CRP-FLD(WSS-I).
           MOVE SPACES TO FRM-IIC-ICYA IN CRP-FLD(WSS-I).
           MOVE SPACES TO FRM-CAI-ICVN IN CRP-FLD(WSS-I).
           MOVE ZEROES TO FRM-IIC-ICVN IN CRP-FLD(WSS-I).
           MOVE SPACES TO FRM-COD-SITU IN CRP-FLD(WSS-I).               
           MOVE SPACES TO FRM-GLS-REFE IN CRP-FLD(WSS-I).               
           MOVE ZEROES TO FRM-NUM-ICLI IN CRP-FLD(WSS-I).
           MOVE SPACES TO FRM-VRF-ICLI IN CRP-FLD(WSS-I).
           MOVE SPACES TO FRM-IND-ICLI IN CRP-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-ICLI IN CRP-FLD(WSS-I).
       FIN-CLR-FRM.
           EXIT.

       CLR-WSS SECTION.
       INI-CLR-WSS.
           MOVE SPACES TO WSS-CAI-IPLL-CRP IN CRP-TS.
           MOVE ZEROES TO WSS-IIC-IPLL-CRP IN CRP-TS.
           MOVE ZEROES TO WSS-NUM-IPLL-CRP IN CRP-TS.                   
           MOVE ZEROES TO WSS-NUM-NREG-CRP IN CRP-TS.
           MOVE SPACES TO WSS-FEC-FPAG-CRP IN CRP-TS.
           MOVE SPACES TO WSS-FEC-FVEN-CRP IN CRP-TS.
           MOVE ZEROES TO WSS-VAL-TOTA-CRP IN CRP-TS.
           MOVE SPACES TO WSS-MSC-STAT-CRP IN CRP-TS.
           MOVE SPACES TO WSS-IND-FPAG-CRP IN CRP-TS.
           MOVE SPACES TO WSS-IND-TCAR-CRP IN CRP-TS.
           MOVE SPACES TO WSS-CAI-ICYA-CRP IN CRP-TS.
           MOVE SPACES TO WSS-IIC-ICYA-CRP IN CRP-TS.
           MOVE SPACES TO WSS-CAI-ICVN-CRP IN CRP-TS.
           MOVE ZEROES TO WSS-IIC-ICVN-CRP IN CRP-TS.
           MOVE SPACES TO WSS-COD-SITU-CRP IN CRP-TS.                   
           MOVE SPACES TO WSS-GLS-REFE-CRP IN CRP-TS.                   
           MOVE ZEROES TO WSS-NUM-ICLI-CRP IN CRP-TS.
           MOVE SPACES TO WSS-VRF-ICLI-CRP IN CRP-TS.
           MOVE SPACES TO WSS-IND-ICLI-CRP IN CRP-TS.
           MOVE SPACES TO WSS-GLS-ICLI-CRP IN CRP-TS.

           IF SCR-OFRM-SRV
               PERFORM CLR-FRM-MTX.
       FIN-CLR-WSS.
           EXIT.

       CLR-FRM-MTX SECTION.
       INI-CLR-FRM-MTX.
           MOVE SPACES TO FRM-CAI-IPLL IN CRP-FLD(1).
           MOVE ZEROES TO FRM-IIC-IPLL IN CRP-FLD(1).
           MOVE ZEROES TO FRM-NUM-IPLL IN CRP-FLD(1).                   
           MOVE ZEROES TO FRM-NUM-NREG IN CRP-FLD(1).
           MOVE SPACES TO FRM-FEC-FPAG IN CRP-FLD(1).
           MOVE SPACES TO FRM-FEC-FVEN IN CRP-FLD(1).
           MOVE ZEROES TO FRM-VAL-TOTA IN CRP-FLD(1).
           MOVE SPACES TO FRM-MSC-STAT IN CRP-FLD(1).
           MOVE SPACES TO FRM-IND-FPAG IN CRP-FLD(1).
           MOVE SPACES TO FRM-IND-TCAR IN CRP-FLD(1).
           MOVE SPACES TO FRM-CAI-ICYA IN CRP-FLD(1).
           MOVE SPACES TO FRM-IIC-ICYA IN CRP-FLD(1).
           MOVE SPACES TO FRM-CAI-ICVN IN CRP-FLD(1).
           MOVE ZEROES TO FRM-IIC-ICVN IN CRP-FLD(1).
           MOVE SPACES TO FRM-COD-SITU IN CRP-FLD(1).                   
           MOVE SPACES TO FRM-GLS-REFE IN CRP-FLD(1).                   
           MOVE ZEROES TO FRM-NUM-ICLI IN CRP-FLD(1).
           MOVE SPACES TO FRM-VRF-ICLI IN CRP-FLD(1).
           MOVE SPACES TO FRM-IND-ICLI IN CRP-FLD(1).
           MOVE SPACES TO FRM-GLS-ICLI IN CRP-FLD(1).
       FIN-CLR-FRM-MTX.
           EXIT.

       CRG-VEC-CRP SECTION.
       INI-CRG-VEC-CRP.
           ADD 1 TO WSS-J.
           IF WSS-J IN WSS-VARI > WSS-JMAX IN WSS-VARI
               GO TO FIN-CRG-VEC-CRP.

           PERFORM CLR-CRP-VEC.
           MOVE PLL-CAI-IPLL IN PLL TO
                             WSS-CAI-IPLL-V IN WSS-VARI(WSS-J).
           MOVE PLL-IIC-IPLL IN PLL TO
                             WSS-IIC-IPLL-V IN WSS-VARI(WSS-J).
           MOVE PLL-NUM-IPLL IN PLL TO                                  
                             WSS-NUM-IPLL-V IN WSS-VARI(WSS-J).         
           MOVE PLL-NUM-RDPL IN PLL TO
                             WSS-NUM-NREG-V IN WSS-VARI(WSS-J).
           MOVE PLL-VAL-TPAG IN PLL TO
                             WSS-VAL-TOTA-V IN WSS-VARI(WSS-J).
           MOVE PLL-MSC-STAT IN PLL TO
                             WSS-MSC-STAT-V IN WSS-VARI(WSS-J).
           MOVE PLL-IND-FPAG IN PLL TO
                             WSS-IND-FPAG-V IN WSS-VARI(WSS-J).
           MOVE PLL-IND-TCAR IN PLL TO
                             WSS-IND-TCAR-V IN WSS-VARI(WSS-J).
           MOVE PLL-CAI-ICYA IN PLL TO
                             WSS-CAI-ICYA-V IN WSS-VARI(WSS-J).
           MOVE PLL-IIC-ICYA IN PLL TO
                             WSS-IIC-ICYA-V IN WSS-VARI(WSS-J).
           MOVE PLL-CAI-ICVN IN PLL TO
                             WSS-CAI-ICVN-V IN WSS-VARI(WSS-J).
           MOVE PLL-IIC-ICVN IN PLL TO
                             WSS-IIC-ICVN-V IN WSS-VARI(WSS-J).
           MOVE PLL-COD-SITU IN PLL TO                                  
                             WSS-COD-SITU-V IN WSS-VARI(WSS-J).         
           MOVE PLL-GLS-REFE IN PLL TO                                  
                             WSS-GLS-REFE-V IN WSS-VARI(WSS-J).         
           MOVE PLL-NUM-SPGO IN PLL TO WSS-NUM-STRA IN WSS-VARI.
           MOVE PLL-NUM-APGO IN PLL TO WSS-NUM-ATRA IN WSS-VARI.
           MOVE PLL-NUM-MPGO IN PLL TO WSS-NUM-MTRA IN WSS-VARI.
           MOVE PLL-NUM-DPGO IN PLL TO WSS-NUM-DTRA IN WSS-VARI.
           MOVE WSS-FEC-FTRA IN WSS-VARI TO
                             WSS-FEC-FPAG-V IN WSS-VARI(WSS-J).
           MOVE PLL-NUM-SVEN IN PLL TO WSS-NUM-STRA IN WSS-VARI.
           MOVE PLL-NUM-AVEN IN PLL TO WSS-NUM-ATRA IN WSS-VARI.
           MOVE PLL-NUM-MVEN IN PLL TO WSS-NUM-MTRA IN WSS-VARI.
           MOVE PLL-NUM-DVEN IN PLL TO WSS-NUM-DTRA IN WSS-VARI.
           MOVE WSS-FEC-FTRA IN WSS-VARI TO
                             WSS-FEC-FVEN-V IN WSS-VARI(WSS-J).
           MOVE SCR-CIC-ICLI TO DBC-CIC-ICLI IN DBC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           MOVE DBC-NUM-ICLI IN DBC TO
                             WSS-NUM-ICLI-V IN WSS-VARI(WSS-J).
           MOVE DBC-VRF-ICLI IN DBC TO
                             WSS-VRF-ICLI-V IN WSS-VARI(WSS-J).
           MOVE DBC-IND-ICLI IN DBC TO
                             WSS-IND-ICLI-V IN WSS-VARI(WSS-J).
           MOVE DBC-GLS-ICLI IN DBC TO
                             WSS-GLS-ICLI-V IN WSS-VARI(WSS-J).
       FIN-CRG-VEC-CRP.
           EXIT.

       CLR-CRP-VEC SECTION.
       INI-CLR-CRP-VEC.
           MOVE SPACES TO WSS-CAI-IPLL-V(WSS-J).
           MOVE ZEROES TO WSS-IIC-IPLL-V(WSS-J).
           MOVE ZEROES TO WSS-NUM-IPLL-V(WSS-J).                        
           MOVE ZEROES TO WSS-NUM-NREG-V(WSS-J).
           MOVE SPACES TO WSS-FEC-FPAG-V(WSS-J).
           MOVE SPACES TO WSS-FEC-FVEN-V(WSS-J).
           MOVE ZEROES TO WSS-VAL-TOTA-V(WSS-J).
           MOVE SPACES TO WSS-MSC-STAT-V(WSS-J).
           MOVE SPACES TO WSS-IND-FPAG-V(WSS-J).
           MOVE SPACES TO WSS-IND-TCAR-V(WSS-J).
           MOVE SPACES TO WSS-CAI-ICYA-V(WSS-J).
           MOVE SPACES TO WSS-IIC-ICYA-V(WSS-J).
           MOVE SPACES TO WSS-CAI-ICVN-V(WSS-J).
           MOVE ZEROES TO WSS-IIC-ICVN-V(WSS-J).
           MOVE SPACES TO WSS-COD-SITU-V(WSS-J).                        
           MOVE SPACES TO WSS-GLS-REFE-V(WSS-J).                        
           MOVE ZEROES TO WSS-NUM-ICLI-V(WSS-J).
           MOVE SPACES TO WSS-VRF-ICLI-V(WSS-J).
           MOVE SPACES TO WSS-IND-ICLI-V(WSS-J).
           MOVE SPACES TO WSS-GLS-ICLI-V(WSS-J).
       FIN-CLR-CRP-VEC.
           EXIT.

       PRO-FLD-QUE SECTION.
       INI-PRO-FLD-QUE.
           MOVE 'P' TO WSS-CAI-IPLL-ATR.       
           MOVE 'P' TO WSS-IIC-IPLL-ATR.       
           MOVE 'P' TO WSS-NUM-IPLL-ATR.                                
           MOVE 'P' TO WSS-NUM-NREG-ATR.       
           MOVE 'P' TO WSS-FEC-FPAG-ATR.       
           MOVE 'P' TO WSS-FEC-FVEN-ATR.       
           MOVE 'P' TO WSS-VAL-TOTA-ATR.       
           MOVE 'P' TO WSS-MSC-STAT-ATR.       
           MOVE 'P' TO WSS-IND-FPAG-ATR.       
           MOVE 'P' TO WSS-IND-TCAR-ATR.       
           MOVE 'P' TO WSS-CAI-ICYA-ATR.       
           MOVE 'P' TO WSS-IIC-ICYA-ATR.       
           MOVE 'P' TO WSS-CAI-ICVN-ATR.       
           MOVE 'P' TO WSS-IIC-ICVN-ATR.       
           MOVE 'P' TO WSS-COD-SITU-ATR.                                
           MOVE 'P' TO WSS-GLS-REFE-ATR.                                
           MOVE 'P' TO WSS-NUM-ICLI-ATR.
           MOVE 'P' TO WSS-VRF-ICLI-ATR.
           MOVE 'P' TO WSS-IND-ICLI-ATR.
           MOVE 'P' TO WSS-GLS-ICLI-ATR.
       FIN-PRO-FLD-QUE.
           EXIT.
