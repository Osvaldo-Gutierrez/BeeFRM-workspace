*%! Codigo adicional para archivo COLPQCPR
*% LARGO_MI_COMMAREA = 20
*% PGM_PTC = TRUE
*% IF MI_COMMAREA                                                 
      *<<<< MI_COMMAREA                                                 
           03 CMA-VARI.                                                 
              05 SCR-IMAX      COMP                   PIC S9(04).       
              05 SCR-I         COMP                   PIC S9(04).       
      *>>>>
*% END
*% IF WSS                                                         
      *<<<< WSS                                                         
       COPY COLWGNOP.                                                   
       COPY COLWGCPR.
       COPY GNSWGCPT.
       COPY COLBRROV.                                                   
       COPY COLBRICG.
       COPY COLBRDLC.
       COPY COLBRRCV.
       COPY COLBRRDC.
       COPY COLBRCAN.                                                   
       COPY COLBRCYA.                                                   
       COPY SGCBRDBC.                                                   
       COPY TABBRCAM.
       COPY GNSWGPES.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CYA-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DLC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CAM-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-ROV-REQA==.   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RCV-REQA==.      
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-ICG-REQA==. 
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RDC-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CAN-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.        
                                                   
       01  WSS-VARI.                                                    
           03  WSS-COD-TCYA.                                            
               05 WSS-GLS-TCYA                   PIC X(02).             
               05 WSS-IND-TCYA                   PIC X(03).             
           03  WSS-IND-RENO    VALUE SPACES      PIC X(03).             
           03  WSS-CPR-IMAX    VALUE ZEROES      PIC 9(03).
           03  WSS-BUS-CLI     VALUE 'N'         PIC X(01).             
           03  WSS-FEC-FINI.                                            
               05 WSS-NUM-SINI                   PIC 9(02).             
               05 WSS-NUM-AINI                   PIC 9(02).             
               05 WSS-NUM-MINI                   PIC 9(02).             
               05 WSS-NUM-DINI                   PIC 9(02).             
           03  WSS-FEC-FTER.                                            
               05 WSS-NUM-STER                   PIC 9(02).             
               05 WSS-NUM-ATER                   PIC 9(02).             
               05 WSS-NUM-MTER                   PIC 9(02).             
               05 WSS-NUM-DTER                   PIC 9(02).             
           03  WSS-SGV-RENO   COMP-3 VALUE ZEROES PIC S9(11)V9(04).     
           03  RCAN-CAN-KEY-ICAN                  PIC X(15).            
           03  WSS-TOT-RCAN   VALUE ZEROES        PIC 9(04).            
           03  WSS-TOT-PTTS   VALUE ZEROES        PIC 9(04).            
           03  WSS-KEY-IVCT                       PIC X(15).
           03  WSS-I                              PIC 9(02).            
           03  WSS-FEC-CNT1                       PIC X(08).
           03  WSS-FEC-CNT                        PIC X(08).
           03  WSS-VAL-CNT     VALUE ZEROES       PIC S9(11)V9(04).
           03  WSS-VAL-CNT1    VALUE ZEROES       PIC S9(11)V9(04).
           03  WSS-COD-PESO                       PIC X(06).         
           03  WSS-CIC-IOPC.                                            
               05 WSS-CAI-IOPC                    PIC X(04).            
               05 WSS-IIC-IOPC                    PIC X(08).        
           03  WSS-CIC-IOPE.                                            
               05 WSS-CAI-IOPE                    PIC X(04).            
               05 WSS-IIC-IOPE                    PIC X(08).  
      *>>>>
*% END
*% IF INI_FIO_IBM
      *<<<<
           IF SCR-OFRM-SRV                                              
              MOVE TSK-TERM-ALF TO CPR-TERM                             
           ELSE                                                         
              MOVE EIBTRMID     TO CPR-TERM.                            
      *>>>>
*% END
*% IF PFS_SCR_ACC                                                 
      *<<<< PFS_SCR_ACC                                                 
           IF FRM-FFLD = FRM-FFLD-PF6                                   
                    OR   FRM-FFLD-PF5                                   
               PERFORM VAL-NUM-KEY-CPR                                  
               PERFORM VAL-NUM-FLD-CPR                                  
               PERFORM RST-STAT                                         
               PERFORM FND-SCR-KEY THRU FIN-SCR-KEY                     
               IF SCR-STAT NOT = SCR-STAT-ABT                           
                   PERFORM SCR-TRL                                      
               ELSE                                                     
                   PERFORM DESSCRTRL                                    
                   GO TO FIN-SCR-ACC.                                   
      *>>>>
*% END
*% IF INI_FND_KEY                                                 
      *<<<< INI_FND_KEY                                                 
           MOVE SPACES TO FRM-MEN1.                                     
           IF FRM-FFLD = FRM-FFLD-PF5                                   
               MOVE PGM-SNUL TO PGM-STAT-OPC                            
               MOVE PGM-SNUL TO PGM-STAT-OPC                            
               SUBTRACT 1 FROM SCR-I GIVING CPR-I                       
               IF CPR-I NOT > ZEROES                                    
                   MOVE 'OPC PROPRV' TO MSG-COD-MENS                    
                   GO TO ERR-FND-KEY                                    
               ELSE                                                     
                   GO TO CARGA-CPR                                      
           ELSE                                                         
           IF FRM-FFLD = FRM-FFLD-PF6                                   
               MOVE PGM-SNUL TO PGM-STAT-OPC                           
               MOVE PGM-SNUL TO PGM-STAT-OPC                            
               ADD 1 SCR-I GIVING CPR-I                                 
               IF CPR-I > SCR-IMAX                                      
                   MOVE 'OPC PRONXT' TO MSG-COD-MENS                    
                   GO TO ERR-FND-KEY                                    
               ELSE                                                     
                   GO TO CARGA-CPR.                                     
           GO TO CON-FND-KEY.                                           
      *>>>>
*% END
*% IF FIN_FND_KEY                                                 
      *<<<< FIN_FND_KEY                                                 
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-FND-KEY.                                       
       CON-FND-KEY.                                                     
              
           PERFORM KEY-ALL-CPR.                                         
            IF OPC-IIC-IOPC IN CPR-FLD > ZEROES                         
               GO TO DSP-CAR-MOV-OPC.                                   
            MOVE DBC-NUM-ICLI IN CPR-FLD TO DBC-NUM-ICLI IN DBC         
            MOVE DBC-IND-ICLI IN CPR-FLD TO DBC-IND-ICLI IN DBC         
            MOVE DBC-GLS-ICLI IN CPR-FLD TO DBC-GLS-ICLI IN DBC         
            MOVE 'DBC-IDC-ICLI' TO FIO-AKEY.                            
            MOVE FIO-GET-KEY  TO FIO-CMND.                              
            PERFORM SGC-FIO-DBC                                         
            GO TO DSP-CAR-MOV-DBC.                                      
       DSP-CAR-MOV-OPC.     
           MOVE OPC-CAI-IOPC IN OPC TO RDC-CAI-IOPC IN RDC.             
           MOVE OPC-IIC-IOPC IN OPC TO RDC-IIC-IOPC IN RDC.             
           MOVE ZEROES              TO RDC-NUM-IDLC IN RDC.             
           MOVE 'RDC-KEY-IDLC' TO FIO-AKEY.                             
           MOVE FIO-GET-NLS TO FIO-CMND.                                
           PERFORM COL-FIO-RDC.                                         
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-FND-KEY.                                       
      *                                                                 
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM COL-FIO-OPC.
      *    IF OPC-IND-RENO IN OPC = SPACES OR                           
      *       OPC-IND-RENO IN OPC NOT = 'PRO'                           
           IF OPC-IND-RENO IN OPC = SPACES                              
               MOVE 'COL'         TO MSG-COD-SIST                       
               MOVE 'OPC NO PRO '    TO MSG-COD-MENS                    
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC IN MSG TO FRM-MENS.                    
           IF OPC-VAL-SCRE IN OPC = ZEROES                              
               MOVE 'COL'         TO MSG-COD-SIST                       
               MOVE 'OPCSINSLDO '    TO MSG-COD-MENS                    
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC IN MSG TO FRM-MENS.                     
           MOVE RDC-CIC-ICLI IN RDC TO DBC-CIC-ICLI IN DBC.             
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM SGC-FIO-DBC.                                         
       DSP-CAR-MOV-DBC.                                         
           IF NOT FIO-STAT-OKS                                          
                GO TO FIN-FND-KEY.                                      
           MOVE FIO-STAT TO PGM-STAT-DBC.                               
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN CPR-FLD.         
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN CPR-FLD.         
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.                        
           PERFORM CPT-BLKS.                                            
           MOVE CPT-STRN TO DBC-GLS-NOMC IN DBC.                        
           MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN CPR-FLD.        

           MOVE ZEROES TO SCR-IMAX.                                     
           MOVE 1 TO SCR-I.                                             
           MOVE '00' TO FIO-STAT.                                       
           PERFORM OBT-DAT-TS.                                          
           MOVE 1 TO SCR-I.                                             
           MOVE SCR-I TO CPR-I.                                         
       CARGA-CPR.                                                       
            MOVE DBC-NUM-ICLI IN CPR-FLD TO DBC-NUM-ICLI IN DBC         
            MOVE DBC-IND-ICLI IN CPR-FLD TO DBC-IND-ICLI IN DBC         
            MOVE DBC-GLS-ICLI IN CPR-FLD TO DBC-GLS-ICLI IN DBC         
            MOVE 'DBC-IDC-ICLI' TO FIO-AKEY.                            
            MOVE FIO-GET-KEY  TO FIO-CMND.                              
            PERFORM SGC-FIO-DBC                                         
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.                        
           PERFORM CPT-BLKS.                                            
           MOVE CPT-STRN TO DBC-GLS-NOMC IN DBC.                        
           MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN CPR-FLD.         
           MOVE FIO-STAT TO PGM-STAT-DBC.                               
           MOVE SPACES TO MSG-COD-MENS.                                 
           IF NOT SCR-OFRM-SRV                                          
           PERFORM PUT-TAB-CPR.                                         
           MOVE '00' TO FIO-STAT.                                       
           GO TO FIN-FND-KEY.                                           
      *                                                                 
       ERR-FND-KEY.                                                     
           MOVE SCR-STAT-CON TO SCR-STAT.                               
                                                                        
           MOVE 'COL'  TO MSG-COD-SIST.                                 
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MEN1.                               
           MOVE '00' TO FIO-STAT.                                       
           GO TO FIN-FND-KEY.                                           
      *>>>>
*% END
*% IF PFS_GET_CON OR PFS_GET_ACC                                  
      *<<<< PFS_GET_CON OR PFS_GET_ACC                                  
           IF NOT (FRM-FFLD = FRM-FFLD-PF6                              
                         OR   FRM-FFLD-PF5)                             
      *>>>>
*% END
*% IF EOF                                                         
      *<<<< EOF                                                         
       COPY COLBFCAN.                                                   
       COPY COLBFRDC.
       COPY COLBFCYA.
       COPY COLBFRCV.
       COPY COLBFICG.
       COPY COLBFDLC.
       COPY COLBFROV.                                                   
       COPY TABBFCAM.                                                  
       COPY GNSBGCPT.
       COPY SGCBFDBC.
       COPY GNSBBTAB.
                                      
       CAR-MOV-ROV SECTION.                                             
       INI-CAR-MOV-ROV.                                                 
            MOVE 'N' TO  WSS-BUS-CLI.                                   
            IF OPC-IIC-IOPC IN CPR-FLD > ZEROES                         
                MOVE OPC-CAI-IOPC IN CPR-FLD TO OPC-CAI-IOPC IN OPC     
                MOVE OPC-IIC-IOPC IN CPR-FLD TO OPC-IIC-IOPC IN OPC     
                MOVE FIO-GET-KEY TO FIO-CMND                            
                PERFORM COL-FIO-OPC                                     
                IF OPC-IND-RENO IN OPC NOT = 'PRO'                      
                   IF FRM-COD-TREN IN CPR-FLD NOT > SPACES              
                      IF OPC-IND-RENO IN OPC NOT > SPACES               
                       MOVE OPC-COD-VCOC IN OPC TO CPR-GLS-VCOC(CPR-I)  
                       MOVE OPC-COD-COOC IN OPC TO CPR-COD-COOC(CPR-I)  
                       MOVE OPC-COD-OFOC IN OPC TO CPR-COD-OFIC(CPR-I)  
                       MOVE OPC-NUM-VCOR IN OPC TO CPR-NUM-VCOR(CPR-I)  
                       MOVE OPC-IND-RENO IN OPC TO CPR-IND-RENO(CPR-I)  
                       MOVE OPC-FEC-FCOL IN OPC TO CPR-FEC-FCAN(CPR-I)  
                       PERFORM OBT-TASA-CPR                             
                       MOVE ICG-SGV-TINT IN ICG TO CPR-SGV-TINT(CPR-I)  
                       MOVE OPC-CAI-IOPC IN OPC TO CPR-CAI-IOPC(CPR-I)  
                       MOVE OPC-IIC-IOPC IN OPC TO CPR-IIC-IOPC(CPR-I)  
                       PERFORM INC-REG-TS                               
                       GO TO FIN-CAR-MOV-ROV                            
                      ELSE                                                 
                          GO TO OPC-CAR-MOV-ROV                         
                   ELSE                                                 
                       GO TO FIN-CAR-MOV-ROV                            
                ELSE                                                    
                    GO TO OPC-CAR-MOV-ROV.                                   
            MOVE 'S' TO WSS-BUS-CLI.                                    
            MOVE DBC-NUM-ICLI IN CPR-FLD TO DBC-NUM-ICLI IN DBC         
            MOVE DBC-IND-ICLI IN CPR-FLD TO DBC-IND-ICLI IN DBC         
            MOVE DBC-GLS-ICLI IN CPR-FLD TO DBC-GLS-ICLI IN DBC         
            MOVE 'DBC-IDC-ICLI' TO FIO-AKEY.                            
            MOVE FIO-GET-KEY  TO FIO-CMND.                              
            PERFORM SGC-FIO-DBC                                         
            MOVE DBC-CIC-ICLI IN DBC TO RDC-CIC-ICLI IN RDC             
            MOVE SPACES              TO RDC-CAI-IOPC IN RDC             
            MOVE ZEROES              TO RDC-NUM-IDLC IN RDC             
            MOVE FIO-GET-NLS TO FIO-CMND.                               
       RDC-CAR-MOV-ROV.                                                 
            PERFORM COL-FIO-RDC                                         
            IF FIO-STAT-OKS AND                                         
               RDC-CIC-ICLI IN RDC = DBC-CIC-ICLI IN DBC AND            
               RDC-NUM-IDLC IN RDC = ZEROES                             
                MOVE RDC-CIC-IOPC IN RDC TO OPC-CIC-IOPC IN OPC         
                MOVE FIO-GET-KEY TO FIO-CMND                            
                PERFORM COL-FIO-OPC                                     
                IF  FIO-STAT-OKS AND                                    
                    OPC-IND-RENO IN OPC = 'PRO' AND
      *PDM      
                    OPC-MSC-STAT IN OPC ='A' AND                     
                    OPC-VAL-SCRE IN OPC > ZEROES                        
                     MOVE OPC-CAI-IOPC IN OPC TO WSS-CAI-IOPE           
                     MOVE OPC-IIC-IOPC IN OPC TO WSS-IIC-IOPE           
                     GO TO LUP-CAR-MOV-ROV                              
                ELSE                                                    
                     MOVE FIO-GET-NXT TO FIO-CMND                       
                     GO TO RDC-CAR-MOV-ROV                              
            ELSE                                                        
                GO TO FIN-CAR-MOV-ROV.                                  
       OPC-CAR-MOV-ROV.                                                 
            MOVE OPC-CAI-IOPC IN CPR-FLD TO OPC-CAI-IOPC IN OPC.        
            MOVE OPC-IIC-IOPC IN CPR-FLD TO OPC-IIC-IOPC IN OPC.        
            MOVE OPC-CAI-IOPC IN CPR-FLD TO WSS-CAI-IOPE.               
            MOVE OPC-IIC-IOPC IN CPR-FLD TO WSS-IIC-IOPE.               
       LUP-CAR-MOV-ROV.                                                 
            MOVE FIO-GET-KEY TO FIO-CMND.                               
            PERFORM COL-FIO-OPC.                                        
            IF OPC-IND-RENO IN OPC NOT > SPACES                         
                MOVE OPC-COD-VCOC IN OPC TO CPR-GLS-VCOC(CPR-I)             
                MOVE OPC-COD-COOC IN OPC TO CPR-COD-COOC(CPR-I)             
                MOVE OPC-COD-OFOC IN OPC TO CPR-COD-OFIC(CPR-I)             
                MOVE OPC-NUM-VCOR IN OPC TO CPR-NUM-VCOR(CPR-I)             
                MOVE OPC-IND-RENO IN OPC TO CPR-IND-RENO(CPR-I)             
                MOVE OPC-FEC-FCOL IN OPC TO CPR-FEC-FCAN(CPR-I)         
                PERFORM OBT-TASA-CPR                                    
                MOVE ICG-SGV-TINT IN ICG TO CPR-SGV-TINT(CPR-I)         
                MOVE WSS-CAI-IOPE        TO CPR-CAI-IOPC(CPR-I)         
                MOVE WSS-IIC-IOPE        TO CPR-IIC-IOPC(CPR-I)         
      *                                                                 
      *         MOVE CAN-CIC-IOPC IN CAN TO CPR-COD-RCAN(CPR-I)         
                PERFORM INC-REG-TS                                      
                IF WSS-BUS-CLI = 'S'                                    
                     MOVE FIO-GET-NXT TO FIO-CMND                       
                     GO TO RDC-CAR-MOV-ROV                              
                ELSE                                                    
                    GO TO FIN-CAR-MOV-ROV.                              
            PERFORM GET-ROV-REN.                                        
            IF FRM-FEC-FTER IN CPR-FLD > ZEROES                         
                MOVE FRM-NUM-SINI IN CPR-FLD TO WSS-NUM-SINI            
                MOVE FRM-NUM-AINI IN CPR-FLD TO WSS-NUM-AINI            
                MOVE FRM-NUM-MINI IN CPR-FLD TO WSS-NUM-MINI            
                MOVE FRM-NUM-DINI IN CPR-FLD TO WSS-NUM-DINI            
                MOVE FRM-NUM-STER IN CPR-FLD TO WSS-NUM-STER            
                MOVE FRM-NUM-ATER IN CPR-FLD TO WSS-NUM-ATER            
                MOVE FRM-NUM-MTER IN CPR-FLD TO WSS-NUM-MTER            
                MOVE FRM-NUM-DTER IN CPR-FLD TO WSS-NUM-DTER            
                IF CAN-FEC-FCAN IN CAN < WSS-FEC-FINI OR                
                   CAN-FEC-FCAN IN CAN > WSS-FEC-FTER                   
                    MOVE CAN-CIC-IOPC IN CAN  TO WSS-CIC-IOPE           
                    MOVE WSS-CIC-IOPE        TO OPC-CIC-IOPC IN OPC     
                    GO TO LUP-CAR-MOV-ROV.                              
                                                                        
            MOVE CAN-CIC-IOPC IN CAN TO CPR-COD-RCAN(CPR-I).            
            MOVE SPACES                  TO ROV-CIC-IOPV IN ROV.        
            MOVE ZEROES                  TO ROV-NUM-IVCT IN ROV.        
      *                                                                 
           MOVE WSS-CAI-IOPE        TO CPR-CAI-IOPC(CPR-I).             
           MOVE WSS-IIC-IOPE        TO CPR-IIC-IOPC(CPR-I).             
           MOVE CAN-FEC-FCAN IN CAN TO CPR-FEC-FCAN(CPR-I).             
           MOVE CAN-NUM-ICAN IN CAN TO CPR-NUM-ICAN(CPR-I).             
      *    MOVE CAN-COD-RCAN IN CAN TO CPR-COD-RCAN(CPR-I).             
           MOVE CAN-SGV-TREN IN CAN TO CPR-SGV-RENO(CPR-I).
           MOVE CAN-SGV-TPAG IN CAN TO CPR-SGV-TPAG(CPR-I).             
                                                                        
           MOVE CAN-CIC-IOPC IN CAN TO RCV-CIC-IOPC IN RCV.             
           MOVE CAN-NUM-ICAN IN CAN TO RCV-NUM-ICAN IN RCV.             
           MOVE SPACES              TO RCV-CAI-IOPV IN RCV.             
           MOVE ZEROES              TO RCV-IIC-IOPV IN RCV.             
           MOVE ZEROES              TO RCV-NUM-IVCT IN RCV.             
           MOVE FIO-GET-NLS TO FIO-CMND.                                
           PERFORM COL-FIO-RCV.                                         
           IF NOT (FIO-STAT-OKS AND                                     
              CAN-CIC-IOPC IN CAN = RCV-CIC-IOPC IN RCV AND               
              CAN-NUM-ICAN IN CAN = RCV-NUM-ICAN IN RCV)                  
               GO TO FIN-CAR-MOV-ROV.                                    


           MOVE RCV-SGV-CAPI IN RCV TO CPR-SGV-CAPI(CPR-I).

           MOVE OPC-COD-VCOC IN OPC TO CPR-GLS-VCOC(CPR-I).             
           MOVE OPC-COD-COOC IN OPC TO CPR-COD-COOC(CPR-I).             
           MOVE OPC-COD-OFOC IN OPC TO CPR-COD-OFIC(CPR-I).             
           MOVE OPC-NUM-VCOR IN OPC TO CPR-NUM-VCOR(CPR-I).             
           MOVE OPC-IND-RENO IN OPC TO CPR-IND-RENO(CPR-I).             
           MOVE OPC-IND-RENO IN OPC TO WSS-IND-RENO.                    
                                                                       
           PERFORM OBT-TASA-CPR.                                        
           MOVE ICG-SGV-TINT IN ICG TO CPR-SGV-TINT(CPR-I).             

           MOVE CPR-COD-RCAN(CPR-I) TO OPC-CIC-IOPC IN OPC.             
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM COL-FIO-OPC.                                         
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-CAR-MOV-ROV.                                    
           MOVE OPC-NUM-IVEN IN OPC TO CPR-NUM-IVEN(CPR-I).

                                                                        
           MOVE CPR-COD-RCAN(CPR-I) TO WSS-CIC-IOPE.                    
           PERFORM INC-REG-TS.                                          
           MOVE WSS-CIC-IOPE        TO OPC-CIC-IOPC IN OPC.             
           IF WSS-IND-RENO  = 'PRO'                                     
           GO TO LUP-CAR-MOV-ROV.                                       
                                                                          
       FIN-CAR-MOV-ROV.                                                 
           EXIT.                                                        

       SRT-CPR-FCAN SECTION.                                            
       INI-SRT-CPR-FCAN.                                                
            SUBTRACT 1 FROM CPR-JMAX GIVING CPR-SMAX.                   
            PERFORM INC-CPR-FCAN VARYING CPR-S FROM 1 BY 1              
            UNTIL CPR-S > CPR-SMAX.                                     
       FIN-SRT-CPR-FCAN.                                                
           EXIT.                                                        
                                                                        
       INC-CPR-FCAN SECTION.                                            
       INI-INC-CPR-FCAN.                                                
            ADD 1 CPR-S GIVING CPR-SMIN.                                
            PERFORM SWT-CPR-FCAN VARYING CPR-SAUX FROM CPR-SMIN BY 1    
            UNTIL CPR-SAUX > CPR-JMAX.                                  
       FIN-INC-CPR-FCAN.                                                
           EXIT.                                                        
                                                                        
       SWT-CPR-FCAN SECTION.                                            
       INI-SWT-CPR-FCAN.                                                
            IF CPR-FEC-FCAN IN CPR-TS(CPR-S) > CPR-FEC-FCAN             
                                           IN CPR-TS(CPR-SAUX)          
                MOVE CPR-FEC-FCAN IN CPR-TS(CPR-S)    TO CPR-AUXI       
                MOVE CPR-FEC-FCAN IN CPR-TS(CPR-SAUX) TO                
                                    CPR-FEC-FCAN IN  CPR-TS(CPR-S)      
                MOVE CPR-AUXI   TO CPR-FEC-FCAN IN CPR-TS(CPR-SAUX).    
       FIN-SWT-CPR-FCAN.                                                
           EXIT.                
                                                                         
       OBT-PARAM SECTION.                                               
       INI-OBT-PARAM.                                                   
       FIN-OBT-PARAM.                                                   
           EXIT.                                                        
                                                                        
       OBT-DAT-TS SECTION.                                              
       INI-OBT-DAT-TS.                                                  
           EXEC CICS HANDLE CONDITION QIDERR(CON-OBT-DAT-TS)            
                                     ITEMERR(CON-OBT-DAT-TS)            
           END-EXEC.                                                    
                                                                        
      *     EXEC CICS DELETEQ TS QUEUE(CPR-NAME-QUE)                    
      *     END-EXEC.                                                   
      *<<<<                                                             
      *    MOVE EIBTRMID      TO CPR-TERM.                              
           MOVE CPR-NAME-QUE   TO QUE-COLA.                             
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
      *>>>>                                                             
       CON-OBT-DAT-TS.                                                  
           MOVE CPR-SI-IMP TO CPR-IMP.                                  
           PERFORM CLR-VEC-TS VARYING CPR-I FROM 1 BY 1 UNTIL           
                   CPR-I > CPR-IMAX.                                    
           MOVE 1 TO CPR-I.                                             
           MOVE OPC-CAI-IOPC IN CPR-FLD TO OPC-CAI-IOPC IN OPC.         
           MOVE OPC-IIC-IOPC IN CPR-FLD TO OPC-IIC-IOPC IN OPC.         
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM COL-FIO-OPC.                                         
           IF OPC-IND-RENO IN OPC = 'PRO'                               
               PERFORM CAR-MOV-ROV                                      
           ELSE                                                         
               PERFORM OBT-OPE-ORI.                                     
                                                                        
       NXT-OBT-DAT-TS.                                                  
           IF CPR-IMP = CPR-SI-IMP                                      
               PERFORM WRT-TS.                                          
       FIN-OBT-DAT-TS.                                                  
           EXIT.                                                        
                                                                        
       PUT-TAB-CPR SECTION.                                             
       INI-PUT-TAB-CPR.                                                 

           PERFORM RED-TS.                                              
           MOVE CPR-IMAX TO WSS-CPR-IMAX.
           PERFORM LLE-DAT-PNT VARYING CPR-I FROM 1 BY 1                
                   UNTIL CPR-I > CPR-IMAX.                              
       FIN-PUT-TAB-CPR.                                                 
           EXIT.                                                        
                                                                        
       LLE-DAT-PNT SECTION.                                             
       INI-LLE-DAT-PNT.                                                 
           MOVE CPR-CAI-IOPC(CPR-I) TO FRM-CAI-IOPC IN CPR-FLD(CPR-I).  
           MOVE CPR-IIC-IOPC(CPR-I) TO FRM-IIC-IOPC IN CPR-FLD(CPR-I).  
           MOVE CPR-FEC-FCAN(CPR-I) TO FRM-FEC-FCAN IN CPR-FLD(CPR-I).  
           MOVE CPR-NUM-ICAN(CPR-I) TO FRM-NUM-ICAN IN CPR-FLD(CPR-I).  
           MOVE CPR-NUM-IVEN(CPR-I) TO FRM-NUM-IVEN IN CPR-FLD(CPR-I).  
           MOVE CPR-SGV-RENO(CPR-I) TO FRM-SGV-RENO IN CPR-FLD(CPR-I).  
           MOVE CPR-SGV-CAPI(CPR-I) TO FRM-SGV-CAPI IN CPR-FLD(CPR-I).  
           MOVE CPR-GLS-VCOC(CPR-I) TO FRM-GLS-VCOC IN CPR-FLD(CPR-I).  
           MOVE CPR-COD-COOC(CPR-I) TO FRM-COD-COOC IN CPR-FLD(CPR-I).  
           MOVE CPR-COD-OFIC(CPR-I) TO FRM-COD-OFIC IN CPR-FLD(CPR-I).  
           MOVE CPR-SGV-TINT(CPR-I) TO FRM-SGV-TINT IN CPR-FLD(CPR-I).  
           MOVE CPR-NUM-VCOR(CPR-I) TO FRM-NUM-VCOR IN CPR-FLD(CPR-I).  
           MOVE CPR-IND-RENO(CPR-I) TO FRM-IND-RENO IN CPR-FLD(CPR-I).
           MOVE CPR-COD-RCAN(CPR-I) TO FRM-COD-RCAN IN CPR-FLD(CPR-I).
           MOVE CPR-SGV-TPAG(CPR-I) TO FRM-SGV-TPAG IN CPR-FLD(CPR-I).    
           SUBTRACT 1 FROM WSS-CPR-IMAX.
       FIN-LLE-DAT-PNT.                                                 
           EXIT.                                                        
                                                                        
       INC-REG-TS SECTION.                                              
       INI-INC-REG-TS.                                                  
            MOVE CPR-SI-IMP TO CPR-IMP.                                 
           ADD 1 TO CPR-I.                                              
           IF CPR-I > CPR-IMAX                                          
               PERFORM WRT-TS                                           
               PERFORM CLR-VEC-TS VARYING CPR-I FROM 1 BY 1             
                                     UNTIL CPR-I > CPR-IMAX             
               MOVE 1 TO CPR-I                                          
               MOVE CPR-NO-IMP TO CPR-IMP.                              
       FIN-INC-REG-TS.                                                  
           EXIT.                                                        
                                                                        
      *<<<<                                                             
      *>>>>                                                             
                                                                        
       WRT-TS SECTION.                                                  
       INI-WRT-TS.                                                      
           ADD 1 TO SCR-IMAX.                                           
      *     EXEC CICS WRITEQ TS QUEUE(CPR-NAME-QUE)                     
      *                         FROM(CPR-TS)                            
      *                         LENGTH(CPR-LONG-TS)                     
      *     END-EXEC.                                                   
      *<<<<                                                             
      *    MOVE EIBTRMID      TO CPR-TERM.                              
           MOVE CPR-NAME-QUE   TO QUE-COLA.                             
           MOVE CPR-TS         TO QUE-ITEM.                             
           MOVE CPR-LONG-TS    TO QUE-LITM.                             
           MOVE QUE-PUT        TO QUE-CMND.                             
           PERFORM GNS-PRO-QUE.                                         
      *>>>>                                                             
       FIN-WRT-TS.                                                      
           EXIT.                                                        
                                                                        
       RED-TS SECTION.                                                  
       INI-RED-TS.                                                      
           MOVE CPR-I TO SCR-I.                                         
           EXEC CICS HANDLE CONDITION QIDERR(FIN-RED-TS)                
                                     ITEMERR(FIN-RED-TS)                
           END-EXEC.                                                    
      *     EXEC CICS READQ TS QUEUE(CPR-NAME-QUE)                      
      *                         INTO(CPR-TS)                            
      *                          LENGTH(CPR-LONG-TS)                    
      *                         ITEM(CPR-I)                             
      *     END-EXEC.                                                   
      *<<<<                         
                                    
      *    MOVE EIBTRMID      TO CPR-TERM.                              
           MOVE CPR-NAME-QUE   TO QUE-COLA.                             
           MOVE CPR-TS         TO QUE-ITEM.                             
           MOVE CPR-LONG-TS    TO QUE-LITM.                             
           MOVE CPR-I              TO QUE-NITM.                         
           MOVE QUE-GET            TO QUE-CMND.                         
           PERFORM GNS-PRO-QUE.                                         
           MOVE QUE-ITEM       TO CPR-TS.                               
      *>>>>                                                             
       FIN-RED-TS.                                                      
           EXIT.                                                        
                                                                        
       CLR-VEC-TS SECTION.                                              
       INI-CLR-VEC-TS.                                                  
           MOVE SPACES TO CPR-CAI-IOPC(CPR-I). 
           MOVE ZEROES TO CPR-IIC-IOPC(CPR-I).
           MOVE ZEROES TO CPR-FEC-FCAN(CPR-I).
           MOVE ZEROES TO CPR-NUM-ICAN(CPR-I).
           MOVE ZEROES TO CPR-NUM-IVEN(CPR-I).
           MOVE ZEROES TO CPR-SGV-RENO(CPR-I).
           MOVE ZEROES TO CPR-SGV-CAPI(CPR-I).
           MOVE SPACES TO CPR-GLS-VCOC(CPR-I).
           MOVE SPACES TO CPR-COD-COOC(CPR-I).
           MOVE ZEROES TO CPR-COD-OFIC(CPR-I).
           MOVE ZEROES TO CPR-SGV-TINT(CPR-I). 
           MOVE ZEROES TO CPR-NUM-VCOR(CPR-I).
           MOVE SPACES TO CPR-IND-RENO(CPR-I).
           MOVE SPACES TO CPR-COD-RCAN(CPR-I).
           MOVE ZEROES TO CPR-SGV-TPAG(CPR-I).
       FIN-CLR-VEC-TS.                                                  
           EXIT.                                                        
                                                                        
       RST-STAT SECTION.                                                
       INI-RST-STAT.                                                    
           MOVE SCR-STPP     TO SCR-STAT.                               
           MOVE SCR-MFLD-NOT TO SCR-MFLD.                               
       FIN-RST-STAT.                                                    
           EXIT.                                                        

       OBT-COD-MNAC SECTION.
       INI-OBT-COD-MNAC.
      * Obtiene Codigo Valor de Cambio de Pesos Mediante CIC de la Moneda '0999'
           MOVE 'TAB'          TO FIO-SIST.
           MOVE 'VLR'          TO TAB-CIC-TTAB IN TAB.
           MOVE '0999  '       TO TAB-CIC-CTAB IN TAB.
           MOVE 'TAB-CIC-TABL' TO FIO-AKEY.
           MOVE FIO-GET-KEY    TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
               MOVE 'TAB'          TO MSG-COD-SIST
               MOVE 'CIC0999  NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
           MOVE TAB-COD-CTAB IN TAB TO WSS-COD-PESO IN WSS-VARI.
       FIN-OBT-COD-MNAC.
           EXIT.

       GET-ROV-REN SECTION.                                             
       INI-GET-ROV-REN.                                                 
           MOVE ZEROES TO WSS-TOT-RCAN.                                 
           MOVE ZEROES TO WSS-TOT-PTTS IN WSS-VARI.                     
           IF OPC-VAL-SCRE IN OPC NOT > ZEROES                          
               PERFORM CHK-ROV-PTTS                                     
               IF WSS-TOT-PTTS IN WSS-VARI > ZEROES                     
                   GO TO VRF-RCAN.                                      
           MOVE OPC-CIC-IOPC IN OPC TO ROV-CIC-IOPC IN ROV.             
           MOVE LOW-VALUES          TO ROV-KEY-IVCT IN ROV. 
           MOVE FIO-GET-NLS TO FIO-CMND.
       CIC-GET-ROV-REN.                                                 
           PERFORM COL-FIO-ROV.                                         
           IF NOT FIO-STAT-OKS OR                                       
              OPC-CIC-IOPC IN OPC NOT = ROV-CIC-IOPC IN ROV             
               GO TO VRF-RCAN.                                          
           IF NOT ( ROV-IIC-IOPV IN ROV > ZEROES AND                    
                    ROV-NUM-IVCT IN ROV > ZEROES )   
               MOVE FIO-GET-NXT TO FIO-CMND       
               GO TO CIC-GET-ROV-REN
           ELSE    
               MOVE ROV-KEY-IVCT IN ROV TO CAN-KEY-ICAN IN CAN          
               MOVE FIO-GET-KEY TO FIO-CMND                             
               PERFORM COL-FIO-CAN                                      
               IF FIO-STAT-OKS AND                                      
                   ( CAN-MSC-STAT IN CAN = 'C' OR                       
                     CAN-MSC-STAT IN CAN = 'W' OR 'P' ) OR              
                   ( OPC-MSC-STAT IN OPC = 'A' AND                      
                     CAN-MSC-STAT IN CAN = 'A' )                        
                       MOVE CAN-KEY-ICAN TO RCAN-CAN-KEY-ICAN           
                       IF OPC-MSC-STAT IN OPC = 'A' AND                 
                          CAN-MSC-STAT IN CAN = 'A' AND                 
                        ( CAN-MSC-CANC IN CAN = 'PTT' OR 'KTT' )        
                           MOVE  1  TO WSS-TOT-PTTS IN WSS-VARI         
                           MOVE CAN-SGV-TREN IN CAN TO WSS-SGV-RENO     
                           MOVE FIO-GET-NXT TO FIO-CMND         
                           GO TO CIC-GET-ROV-REN                        
                       ELSE                                             
                           ADD 1                   TO WSS-TOT-RCAN      
                           ADD CAN-SGV-TREN IN CAN TO WSS-SGV-RENO      
                           MOVE FIO-GET-NXT TO FIO-CMND                 
                           GO TO CIC-GET-ROV-REN.

       VRF-RCAN.                                                        
           IF WSS-TOT-RCAN = ZEROES             AND                     
              WSS-TOT-PTTS IN WSS-VARI = ZEROES                         
               GO TO FIN-GET-ROV-REN.                                   
           MOVE RCAN-CAN-KEY-ICAN TO CAN-KEY-ICAN.                      
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM COL-FIO-CAN.                                         
       FIN-GET-ROV-REN.                                                 
           EXIT.                                                        
       CHK-ROV-PTTS SECTION.                                            
       INI-CHK-ROV-PTTS.                                                
           MOVE OPC-CIC-IOPC IN OPC TO ROV-CIC-IOPV IN ROV.             
           MOVE ZEROES              TO ROV-NUM-IVCT IN ROV.             
           MOVE 'ROV-KEY-IVCT' TO FIO-AKEY.                             
           MOVE FIO-GET-NLS    TO FIO-CMND.                             
           PERFORM COL-FIO-ROV.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    OPC-CIC-IOPC IN OPC = ROV-CIC-IOPV IN ROV )         
               GO TO FIN-CHK-ROV-PTTS.                                  
           MOVE ROV-CIC-IOPC IN ROV TO WSS-CIC-IOPC IN WSS-VARI.        
       CAN-CHK-ROV-PTTS.                                                
           MOVE ROV-KEY-IVCT IN ROV TO CAN-KEY-ICAN IN CAN.             
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM COL-FIO-CAN.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    OPC-MSC-STAT IN OPC = 'A' AND                       
                    CAN-MSC-STAT IN CAN = 'A' AND                       
                  ( CAN-MSC-CANC IN CAN = 'PTT' OR 'KTT' ))             
               GO TO FIN-CHK-ROV-PTTS.                                  
           MOVE LOW-VALUES TO ROV-KEY-IVCT IN ROV.                      
           MOVE FIO-GET-NLS TO FIO-CMND.  
       LUP-CHK-ROV-PTTS.  
           PERFORM COL-FIO-ROV.
           IF FIO-STAT-OKS AND
              ROV-CIC-IOPC IN ROV = WSS-CIC-IOPC IN WSS-VARI 
               IF ROV-CIC-IOPV IN ROV NOT = OPC-CIC-IOPC IN OPC         
                   MOVE ROV-KEY-IVCT IN ROV TO WSS-KEY-IVCT IN WSS-VARI 
                   ADD 1 TO WSS-TOT-PTTS IN WSS-VARI                    
                   MOVE FIO-GET-NXT TO FIO-CMND                         
                   GO TO LUP-CHK-ROV-PTTS                               
               ELSE                                                     
                   MOVE WSS-KEY-IVCT IN WSS-VARI TO RCAN-CAN-KEY-ICAN   
                                                          IN WSS-VARI.  
       FIN-CHK-ROV-PTTS.                                                
           EXIT.                                                        
       OBT-TASA-CPR SECTION.                                            
       INI-OBT-TASA-CPR.                                                
           MOVE 'ICG-KEY-IICG' TO FIO-AKEY.                             
           MOVE OPC-CIC-IOPC IN OPC TO ICG-CIC-IOPC IN ICG.             
           MOVE ZEROES              TO ICG-NUM-ICAN IN ICG.             
           MOVE SPACES              TO ICG-CIC-IOPV IN ICG              
           MOVE ZEROES              TO ICG-NUM-IVCT IN ICG              
           MOVE 'INT'               TO ICG-IND-TTAS IN ICG.             
           MOVE ZEROES              TO ICG-FEC-FINI IN ICG.             
           MOVE FIO-GET-NLS TO FIO-CMND.                                
           PERFORM COL-FIO-ICG.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    ICG-CIC-IOPC IN ICG = OPC-CIC-IOPC IN OPC AND       
                    ICG-NUM-ICAN IN ICG = ZEROES              AND       
                    ICG-CIC-IOPV IN ICG = SPACES              AND       
                    ICG-NUM-IVCT IN ICG = ZEROES              AND       
                    ICG-IND-TTAS IN ICG = 'INT'  )                      
                     MOVE ZEROES TO ICG-SGV-TINT IN ICG.                
       FIN-OBT-TASA-CPR.                                                
           EXIT.                                                        
      *RUTINA DE OBTENCION DE NUMERO DE OPERACION                       
       OBT-OPE-ORI SECTION.                                             
       INI-OBT-OPE-ORI.                                                 
           MOVE OPC-CAI-IOPC IN CPR-FLD TO NOP-CAI-IOPC.                
           MOVE OPC-IIC-IOPC IN CPR-FLD TO NOP-IIC-IOPC.                
           MOVE NOP-CIC-IOPC  IN NOP-VARI    TO OPC-CIC-IOPC IN OPC     
           MOVE FIO-GET-KEY TO FIO-CMND                                 
           PERFORM COL-FIO-OPC                                          
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-OBT-OPE-ORI.                                   
                                                                        
           IF OPC-IND-RENO IN OPC NOT > SPACES                          
               MOVE NOP-CIC-IOPC IN NOP-VARI TO NOP-CIC-ORIG IN NOP-VARI
               GO TO FIN-OBT-OPE-ORI.                                   
                                                                        
                                                                        
           MOVE SPACES TO ROV.                                          
           MOVE SPACES TO NOP-CIC-ORIG IN NOP-VARI.                     
           MOVE SPACES TO NOP-1        IN NOP-VARI.                     
       NXT-OBT-OPE-ORI.                                                 
           MOVE NOP-CIC-IOPC IN NOP-VARI  TO ROV-CIC-IOPC IN ROV.       
           MOVE ROV-CIC-IOPC IN ROV       TO NOP-COD-OPCR IN NOP-VARI.  
       LUP-OBT-OPE-ORI.                                                 
           MOVE FIO-GET-NLS TO FIO-CMND.                                
           PERFORM COL-FIO-ROV.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    NOP-COD-OPCR  = ROV-CIC-IOPC IN ROV )               
              GO TO FIN-OBT-OPE-ORI.                                    
           IF NOP-1 = SPACES                                            
              MOVE ROV-CIC-IOPV IN ROV TO  NOP-CIC-ORIG IN NOP-VARI     
              MOVE  'N'                TO  NOP-1.                       
           MOVE ROV-CIC-IOPV IN ROV TO DLC-CIC-IOPC IN DLC.             
           MOVE ROV-CIC-IOPV IN ROV TO NOP-COD-OPCR IN NOP-VARI.        
           MOVE 001                 TO DLC-NUM-IDLC IN DLC.             
           MOVE FIO-GET-KEY         TO FIO-CMND.                        
           PERFORM COL-FIO-DLC.                                         
           IF NOT FIO-STAT-OKS                                          
              GO TO FIN-OBT-OPE-ORI.                                    
           MOVE OPC-COD-VCOC IN OPC TO CPR-GLS-VCOC(CPR-I).             
           MOVE OPC-COD-COOC IN OPC TO CPR-COD-COOC(CPR-I).             
           MOVE OPC-COD-OFOC IN OPC TO CPR-COD-OFIC(CPR-I).             
           MOVE OPC-NUM-VCOR IN OPC TO CPR-NUM-VCOR(CPR-I).             
           MOVE DLC-IND-SCTB IN DLC TO CPR-IND-RENO(CPR-I).             
           MOVE OPC-FEC-FCOL IN OPC TO CPR-FEC-FCAN(CPR-I).             
           MOVE OPC-IND-RENO IN OPC TO CPR-IND-RENO(CPR-I).             
           PERFORM OBT-TASA-CPR.                                        
           MOVE ICG-SGV-TINT IN ICG TO CPR-SGV-TINT(CPR-I).             
           MOVE ROV-CAI-IOPC IN ROV TO CPR-CAI-IOPC(CPR-I).             
           MOVE ROV-IIC-IOPC IN ROV TO CPR-IIC-IOPC(CPR-I).             
           MOVE ROV-CIC-IOPV IN ROV TO CPR-COD-RCAN(CPR-I).             
           MOVE ROV-KEY-IVCT IN ROV TO CAN-KEY-ICAN IN CAN.             
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM COL-FIO-CAN.                                         
           MOVE CAN-FEC-FCAN IN CAN TO CPR-FEC-FCAN(CPR-I).             
           MOVE CAN-NUM-ICAN IN CAN TO CPR-NUM-ICAN(CPR-I).             
      *    MOVE CAN-COD-RCAN IN CAN TO CPR-COD-RCAN(CPR-I).             
           MOVE CAN-SGV-TREN IN CAN TO CPR-SGV-RENO(CPR-I).             
           MOVE CAN-SGV-TPAG IN CAN TO CPR-SGV-TPAG(CPR-I).             
                                                                        
           MOVE CAN-CIC-IOPC IN CAN TO RCV-CIC-IOPC IN RCV.             
           MOVE CAN-NUM-ICAN IN CAN TO RCV-NUM-ICAN IN RCV.             
           MOVE SPACES              TO RCV-CAI-IOPV IN RCV.             
           MOVE ZEROES              TO RCV-IIC-IOPV IN RCV.             
           MOVE ZEROES              TO RCV-NUM-IVCT IN RCV.             
           MOVE FIO-GET-NLS TO FIO-CMND.                                
           PERFORM COL-FIO-RCV.                                         
           MOVE RCV-SGV-CAPI IN RCV TO CPR-SGV-CAPI(CPR-I).             
           MOVE CAN-CIC-IOPC IN CAN TO CYA-CIC-IOPC IN CYA.             
           MOVE CAN-NUM-ICAN IN CAN TO CYA-NUM-ICAN IN CYA.             
           MOVE ZEROES              TO CYA-NUM-ICYA IN CYA.             
           MOVE FIO-GET-NLS TO FIO-CMND.                                
           PERFORM COL-FIO-CYA.                                         
           IF  ( FIO-STAT-OKS AND                                       
              CAN-CIC-IOPC IN CAN = CYA-CIC-IOPC IN CYA AND             
              CAN-NUM-ICAN IN CAN = CYA-NUM-ICAN IN CYA )               
               MOVE CYA-IND-TCYA IN CYA TO WSS-COD-TCYA                 
               IF WSS-GLS-TCYA = 'TR' OR                                
                  WSS-GLS-TCYA = 'PB'                                   
                   MOVE WSS-IND-TCYA        TO CPR-IND-RENO(CPR-I).     
                                                                        
           PERFORM INC-REG-TS.                                          
           MOVE ROV-CIC-IOPV IN ROV TO  NOP-CIC-ORIG IN NOP-VARI.       
           IF ( DLC-IND-SCTB IN DLC = SPACES OR 'MUL' )                 
                 MOVE ROV-CIC-IOPV IN ROV TO  NOP-CIC-ORIG IN NOP-VARI  
                 GO TO FIN-OBT-OPE-ORI                                  
           ELSE                                                         
              MOVE SPACES TO ROV                                        
              MOVE NOP-COD-OPCR TO ROV-CIC-IOPC IN ROV                  
              GO TO LUP-OBT-OPE-ORI                                     
           END-IF.                                                      
       FIN-OBT-OPE-ORI.                                                 
           EXIT.                                                        
      *>>>>                                                             
*% END
