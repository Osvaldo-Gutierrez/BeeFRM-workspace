*% IF INI_FST_INP
      *<<<< INI_FST_INP
           IF NOT FIO-STAT-OKS
             DISPLAY 'ABORTO: ERROR APERTURA ARCHIVO COLGCO FIO-STAT '
                      FIO-STAT
             MOVE PGM-SMAL TO PGM-STAT-SRT
             GO TO FIN-INP-SRT.

           MOVE FIO-INP TO FIO-CMND.
           PERFORM TAB-FIO-OFI.
           IF NOT FIO-STAT-OKS
             DISPLAY 'ABORTO: ERROR APERTURA ARCHIVO TABOFI FIO-STAT '
                      FIO-STAT
             MOVE PGM-SMAL TO PGM-STAT-SRT
             GO TO FIN-INP-SRT.

           MOVE FIO-INP TO FIO-CMND.
           MOVE 'TAB' TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
             DISPLAY 'ABORTO: ERROR APERTURA ARCHIVO COLGCO FIO-STAT '
                      FIO-STAT
             MOVE PGM-SMAL TO PGM-STAT-SRT
             GO TO FIN-INP-SRT.

           DISPLAY 'APERTURA ARCHIVOS ANEXOS OK'.
      * Captura Codigo de Oficina
           DISPLAY 'OFICINA     (*=TODAS) : '.
           ACCEPT WSS-COD-OFIC IN WSS-VARI.
           DISPLAY WSS-COD-OFIC IN WSS-VARI.

           DISPLAY 'FIN CAPTURA PARAMETRO'.
           DISPLAY 'INICIO VALIDACION PARAMETRO'.

           IF WSS-COD-OFIC IN WSS-VARI NOT > SPACES
             DISPLAY 'DEBE EXISTIR OFICINA O *'
             MOVE PGM-SMAL TO PGM-STAT-SRT
             GO TO FIN-INP-SRT.

      *VAL-OFI Valida Codigo de Oficina en TABLAS
           IF WSS-COD-OFIC IN WSS-VARI NOT > SPACES
               MOVE '*' TO WSS-COD-OFIC IN WSS-VARI.
           IF WSS-COD-OFIC IN WSS-VARI NOT = '*'
               DISPLAY WSS-COD-OFIC IN WSS-VARI
               MOVE WSS-COD-OFIC IN WSS-VARI TO OFI-COD-OFIC IN OFI
               PERFORM VAL-OFI
               IF MSG-COD-MENS > SPACES
                   DISPLAY MSG-GLS-DESC IN MSG
                   MOVE PGM-SMAL TO PGM-STAT-SRT
                   GO TO FIN-INP-SRT.
      *>>>>
*% END
*% IF FIN_FST_INP
      *<<<< FIN_FST_INP
      *>>>>
*% END
*% IF LUP_INP
      *<<<< LUP_INP
           IF WSS-COD-OFIC IN WSS-VARI = '*'
               GO TO MOV-INP-SRT.

           IF GCO-COD-OFOC IN GCO = WSS-COD-OFIC IN WSS-VARI
               MOVE GCO-GLS-CCSB IN GCO TO GCO-GLS-CCSB IN SRT
               MOVE GCO-COD-OFOC IN GCO TO GCO-COD-OFOC IN SRT
               MOVE GCO-VAL-SCRE IN GCO TO GCO-VAL-SCRE IN SRT
               RELEASE SRT.
           GO TO NXT-INP-SRT.
      *>>>>
*% END
