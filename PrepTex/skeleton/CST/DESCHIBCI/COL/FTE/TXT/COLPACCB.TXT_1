*% IF WSS
      *<<<< WSS
       COPY GNSWGRQA REPLACING ==01  ADR-REQA== BY ==02  SCR-CCB-REQA==.
       02 SCR-KEY-ICCB                     PIC X(03).
       
       01  WSS-VARI.
*% IF PGM_PU
      * EVA-INI  07/08
           03  WSS-FEC-FHOY.
               05  WSS-NUM-DHOY                       PIC 9(02).
               05  WSS-NUM-MHOY                       PIC 9(02).
               05  WSS-NUM-SHOY                       PIC 9(02).
               05  WSS-NUM-AHOY                       PIC 9(02).
      * EVA-FIN  07/08
*% END
           03  WSS-FEC-FCOM.
               05  WSS-NUM-SCOM                       PIC 9(02).
               05  WSS-NUM-ACOM                       PIC 9(02).
               05  WSS-NUM-MCOM                       PIC 9(02).
               05  WSS-NUM-DCOM                       PIC 9(02).
           03  WSS-UBFD.
               05  WSS-TAB-UBFD                      PIC X(03).
               05  WSS-COD-UBFD                      PIC X(12).
           03  BUS-POR-KEYS             VALUE SPACES PIC X(01).
           03  BUS-POR-CLIE             VALUE 'C'    PIC X(01).
           03  WSS-ICOR                 VALUE 0      PIC 9(01).
               88 NO-ASIGNE-CORRELATIVO VALUE 0.
           03 WSS-FEC-FEC1.
              05 WSS-NUM-SEC1           PIC 9(02).
              05 WSS-NUM-AEC1           PIC 9(02).
              05 WSS-NUM-MEC1           PIC 9(02).
              05 WSS-NUM-DEC1           PIC 9(02).
           03 WSS-FEC-FEC2.
              05 WSS-NUM-SEC2           PIC 9(02).
              05 WSS-NUM-AEC2           PIC 9(02).
              05 WSS-NUM-MEC2           PIC 9(02).
              05 WSS-NUM-DEC2           PIC 9(02).
      * var necesarias para controlar que el valor gastos cambio
      * sea ingresado sin decimales
           03 WSS-SGV-GAST VALUE ZEROES PIC 9(11).
           03 WSS-DIFE     VALUE ZEROES PIC 9(02)V9(04).
       COPY GNSWGSTR.
       COPY SGCBRDBC.
       COPY COLBROPC.
       COPY COLBRCCT.
       COPY COLBRDLC.
       COPY COLBRRDC.
       COPY GNSWGCPT.
*% IF WSS_DTC
      *<<<< WSS_DTC
      *>>>>
*% END
