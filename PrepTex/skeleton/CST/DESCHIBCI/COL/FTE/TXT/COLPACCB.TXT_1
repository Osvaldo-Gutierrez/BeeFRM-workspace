*% IF INI_SCR_MOD
*% IF PGM_PU
      *<<<< INI_SCR_MOD
           GO TO SAL-SCR-MOD. 
      *>>>>
*% END      
*% END
*% IF PFS_SCR_MOD
*% IF PGM_PU
       SAL-SCR-MOD.
       
           IF SCR-INST = SCR-INST-HKY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM HBL-KEY
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
               PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
               PERFORM GET-ACC.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8 OR
                         FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM XCT-PRGP.
           PERFORM DESSCRTRL.
*% END
*% IF PGM_PQ
      *<<<< PFS_SCR_MOD                                                                                         
           IF ( FRM-FFLD = FRM-FFLD-PF8 OR FRM-FFLD-PF7 ) AND           
              SCR-STAT = SCR-STAT-CON                                   
               PERFORM VAL-NUM-KEY-CCB                                  
               PERFORM VAL-NUM-FLD-CCB.                                 
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8                   
               PERFORM RST-STAT                                         
               PERFORM FND-SCR-KEY THRU FIN-SCR-KEY                     
               IF SCR-STAT NOT = SCR-STAT-ABT                           
                   PERFORM SCR-TRL                                                                       
               ELSE                                                     
                   MOVE SPACES TO SCR-CCB-REQA                          
                   PERFORM DESSCRTRL                                    
                   GO TO FIN-SCR-MOD.                                   
           MOVE SPACES TO SCR-CCB-REQA.
      *>>>>
*% END
*% END
*% IF PFS_SCR_GFLD
*% IF PGM_PU
      *<<<< PFS_SCR_GFLD                                                
           IF NOT ( ( SCR-CCPP = 'MOD' OR  SCR-CCPP = 'ACC' OR          
                      SCR-CCPP = 'ELI'                      OR          
                    ( SCR-CCPP = 'ACT' AND SCR-ACTU = SCR-ACTU-MOD ) )  
                      AND ( FRM-FFLD = FRM-FFLD-PF8 OR                  
                            FRM-FFLD = FRM-FFLD-PF7 ) )                 
      *>>>>
*% END
*% END      
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC           
           IF FRM-FFLD = FRM-FFLD-PF8 OR FRM-FFLD-PF7
               PERFORM VAL-NUM-KEY-CCB
               PERFORM VAL-NUM-FLD-CCB
               PERFORM RST-STAT
               PERFORM FND-SCR-KEY THRU FIN-SCR-KEY
               IF SCR-STAT NOT = SCR-STAT-ABT
                   PERFORM SCR-TRL
               ELSE
                   MOVE SPACES TO SCR-CCB-REQA
                   PERFORM DESSCRTRL
                   GO TO FIN-SCR-ACC.
           MOVE SPACES TO SCR-CCB-REQA.
      *>>>>
*% END
*% IF PFS_SCR_ELI
      *<<<< PFS_SCR_ELI           
           IF FRM-FFLD = FRM-FFLD-PF8 OR FRM-FFLD-PF7
               PERFORM VAL-NUM-KEY-CCB
               PERFORM VAL-NUM-FLD-CCB
               PERFORM RST-STAT
               PERFORM FND-SCR-KEY THRU FIN-SCR-KEY
               IF SCR-STAT NOT = SCR-STAT-ABT
                   PERFORM SCR-TRL
               ELSE
                   MOVE SPACES TO SCR-CCB-REQA
                   PERFORM DESSCRTRL
                   GO TO FIN-SCR-ELI.
           MOVE SPACES TO SCR-CCB-REQA.
      *>>>>
*% END
*% IF PFS_SCR_ACT
      *<<<< PFS_SCR_ACT           
           IF ( FRM-FFLD = FRM-FFLD-PF8 OR FRM-FFLD-PF7 ) AND
              SCR-STAT = SCR-STAT-CON
               PERFORM VAL-NUM-KEY-CCB
               PERFORM VAL-NUM-FLD-CCB.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8
               PERFORM RST-STAT
               PERFORM FND-SCR-KEY THRU FIN-SCR-KEY
               IF SCR-STAT NOT = SCR-STAT-ABT
                   PERFORM SCR-TRL
               ELSE
                   MOVE SPACES TO SCR-CCB-REQA
                   PERFORM DESSCRTRL
                   GO TO FIN-SCR-ACT.
           MOVE SPACES TO SCR-CCB-REQA.
      *>>>>
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           MOVE +509 TO SCR-TCMA.
           IF SCR-DESDE-CMN
               MOVE SPACES TO SCR-CCB-REQA.
              
           MOVE ZEROES TO CCB-SGV-GAST IN CCB.
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
      *>>>>
*% END
*% IF FND_KEY
      *<<<< FND_KEY
      *AFRI-INI/JUL2008 (MAE)
      *    VALIDA QUE LA OPERACION TENGA SALDO
           IF SCR-CCPP = 'ING'                                          
              PERFORM VAL-OPC-ZERO
              IF CCB-IND-ZERO IN CCB-VARI = 'S'
                  MOVE 'OPC ZEROES' TO CCB-COD-MENS IN CCB-VARI
                  MOVE 'COL'            TO MSG-COD-SIST
                  MOVE CCB-COD-MENS IN CCB-VARI TO MSG-COD-MENS
                  PERFORM GET-MSG
                  MOVE MSG-GLS-DESC     TO FRM-MENS
                  MOVE '00' TO FIO-STAT                                 
                  MOVE SCR-STAT-CON TO SCR-STAT                         
                  GO TO FIN-FND-KEY.

      *AFRI-FIN/JUL2008 (MAE)
           IF SCR-CCPP = 'ING' OR 'ELI'
               PERFORM BUS-COR-CCB
               PERFORM CLR-CCB
               GO TO CON-FND-KEY.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8
               PERFORM LEE-NXT-PRV
               IF FIO-STAT-OKS
                   PERFORM SCR-CLRA-CCB
                   GO TO CON-FND-KEY
               ELSE
                   MOVE SPACES TO PGM-STAT
                   IF FRM-FFLD = FRM-FFLD-PF7
                       MOVE 'COL' TO MSG-COD-SIST
                       MOVE 'CCB    NOPRV' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE '00' TO FIO-STAT
                       MOVE SCR-STAT-CON TO SCR-STAT
                       GO TO FIN-FND-KEY
                   ELSE
                       MOVE 'COL' TO MSG-COD-SIST
                       MOVE 'CCB    NONXT' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE '00' TO FIO-STAT
                       MOVE SCR-STAT-CON TO SCR-STAT
                       GO TO FIN-FND-KEY.
           IF CCB-IIC-IOPC IN CCB NOT > ZEROES
               PERFORM ANL-KEY-CCB
               IF NOT FIO-STAT-OKS
                   GO TO NXT-FND-KEY.
           IF CCB-NUM-ICCB IN CCB NOT > ZEROES 
               PERFORM BUS-COR-CCB
               PERFORM CLR-CCB.
       CON-FND-KEY.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
           IF SCR-CCPP = 'ACT' AND NO-ASIGNE-CORRELATIVO AND 
              NOT FIO-STAT-OKS
               PERFORM BUS-COR-CCB
               PERFORM CLR-CCB
               MOVE '23' TO FIO-STAT.

      *AFRI-INI/JUL2008 (MAE)
      *    VALIDA QUE LA OPERACION TENGA SALDO
           IF FIO-STAT-OKS  AND  SCR-CCPP = 'MOD'                       
               PERFORM VAL-OPC-MOD
               IF (CCB-IND-ERRO IN CCB-VARI = 'S' OR
                   CCB-IND-MOD  IN CCB-VARI = 'S')
                    MOVE 'COL'            TO MSG-COD-SIST
                    MOVE CCB-COD-MENS IN CCB-VARI TO MSG-COD-MENS
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC     TO FRM-MENS
                    MOVE '00' TO FIO-STAT                               
                    MOVE SCR-STAT-CON TO SCR-STAT                       
                    GO TO FIN-FND-KEY.

      *AFRI-FIN/JUL2008 (MAE)
           IF FIO-STAT-OKS AND (SCR-CCPP = 'MOD' OR SCR-CCPP = 'ELI' OR 
              (SCR-CCPP = 'ACT' AND SCR-ACTU = SCR-ACTU-MOD))
                PERFORM BUS-SIG-CCB
                IF FIO-STAT-OKS
                    MOVE 'COL' TO MSG-COD-SIST
                    MOVE 'NUMICCBINV' TO MSG-COD-MENS
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    MOVE '00' TO FIO-STAT
                    MOVE SCR-STAT-CON TO SCR-STAT
                    GO TO FIN-FND-KEY
                ELSE
                    MOVE '00' TO FIO-STAT.

           IF FIO-STAT-OKS
               MOVE ADR-CCB-REQA TO SCR-CCB-REQA.
           IF  BUS-POR-KEYS = BUS-POR-CLIE
                IF FIO-STAT-OKS
                    PERFORM BUS-CCB-SCBA
                    MOVE '00' TO FIO-STAT
                    GO TO NXT-FND-KEY
                ELSE
                    PERFORM BUS-CCB-SCBA
                    MOVE '23' TO FIO-STAT
                    GO TO NXT-FND-KEY.
           IF FIO-STAT-OKS
               PERFORM BUS-CCB-SCBA
               PERFORM ANL-CIC-IOPC
               GO TO NXT-FND-KEY.
           IF ( SCR-CCPP = 'ING' OR SCR-CCPP = 'ACT' )
               PERFORM BUS-CCB-SCBA
               PERFORM ANL-CIC-IOPC
               IF FIO-STAT-OKS
                   PERFORM PUT-OPC-CCB
                   PERFORM PUT-DLC-CCB
                   MOVE '23' TO FIO-STAT
               ELSE
                   MOVE SCR-STAT-CON TO SCR-STAT
                   MOVE '00' TO FIO-STAT.
       NXT-FND-KEY.
           IF PGM-STAT-OPC-OKS AND 
              OPC-MSC-STAT IN OPC NOT = 'A'
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'OPC    NOACA' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE SCR-STAT-CON TO SCR-STAT
               IF  SCR-CCPP = 'ING'
                   MOVE '23' TO FIO-STAT
               ELSE
                   MOVE '00' TO FIO-STAT.
           
           GO TO EXT-FND-KEY.
      *>>>>
*% END
*% IF PFS_GET_CON OR PFS_GET_ACC OR PFS_SCR_GKEY
      *<<<< PFS_GET_CON OR PFS_GET_ACC OR PFS_SCR_GKEY
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 OR FRM-FFLD = FRM-FFLD-PF7 )
      *>>>>
*% END
*% IF FIN_CMN_MOD
      *<<<< FIN_CMN_MOD
           IF NOT FIO-STAT-OKS
      *        Error inconsistencia en el ingreso : informar
               MOVE FIO-MENS TO FRM-MENS
               GO TO FIN-CMN-MOD.
           PERFORM MOD-DLC.
           IF NOT FIO-STAT-OKS
               PERFORM BCK-OUT
               GO TO FIN-CMN-MOD.
      *>>>>
*% END
*% IF INI_CMN_ING
      *<<<< INI_CMN_ING                                                 
           MOVE ZEROES TO CCB-NUM-IDLC IN CCB.                          
           MOVE ZEROES TO CCB-NUM-ICCB IN CCB.                          
      *AFRI-INI/NOV2008 (MAE)
      *    MOVE ZEROES TO CCB-FEC-FTRN IN CCB.
      *    MOVE ZEROES TO CCB-HRA-HRTR IN CCB.
      *AFRI-INI/NOV2008 (MAE)
           MOVE ZEROES TO CCB-FEC-FCCB IN CCB.                          
           MOVE ZEROES TO CCB-FEC-FING IN CCB.                          
           MOVE ZEROES TO CCB-FEC-FINP IN CCB.                          
           MOVE ZEROES TO CCB-SGV-GAST IN CCB.                          
           MOVE ZEROES TO CCB-NUM-IVCT IN CCB.                          
      * EVA-INI / MAY2008
           PERFORM GET-FHOY.
           MOVE HOY-SHOY TO CCB-NUM-SINP IN CCB.                        
           MOVE HOY-AHOY TO CCB-NUM-AINP IN CCB.                        
           MOVE HOY-MHOY TO CCB-NUM-MINP IN CCB.                        
           MOVE HOY-DHOY TO CCB-NUM-DINP IN CCB.                        
      * EVA-FIN / MAY2008
      *AFRI-INI/NOV2008 (MAE)
           MOVE HOY-STP-ITRN TO CCB-STP-ITRN IN CCB.
      *AFRI-FIN/NOV2008 (MAE)
      *>>>>                                                             
      *AFRI-INI/JUN2008  (MAE)
      *    VALIDA QUE LA OPERACION TENGA SALDO
           PERFORM VAL-OPC-ZERO.
           IF CCB-IND-ZERO IN CCB-VARI = 'S'
              MOVE 'OPC ZEROES' TO CCB-COD-MENS IN CCB-VARI
              MOVE 'COL'            TO MSG-COD-SIST
              MOVE CCB-COD-MENS IN CCB-VARI TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC     TO FRM-MENS
              MOVE FRM-SUAR-MAL     TO FRM-SUAR
              GO TO FIN-CMN-ING.
      *AFRI-FIN/JUN2008  (MAE)
      *AFRI-INI/MARZO-2009 (MAE)
           MOVE  CCB-CAI-IOPC IN CCB-FLD TO DLC-CAI-IOPC IN DLC.
           MOVE  CCB-IIC-IOPC IN CCB-FLD TO DLC-IIC-IOPC IN DLC.
           MOVE  CCB-NUM-IDLC IN CCB-FLD TO DLC-NUM-IDLC IN DLC.
           MOVE FIO-GET-KEY               TO FIO-CMND.
           PERFORM COL-FIO-DLC.
           IF NOT FIO-STAT-OKS
                MOVE 'DLCNOEXIS' TO CCB-COD-MENS IN CCB-VARI
                MOVE 'S'    TO CCB-IND-ERRO IN CCB-VARI
                MOVE CCB-COD-MENS IN CCB-VARI TO MSG-COD-MENS
                PERFORM GET-MSG
                MOVE MSG-GLS-DESC     TO FRM-MENS
                MOVE FRM-SUAR-MAL     TO FRM-SUAR
                GO TO FIN-CMN-ING.
           IF CCB-IND-SCBZ IN CCB-FLD = 'NIS'  AND                      
              DLC-IND-SCTB IN DLC NOT = 'CAS'                           
               MOVE 'DLCNOT=CAS' TO CCB-COD-MENS IN CCB-VARI
               MOVE 'S'    TO CCB-IND-ERRO IN CCB-VARI
               MOVE CCB-COD-MENS IN CCB-VARI TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC     TO FRM-MENS
               MOVE FRM-SUAR-MAL     TO FRM-SUAR
               GO TO FIN-CMN-ING.
      *AFRI-FIN/MARZO-2009 (MAE)
*% END
*% IF FIN_CMN_ING
      *<<<< FIN_CMN_ING                                                 
           IF NOT FIO-STAT-OKS
      *        Error inconsistencia en el ingreso : informar
               MOVE FIO-MENS TO FRM-MENS
               GO TO FIN-CMN-ING.
           PERFORM MOD-DLC.
           IF NOT FIO-STAT-OKS
               PERFORM BCK-OUT
               GO TO FIN-CMN-ING.
      *>>>>
*% END
*% IF CMN_ELI
      *<<<<
      *MVG-INI 14-JUN-2019 IGUALACION COL
           GO TO FIN-CMN. 
      *MVG-FIN 14-JUN-2019 IGUALACION COL
      *>>>>            
*% END
*% IF FIN_CMN_ELI
      *<<<< FIN_CMN_ELI
      *MVG-INI 14-JUN-2019 IGUALACION COL
       FIN-CMN.
      *MVG-FIN 14-JUN-2019 IGUALACION COL
      *AFRI-INI/JUN2008 (MAE)
           PERFORM VAL-OPC-ELI.
           IF CCB-IND-ERRO IN CCB-VARI = 'S' OR
              CCB-IND-ELI IN CCB-VARI = 'S'
              MOVE 'COL'            TO MSG-COD-SIST
              MOVE CCB-COD-MENS IN CCB-VARI TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC     TO FRM-MENS
              MOVE FRM-SUAR-MAL     TO FRM-SUAR
              GO TO FIN-CMN-ELI.

      *AFRI-FIN/JUN2008 (MAE)

           MOVE FIO-DEL TO FIO-CMND.                                    
           PERFORM COL-FIO-CCB.                                         
      *<<<< FIN_CMN_ELI                                                 

           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ELI.
           PERFORM KEY-CCB-CCB.
           SUBTRACT 1 FROM CCB-NUM-ICCB IN CCB.
           IF CCB-NUM-ICCB IN CCB > ZEROES
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM COL-FIO-CCB
               IF FIO-STAT-OKS
                   NEXT SENTENCE
               ELSE
                   MOVE 'COL' TO MSG-COD-SIST
                   MOVE 'CCB-1    NEX' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   GO TO FIN-CMN-ELI
           ELSE
               MOVE SPACES TO CCB-IND-SCBZ IN CCB.
           MOVE CCB-IND-SCBZ IN CCB TO CCB-IND-SCBZ IN CCB-FLD.
           PERFORM MOD-DLC.
           IF NOT FIO-STAT-OKS
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'DLC    NOMOD' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS.
      *>>>>
*% END
*% IF INI_HBL_IKY
      *<<<< INI_HBL_IKY      
           GO TO SAL-HBL-IKY.
      *>>>>
*% END
*% IF FIN_HBL_IKY
*% IF PGM_PQ
      *<<<< FIN_HBL_IKY
       SAL-HBL-IKY.
       
           IF SCR-DESDE-CMN                                             
               MOVE CCB-GLS-MENS TO FRM-MENS                            
               MOVE LOW-VALUES TO CCB-FLD                               
               MOVE FRM-MENS TO CCB-GLS-MENS.                           
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.                           
           PERFORM PRO-MKY-CCB.                                         
           PERFORM PRO-AKY-CCB.                                         
           PERFORM UNP-IKY-CCB.                                         
           PERFORM FST-KEY-CCB.                                         
           MOVE '1' TO FRM-CPIM-AEY.
      *>>>>
*% END
*% IF PGM_PU
      *<<<< FIN_HBL_IKY
       SAL-HBL-IKY.
       
           IF SCR-DESDE-CMN                                             
               MOVE CCB-GLS-MENS TO FRM-MENS                            
               MOVE LOW-VALUES TO CCB-FLD                               
               MOVE FRM-MENS TO CCB-GLS-MENS.                           
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.                           
           PERFORM PRO-FLD-CCB.                                         
           PERFORM PRO-MKY-CCB.                                         
           PERFORM UNP-IKY-CCB.                                         
           PERFORM FST-KEY-CCB.                                         
           MOVE '1' TO FRM-CPIM-AEY.
      *>>>>
*% END
*% END
*% IF INI_HBL_KEY
      *<<<< INI_HBL_KEY
           GO TO SAL-HBL-KEY.
      *>>>>
*% END
*% IF FIN_HBL_KEY
*% IF PGM_PQ
      *<<<< FIN_HBL_KEY
       SAL-HBL-KEY.
       
           IF SCR-DESDE-CMN                                             
               MOVE CCB-GLS-MENS TO FRM-MENS                            
               MOVE LOW-VALUES TO CCB-FLD                               
               MOVE FRM-MENS TO CCB-GLS-MENS.                           
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.                           
           PERFORM UNP-KEY-CCB.                                         
           PERFORM FST-KEY-CCB.                                         
           MOVE '1' TO FRM-CPIM-AEY.
      *>>>>
*% END
*% IF PGM_PU
      *<<<< FIN_HBL_KEY
       SAL-HBL-KEY.
       
           IF SCR-DESDE-CMN                                             
               MOVE CCB-GLS-MENS TO FRM-MENS                            
               MOVE LOW-VALUES TO CCB-FLD                               
               MOVE FRM-MENS TO CCB-GLS-MENS.                           
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.                           
           PERFORM PRO-FLD-CCB.                                         
           PERFORM UNP-KEY-CCB.                                         
           PERFORM FST-KEY-CCB.                                         
           MOVE '1' TO FRM-CPIM-AEY.
      *>>>>
*% END
*% END
