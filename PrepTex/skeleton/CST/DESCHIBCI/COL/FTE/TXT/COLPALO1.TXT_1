      *<<<< WSS_DTC
      * ISP-INI 06-11-2011 1110_02 BCI CGM Log para tablas POC y PVC
      *         VSN=$11.10.11$
      *Entidades de SGC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-POC-REQA==.
      *
       01   VAR-PROGRAMA.
            03 PGM-COD-CREB              VALUE ' '       PIC X(03).
      * ISP-FIN 06-11-2011 1110_02 BCI CGM Log para tablas POC y PVC
      *         VSN=$11.10.11$
      *>>>>
*% END
*% IF INI_FIO_IBM
      *<<<< INI_FIO
      * ISP-INI 06-11-2011 1110_02 BCI CGM Log para tablas POC y PVC
      *         VSN=$11.10.11$
           PERFORM GET-FHOY.
           IF SCR-INST = 'GAB' AND
              ( FRM-FFLD = FRM-FFLD-PF8 OR
                FRM-FFLD = FRM-FFLD-PF7 )
                  MOVE 'GKY' TO SCR-INST.
      * ISP-FIN 06-11-2011 1110_02 BCI CGM Log para tablas POC y PVC
      *         VSN=$11.10.11$
      *>>>> INI_FIO
*% END
*% IF FND_KEY_CMND
      * ISP-INI 06-11-2011 1110_02 BCI CGM Log para tablas POC y PVC
      *         VSN=$11.10.11$
      *<<<< FND_KEY_CMND
      *     IF FRM-COD-CREB IN LO1-FLD = 'PRO' OR
      *        FRM-COD-CREB IN LO1-FLD = 'CLI'
      *         MOVE 'N' TO WSS-LEE-DBC-DESDE-LAP
      *         PERFORM OBTENER-NOMBRE-DBC.
           PERFORM SET-KEY-LAP.
      
           MOVE FIO-GET-NLS TO FIO-CMND.
      * ISP-FIN 06-11-2011 1110_02 BCI CGM Log para tablas POC y PVC
      *         VSN=$11.10.11$
      *>>>> FND_KEY_CMND
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
      * ISP-INI 06-11-2011 1110_02 BCI CGM Log para tablas POC y PVC
      *         VSN=$11.10.11$
           IF NOT FIO-STAT-OKS
               GO TO FIN-FND-KEY.
      
      *     IF FIO-STAT-OKS
      *         IF LAP-COD-ENTF IN LAP NOT = WSS-COD-ENTI
      *              MOVE '23'   TO FIO-STAT
      *              MOVE FIO-STAT TO PGM-STAT-LAP
      *              GO TO FIN-FND-KEY.
      
      *     IF NOT ( FRM-COD-CREB IN LO1-FLD = 'PRO' OR
      *              FRM-COD-CREB IN LO1-FLD = 'CLI' )
      *               MOVE 'S' TO WSS-LEE-DBC-DESDE-LAP
      *               PERFORM OBTENER-NOMBRE-DBC.
      
      *     MOVE LAP-CIC-ICLI IN LAP TO WSS-CIC-ICLI.
      
      *     IF FRM-COD-CREB IN LO1-FLD = 'PRO'
      *         IF LAP-COD-KLAP IN LO1-FLD NOT =
      *            LAP-COD-KLAP IN LAP    OR
      *            LAP-IIC-IPOC IN LO1-FLD NOT =
      *            LAP-IIC-IPOC IN LAP
      *              MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
      *              MOVE '23' TO FIO-STAT
      *              MOVE '23' TO PGM-STAT-LAP
      *              MOVE 'LO1NONXTOIPE' TO MSG-COD-MENS
      *              MOVE 'COL' TO MSG-COD-SIST
      *              PERFORM GET-MSG
      *              MOVE MSG-GLS-DESC TO FRM-MENS
      *              GO TO FIN-FND-KEY.
      
      *     IF FRM-COD-CREB IN LO1-FLD = 'AUT'
      *         IF LAP-COD-AUTO IN LO1-FLD NOT =
      *            LAP-COD-AUTO IN LAP
      *              MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
      *              MOVE '23' TO FIO-STAT
      *              MOVE '23' TO PGM-STAT-LAP
      *              MOVE 'LO1NONXTOAUT' TO MSG-COD-MENS
      *              MOVE 'COL' TO MSG-COD-SIST
      *              PERFORM GET-MSG
      *              MOVE MSG-GLS-DESC TO FRM-MENS
      *              GO TO FIN-FND-KEY.
      
      *     IF FRM-COD-CREB IN LO1-FLD = 'CLI'
      *         IF LAP-CIC-ICLI IN LAP     NOT =
      *            DBC-CIC-ICLI IN DBC
      *              MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
      *              MOVE '23' TO FIO-STAT
      *              MOVE '23' TO PGM-STAT-LAP
      *              MOVE 'LO1NONXTOCLI' TO MSG-COD-MENS
      *              MOVE 'COL' TO MSG-COD-SIST
      *              PERFORM GET-MSG
      *              MOVE MSG-GLS-DESC TO FRM-MENS
      *              GO TO FIN-FND-KEY.
      
           IF SCR-CMND = 'CON'
              IF FRM-FFLD = FRM-FFLD-PF8
                    PERFORM SET-KEY-LAP
                    PERFORM PRO-PF8
              ELSE
                IF FRM-FFLD = FRM-FFLD-PF7
                    PERFORM SET-KEY-LAP
                    PERFORM PRO-PF7.
      
      *     IF PGM-STAT-LAP-OKS
      *     IF FRM-FFLD = FRM-FFLD-PF7 OR
      *        FRM-FFLD = FRM-FFLD-PF8
      *         MOVE 'S' TO WSS-LEE-DBC-DESDE-LAP
               PERFORM OBTENER-NOMBRE-DBC.
      
           IF PGM-STAT-LAP-OKS
               IF LAP-COD-ENTF IN LAP = WSS-COD-ENTI
                   MOVE '00'   TO FIO-STAT
                   MOVE LAP-COD-CONT IN LAP TO POC
                   MOVE '00'   TO PGM-STAT-POC.
      
      * ISP-FIN 06-11-2011 1110_02 BCI CGM Log para tablas POC y PVC
      *         VSN=$11.10.11$
      *>>>> FIN_FND_KEY
*% END
*% IF INI_SCR_DMSG
*% IF PGM_PQ
      *<<<< INI_SCR_DMSG
      * ISP-INI 06-11-2011 1110_02 BCI CGM Log para tablas POC y PVC
      *         VSN=$11.10.11$
      * Con este codigo logramos que una vez terminada la consulta
      * las claves no se borren de la pantalla
           IF SCR-CMND = 'CON'
                AND
              SCR-INST = SCR-INST-GKY
                AND
              FRM-FFLD NOT = FRM-FFLD-RET
                  MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR.
      * ISP-FIN 06-11-2011 1110_02 BCI CGM Log para tablas POC y PVC
      *         VSN=$11.10.11$
      *>>>> INI_SCR_DMSG
*% END
*% END
