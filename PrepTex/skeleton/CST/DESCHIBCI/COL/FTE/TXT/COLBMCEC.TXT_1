*%! Codigo adicional para archivo
*% IF INI_INI
      MOVE +400 TO SCR-TCMA.
*%END
*% IF INI_CEC_FEC_FCUA
      *<<<< INI_CEC_FEC_FCUA
           MOVE FEC-CHOY-N TO FEC-CHOY.
      *>>>>
*%END
*% IF INI_CEC_FEC_FCUC
      *<<<< INI_CEC_FEC_FCUC
           MOVE FEC-CHOY-N TO FEC-CHOY.
      *>>>>
*%END
*% IF INI_CEC_FEC_FVES
      *<<<< INI_CEC_FEC_FVES
            MOVE FEC-CHOY-N TO FEC-CHOY.
      *>>>>
*%END
*% IF INI_CEC_FEC_FCOI
      *<<<< INI_CEC_FEC_FCOI
           MOVE FEC-CHOY-N TO FEC-CHOY.
      *>>>>
*%END
*% IF INI_CEC_FEC_FTPG
      *<<<< INI_CEC_FEC_FTPG
           MOVE FEC-CHOY-N TO FEC-CHOY.
      *>>>>
*%END
*% IF INI_PUT_DBC_CEC     
      *<<<< INI_PUT_DBC_CEC
            IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8
               MOVE SCR-CIC-ICLI TO DBC-CIC-ICLI IN DBC
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM SGC-FIO-DBC
               IF FIO-STAT-OKS
                  MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN CEC-FLD
                  MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN CEC-FLD
                  MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN CEC-FLD
                  MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN CEC-FLD
                  MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
                  PERFORM CPT-BLKS
                  MOVE CPT-STRN            TO FRM-GLS-NOMC IN CEC-FLD.
      *>>>>
*%END
*% IF FIN_DBC_VRF_ICLI
      *<<<< FIN_DBC_VRF_ICLI
           IF MSG-COD-MENS = SPACES AND                             
              DBC-NUM-ICLI IN CEC-FLD > ZEROES                      
               MOVE DBC-NUM-ICLI IN CEC-FLD TO DBC-NUM-ICLI IN DBC     
               MOVE DBC-IND-ICLI IN CEC-FLD TO DBC-IND-ICLI IN DBC     
               MOVE DBC-GLS-ICLI IN CEC-FLD TO DBC-GLS-ICLI IN DBC     
               MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY                        
               MOVE FIO-GET-KEY TO FIO-CMND                            
               PERFORM SGC-FIO-DBC                                     
               IF NOT FIO-STAT-OKS                                     
                  MOVE 'SGC'        TO MSG-COD-SIST                   
                   MOVE 'CLI    NEX' TO MSG-COD-MENS                   
                   PERFORM GET-MSG                                     
                   MOVE SPACES       TO FRM-GLS-NOMC IN CEC-FLD        
                   MOVE MSG-GLS-DESC TO FRM-MENS                       
                   MOVE FRM-SUAR-MAL TO FRM-SUAR                       
               ELSE                                                    
                   MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN                
                   PERFORM CPT-BLKS                                    
                   MOVE CPT-STRN            TO FRM-GLS-NOMC IN CEC-FLD 
                   MOVE DBC-CAI-ICLI IN DBC TO SCR-CAI-ICLI 
                   MOVE DBC-IIC-ICLI IN DBC TO SCR-IIC-ICLI.
      *>>>>
*% END
*% IF FIN_KEY_CEC_CEC
      *<<<< FIN_KEY_CEC_CEC
             MOVE SCR-CAI-ICLI TO CEC-CAI-IENT IN CEC.
             MOVE SCR-IIC-ICLI TO CEC-IIC-IENT IN CEC.
             MOVE 'CAE'  TO CEC-COD-ENTI IN CEC.
             MOVE ZEROES TO CEC-FEC-FENT IN CEC.
             MOVE SPACES TO CEC-COD-RENT IN CEC.
             MOVE SPACES TO CEC-COD-CCSE IN CEC.
             MOVE SPACES TO CEC-CIC-ISEN IN CEC.
             MOVE ZEROES TO CEC-NUM-ISEN IN CEC.            
      *>>>>
*% END
