*% PGM_PTC = TRUE
*% LARGO_TOTAL_COMMAREA = 3000
*% IF MI_COMMAREA
           03 SCR-NUM-PANT               PIC 9(0005).
           03 SCR-MOG-FLD                PIC X(1100).
*% END
*% IF WSS
      *<<<< WSS
       01 WSS-NUM-CCUR.
           03 WSS-NUM-ICUR-EDT  PIC  ZZ9.
           03 WSS-NUM-ICUR-ALF       PIC X(03).
           03 WSS-NUM-ICUR      REDEFINES
              WSS-NUM-ICUR-ALF       PIC 9(03).
       COPY TABBROFI.
       COPY COLBRRDC.
       COPY COLBRCEN.
       COPY COLBRDLC.
       COPY SGCBRDBC.
       COPY DEUBRDBD.
       COPY COLWXMOG.
      *>>>>
      *<<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OFI-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBD-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CEN-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DLC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RDC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSC-REQA==.
       COPY GNSBRMSC.
       COPY GNSWVNUM.
       COPY GNSWCNUM.
       COPY GNSWCVRF.
       COPY GNSWVVRF.
       COPY GNSWGIDC.
      *>>>>
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-OFRM-SRV
                MOVE TSK-TERM-ALF TO MOG-COD-TERM
                MOVE TSK-TERM-ALF TO MOG-COD-TERM-MAS
           ELSE
                MOVE EIBTRMID     TO MOG-COD-TERM
                MOVE EIBTRMID     TO MOG-COD-TERM-MAS.
           MOVE +3859 TO SCR-TCMA.
           PERFORM GNS-ERR-QUE.
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )
               MOVE SCR-MOG-FLD TO MOG-FLD
               GO TO FIN-INI.

           IF ( FRM-FFLD = FRM-FFLD-PF6 )
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM
               ADD 1              TO QUE-NITM
               MOVE MOG-NOM-TSMOG TO QUE-COLA
               MOVE MOG-LEN-TSMOG TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT NOT = QUE-STAT-OKS
                  MOVE SCR-MOG-FLD TO MOG-FLD
                  GO TO FIN-INI.
      *>>>>
*% END
*% IF PGM_POS_EDB
           MOVE LOW-VALUES TO MOG-EDT.
*% END
*% IF FIN_PGM_STAT
           03 PGM-STAT.
              05 PGM-STAT-DBC.
                 88 PGM-STAT-DBC-NUL    VALUE '  '.
                 07 PGM-STT1-DBC        VALUE '2' PIC X(01).
                    88 PGM-STAT-DBC-OKS VALUE '0'.
                 07 PGM-STT2-DBC        VALUE '3' PIC X(01).
              05 PGM-STAT-OPC.
                 88 PGM-STAT-OPC-NUL    VALUE '  '.
                 07 PGM-STT1-OPC        VALUE '2' PIC X(01).
                    88 PGM-STAT-OPC-OKS VALUE '0'.
                 07 PGM-STT2-OPC        VALUE '3' PIC X(01).
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM VAL-NUM-KEY-MOG
               PERFORM VAL-NUM-FLD-MOG
               PERFORM SCRL-TAB-MOG
               PERFORM SCR-TRL.
      *>>>>
*% END
*% IF SCR_RST
           MOVE LOW-VALUES TO MOG-EDT.
*% END
*% IF FND_KEY
           IF DBC-NUM-ICLI IN MOG-FLD > ZEROES
      *IDC-CLI Busca Cliente por medio de IdC
               MOVE DBC-NUM-ICLI IN MOG-FLD TO DBC-NUM-ICLI IN DBC
               MOVE DBC-IND-ICLI IN MOG-FLD TO DBC-IND-ICLI IN DBC
               MOVE DBC-GLS-ICLI IN MOG-FLD TO DBC-GLS-ICLI IN DBC
               MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM SGC-FIO-DBC
               IF NOT FIO-STAT-OKS
                   MOVE 'COL'          TO MSG-COD-SIST
                   MOVE 'CLI    INEX' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE SCR-STAT-CON TO SCR-STAT
                   MOVE '00'         TO FIO-STAT
                   GO TO FIN-FND-KEY
               ELSE
                   MOVE DBC-CIC-ICLI IN DBC TO DBD-CIC-ICLI IN DBD
                   MOVE FIO-GET-KEY TO FIO-CMND
                   PERFORM DEU-FIO-DBD
                   IF NOT FIO-STAT-OKS
                       MOVE 'COL'          TO MSG-COD-SIST
                       MOVE 'DBD    INEX' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE SCR-STAT-CON TO SCR-STAT
                       MOVE '00'         TO FIO-STAT
                       GO TO FIN-FND-KEY
                   ELSE
                       MOVE DBD-NUM-VCCP IN DBD TO
                            FRM-NUM-PROR IN MOG-FLD
                       MOVE DBD-NUM-VCCR IN DBD TO
                            FRM-NUM-RENR IN MOG-FLD
                       MOVE DBD-NUM-VCCG IN DBD TO
                            FRM-NUM-RNGR IN MOG-FLD
                       IF FRM-COD-DOPE IN MOG-FLD = 'S'
                           PERFORM PUT-FND-MOG
                           MOVE SCR-STAT-CON TO SCR-STAT
                           MOVE '00'         TO FIO-STAT
                           GO TO FIN-FND-KEY
                       ELSE
                           MOVE SCR-STAT-CON TO SCR-STAT
                           MOVE '00'         TO FIO-STAT
                           GO TO FIN-FND-KEY.

           IF OPC-IIC-IOPC IN MOG-FLD > ZEROES
               MOVE 'OPC'                    TO CEN-COD-ENTI IN CEN
               MOVE OPC-CAI-IOPC IN MOG-FLD  TO WSS-CAI-IOPC
               MOVE OPC-IIC-IOPC IN MOG-FLD  TO WSS-IIC-IOPC
               MOVE WSS-CIC-IOPC IN WSS-VARI TO CEN-CIC-IENT IN CEN
               MOVE ZEROES                   TO CEN-FEC-FENT IN CEN
               MOVE SPACES                   TO CEN-COD-RENT IN CEN
               MOVE ZEROES                   TO CEN-NUM-IENT IN CEN
               MOVE SPACES                   TO CEN-COD-CCSE IN CEN
               MOVE SPACES                   TO CEN-CAI-ISEN IN CEN
               MOVE SPACES                   TO CEN-IIC-ISEN IN CEN
               MOVE ZEROES                   TO CEN-NUM-ISEN IN CEN
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM COL-FIO-CEN
               IF NOT FIO-STAT-OKS
                   MOVE 'COL'          TO MSG-COD-SIST
                   MOVE 'CEN    INEX' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE SCR-STAT-CON TO SCR-STAT
                   MOVE '00'         TO FIO-STAT
                   GO TO EXT-FND-KEY
               ELSE
                   MOVE CEN-NUM-CON1 IN CEN TO
                        FRM-NUM-PROO IN MOG-FLD
                   MOVE CEN-NUM-CON2 IN CEN TO
                        FRM-NUM-RENO IN MOG-FLD
                   MOVE CEN-NUM-CON3 IN CEN TO
                        FRM-NUM-RNGO IN MOG-FLD
                   MOVE SCR-STAT-CON TO SCR-STAT
                   MOVE '00'         TO FIO-STAT
                   GO TO EXT-FND-KEY.
*% END
*% IF FIN_FND_KEY
           MOVE FIO-STAT TO PGM-STAT-OPC.
           GO TO EXT-FND-KEY.
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
      *>>>>
*% END
*% IF EOF
       COPY GNSBVNUM.
       COPY GNSBEGLS.
       COPY TABBBOFI.
       COPY TABBFOFI.
      *<<<< EOF
       COPY COLBFCEN.
       COPY COLBFRDC.
       COPY COLBFDLC.
       COPY SGCBFDBC.
       COPY DEUBFDBD.
       COPY GNSBVIND.
       COPY GNSBVMSC.
       COPY GNSBFMSC.
       COPY GNSBVIDC.
       COPY GNSBPVRF.
       PUT-FND-MOG SECTION.
       INI-PUT-FND-MOG.
           MOVE DBC-CAI-ICLI IN DBC     TO RDC-CAI-ICLI IN RDC.
           MOVE DBC-IIC-ICLI IN DBC     TO RDC-IIC-ICLI IN RDC.
           MOVE SPACES                  TO RDC-CAI-IOPC IN RDC.
           MOVE ZEROES                  TO RDC-IIC-IOPC IN RDC.
           MOVE ZEROES                  TO RDC-NUM-IDLC IN RDC.
           MOVE FIO-GET-NLS TO FIO-CMND.

       LUP-LEE-RDC.
           PERFORM COL-FIO-RDC.
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF
               GO TO EXI-FND-MOG.

           IF NOT ( RDC-CAI-ICLI IN RDC  = DBC-CAI-ICLI IN DBC AND
                    RDC-IIC-ICLI IN RDC  = DBC-IIC-ICLI IN DBC )
               GO TO EXI-FND-MOG.

           MOVE 'OPC'                    TO CEN-COD-ENTI IN CEN.
           MOVE RDC-CIC-IOPC IN RDC      TO CEN-CIC-IENT IN CEN.
           MOVE ZEROES                   TO CEN-FEC-FENT IN CEN.
           MOVE SPACES                   TO CEN-COD-RENT IN CEN.
           MOVE ZEROES                   TO CEN-NUM-IENT IN CEN.
           MOVE SPACES                   TO CEN-COD-CCSE IN CEN.
           MOVE SPACES                   TO CEN-CAI-ISEN IN CEN.
           MOVE SPACES                   TO CEN-IIC-ISEN IN CEN.
           MOVE ZEROES                   TO CEN-NUM-ISEN IN CEN.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-CEN.
           IF NOT FIO-STAT-OKS
               GO TO NXT-LEE-RDC.

           PERFORM CRG-VEC-MOG.

           IF (SCR-OFRM-SRV AND WSS-J > WSS-NITMMAX)
              GO TO EXI-FND-MOG.

           IF ((NOT SCR-OFRM-SRV) AND WSS-J > WSS-JMAX)
                 GO TO EXI-FND-MOG.

       NXT-LEE-RDC.
           MOVE FIO-GET-NXT     TO FIO-CMND.
           GO TO LUP-LEE-RDC.
       EXI-FND-MOG.
           PERFORM PUT-TAB.
       FIN-PUT-FND-MOG.
           EXIT.
       PUT-TAB SECTION.
       INI-PUT-TAB.
           MOVE MOG-NOM-TSMOG     TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE MOG-NOM-TSMOG-MAS TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE 1 TO SCR-NUM-PANT.
           MOVE SPACES TO MOG-VEC-TS.
           MOVE LOW-VALUES TO WSS-CAI-IOPC-IDX-FRM.
      *
           MOVE 1 TO FRM-IFLD.
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.

           MOVE 1 TO WSS-F IN WSS-VARI.
       LUP-PUT-TAB.
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI
              GO TO FIN-PUT-TAB.
           IF WSS-IIC-IOPC-V(WSS-F) > ZEROES
              PERFORM PUT-TAB-MOG
              PERFORM PUT-MOG-QUE
              PERFORM SET-FRM-IFLD
           ELSE
           IF FRM-IFLD = 1 AND WSS-F = 1
              MOVE 'COL' TO MSG-COD-SIST
              MOVE 'MOG    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE '00' TO FIO-STAT
      *       MOVE 'S' TO SCR-IERR
              GO TO FIN-PUT-TAB
           ELSE
              MOVE '00' TO FIO-STAT
              GO TO FIN-PUT-TAB.

           ADD 1 TO WSS-F IN WSS-VARI.
           GO TO LUP-PUT-TAB.
       FIN-PUT-TAB.
           MOVE 1                  TO QUE-NITM.
           MOVE MOG-LEN-TSMOG-MAS  TO QUE-LITM.
           MOVE MOG-NOM-TSMOG-MAS  TO QUE-COLA.

           IF WSS-NITM > WSS-NITMMAX
              MOVE 'S'           TO MOG-MAS-IND-HMAS
           ELSE
              MOVE 'N'           TO MOG-MAS-IND-HMAS.

           MOVE WSS-NITM         TO WSS-NUM-ICUR.
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.
           MOVE WSS-NUM-ICUR-ALF TO MOG-MAS-NUM-TOCC.
           MOVE MOG-MAS-ITEM     TO QUE-ITEM.
           MOVE QUE-PUT          TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF WSS-NITM > WSS-IMAX
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'MOGTABDMAS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       EXT-PUT-TAB.
           EXIT.

       SET-FRM-IFLD SECTION.
       INI-SET-FRM-IFLD.
           IF SCR-OFRM-SRV
                MOVE 1 TO FRM-IFLD
           ELSE
                ADD 1 TO FRM-IFLD.
       FIN-SET-FRM-IFLD.
           EXIT.
      *--------------------------------------------
       SCRL-TAB-MOG SECTION.
       INI-SCRL-TAB-MOG.
           MOVE SPACES TO MSG-COD-MENS.

           IF FRM-FFLD = FRM-FFLD-PF6
               PERFORM PUT-TAB-ASC
           ELSE
               PERFORM PUT-TAB-DSC.
           IF MSG-COD-MENS > SPACES
               MOVE 'COL' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-MOG.
           EXIT.

      *--------------------------------------------
       PUT-TAB-MOG SECTION.
       INI-PUT-TAB-MOG.
           PERFORM CLR-WSS.
           MOVE WSS-CAI-IOPC-V(WSS-F) TO WSS-CAI-IOPC-MOG IN WSS-VARI.
           MOVE WSS-IIC-IOPC-V(WSS-F) TO WSS-IIC-IOPC-MOG IN WSS-VARI.
           MOVE WSS-NUM-PROD-V(WSS-F) TO WSS-NUM-PROD-MOG IN WSS-VARI.
           MOVE WSS-NUM-REND-V(WSS-F) TO WSS-NUM-REND-MOG IN WSS-VARI.
           MOVE WSS-NUM-RNGD-V(WSS-F) TO WSS-NUM-RNGD-MOG IN WSS-VARI.

           MOVE WSS-CAI-IOPC-MOG IN WSS-VARI TO WSS-CAI-IOPC-FRM.
           MOVE WSS-IIC-IOPC-MOG IN WSS-VARI TO WSS-IIC-IOPC-FRM.
           MOVE WSS-NUM-PROD-MOG IN WSS-VARI TO WSS-NUM-PROD-FRM.
           MOVE WSS-NUM-REND-MOG IN WSS-VARI TO WSS-NUM-REND-FRM.
           MOVE WSS-NUM-RNGD-MOG IN WSS-VARI TO WSS-NUM-RNGD-FRM.

           IF FRM-IFLD > WSS-IMAX
              GO TO FIN-PUT-TAB-MOG.

           MOVE WSS-CAI-IOPC-V(WSS-F)      TO
               FRM-CAI-IOPC IN MOG-FLD(FRM-IFLD).
           MOVE WSS-IIC-IOPC-V(WSS-F)      TO
               FRM-IIC-IOPC IN MOG-FLD(FRM-IFLD).
           MOVE WSS-NUM-PROD-V(WSS-F)      TO
               FRM-NUM-PROD IN MOG-FLD(FRM-IFLD).
           MOVE WSS-NUM-REND-V(WSS-F)      TO
               FRM-NUM-REND IN MOG-FLD(FRM-IFLD).
           MOVE WSS-NUM-RNGD-V(WSS-F)      TO
               FRM-NUM-RNGD IN MOG-FLD(FRM-IFLD).

       FIN-PUT-TAB-MOG.
           EXIT.
      *----------------------------------------------------
       PUT-MOG-QUE SECTION.
       INI-PUT-MOG-QUE.
           PERFORM PRO-FLD-QUE.
           ADD 1 TO WSS-NITM.
           MOVE MOG-NOM-TSMOG    TO QUE-COLA.
           MOVE MOG-LEN-TSMOG    TO QUE-LITM.
           MOVE WSS-NITM         TO QUE-NITM.
           MOVE SPACES           TO QUE-ITEM.
           IF SCR-OFRM-SRV
              MOVE MOG-VEC-TS    TO QUE-ITEM
           ELSE
              MOVE WSS-CAI-IOPC-IDX-FRM  TO QUE-ITEM.
           MOVE QUE-PUT  TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
       FIN-PUT-MOG-QUE.
           EXIT.
      *----------------------------------------------------

       PUT-TAB-ASC SECTION.
       INI-PUT-TAB-ASC.
           MOVE SPACES TO MSG-COD-MENS.
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.
           MOVE 0 TO FRM-IFLD.
       LUP-PUT-TAB-ASC.
           ADD 1              TO QUE-NITM.
           MOVE MOG-NOM-TSMOG TO QUE-COLA.
           MOVE MOG-LEN-TSMOG TO QUE-LITM.
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD = 0
                   MOVE 'MOGTABNONXT' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-ASC
               ELSE
                   GO TO FIN-PUT-TAB-ASC.
           ADD 1 TO FRM-IFLD.
           MOVE QUE-ITEM TO FRM-CAI-IOPC-IDX(FRM-IFLD).
           IF FRM-IFLD < WSS-IMAX
               GO TO LUP-PUT-TAB-ASC.
       FIN-PUT-TAB-ASC.
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX
               ADD 1 FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1
                                 UNTIL WSS-I > WSS-IMAX.
           IF FRM-IFLD > 0
               ADD 1 TO SCR-NUM-PANT.
           IF FRM-IFLD = WSS-IMAX
               ADD 1              TO QUE-NITM
               MOVE MOG-NOM-TSMOG TO QUE-COLA
               MOVE MOG-LEN-TSMOG TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT = QUE-STAT-OKS
                   MOVE 'COL'        TO MSG-COD-SIST
                   MOVE 'MOGTABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-ASC.
           EXIT.

       PUT-TAB-DSC SECTION.
       INI-PUT-TAB-DSC.
           MOVE SPACES TO MSG-COD-MENS.
           IF SCR-NUM-PANT = 1
               MOVE 'MOGTABNOPRV' TO MSG-COD-MENS
               GO TO EXT-PUT-TAB-DSC.

           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.
           ADD 1 TO QUE-NITM.
           ADD 1 WSS-IMAX GIVING FRM-IFLD.
       LUP-PUT-TAB-DSC.
           SUBTRACT  1 FROM QUE-NITM.
           MOVE MOG-NOM-TSMOG TO QUE-COLA
           MOVE MOG-LEN-TSMOG TO QUE-LITM
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD > WSS-IMAX
                   MOVE 'MOGTABNOPRV' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-DSC
               ELSE
                   GO TO FIN-PUT-TAB-DSC.
           SUBTRACT 1 FROM FRM-IFLD.
           MOVE QUE-ITEM TO FRM-CAI-IOPC-IDX(FRM-IFLD).
           IF FRM-IFLD > 1
               GO TO LUP-PUT-TAB-DSC.
       FIN-PUT-TAB-DSC.

           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1
                                 UNTIL WSS-I < 1.

           IF FRM-IFLD NOT > WSS-IMAX
               SUBTRACT 1 FROM SCR-NUM-PANT
               MOVE 'COL'        TO MSG-COD-SIST
               MOVE 'MOGTABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-DSC.
           EXIT.


       CLR-FRM SECTION.
       INI-CLR-FRM.
           MOVE SPACES TO FRM-CAI-IOPC IN MOG-FLD(WSS-I).
           MOVE ZEROES TO FRM-IIC-IOPC IN MOG-FLD(WSS-I).
           MOVE ZEROES TO FRM-NUM-PROD IN MOG-FLD(WSS-I).
           MOVE ZEROES TO FRM-NUM-REND IN MOG-FLD(WSS-I).
           MOVE ZEROES TO FRM-NUM-RNGD IN MOG-FLD(WSS-I).
       FIN-CLR-FRM.
           EXIT.

       CLR-WSS SECTION.
       INI-CLR-WSS.
           MOVE SPACES TO WSS-CAI-IOPC-MOG IN WSS-VARI.
           MOVE ZEROES TO WSS-IIC-IOPC-MOG IN WSS-VARI.
           MOVE ZEROES TO WSS-NUM-PROD-MOG IN WSS-VARI.
           MOVE ZEROES TO WSS-NUM-REND-MOG IN WSS-VARI.
           MOVE ZEROES TO WSS-NUM-RNGD-MOG IN WSS-VARI.

           IF SCR-OFRM-SRV
               PERFORM CLR-FRM-MTX.
       FIN-CLR-WSS.
           EXIT.

       CLR-FRM-MTX SECTION.
       INI-CLR-FRM-MTX.
           MOVE SPACES TO FRM-CAI-IOPC IN MOG-FLD(1).
           MOVE ZEROES TO FRM-IIC-IOPC IN MOG-FLD(1).
           MOVE ZEROES TO FRM-NUM-PROD IN MOG-FLD(1).
           MOVE ZEROES TO FRM-NUM-REND IN MOG-FLD(1).
           MOVE ZEROES TO FRM-NUM-RNGD IN MOG-FLD(1).
       FIN-CLR-FRM-MTX.
           EXIT.

       CRG-VEC-MOG SECTION.
       INI-CRG-VEC-MOG.
           ADD 1 TO WSS-J.
           IF WSS-J IN WSS-VARI > WSS-JMAX IN WSS-VARI
               GO TO FIN-CRG-VEC-MOG.

           PERFORM CLR-MOG-VEC.
           MOVE CEN-CAI-IENT IN CEN      TO
                             WSS-CAI-IOPC-V IN WSS-VARI(WSS-J).
           MOVE CEN-IIC-IENT IN CEN      TO
                             WSS-IIC-IOPC-V IN WSS-VARI(WSS-J).
           MOVE CEN-NUM-CON1 IN CEN      TO
                             WSS-NUM-PROD-V IN WSS-VARI(WSS-J).
           MOVE CEN-NUM-CON2 IN CEN      TO
                             WSS-NUM-REND-V IN WSS-VARI(WSS-J).
           MOVE CEN-NUM-CON3 IN CEN      TO
                             WSS-NUM-RNGD-V IN WSS-VARI(WSS-J).
       FIN-CRG-VEC-MOG.
           EXIT.

       CLR-MOG-VEC SECTION.
       INI-CLR-MOG-VEC.
           MOVE SPACES TO    WSS-CAI-IOPC-V(WSS-I).
           MOVE ZEROES TO    WSS-IIC-IOPC-V(WSS-I).
           MOVE ZEROES TO    WSS-NUM-PROD-V(WSS-I).
           MOVE ZEROES TO    WSS-NUM-REND-V(WSS-I).
           MOVE ZEROES TO    WSS-NUM-RNGD-V(WSS-I).
       FIN-CLR-MOG-VEC.
           EXIT.

       PRO-FLD-QUE SECTION.
       INI-PRO-FLD-QUE.
           MOVE 'P' TO WSS-CAI-IOPC-ATR.
           MOVE 'P' TO WSS-IIC-IOPC-ATR.
           MOVE 'P' TO WSS-NUM-PROD-ATR.
           MOVE 'P' TO WSS-NUM-REND-ATR.
           MOVE 'P' TO WSS-NUM-RNGD-ATR.
       FIN-PRO-FLD-QUE.
           EXIT.
      *>>>>
*% END
