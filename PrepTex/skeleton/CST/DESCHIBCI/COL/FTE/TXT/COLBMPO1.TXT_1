*%! Codigo adicional para programa PO1.
*% IF SETTAG
*% FNCBQ = "111002"
*% VSNBQ = "11.10.11"
*% FNCBU = "111002"
*% VSNBU = "11.10.11"
*% END
*% IF FIN_PUT_ALL_PO1
      *<<<< FIN_PUT_ALL_PO1
           PERFORM PUT-ALL-PO2.
      *Busca glosa en tablas
           IF POC-COD-CVTI IN PO1-FLD > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'VLR' TO TAB-COD-TTAB IN TAB
               MOVE 'I' TO WSS-COD-TVTI
               MOVE POC-COD-CVTI IN PO1-FLD TO WSS-COD-CVTI
               MOVE POC-MSC-PBTI IN PO1-FLD TO WSS-COD-TCTI
               MOVE WSS-COD-VCAM TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               IF POC-COD-CVTI IN PO1-FLD > SPACES
                   MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-CVTI IN PO1-FLD
               ELSE
                   MOVE SPACES TO FRM-GLS-CVTI IN PO1-FLD.
      *BUS-TAB busca glosa de codigo en tabla
           IF POC-COD-AOOC IN PO1-FLD > SPACES
               MOVE POC-COD-TOOC IN PO1-FLD TO WSS-COD-TOOC
               MOVE POC-COD-AOOC IN PO1-FLD TO WSS-COD-AOOC
               MOVE 'COL' TO TAB-COD-SIST
               MOVE 'AUX' TO TAB-COD-TTAB IN TAB
               MOVE WSS-COD-COOC TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-AOOC IN PO1-FLD.
           IF POC-IND-RENO IN PO1-FLD > SPACES
               MOVE POC-IND-RENO IN PO1-FLD TO NUM-NUME
               MOVE NUM-ZERO-S TO NUM-ZERO
               MOVE NUM-SGNO-N TO NUM-SGNO
               MOVE 3 TO NUM-NENT
               MOVE 0 TO NUM-NDCM
               PERFORM VAL-NUM
               IF NUM-STAT-OKS
                   MOVE SPACES TO FRM-GLS-RENO IN PO1-FLD.
           IF POC-COD-SEGD IN PO1-FLD > SPACES
               MOVE POC-COD-SEGD IN PO1-FLD TO NUM-NUME
               MOVE NUM-ZERO-S TO NUM-ZERO
               MOVE NUM-SGNO-N TO NUM-SGNO
               MOVE 3 TO NUM-NENT
               MOVE 0 TO NUM-NDCM
               PERFORM VAL-NUM
               IF NUM-STAT-OKS
                   MOVE SPACES TO FRM-GLS-SEGD IN PO1-FLD.
      *>>>>
*% END
*% IF FIN_SCR_CLRA_PO1
      *<<<< FIN_SCR_CLRA_PO1
           PERFORM SCR-CLRA-PO2.
      *>>>>
*% END
*% IF POC_GLS_PCMI
      *<<<< POC-GLS-PCMI
           IF POC-GLS-PCMI IN PO1-FLD = ZEROES
               GO TO FIN-VAL-POC-GLS-PCMI.
           MOVE POC-GLS-PCMI IN PO1-FLD TO WSS-FEC-RANG.
           PERFORM VAL-FEC-RANGO.
           IF FRM-SUAR-MAL = FRM-SUAR
               MOVE 'COL' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>>
*% END
*% IF POC_GLS_PCMA
      *<<<< POC-GLS-PCMA
           IF POC-GLS-PCMA IN PO1-FLD = ZEROES
               GO TO FIN-VAL-POC-GLS-PCMA.
           MOVE POC-GLS-PCMA IN PO1-FLD TO WSS-FEC-RANG.
           PERFORM VAL-FEC-RANGO.
           IF FRM-SUAR-MAL = FRM-SUAR
               MOVE 'COL' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>>
*% END
*% IF POC_IND_MTIC
      *<<<< POC-IND-MTIC
           IF POC-IND-MTIC IN PO1-FLD NOT > SPACES
               MOVE 'N' TO POC-IND-MTIC IN PO1-FLD.
      *>>>>
*% END
*% IF POC_COD_CVTI
      *<<<<
      *VAL-COD Valida codigo de tabla
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'VLR' TO TAB-COD-TTAB IN TAB.
           MOVE 'I' TO WSS-COD-TVTI.
           MOVE POC-COD-CVTI IN PO1-FLD TO WSS-COD-CVTI.
           MOVE POC-MSC-PBTI IN PO1-FLD TO WSS-COD-TCTI.
           MOVE WSS-COD-VCAM TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT = 'COD    NEX' AND
                POC-COD-CVTI IN PO1-FLD > SPACES
                MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-CVTI IN PO1-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-CVTI IN PO1-FLD.
      *>>>>
*% END
*% IF POC_COD_AOOC
      *<<<<
      *VAL-COD Valida codigo de tabla
           MOVE POC-COD-TOOC IN PO1-FLD TO WSS-COD-TOOC.
           MOVE POC-COD-AOOC IN PO1-FLD TO WSS-COD-AOOC.
           MOVE 'COL' TO TAB-COD-SIST.
           MOVE 'AUX' TO TAB-COD-TTAB IN TAB.
           MOVE WSS-COD-COOC TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT = 'COD    NEX' AND
                POC-COD-AOOC IN PO1-FLD > SPACES
                MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-AOOC IN PO1-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-AOOC IN PO1-FLD.
      *>>>>
*% END
*% IF POC_IND_RENO
      *<<<< POC_IND_RENO
           IF POC-IND-RENO IN PO1-FLD > SPACES
               MOVE POC-IND-RENO IN PO1-FLD TO NUM-NUME
               MOVE NUM-ZERO-S TO NUM-ZERO
               MOVE NUM-SGNO-N TO NUM-SGNO
               MOVE 3 TO NUM-NENT
               MOVE 0 TO NUM-NDCM
               PERFORM VAL-NUM
               IF NUM-STAT-OKS
                   IF NOT (NUM-NN9N NOT < ZEROES AND
                           NUM-NN9N NOT > 99 )
                       MOVE 'COL' TO MSG-COD-SIST
                       MOVE 'INDRENONOFAM' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE FRM-SUAR-MAL TO FRM-SUAR
                       GO TO FIN-VAL-POC-IND-RENO
                   ELSE
                       MOVE NUM-NN9N TO WSS-NUM-DIGT
                       MOVE WSS-NUM-DIGT TO POC-IND-RENO IN PO1-FLD
                       MOVE SPACES TO FRM-GLS-RENO IN PO1-FLD
                       GO TO FIN-VAL-POC-IND-RENO.
      *>>>>
*% END
*% IF POC_COD_SEGD
      *<<<< POC_COD_SEGD
           IF POC-COD-SEGD IN PO1-FLD > SPACES
               MOVE POC-COD-SEGD IN PO1-FLD TO NUM-NUME
               MOVE NUM-ZERO-S TO NUM-ZERO
               MOVE NUM-SGNO-N TO NUM-SGNO
               MOVE 6 TO NUM-NENT
               MOVE 0 TO NUM-NDCM
               PERFORM VAL-NUM
               IF NUM-STAT-OKS
                   IF NOT (NUM-NN9N NOT < ZEROES AND
                           NUM-NN9N NOT > 99 )
                       MOVE 'COL' TO MSG-COD-SIST
                       MOVE 'CODSEGDNOFAM' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE FRM-SUAR-MAL TO FRM-SUAR
                       GO TO FIN-VAL-POC-COD-SEGD
                   ELSE
                       MOVE NUM-NN9N TO WSS-NUM-DIGT 
                       MOVE WSS-NUM-DIGT TO POC-COD-SEGD IN PO1-FLD
                       MOVE SPACES TO FRM-GLS-SEGD IN PO1-FLD
                       GO TO FIN-VAL-POC-COD-SEGD.
      *>>>>
*% END
*% IF VAL_CON_FLD_PO1
      *<<<< VAL_CON_FLD_PO1
      * Valida Consistencias entre Campos.
      *ALD
           MOVE POC-MSC-BCTI IN PO1-FLD TO POC-MSC-BCTI IN POC.
           MOVE POC-MSC-PBTI IN PO1-FLD TO POC-MSC-PBTI IN POC.
           MOVE POC-MSC-BPBT IN PO1-FLD TO POC-MSC-BPBT IN POC.
           MOVE POC-MSC-SPBT IN PO1-FLD TO POC-MSC-SPBT IN POC.
           MOVE POC-COD-CVTI IN PO1-FLD TO POC-COD-CVTI IN POC.
           MOVE POC-MSC-TFPR IN PO1-FLD TO POC-MSC-TFPR IN POC.
           MOVE POC-NUM-PREP IN PO1-FLD TO POC-NUM-PREP IN POC.
           MOVE POC-MSC-UTPR IN PO1-FLD TO POC-MSC-UTPR IN POC.
           IF POC-COD-TIPT IN POC > SPACES OR
              POC-COD-CVTI IN POC > SPACES OR
              POC-MSC-TFPR IN POC > SPACES OR
              POC-NUM-PREP IN POC > ZEROES OR
              POC-MSC-UTPR IN POC > SPACES
               MOVE 'COL' TO TAB-COD-SIST
               MOVE 'INT' TO TAB-COD-TTAB IN TAB
               MOVE POC-COD-TINT IN POC TO TAB-COD-CTAB IN TAB
               PERFORM VAL-COD
               IF MSG-COD-MENS = 'COD    NEX'  OR 'COD    NVG'
                   MOVE 'COL' TO MSG-COD-SIST
                   MOVE 'CODTINT' TO MSG-COD-TMSG
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   GO TO FIN-VAL-CON-FLD-PO1
               ELSE
                   NEXT SENTENCE
           ELSE
               IF POC-IND-MTIC IN PO1-FLD NOT = 'S'
                   MOVE 'COL' TO MSG-COD-SIST
                   MOVE 'CODTINTOBL' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   GO TO FIN-VAL-CON-FLD-PO1.
           IF NOT ( POC-VAL-CMAX IN PO1-FLD = ZEROES OR
              POC-VAL-CMIN IN PO1-FLD NOT > POC-VAL-CMAX IN PO1-FLD)
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'VALCMAXCMIN<' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CON-FLD-PO1.
           IF POC-NUM-VMAX IN PO1-FLD > ZEROES AND
              POC-NUM-VMIN IN PO1-FLD > ZEROES AND
              POC-NUM-VMIN IN PO1-FLD > POC-NUM-VMAX IN PO1-FLD
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'NUMVMAXVMIN<' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CON-FLD-PO1.
      * Valida Consistencias entre Periodos.
           IF ( POC-IND-PMIN IN PO1-FLD NOT = SPACES AND
                POC-NUM-PMIN IN PO1-FLD = ZEROES ) OR
              ( POC-IND-PMIN IN PO1-FLD = SPACES AND
                POC-NUM-PMIN IN PO1-FLD NOT = ZEROES )
                  MOVE 'COL' TO MSG-COD-SIST
                  MOVE 'INDPMINPMIN' TO MSG-COD-MENS
                  PERFORM GET-MSG
      *           STRING MSG-GLS-DESC
      *                  MSG-GLS-DATA DELIMITED BY SIZE
      *           INTO FRM-MENS

                  MOVE MSG-GLS-DESC TO CPT-STR1
                  MOVE MSG-GLS-DATA TO CPT-STR2
                  PERFORM CPT-BLKS
                  MOVE CPT-STRN     TO FRM-MENS

                  MOVE FRM-SUAR-MAL TO FRM-SUAR
                  GO TO FIN-VAL-CON-FLD-PO1.
           IF ( POC-IND-PMAX IN PO1-FLD NOT = SPACES AND
                POC-NUM-PMAX IN PO1-FLD = ZEROES ) OR
              ( POC-IND-PMAX IN PO1-FLD = SPACES AND
                POC-NUM-PMAX IN PO1-FLD NOT = ZEROES )
                  MOVE 'COL' TO MSG-COD-SIST
                  MOVE 'INDPMAXPMAX' TO MSG-COD-MENS
                  PERFORM GET-MSG

      *           STRING MSG-GLS-DESC
      *                  MSG-GLS-DATA DELIMITED BY SIZE
      *           INTO FRM-MENS

                  MOVE MSG-GLS-DESC TO CPT-STR1
                  MOVE MSG-GLS-DATA TO CPT-STR2
                  PERFORM CPT-BLKS
                  MOVE CPT-STRN     TO FRM-MENS

                  MOVE FRM-SUAR-MAL TO FRM-SUAR
                  GO TO FIN-VAL-CON-FLD-PO1.
           IF ( POC-NUM-PMIN IN PO1-FLD > ZEROES AND
                POC-NUM-PMAX IN PO1-FLD > ZEROES )
               MOVE POC-IND-PMIN IN PO1-FLD TO MSC-COD-CMSC IN MSC
               MOVE POC-NUM-PMIN IN PO1-FLD TO
               WSS-NUM-NFEC IN WSS-FEC-VARI
               PERFORM SUM-FEC-NFEC
               MOVE FEC-FECH TO WSS-FEC-NFE1
               MOVE POC-IND-PMAX IN PO1-FLD TO MSC-COD-CMSC IN MSC
               MOVE POC-NUM-PMAX IN PO1-FLD TO
               WSS-NUM-NFEC IN WSS-FEC-VARI
               PERFORM SUM-FEC-NFEC
               MOVE FEC-FECH TO WSS-FEC-NFE2
               PERFORM CMP-FEC-NFEC
               IF FRM-SUAR = FRM-SUAR-MAL
                   MOVE 'COL' TO MSG-COD-SIST
                   MOVE 'NUMPMINNORG' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MEN1
                   GO TO FIN-VAL-CON-FLD-PO1.
           IF ( POC-MSC-UTPR IN PO1-FLD NOT = SPACES AND
                POC-NUM-PREP IN PO1-FLD = ZEROES ) OR
      *SMC-INI 30.08.1993
              ( POC-MSC-UTPR IN PO1-FLD = SPACES AND
                POC-NUM-PREP IN PO1-FLD NOT = ZEROES  AND
                POC-MSC-TFPR IN PO1-FLD NOT = 'F' )
      *SMC-FIN 30.08.1993

                  MOVE 'COL' TO MSG-COD-SIST
                  MOVE 'MSCUTPRPREP' TO MSG-COD-MENS
                  PERFORM GET-MSG

      *           STRING MSG-GLS-DESC
      *                  MSG-GLS-DATA DELIMITED BY SIZE
      *           INTO FRM-MENS

                  MOVE MSG-GLS-DESC TO CPT-STR1
                  MOVE MSG-GLS-DATA TO CPT-STR2
                  PERFORM CPT-BLKS
                  MOVE CPT-STRN     TO FRM-MENS

                  MOVE FRM-SUAR-MAL TO FRM-SUAR
                  GO TO FIN-VAL-CON-FLD-PO1.
           MOVE POC-GLS-PCMI IN PO1-FLD TO WSS-GLS-PCMI.
           MOVE POC-GLS-PCMA IN PO1-FLD TO WSS-GLS-PCMA.
           IF WSS-NUM-MCMA > WSS-NUM-MCMI
               GO TO FIN-VAL-PERIODO
           ELSE
               IF WSS-NUM-MCMI > WSS-NUM-MCMA
                   MOVE 'COL' TO MSG-COD-SIST
                   MOVE 'NUMMCMIMCMA>' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   GO TO FIN-VAL-CON-FLD-PO1
               ELSE
                   IF WSS-NUM-DCMI > WSS-NUM-DCMA
                       MOVE 'COL' TO MSG-COD-SIST
                       MOVE 'NUMDCMIDCMA>' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE FRM-SUAR-MAL TO FRM-SUAR
                       GO TO FIN-VAL-CON-FLD-PO1
                   ELSE
                       GO TO FIN-VAL-PERIODO.
       FIN-VAL-PERIODO.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       CMP-FEC-NFEC SECTION.
       INI-CMP-FEC-NFEC.
           MOVE WSS-FEC-NFE1 IN WSS-FEC-VARI TO FEC-FEC1.
           MOVE WSS-FEC-NFE2 IN WSS-FEC-VARI TO FEC-FEC2.
           MOVE FEC-FORM-FEC TO FEC-FORM.
           MOVE FEC-CMP-FEC TO FEC-CMND.
           PERFORM PRO-FEC.
           IF FEC-STAT-GRT
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
       FIN-CMP-FEC-NFEC.
           EXIT.

       SUM-FEC-NFEC SECTION.
       INI-SUM-FEC-NFEC.
           MOVE 'TAB' TO MSC-COD-SIST.
           MOVE 'IPER' TO MSC-COD-TMSC IN MSC.
           PERFORM BUS-MSC.
           IF ( MSC-CIC-CMSC = 'D' OR
                MSC-CIC-CMSC = 'M' OR
                MSC-CIC-CMSC = 'A' )
               MOVE FEC-FORM-FEC TO FEC-FORM
               PERFORM SUM-FEC-FCOL
      *        PERFORM PRO-FEC
               PERFORM CAL-FEC
           ELSE
               MOVE ZEROES TO FEC-FECH.               
       FIN-SUM-FEC-NFEC.
           EXIT.

       SUM-FEC-FCOL SECTION.
       INI-SUM-FEC-FCOL.
           PERFORM GET-FHOY.
           MOVE HOY-DHOY TO FEC-ITM1.
           MOVE HOY-MHOY TO FEC-ITM2.
           MOVE HOY-SHOY TO FEC-ITM3.
           MOVE HOY-AHOY TO FEC-ITM4.
           IF MSC-CIC-CMSC = 'D'
               MOVE FEC-SUM-DIA                  TO FEC-CMND
               MOVE WSS-NUM-NFEC IN WSS-FEC-VARI TO FEC-NDIA
           ELSE
               IF MSC-CIC-CMSC = 'M'
                   MOVE FEC-SUM-MES                  TO FEC-CMND
                   MOVE WSS-NUM-NFEC IN WSS-FEC-VARI TO FEC-NMES
               ELSE
                   IF MSC-CIC-CMSC = 'A'
                       MOVE FEC-SUM-ANO             TO FEC-CMND
                       MOVE WSS-NUM-NFEC IN WSS-FEC-VARI TO FEC-NANO.
       FIN-SUM-FEC-FCOL.
           EXIT.

       VAL-FEC-RANGO SECTION.
       INI-VAL-FEC-RANGO.
           IF WSS-NUM-MRAN = 02
                IF WSS-NUM-DRAN NOT < 0 AND WSS-NUM-DRAN < 30
                    GO TO FIN-VAL-FEB
                ELSE
                    MOVE 'DIANORANGO' TO MSG-COD-MENS
                    MOVE FRM-SUAR-MAL TO FRM-SUAR
                    GO TO FIN-VAL-FEC-RANGO.
       FIN-VAL-FEB.
           IF WSS-NUM-MRAN NOT < 0 AND WSS-NUM-MRAN < 13
               IF WSS-NUM-MRAN = 00 OR
                  WSS-NUM-MRAN = 01 OR
                  WSS-NUM-MRAN = 03 OR
                  WSS-NUM-MRAN = 05 OR
                  WSS-NUM-MRAN = 07 OR
                  WSS-NUM-MRAN = 08 OR
                  WSS-NUM-MRAN = 10 OR
                  WSS-NUM-MRAN = 12
                   IF WSS-NUM-DRAN NOT < 0 AND WSS-NUM-DRAN < 32
                       GO TO FIN-VAL-FEC-RANGO
                   ELSE
                       MOVE 'DIANORANGO' TO MSG-COD-MENS
                       MOVE FRM-SUAR-MAL TO FRM-SUAR
                       GO TO FIN-VAL-FEC-RANGO
               ELSE
                   IF WSS-NUM-DRAN NOT < 0 AND WSS-NUM-DRAN < 31
                       GO TO FIN-VAL-FEC-RANGO
                   ELSE
                       MOVE 'DIANORANGO' TO MSG-COD-MENS
                       MOVE FRM-SUAR-MAL TO FRM-SUAR
                       GO TO FIN-VAL-FEC-RANGO
           ELSE
               MOVE 'MESNORANGO' TO MSG-COD-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
       FIN-VAL-FEC-RANGO.
           EXIT.
      *>>>>
*% END
