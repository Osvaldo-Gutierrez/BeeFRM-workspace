*%! Codigo adicional para archivo COLPQCHI
*% IF SETTAG
*% FNCPQ = "IMPPRORROGA1"
*% VSNPQ = "05.11.11.1"
*% END
*% LARGO_MI_COMMAREA = 20
*% PGM_PTC = TRUE
*% IF MI_COMMAREA                                                 
      *<<<< MI_COMMAREA                                                 
           03 CMA-VARI.                                                 
              05 SCR-IMAX      COMP                   PIC S9(04).       
              05 SCR-I         COMP                   PIC S9(04).       
      *>>>>
*% END
*% IF WSS                                                         
      *<<<< WSS 
       COPY COLWGCHI.                                                   
       COPY GNSWGCPT.                                                   
       COPY COLBRROV.                                                   
       COPY COLBRICG.                                                   
       COPY COLBRDLC.                                                   
       COPY COLBRCXE.                                                   
       COPY COLBRRCV.                                                   
       COPY COLBRRDC.                                                   
       COPY COLBRCAN.                                                   
       COPY SGCBRDBC.                                                   
       COPY TABBRCAM.                                                   
       COPY GNSWGPES.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CXE-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DLC-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CAM-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-ROV-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RCV-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-ICG-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RDC-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CAN-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.        
                                                                     
       01  WSS-VARI.                                                    
           03  WSS-IND-RENO    VALUE SPACES      PIC X(03).             
           03  WSS-CHI-IMAX    VALUE ZEROES      PIC 9(03).             
           03  WSS-BUS-CLI     VALUE 'N'         PIC X(01).             
           03  WSS-FEC-FINI.                                            
               05 WSS-NUM-SINI                   PIC 9(02).             
               05 WSS-NUM-AINI                   PIC 9(02).             
               05 WSS-NUM-MINI                   PIC 9(02).             
               05 WSS-NUM-DINI                   PIC 9(02).             
           03  WSS-FEC-FTER.                                            
               05 WSS-NUM-STER                   PIC 9(02).             
               05 WSS-NUM-ATER                   PIC 9(02).             
               05 WSS-NUM-MTER                   PIC 9(02).             
               05 WSS-NUM-DTER                   PIC 9(02).             
           03  WSS-SGV-RENO   COMP-3 VALUE ZEROES PIC S9(11)V9(04).     
           03  RCAN-CAN-KEY-ICAN                  PIC X(15).            
           03  WSS-TOT-RCAN   VALUE ZEROES        PIC 9(04).            
           03  WSS-TOT-PTTS   VALUE ZEROES        PIC 9(04).            
           03  WSS-KEY-IVCT                       PIC X(15).            
           03  WSS-I                              PIC 9(02).            
           03  WSS-FEC-CNT1                       PIC X(08).            
           03  WSS-FEC-CNT                        PIC X(08).            
           03  WSS-VAL-CNT     VALUE ZEROES       PIC S9(11)V9(04).     
           03  WSS-VAL-CNT1    VALUE ZEROES       PIC S9(11)V9(04).     
           03  WSS-COD-PESO                       PIC X(06).            
           03  WSS-CIC-IOPC.                                            
               05 WSS-CAI-IOPC                    PIC X(04).            
               05 WSS-IIC-IOPC                    PIC X(08).            
           03  WSS-CIC-IOPE.                                            
               05 WSS-CAI-IOPE                    PIC X(04).            
               05 WSS-IIC-IOPE                    PIC X(08).            

      *>>>>
*% END
*% IF INI_FIO_IBM
      *<<<<
           IF SCR-OFRM-SRV                                              
              MOVE TSK-TERM-ALF TO CHI-TERM                             
           ELSE                                                         
              MOVE EIBTRMID     TO CHI-TERM.                            
      *>>>>
*% END
*% IF INI_FND_CXE
      *<<<< INI_FND_CXE
           MOVE SPACES                    TO CXE-GLS-FLAG IN CXE.   
           MOVE CXE-CAI-IOPC IN CHI-FLD   TO CXE-CAI-IOPC IN CXE.   
           MOVE CXE-IIC-IOPC IN CHI-FLD   TO CXE-IIC-IOPC IN CXE.   
           MOVE ZEROES                    TO CXE-NUM-EVEN IN CXE.   
           MOVE 'CUR'                     TO CXE-COD-EVEN IN CXE.   
           MOVE SPACES                    TO CXE-CIC-IOPV IN CXE.   
           MOVE ZEROES                    TO CXE-NUM-IVCT IN CXE.   
           MOVE 'IMPPRG'                  TO CXE-COD-CPTO IN CXE.   
      *>>>>
*% END                
*% IF PFS_SCR_ACC                                                 
      *<<<< PFS_SCR_ACC                                                 
           IF FRM-FFLD = FRM-FFLD-PF6                     
                    OR   FRM-FFLD-PF5                     
               PERFORM VAL-NUM-KEY-CHI                    
               PERFORM VAL-NUM-FLD-CHI                    
               PERFORM RST-STAT                           
               PERFORM FND-SCR-KEY THRU FIN-SCR-KEY       
               IF SCR-STAT NOT = SCR-STAT-ABT             
                  PERFORM SCR-TRL                        
               ELSE                                       
                  PERFORM DESSCRTRL                      
                  GO TO FIN-SCR-ACC.                     
      *>>>>
*% END
*% IF INI_FND_KEY                                                 
      *<<<< INI_FND_KEY 
           MOVE SPACES TO FRM-MEN1.                           
           IF FRM-FFLD = FRM-FFLD-PF5                         
               MOVE PGM-SNUL TO PGM-STAT-OPC                  
               MOVE PGM-SNUL TO PGM-STAT-OPC                  
               SUBTRACT 1 FROM SCR-I GIVING CHI-I             
               IF CHI-I NOT > ZEROES                          
                   MOVE 'OPC PROPRV' TO MSG-COD-MENS          
                   GO TO ERR-FND-KEY                          
               ELSE                                           
                   GO TO CARGA-CHI                            
           ELSE                                               
           IF FRM-FFLD = FRM-FFLD-PF6                         
               MOVE PGM-SNUL TO PGM-STAT-OPC                  
               MOVE PGM-SNUL TO PGM-STAT-OPC                  
               ADD 1 SCR-I GIVING CHI-I                       
               IF CHI-I > SCR-IMAX                            
                   MOVE 'OPC PRONXT' TO MSG-COD-MENS          
                   GO TO ERR-FND-KEY                          
               ELSE                                           
                   GO TO CARGA-CHI.                           
           GO TO CON-FND-KEY.                                 
      *>>>>
*% END
*% IF FIN_FND_KEY                                                 
      *<<<< FIN_FND_KEY     
           IF NOT FIO-STAT-OKS                                       
              GO TO FIN-FND-KEY.                                    
       CON-FND-KEY.                 
                                 
              PERFORM KEY-ALL-CHI.                                      
            IF OPC-IIC-IOPC IN CHI-FLD > ZEROES                         
               GO TO DSP-CAR-MOV-OPC.                                   
            MOVE DBC-NUM-ICLI IN CHI-FLD TO DBC-NUM-ICLI IN DBC        
           MOVE DBC-IND-ICLI IN CHI-FLD TO DBC-IND-ICLI IN DBC     
           MOVE DBC-GLS-ICLI IN CHI-FLD TO DBC-GLS-ICLI IN DBC     
           MOVE 'DBC-IDC-ICLI' TO FIO-AKEY.                         
           MOVE FIO-GET-KEY  TO FIO-CMND.                           
           PERFORM SGC-FIO-DBC                                     
           GO TO DSP-CAR-MOV-DBC.                                   
       DSP-CAR-MOV-OPC.                                              
           MOVE OPC-CAI-IOPC IN OPC TO RDC-CAI-IOPC IN RDC.          
           MOVE OPC-IIC-IOPC IN OPC TO RDC-IIC-IOPC IN RDC.          
           MOVE ZEROES              TO RDC-NUM-IDLC IN RDC.          
           MOVE 'RDC-KEY-IDLC' TO FIO-AKEY.                          
           MOVE FIO-GET-NLS TO FIO-CMND.                             
           PERFORM COL-FIO-RDC.                                      
           IF NOT FIO-STAT-OKS                                       
               GO TO FIN-FND-KEY.                                    
    *                                                              
           MOVE FIO-GET-KEY TO FIO-CMND.                             
           PERFORM COL-FIO-OPC.                                      
           IF OPC-IND-RENO IN OPC = SPACES OR                        
               OPC-IND-RENO IN OPC NOT = 'PRO'                        
                MOVE 'COL'         TO MSG-COD-SIST                    
                MOVE 'OPC NO PRO '    TO MSG-COD-MENS                 
                PERFORM GET-MSG                                       
                MOVE MSG-GLS-DESC IN MSG TO FRM-MENS.                 
           IF OPC-VAL-SCRE IN OPC = ZEROES                           
                MOVE 'COL'         TO MSG-COD-SIST                    
                MOVE 'OPCSINSLDO '    TO MSG-COD-MENS                 
                PERFORM GET-MSG                                       
                MOVE MSG-GLS-DESC IN MSG TO FRM-MENS.                 
           MOVE RDC-CIC-ICLI IN RDC TO DBC-CIC-ICLI IN DBC.          
           MOVE FIO-GET-KEY TO FIO-CMND.                             
           PERFORM SGC-FIO-DBC.                                      
       DSP-CAR-MOV-DBC.                                              
           IF NOT FIO-STAT-OKS                                       
               GO TO FIN-FND-KEY.                                   
           MOVE FIO-STAT TO PGM-STAT-DBC.                            
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN CHI-FLD.       
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN CHI-FLD.      
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.                     
           PERFORM CPT-BLKS.                                         
           MOVE CPT-STRN TO DBC-GLS-NOMC IN DBC.                     
           MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN CHI-FLD.      
                                                                   
           MOVE ZEROES TO SCR-IMAX.                                  
           MOVE 1 TO SCR-I.                                          
           MOVE '00' TO FIO-STAT.                                    
           PERFORM OBT-DAT-TS.                                       
           MOVE 1 TO SCR-I.                                          
           MOVE SCR-I TO CHI-I.                                      
       CARGA-CHI.                                                    
           MOVE DBC-NUM-ICLI IN CHI-FLD TO DBC-NUM-ICLI IN DBC       
           MOVE DBC-IND-ICLI IN CHI-FLD TO DBC-IND-ICLI IN DBC       
           MOVE DBC-GLS-ICLI IN CHI-FLD TO DBC-GLS-ICLI IN DBC       
           MOVE 'DBC-IDC-ICLI' TO FIO-AKEY.                          
           MOVE FIO-GET-KEY  TO FIO-CMND.                            
           PERFORM SGC-FIO-DBC                                       
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.                     
           PERFORM CPT-BLKS.                                         
           MOVE CPT-STRN TO DBC-GLS-NOMC IN DBC.                     
           MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN CHI-FLD.      
           MOVE FIO-STAT TO PGM-STAT-DBC.                            
           MOVE SPACES TO MSG-COD-MENS.                              
           IF NOT SCR-OFRM-SRV                                       
              PERFORM PUT-TAB-CHI.                                      
           MOVE '00' TO FIO-STAT.                                    
           GO TO FIN-FND-KEY.                                        
      *                                                              
       ERR-FND-KEY.                                                  
           MOVE SCR-STAT-CON TO SCR-STAT.

           MOVE 'COL'  TO MSG-COD-SIST.                              
           PERFORM GET-MSG.                                          
           MOVE MSG-GLS-DESC TO FRM-MEN1.                            
           MOVE '00' TO FIO-STAT.                                    
           GO TO FIN-FND-KEY.                                        
      *>>>>
*% END
*% IF PFS_GET_CON OR PFS_GET_ACC                                  
      *<<<< PFS_GET_CON OR PFS_GET_ACC                                  
           IF NOT (FRM-FFLD = FRM-FFLD-PF6                              
                         OR   FRM-FFLD-PF5)                             
      *>>>>
*% END
*% IF EOF                                                         
      *<<<< EOF                                                         
       COPY COLBFCAN.                                                   
       COPY COLBFRDC.                                                   
       COPY COLBFRCV.                                                   
       COPY COLBFICG.                                                   
       COPY COLBFDLC.                                                   
       COPY COLBFCXE.                                                   
       COPY COLBFROV.                                                   
       COPY TABBFCAM.                                                   
       COPY GNSBGCPT.                                                   
       COPY SGCBFDBC.                                                   
       COPY GNSBBTAB.                                                   
                                                                        
       CAR-MOV-ROV SECTION.                                             
       INI-CAR-MOV-ROV.                                                 
            MOVE 'N' TO  WSS-BUS-CLI.                                   
            IF OPC-IIC-IOPC IN CHI-FLD > ZEROES                         
                MOVE OPC-CAI-IOPC IN CHI-FLD TO OPC-CAI-IOPC IN OPC     
                MOVE OPC-IIC-IOPC IN CHI-FLD TO OPC-IIC-IOPC IN OPC     
                MOVE FIO-GET-KEY TO FIO-CMND                            
                PERFORM COL-FIO-OPC                                     
                IF OPC-IND-RENO IN OPC NOT = 'PRO'                      
                   IF FRM-COD-TREN IN CHI-FLD NOT > SPACES              
                      IF OPC-IND-RENO IN OPC NOT > SPACES               
                       MOVE OPC-COD-VCOC IN OPC TO CHI-GLS-VCOC(CHI-I)  
                       MOVE OPC-COD-COOC IN OPC TO CHI-COD-COOC(CHI-I)  
                       MOVE OPC-COD-OFOC IN OPC TO CHI-COD-OFIC(CHI-I)  
                       MOVE OPC-NUM-VCOR IN OPC TO CHI-NUM-VCOR(CHI-I)  
                       MOVE OPC-IND-RENO IN OPC TO CHI-IND-RENO(CHI-I)  
                       MOVE OPC-FEC-FCOL IN OPC TO CHI-FEC-FCAN(CHI-I)  
                       MOVE OPC-CIC-IOPC IN OPC TO WSS-CIC-IOPE         
                       PERFORM OBT-VAL-IMPU                             
                       PERFORM OBT-TASA-CHI                             
                       MOVE ICG-SGV-TINT IN ICG TO CHI-SGV-TINT(CHI-I)  
                       MOVE OPC-CAI-IOPC IN OPC TO CHI-CAI-IOPC(CHI-I)  
                       MOVE OPC-IIC-IOPC IN OPC TO CHI-IIC-IOPC(CHI-I)  
                       PERFORM INC-REG-TS                               
                       GO TO FIN-CAR-MOV-ROV                            
                      ELSE                                              
                          GO TO OPC-CAR-MOV-ROV                         
                   ELSE                                                 
                       GO TO FIN-CAR-MOV-ROV                            
                ELSE                                                    
                    GO TO OPC-CAR-MOV-ROV.                              
            MOVE 'S' TO WSS-BUS-CLI.                                    
            MOVE DBC-NUM-ICLI IN CHI-FLD TO DBC-NUM-ICLI IN DBC         
            MOVE DBC-IND-ICLI IN CHI-FLD TO DBC-IND-ICLI IN DBC         
            MOVE DBC-GLS-ICLI IN CHI-FLD TO DBC-GLS-ICLI IN DBC         
            MOVE 'DBC-IDC-ICLI' TO FIO-AKEY.                            
            MOVE FIO-GET-KEY  TO FIO-CMND.                              
            PERFORM SGC-FIO-DBC                                         
            MOVE DBC-CIC-ICLI IN DBC TO RDC-CIC-ICLI IN RDC             
            MOVE SPACES              TO RDC-CAI-IOPC IN RDC             
            MOVE ZEROES              TO RDC-NUM-IDLC IN RDC             
            MOVE FIO-GET-NLS TO FIO-CMND.                               
       RDC-CAR-MOV-ROV.                                                 
            PERFORM COL-FIO-RDC                                         
            IF FIO-STAT-OKS AND                                         
               RDC-CIC-ICLI IN RDC = DBC-CIC-ICLI IN DBC AND            
               RDC-NUM-IDLC IN RDC = ZEROES                             
                MOVE RDC-CIC-IOPC IN RDC TO OPC-CIC-IOPC IN OPC         
                MOVE FIO-GET-KEY TO FIO-CMND                            
                PERFORM COL-FIO-OPC                                     
                IF  FIO-STAT-OKS AND                                    
                    OPC-IND-RENO IN OPC = 'PRO' AND                     
      *PDM                                                              
                    OPC-MSC-STAT IN OPC ='A' AND                        
                    OPC-VAL-SCRE IN OPC > ZEROES                        
                     MOVE OPC-CAI-IOPC IN OPC TO WSS-CAI-IOPE           
                     MOVE OPC-IIC-IOPC IN OPC TO WSS-IIC-IOPE           
                     GO TO LUP-CAR-MOV-ROV                              
                ELSE                                                    
                     MOVE FIO-GET-NXT TO FIO-CMND                       
                     GO TO RDC-CAR-MOV-ROV                              
            ELSE                                                        
                GO TO FIN-CAR-MOV-ROV.                                  
       OPC-CAR-MOV-ROV.                                                 
            MOVE OPC-CAI-IOPC IN CHI-FLD TO OPC-CAI-IOPC IN OPC.        
            MOVE OPC-IIC-IOPC IN CHI-FLD TO OPC-IIC-IOPC IN OPC.        
            MOVE OPC-CAI-IOPC IN CHI-FLD TO WSS-CAI-IOPE.               
            MOVE OPC-IIC-IOPC IN CHI-FLD TO WSS-IIC-IOPE.               
       LUP-CAR-MOV-ROV.                                                 
            MOVE FIO-GET-KEY TO FIO-CMND.                               
            PERFORM COL-FIO-OPC.                                        
            IF OPC-IND-RENO IN OPC NOT > SPACES                         
                MOVE OPC-COD-VCOC IN OPC TO CHI-GLS-VCOC(CHI-I)         
                MOVE OPC-COD-COOC IN OPC TO CHI-COD-COOC(CHI-I)         
                MOVE OPC-COD-OFOC IN OPC TO CHI-COD-OFIC(CHI-I)         
                MOVE OPC-NUM-VCOR IN OPC TO CHI-NUM-VCOR(CHI-I)         
                MOVE OPC-IND-RENO IN OPC TO CHI-IND-RENO(CHI-I)         
                MOVE OPC-FEC-FCOL IN OPC TO CHI-FEC-FCAN(CHI-I)         
                PERFORM OBT-TASA-CHI                                    
                MOVE ICG-SGV-TINT IN ICG TO CHI-SGV-TINT(CHI-I)         
                MOVE WSS-CAI-IOPE        TO CHI-CAI-IOPC(CHI-I)         
                MOVE WSS-IIC-IOPE        TO CHI-IIC-IOPC(CHI-I)         
                PERFORM OBT-VAL-IMPU                                    
      *                                                                 
      *         MOVE CAN-CIC-IOPC IN CAN TO CHI-COD-RCAN(CHI-I)         
                PERFORM INC-REG-TS                                      
                IF WSS-BUS-CLI = 'S'                                    
                     MOVE FIO-GET-NXT TO FIO-CMND                       
                     GO TO RDC-CAR-MOV-ROV                              
                ELSE                                                    
                    GO TO FIN-CAR-MOV-ROV.                              
            PERFORM GET-ROV-REN.                                        
            IF FRM-FEC-FTER IN CHI-FLD > ZEROES                         
                MOVE FRM-NUM-SINI IN CHI-FLD TO WSS-NUM-SINI            
                MOVE FRM-NUM-AINI IN CHI-FLD TO WSS-NUM-AINI            
                MOVE FRM-NUM-MINI IN CHI-FLD TO WSS-NUM-MINI            
                MOVE FRM-NUM-DINI IN CHI-FLD TO WSS-NUM-DINI            
                MOVE FRM-NUM-STER IN CHI-FLD TO WSS-NUM-STER            
                MOVE FRM-NUM-ATER IN CHI-FLD TO WSS-NUM-ATER            
                MOVE FRM-NUM-MTER IN CHI-FLD TO WSS-NUM-MTER            
                MOVE FRM-NUM-DTER IN CHI-FLD TO WSS-NUM-DTER            
                IF CAN-FEC-FCAN IN CAN < WSS-FEC-FINI OR                
                   CAN-FEC-FCAN IN CAN > WSS-FEC-FTER                   
                    MOVE CAN-CIC-IOPC IN CAN  TO WSS-CIC-IOPE           
                    MOVE WSS-CIC-IOPE        TO OPC-CIC-IOPC IN OPC     
                    GO TO LUP-CAR-MOV-ROV.                              
                                                                        
            MOVE CAN-CIC-IOPC IN CAN TO CHI-COD-RCAN(CHI-I).            
            MOVE SPACES                  TO ROV-CIC-IOPV IN ROV.        
            MOVE ZEROES                  TO ROV-NUM-IVCT IN ROV.        
      *                                                                 
           MOVE WSS-CAI-IOPE        TO CHI-CAI-IOPC(CHI-I).             
           MOVE WSS-IIC-IOPE        TO CHI-IIC-IOPC(CHI-I).             
           MOVE CAN-FEC-FCAN IN CAN TO CHI-FEC-FCAN(CHI-I).             
      *    MOVE ZEROES              TO CHI-NUM-ICAN(CHI-I).             
      *    MOVE CAN-COD-RCAN IN CAN TO CHI-COD-RCAN(CHI-I).             
           MOVE CAN-SGV-TREN IN CAN TO CHI-SGV-RENO(CHI-I).             
           MOVE CAN-SGV-TPAG IN CAN TO CHI-SGV-TPAG(CHI-I).             
                                                                        
           MOVE CAN-CIC-IOPC IN CAN TO RCV-CIC-IOPC IN RCV.             
           MOVE CAN-NUM-ICAN IN CAN TO RCV-NUM-ICAN IN RCV.             
           MOVE SPACES              TO RCV-CAI-IOPV IN RCV.             
           MOVE ZEROES              TO RCV-IIC-IOPV IN RCV.             
           MOVE ZEROES              TO RCV-NUM-IVCT IN RCV.             
           MOVE FIO-GET-NLS TO FIO-CMND.                                
           PERFORM COL-FIO-RCV.                                         
           IF NOT (FIO-STAT-OKS AND                                     
              CAN-CIC-IOPC IN CAN = RCV-CIC-IOPC IN RCV AND             
              CAN-NUM-ICAN IN CAN = RCV-NUM-ICAN IN RCV)                
               GO TO FIN-CAR-MOV-ROV.                                   
                                                                        
                                                                        
           MOVE RCV-SGV-CAPI IN RCV TO CHI-SGV-CAPI(CHI-I).             
                                                                        
           MOVE OPC-COD-VCOC IN OPC TO CHI-GLS-VCOC(CHI-I).             
           MOVE OPC-COD-COOC IN OPC TO CHI-COD-COOC(CHI-I).             
           MOVE OPC-COD-OFOC IN OPC TO CHI-COD-OFIC(CHI-I).             
           MOVE OPC-NUM-VCOR IN OPC TO CHI-NUM-VCOR(CHI-I).             
           MOVE OPC-IND-RENO IN OPC TO CHI-IND-RENO(CHI-I).             
           MOVE OPC-IND-RENO IN OPC TO WSS-IND-RENO.                    
           MOVE WSS-CIC-IOPE        TO DLC-CIC-IOPC IN DLC.             
           MOVE 1                   TO DLC-NUM-IDLC IN DLC.             
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM COL-FIO-DLC.                                         
           IF FIO-STAT-OKS                                              
               MOVE DLC-VAL-IMPU IN DLC TO CHI-SGV-TPAG(CHI-I)          
               MOVE DLC-NUM-PIMP IN DLC TO CHI-NUM-ICAN(CHI-I)          
           ELSE                                                         
               MOVE ZEROES              TO CHI-NUM-ICAN(CHI-I)          
               MOVE ZEROES              TO CHI-SGV-TPAG(CHI-I).         
      *    MOVE LOW-VALUES TO CXE.                                      
      *    MOVE SPACES               TO CXE-GLS-FLAG IN CXE.            
      *    MOVE WSS-CIC-IOPE         TO CXE-CIC-IOPC IN CXE.            
      *    MOVE ZEROES               TO CXE-NUM-EVEN IN CXE.            
      *    MOVE 'CUR'                TO CXE-COD-EVEN IN CXE.            
      *    MOVE SPACES               TO CXE-CIC-IOPV IN CXE.            
      *    MOVE ZEROES               TO CXE-NUM-IVCT IN CXE.            
      *    MOVE 'IMPPRG'             TO CXE-COD-CPTO IN CXE.            
      *    MOVE FIO-GET-KEY TO FIO-CMND.                                
      *    PERFORM COL-FIO-CXE.                                         
      *    IF FIO-STAT-OKS                                              
      *        MOVE CXE-SGV-TINT IN CXE TO CHI-NUM-ICAN(CHI-I)          
      *    ELSE                                                         
      *        MOVE ZEROES              TO CHI-NUM-ICAN(CHI-I).         
                                                                        
           PERFORM OBT-TASA-CHI.                                        
           MOVE ICG-SGV-TINT IN ICG TO CHI-SGV-TINT(CHI-I).             
                                                                        
           MOVE CHI-COD-RCAN(CHI-I) TO OPC-CIC-IOPC IN OPC.             
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM COL-FIO-OPC.                                         
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-CAR-MOV-ROV.                                   
           MOVE OPC-NUM-IVEN IN OPC TO CHI-NUM-IVEN(CHI-I).             
                                                                        
                                                                        
           MOVE CHI-COD-RCAN(CHI-I) TO WSS-CIC-IOPE.                    
           PERFORM INC-REG-TS.                                          
           MOVE WSS-CIC-IOPE        TO OPC-CIC-IOPC IN OPC.             
           IF WSS-IND-RENO  = 'PRO'                                     
                GO TO LUP-CAR-MOV-ROV.                                  
                                                                        
       FIN-CAR-MOV-ROV.                                                 
           EXIT.                                                        
                                                                        
       SRT-CHI-FCAN SECTION.                                            
       INI-SRT-CHI-FCAN.                                                
            SUBTRACT 1 FROM CHI-JMAX GIVING CHI-SMAX.                   
            PERFORM INC-CHI-FCAN VARYING CHI-S FROM 1 BY 1              
            UNTIL CHI-S > CHI-SMAX.                                     
       FIN-SRT-CHI-FCAN.                                                
           EXIT.                                                        
                                                                        
       INC-CHI-FCAN SECTION.                                            
       INI-INC-CHI-FCAN.                                                
            ADD 1 CHI-S GIVING CHI-SMIN.                                
            PERFORM SWT-CHI-FCAN VARYING CHI-SAUX FROM CHI-SMIN BY 1    
            UNTIL CHI-SAUX > CHI-JMAX.                                  
       FIN-INC-CHI-FCAN.                                                
           EXIT.                                                        
                                                                        
       SWT-CHI-FCAN SECTION.                                            
       INI-SWT-CHI-FCAN.                                                
            IF CHI-FEC-FCAN IN CHI-TS(CHI-S) > CHI-FEC-FCAN             
                                           IN CHI-TS(CHI-SAUX)          
                MOVE CHI-FEC-FCAN IN CHI-TS(CHI-S)    TO CHI-AUXI       
                MOVE CHI-FEC-FCAN IN CHI-TS(CHI-SAUX) TO                
                                    CHI-FEC-FCAN IN  CHI-TS(CHI-S)      
                MOVE CHI-AUXI   TO CHI-FEC-FCAN IN CHI-TS(CHI-SAUX).    
       FIN-SWT-CHI-FCAN.                                                
           EXIT.                                                        
                                                                        
       OBT-PARAM SECTION.                                               
       INI-OBT-PARAM.                                                   
       FIN-OBT-PARAM.                                                   
           EXIT.                                                        
                                                                        
       OBT-DAT-TS SECTION.                                              
       INI-OBT-DAT-TS.                                                  
           EXEC CICS HANDLE CONDITION QIDERR(CON-OBT-DAT-TS)            
                                     ITEMERR(CON-OBT-DAT-TS)            
           END-EXEC.                                                    
                                                                        
      *     EXEC CICS DELETEQ TS QUEUE(CHI-NAME-QUE)                    
      *     END-EXEC.                                                   
      *<<<<                                                             
      *    MOVE EIBTRMID      TO CHI-TERM.                              
           MOVE CHI-NAME-QUE   TO QUE-COLA.                             
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
      *>>>>                                                             
       CON-OBT-DAT-TS.                                                  
           MOVE CHI-SI-IMP TO CHI-IMP.                                  
           PERFORM CLR-VEC-TS VARYING CHI-I FROM 1 BY 1 UNTIL           
                   CHI-I > CHI-IMAX.                                    
           MOVE 1 TO CHI-I.                                             
           PERFORM CAR-MOV-ROV.                                         
                                                                        
       NXT-OBT-DAT-TS.                                                  
           IF CHI-IMP = CHI-SI-IMP                                      
               PERFORM WRT-TS.                                          
       FIN-OBT-DAT-TS.                                                  
           EXIT.                                                        
                                                                        
       PUT-TAB-CHI SECTION.                                             
       INI-PUT-TAB-CHI.                                                 
                                                                        
           PERFORM RED-TS.                                              
           MOVE CHI-IMAX TO WSS-CHI-IMAX.                               
           PERFORM LLE-DAT-PNT VARYING CHI-I FROM 1 BY 1                
                   UNTIL CHI-I > CHI-IMAX.                              
       FIN-PUT-TAB-CHI.                                                 
           EXIT.                                                        
                                                                        
       LLE-DAT-PNT SECTION.                                             
       INI-LLE-DAT-PNT.                                                 
           MOVE CHI-CAI-IOPC(CHI-I) TO FRM-CAI-IOPC IN CHI-FLD(CHI-I).  
           MOVE CHI-IIC-IOPC(CHI-I) TO FRM-IIC-IOPC IN CHI-FLD(CHI-I).  
           MOVE CHI-FEC-FCAN(CHI-I) TO FRM-FEC-FCAN IN CHI-FLD(CHI-I).  
           MOVE CHI-NUM-ICAN(CHI-I) TO FRM-NUM-ICAN IN CHI-FLD(CHI-I).  
           MOVE CHI-NUM-IVEN(CHI-I) TO FRM-NUM-IVEN IN CHI-FLD(CHI-I).  
           MOVE CHI-SGV-RENO(CHI-I) TO FRM-SGV-RENO IN CHI-FLD(CHI-I).  
           MOVE CHI-SGV-CAPI(CHI-I) TO FRM-SGV-CAPI IN CHI-FLD(CHI-I).  
           MOVE CHI-GLS-VCOC(CHI-I) TO FRM-GLS-VCOC IN CHI-FLD(CHI-I).  
           MOVE CHI-COD-COOC(CHI-I) TO FRM-COD-COOC IN CHI-FLD(CHI-I).  
           MOVE CHI-COD-OFIC(CHI-I) TO FRM-COD-OFIC IN CHI-FLD(CHI-I).  
           MOVE CHI-SGV-TINT(CHI-I) TO FRM-SGV-TINT IN CHI-FLD(CHI-I).  
           MOVE CHI-NUM-VCOR(CHI-I) TO FRM-NUM-VCOR IN CHI-FLD(CHI-I).  
           MOVE CHI-IND-RENO(CHI-I) TO FRM-IND-RENO IN CHI-FLD(CHI-I).  
           MOVE CHI-COD-RCAN(CHI-I) TO FRM-COD-RCAN IN CHI-FLD(CHI-I).  
           MOVE CHI-SGV-TPAG(CHI-I) TO FRM-SGV-TPAG IN CHI-FLD(CHI-I).  
           SUBTRACT 1 FROM WSS-CHI-IMAX.                                
       FIN-LLE-DAT-PNT.                                                 
           EXIT.                                                        
                                                                        
       INC-REG-TS SECTION.                                              
       INI-INC-REG-TS.                                                  
            MOVE CHI-SI-IMP TO CHI-IMP.                                 
           ADD 1 TO CHI-I.                                              
           IF CHI-I > CHI-IMAX                                          
               PERFORM WRT-TS                                           
               PERFORM CLR-VEC-TS VARYING CHI-I FROM 1 BY 1             
                                     UNTIL CHI-I > CHI-IMAX             
                     MOVE 1 TO CHI-I                                          
               MOVE CHI-NO-IMP TO CHI-IMP.                              
       FIN-INC-REG-TS.                                                  
           EXIT.                                                        
                                                                        
      *<<<<                                                             
      *>>>>                                                             
                                                                        
       WRT-TS SECTION.                                                  
       INI-WRT-TS.                                                      
           ADD 1 TO SCR-IMAX.                                           
      *     EXEC CICS WRITEQ TS QUEUE(CHI-NAME-QUE)                     
      *                         FROM(CHI-TS)                            
      *                         LENGTH(CHI-LONG-TS)                     
      *     END-EXEC.                                                   
      *<<<<                                                             
      *    MOVE EIBTRMID      TO CHI-TERM.                              
           MOVE CHI-NAME-QUE   TO QUE-COLA.                             
           MOVE CHI-TS         TO QUE-ITEM.                             
           MOVE CHI-LONG-TS    TO QUE-LITM.                             
           MOVE QUE-PUT        TO QUE-CMND.                             
           PERFORM GNS-PRO-QUE.                                         
      *>>>>                                                             
       FIN-WRT-TS.                                                      
           EXIT.                                                        
                                                                        
       RED-TS SECTION.                                                  
       INI-RED-TS.                                                      
           MOVE CHI-I TO SCR-I.                                         
           EXEC CICS HANDLE CONDITION QIDERR(FIN-RED-TS)                
                                     ITEMERR(FIN-RED-TS)                
           END-EXEC.                                                    
      *     EXEC CICS READQ TS QUEUE(CHI-NAME-QUE)                      
      *                         INTO(CHI-TS)                            
      *                          LENGTH(CHI-LONG-TS)                    
      *                         ITEM(CHI-I)                             
      *     END-EXEC.                                                   
      *<<<<                                                             
                                                                        
      *    MOVE EIBTRMID      TO CHI-TERM.                              
           MOVE CHI-NAME-QUE   TO QUE-COLA.                             
           MOVE CHI-TS         TO QUE-ITEM.                             
           MOVE CHI-LONG-TS    TO QUE-LITM.                             
           MOVE CHI-I              TO QUE-NITM.                         
           MOVE QUE-GET            TO QUE-CMND.                         
           PERFORM GNS-PRO-QUE.                                         
           MOVE QUE-ITEM       TO CHI-TS.                               
      *>>>>                                                             
       FIN-RED-TS.                                                      
           EXIT.                                                        
                                                                        
       CLR-VEC-TS SECTION.                                              
       INI-CLR-VEC-TS.                                                  
           MOVE SPACES TO CHI-CAI-IOPC(CHI-I).                          
           MOVE ZEROES TO CHI-IIC-IOPC(CHI-I).                          
           MOVE ZEROES TO CHI-FEC-FCAN(CHI-I).                          
           MOVE ZEROES TO CHI-NUM-ICAN(CHI-I).                          
           MOVE ZEROES TO CHI-NUM-IVEN(CHI-I).                          
           MOVE ZEROES TO CHI-SGV-RENO(CHI-I).                          
           MOVE ZEROES TO CHI-SGV-CAPI(CHI-I).                          
           MOVE SPACES TO CHI-GLS-VCOC(CHI-I).                          
           MOVE SPACES TO CHI-COD-COOC(CHI-I).                          
           MOVE ZEROES TO CHI-COD-OFIC(CHI-I).                          
           MOVE ZEROES TO CHI-SGV-TINT(CHI-I).                          
           MOVE ZEROES TO CHI-NUM-VCOR(CHI-I).                          
           MOVE SPACES TO CHI-IND-RENO(CHI-I).                          
           MOVE SPACES TO CHI-COD-RCAN(CHI-I).                          
           MOVE ZEROES TO CHI-SGV-TPAG(CHI-I).                          
       FIN-CLR-VEC-TS.                                                  
           EXIT.                                                        
                                                                        
       RST-STAT SECTION.                                                
       INI-RST-STAT.                                                    
           MOVE SCR-STPP     TO SCR-STAT.                               
           MOVE SCR-MFLD-NOT TO SCR-MFLD.                               
       FIN-RST-STAT.                                                    
           EXIT.                                                        
                                                                        
       OBT-COD-MNAC SECTION.                                            
       INI-OBT-COD-MNAC.                                                
      * Obtiene Codigo Valor de Cambio de Pesos Mediante CIC de la Moned
           MOVE 'TAB'          TO FIO-SIST.                             
           MOVE 'VLR'          TO TAB-CIC-TTAB IN TAB.                  
           MOVE '0999  '       TO TAB-CIC-CTAB IN TAB.                  
           MOVE 'TAB-CIC-TABL' TO FIO-AKEY.                             
           MOVE FIO-GET-KEY    TO FIO-CMND.                             
           PERFORM GNS-FIO-TAB.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE 'TAB'          TO MSG-COD-SIST                      
               MOVE 'CIC0999  NEX' TO MSG-COD-MENS                      
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE FRM-SUAR-MAL TO FRM-SUAR.                           
           MOVE TAB-COD-CTAB IN TAB TO WSS-COD-PESO IN WSS-VARI.        
       FIN-OBT-COD-MNAC.                                                
           EXIT.                                                        
                                                                        
       GET-ROV-REN SECTION.                                             
       INI-GET-ROV-REN.                                                 
           MOVE ZEROES TO WSS-TOT-RCAN.                                 
           MOVE ZEROES TO WSS-TOT-PTTS IN WSS-VARI.                     
           IF OPC-VAL-SCRE IN OPC NOT > ZEROES                          
               PERFORM CHK-ROV-PTTS                                     
               IF WSS-TOT-PTTS IN WSS-VARI > ZEROES                     
                   GO TO VRF-RCAN.                                      
           MOVE OPC-CIC-IOPC IN OPC TO ROV-CIC-IOPC IN ROV.             
           MOVE LOW-VALUES          TO ROV-KEY-IVCT IN ROV.             
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       CIC-GET-ROV-REN.                                                 
           PERFORM COL-FIO-ROV.                                         
           IF NOT FIO-STAT-OKS OR                                       
              OPC-CIC-IOPC IN OPC NOT = ROV-CIC-IOPC IN ROV             
               GO TO VRF-RCAN.                                          
           IF NOT ( ROV-IIC-IOPV IN ROV > ZEROES AND                    
                    ROV-NUM-IVCT IN ROV > ZEROES )                      
               MOVE FIO-GET-NXT TO FIO-CMND                             
               GO TO CIC-GET-ROV-REN                                    
           ELSE                                                         
               MOVE ROV-KEY-IVCT IN ROV TO CAN-KEY-ICAN IN CAN          
               MOVE FIO-GET-KEY TO FIO-CMND                             
               PERFORM COL-FIO-CAN                                      
               IF FIO-STAT-OKS AND                                      
                   ( CAN-MSC-STAT IN CAN = 'C' OR                       
                     CAN-MSC-STAT IN CAN = 'W' OR 'P' ) OR              
                   ( OPC-MSC-STAT IN OPC = 'A' AND                      
                     CAN-MSC-STAT IN CAN = 'A' )                        
                       MOVE CAN-KEY-ICAN TO RCAN-CAN-KEY-ICAN           
                       IF OPC-MSC-STAT IN OPC = 'A' AND                 
                          CAN-MSC-STAT IN CAN = 'A' AND                 
                        ( CAN-MSC-CANC IN CAN = 'PTT' OR 'KTT' )        
                           MOVE  1  TO WSS-TOT-PTTS IN WSS-VARI         
                           MOVE CAN-SGV-TREN IN CAN TO WSS-SGV-RENO     
                           MOVE FIO-GET-NXT TO FIO-CMND                 
                           GO TO CIC-GET-ROV-REN                        
                       ELSE                                             
                           ADD 1                   TO WSS-TOT-RCAN      
                           ADD CAN-SGV-TREN IN CAN TO WSS-SGV-RENO      
                           MOVE FIO-GET-NXT TO FIO-CMND                 
                           GO TO CIC-GET-ROV-REN.                       
                                                                        
       VRF-RCAN.                                                        
           IF WSS-TOT-RCAN = ZEROES             AND                     
              WSS-TOT-PTTS IN WSS-VARI = ZEROES                         
               GO TO FIN-GET-ROV-REN.                                   
           MOVE RCAN-CAN-KEY-ICAN TO CAN-KEY-ICAN.                      
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM COL-FIO-CAN.                                         
       FIN-GET-ROV-REN.                                                 
           EXIT.                                                        
       CHK-ROV-PTTS SECTION.                                            
       INI-CHK-ROV-PTTS.                                                
           MOVE OPC-CIC-IOPC IN OPC TO ROV-CIC-IOPV IN ROV.             
           MOVE ZEROES              TO ROV-NUM-IVCT IN ROV.             
           MOVE 'ROV-KEY-IVCT' TO FIO-AKEY.                             
           MOVE FIO-GET-NLS    TO FIO-CMND.                             
           PERFORM COL-FIO-ROV.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    OPC-CIC-IOPC IN OPC = ROV-CIC-IOPV IN ROV )         
               GO TO FIN-CHK-ROV-PTTS.                                  
           MOVE ROV-CIC-IOPC IN ROV TO WSS-CIC-IOPC IN WSS-VARI.        
       CAN-CHK-ROV-PTTS.                                                
           MOVE ROV-KEY-IVCT IN ROV TO CAN-KEY-ICAN IN CAN.             
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM COL-FIO-CAN.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    OPC-MSC-STAT IN OPC = 'A' AND                       
                    CAN-MSC-STAT IN CAN = 'A' AND                       
                  ( CAN-MSC-CANC IN CAN = 'PTT' OR 'KTT' ))             
               GO TO FIN-CHK-ROV-PTTS.                                  
           MOVE LOW-VALUES TO ROV-KEY-IVCT IN ROV.                      
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-CHK-ROV-PTTS.                                                
           PERFORM COL-FIO-ROV.                                         
           IF FIO-STAT-OKS AND                                          
              ROV-CIC-IOPC IN ROV = WSS-CIC-IOPC IN WSS-VARI            
               IF ROV-CIC-IOPV IN ROV NOT = OPC-CIC-IOPC IN OPC         
                   MOVE ROV-KEY-IVCT IN ROV TO WSS-KEY-IVCT IN WSS-VARI 
                   ADD 1 TO WSS-TOT-PTTS IN WSS-VARI                    
                   MOVE FIO-GET-NXT TO FIO-CMND                         
                   GO TO LUP-CHK-ROV-PTTS                               
               ELSE                                                     
                   MOVE WSS-KEY-IVCT IN WSS-VARI TO RCAN-CAN-KEY-ICAN   
                                                          IN WSS-VARI.  
       FIN-CHK-ROV-PTTS.                                                
           EXIT.                                                        
       OBT-TASA-CHI SECTION.                                            
       INI-OBT-TASA-CHI.                                                
           MOVE 'ICG-KEY-IICG' TO FIO-AKEY.                             
           MOVE OPC-CIC-IOPC IN OPC TO ICG-CIC-IOPC IN ICG.             
           MOVE ZEROES              TO ICG-NUM-ICAN IN ICG.             
           MOVE SPACES              TO ICG-CIC-IOPV IN ICG              
           MOVE ZEROES              TO ICG-NUM-IVCT IN ICG              
           MOVE 'INT'               TO ICG-IND-TTAS IN ICG.             
           MOVE ZEROES              TO ICG-FEC-FINI IN ICG.             
           MOVE FIO-GET-NLS TO FIO-CMND.                                
           PERFORM COL-FIO-ICG.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    ICG-CIC-IOPC IN ICG = OPC-CIC-IOPC IN OPC AND       
                    ICG-NUM-ICAN IN ICG = ZEROES              AND       
                    ICG-CIC-IOPV IN ICG = SPACES              AND       
                    ICG-NUM-IVCT IN ICG = ZEROES              AND       
                    ICG-IND-TTAS IN ICG = 'INT'  )                      
                     MOVE ZEROES TO ICG-SGV-TINT IN ICG.                
       FIN-OBT-TASA-CHI.                                                
           EXIT.                                                        
       OBT-VAL-IMPU SECTION.                                            
       INI-OBT-VAL-IMPU.                                                
           MOVE OPC-CIC-IOPC IN OPC TO DLC-CIC-IOPC IN DLC.             
           MOVE 1                   TO DLC-NUM-IDLC IN DLC.             
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM COL-FIO-DLC.                                         
           IF FIO-STAT-OKS                                              
               MOVE DLC-VAL-IMPU IN DLC TO CHI-SGV-TPAG(CHI-I)          
               MOVE DLC-VAL-IMPU IN DLC TO CHI-SGV-TPAG(CHI-I)          
               MOVE DLC-NUM-PIMP IN DLC TO CHI-NUM-ICAN(CHI-I)          
           ELSE                                                         
               MOVE ZEROES              TO CHI-SGV-TPAG(CHI-I).         
      *    MOVE LOW-VALUES TO CXE.                                      
      *    MOVE SPACES               TO CXE-GLS-FLAG IN CXE.            
      *    MOVE WSS-CIC-IOPE         TO CXE-CIC-IOPC IN CXE.            
      *    MOVE ZEROES               TO CXE-NUM-EVEN IN CXE.            
      *    MOVE 'CUR'                TO CXE-COD-EVEN IN CXE.            
      *    MOVE SPACES               TO CXE-CIC-IOPV IN CXE.            
      *    MOVE ZEROES               TO CXE-NUM-IVCT IN CXE.            
      *    MOVE 'IMPPRG'             TO CXE-COD-CPTO IN CXE.            
      *    MOVE FIO-GET-KEY TO FIO-CMND.                                
      *    PERFORM COL-FIO-CXE.                                         
      *    IF FIO-STAT-OKS                                              
      *        MOVE CXE-SGV-TINT IN CXE TO CHI-NUM-ICAN(CHI-I).         
      *    ELSE                                                         
      *        MOVE ZEROES              TO CHI-NUM-ICAN(CHI-I).         
       FIN-OBT-VAL-IMPU.                                                
           EXIT.                                                        
      *>>>>                                                             
*% END
