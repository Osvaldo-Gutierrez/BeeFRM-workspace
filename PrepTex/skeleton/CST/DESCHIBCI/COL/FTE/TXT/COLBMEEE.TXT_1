*% IF FIN_SGC_GLS_NOMC
      *<<<< FIN_SGC_GLS_NOMC
           IF SGC-GLS-NOMC NOT > SPACES 
               MOVE 'ERCIRA NEIRA ' TO SGC-GLS-NOMC
      *>>>>
*% END


*% IF INI_SGC_NUM_EDAD
      *<<<< INI_SGC_NUM_EDAD
           IF ((SGC_NUM_EDAD < CTE-EDAD-MIN)
                OR (SGC_NUM_EDAD > CTE-EDAD-MAX))
                   MOVE 'SGC'          TO MSG-COD-SIST
                   MOVE 'EDADCLNORANG' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   GO TO FIN-VAL-SGC-NUM-EDAD.  
      *>>>>
*% END


*% IF INI_VAL_GLS_COMU
      *<<<< INI_VAL_GLS_COM-FFVI
           IF (COM_GLS_FFVI < COM_GLS_FIVI)
                   MOVE 'COL'          TO MSG-COD-SIST
                   MOVE 'FECHAFININVA' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   GO TO FIN-VAL-COM-GLS-FFVI.  
      *>>>>
*% END

