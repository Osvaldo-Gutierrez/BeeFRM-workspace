      *
      * COLBQCUC
      * Version 3.0.1 Nov-2005
      * 18-Mar-10 02:01 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CUC"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMCUC.TXT'
       SET-TAG-COLBQCUC SECTION.
       INI-SET-TAG-COLBQCUC.
           MOVE
           '<IDNMOD NOM=$COLBQCUC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$18-Mar-10 02:01 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCUC.
           EXIT.
           
       PUT-ALL-CUC SECTION.
       INI-PUT-ALL-CUC.
*% INCLUDE INI_PUT_ALL_CUC OF 'COLBMCUC.TXT'
           PERFORM PUT-CUC-CUC.
           PERFORM PUT-OFI-CUC.
*% INCLUDE FIN_PUT_ALL_CUC OF 'COLBMCUC.TXT'
       FIN-PUT-ALL-CUC.
           EXIT.
       
       PUT-CUC-CUC SECTION.
       INI-PUT-CUC-CUC.
*% INCLUDE INI_PUT_CUC_CUC OF 'COLBMCUC.TXT'
           IF PGM-STAT-CUC-NUL
               GO TO FIN-PUT-CUC-CUC.
           IF NOT PGM-STAT-CUC-OKS
               PERFORM CLR-CUC-CUC
               GO TO FIN-PUT-CUC-CUC.
           MOVE CUC-COD-OFI-CUA IN CUC TO CUC-COD-OFI-CUA IN CUC-FLD.
*% RREG = "CUC"
*% RREG_FRM = "CUC"
*% NOMB = "CUA"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CUC-NUM-DUA IN CUC TO CUC-NUM-DUA IN CUC-FLD.
           MOVE CUC-NUM-MUA IN CUC TO CUC-NUM-MUA IN CUC-FLD.
           MOVE CUC-NUM-SUA IN CUC TO CUC-NUM-SUA IN CUC-FLD.
           MOVE CUC-NUM-AUA IN CUC TO CUC-NUM-AUA IN CUC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE CUC-HH-CUA IN CUC TO CUC-HH-CUA IN CUC-FLD.
           MOVE CUC-MM-CUA IN CUC TO CUC-MM-CUA IN CUC-FLD.
           MOVE CUC-SS-CUA IN CUC TO CUC-SS-CUA IN CUC-FLD.
      *
      *    CAMPO INDEXADO   CUC-DRS-CAR-ABO
           MOVE CUC-DRS-CAR-ABO IN CUC(1) TO 
                CUC-DRS-CAR-ABO IN CUC-FLD(1).
           MOVE CUC-DRS-CAR-ABO IN CUC(2) TO 
                CUC-DRS-CAR-ABO IN CUC-FLD(2).
           MOVE CUC-DRS-CAR-ABO IN CUC(3) TO 
                CUC-DRS-CAR-ABO IN CUC-FLD(3).
           MOVE CUC-DRS-CAR-ABO IN CUC(4) TO 
                CUC-DRS-CAR-ABO IN CUC-FLD(4).
           MOVE CUC-DRS-CAR-ABO IN CUC(5) TO 
                CUC-DRS-CAR-ABO IN CUC-FLD(5).
      *
      *    CAMPO INDEXADO   CUC-CTD-CUR
           MOVE CUC-CTD-CUR IN CUC(1) TO CUC-CTD-CUR IN CUC-FLD(1).
           MOVE CUC-CTD-CUR IN CUC(2) TO CUC-CTD-CUR IN CUC-FLD(2).
           MOVE CUC-CTD-CUR IN CUC(3) TO CUC-CTD-CUR IN CUC-FLD(3).
           MOVE CUC-CTD-CUR IN CUC(4) TO CUC-CTD-CUR IN CUC-FLD(4).
           MOVE CUC-CTD-CUR IN CUC(5) TO CUC-CTD-CUR IN CUC-FLD(5).
      *
      *    CAMPO INDEXADO   CUC-VAL-LIQ-CUR
           MOVE CUC-VAL-LIQ-CUR IN CUC(1) TO 
                CUC-VAL-LIQ-CUR IN CUC-FLD(1).
           MOVE CUC-VAL-LIQ-CUR IN CUC(2) TO 
                CUC-VAL-LIQ-CUR IN CUC-FLD(2).
           MOVE CUC-VAL-LIQ-CUR IN CUC(3) TO 
                CUC-VAL-LIQ-CUR IN CUC-FLD(3).
           MOVE CUC-VAL-LIQ-CUR IN CUC(4) TO 
                CUC-VAL-LIQ-CUR IN CUC-FLD(4).
           MOVE CUC-VAL-LIQ-CUR IN CUC(5) TO 
                CUC-VAL-LIQ-CUR IN CUC-FLD(5).
      *
      *    CAMPO INDEXADO   CUC-VAL-CAN
           MOVE CUC-VAL-CAN IN CUC(1) TO CUC-VAL-CAN IN CUC-FLD(1).
           MOVE CUC-VAL-CAN IN CUC(2) TO CUC-VAL-CAN IN CUC-FLD(2).
           MOVE CUC-VAL-CAN IN CUC(3) TO CUC-VAL-CAN IN CUC-FLD(3).
           MOVE CUC-VAL-CAN IN CUC(4) TO CUC-VAL-CAN IN CUC-FLD(4).
           MOVE CUC-VAL-CAN IN CUC(5) TO CUC-VAL-CAN IN CUC-FLD(5).
      *
      *    CAMPO INDEXADO   CUC-CTD-CAN
           MOVE CUC-CTD-CAN IN CUC(1) TO CUC-CTD-CAN IN CUC-FLD(1).
           MOVE CUC-CTD-CAN IN CUC(2) TO CUC-CTD-CAN IN CUC-FLD(2).
           MOVE CUC-CTD-CAN IN CUC(3) TO CUC-CTD-CAN IN CUC-FLD(3).
           MOVE CUC-CTD-CAN IN CUC(4) TO CUC-CTD-CAN IN CUC-FLD(4).
           MOVE CUC-CTD-CAN IN CUC(5) TO CUC-CTD-CAN IN CUC-FLD(5).
*% INCLUDE FIN_PUT_CUC_CUC OF 'COLBMCUC.TXT'
       FIN-PUT-CUC-CUC.
           EXIT.
       
       PUT-OFI-CUC SECTION.
       INI-PUT-OFI-CUC.
*% INCLUDE INI_PUT_OFI_CUC OF 'COLBMCUC.TXT'
           IF PGM-STAT-OFI-NUL
               GO TO FIN-PUT-OFI-CUC.
           IF NOT PGM-STAT-OFI-OKS
               PERFORM CLR-OFI-CUC
               GO TO FIN-PUT-OFI-CUC.
           MOVE OFI-GLS-DESC IN OFI TO OFI-GLS-DESC IN CUC-FLD.
*% INCLUDE FIN_PUT_OFI_CUC OF 'COLBMCUC.TXT'
       FIN-PUT-OFI-CUC.
           EXIT.
       
       KEY-ALL-CUC SECTION.
       INI-KEY-ALL-CUC.
*% INCLUDE INI_KEY_ALL_CUC OF 'COLBMCUC.TXT'
           PERFORM KEY-CUC-CUC.
           PERFORM KEY-OFI-CUC.
*% INCLUDE FIN_KEY_ALL_CUC OF 'COLBMCUC.TXT'
       FIN-KEY-ALL-CUC.
           EXIT.
       
       KEY-CUC-CUC SECTION.
       INI-KEY-CUC-CUC.
*% INCLUDE INI_KEY_CUC_CUC OF 'COLBMCUC.TXT'
           MOVE CUC-COD-OFI-CUA IN CUC-FLD TO CUC-COD-OFI-CUA IN CUC.
*% INCLUDE FIN_KEY_CUC_CUC OF 'COLBMCUC.TXT'
       FIN-KEY-CUC-CUC.
           EXIT.
       
       KEY-OFI-CUC SECTION.
       INI-KEY-OFI-CUC.
*% INCLUDE INI_KEY_OFI_CUC OF 'COLBMCUC.TXT'
*% INCLUDE FIN_KEY_OFI_CUC OF 'COLBMCUC.TXT'
       FIN-KEY-OFI-CUC.
           EXIT.
       
       SCR-CLRA-CUC SECTION.
       INI-SCR-CLRA-CUC.
*% INCLUDE INI_SCR_CLRA_CUC OF 'COLBMCUC.TXT'
           PERFORM CLR-KEY-CUC.
           PERFORM CLR-FLD-CUC.
*% INCLUDE FIN_SCR_CLRA_CUC OF 'COLBMCUC.TXT'
       FIN-SCR-CLRA-CUC.
           EXIT.
       
       CLR-KEY-CUC SECTION.
       INI-CLR-KEY-CUC.
*% INCLUDE INI_CLR_KEY_CUC OF 'COLBMCUC.TXT'
           MOVE ZEROES TO CUC-COD-OFI-CUA IN CUC-FLD.
*% INCLUDE FIN_CLR_KEY_CUC OF 'COLBMCUC.TXT'
       FIN-CLR-KEY-CUC.
           EXIT.
       
       CLR-FLD-CUC SECTION.
       INI-CLR-FLD-CUC.
*% INCLUDE INI_CLR_FLD_CUC OF 'COLBMCUC.TXT'
           PERFORM CLR-CUC-CUC.
           PERFORM CLR-OFI-CUC.
*% INCLUDE FIN_CLR_FLD_CUC OF 'COLBMCUC.TXT'
       FIN-CLR-FLD-CUC.
           EXIT.
       
       CLR-CUC-CUC SECTION.
       INI-CLR-CUC-CUC.
*% INCLUDE INI_CLR_CUC_CUC OF 'COLBMCUC.TXT'
*% RREG = "CUC"
*% RREG_FRM = "CUC"
*% NOMB = "CUA"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CUC-FEC-CUA IN CUC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CUC-HH-CUA IN CUC-FLD.
           MOVE ZEROES TO CUC-MM-CUA IN CUC-FLD.
           MOVE ZEROES TO CUC-SS-CUA IN CUC-FLD.
           MOVE SPACES TO CUC-DRS-CAR-ABO IN CUC-FLD(1).
           MOVE SPACES TO CUC-DRS-CAR-ABO IN CUC-FLD(2).
           MOVE SPACES TO CUC-DRS-CAR-ABO IN CUC-FLD(3).
           MOVE SPACES TO CUC-DRS-CAR-ABO IN CUC-FLD(4).
           MOVE SPACES TO CUC-DRS-CAR-ABO IN CUC-FLD(5).
           MOVE ZEROES TO CUC-CTD-CUR IN CUC-FLD(1).
           MOVE ZEROES TO CUC-CTD-CUR IN CUC-FLD(2).
           MOVE ZEROES TO CUC-CTD-CUR IN CUC-FLD(3).
           MOVE ZEROES TO CUC-CTD-CUR IN CUC-FLD(4).
           MOVE ZEROES TO CUC-CTD-CUR IN CUC-FLD(5).
           MOVE ZEROES TO CUC-VAL-LIQ-CUR IN CUC-FLD(1).
           MOVE ZEROES TO CUC-VAL-LIQ-CUR IN CUC-FLD(2).
           MOVE ZEROES TO CUC-VAL-LIQ-CUR IN CUC-FLD(3).
           MOVE ZEROES TO CUC-VAL-LIQ-CUR IN CUC-FLD(4).
           MOVE ZEROES TO CUC-VAL-LIQ-CUR IN CUC-FLD(5).
           MOVE ZEROES TO CUC-VAL-CAN IN CUC-FLD(1).
           MOVE ZEROES TO CUC-VAL-CAN IN CUC-FLD(2).
           MOVE ZEROES TO CUC-VAL-CAN IN CUC-FLD(3).
           MOVE ZEROES TO CUC-VAL-CAN IN CUC-FLD(4).
           MOVE ZEROES TO CUC-VAL-CAN IN CUC-FLD(5).
           MOVE ZEROES TO CUC-CTD-CAN IN CUC-FLD(1).
           MOVE ZEROES TO CUC-CTD-CAN IN CUC-FLD(2).
           MOVE ZEROES TO CUC-CTD-CAN IN CUC-FLD(3).
           MOVE ZEROES TO CUC-CTD-CAN IN CUC-FLD(4).
           MOVE ZEROES TO CUC-CTD-CAN IN CUC-FLD(5).
*% INCLUDE FIN_CLR_CUC_CUC OF 'COLBMCUC.TXT'
       FIN-CLR-CUC-CUC.
           EXIT.
       
       CLR-OFI-CUC SECTION.
       INI-CLR-OFI-CUC.
*% INCLUDE INI_CLR_OFI_CUC OF 'COLBMCUC.TXT'
           MOVE SPACES TO OFI-GLS-DESC IN CUC-FLD.
*% INCLUDE FIN_CLR_OFI_CUC OF 'COLBMCUC.TXT'
       FIN-CLR-OFI-CUC.
           EXIT.
       
       FST-KEY-CUC SECTION.
       INI-FST-KEY-CUC.
*% INCLUDE INI_FST_KEY_CUC OF 'COLBMCUC.TXT'
           MOVE -1 TO CUC-COD-OFI-CUA-LEN IN CUC-FLD.
*% INCLUDE FIN_FST_KEY_CUC OF 'COLBMCUC.TXT'
       FIN-FST-KEY-CUC.
           EXIT.
       
       FST-FLD-CUC SECTION.
       INI-FST-FLD-CUC.
*% INCLUDE INI_FST_FLD_CUC OF 'COLBMCUC.TXT'
           MOVE -1 TO OFI-GLS-DESC-LEN IN CUC-FLD.
*% INCLUDE FIN_FST_FLD_CUC OF 'COLBMCUC.TXT'
       FIN-FST-FLD-CUC.
           EXIT.
       
       PRO-KEY-CUC SECTION.
       INI-PRO-KEY-CUC.
*% INCLUDE INI_PRO_KEY_CUC OF 'COLBMCUC.TXT'
           PERFORM PRO-IKY-CUC.
           PERFORM PRO-AKY-CUC.
           PERFORM PRO-MKY-CUC.
*% INCLUDE FIN_PRO_KEY_CUC OF 'COLBMCUC.TXT'
       FIN-PRO-KEY-CUC.
           EXIT.
       
       PRO-IKY-CUC SECTION.
       INI-PRO-IKY-CUC.
*% INCLUDE INI_PRO_IKY_CUC OF 'COLBMCUC.TXT'
           MOVE FRM-CPIM-AEY TO CUC-COD-OFI-CUA-ATR IN CUC-FLD.
*% INCLUDE FIN_PRO_IKY_CUC OF 'COLBMCUC.TXT'
       FIN-PRO-IKY-CUC.
           EXIT.
       
       PRO-MKY-CUC SECTION.
       INI-PRO-MKY-CUC.
*% INCLUDE INI_PRO_MKY_CUC OF 'COLBMCUC.TXT'
*% INCLUDE FIN_PRO_MKY_CUC OF 'COLBMCUC.TXT'
       FIN-PRO-MKY-CUC.
           EXIT.
       
       PRO-AKY-CUC SECTION.
       INI-PRO-AKY-CUC.
*% INCLUDE INI_PRO_AKY_CUC OF 'COLBMCUC.TXT'
*% INCLUDE FIN_PRO_AKY_CUC OF 'COLBMCUC.TXT'
       FIN-PRO-AKY-CUC.
           EXIT.
       
       UNP-KEY-CUC SECTION.
       INI-UNP-KEY-CUC.
*% INCLUDE INI_UNP_KEY_CUC OF 'COLBMCUC.TXT'
           PERFORM UNP-IKY-CUC.
           PERFORM UNP-AKY-CUC.
           PERFORM UNP-MKY-CUC.
*% INCLUDE FIN_UNP_KEY_CUC OF 'COLBMCUC.TXT'
       FIN-UNP-KEY-CUC.
           EXIT.
       
       UNP-IKY-CUC SECTION.
       INI-UNP-IKY-CUC.
*% INCLUDE INI_UNP_IKY_CUC OF 'COLBMCUC.TXT'
           MOVE FRM-CPIM-UBY TO CUC-COD-OFI-CUA-ATR.
*% INCLUDE FIN_UNP_IKY_CUC OF 'COLBMCUC.TXT'
       FIN-UNP-IKY-CUC.
           EXIT.
       
       UNP-MKY-CUC SECTION.
       INI-UNP-MKY-CUC.
*% INCLUDE INI_UNP_MKY_CUC OF 'COLBMCUC.TXT'
*% INCLUDE FIN_UNP_MKY_CUC OF 'COLBMCUC.TXT'
       FIN-UNP-MKY-CUC.
           EXIT.
       
       UNP-AKY-CUC SECTION.
       INI-UNP-AKY-CUC.
*% INCLUDE INI_UNP_AKY_CUC OF 'COLBMCUC.TXT'
*% INCLUDE FIN_UNP_AKY_CUC OF 'COLBMCUC.TXT'
       FIN-UNP-AKY-CUC.
           EXIT.
       
       UNP-FLD-CUC SECTION.
       INI-UNP-FLD-CUC.
*% INCLUDE INI_UNP_FLD_CUC OF 'COLBMCUC.TXT'
           PERFORM UNP-CUC-CUC.
           PERFORM UNP-OFI-CUC.
*% INCLUDE FIN_UNP_FLD_CUC OF 'COLBMCUC.TXT'
       FIN-UNP-FLD-CUC.
           EXIT.
       
       UNP-CUC-CUC SECTION.
       INI-UNP-CUC-CUC.
*% INCLUDE INI_UNP_CUC_CUC OF 'COLBMCUC.TXT'
*% RREG = "CUC"
*% RREG_FRM = "CUC"
*% NOMB = "CUA"
           MOVE FRM-CPIM-UBY TO CUC-FEC-CUA-ATR IN CUC-FLD.
           MOVE FRM-CPIM-UBY TO CUC-HH-CUA-ATR IN CUC-FLD.
           MOVE FRM-CPIM-UBY TO CUC-MM-CUA-ATR IN CUC-FLD.
           MOVE FRM-CPIM-UBY TO CUC-SS-CUA-ATR IN CUC-FLD.
           MOVE FRM-CPIM-UBY TO CUC-DRS-CAR-ABO-ATR IN CUC-FLD(1).
           MOVE FRM-CPIM-UBY TO CUC-DRS-CAR-ABO-ATR IN CUC-FLD(2).
           MOVE FRM-CPIM-UBY TO CUC-DRS-CAR-ABO-ATR IN CUC-FLD(3).
           MOVE FRM-CPIM-UBY TO CUC-DRS-CAR-ABO-ATR IN CUC-FLD(4).
           MOVE FRM-CPIM-UBY TO CUC-DRS-CAR-ABO-ATR IN CUC-FLD(5).
           MOVE FRM-CPIM-UBY TO CUC-CTD-CUR-ATR IN CUC-FLD(1).
           MOVE FRM-CPIM-UBY TO CUC-CTD-CUR-ATR IN CUC-FLD(2).
           MOVE FRM-CPIM-UBY TO CUC-CTD-CUR-ATR IN CUC-FLD(3).
           MOVE FRM-CPIM-UBY TO CUC-CTD-CUR-ATR IN CUC-FLD(4).
           MOVE FRM-CPIM-UBY TO CUC-CTD-CUR-ATR IN CUC-FLD(5).
           MOVE FRM-CPIM-UBY TO CUC-VAL-LIQ-CUR-ATR IN CUC-FLD(1).
           MOVE FRM-CPIM-UBY TO CUC-VAL-LIQ-CUR-ATR IN CUC-FLD(2).
           MOVE FRM-CPIM-UBY TO CUC-VAL-LIQ-CUR-ATR IN CUC-FLD(3).
           MOVE FRM-CPIM-UBY TO CUC-VAL-LIQ-CUR-ATR IN CUC-FLD(4).
           MOVE FRM-CPIM-UBY TO CUC-VAL-LIQ-CUR-ATR IN CUC-FLD(5).
           MOVE FRM-CPIM-UBY TO CUC-VAL-CAN-ATR IN CUC-FLD(1).
           MOVE FRM-CPIM-UBY TO CUC-VAL-CAN-ATR IN CUC-FLD(2).
           MOVE FRM-CPIM-UBY TO CUC-VAL-CAN-ATR IN CUC-FLD(3).
           MOVE FRM-CPIM-UBY TO CUC-VAL-CAN-ATR IN CUC-FLD(4).
           MOVE FRM-CPIM-UBY TO CUC-VAL-CAN-ATR IN CUC-FLD(5).
           MOVE FRM-CPIM-UBY TO CUC-CTD-CAN-ATR IN CUC-FLD(1).
           MOVE FRM-CPIM-UBY TO CUC-CTD-CAN-ATR IN CUC-FLD(2).
           MOVE FRM-CPIM-UBY TO CUC-CTD-CAN-ATR IN CUC-FLD(3).
           MOVE FRM-CPIM-UBY TO CUC-CTD-CAN-ATR IN CUC-FLD(4).
           MOVE FRM-CPIM-UBY TO CUC-CTD-CAN-ATR IN CUC-FLD(5).
*% INCLUDE FIN_UNP_CUC_CUC OF 'COLBMCUC.TXT'
       FIN-UNP-CUC-CUC.
           EXIT.
       
       UNP-OFI-CUC SECTION.
       INI-UNP-OFI-CUC.
*% INCLUDE INI_UNP_OFI_CUC OF 'COLBMCUC.TXT'
           MOVE FRM-CPIM-UBY TO OFI-GLS-DESC-ATR IN CUC-FLD.
*% INCLUDE FIN_UNP_OFI_CUC OF 'COLBMCUC.TXT'
       FIN-UNP-OFI-CUC.
           EXIT.
       
       VAL-KEY-CUC SECTION.
       INI-VAL-KEY-CUC.
           PERFORM VAL-IKY-CUC.
           PERFORM VAL-MKY-CUC.
           PERFORM VAL-AKY-CUC.
       FIN-VAL-KEY-CUC.
           EXIT.
       
       VAL-IKY-CUC SECTION.
       INI-VAL-IKY-CUC.
*% INCLUDE INI_VAL_IKY_CUC OF 'COLBMCUC.TXT'
           PERFORM VAL-CUC-COD-OFI-CUA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CUC-COD-OFI-CUA-LEN IN CUC-FLD
                GO TO FIN-VAL-IKY-CUC.
*% INCLUDE FIN_VAL_IKY_CUC OF 'COLBMCUC.TXT'
       FIN-VAL-IKY-CUC.
           EXIT.
       
       VAL-MKY-CUC SECTION.
       INI-VAL-MKY-CUC.
*% INCLUDE INI_VAL_MKY_CUC OF 'COLBMCUC.TXT'
*% INCLUDE FIN_VAL_MKY_CUC OF 'COLBMCUC.TXT'
       FIN-VAL-MKY-CUC.
           EXIT.
       
       VAL-AKY-CUC SECTION.
       INI-VAL-AKY-CUC.
*% INCLUDE INI_VAL_AKY_CUC OF 'COLBMCUC.TXT'
*% INCLUDE FIN_VAL_AKY_CUC OF 'COLBMCUC.TXT'
       FIN-VAL-AKY-CUC.
           EXIT.
       
       VAL-CUC-COD-OFI-CUA SECTION.
       INI-VAL-CUC-COD-OFI-CUA.
*% LBL = "CUC_COD_OFI_CUA OF 'COLBMCUC.TXT'"
*% INCLUDE INI_CUC_COD_OFI_CUA OF 'COLBMCUC.TXT'
*% INCLUDE CUC_COD_OFI_CUA OF 'COLBMCUC.TXT'
*% INCLUDE FIN_CUC_COD_OFI_CUA OF 'COLBMCUC.TXT'
       FIN-VAL-CUC-COD-OFI-CUA.
           EXIT.
*% RREG = "CUC"
*% RREG_FRM = "CUC"
*% NOMB = "CUA"
*% IZR = "S"
       
       VAL-CON-KEY-CUC SECTION.
       INI-VAL-CON-KEY-CUC.
*% INCLUDE INI_VAL_CON_KEY_CUC OF 'COLBMCUC.TXT'
           IF     CUC-COD-OFI-CUA IN CUC-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CUC.
*% INCLUDE FIN_VAL_CON_KEY_CUC OF 'COLBMCUC.TXT'
       FIN-VAL-CON-KEY-CUC.
           EXIT.
       
       VAL-CON-IKY-CUC SECTION.
       INI-VAL-CON-IKY-CUC.
*% INCLUDE INI_VAL_CON_IKY_CUC OF 'COLBMCUC.TXT'
           IF     CUC-COD-OFI-CUA IN CUC-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CUC.
*% INCLUDE FIN_VAL_CON_IKY_CUC OF 'COLBMCUC.TXT'
       FIN-VAL-CON-IKY-CUC.
           EXIT.
       
       VAL-NUM-KEY-CUC SECTION.
       INI-VAL-NUM-KEY-CUC.
           PERFORM VAL-NUM-IKY-CUC.
           PERFORM VAL-NUM-MKY-CUC.
           PERFORM VAL-NUM-AKY-CUC.
       FIN-VAL-NUM-KEY-CUC.
           EXIT.
       
       VAL-NUM-IKY-CUC SECTION.
       INI-VAL-NUM-IKY-CUC.
*% INCLUDE INI_VAL_NUM_IKY_CUC OF 'COLBMCUC.TXT'
       VAL-NUM-CUC-COD-OFI-CUA.
*% BUF = "CUC-FLD"
*% FLD = "CUC-COD-OFI-CUA"
*% ENT = "3"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-CUC.
*% INCLUDE FIN_VAL_NUM_IKY_CUC OF 'COLBMCUC.TXT'
       FIN-VAL-NUM-IKY-CUC.
           EXIT.
       
       VAL-NUM-MKY-CUC SECTION.
       INI-VAL-NUM-MKY-CUC.
*% INCLUDE INI_VAL_NUM_MKY_CUC OF 'COLBMCUC.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_CUC OF 'COLBMCUC.TXT'
       FIN-VAL-NUM-MKY-CUC.
           EXIT.
       
       VAL-NUM-AKY-CUC SECTION.
       INI-VAL-NUM-AKY-CUC.
*% INCLUDE INI_VAL_NUM_AKY_CUC OF 'COLBMCUC.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_CUC OF 'COLBMCUC.TXT'
       FIN-VAL-NUM-AKY-CUC.
           EXIT.
       
       VAL-NUM-FLD-CUC SECTION.
       INI-VAL-NUM-FLD-CUC.
*% INCLUDE INI_VAL_NUM_FLD_CUC OF 'COLBMCUC.TXT'
       VAL-NUM-CUC-HH-CUA.
*% BUF = "CUC-FLD"
*% FLD = "CUC-HH-CUA"
*% ENT = "2"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-CUC.
       VAL-NUM-CUC-MM-CUA.
*% BUF = "CUC-FLD"
