      *
      * COLBQCBN
      * Version 3.0.1 Nov-2005
      * 09-Mar-09 06:29 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CBN"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMCBN.TXT'
       SET-TAG-COLBQCBN SECTION.
       INI-SET-TAG-COLBQCBN.
           MOVE
           '<IDNMOD NOM=$COLBQCBN$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$09-Mar-09 06:29 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCBN.
           EXIT.
           
       PUT-ALL-CBN SECTION.
       INI-PUT-ALL-CBN.
*% INCLUDE INI_PUT_ALL_CBN OF 'COLBMCBN.TXT'
           PERFORM PUT-RCO-CBN.
*% INCLUDE FIN_PUT_ALL_CBN OF 'COLBMCBN.TXT'
       FIN-PUT-ALL-CBN.
           EXIT.
       
       PUT-RCO-CBN SECTION.
       INI-PUT-RCO-CBN.
*% INCLUDE INI_PUT_RCO_CBN OF 'COLBMCBN.TXT'
           IF PGM-STAT-RCO-NUL
               GO TO FIN-PUT-RCO-CBN.
           IF NOT PGM-STAT-RCO-OKS
               PERFORM CLR-RCO-CBN
               GO TO FIN-PUT-RCO-CBN.
           MOVE RCO-CAI-IOPE IN RCO TO RCO-CAI-IOPE IN CBN-FLD.
           MOVE RCO-COD-TREL IN RCO TO RCO-COD-TREL IN CBN-FLD.
           MOVE RCO-IIC-IOPE IN RCO TO RCO-IIC-IOPE IN CBN-FLD.
*% INCLUDE FIN_PUT_RCO_CBN OF 'COLBMCBN.TXT'
       FIN-PUT-RCO-CBN.
           EXIT.
       
       KEY-ALL-CBN SECTION.
       INI-KEY-ALL-CBN.
*% INCLUDE INI_KEY_ALL_CBN OF 'COLBMCBN.TXT'
           PERFORM KEY-RCO-CBN.
*% INCLUDE FIN_KEY_ALL_CBN OF 'COLBMCBN.TXT'
       FIN-KEY-ALL-CBN.
           EXIT.
       
       KEY-RCO-CBN SECTION.
       INI-KEY-RCO-CBN.
*% INCLUDE INI_KEY_RCO_CBN OF 'COLBMCBN.TXT'
           MOVE RCO-CAI-IOPE IN CBN-FLD TO RCO-CAI-IOPE IN RCO.
           MOVE RCO-COD-TREL IN CBN-FLD TO RCO-COD-TREL IN RCO.
           MOVE RCO-IIC-IOPE IN CBN-FLD TO RCO-IIC-IOPE IN RCO.
*% INCLUDE FIN_KEY_RCO_CBN OF 'COLBMCBN.TXT'
       FIN-KEY-RCO-CBN.
           EXIT.
       
       SCR-CLRA-CBN SECTION.
       INI-SCR-CLRA-CBN.
*% INCLUDE INI_SCR_CLRA_CBN OF 'COLBMCBN.TXT'
           PERFORM CLR-KEY-CBN.
           PERFORM CLR-FLD-CBN.
*% INCLUDE FIN_SCR_CLRA_CBN OF 'COLBMCBN.TXT'
       FIN-SCR-CLRA-CBN.
           EXIT.
       
       CLR-KEY-CBN SECTION.
       INI-CLR-KEY-CBN.
*% INCLUDE INI_CLR_KEY_CBN OF 'COLBMCBN.TXT'
           MOVE SPACES TO RCO-CAI-IOPE IN CBN-FLD.
           MOVE SPACES TO RCO-COD-TREL IN CBN-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "CLIE"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "CLIE"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-CLIE IN CBN-FLD.
           MOVE SPACES TO FRM-VRF-CLIE IN CBN-FLD.
           MOVE SPACES TO FRM-IND-CLIE IN CBN-FLD.
           MOVE SPACES TO FRM-GLS-CLIE IN CBN-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO RCO-IIC-IOPE IN CBN-FLD.
*% INCLUDE FIN_CLR_KEY_CBN OF 'COLBMCBN.TXT'
       FIN-CLR-KEY-CBN.
           EXIT.
       
       CLR-FLD-CBN SECTION.
       INI-CLR-FLD-CBN.
*% INCLUDE INI_CLR_FLD_CBN OF 'COLBMCBN.TXT'
           MOVE SPACES TO FRM-GLS-TREL IN CBN-FLD.
           MOVE SPACES TO FRM-GLS-NOMC IN CBN-FLD(1).
           MOVE SPACES TO FRM-GLS-NOMC IN CBN-FLD(2).
           MOVE SPACES TO FRM-GLS-NOMC IN CBN-FLD(3).
           MOVE SPACES TO FRM-GLS-NOMC IN CBN-FLD(4).
           MOVE SPACES TO FRM-GLS-NOMC IN CBN-FLD(5).
           MOVE SPACES TO FRM-GLS-NOMC IN CBN-FLD(6).
           MOVE SPACES TO FRM-GLS-NOMC IN CBN-FLD(7).
           MOVE SPACES TO FRM-GLS-NOMC IN CBN-FLD(8).
           MOVE SPACES TO FRM-GLS-NOMC IN CBN-FLD(9).
           MOVE SPACES TO FRM-GLS-NOMC IN CBN-FLD(10).
           MOVE SPACES TO FRM-GLS-NOMC IN CBN-FLD(11).
           MOVE SPACES TO FRM-COD-TREL IN CBN-FLD(1).
           MOVE SPACES TO FRM-COD-TREL IN CBN-FLD(2).
           MOVE SPACES TO FRM-COD-TREL IN CBN-FLD(3).
           MOVE SPACES TO FRM-COD-TREL IN CBN-FLD(4).
           MOVE SPACES TO FRM-COD-TREL IN CBN-FLD(5).
           MOVE SPACES TO FRM-COD-TREL IN CBN-FLD(6).
           MOVE SPACES TO FRM-COD-TREL IN CBN-FLD(7).
           MOVE SPACES TO FRM-COD-TREL IN CBN-FLD(8).
           MOVE SPACES TO FRM-COD-TREL IN CBN-FLD(9).
           MOVE SPACES TO FRM-COD-TREL IN CBN-FLD(10).
           MOVE SPACES TO FRM-COD-TREL IN CBN-FLD(11).
