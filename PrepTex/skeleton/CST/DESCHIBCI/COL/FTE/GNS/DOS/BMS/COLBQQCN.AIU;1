       VAL-CON-KEY-QCN SECTION.
       INI-VAL-CON-KEY-QCN.
*% INCLUDE INI_VAL_CON_KEY_QCN OF 'COLBMQCN.TXT'
           IF     CAN-CAI-IOPC IN QCN-FLD NOT > SPACES 
              AND CAN-IIC-IOPC IN QCN-FLD NOT > ZEROES 
              AND FRM-IND-HMAS IN QCN-FLD NOT > SPACES 
              AND FRM-CAI-IOPC IN QCN-FLD NOT > SPACES 
              AND FRM-IIC-IOPC IN QCN-FLD NOT > ZEROES 
              AND FRM-NUM-ICAN IN QCN-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-QCN.
*% INCLUDE FIN_VAL_CON_KEY_QCN OF 'COLBMQCN.TXT'
       FIN-VAL-CON-KEY-QCN.
           EXIT.
       
       VAL-CON-IKY-QCN SECTION.
       INI-VAL-CON-IKY-QCN.
*% INCLUDE INI_VAL_CON_IKY_QCN OF 'COLBMQCN.TXT'
           IF     CAN-CAI-IOPC IN QCN-FLD NOT > SPACES 
              AND CAN-IIC-IOPC IN QCN-FLD NOT > ZEROES 
              AND FRM-IND-HMAS IN QCN-FLD NOT > SPACES 
              AND FRM-CAI-IOPC IN QCN-FLD NOT > SPACES 
              AND FRM-IIC-IOPC IN QCN-FLD NOT > ZEROES 
              AND FRM-NUM-ICAN IN QCN-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-QCN.
*% INCLUDE FIN_VAL_CON_IKY_QCN OF 'COLBMQCN.TXT'
       FIN-VAL-CON-IKY-QCN.
           EXIT.
       
       VAL-NUM-KEY-QCN SECTION.
       INI-VAL-NUM-KEY-QCN.
           PERFORM VAL-NUM-IKY-QCN.
           PERFORM VAL-NUM-MKY-QCN.
           PERFORM VAL-NUM-AKY-QCN.
       FIN-VAL-NUM-KEY-QCN.
           EXIT.
       
       VAL-NUM-IKY-QCN SECTION.
       INI-VAL-NUM-IKY-QCN.
*% INCLUDE INI_VAL_NUM_IKY_QCN OF 'COLBMQCN.TXT'
       VAL-NUM-CAN-IIC-IOPC.
*% BUF = "QCN-FLD"
*% FLD = "CAN-IIC-IOPC"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
