      *
      * COLBQNTR
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:11 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "NTR"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMNTR.TXT'
       SET-TAG-COLBQNTR SECTION.
       INI-SET-TAG-COLBQNTR.
           MOVE
           '<IDNMOD NOM=$COLBQNTR$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:11 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQNTR.
           EXIT.
           
       PUT-ALL-NTR SECTION.
       INI-PUT-ALL-NTR.
*% INCLUDE INI_PUT_ALL_NTR OF 'COLBMNTR.TXT'
           PERFORM PUT-NTR-NTR.
*% INCLUDE FIN_PUT_ALL_NTR OF 'COLBMNTR.TXT'
       FIN-PUT-ALL-NTR.
           EXIT.
       
       PUT-NTR-NTR SECTION.
       INI-PUT-NTR-NTR.
*% INCLUDE INI_PUT_NTR_NTR OF 'COLBMNTR.TXT'
           IF PGM-STAT-NTR-NUL
               GO TO FIN-PUT-NTR-NTR.
           IF NOT PGM-STAT-NTR-OKS
               PERFORM CLR-NTR-NTR
               GO TO FIN-PUT-NTR-NTR.
           MOVE NTR-COD-NOTA IN NTR TO NTR-COD-NOTA IN NTR-FLD.
           MOVE NTR-COD-OFIC IN NTR TO NTR-COD-OFIC IN NTR-FLD.
           MOVE NTR-COD-TDOC IN NTR TO NTR-COD-TDOC IN NTR-FLD.
           MOVE NTR-CIC-GNOT IN NTR TO NTR-CIC-GNOT IN NTR-FLD.
           MOVE NTR-IND-RETE IN NTR TO NTR-IND-RETE IN NTR-FLD.
           MOVE NTR-GLS-CCTE IN NTR TO NTR-GLS-CCTE IN NTR-FLD.
*% RREG = "NTR"
*% RREG_FRM = "NTR"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE NTR-NUM-DTRN IN NTR TO NTR-NUM-DTRN IN NTR-FLD.
           MOVE NTR-NUM-MTRN IN NTR TO NTR-NUM-MTRN IN NTR-FLD.
           MOVE NTR-NUM-STRN IN NTR TO NTR-NUM-STRN IN NTR-FLD.
           MOVE NTR-NUM-ATRN IN NTR TO NTR-NUM-ATRN IN NTR-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE NTR-IND-VIGE IN NTR TO NTR-IND-VIGE IN NTR-FLD.
           MOVE NTR-VAL-GNOT IN NTR TO NTR-VAL-GNOT IN NTR-FLD.
           MOVE NTR-NUM-TR01 IN NTR TO NTR-NUM-TR01 IN NTR-FLD.
           MOVE NTR-VAL-MT01 IN NTR TO NTR-VAL-MT01 IN NTR-FLD.
           MOVE NTR-NUM-TR02 IN NTR TO NTR-NUM-TR02 IN NTR-FLD.
           MOVE NTR-VAL-MT02 IN NTR TO NTR-VAL-MT02 IN NTR-FLD.
           MOVE NTR-NUM-TR03 IN NTR TO NTR-NUM-TR03 IN NTR-FLD.
           MOVE NTR-VAL-MT03 IN NTR TO NTR-VAL-MT03 IN NTR-FLD.
           MOVE NTR-NUM-TR04 IN NTR TO NTR-NUM-TR04 IN NTR-FLD.
           MOVE NTR-VAL-MT04 IN NTR TO NTR-VAL-MT04 IN NTR-FLD.
           MOVE NTR-NUM-TR05 IN NTR TO NTR-NUM-TR05 IN NTR-FLD.
           MOVE NTR-VAL-MT05 IN NTR TO NTR-VAL-MT05 IN NTR-FLD.
           MOVE NTR-NUM-TR06 IN NTR TO NTR-NUM-TR06 IN NTR-FLD.
           MOVE NTR-VAL-MT06 IN NTR TO NTR-VAL-MT06 IN NTR-FLD.
           MOVE NTR-NUM-TR07 IN NTR TO NTR-NUM-TR07 IN NTR-FLD.
           MOVE NTR-VAL-MT07 IN NTR TO NTR-VAL-MT07 IN NTR-FLD.
           MOVE NTR-NUM-TR08 IN NTR TO NTR-NUM-TR08 IN NTR-FLD.
           MOVE NTR-VAL-MT08 IN NTR TO NTR-VAL-MT08 IN NTR-FLD.
           MOVE NTR-NUM-TR09 IN NTR TO NTR-NUM-TR09 IN NTR-FLD.
           MOVE NTR-VAL-MT09 IN NTR TO NTR-VAL-MT09 IN NTR-FLD.
           MOVE NTR-NUM-TR10 IN NTR TO NTR-NUM-TR10 IN NTR-FLD.
           MOVE NTR-VAL-MT10 IN NTR TO NTR-VAL-MT10 IN NTR-FLD.
           MOVE NTR-NUM-TR11 IN NTR TO NTR-NUM-TR11 IN NTR-FLD.
           MOVE NTR-VAL-MT11 IN NTR TO NTR-VAL-MT11 IN NTR-FLD.
           MOVE NTR-NUM-TR12 IN NTR TO NTR-NUM-TR12 IN NTR-FLD.
           MOVE NTR-VAL-MT12 IN NTR TO NTR-VAL-MT12 IN NTR-FLD.
           MOVE NTR-NUM-TR13 IN NTR TO NTR-NUM-TR13 IN NTR-FLD.
           MOVE NTR-VAL-MT13 IN NTR TO NTR-VAL-MT13 IN NTR-FLD.
           MOVE NTR-NUM-TR14 IN NTR TO NTR-NUM-TR14 IN NTR-FLD.
           MOVE NTR-VAL-MT14 IN NTR TO NTR-VAL-MT14 IN NTR-FLD.
*% BUF = "NTR-FLD"
*% SIS = "TAB"
*% TAB = "NOT "
*% FLD = "NTR-COD-NOTA"
*% GLS = "FRM-GLS-NOTA"
*% TIENE_GLOSA = TRUE
*% LGL = 25
*% INCLUDE 'BUSTAB'
*% BUF = "NTR-FLD"
*% SIS = "TAB"
*% TAB = "OFI "
*% FLD = "NTR-COD-OFIC"
*% GLS = "FRM-GLS-OFIC"
*% TIENE_GLOSA = TRUE
*% LGL = 20
*% INCLUDE 'BUSOFI          '
*% BUF = "NTR-FLD"
*% SIS = "TAB"
*% TAB = "DOC "
*% FLD = "NTR-COD-TDOC"
*% GLS = "FRM-GLS-TDOC"
*% TIENE_GLOSA = TRUE
*% LGL = 20
*% INCLUDE 'BUSTAB'
*% INCLUDE FIN_PUT_NTR_NTR OF 'COLBMNTR.TXT'
       FIN-PUT-NTR-NTR.
           EXIT.
       
       KEY-ALL-NTR SECTION.
       INI-KEY-ALL-NTR.
*% INCLUDE INI_KEY_ALL_NTR OF 'COLBMNTR.TXT'
           PERFORM KEY-NTR-NTR.
*% INCLUDE FIN_KEY_ALL_NTR OF 'COLBMNTR.TXT'
       FIN-KEY-ALL-NTR.
           EXIT.
       
       KEY-NTR-NTR SECTION.
       INI-KEY-NTR-NTR.
*% INCLUDE INI_KEY_NTR_NTR OF 'COLBMNTR.TXT'
           MOVE NTR-COD-NOTA IN NTR-FLD TO NTR-COD-NOTA IN NTR.
           MOVE NTR-COD-OFIC IN NTR-FLD TO NTR-COD-OFIC IN NTR.
           MOVE NTR-COD-TDOC IN NTR-FLD TO NTR-COD-TDOC IN NTR.
*% INCLUDE FIN_KEY_NTR_NTR OF 'COLBMNTR.TXT'
       FIN-KEY-NTR-NTR.
           EXIT.
       
       SCR-CLRA-NTR SECTION.
       INI-SCR-CLRA-NTR.
*% INCLUDE INI_SCR_CLRA_NTR OF 'COLBMNTR.TXT'
           PERFORM CLR-KEY-NTR.
           PERFORM CLR-FLD-NTR.
*% INCLUDE FIN_SCR_CLRA_NTR OF 'COLBMNTR.TXT'
       FIN-SCR-CLRA-NTR.
           EXIT.
       
       CLR-KEY-NTR SECTION.
       INI-CLR-KEY-NTR.
*% INCLUDE INI_CLR_KEY_NTR OF 'COLBMNTR.TXT'
           MOVE SPACES TO NTR-COD-NOTA IN NTR-FLD.
           MOVE SPACES TO NTR-COD-OFIC IN NTR-FLD.
           MOVE SPACES TO NTR-COD-TDOC IN NTR-FLD.
*% INCLUDE FIN_CLR_KEY_NTR OF 'COLBMNTR.TXT'
       FIN-CLR-KEY-NTR.
           EXIT.
       
       CLR-FLD-NTR SECTION.
       INI-CLR-FLD-NTR.
*% INCLUDE INI_CLR_FLD_NTR OF 'COLBMNTR.TXT'
           MOVE SPACES TO FRM-GLS-NOTA IN NTR-FLD.
           MOVE SPACES TO FRM-GLS-OFIC IN NTR-FLD.
           MOVE SPACES TO FRM-GLS-TDOC IN NTR-FLD.
           PERFORM CLR-NTR-NTR.
*% INCLUDE FIN_CLR_FLD_NTR OF 'COLBMNTR.TXT'
       FIN-CLR-FLD-NTR.
           EXIT.
       
       CLR-NTR-NTR SECTION.
       INI-CLR-NTR-NTR.
*% INCLUDE INI_CLR_NTR_NTR OF 'COLBMNTR.TXT'
           MOVE SPACES TO NTR-CIC-GNOT IN NTR-FLD.
           MOVE SPACES TO NTR-IND-RETE IN NTR-FLD.
           MOVE SPACES TO NTR-GLS-CCTE IN NTR-FLD.
*% RREG = "NTR"
*% RREG_FRM = "NTR"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO NTR-FEC-FTRN IN NTR-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO NTR-IND-VIGE IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-GNOT IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR01 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT01 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR02 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT02 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR03 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT03 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR04 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT04 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR05 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT05 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR06 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT06 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR07 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT07 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR08 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT08 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR09 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT09 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR10 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT10 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR11 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT11 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR12 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT12 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR13 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT13 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR14 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT14 IN NTR-FLD.
*% INCLUDE FIN_CLR_NTR_NTR OF 'COLBMNTR.TXT'
       FIN-CLR-NTR-NTR.
           EXIT.
       
       FST-KEY-NTR SECTION.
       INI-FST-KEY-NTR.
*% INCLUDE INI_FST_KEY_NTR OF 'COLBMNTR.TXT'
           MOVE -1 TO NTR-COD-NOTA-LEN IN NTR-FLD.
*% INCLUDE FIN_FST_KEY_NTR OF 'COLBMNTR.TXT'
       FIN-FST-KEY-NTR.
           EXIT.
       
       FST-FLD-NTR SECTION.
       INI-FST-FLD-NTR.
*% INCLUDE INI_FST_FLD_NTR OF 'COLBMNTR.TXT'
           MOVE -1 TO NTR-CIC-GNOT-LEN IN NTR-FLD.
*% INCLUDE FIN_FST_FLD_NTR OF 'COLBMNTR.TXT'
       FIN-FST-FLD-NTR.
           EXIT.
       
       PRO-KEY-NTR SECTION.
       INI-PRO-KEY-NTR.
*% INCLUDE INI_PRO_KEY_NTR OF 'COLBMNTR.TXT'
           PERFORM PRO-IKY-NTR.
           PERFORM PRO-AKY-NTR.
           PERFORM PRO-MKY-NTR.
*% INCLUDE FIN_PRO_KEY_NTR OF 'COLBMNTR.TXT'
       FIN-PRO-KEY-NTR.
           EXIT.
       
       PRO-IKY-NTR SECTION.
       INI-PRO-IKY-NTR.
*% INCLUDE INI_PRO_IKY_NTR OF 'COLBMNTR.TXT'
           MOVE FRM-CPIM-AEY TO NTR-COD-NOTA-ATR IN NTR-FLD.
           MOVE FRM-CPIM-AEY TO NTR-COD-OFIC-ATR IN NTR-FLD.
           MOVE FRM-CPIM-AEY TO NTR-COD-TDOC-ATR IN NTR-FLD.
*% INCLUDE FIN_PRO_IKY_NTR OF 'COLBMNTR.TXT'
       FIN-PRO-IKY-NTR.
           EXIT.
       
