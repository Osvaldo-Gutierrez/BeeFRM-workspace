       VAL-KEY-NTR SECTION.
       INI-VAL-KEY-NTR.
           PERFORM VAL-IKY-NTR.
           PERFORM VAL-MKY-NTR.
           PERFORM VAL-AKY-NTR.
       FIN-VAL-KEY-NTR.
           EXIT.
       
       VAL-IKY-NTR SECTION.
       INI-VAL-IKY-NTR.
*% INCLUDE INI_VAL_IKY_NTR OF 'COLBMNTR.TXT'
           PERFORM VAL-NTR-COD-NOTA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO NTR-COD-NOTA-LEN IN NTR-FLD
                GO TO FIN-VAL-IKY-NTR.
           PERFORM VAL-NTR-COD-OFIC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO NTR-COD-OFIC-LEN IN NTR-FLD
                GO TO FIN-VAL-IKY-NTR.
           PERFORM VAL-NTR-COD-TDOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO NTR-COD-TDOC-LEN IN NTR-FLD
                GO TO FIN-VAL-IKY-NTR.
*% INCLUDE FIN_VAL_IKY_NTR OF 'COLBMNTR.TXT'
       FIN-VAL-IKY-NTR.
           EXIT.
       
       VAL-MKY-NTR SECTION.
       INI-VAL-MKY-NTR.
*% INCLUDE INI_VAL_MKY_NTR OF 'COLBMNTR.TXT'
*% INCLUDE FIN_VAL_MKY_NTR OF 'COLBMNTR.TXT'
       FIN-VAL-MKY-NTR.
           EXIT.
       
