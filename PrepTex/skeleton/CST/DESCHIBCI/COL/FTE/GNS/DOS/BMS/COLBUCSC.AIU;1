*% FRM = "CSC"
      *
      * COLBUCSC
      * Version 3.0.1 Nov-2005
      * 12-Jul-19 03:49 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMCSC.TXT'
       SET-TAG-COLBUCSC SECTION.
       INI-SET-TAG-COLBUCSC.
           MOVE
           '<IDNMOD NOM=$COLBUCSC$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$12-Jul-19 03:49 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCSC.
           EXIT.
           
       RET-ALL-CSC SECTION.
       INI-RET-ALL-CSC.
*% INCLUDE INI_RET_ALL_CSC OF 'COLBMCSC.TXT'
           PERFORM KEY-ALL-CSC.
           PERFORM RET-OPC-CSC.
*% INCLUDE FIN_RET_ALL_CSC OF 'COLBMCSC.TXT'
       FIN-RET-ALL-CSC.
           EXIT.
       
       RET-OPC-CSC SECTION.
       INI-RET-OPC-CSC.
*% INCLUDE INI_RET_OPC_CSC OF 'COLBMCSC.TXT'
*% INCLUDE FIN_RET_OPC_CSC OF 'COLBMCSC.TXT'
       FIN-RET-OPC-CSC.
           EXIT.
       
       KEY-ALL-CSC SECTION.
       INI-KEY-ALL-CSC.
*% INCLUDE INI_KEY_ALL_CSC OF 'COLBMCSC.TXT'
           PERFORM KEY-OPC-CSC.
*% INCLUDE FIN_KEY_ALL_CSC OF 'COLBMCSC.TXT'
       FIN-KEY-ALL-CSC.
           EXIT.
       
       KEY-OPC-CSC SECTION.
       INI-KEY-OPC-CSC.
*% INCLUDE INI_KEY_OPC_CSC OF 'COLBMCSC.TXT'
           MOVE OPC-CAI-IOPC IN CSC-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE OPC-IIC-IOPC IN CSC-FLD TO OPC-IIC-IOPC IN OPC.
*% INCLUDE FIN_KEY_OPC_CSC OF 'COLBMCSC.TXT'
       FIN-KEY-OPC-CSC.
           EXIT.
       
       SCR-CLRA-CSC SECTION.
       INI-SCR-CLRA-CSC.
*% INCLUDE INI_SCR_CLRA_CSC OF 'COLBMCSC.TXT'
           PERFORM CLR-KEY-CSC.
           PERFORM CLR-FLD-CSC.
*% INCLUDE FIN_SCR_CLRA_CSC OF 'COLBMCSC.TXT'
       FIN-SCR-CLRA-CSC.
           EXIT.
       
       CLR-KEY-CSC SECTION.
       INI-CLR-KEY-CSC.
*% INCLUDE INI_CLR_KEY_CSC OF 'COLBMCSC.TXT'
           MOVE SPACES TO OPC-CAI-IOPC IN CSC-FLD.
           MOVE ZEROES TO OPC-IIC-IOPC IN CSC-FLD.
           MOVE SPACES TO FRM-MSC-CANC IN CSC-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAL"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FCAL IN CSC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-VAL-TDES IN CSC-FLD.
           MOVE ZEROES TO FRM-VAL-TMOR IN CSC-FLD.
           MOVE ZEROES TO FRM-VAL-VPAG IN CSC-FLD.
*% INCLUDE FIN_CLR_KEY_CSC OF 'COLBMCSC.TXT'
       FIN-CLR-KEY-CSC.
           EXIT.
       
       CLR-FLD-CSC SECTION.
       INI-CLR-FLD-CSC.
*% INCLUDE INI_CLR_FLD_CSC OF 'COLBMCSC.TXT'
           MOVE ZEROES TO FRM-VAL-SREN IN CSC-FLD.
           MOVE ZEROES TO FRM-VAL-SDIF IN CSC-FLD.
           MOVE ZEROES TO FRM-VAL-STOT IN CSC-FLD.
           MOVE SPACES TO FRM-GLS-TCAN IN CSC-FLD(1).
           MOVE SPACES TO FRM-GLS-TCAN IN CSC-FLD(2).
           MOVE SPACES TO FRM-GLS-TCAN IN CSC-FLD(3).
           MOVE ZEROES TO FRM-NUM-NCAN IN CSC-FLD(1).
           MOVE ZEROES TO FRM-NUM-NCAN IN CSC-FLD(2).
           MOVE ZEROES TO FRM-NUM-NCAN IN CSC-FLD(3).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FVEN IN CSC-FLD(1).
           MOVE ZEROES TO FRM-FEC-FVEN IN CSC-FLD(2).
           MOVE ZEROES TO FRM-FEC-FVEN IN CSC-FLD(3).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-VAL-CAPI IN CSC-FLD(1).
           MOVE ZEROES TO FRM-VAL-CAPI IN CSC-FLD(2).
           MOVE ZEROES TO FRM-VAL-CAPI IN CSC-FLD(3).
           MOVE ZEROES TO FRM-VAL-INTE IN CSC-FLD(1).
           MOVE ZEROES TO FRM-VAL-INTE IN CSC-FLD(2).
           MOVE ZEROES TO FRM-VAL-INTE IN CSC-FLD(3).
           MOVE ZEROES TO FRM-VAL-MORA IN CSC-FLD(1).
           MOVE ZEROES TO FRM-VAL-MORA IN CSC-FLD(2).
           MOVE ZEROES TO FRM-VAL-MORA IN CSC-FLD(3).
           MOVE ZEROES TO FRM-VAL-HONO IN CSC-FLD(1).
           MOVE ZEROES TO FRM-VAL-HONO IN CSC-FLD(2).
           MOVE ZEROES TO FRM-VAL-HONO IN CSC-FLD(3).
           MOVE ZEROES TO FRM-VAL-IMPT IN CSC-FLD(1).
           MOVE ZEROES TO FRM-VAL-IMPT IN CSC-FLD(2).
           MOVE ZEROES TO FRM-VAL-IMPT IN CSC-FLD(3).
           MOVE ZEROES TO FRM-VAL-GAST IN CSC-FLD(1).
           MOVE ZEROES TO FRM-VAL-GAST IN CSC-FLD(2).
           MOVE ZEROES TO FRM-VAL-GAST IN CSC-FLD(3).
           MOVE ZEROES TO FRM-VAL-COMI IN CSC-FLD(1).
           MOVE ZEROES TO FRM-VAL-COMI IN CSC-FLD(2).
           MOVE ZEROES TO FRM-VAL-COMI IN CSC-FLD(3).
           MOVE ZEROES TO FRM-VAL-RENO IN CSC-FLD(1).
           MOVE ZEROES TO FRM-VAL-RENO IN CSC-FLD(2).
           MOVE ZEROES TO FRM-VAL-RENO IN CSC-FLD(3).
           MOVE ZEROES TO FRM-VAL-DIFE IN CSC-FLD(1).
           MOVE ZEROES TO FRM-VAL-DIFE IN CSC-FLD(2).
           MOVE ZEROES TO FRM-VAL-DIFE IN CSC-FLD(3).
           MOVE ZEROES TO FRM-VAL-SEGU IN CSC-FLD(1).
           MOVE ZEROES TO FRM-VAL-SEGU IN CSC-FLD(2).
           MOVE ZEROES TO FRM-VAL-SEGU IN CSC-FLD(3).
           MOVE ZEROES TO FRM-VAL-TOTA IN CSC-FLD(1).
           MOVE ZEROES TO FRM-VAL-TOTA IN CSC-FLD(2).
           MOVE ZEROES TO FRM-VAL-TOTA IN CSC-FLD(3).
           PERFORM CLR-OPC-CSC.
*% INCLUDE FIN_CLR_FLD_CSC OF 'COLBMCSC.TXT'
       FIN-CLR-FLD-CSC.
           EXIT.
       
       CLR-OPC-CSC SECTION.
       INI-CLR-OPC-CSC.
*% INCLUDE INI_CLR_OPC_CSC OF 'COLBMCSC.TXT'
*% INCLUDE FIN_CLR_OPC_CSC OF 'COLBMCSC.TXT'
       FIN-CLR-OPC-CSC.
           EXIT.
       
       FST-KEY-CSC SECTION.
       INI-FST-KEY-CSC.
*% INCLUDE INI_FST_KEY_CSC OF 'COLBMCSC.TXT'
           MOVE -1 TO OPC-CAI-IOPC-LEN IN CSC-FLD.
*% INCLUDE FIN_FST_KEY_CSC OF 'COLBMCSC.TXT'
       FIN-FST-KEY-CSC.
           EXIT.
       
       FST-FLD-CSC SECTION.
       INI-FST-FLD-CSC.
*% INCLUDE INI_FST_FLD_CSC OF 'COLBMCSC.TXT'
*% INCLUDE FIN_FST_FLD_CSC OF 'COLBMCSC.TXT'
       FIN-FST-FLD-CSC.
           EXIT.
       
       PRO-KEY-CSC SECTION.
       INI-PRO-KEY-CSC.
*% INCLUDE INI_PRO_KEY_CSC OF 'COLBMCSC.TXT'
           MOVE FRM-CPIM-AEY TO FRM-MSC-CANC-ATR IN CSC-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAL"
           MOVE FRM-CPIM-AEY TO FRM-FEC-FCAL-ATR IN CSC-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VAL-TMOR-ATR IN CSC-FLD.
           PERFORM PRO-MKY-CSC.
*% INCLUDE FIN_PRO_KEY_CSC OF 'COLBMCSC.TXT'
       FIN-PRO-KEY-CSC.
           EXIT.
       
       PRO-MKY-CSC SECTION.
       INI-PRO-MKY-CSC.
*% INCLUDE INI_PRO_MKY_CSC OF 'COLBMCSC.TXT'
*% INCLUDE FIN_PRO_MKY_CSC OF 'COLBMCSC.TXT'
       FIN-PRO-MKY-CSC.
           EXIT.
       
       PRO-FLD-CSC SECTION.
       INI-PRO-FLD-CSC.
*% INCLUDE INI_PRO_FLD_CSC OF 'COLBMCSC.TXT'
           PERFORM PRO-OPC-CSC.
*% INCLUDE FIN_PRO_FLD_CSC OF 'COLBMCSC.TXT'
       FIN-PRO-FLD-CSC.
           EXIT.
       
       PRO-OPC-CSC SECTION.
       INI-PRO-OPC-CSC.
*% INCLUDE INI_PRO_OPC_CSC OF 'COLBMCSC.TXT'
*% INCLUDE FIN_PRO_OPC_CSC OF 'COLBMCSC.TXT'
       FIN-PRO-OPC-CSC.
           EXIT.
       
       UNP-KEY-CSC SECTION.
       INI-UNP-KEY-CSC.
*% INCLUDE INI_UNP_KEY_CSC OF 'COLBMCSC.TXT'
           MOVE FRM-CPIM-UBY TO FRM-MSC-CANC-ATR IN CSC-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAL"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FCAL-ATR IN CSC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-VAL-TMOR-ATR IN CSC-FLD.
           PERFORM UNP-IKY-CSC.
           PERFORM UNP-AKY-CSC.
           PERFORM UNP-MKY-CSC.
*% INCLUDE FIN_UNP_KEY_CSC OF 'COLBMCSC.TXT'
       FIN-UNP-KEY-CSC.
           EXIT.
       
       UNP-IKY-CSC SECTION.
       INI-UNP-IKY-CSC.
*% INCLUDE INI_UNP_IKY_CSC OF 'COLBMCSC.TXT'
           MOVE FRM-CPIM-UBY TO OPC-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO OPC-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-MSC-CANC-ATR.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAL"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FCAL-ATR IN CSC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-VAL-TMOR-ATR.
*% INCLUDE FIN_UNP_IKY_CSC OF 'COLBMCSC.TXT'
       FIN-UNP-IKY-CSC.
           EXIT.
       
       UNP-MKY-CSC SECTION.
       INI-UNP-MKY-CSC.
*% INCLUDE INI_UNP_MKY_CSC OF 'COLBMCSC.TXT'
*% INCLUDE FIN_UNP_MKY_CSC OF 'COLBMCSC.TXT'
       FIN-UNP-MKY-CSC.
           EXIT.
       
       UNP-AKY-CSC SECTION.
       INI-UNP-AKY-CSC.
*% INCLUDE INI_UNP_AKY_CSC OF 'COLBMCSC.TXT'
*% INCLUDE FIN_UNP_AKY_CSC OF 'COLBMCSC.TXT'
       FIN-UNP-AKY-CSC.
           EXIT.
       
       UNP-FLD-CSC SECTION.
       INI-UNP-FLD-CSC.
*% INCLUDE INI_UNP_FLD_CSC OF 'COLBMCSC.TXT'
           PERFORM UNP-OPC-CSC.
*% INCLUDE FIN_UNP_FLD_CSC OF 'COLBMCSC.TXT'
       FIN-UNP-FLD-CSC.
           EXIT.
       
       UNP-OPC-CSC SECTION.
       INI-UNP-OPC-CSC.
*% INCLUDE INI_UNP_OPC_CSC OF 'COLBMCSC.TXT'
*% INCLUDE FIN_UNP_OPC_CSC OF 'COLBMCSC.TXT'
       FIN-UNP-OPC-CSC.
           EXIT.
       
       VAL-MKY-CSC SECTION.
       INI-VAL-MKY-CSC.
*% INCLUDE INI_VAL_MKY_CSC OF 'COLBMCSC.TXT'
*% INCLUDE FIN_VAL_MKY_CSC OF 'COLBMCSC.TXT'
       FIN-VAL-MKY-CSC.
           EXIT.
       
       VAL-FLD-CSC SECTION.
       INI-VAL-FLD-CSC.
*% INCLUDE INI_VAL_FLD_CSC OF 'COLBMCSC.TXT'
           PERFORM VAL-FRM-VAL-SREN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-SREN-LEN IN CSC-FLD
                GO TO FIN-VAL-FLD-CSC.
           PERFORM VAL-FRM-VAL-SDIF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-SDIF-LEN IN CSC-FLD
                GO TO FIN-VAL-FLD-CSC.
           PERFORM VAL-FRM-VAL-STOT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-STOT-LEN IN CSC-FLD
                GO TO FIN-VAL-FLD-CSC.
           PERFORM VAL-FRM-GLS-TCAN
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 3
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-GLS-TCAN-LEN IN CSC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CSC.
           PERFORM VAL-FRM-NUM-NCAN
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 3
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-NUM-NCAN-LEN IN CSC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CSC.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FVEN VARYING FRM-IFLD
                FROM 1 BY 1 UNTIL FRM-IFLD > 3 OR
                FRM-SUAR = FRM-SUAR-MAL OR
                FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FVEN-LEN IN CSC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CSC.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-VAL-CAPI
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 3
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-CAPI-LEN IN CSC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CSC.
           PERFORM VAL-FRM-VAL-INTE
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 3
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-INTE-LEN IN CSC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CSC.
           PERFORM VAL-FRM-VAL-MORA
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 3
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-MORA-LEN IN CSC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CSC.
           PERFORM VAL-FRM-VAL-HONO
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 3
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-HONO-LEN IN CSC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CSC.
           PERFORM VAL-FRM-VAL-IMPT
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 3
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-IMPT-LEN IN CSC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CSC.
           PERFORM VAL-FRM-VAL-GAST
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 3
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-GAST-LEN IN CSC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CSC.
           PERFORM VAL-FRM-VAL-COMI
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 3
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-COMI-LEN IN CSC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CSC.
           PERFORM VAL-FRM-VAL-RENO
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 3
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-RENO-LEN IN CSC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CSC.
           PERFORM VAL-FRM-VAL-DIFE
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 3
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-DIFE-LEN IN CSC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CSC.
           PERFORM VAL-FRM-VAL-SEGU
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 3
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-SEGU-LEN IN CSC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CSC.
           PERFORM VAL-FRM-VAL-TOTA
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 3
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-TOTA-LEN IN CSC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CSC.
*% INCLUDE FIN_VAL_FLD_CSC OF 'COLBMCSC.TXT'
       FIN-VAL-FLD-CSC.
           EXIT.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
