*% LBL = "FRM_VAL_STOT OF 'COLBMCSC.TXT'"
*% INCLUDE INI_FRM_VAL_STOT OF 'COLBMCSC.TXT'
*% INCLUDE FRM_VAL_STOT OF 'COLBMCSC.TXT'
