      *
      * COLBQDPL
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:57 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "DPL"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMDPL.TXT'
       SET-TAG-COLBQDPL SECTION.
       INI-SET-TAG-COLBQDPL.
           MOVE
           '<IDNMOD NOM=$COLBQDPL$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:57 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQDPL.
           EXIT.
           
       PUT-ALL-DPL SECTION.
       INI-PUT-ALL-DPL.
*% INCLUDE INI_PUT_ALL_DPL OF 'COLBMDPL.TXT'
           PERFORM PUT-DPL-DPL.
           PERFORM PUT-DBC-DPL.
*% INCLUDE FIN_PUT_ALL_DPL OF 'COLBMDPL.TXT'
       FIN-PUT-ALL-DPL.
           EXIT.
       
       PUT-DPL-DPL SECTION.
       INI-PUT-DPL-DPL.
*% INCLUDE INI_PUT_DPL_DPL OF 'COLBMDPL.TXT'
           IF PGM-STAT-DPL-NUL
               GO TO FIN-PUT-DPL-DPL.
           IF NOT PGM-STAT-DPL-OKS
               PERFORM CLR-DPL-DPL
               GO TO FIN-PUT-DPL-DPL.
           MOVE DPL-CAI-IPLL IN DPL TO DPL-CAI-IPLL IN DPL-FLD.
           MOVE DPL-IIC-IPLL IN DPL TO DPL-IIC-IPLL IN DPL-FLD.
           MOVE DPL-CAI-ICVN IN DPL TO DPL-CAI-ICVN IN DPL-FLD.
           MOVE DPL-IIC-ICVN IN DPL TO DPL-IIC-ICVN IN DPL-FLD.
           MOVE DPL-NUM-IPLL IN DPL TO DPL-NUM-IPLL IN DPL-FLD.
           MOVE DPL-NUM-IDPL IN DPL TO DPL-NUM-IDPL IN DPL-FLD.
           MOVE DPL-CAI-IOPC IN DPL TO DPL-CAI-IOPC IN DPL-FLD.
           MOVE DPL-IIC-IOPC IN DPL TO DPL-IIC-IOPC IN DPL-FLD.
           MOVE DPL-COD-VCOC IN DPL TO DPL-COD-VCOC IN DPL-FLD.
           MOVE DPL-NUM-IVEN IN DPL TO DPL-NUM-IVEN IN DPL-FLD.
           MOVE DPL-NUM-VCOR IN DPL TO DPL-NUM-VCOR IN DPL-FLD.
           MOVE DPL-VAL-CUOT IN DPL TO DPL-VAL-CUOT IN DPL-FLD.
*% RREG = "DPL"
*% RREG_FRM = "DPL"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DPL-NUM-DVEN IN DPL TO DPL-NUM-DVEN IN DPL-FLD.
           MOVE DPL-NUM-MVEN IN DPL TO DPL-NUM-MVEN IN DPL-FLD.
           MOVE DPL-NUM-SVEN IN DPL TO DPL-NUM-SVEN IN DPL-FLD.
           MOVE DPL-NUM-AVEN IN DPL TO DPL-NUM-AVEN IN DPL-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE DPL-VAL-MPAG IN DPL TO DPL-VAL-MPAG IN DPL-FLD.
*% RREG = "DPL"
*% RREG_FRM = "DPL"
*% NOMB = "FING"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DPL-NUM-DING IN DPL TO DPL-NUM-DING IN DPL-FLD.
           MOVE DPL-NUM-MING IN DPL TO DPL-NUM-MING IN DPL-FLD.
           MOVE DPL-NUM-SING IN DPL TO DPL-NUM-SING IN DPL-FLD.
           MOVE DPL-NUM-AING IN DPL TO DPL-NUM-AING IN DPL-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE DPL-VAL-MPGO IN DPL TO DPL-VAL-MPGO IN DPL-FLD.
           MOVE DPL-COD-NOVD IN DPL TO DPL-COD-NOVD IN DPL-FLD.
           MOVE DPL-COD-ESTA IN DPL TO DPL-COD-ESTA IN DPL-FLD.
*% INCLUDE FIN_PUT_DPL_DPL OF 'COLBMDPL.TXT'
       FIN-PUT-DPL-DPL.
           EXIT.
       
       PUT-DBC-DPL SECTION.
       INI-PUT-DBC-DPL.
*% INCLUDE INI_PUT_DBC_DPL OF 'COLBMDPL.TXT'
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-DPL.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-DPL
               GO TO FIN-PUT-DBC-DPL.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN DPL-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN DPL-FLD.
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN DPL-FLD.
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN DPL-FLD.
      * IDC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN DPL-FLD.
*% INCLUDE FIN_PUT_DBC_DPL OF 'COLBMDPL.TXT'
       FIN-PUT-DBC-DPL.
           EXIT.
       
       KEY-ALL-DPL SECTION.
       INI-KEY-ALL-DPL.
*% INCLUDE INI_KEY_ALL_DPL OF 'COLBMDPL.TXT'
           PERFORM KEY-DPL-DPL.
           PERFORM KEY-DBC-DPL.
*% INCLUDE FIN_KEY_ALL_DPL OF 'COLBMDPL.TXT'
       FIN-KEY-ALL-DPL.
           EXIT.
       
       KEY-DPL-DPL SECTION.
       INI-KEY-DPL-DPL.
*% INCLUDE INI_KEY_DPL_DPL OF 'COLBMDPL.TXT'
           MOVE DPL-CAI-IPLL IN DPL-FLD TO DPL-CAI-IPLL IN DPL.
           MOVE DPL-IIC-IPLL IN DPL-FLD TO DPL-IIC-IPLL IN DPL.
           MOVE DPL-CAI-ICVN IN DPL-FLD TO DPL-CAI-ICVN IN DPL.
           MOVE DPL-IIC-ICVN IN DPL-FLD TO DPL-IIC-ICVN IN DPL.
           MOVE DPL-NUM-IPLL IN DPL-FLD TO DPL-NUM-IPLL IN DPL.
           MOVE DPL-NUM-IDPL IN DPL-FLD TO DPL-NUM-IDPL IN DPL.
*% INCLUDE FIN_KEY_DPL_DPL OF 'COLBMDPL.TXT'
       FIN-KEY-DPL-DPL.
           EXIT.
       
       KEY-DBC-DPL SECTION.
       INI-KEY-DBC-DPL.
*% INCLUDE INI_KEY_DBC_DPL OF 'COLBMDPL.TXT'
*% INCLUDE FIN_KEY_DBC_DPL OF 'COLBMDPL.TXT'
       FIN-KEY-DBC-DPL.
           EXIT.
       
       SCR-CLRA-DPL SECTION.
       INI-SCR-CLRA-DPL.
*% INCLUDE INI_SCR_CLRA_DPL OF 'COLBMDPL.TXT'
           PERFORM CLR-KEY-DPL.
           PERFORM CLR-FLD-DPL.
*% INCLUDE FIN_SCR_CLRA_DPL OF 'COLBMDPL.TXT'
       FIN-SCR-CLRA-DPL.
           EXIT.
       
       CLR-KEY-DPL SECTION.
       INI-CLR-KEY-DPL.
*% INCLUDE INI_CLR_KEY_DPL OF 'COLBMDPL.TXT'
           MOVE SPACES TO DPL-CAI-IPLL IN DPL-FLD.
           MOVE ZEROES TO DPL-IIC-IPLL IN DPL-FLD.
           MOVE SPACES TO DPL-CAI-ICVN IN DPL-FLD.
           MOVE ZEROES TO DPL-IIC-ICVN IN DPL-FLD.
           MOVE ZEROES TO DPL-NUM-IPLL IN DPL-FLD.
           MOVE ZEROES TO DPL-NUM-IDPL IN DPL-FLD.
*% INCLUDE FIN_CLR_KEY_DPL OF 'COLBMDPL.TXT'
       FIN-CLR-KEY-DPL.
           EXIT.
       
       CLR-FLD-DPL SECTION.
       INI-CLR-FLD-DPL.
*% INCLUDE INI_CLR_FLD_DPL OF 'COLBMDPL.TXT'
           MOVE SPACES TO FRM-GLS-VCOC IN DPL-FLD.
           MOVE SPACES TO FRM-GLS-NOVD IN DPL-FLD.
           MOVE SPACES TO FRM-GLS-ESTA IN DPL-FLD.
           PERFORM CLR-DPL-DPL.
           PERFORM CLR-DBC-DPL.
*% INCLUDE FIN_CLR_FLD_DPL OF 'COLBMDPL.TXT'
       FIN-CLR-FLD-DPL.
           EXIT.
       
       CLR-DPL-DPL SECTION.
       INI-CLR-DPL-DPL.
*% INCLUDE INI_CLR_DPL_DPL OF 'COLBMDPL.TXT'
           MOVE SPACES TO DPL-CAI-IOPC IN DPL-FLD.
           MOVE ZEROES TO DPL-IIC-IOPC IN DPL-FLD.
           MOVE SPACES TO DPL-COD-VCOC IN DPL-FLD.
           MOVE ZEROES TO DPL-NUM-IVEN IN DPL-FLD.
           MOVE ZEROES TO DPL-NUM-VCOR IN DPL-FLD.
           MOVE ZEROES TO DPL-VAL-CUOT IN DPL-FLD.
*% RREG = "DPL"
*% RREG_FRM = "DPL"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DPL-FEC-FVEN IN DPL-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DPL-VAL-MPAG IN DPL-FLD.
*% RREG = "DPL"
*% RREG_FRM = "DPL"
*% NOMB = "FING"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DPL-FEC-FING IN DPL-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DPL-VAL-MPGO IN DPL-FLD.
           MOVE SPACES TO DPL-COD-NOVD IN DPL-FLD.
           MOVE SPACES TO DPL-COD-ESTA IN DPL-FLD.
*% INCLUDE FIN_CLR_DPL_DPL OF 'COLBMDPL.TXT'
       FIN-CLR-DPL-DPL.
           EXIT.
       
       CLR-DBC-DPL SECTION.
       INI-CLR-DBC-DPL.
*% INCLUDE INI_CLR_DBC_DPL OF 'COLBMDPL.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN DPL-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN DPL-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN DPL-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN DPL-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO DBC-GLS-NOMC IN DPL-FLD.
*% INCLUDE FIN_CLR_DBC_DPL OF 'COLBMDPL.TXT'
       FIN-CLR-DBC-DPL.
           EXIT.
       
       FST-KEY-DPL SECTION.
       INI-FST-KEY-DPL.
*% INCLUDE INI_FST_KEY_DPL OF 'COLBMDPL.TXT'
           MOVE -1 TO DPL-CAI-IPLL-LEN IN DPL-FLD.
*% INCLUDE FIN_FST_KEY_DPL OF 'COLBMDPL.TXT'
       FIN-FST-KEY-DPL.
           EXIT.
       
       FST-FLD-DPL SECTION.
       INI-FST-FLD-DPL.
*% INCLUDE INI_FST_FLD_DPL OF 'COLBMDPL.TXT'
           MOVE -1 TO DPL-CAI-IOPC-LEN IN DPL-FLD.
*% INCLUDE FIN_FST_FLD_DPL OF 'COLBMDPL.TXT'
       FIN-FST-FLD-DPL.
           EXIT.
       
       PRO-KEY-DPL SECTION.
       INI-PRO-KEY-DPL.
*% INCLUDE INI_PRO_KEY_DPL OF 'COLBMDPL.TXT'
           PERFORM PRO-IKY-DPL.
           PERFORM PRO-AKY-DPL.
           PERFORM PRO-MKY-DPL.
*% INCLUDE FIN_PRO_KEY_DPL OF 'COLBMDPL.TXT'
       FIN-PRO-KEY-DPL.
           EXIT.
       
       PRO-IKY-DPL SECTION.
       INI-PRO-IKY-DPL.
*% INCLUDE INI_PRO_IKY_DPL OF 'COLBMDPL.TXT'
           MOVE FRM-CPIM-AEY TO DPL-CAI-IPLL-ATR IN DPL-FLD.
           MOVE FRM-CPIM-AEY TO DPL-IIC-IPLL-ATR IN DPL-FLD.
           MOVE FRM-CPIM-AEY TO DPL-CAI-ICVN-ATR IN DPL-FLD.
           MOVE FRM-CPIM-AEY TO DPL-IIC-ICVN-ATR IN DPL-FLD.
           MOVE FRM-CPIM-AEY TO DPL-NUM-IPLL-ATR IN DPL-FLD.
           MOVE FRM-CPIM-AEY TO DPL-NUM-IDPL-ATR IN DPL-FLD.
*% INCLUDE FIN_PRO_IKY_DPL OF 'COLBMDPL.TXT'
       FIN-PRO-IKY-DPL.
           EXIT.
       
       PRO-MKY-DPL SECTION.
       INI-PRO-MKY-DPL.
*% INCLUDE INI_PRO_MKY_DPL OF 'COLBMDPL.TXT'
*% INCLUDE FIN_PRO_MKY_DPL OF 'COLBMDPL.TXT'
       FIN-PRO-MKY-DPL.
           EXIT.
       
       PRO-AKY-DPL SECTION.
       INI-PRO-AKY-DPL.
*% INCLUDE INI_PRO_AKY_DPL OF 'COLBMDPL.TXT'
*% INCLUDE FIN_PRO_AKY_DPL OF 'COLBMDPL.TXT'
       FIN-PRO-AKY-DPL.
           EXIT.
       
       UNP-KEY-DPL SECTION.
       INI-UNP-KEY-DPL.
*% INCLUDE INI_UNP_KEY_DPL OF 'COLBMDPL.TXT'
           PERFORM UNP-IKY-DPL.
           PERFORM UNP-AKY-DPL.
           PERFORM UNP-MKY-DPL.
*% INCLUDE FIN_UNP_KEY_DPL OF 'COLBMDPL.TXT'
       FIN-UNP-KEY-DPL.
           EXIT.
       
       UNP-IKY-DPL SECTION.
       INI-UNP-IKY-DPL.
*% INCLUDE INI_UNP_IKY_DPL OF 'COLBMDPL.TXT'
           MOVE FRM-CPIM-UBY TO DPL-CAI-IPLL-ATR.
           MOVE FRM-CPIM-UBY TO DPL-IIC-IPLL-ATR.
           MOVE FRM-CPIM-UBY TO DPL-CAI-ICVN-ATR.
           MOVE FRM-CPIM-UBY TO DPL-IIC-ICVN-ATR.
           MOVE FRM-CPIM-UBY TO DPL-NUM-IPLL-ATR.
           MOVE FRM-CPIM-UBY TO DPL-NUM-IDPL-ATR.
*% INCLUDE FIN_UNP_IKY_DPL OF 'COLBMDPL.TXT'
       FIN-UNP-IKY-DPL.
           EXIT.
       
       UNP-MKY-DPL SECTION.
       INI-UNP-MKY-DPL.
*% INCLUDE INI_UNP_MKY_DPL OF 'COLBMDPL.TXT'
*% INCLUDE FIN_UNP_MKY_DPL OF 'COLBMDPL.TXT'
       FIN-UNP-MKY-DPL.
           EXIT.
       
       UNP-AKY-DPL SECTION.
       INI-UNP-AKY-DPL.
*% INCLUDE INI_UNP_AKY_DPL OF 'COLBMDPL.TXT'
*% INCLUDE FIN_UNP_AKY_DPL OF 'COLBMDPL.TXT'
       FIN-UNP-AKY-DPL.
           EXIT.
       
       UNP-FLD-DPL SECTION.
       INI-UNP-FLD-DPL.
*% INCLUDE INI_UNP_FLD_DPL OF 'COLBMDPL.TXT'
           PERFORM UNP-DPL-DPL.
           PERFORM UNP-DBC-DPL.
*% INCLUDE FIN_UNP_FLD_DPL OF 'COLBMDPL.TXT'
       FIN-UNP-FLD-DPL.
           EXIT.
       
       UNP-DPL-DPL SECTION.
       INI-UNP-DPL-DPL.
*% INCLUDE INI_UNP_DPL_DPL OF 'COLBMDPL.TXT'
           MOVE FRM-CPIM-UBY TO DPL-CAI-IOPC-ATR IN DPL-FLD.
           MOVE FRM-CPIM-UBY TO DPL-IIC-IOPC-ATR IN DPL-FLD.
           MOVE FRM-CPIM-UBY TO DPL-NUM-IVEN-ATR IN DPL-FLD.
           MOVE FRM-CPIM-UBY TO DPL-VAL-MPAG-ATR IN DPL-FLD.
*% RREG = "DPL"
*% RREG_FRM = "DPL"
*% NOMB = "FING"
           MOVE FRM-CPIM-UBY TO DPL-FEC-FING-ATR IN DPL-FLD.
           MOVE FRM-CPIM-UBY TO DPL-VAL-MPGO-ATR IN DPL-FLD.
           MOVE FRM-CPIM-UBY TO DPL-COD-NOVD-ATR IN DPL-FLD.
           MOVE FRM-CPIM-UBY TO DPL-COD-ESTA-ATR IN DPL-FLD.
*% INCLUDE FIN_UNP_DPL_DPL OF 'COLBMDPL.TXT'
       FIN-UNP-DPL-DPL.
           EXIT.
       
       UNP-DBC-DPL SECTION.
       INI-UNP-DBC-DPL.
*% INCLUDE INI_UNP_DBC_DPL OF 'COLBMDPL.TXT'
*% INCLUDE FIN_UNP_DBC_DPL OF 'COLBMDPL.TXT'
       FIN-UNP-DBC-DPL.
           EXIT.
       
       VAL-KEY-DPL SECTION.
       INI-VAL-KEY-DPL.
           PERFORM VAL-IKY-DPL.
           PERFORM VAL-MKY-DPL.
           PERFORM VAL-AKY-DPL.
       FIN-VAL-KEY-DPL.
           EXIT.
       
       VAL-IKY-DPL SECTION.
       INI-VAL-IKY-DPL.
*% INCLUDE INI_VAL_IKY_DPL OF 'COLBMDPL.TXT'
           PERFORM VAL-DPL-CAI-IPLL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DPL-CAI-IPLL-LEN IN DPL-FLD
                GO TO FIN-VAL-IKY-DPL.
           PERFORM VAL-DPL-IIC-IPLL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DPL-IIC-IPLL-LEN IN DPL-FLD
                GO TO FIN-VAL-IKY-DPL.
           PERFORM VAL-DPL-CAI-ICVN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DPL-CAI-ICVN-LEN IN DPL-FLD
                GO TO FIN-VAL-IKY-DPL.
           PERFORM VAL-DPL-IIC-ICVN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DPL-IIC-ICVN-LEN IN DPL-FLD
                GO TO FIN-VAL-IKY-DPL.
           PERFORM VAL-DPL-NUM-IPLL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DPL-NUM-IPLL-LEN IN DPL-FLD
                GO TO FIN-VAL-IKY-DPL.
           PERFORM VAL-DPL-NUM-IDPL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DPL-NUM-IDPL-LEN IN DPL-FLD
                GO TO FIN-VAL-IKY-DPL.
*% INCLUDE FIN_VAL_IKY_DPL OF 'COLBMDPL.TXT'
       FIN-VAL-IKY-DPL.
           EXIT.
       
       VAL-MKY-DPL SECTION.
       INI-VAL-MKY-DPL.
*% INCLUDE INI_VAL_MKY_DPL OF 'COLBMDPL.TXT'
*% INCLUDE FIN_VAL_MKY_DPL OF 'COLBMDPL.TXT'
       FIN-VAL-MKY-DPL.
           EXIT.
       
       VAL-AKY-DPL SECTION.
       INI-VAL-AKY-DPL.
*% INCLUDE INI_VAL_AKY_DPL OF 'COLBMDPL.TXT'
*% INCLUDE FIN_VAL_AKY_DPL OF 'COLBMDPL.TXT'
       FIN-VAL-AKY-DPL.
           EXIT.
       
       VAL-DPL-CAI-IPLL SECTION.
       INI-VAL-DPL-CAI-IPLL.
*% LBL = "DPL_CAI_IPLL OF 'COLBMDPL.TXT'"
*% INCLUDE INI_DPL_CAI_IPLL OF 'COLBMDPL.TXT'
           IF DPL-CAI-IPLL IN DPL-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-DPL-CAI-IPLL.
*% INCLUDE DPL_CAI_IPLL OF 'COLBMDPL.TXT'
*% INCLUDE FIN_DPL_CAI_IPLL OF 'COLBMDPL.TXT'
       FIN-VAL-DPL-CAI-IPLL.
           EXIT.
       
       VAL-DPL-IIC-IPLL SECTION.
       INI-VAL-DPL-IIC-IPLL.
*% LBL = "DPL_IIC_IPLL OF 'COLBMDPL.TXT'"
*% INCLUDE INI_DPL_IIC_IPLL OF 'COLBMDPL.TXT'
           IF DPL-IIC-IPLL IN DPL-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-DPL-IIC-IPLL.
*% INCLUDE DPL_IIC_IPLL OF 'COLBMDPL.TXT'
*% INCLUDE FIN_DPL_IIC_IPLL OF 'COLBMDPL.TXT'
       FIN-VAL-DPL-IIC-IPLL.
           EXIT.
       
       VAL-DPL-CAI-ICVN SECTION.
       INI-VAL-DPL-CAI-ICVN.
*% LBL = "DPL_CAI_ICVN OF 'COLBMDPL.TXT'"
*% INCLUDE INI_DPL_CAI_ICVN OF 'COLBMDPL.TXT'
           IF DPL-CAI-ICVN IN DPL-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-DPL-CAI-ICVN.
*% INCLUDE DPL_CAI_ICVN OF 'COLBMDPL.TXT'
*% INCLUDE FIN_DPL_CAI_ICVN OF 'COLBMDPL.TXT'
       FIN-VAL-DPL-CAI-ICVN.
           EXIT.
       
       VAL-DPL-IIC-ICVN SECTION.
       INI-VAL-DPL-IIC-ICVN.
*% LBL = "DPL_IIC_ICVN OF 'COLBMDPL.TXT'"
*% INCLUDE INI_DPL_IIC_ICVN OF 'COLBMDPL.TXT'
           IF DPL-IIC-ICVN IN DPL-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-DPL-IIC-ICVN.
*% INCLUDE DPL_IIC_ICVN OF 'COLBMDPL.TXT'
*% INCLUDE FIN_DPL_IIC_ICVN OF 'COLBMDPL.TXT'
       FIN-VAL-DPL-IIC-ICVN.
           EXIT.
       
       VAL-DPL-NUM-IPLL SECTION.
       INI-VAL-DPL-NUM-IPLL.
*% LBL = "DPL_NUM_IPLL OF 'COLBMDPL.TXT'"
*% INCLUDE INI_DPL_NUM_IPLL OF 'COLBMDPL.TXT'
           IF DPL-NUM-IPLL IN DPL-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-DPL-NUM-IPLL.
*% INCLUDE DPL_NUM_IPLL OF 'COLBMDPL.TXT'
*% INCLUDE FIN_DPL_NUM_IPLL OF 'COLBMDPL.TXT'
       FIN-VAL-DPL-NUM-IPLL.
           EXIT.
       
       VAL-DPL-NUM-IDPL SECTION.
       INI-VAL-DPL-NUM-IDPL.
*% LBL = "DPL_NUM_IDPL OF 'COLBMDPL.TXT'"
*% INCLUDE INI_DPL_NUM_IDPL OF 'COLBMDPL.TXT'
*% INCLUDE DPL_NUM_IDPL OF 'COLBMDPL.TXT'
*% INCLUDE FIN_DPL_NUM_IDPL OF 'COLBMDPL.TXT'
       FIN-VAL-DPL-NUM-IDPL.
           EXIT.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% IZR = "S"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% IZR = "S"
*% RREG = "DPL"
*% RREG_FRM = "DPL"
*% NOMB = "FVEN"
*% IZR = "S"
*% RREG = "DPL"
*% RREG_FRM = "DPL"
*% NOMB = "FING"
*% IZR = "N"
       
       VAL-CON-KEY-DPL SECTION.
       INI-VAL-CON-KEY-DPL.
*% INCLUDE INI_VAL_CON_KEY_DPL OF 'COLBMDPL.TXT'
           IF     DPL-CAI-IPLL IN DPL-FLD NOT > SPACES 
              AND DPL-IIC-IPLL IN DPL-FLD NOT > ZEROES 
              AND DPL-CAI-ICVN IN DPL-FLD NOT > SPACES 
              AND DPL-IIC-ICVN IN DPL-FLD NOT > ZEROES 
              AND DPL-NUM-IPLL IN DPL-FLD NOT > ZEROES 
              AND DPL-NUM-IDPL IN DPL-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-DPL.
*% INCLUDE FIN_VAL_CON_KEY_DPL OF 'COLBMDPL.TXT'
       FIN-VAL-CON-KEY-DPL.
           EXIT.
       
       VAL-CON-IKY-DPL SECTION.
       INI-VAL-CON-IKY-DPL.
*% INCLUDE INI_VAL_CON_IKY_DPL OF 'COLBMDPL.TXT'
           IF     DPL-CAI-IPLL IN DPL-FLD NOT > SPACES 
              AND DPL-IIC-IPLL IN DPL-FLD NOT > ZEROES 
              AND DPL-CAI-ICVN IN DPL-FLD NOT > SPACES 
              AND DPL-IIC-ICVN IN DPL-FLD NOT > ZEROES 
              AND DPL-NUM-IPLL IN DPL-FLD NOT > ZEROES 
              AND DPL-NUM-IDPL IN DPL-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-DPL.
*% INCLUDE FIN_VAL_CON_IKY_DPL OF 'COLBMDPL.TXT'
       FIN-VAL-CON-IKY-DPL.
           EXIT.
       
       VAL-NUM-KEY-DPL SECTION.
       INI-VAL-NUM-KEY-DPL.
           PERFORM VAL-NUM-IKY-DPL.
           PERFORM VAL-NUM-MKY-DPL.
           PERFORM VAL-NUM-AKY-DPL.
       FIN-VAL-NUM-KEY-DPL.
           EXIT.
       
       VAL-NUM-IKY-DPL SECTION.
       INI-VAL-NUM-IKY-DPL.
*% INCLUDE INI_VAL_NUM_IKY_DPL OF 'COLBMDPL.TXT'
       VAL-NUM-DPL-IIC-IPLL.
