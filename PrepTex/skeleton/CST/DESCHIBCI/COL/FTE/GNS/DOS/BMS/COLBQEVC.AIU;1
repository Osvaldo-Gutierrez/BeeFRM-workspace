      *
      * COLBQEVC
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:59 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "EVC"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMEVC.TXT'
       SET-TAG-COLBQEVC SECTION.
       INI-SET-TAG-COLBQEVC.
           MOVE
           '<IDNMOD NOM=$COLBQEVC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:59 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQEVC.
           EXIT.
           
       PUT-ALL-EVC SECTION.
       INI-PUT-ALL-EVC.
*% INCLUDE INI_PUT_ALL_EVC OF 'COLBMEVC.TXT'
           PERFORM PUT-OPC-EVC.
           PERFORM PUT-EVC-EVC.
           PERFORM PUT-PVC-EVC.
*% INCLUDE FIN_PUT_ALL_EVC OF 'COLBMEVC.TXT'
       FIN-PUT-ALL-EVC.
           EXIT.
       
       PUT-OPC-EVC SECTION.
       INI-PUT-OPC-EVC.
*% INCLUDE INI_PUT_OPC_EVC OF 'COLBMEVC.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-EVC.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-EVC
               GO TO FIN-PUT-OPC-EVC.
           MOVE OPC-MSC-TVOC IN OPC TO OPC-MSC-TVOC IN EVC-FLD.
           MOVE OPC-COD-TOOC IN OPC TO OPC-COD-TOOC IN EVC-FLD.
           MOVE OPC-COD-OFOC IN OPC TO OPC-COD-OFOC IN EVC-FLD.
*% BUF = "EVC-FLD"
*% SIS = "TAB"
*% TAB = "TVAL"
*% FLD = "OPC-MSC-TVOC"
*% GLS = "FRM-GLS-TVOC"
*% TIENE_GLOSA = TRUE
*% LGL = 13
*% INCLUDE 'BUSMSC'
*% BUF = "EVC-FLD"
*% SIS = "TAB"
*% TAB = "TIO "
*% FLD = "OPC-COD-TOOC"
*% GLS = "FRM-GLS-TOOC"
*% TIENE_GLOSA = TRUE
*% LGL = 14
*% INCLUDE 'BUSTAB'
*% BUF = "EVC-FLD"
*% SIS = "TAB"
*% TAB = "OFI "
*% FLD = "OPC-COD-OFOC"
*% GLS = "FRM-GLS-OFOC"
*% TIENE_GLOSA = TRUE
*% LGL = 14
*% INCLUDE 'BUSOFI          '
*% INCLUDE FIN_PUT_OPC_EVC OF 'COLBMEVC.TXT'
       FIN-PUT-OPC-EVC.
           EXIT.
       
       PUT-EVC-EVC SECTION.
       INI-PUT-EVC-EVC.
*% INCLUDE INI_PUT_EVC_EVC OF 'COLBMEVC.TXT'
           IF PGM-STAT-EVC-NUL
               GO TO FIN-PUT-EVC-EVC.
           IF NOT PGM-STAT-EVC-OKS
               PERFORM CLR-EVC-EVC
               GO TO FIN-PUT-EVC-EVC.
           MOVE EVC-CAI-IOPC IN EVC TO EVC-CAI-IOPC IN EVC-FLD.
           MOVE EVC-IIC-IOPC IN EVC TO EVC-IIC-IOPC IN EVC-FLD.
           MOVE EVC-NUM-IEVC IN EVC TO EVC-NUM-IEVC IN EVC-FLD.
           MOVE EVC-NUM-IDLC IN EVC TO EVC-NUM-IDLC IN EVC-FLD.
           MOVE EVC-NUM-VIGU IN EVC TO EVC-NUM-VIGU IN EVC-FLD.
*% RREG = "EVC"
*% RREG_FRM = "EVC"
*% NOMB = "FPVC"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE EVC-NUM-DPVC IN EVC TO EVC-NUM-DPVC IN EVC-FLD.
           MOVE EVC-NUM-MPVC IN EVC TO EVC-NUM-MPVC IN EVC-FLD.
           MOVE EVC-NUM-SPVC IN EVC TO EVC-NUM-SPVC IN EVC-FLD.
           MOVE EVC-NUM-APVC IN EVC TO EVC-NUM-APVC IN EVC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE EVC-NUM-DPVE IN EVC TO EVC-NUM-DPVE IN EVC-FLD.
           MOVE EVC-MSC-TCVE IN EVC TO EVC-MSC-TCVE IN EVC-FLD.
           MOVE EVC-NUM-DESD IN EVC TO EVC-NUM-DESD IN EVC-FLD.
           MOVE EVC-VAL-VENC IN EVC TO EVC-VAL-VENC IN EVC-FLD.
           MOVE EVC-NUM-SEVE IN EVC TO EVC-NUM-SEVE IN EVC-FLD.
           MOVE EVC-IND-UTSV IN EVC TO EVC-IND-UTSV IN EVC-FLD.
           MOVE EVC-SGV-FVAR IN EVC TO EVC-SGV-FVAR IN EVC-FLD.
           MOVE EVC-IND-CAUT IN EVC TO EVC-IND-CAUT IN EVC-FLD.
           MOVE EVC-IND-FVAR IN EVC TO EVC-IND-FVAR IN EVC-FLD.
*% BUF = "EVC-FLD"
*% SIS = "COL"
*% TAB = "CVC "
*% FLD = "EVC-MSC-TCVE"
*% GLS = "FRM-GLS-TCVE"
*% TIENE_GLOSA = TRUE
*% LGL = 40
*% INCLUDE 'BUSTAB'
*% BUF = "EVC-FLD"
*% SIS = "TAB"
*% TAB = "IPER"
*% FLD = "EVC-IND-UTSV"
*% GLS = "FRM-GLS-UTSV"
*% TIENE_GLOSA = TRUE
*% LGL = 7
*% INCLUDE 'BUSMSC'
*% INCLUDE FIN_PUT_EVC_EVC OF 'COLBMEVC.TXT'
       FIN-PUT-EVC-EVC.
           EXIT.
       
       PUT-PVC-EVC SECTION.
       INI-PUT-PVC-EVC.
*% INCLUDE INI_PUT_PVC_EVC OF 'COLBMEVC.TXT'
           IF PGM-STAT-PVC-NUL
               GO TO FIN-PUT-PVC-EVC.
           IF NOT PGM-STAT-PVC-OKS
               PERFORM CLR-PVC-EVC
               GO TO FIN-PUT-PVC-EVC.
           MOVE PVC-CIC-IPVC IN PVC TO PVC-CIC-IPVC IN EVC-FLD.
*% INCLUDE FIN_PUT_PVC_EVC OF 'COLBMEVC.TXT'
       FIN-PUT-PVC-EVC.
           EXIT.
       
       KEY-ALL-EVC SECTION.
       INI-KEY-ALL-EVC.
*% INCLUDE INI_KEY_ALL_EVC OF 'COLBMEVC.TXT'
           PERFORM KEY-OPC-EVC.
           PERFORM KEY-EVC-EVC.
           PERFORM KEY-PVC-EVC.
*% INCLUDE FIN_KEY_ALL_EVC OF 'COLBMEVC.TXT'
       FIN-KEY-ALL-EVC.
           EXIT.
       
       KEY-OPC-EVC SECTION.
       INI-KEY-OPC-EVC.
*% INCLUDE INI_KEY_OPC_EVC OF 'COLBMEVC.TXT'
*% INCLUDE FIN_KEY_OPC_EVC OF 'COLBMEVC.TXT'
       FIN-KEY-OPC-EVC.
           EXIT.
       
       KEY-EVC-EVC SECTION.
       INI-KEY-EVC-EVC.
*% INCLUDE INI_KEY_EVC_EVC OF 'COLBMEVC.TXT'
           MOVE EVC-CAI-IOPC IN EVC-FLD TO EVC-CAI-IOPC IN EVC.
           MOVE EVC-IIC-IOPC IN EVC-FLD TO EVC-IIC-IOPC IN EVC.
           MOVE EVC-NUM-IEVC IN EVC-FLD TO EVC-NUM-IEVC IN EVC.
*% INCLUDE FIN_KEY_EVC_EVC OF 'COLBMEVC.TXT'
       FIN-KEY-EVC-EVC.
           EXIT.
       
       KEY-PVC-EVC SECTION.
       INI-KEY-PVC-EVC.
*% INCLUDE INI_KEY_PVC_EVC OF 'COLBMEVC.TXT'
           MOVE PVC-CIC-IPVC IN EVC-FLD TO PVC-CIC-IPVC IN PVC.
*% INCLUDE FIN_KEY_PVC_EVC OF 'COLBMEVC.TXT'
       FIN-KEY-PVC-EVC.
           EXIT.
       
       SCR-CLRA-EVC SECTION.
       INI-SCR-CLRA-EVC.
*% INCLUDE INI_SCR_CLRA_EVC OF 'COLBMEVC.TXT'
           PERFORM CLR-KEY-EVC.
           PERFORM CLR-FLD-EVC.
*% INCLUDE FIN_SCR_CLRA_EVC OF 'COLBMEVC.TXT'
       FIN-SCR-CLRA-EVC.
           EXIT.
       
       CLR-KEY-EVC SECTION.
       INI-CLR-KEY-EVC.
*% INCLUDE INI_CLR_KEY_EVC OF 'COLBMEVC.TXT'
           MOVE SPACES TO EVC-CAI-IOPC IN EVC-FLD.
           MOVE ZEROES TO EVC-IIC-IOPC IN EVC-FLD.
           MOVE ZEROES TO EVC-NUM-IEVC IN EVC-FLD.
           MOVE SPACES TO PVC-CIC-IPVC IN EVC-FLD.
*% INCLUDE FIN_CLR_KEY_EVC OF 'COLBMEVC.TXT'
       FIN-CLR-KEY-EVC.
           EXIT.
       
       CLR-FLD-EVC SECTION.
       INI-CLR-FLD-EVC.
*% INCLUDE INI_CLR_FLD_EVC OF 'COLBMEVC.TXT'
           MOVE SPACES TO FRM-GLS-TVOC IN EVC-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN EVC-FLD.
           MOVE SPACES TO FRM-GLS-OFOC IN EVC-FLD.
           MOVE ZEROES TO FRM-NUM-DURV IN EVC-FLD.
           MOVE SPACES TO FRM-IND-UTDV IN EVC-FLD.
           MOVE SPACES TO FRM-GLS-UTDV IN EVC-FLD.
           MOVE ZEROES TO FRM-NUM-SPVE IN EVC-FLD.
           MOVE SPACES TO FRM-IND-UTPV IN EVC-FLD.
           MOVE SPACES TO FRM-GLS-UTPV IN EVC-FLD.
           MOVE SPACES TO FRM-GLS-TCVE IN EVC-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN EVC-FLD.
           MOVE SPACES TO FRM-GLS-UTSV IN EVC-FLD.
           PERFORM CLR-OPC-EVC.
           PERFORM CLR-EVC-EVC.
           PERFORM CLR-PVC-EVC.
*% INCLUDE FIN_CLR_FLD_EVC OF 'COLBMEVC.TXT'
       FIN-CLR-FLD-EVC.
           EXIT.
       
       CLR-OPC-EVC SECTION.
       INI-CLR-OPC-EVC.
*% INCLUDE INI_CLR_OPC_EVC OF 'COLBMEVC.TXT'
           MOVE SPACES TO OPC-MSC-TVOC IN EVC-FLD.
           MOVE SPACES TO OPC-COD-TOOC IN EVC-FLD.
           MOVE SPACES TO OPC-COD-OFOC IN EVC-FLD.
*% INCLUDE FIN_CLR_OPC_EVC OF 'COLBMEVC.TXT'
       FIN-CLR-OPC-EVC.
           EXIT.
       
       CLR-EVC-EVC SECTION.
       INI-CLR-EVC-EVC.
*% INCLUDE INI_CLR_EVC_EVC OF 'COLBMEVC.TXT'
           MOVE ZEROES TO EVC-NUM-IDLC IN EVC-FLD.
           MOVE ZEROES TO EVC-NUM-VIGU IN EVC-FLD.
*% RREG = "EVC"
*% RREG_FRM = "EVC"
*% NOMB = "FPVC"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO EVC-FEC-FPVC IN EVC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO EVC-NUM-DPVE IN EVC-FLD.
           MOVE SPACES TO EVC-MSC-TCVE IN EVC-FLD.
           MOVE ZEROES TO EVC-NUM-DESD IN EVC-FLD.
           MOVE ZEROES TO EVC-VAL-VENC IN EVC-FLD.
           MOVE ZEROES TO EVC-NUM-SEVE IN EVC-FLD.
           MOVE SPACES TO EVC-IND-UTSV IN EVC-FLD.
           MOVE ZEROES TO EVC-SGV-FVAR IN EVC-FLD.
           MOVE SPACES TO EVC-IND-CAUT IN EVC-FLD.
           MOVE SPACES TO EVC-IND-FVAR IN EVC-FLD.
*% INCLUDE FIN_CLR_EVC_EVC OF 'COLBMEVC.TXT'
       FIN-CLR-EVC-EVC.
           EXIT.
       
       CLR-PVC-EVC SECTION.
       INI-CLR-PVC-EVC.
*% INCLUDE INI_CLR_PVC_EVC OF 'COLBMEVC.TXT'
*% INCLUDE FIN_CLR_PVC_EVC OF 'COLBMEVC.TXT'
       FIN-CLR-PVC-EVC.
           EXIT.
       
       FST-KEY-EVC SECTION.
       INI-FST-KEY-EVC.
*% INCLUDE INI_FST_KEY_EVC OF 'COLBMEVC.TXT'
           MOVE -1 TO EVC-CAI-IOPC-LEN IN EVC-FLD.
*% INCLUDE FIN_FST_KEY_EVC OF 'COLBMEVC.TXT'
       FIN-FST-KEY-EVC.
           EXIT.
       
       FST-FLD-EVC SECTION.
       INI-FST-FLD-EVC.
*% INCLUDE INI_FST_FLD_EVC OF 'COLBMEVC.TXT'
           MOVE -1 TO FRM-NUM-DURV-LEN IN EVC-FLD.
*% INCLUDE FIN_FST_FLD_EVC OF 'COLBMEVC.TXT'
       FIN-FST-FLD-EVC.
           EXIT.
       
       PRO-KEY-EVC SECTION.
       INI-PRO-KEY-EVC.
*% INCLUDE INI_PRO_KEY_EVC OF 'COLBMEVC.TXT'
           PERFORM PRO-IKY-EVC.
           PERFORM PRO-AKY-EVC.
           PERFORM PRO-MKY-EVC.
*% INCLUDE FIN_PRO_KEY_EVC OF 'COLBMEVC.TXT'
       FIN-PRO-KEY-EVC.
           EXIT.
       
       PRO-IKY-EVC SECTION.
       INI-PRO-IKY-EVC.
*% INCLUDE INI_PRO_IKY_EVC OF 'COLBMEVC.TXT'
           MOVE FRM-CPIM-AEY TO EVC-CAI-IOPC-ATR IN EVC-FLD.
           MOVE FRM-CPIM-AEY TO EVC-IIC-IOPC-ATR IN EVC-FLD.
           MOVE FRM-CPIM-AEY TO PVC-CIC-IPVC-ATR IN EVC-FLD.
*% INCLUDE FIN_PRO_IKY_EVC OF 'COLBMEVC.TXT'
       FIN-PRO-IKY-EVC.
           EXIT.
       
       PRO-MKY-EVC SECTION.
       INI-PRO-MKY-EVC.
*% INCLUDE INI_PRO_MKY_EVC OF 'COLBMEVC.TXT'
*% INCLUDE FIN_PRO_MKY_EVC OF 'COLBMEVC.TXT'
       FIN-PRO-MKY-EVC.
           EXIT.
       
       PRO-AKY-EVC SECTION.
       INI-PRO-AKY-EVC.
*% INCLUDE INI_PRO_AKY_EVC OF 'COLBMEVC.TXT'
           MOVE FRM-CPIM-AEY TO EVC-NUM-IEVC-ATR IN EVC-FLD.
*% INCLUDE FIN_PRO_AKY_EVC OF 'COLBMEVC.TXT'
       FIN-PRO-AKY-EVC.
           EXIT.
       
       UNP-KEY-EVC SECTION.
       INI-UNP-KEY-EVC.
*% INCLUDE INI_UNP_KEY_EVC OF 'COLBMEVC.TXT'
           PERFORM UNP-IKY-EVC.
           PERFORM UNP-AKY-EVC.
           PERFORM UNP-MKY-EVC.
*% INCLUDE FIN_UNP_KEY_EVC OF 'COLBMEVC.TXT'
       FIN-UNP-KEY-EVC.
           EXIT.
       
       UNP-IKY-EVC SECTION.
       INI-UNP-IKY-EVC.
*% INCLUDE INI_UNP_IKY_EVC OF 'COLBMEVC.TXT'
           MOVE FRM-CPIM-UBY TO EVC-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO EVC-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO PVC-CIC-IPVC-ATR.
*% INCLUDE FIN_UNP_IKY_EVC OF 'COLBMEVC.TXT'
       FIN-UNP-IKY-EVC.
           EXIT.
       
       UNP-MKY-EVC SECTION.
       INI-UNP-MKY-EVC.
*% INCLUDE INI_UNP_MKY_EVC OF 'COLBMEVC.TXT'
*% INCLUDE FIN_UNP_MKY_EVC OF 'COLBMEVC.TXT'
       FIN-UNP-MKY-EVC.
           EXIT.
       
       UNP-AKY-EVC SECTION.
       INI-UNP-AKY-EVC.
*% INCLUDE INI_UNP_AKY_EVC OF 'COLBMEVC.TXT'
           MOVE FRM-CPIM-UBY TO EVC-NUM-IEVC-ATR.
*% INCLUDE FIN_UNP_AKY_EVC OF 'COLBMEVC.TXT'
       FIN-UNP-AKY-EVC.
           EXIT.
       
       UNP-FLD-EVC SECTION.
       INI-UNP-FLD-EVC.
*% INCLUDE INI_UNP_FLD_EVC OF 'COLBMEVC.TXT'
           MOVE FRM-CPIM-UBY TO FRM-NUM-DURV-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IND-UTDV-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-NUM-SPVE-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IND-UTPV-ATR IN EVC-FLD.
           PERFORM UNP-OPC-EVC.
           PERFORM UNP-EVC-EVC.
           PERFORM UNP-PVC-EVC.
*% INCLUDE FIN_UNP_FLD_EVC OF 'COLBMEVC.TXT'
       FIN-UNP-FLD-EVC.
           EXIT.
       
       UNP-OPC-EVC SECTION.
       INI-UNP-OPC-EVC.
*% INCLUDE INI_UNP_OPC_EVC OF 'COLBMEVC.TXT'
*% INCLUDE FIN_UNP_OPC_EVC OF 'COLBMEVC.TXT'
       FIN-UNP-OPC-EVC.
           EXIT.
       
       UNP-EVC-EVC SECTION.
       INI-UNP-EVC-EVC.
*% INCLUDE INI_UNP_EVC_EVC OF 'COLBMEVC.TXT'
           MOVE FRM-CPIM-UBY TO EVC-NUM-IDLC-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO EVC-NUM-VIGU-ATR IN EVC-FLD.
*% RREG = "EVC"
*% RREG_FRM = "EVC"
*% NOMB = "FPVC"
           MOVE FRM-CPIM-UBY TO EVC-FEC-FPVC-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO EVC-NUM-DPVE-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO EVC-MSC-TCVE-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO EVC-NUM-DESD-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO EVC-VAL-VENC-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO EVC-NUM-SEVE-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO EVC-IND-UTSV-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO EVC-SGV-FVAR-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO EVC-IND-CAUT-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO EVC-IND-FVAR-ATR IN EVC-FLD.
*% INCLUDE FIN_UNP_EVC_EVC OF 'COLBMEVC.TXT'
       FIN-UNP-EVC-EVC.
           EXIT.
       
       UNP-PVC-EVC SECTION.
       INI-UNP-PVC-EVC.
*% INCLUDE INI_UNP_PVC_EVC OF 'COLBMEVC.TXT'
*% INCLUDE FIN_UNP_PVC_EVC OF 'COLBMEVC.TXT'
       FIN-UNP-PVC-EVC.
           EXIT.
       
       VAL-KEY-EVC SECTION.
       INI-VAL-KEY-EVC.
           PERFORM VAL-IKY-EVC.
           PERFORM VAL-MKY-EVC.
           PERFORM VAL-AKY-EVC.
       FIN-VAL-KEY-EVC.
           EXIT.
       
       VAL-IKY-EVC SECTION.
       INI-VAL-IKY-EVC.
*% INCLUDE INI_VAL_IKY_EVC OF 'COLBMEVC.TXT'
           PERFORM VAL-EVC-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EVC-CAI-IOPC-LEN IN EVC-FLD
                GO TO FIN-VAL-IKY-EVC.
           PERFORM VAL-EVC-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EVC-IIC-IOPC-LEN IN EVC-FLD
                GO TO FIN-VAL-IKY-EVC.
           PERFORM VAL-PVC-CIC-IPVC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-CIC-IPVC-LEN IN EVC-FLD
                GO TO FIN-VAL-IKY-EVC.
*% INCLUDE FIN_VAL_IKY_EVC OF 'COLBMEVC.TXT'
       FIN-VAL-IKY-EVC.
           EXIT.
       
       VAL-MKY-EVC SECTION.
       INI-VAL-MKY-EVC.
*% INCLUDE INI_VAL_MKY_EVC OF 'COLBMEVC.TXT'
*% INCLUDE FIN_VAL_MKY_EVC OF 'COLBMEVC.TXT'
       FIN-VAL-MKY-EVC.
           EXIT.
       
       VAL-AKY-EVC SECTION.
       INI-VAL-AKY-EVC.
*% INCLUDE INI_VAL_AKY_EVC OF 'COLBMEVC.TXT'
           PERFORM VAL-EVC-NUM-IEVC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EVC-NUM-IEVC-LEN IN EVC-FLD
                GO TO FIN-VAL-AKY-EVC.
*% INCLUDE FIN_VAL_AKY_EVC OF 'COLBMEVC.TXT'
       FIN-VAL-AKY-EVC.
           EXIT.
       
       VAL-EVC-CAI-IOPC SECTION.
       INI-VAL-EVC-CAI-IOPC.
*% LBL = "EVC_CAI_IOPC OF 'COLBMEVC.TXT'"
*% INCLUDE INI_EVC_CAI_IOPC OF 'COLBMEVC.TXT'
           IF EVC-CAI-IOPC IN EVC-FLD NOT > SPACES
              MOVE SPACES TO EVC-CAI-IOPC IN EVC-FLD.
*% INCLUDE EVC_CAI_IOPC OF 'COLBMEVC.TXT'
*% INCLUDE FIN_EVC_CAI_IOPC OF 'COLBMEVC.TXT'
       FIN-VAL-EVC-CAI-IOPC.
           EXIT.
       
       VAL-EVC-IIC-IOPC SECTION.
       INI-VAL-EVC-IIC-IOPC.
*% LBL = "EVC_IIC_IOPC OF 'COLBMEVC.TXT'"
*% INCLUDE INI_EVC_IIC_IOPC OF 'COLBMEVC.TXT'
*% INCLUDE EVC_IIC_IOPC OF 'COLBMEVC.TXT'
*% INCLUDE FIN_EVC_IIC_IOPC OF 'COLBMEVC.TXT'
       FIN-VAL-EVC-IIC-IOPC.
           EXIT.
       
       VAL-EVC-NUM-IEVC SECTION.
       INI-VAL-EVC-NUM-IEVC.
*% LBL = "EVC_NUM_IEVC OF 'COLBMEVC.TXT'"
*% INCLUDE INI_EVC_NUM_IEVC OF 'COLBMEVC.TXT'
*% INCLUDE EVC_NUM_IEVC OF 'COLBMEVC.TXT'
*% INCLUDE FIN_EVC_NUM_IEVC OF 'COLBMEVC.TXT'
       FIN-VAL-EVC-NUM-IEVC.
           EXIT.
       
       VAL-PVC-CIC-IPVC SECTION.
       INI-VAL-PVC-CIC-IPVC.
*% LBL = "PVC_CIC_IPVC OF 'COLBMEVC.TXT'"
*% INCLUDE INI_PVC_CIC_IPVC OF 'COLBMEVC.TXT'
           IF PVC-CIC-IPVC IN EVC-FLD NOT > SPACES
              MOVE SPACES TO PVC-CIC-IPVC IN EVC-FLD.
*% INCLUDE PVC_CIC_IPVC OF 'COLBMEVC.TXT'
*% INCLUDE FIN_PVC_CIC_IPVC OF 'COLBMEVC.TXT'
       FIN-VAL-PVC-CIC-IPVC.
           EXIT.
*% RREG = "EVC"
*% RREG_FRM = "EVC"
*% NOMB = "FPVC"
*% IZR = "S"
       
       VAL-CON-KEY-EVC SECTION.
       INI-VAL-CON-KEY-EVC.
*% INCLUDE INI_VAL_CON_KEY_EVC OF 'COLBMEVC.TXT'
           IF     EVC-CAI-IOPC IN EVC-FLD NOT > SPACES 
              AND EVC-IIC-IOPC IN EVC-FLD NOT > ZEROES 
              AND EVC-NUM-IEVC IN EVC-FLD NOT > ZEROES 
              AND PVC-CIC-IPVC IN EVC-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-EVC.
*% INCLUDE FIN_VAL_CON_KEY_EVC OF 'COLBMEVC.TXT'
       FIN-VAL-CON-KEY-EVC.
           EXIT.
       
