       INI-VAL-MKY-DPC.
*% INCLUDE INI_VAL_MKY_DPC OF 'COLBMDPC.TXT'
*% INCLUDE FIN_VAL_MKY_DPC OF 'COLBMDPC.TXT'
       FIN-VAL-MKY-DPC.
           EXIT.
       
       VAL-FLD-DPC SECTION.
       INI-VAL-FLD-DPC.
*% INCLUDE INI_VAL_FLD_DPC OF 'COLBMDPC.TXT'
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPRO"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FPRO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FPRO-LEN IN DPC-FLD
                GO TO FIN-VAL-FLD-DPC.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-NUM-FOI2.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-NUM-FOI2-LEN IN DPC-FLD
                GO TO FIN-VAL-FLD-DPC.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FINI"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FINI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FINI-LEN IN DPC-FLD
                GO TO FIN-VAL-FLD-DPC.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-NUM-FOF2.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-NUM-FOF2-LEN IN DPC-FLD
                GO TO FIN-VAL-FLD-DPC.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FTERM"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FTERM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FTERM-LEN IN DPC-FLD
                GO TO FIN-VAL-FLD-DPC.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-VAL-SDAN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-SDAN-LEN IN DPC-FLD
                GO TO FIN-VAL-FLD-DPC.
           PERFORM VAL-FRM-VAL-SHAN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-SHAN-LEN IN DPC-FLD
                GO TO FIN-VAL-FLD-DPC.
           PERFORM VAL-FRM-VAL-MDPE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-MDPE-LEN IN DPC-FLD
                GO TO FIN-VAL-FLD-DPC.
           PERFORM VAL-FRM-VAL-MHPE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-MHPE-LEN IN DPC-FLD
                GO TO FIN-VAL-FLD-DPC.
           PERFORM VAL-FRM-VAL-SDNV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-SDNV-LEN IN DPC-FLD
                GO TO FIN-VAL-FLD-DPC.
           PERFORM VAL-FRM-VAL-SHNV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-SHNV-LEN IN DPC-FLD
                GO TO FIN-VAL-FLD-DPC.
           PERFORM VAL-FRM-VAL-MACU.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-MACU-LEN IN DPC-FLD
                GO TO FIN-VAL-FLD-DPC.
           PERFORM VAL-FRM-NUM-RACU.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-NUM-RACU-LEN IN DPC-FLD
                GO TO FIN-VAL-FLD-DPC.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FELI"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FELI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
