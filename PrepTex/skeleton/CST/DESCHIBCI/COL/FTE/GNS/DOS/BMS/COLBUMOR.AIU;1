*% FRM = "MOR"
      *
      * COLBUMOR
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:08 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMMOR.TXT'
       SET-TAG-COLBUMOR SECTION.
       INI-SET-TAG-COLBUMOR.
           MOVE
           '<IDNMOD NOM=$COLBUMOR$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:08 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUMOR.
           EXIT.
           
       RET-ALL-MOR SECTION.
       INI-RET-ALL-MOR.
*% INCLUDE INI_RET_ALL_MOR OF 'COLBMMOR.TXT'
           PERFORM KEY-ALL-MOR.
           PERFORM RET-MOR-MOR.
*% INCLUDE FIN_RET_ALL_MOR OF 'COLBMMOR.TXT'
       FIN-RET-ALL-MOR.
           EXIT.
       
       RET-MOR-MOR SECTION.
       INI-RET-MOR-MOR.
*% INCLUDE INI_RET_MOR_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_RET_MOR_MOR OF 'COLBMMOR.TXT'
       FIN-RET-MOR-MOR.
           EXIT.
       
       KEY-ALL-MOR SECTION.
       INI-KEY-ALL-MOR.
*% INCLUDE INI_KEY_ALL_MOR OF 'COLBMMOR.TXT'
           PERFORM KEY-MOR-MOR.
*% INCLUDE FIN_KEY_ALL_MOR OF 'COLBMMOR.TXT'
       FIN-KEY-ALL-MOR.
           EXIT.
       
       KEY-MOR-MOR SECTION.
       INI-KEY-MOR-MOR.
*% INCLUDE INI_KEY_MOR_MOR OF 'COLBMMOR.TXT'
           MOVE MOR-COD-OFI IN MOR-FLD TO MOR-COD-OFI IN MOR.
*% INCLUDE FIN_KEY_MOR_MOR OF 'COLBMMOR.TXT'
       FIN-KEY-MOR-MOR.
           EXIT.
       
       SCR-CLRA-MOR SECTION.
       INI-SCR-CLRA-MOR.
*% INCLUDE INI_SCR_CLRA_MOR OF 'COLBMMOR.TXT'
           PERFORM CLR-KEY-MOR.
           PERFORM CLR-FLD-MOR.
*% INCLUDE FIN_SCR_CLRA_MOR OF 'COLBMMOR.TXT'
       FIN-SCR-CLRA-MOR.
           EXIT.
       
       CLR-KEY-MOR SECTION.
       INI-CLR-KEY-MOR.
*% INCLUDE INI_CLR_KEY_MOR OF 'COLBMMOR.TXT'
           MOVE ZEROES TO MOR-COD-OFI IN MOR-FLD.
           MOVE SPACES TO FRM-COD-EJE IN MOR-FLD.
*% INCLUDE FIN_CLR_KEY_MOR OF 'COLBMMOR.TXT'
       FIN-CLR-KEY-MOR.
           EXIT.
       
       CLR-FLD-MOR SECTION.
       INI-CLR-FLD-MOR.
*% INCLUDE INI_CLR_FLD_MOR OF 'COLBMMOR.TXT'
           MOVE SPACES TO FRM-INF-DES IN MOR-FLD.
           MOVE SPACES TO FRM-INF-HAS IN MOR-FLD.
           MOVE SPACES TO FRM-GLS-OFI IN MOR-FLD.
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(1).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(2).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(3).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(4).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(5).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(6).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(7).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(8).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(9).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(10).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(11).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(12).
           PERFORM CLR-MOR-MOR.
*% INCLUDE FIN_CLR_FLD_MOR OF 'COLBMMOR.TXT'
       FIN-CLR-FLD-MOR.
           EXIT.
       
       CLR-MOR-MOR SECTION.
       INI-CLR-MOR-MOR.
*% INCLUDE INI_CLR_MOR_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_CLR_MOR_MOR OF 'COLBMMOR.TXT'
       FIN-CLR-MOR-MOR.
           EXIT.
       
       FST-KEY-MOR SECTION.
       INI-FST-KEY-MOR.
*% INCLUDE INI_FST_KEY_MOR OF 'COLBMMOR.TXT'
           MOVE -1 TO MOR-COD-OFI-LEN IN MOR-FLD.
*% INCLUDE FIN_FST_KEY_MOR OF 'COLBMMOR.TXT'
       FIN-FST-KEY-MOR.
           EXIT.
       
       FST-FLD-MOR SECTION.
       INI-FST-FLD-MOR.
*% INCLUDE INI_FST_FLD_MOR OF 'COLBMMOR.TXT'
           MOVE -1 TO FRM-INF-DES-LEN IN MOR-FLD.
*% INCLUDE FIN_FST_FLD_MOR OF 'COLBMMOR.TXT'
       FIN-FST-FLD-MOR.
           EXIT.
       
       PRO-KEY-MOR SECTION.
       INI-PRO-KEY-MOR.
*% INCLUDE INI_PRO_KEY_MOR OF 'COLBMMOR.TXT'
           MOVE FRM-CPIM-AEY TO FRM-COD-EJE-ATR IN MOR-FLD.
           PERFORM PRO-MKY-MOR.
*% INCLUDE FIN_PRO_KEY_MOR OF 'COLBMMOR.TXT'
       FIN-PRO-KEY-MOR.
           EXIT.
       
       PRO-MKY-MOR SECTION.
       INI-PRO-MKY-MOR.
*% INCLUDE INI_PRO_MKY_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_PRO_MKY_MOR OF 'COLBMMOR.TXT'
       FIN-PRO-MKY-MOR.
           EXIT.
       
       PRO-FLD-MOR SECTION.
       INI-PRO-FLD-MOR.
*% INCLUDE INI_PRO_FLD_MOR OF 'COLBMMOR.TXT'
           MOVE FRM-CPIM-AEY TO FRM-INF-DES-ATR IN MOR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-INF-HAS-ATR IN MOR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-OFI-ATR IN MOR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-LIN-DET-ATR IN MOR-FLD(1).
           MOVE FRM-CPIM-AEY TO FRM-LIN-DET-ATR IN MOR-FLD(2).
           MOVE FRM-CPIM-AEY TO FRM-LIN-DET-ATR IN MOR-FLD(3).
           MOVE FRM-CPIM-AEY TO FRM-LIN-DET-ATR IN MOR-FLD(4).
           MOVE FRM-CPIM-AEY TO FRM-LIN-DET-ATR IN MOR-FLD(5).
           MOVE FRM-CPIM-AEY TO FRM-LIN-DET-ATR IN MOR-FLD(6).
           MOVE FRM-CPIM-AEY TO FRM-LIN-DET-ATR IN MOR-FLD(7).
           MOVE FRM-CPIM-AEY TO FRM-LIN-DET-ATR IN MOR-FLD(8).
           MOVE FRM-CPIM-AEY TO FRM-LIN-DET-ATR IN MOR-FLD(9).
           MOVE FRM-CPIM-AEY TO FRM-LIN-DET-ATR IN MOR-FLD(10).
           MOVE FRM-CPIM-AEY TO FRM-LIN-DET-ATR IN MOR-FLD(11).
           MOVE FRM-CPIM-AEY TO FRM-LIN-DET-ATR IN MOR-FLD(12).
           PERFORM PRO-MOR-MOR.
*% INCLUDE FIN_PRO_FLD_MOR OF 'COLBMMOR.TXT'
       FIN-PRO-FLD-MOR.
           EXIT.
       
       PRO-MOR-MOR SECTION.
       INI-PRO-MOR-MOR.
*% INCLUDE INI_PRO_MOR_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_PRO_MOR_MOR OF 'COLBMMOR.TXT'
       FIN-PRO-MOR-MOR.
           EXIT.
       
       UNP-KEY-MOR SECTION.
       INI-UNP-KEY-MOR.
*% INCLUDE INI_UNP_KEY_MOR OF 'COLBMMOR.TXT'
           MOVE FRM-CPIM-UBY TO FRM-COD-EJE-ATR IN MOR-FLD.
           PERFORM UNP-IKY-MOR.
           PERFORM UNP-AKY-MOR.
           PERFORM UNP-MKY-MOR.
*% INCLUDE FIN_UNP_KEY_MOR OF 'COLBMMOR.TXT'
       FIN-UNP-KEY-MOR.
           EXIT.
       
       UNP-IKY-MOR SECTION.
       INI-UNP-IKY-MOR.
*% INCLUDE INI_UNP_IKY_MOR OF 'COLBMMOR.TXT'
           MOVE FRM-CPIM-UBY TO MOR-COD-OFI-ATR.
           MOVE FRM-CPIM-UBY TO FRM-COD-EJE-ATR.
*% INCLUDE FIN_UNP_IKY_MOR OF 'COLBMMOR.TXT'
       FIN-UNP-IKY-MOR.
           EXIT.
       
       UNP-MKY-MOR SECTION.
       INI-UNP-MKY-MOR.
*% INCLUDE INI_UNP_MKY_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_UNP_MKY_MOR OF 'COLBMMOR.TXT'
       FIN-UNP-MKY-MOR.
           EXIT.
       
       UNP-AKY-MOR SECTION.
       INI-UNP-AKY-MOR.
*% INCLUDE INI_UNP_AKY_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_UNP_AKY_MOR OF 'COLBMMOR.TXT'
       FIN-UNP-AKY-MOR.
           EXIT.
       
       UNP-FLD-MOR SECTION.
       INI-UNP-FLD-MOR.
*% INCLUDE INI_UNP_FLD_MOR OF 'COLBMMOR.TXT'
           MOVE FRM-CPIM-UBY TO FRM-INF-DES-ATR IN MOR-FLD.
           MOVE FRM-CPIM-UBY TO FRM-INF-HAS-ATR IN MOR-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-OFI-ATR IN MOR-FLD.
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(1).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(2).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(3).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(4).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(5).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(6).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(7).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(8).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(9).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(10).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(11).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(12).
           PERFORM UNP-MOR-MOR.
*% INCLUDE FIN_UNP_FLD_MOR OF 'COLBMMOR.TXT'
       FIN-UNP-FLD-MOR.
           EXIT.
       
       UNP-MOR-MOR SECTION.
       INI-UNP-MOR-MOR.
*% INCLUDE INI_UNP_MOR_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_UNP_MOR_MOR OF 'COLBMMOR.TXT'
       FIN-UNP-MOR-MOR.
           EXIT.
       
       VAL-MKY-MOR SECTION.
       INI-VAL-MKY-MOR.
*% INCLUDE INI_VAL_MKY_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_VAL_MKY_MOR OF 'COLBMMOR.TXT'
       FIN-VAL-MKY-MOR.
           EXIT.
       
       VAL-FLD-MOR SECTION.
       INI-VAL-FLD-MOR.
*% INCLUDE INI_VAL_FLD_MOR OF 'COLBMMOR.TXT'
           PERFORM VAL-FRM-INF-DES.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-INF-DES-LEN IN MOR-FLD
                GO TO FIN-VAL-FLD-MOR.
           PERFORM VAL-FRM-INF-HAS.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-INF-HAS-LEN IN MOR-FLD
                GO TO FIN-VAL-FLD-MOR.
           PERFORM VAL-FRM-GLS-OFI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-OFI-LEN IN MOR-FLD
                GO TO FIN-VAL-FLD-MOR.
           PERFORM VAL-FRM-LIN-DET
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 12
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-LIN-DET-LEN IN MOR-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-MOR.
*% INCLUDE FIN_VAL_FLD_MOR OF 'COLBMMOR.TXT'
       FIN-VAL-FLD-MOR.
           EXIT.
       
       VAL-FRM-INF-DES SECTION.
       INI-VAL-FRM-INF-DES.
*% LBL = "FRM_INF_DES OF 'COLBMMOR.TXT'"
*% INCLUDE INI_FRM_INF_DES OF 'COLBMMOR.TXT'
           IF FRM-INF-DES IN MOR-FLD NOT > SPACES
              MOVE SPACES TO FRM-INF-DES IN MOR-FLD.
*% INCLUDE FRM_INF_DES OF 'COLBMMOR.TXT'
*% INCLUDE FIN_FRM_INF_DES OF 'COLBMMOR.TXT'
       FIN-VAL-FRM-INF-DES.
           EXIT.
       
       VAL-FRM-INF-HAS SECTION.
       INI-VAL-FRM-INF-HAS.
*% LBL = "FRM_INF_HAS OF 'COLBMMOR.TXT'"
*% INCLUDE INI_FRM_INF_HAS OF 'COLBMMOR.TXT'
           IF FRM-INF-HAS IN MOR-FLD NOT > SPACES
              MOVE SPACES TO FRM-INF-HAS IN MOR-FLD.
*% INCLUDE FRM_INF_HAS OF 'COLBMMOR.TXT'
*% INCLUDE FIN_FRM_INF_HAS OF 'COLBMMOR.TXT'
       FIN-VAL-FRM-INF-HAS.
           EXIT.
       
       VAL-FRM-GLS-OFI SECTION.
       INI-VAL-FRM-GLS-OFI.
*% LBL = "FRM_GLS_OFI OF 'COLBMMOR.TXT'"
*% INCLUDE INI_FRM_GLS_OFI OF 'COLBMMOR.TXT'
           IF FRM-GLS-OFI IN MOR-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-OFI IN MOR-FLD.
*% INCLUDE FRM_GLS_OFI OF 'COLBMMOR.TXT'
*% INCLUDE FIN_FRM_GLS_OFI OF 'COLBMMOR.TXT'
       FIN-VAL-FRM-GLS-OFI.
           EXIT.
       
       VAL-FRM-LIN-DET SECTION.
       INI-VAL-FRM-LIN-DET.
*% LBL = "FRM_LIN_DET OF 'COLBMMOR.TXT'"
*% INCLUDE INI_FRM_LIN_DET OF 'COLBMMOR.TXT'
           IF FRM-LIN-DET IN MOR-FLD(FRM-IFLD) NOT > SPACES
              MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(FRM-IFLD).
*% INCLUDE FRM_LIN_DET OF 'COLBMMOR.TXT'
*% INCLUDE FIN_FRM_LIN_DET OF 'COLBMMOR.TXT'
       FIN-VAL-FRM-LIN-DET.
           EXIT.
       
       VAL-CON-FLD-MOR SECTION.
       INI-VAL-CON-FLD-MOR.
*% INCLUDE VAL_CON_FLD_MOR OF 'COLBMMOR.TXT'
       FIN-VAL-CON-FLD-MOR.
           EXIT.
       
       VAL-CON-MOR SECTION.
       INI-VAL-CON-MOR.
*% INCLUDE VAL_CON_MOR OF 'COLBMMOR.TXT'
       FIN-VAL-CON-MOR.
           EXIT.
       
       VAL-NUM-KEY-MOR SECTION.
       INI-VAL-NUM-KEY-MOR.
           PERFORM VAL-NUM-IKY-MOR.
           PERFORM VAL-NUM-MKY-MOR.
           PERFORM VAL-NUM-AKY-MOR.
       FIN-VAL-NUM-KEY-MOR.
           EXIT.
       
       VAL-NUM-IKY-MOR SECTION.
       INI-VAL-NUM-IKY-MOR.
*% INCLUDE INI_VAL_NUM_IKY_MOR OF 'COLBMMOR.TXT'
       VAL-NUM-MOR-COD-OFI.
*% BUF = "MOR-FLD"
*% FLD = "MOR-COD-OFI"
*% ENT = "3"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-MOR.
*% INCLUDE FIN_VAL_NUM_IKY_MOR OF 'COLBMMOR.TXT'
       FIN-VAL-NUM-IKY-MOR.
           EXIT.
       
       VAL-NUM-MKY-MOR SECTION.
       INI-VAL-NUM-MKY-MOR.
*% INCLUDE INI_VAL_NUM_MKY_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_MOR OF 'COLBMMOR.TXT'
       FIN-VAL-NUM-MKY-MOR.
           EXIT.
       
       VAL-NUM-AKY-MOR SECTION.
       INI-VAL-NUM-AKY-MOR.
*% INCLUDE INI_VAL_NUM_AKY_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_MOR OF 'COLBMMOR.TXT'
       FIN-VAL-NUM-AKY-MOR.
           EXIT.
       
       VAL-NUM-FLD-MOR SECTION.
       INI-VAL-NUM-FLD-MOR.
*% INCLUDE INI_VAL_NUM_FLD_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_VAL_NUM_FLD_MOR OF 'COLBMMOR.TXT'
       FIN-VAL-NUM-FLD-MOR.
           EXIT.
       
       EDT-KEY-MOR SECTION.
       INI-EDT-KEY-MOR.
           PERFORM EDT-IKY-MOR.
           PERFORM EDT-MKY-MOR.
           PERFORM EDT-AKY-MOR.
*% INCLUDE EDT_KEY_MOR OF 'COLBMMOR.TXT'
       FIN-EDT-KEY-MOR.
           EXIT.
       
       EDT-IKY-MOR SECTION.
       INI-EDT-IKY-MOR.
*% INCLUDE EDT_IKY_MOR OF 'COLBMMOR.TXT'
           IF MOR-COD-OFI-LEN IN MOR-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO MOR-COD-OFI-LEN IN MOR-FLD
                GO TO FIN-EDT-IKY-MOR.
           MOVE MOR-COD-OFI IN MOR-FLD TO MOR-COD-OFI-EDT IN MOR-EDT.
           MOVE MOR-COD-OFI-EDT IN MOR-EDT TO 
                MOR-COD-OFI-ALF IN MOR-FLD.
*% INCLUDE FIN_EDT_IKY_MOR OF 'COLBMMOR.TXT'
       FIN-EDT-IKY-MOR.
           EXIT.
       
       EDT-MKY-MOR SECTION.
       INI-EDT-MKY-MOR.
*% INCLUDE EDT_MKY_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_EDT_MKY_MOR OF 'COLBMMOR.TXT'
       FIN-EDT-MKY-MOR.
           EXIT.
       
       EDT-AKY-MOR SECTION.
       INI-EDT-AKY-MOR.
*% INCLUDE EDT_AKY_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_EDT_AKY_MOR OF 'COLBMMOR.TXT'
       FIN-EDT-AKY-MOR.
           EXIT.
       
       EDT-FLD-MOR SECTION.
       INI-EDT-FLD-MOR.
*% INCLUDE EDT_FLD_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_EDT_FLD_MOR OF 'COLBMMOR.TXT'
       FIN-EDT-FLD-MOR.
           EXIT.
