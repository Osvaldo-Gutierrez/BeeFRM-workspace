*% FRM = "DLC"
      *
      * COLBUDLC
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:53 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMDLC.TXT'
       SET-TAG-COLBUDLC SECTION.
       INI-SET-TAG-COLBUDLC.
           MOVE
           '<IDNMOD NOM=$COLBUDLC$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:53 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUDLC.
           EXIT.
           
       RET-ALL-DLC SECTION.
       INI-RET-ALL-DLC.
*% INCLUDE INI_RET_ALL_DLC OF 'COLBMDLC.TXT'
           PERFORM KEY-ALL-DLC.
           PERFORM RET-DLC-DLC.
           PERFORM RET-ICG-DLC.
           PERFORM RET-RDC-DLC.
           PERFORM RET-OPC-DLC.
           PERFORM RET-DBC-DLC.
*% INCLUDE FIN_RET_ALL_DLC OF 'COLBMDLC.TXT'
       FIN-RET-ALL-DLC.
           EXIT.
       
       RET-DLC-DLC SECTION.
       INI-RET-DLC-DLC.
*% INCLUDE INI_RET_DLC_DLC OF 'COLBMDLC.TXT'
           MOVE DLC-CAI-IOCO IN DLC-FLD TO DLC-CAI-IOCO IN DLC.
           MOVE DLC-IIC-IOCO IN DLC-FLD TO DLC-IIC-IOCO IN DLC.
           MOVE DLC-NUM-IDLO IN DLC-FLD TO DLC-NUM-IDLO IN DLC.
           MOVE DLC-VAL-NDLC IN DLC-FLD TO DLC-VAL-NDLC IN DLC.
           MOVE DLC-VAL-SDLC IN DLC-FLD TO DLC-VAL-SDLC IN DLC.
           MOVE DLC-IND-RIES IN DLC-FLD TO DLC-IND-RIES IN DLC.
           MOVE DLC-COD-TDOC IN DLC-FLD TO DLC-COD-TDOC IN DLC.
           MOVE DLC-COD-UBFD IN DLC-FLD TO DLC-COD-UBFD IN DLC.
           MOVE DLC-MSC-TIMP IN DLC-FLD TO DLC-MSC-TIMP IN DLC.
           MOVE DLC-VAL-IMPU IN DLC-FLD TO DLC-VAL-IMPU IN DLC.
           MOVE DLC-COD-PLZA IN DLC-FLD TO DLC-COD-PLZA IN DLC.
           MOVE DLC-COD-NOTC IN DLC-FLD TO DLC-COD-NOTC IN DLC.
           MOVE DLC-VAL-GNOT IN DLC-FLD TO DLC-VAL-GNOT IN DLC.
           MOVE DLC-COD-PLZO IN DLC-FLD TO DLC-COD-PLZO IN DLC.
           MOVE DLC-MSC-MIPL IN DLC-FLD TO DLC-MSC-MIPL IN DLC.
           MOVE DLC-VAL-IPEN IN DLC-FLD TO DLC-VAL-IPEN IN DLC.
           MOVE DLC-IND-SCRT IN DLC-FLD TO DLC-IND-SCRT IN DLC.
*% RREG = "DLC"
*% RREG_FRM = "DLC"
*% NOMB = "FDEV"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DLC-NUM-DDEV IN DLC-FLD TO DLC-NUM-DDEV IN DLC.
           MOVE DLC-NUM-MDEV IN DLC-FLD TO DLC-NUM-MDEV IN DLC.
           MOVE DLC-NUM-SDEV IN DLC-FLD TO DLC-NUM-SDEV IN DLC.
           MOVE DLC-NUM-ADEV IN DLC-FLD TO DLC-NUM-ADEV IN DLC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DLC-IND-SCTB IN DLC-FLD TO DLC-IND-SCTB IN DLC.
           MOVE DLC-VAL-READ IN DLC-FLD TO DLC-VAL-READ IN DLC.
           MOVE DLC-IND-SCBZ IN DLC-FLD TO DLC-IND-SCBZ IN DLC.
           MOVE DLC-VAL-INTD IN DLC-FLD TO DLC-VAL-INTD IN DLC.
           MOVE DLC-COD-PAGO IN DLC-FLD TO DLC-COD-PAGO IN DLC.
           MOVE DLC-IND-ESTA IN DLC-FLD TO DLC-IND-ESTA IN DLC.
           MOVE DLC-IND-ETAP IN DLC-FLD TO DLC-IND-ETAP IN DLC.
           MOVE DLC-IND-HPER IN DLC-FLD TO DLC-IND-HPER IN DLC.
*% INCLUDE FIN_RET_DLC_DLC OF 'COLBMDLC.TXT'
       FIN-RET-DLC-DLC.
           EXIT.
       
       RET-ICG-DLC SECTION.
       INI-RET-ICG-DLC.
*% INCLUDE INI_RET_ICG_DLC OF 'COLBMDLC.TXT'
           MOVE ICG-COD-TINT IN DLC-FLD TO ICG-COD-TINT IN ICG.
           MOVE ICG-SGV-TINT IN DLC-FLD TO ICG-SGV-TINT IN ICG.
*% INCLUDE FIN_RET_ICG_DLC OF 'COLBMDLC.TXT'
       FIN-RET-ICG-DLC.
           EXIT.
       
       RET-RDC-DLC SECTION.
       INI-RET-RDC-DLC.
*% INCLUDE INI_RET_RDC_DLC OF 'COLBMDLC.TXT'
           MOVE RDC-NUM-DIRE IN DLC-FLD TO RDC-NUM-DIRE IN RDC.
*% INCLUDE FIN_RET_RDC_DLC OF 'COLBMDLC.TXT'
       FIN-RET-RDC-DLC.
           EXIT.
       
       RET-OPC-DLC SECTION.
       INI-RET-OPC-DLC.
*% INCLUDE INI_RET_OPC_DLC OF 'COLBMDLC.TXT'
           MOVE OPC-MSC-TVOC IN DLC-FLD TO OPC-MSC-TVOC IN OPC.
           MOVE OPC-COD-TOOC IN DLC-FLD TO OPC-COD-TOOC IN OPC.
           MOVE OPC-COD-OFOC IN DLC-FLD TO OPC-COD-OFOC IN OPC.
*% INCLUDE FIN_RET_OPC_DLC OF 'COLBMDLC.TXT'
       FIN-RET-OPC-DLC.
           EXIT.
       
       RET-DBC-DLC SECTION.
       INI-RET-DBC-DLC.
*% INCLUDE INI_RET_DBC_DLC OF 'COLBMDLC.TXT'
*% INCLUDE FIN_RET_DBC_DLC OF 'COLBMDLC.TXT'
       FIN-RET-DBC-DLC.
           EXIT.
       
       KEY-ALL-DLC SECTION.
       INI-KEY-ALL-DLC.
*% INCLUDE INI_KEY_ALL_DLC OF 'COLBMDLC.TXT'
           PERFORM KEY-DLC-DLC.
           PERFORM KEY-ICG-DLC.
           PERFORM KEY-RDC-DLC.
           PERFORM KEY-OPC-DLC.
           PERFORM KEY-DBC-DLC.
*% INCLUDE FIN_KEY_ALL_DLC OF 'COLBMDLC.TXT'
       FIN-KEY-ALL-DLC.
           EXIT.
       
       KEY-DLC-DLC SECTION.
       INI-KEY-DLC-DLC.
*% INCLUDE INI_KEY_DLC_DLC OF 'COLBMDLC.TXT'
           MOVE DLC-CAI-IOPC IN DLC-FLD TO DLC-CAI-IOPC IN DLC.
           MOVE DLC-IIC-IOPC IN DLC-FLD TO DLC-IIC-IOPC IN DLC.
           MOVE DLC-NUM-IDLC IN DLC-FLD TO DLC-NUM-IDLC IN DLC.
*% INCLUDE FIN_KEY_DLC_DLC OF 'COLBMDLC.TXT'
       FIN-KEY-DLC-DLC.
           EXIT.
       
       KEY-ICG-DLC SECTION.
       INI-KEY-ICG-DLC.
*% INCLUDE INI_KEY_ICG_DLC OF 'COLBMDLC.TXT'
*% INCLUDE FIN_KEY_ICG_DLC OF 'COLBMDLC.TXT'
       FIN-KEY-ICG-DLC.
           EXIT.
       
       KEY-RDC-DLC SECTION.
       INI-KEY-RDC-DLC.
*% INCLUDE INI_KEY_RDC_DLC OF 'COLBMDLC.TXT'
           MOVE RDC-CAI-ICLI IN DLC-FLD TO RDC-CAI-ICLI IN RDC.
           MOVE RDC-IIC-ICLI IN DLC-FLD TO RDC-IIC-ICLI IN RDC.
*% INCLUDE FIN_KEY_RDC_DLC OF 'COLBMDLC.TXT'
       FIN-KEY-RDC-DLC.
           EXIT.
       
       KEY-OPC-DLC SECTION.
       INI-KEY-OPC-DLC.
*% INCLUDE INI_KEY_OPC_DLC OF 'COLBMDLC.TXT'
*% INCLUDE FIN_KEY_OPC_DLC OF 'COLBMDLC.TXT'
       FIN-KEY-OPC-DLC.
           EXIT.
       
       KEY-DBC-DLC SECTION.
       INI-KEY-DBC-DLC.
*% INCLUDE INI_KEY_DBC_DLC OF 'COLBMDLC.TXT'
           MOVE DBC-GLS-NOMC IN DLC-FLD TO DBC-GLS-NOMC IN DBC.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN DLC-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN DLC-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN DLC-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN DLC-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_KEY_DBC_DLC OF 'COLBMDLC.TXT'
       FIN-KEY-DBC-DLC.
           EXIT.
       
       SCR-CLRA-DLC SECTION.
       INI-SCR-CLRA-DLC.
*% INCLUDE INI_SCR_CLRA_DLC OF 'COLBMDLC.TXT'
           PERFORM CLR-KEY-DLC.
           PERFORM CLR-FLD-DLC.
*% INCLUDE FIN_SCR_CLRA_DLC OF 'COLBMDLC.TXT'
       FIN-SCR-CLRA-DLC.
           EXIT.
       
       CLR-KEY-DLC SECTION.
       INI-CLR-KEY-DLC.
*% INCLUDE INI_CLR_KEY_DLC OF 'COLBMDLC.TXT'
           MOVE SPACES TO RDC-CAI-ICLI IN DLC-FLD.
           MOVE SPACES TO RDC-IIC-ICLI IN DLC-FLD.
           MOVE SPACES TO DLC-CAI-IOPC IN DLC-FLD.
           MOVE ZEROES TO DLC-IIC-IOPC IN DLC-FLD.
           MOVE ZEROES TO DLC-NUM-IDLC IN DLC-FLD.
           MOVE SPACES TO DBC-GLS-NOMC IN DLC-FLD.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN DLC-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN DLC-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN DLC-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN DLC-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_KEY_DLC OF 'COLBMDLC.TXT'
       FIN-CLR-KEY-DLC.
           EXIT.
       
       CLR-FLD-DLC SECTION.
       INI-CLR-FLD-DLC.
*% INCLUDE INI_CLR_FLD_DLC OF 'COLBMDLC.TXT'
           MOVE SPACES TO FRM-IND-VSTR IN DLC-FLD.
           MOVE SPACES TO FRM-CIC-TDOC IN DLC-FLD.
           MOVE SPACES TO FRM-CIC-OPEC IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-TVOC IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-OFOC IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-TINT IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-TDOC IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-TIMP IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-UBFD IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-NOTC IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-PLZA IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-MIPL IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-PLZO IN DLC-FLD.
           MOVE ZEROES TO FRM-NUM-PIMP IN DLC-FLD.
           MOVE SPACES TO FRM-IND-PIMP IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-PIMP IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-PAGO IN DLC-FLD.
           PERFORM CLR-DLC-DLC.
           PERFORM CLR-ICG-DLC.
           PERFORM CLR-RDC-DLC.
           PERFORM CLR-OPC-DLC.
           PERFORM CLR-DBC-DLC.
*% INCLUDE FIN_CLR_FLD_DLC OF 'COLBMDLC.TXT'
       FIN-CLR-FLD-DLC.
           EXIT.
       
       CLR-DLC-DLC SECTION.
       INI-CLR-DLC-DLC.
*% INCLUDE INI_CLR_DLC_DLC OF 'COLBMDLC.TXT'
           MOVE SPACES TO DLC-CAI-IOCO IN DLC-FLD.
           MOVE SPACES TO DLC-IIC-IOCO IN DLC-FLD.
           MOVE ZEROES TO DLC-NUM-IDLO IN DLC-FLD.
           MOVE ZEROES TO DLC-VAL-NDLC IN DLC-FLD.
           MOVE ZEROES TO DLC-VAL-SDLC IN DLC-FLD.
           MOVE SPACES TO DLC-IND-RIES IN DLC-FLD.
           MOVE SPACES TO DLC-COD-TDOC IN DLC-FLD.
           MOVE SPACES TO DLC-COD-UBFD IN DLC-FLD.
           MOVE SPACES TO DLC-MSC-TIMP IN DLC-FLD.
           MOVE ZEROES TO DLC-VAL-IMPU IN DLC-FLD.
           MOVE SPACES TO DLC-COD-PLZA IN DLC-FLD.
           MOVE SPACES TO DLC-COD-NOTC IN DLC-FLD.
           MOVE ZEROES TO DLC-VAL-GNOT IN DLC-FLD.
           MOVE SPACES TO DLC-COD-PLZO IN DLC-FLD.
           MOVE SPACES TO DLC-MSC-MIPL IN DLC-FLD.
           MOVE ZEROES TO DLC-VAL-IPEN IN DLC-FLD.
           MOVE SPACES TO DLC-IND-SCRT IN DLC-FLD.
*% RREG = "DLC"
*% RREG_FRM = "DLC"
*% NOMB = "FDEV"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DLC-FEC-FDEV IN DLC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO DLC-IND-SCTB IN DLC-FLD.
           MOVE ZEROES TO DLC-VAL-READ IN DLC-FLD.
           MOVE SPACES TO DLC-IND-SCBZ IN DLC-FLD.
           MOVE ZEROES TO DLC-VAL-INTD IN DLC-FLD.
           MOVE SPACES TO DLC-COD-PAGO IN DLC-FLD.
           MOVE SPACES TO DLC-IND-ESTA IN DLC-FLD.
           MOVE SPACES TO DLC-IND-ETAP IN DLC-FLD.
           MOVE SPACES TO DLC-IND-HPER IN DLC-FLD.
*% INCLUDE FIN_CLR_DLC_DLC OF 'COLBMDLC.TXT'
       FIN-CLR-DLC-DLC.
           EXIT.
       
       CLR-ICG-DLC SECTION.
       INI-CLR-ICG-DLC.
*% INCLUDE INI_CLR_ICG_DLC OF 'COLBMDLC.TXT'
           MOVE SPACES TO ICG-COD-TINT IN DLC-FLD.
           MOVE ZEROES TO ICG-SGV-TINT IN DLC-FLD.
*% INCLUDE FIN_CLR_ICG_DLC OF 'COLBMDLC.TXT'
       FIN-CLR-ICG-DLC.
           EXIT.
       
       CLR-RDC-DLC SECTION.
       INI-CLR-RDC-DLC.
*% INCLUDE INI_CLR_RDC_DLC OF 'COLBMDLC.TXT'
           MOVE ZEROES TO RDC-NUM-DIRE IN DLC-FLD.
*% INCLUDE FIN_CLR_RDC_DLC OF 'COLBMDLC.TXT'
       FIN-CLR-RDC-DLC.
           EXIT.
       
       CLR-OPC-DLC SECTION.
       INI-CLR-OPC-DLC.
*% INCLUDE INI_CLR_OPC_DLC OF 'COLBMDLC.TXT'
           MOVE SPACES TO OPC-MSC-TVOC IN DLC-FLD.
           MOVE SPACES TO OPC-COD-TOOC IN DLC-FLD.
           MOVE SPACES TO OPC-COD-OFOC IN DLC-FLD.
*% INCLUDE FIN_CLR_OPC_DLC OF 'COLBMDLC.TXT'
       FIN-CLR-OPC-DLC.
           EXIT.
       
       CLR-DBC-DLC SECTION.
       INI-CLR-DBC-DLC.
*% INCLUDE INI_CLR_DBC_DLC OF 'COLBMDLC.TXT'
*% INCLUDE FIN_CLR_DBC_DLC OF 'COLBMDLC.TXT'
       FIN-CLR-DBC-DLC.
           EXIT.
       
       FST-KEY-DLC SECTION.
       INI-FST-KEY-DLC.
*% INCLUDE INI_FST_KEY_DLC OF 'COLBMDLC.TXT'
           MOVE -1 TO DLC-CAI-IOPC-LEN IN DLC-FLD.
*% INCLUDE FIN_FST_KEY_DLC OF 'COLBMDLC.TXT'
       FIN-FST-KEY-DLC.
           EXIT.
       
       FST-FLD-DLC SECTION.
       INI-FST-FLD-DLC.
*% INCLUDE INI_FST_FLD_DLC OF 'COLBMDLC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
           MOVE FRM-POS-CURS TO DBC-NUM-ICLI-LEN IN DLC-FLD.
      * VRF-FIN --> BMCHIBCI004.SRCAIU ( FST-KEY Y FST-FLD )
*% INCLUDE FIN_FST_FLD_DLC OF 'COLBMDLC.TXT'
       FIN-FST-FLD-DLC.
           EXIT.
       
       PRO-KEY-DLC SECTION.
       INI-PRO-KEY-DLC.
*% INCLUDE INI_PRO_KEY_DLC OF 'COLBMDLC.TXT'
           PERFORM PRO-MKY-DLC.
*% INCLUDE FIN_PRO_KEY_DLC OF 'COLBMDLC.TXT'
       FIN-PRO-KEY-DLC.
           EXIT.
       
       PRO-MKY-DLC SECTION.
       INI-PRO-MKY-DLC.
*% INCLUDE INI_PRO_MKY_DLC OF 'COLBMDLC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO DBC-NUM-ICLI-ATR IN DLC-FLD.
           MOVE FRM-CPIM-AEY TO DBC-VRF-ICLI-ATR IN DLC-FLD.
           MOVE FRM-CPIM-AEY TO DBC-IND-ICLI-ATR IN DLC-FLD.
           MOVE FRM-CPIM-AEY TO DBC-GLS-ICLI-ATR IN DLC-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
*% INCLUDE FIN_PRO_MKY_DLC OF 'COLBMDLC.TXT'
       FIN-PRO-MKY-DLC.
           EXIT.
       
       PRO-FLD-DLC SECTION.
       INI-PRO-FLD-DLC.
*% INCLUDE INI_PRO_FLD_DLC OF 'COLBMDLC.TXT'
           MOVE FRM-CPIM-AEY TO FRM-NUM-PIMP-ATR IN DLC-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-PAGO-ATR IN DLC-FLD.
           PERFORM PRO-DLC-DLC.
           PERFORM PRO-ICG-DLC.
           PERFORM PRO-RDC-DLC.
           PERFORM PRO-OPC-DLC.
           PERFORM PRO-DBC-DLC.
*% INCLUDE FIN_PRO_FLD_DLC OF 'COLBMDLC.TXT'
       FIN-PRO-FLD-DLC.
           EXIT.
       
       PRO-DLC-DLC SECTION.
       INI-PRO-DLC-DLC.
*% INCLUDE INI_PRO_DLC_DLC OF 'COLBMDLC.TXT'
           MOVE FRM-CPIM-AEY TO DLC-VAL-NDLC-ATR IN DLC-FLD.
           MOVE FRM-CPIM-AEY TO DLC-IND-RIES-ATR IN DLC-FLD.
           MOVE FRM-CPIM-AEY TO DLC-COD-TDOC-ATR IN DLC-FLD.
           MOVE FRM-CPIM-AEY TO DLC-COD-UBFD-ATR IN DLC-FLD.
           MOVE FRM-CPIM-AEY TO DLC-MSC-TIMP-ATR IN DLC-FLD.
           MOVE FRM-CPIM-AEY TO DLC-VAL-IMPU-ATR IN DLC-FLD.
           MOVE FRM-CPIM-AEY TO DLC-COD-PLZA-ATR IN DLC-FLD.
           MOVE FRM-CPIM-AEY TO DLC-COD-NOTC-ATR IN DLC-FLD.
           MOVE FRM-CPIM-AEY TO DLC-VAL-GNOT-ATR IN DLC-FLD.
           MOVE FRM-CPIM-AEY TO DLC-MSC-MIPL-ATR IN DLC-FLD.
           MOVE FRM-CPIM-AEY TO DLC-IND-SCRT-ATR IN DLC-FLD.
*% RREG = "DLC"
*% RREG_FRM = "DLC"
*% NOMB = "FDEV"
           MOVE FRM-CPIM-AEY TO DLC-FEC-FDEV-ATR IN DLC-FLD.
           MOVE FRM-CPIM-AEY TO DLC-IND-SCTB-ATR IN DLC-FLD.
           MOVE FRM-CPIM-AEY TO DLC-IND-SCBZ-ATR IN DLC-FLD.
           MOVE FRM-CPIM-AEY TO DLC-COD-PAGO-ATR IN DLC-FLD.
           MOVE FRM-CPIM-AEY TO DLC-IND-ESTA-ATR IN DLC-FLD.
           MOVE FRM-CPIM-AEY TO DLC-IND-ETAP-ATR IN DLC-FLD.
           MOVE FRM-CPIM-AEY TO DLC-IND-HPER-ATR IN DLC-FLD.
*% INCLUDE FIN_PRO_DLC_DLC OF 'COLBMDLC.TXT'
       FIN-PRO-DLC-DLC.
           EXIT.
       
       PRO-ICG-DLC SECTION.
       INI-PRO-ICG-DLC.
*% INCLUDE INI_PRO_ICG_DLC OF 'COLBMDLC.TXT'
           MOVE FRM-CPIM-AEY TO ICG-SGV-TINT-ATR IN DLC-FLD.
*% INCLUDE FIN_PRO_ICG_DLC OF 'COLBMDLC.TXT'
       FIN-PRO-ICG-DLC.
           EXIT.
       
       PRO-RDC-DLC SECTION.
       INI-PRO-RDC-DLC.
*% INCLUDE INI_PRO_RDC_DLC OF 'COLBMDLC.TXT'
           MOVE FRM-CPIM-AEY TO RDC-NUM-DIRE-ATR IN DLC-FLD.
*% INCLUDE FIN_PRO_RDC_DLC OF 'COLBMDLC.TXT'
       FIN-PRO-RDC-DLC.
           EXIT.
       
       PRO-OPC-DLC SECTION.
       INI-PRO-OPC-DLC.
*% INCLUDE INI_PRO_OPC_DLC OF 'COLBMDLC.TXT'
*% INCLUDE FIN_PRO_OPC_DLC OF 'COLBMDLC.TXT'
       FIN-PRO-OPC-DLC.
           EXIT.
       
       PRO-DBC-DLC SECTION.
       INI-PRO-DBC-DLC.
*% INCLUDE INI_PRO_DBC_DLC OF 'COLBMDLC.TXT'
*% INCLUDE FIN_PRO_DBC_DLC OF 'COLBMDLC.TXT'
       FIN-PRO-DBC-DLC.
           EXIT.
       
       UNP-KEY-DLC SECTION.
       INI-UNP-KEY-DLC.
*% INCLUDE INI_UNP_KEY_DLC OF 'COLBMDLC.TXT'
           PERFORM UNP-IKY-DLC.
           PERFORM UNP-AKY-DLC.
           PERFORM UNP-MKY-DLC.
*% INCLUDE FIN_UNP_KEY_DLC OF 'COLBMDLC.TXT'
       FIN-UNP-KEY-DLC.
           EXIT.
       
       UNP-IKY-DLC SECTION.
       INI-UNP-IKY-DLC.
*% INCLUDE INI_UNP_IKY_DLC OF 'COLBMDLC.TXT'
           MOVE FRM-CPIM-UBY TO DLC-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO DLC-IIC-IOPC-ATR.
*% INCLUDE FIN_UNP_IKY_DLC OF 'COLBMDLC.TXT'
       FIN-UNP-IKY-DLC.
           EXIT.
       
       UNP-MKY-DLC SECTION.
       INI-UNP-MKY-DLC.
*% INCLUDE INI_UNP_MKY_DLC OF 'COLBMDLC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO DBC-NUM-ICLI-ATR IN DLC-FLD.
           MOVE FRM-CPIM-UBY TO DBC-VRF-ICLI-ATR IN DLC-FLD.
           MOVE FRM-CPIM-UBY TO DBC-IND-ICLI-ATR IN DLC-FLD.
           MOVE FRM-CPIM-UBY TO DBC-GLS-ICLI-ATR IN DLC-FLD.
      * IDC-FIN --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
*% INCLUDE FIN_UNP_MKY_DLC OF 'COLBMDLC.TXT'
       FIN-UNP-MKY-DLC.
           EXIT.
       
       UNP-AKY-DLC SECTION.
       INI-UNP-AKY-DLC.
*% INCLUDE INI_UNP_AKY_DLC OF 'COLBMDLC.TXT'
           MOVE FRM-CPIM-UBY TO DLC-NUM-IDLC-ATR.
           MOVE FRM-CPIM-UBY TO DBC-GLS-NOMC-ATR.
*% INCLUDE FIN_UNP_AKY_DLC OF 'COLBMDLC.TXT'
       FIN-UNP-AKY-DLC.
           EXIT.
       
       UNP-FLD-DLC SECTION.
       INI-UNP-FLD-DLC.
*% INCLUDE INI_UNP_FLD_DLC OF 'COLBMDLC.TXT'
           MOVE FRM-CPIM-UBY TO FRM-NUM-PIMP-ATR IN DLC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-PAGO-ATR IN DLC-FLD.
           PERFORM UNP-DLC-DLC.
           PERFORM UNP-ICG-DLC.
           PERFORM UNP-RDC-DLC.
           PERFORM UNP-OPC-DLC.
           PERFORM UNP-DBC-DLC.
*% INCLUDE FIN_UNP_FLD_DLC OF 'COLBMDLC.TXT'
       FIN-UNP-FLD-DLC.
           EXIT.
       
       UNP-DLC-DLC SECTION.
       INI-UNP-DLC-DLC.
*% INCLUDE INI_UNP_DLC_DLC OF 'COLBMDLC.TXT'
           MOVE FRM-CPIM-UBY TO DLC-VAL-NDLC-ATR IN DLC-FLD.
           MOVE FRM-CPIM-UBY TO DLC-IND-RIES-ATR IN DLC-FLD.
           MOVE FRM-CPIM-UBY TO DLC-COD-TDOC-ATR IN DLC-FLD.
           MOVE FRM-CPIM-UBY TO DLC-COD-UBFD-ATR IN DLC-FLD.
           MOVE FRM-CPIM-UBY TO DLC-MSC-TIMP-ATR IN DLC-FLD.
           MOVE FRM-CPIM-UBY TO DLC-VAL-IMPU-ATR IN DLC-FLD.
           MOVE FRM-CPIM-UBY TO DLC-COD-PLZA-ATR IN DLC-FLD.
           MOVE FRM-CPIM-UBY TO DLC-COD-NOTC-ATR IN DLC-FLD.
           MOVE FRM-CPIM-UBY TO DLC-VAL-GNOT-ATR IN DLC-FLD.
           MOVE FRM-CPIM-UBY TO DLC-MSC-MIPL-ATR IN DLC-FLD.
           MOVE FRM-CPIM-UBY TO DLC-IND-SCRT-ATR IN DLC-FLD.
*% RREG = "DLC"
*% RREG_FRM = "DLC"
*% NOMB = "FDEV"
           MOVE FRM-CPIM-UBY TO DLC-FEC-FDEV-ATR IN DLC-FLD.
           MOVE FRM-CPIM-UBY TO DLC-IND-SCTB-ATR IN DLC-FLD.
           MOVE FRM-CPIM-UBY TO DLC-IND-SCBZ-ATR IN DLC-FLD.
           MOVE FRM-CPIM-UBY TO DLC-COD-PAGO-ATR IN DLC-FLD.
           MOVE FRM-CPIM-UBY TO DLC-IND-ESTA-ATR IN DLC-FLD.
           MOVE FRM-CPIM-UBY TO DLC-IND-ETAP-ATR IN DLC-FLD.
           MOVE FRM-CPIM-UBY TO DLC-IND-HPER-ATR IN DLC-FLD.
*% INCLUDE FIN_UNP_DLC_DLC OF 'COLBMDLC.TXT'
       FIN-UNP-DLC-DLC.
           EXIT.
       
