*% FRM = "INT"
      *
      * COLBUINT
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:04 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMINT.TXT'
       SET-TAG-COLBUINT SECTION.
       INI-SET-TAG-COLBUINT.
           MOVE
           '<IDNMOD NOM=$COLBUINT$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:04 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUINT.
           EXIT.
           
       RET-ALL-INT SECTION.
       INI-RET-ALL-INT.
*% INCLUDE INI_RET_ALL_INT OF 'COLBMINT.TXT'
           PERFORM KEY-ALL-INT.
           PERFORM RET-INT-INT.
*% INCLUDE FIN_RET_ALL_INT OF 'COLBMINT.TXT'
       FIN-RET-ALL-INT.
           EXIT.
       
       RET-INT-INT SECTION.
       INI-RET-INT-INT.
*% INCLUDE INI_RET_INT_INT OF 'COLBMINT.TXT'
           MOVE INT-CIC-TINT IN INT-FLD TO INT-CIC-TINT IN INT.
           MOVE INT-EXT-CTAB IN INT-FLD TO INT-EXT-CTAB IN INT.
           MOVE INT-GLS-DESC IN INT-FLD TO INT-GLS-DESC IN INT.
           MOVE INT-GLS-ABRV IN INT-FLD TO INT-GLS-ABRV IN INT.
           MOVE INT-GLS-DCOR IN INT-FLD TO INT-GLS-DCOR IN INT.
           MOVE INT-IND-VIGE IN INT-FLD TO INT-IND-VIGE IN INT.
*% RREG = "INT"
*% RREG_FRM = "INT"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE INT-NUM-DTRN IN INT-FLD TO INT-NUM-DTRN IN INT.
           MOVE INT-NUM-MTRN IN INT-FLD TO INT-NUM-MTRN IN INT.
           MOVE INT-NUM-STRN IN INT-FLD TO INT-NUM-STRN IN INT.
           MOVE INT-NUM-ATRN IN INT-FLD TO INT-NUM-ATRN IN INT.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_RET_INT_INT OF 'COLBMINT.TXT'
       FIN-RET-INT-INT.
           EXIT.
       
       KEY-ALL-INT SECTION.
       INI-KEY-ALL-INT.
*% INCLUDE INI_KEY_ALL_INT OF 'COLBMINT.TXT'
           PERFORM KEY-INT-INT.
*% INCLUDE FIN_KEY_ALL_INT OF 'COLBMINT.TXT'
       FIN-KEY-ALL-INT.
           EXIT.
       
       KEY-INT-INT SECTION.
       INI-KEY-INT-INT.
*% INCLUDE INI_KEY_INT_INT OF 'COLBMINT.TXT'
           MOVE INT-MSC-BCTI IN INT-FLD TO INT-MSC-BCTI IN INT.
           MOVE INT-MSC-PBTI IN INT-FLD TO INT-MSC-PBTI IN INT.
           MOVE INT-MSC-BPBT IN INT-FLD TO INT-MSC-BPBT IN INT.
           MOVE INT-MSC-SPBT IN INT-FLD TO INT-MSC-SPBT IN INT.
           MOVE INT-COD-CVTI IN INT-FLD TO INT-COD-CVTI IN INT.
           MOVE INT-MSC-TFPR IN INT-FLD TO INT-MSC-TFPR IN INT.
           MOVE INT-NUM-PREP IN INT-FLD TO INT-NUM-PREP IN INT.
           MOVE INT-IND-UTPR IN INT-FLD TO INT-IND-UTPR IN INT.
*% INCLUDE FIN_KEY_INT_INT OF 'COLBMINT.TXT'
       FIN-KEY-INT-INT.
           EXIT.
       
       SCR-CLRA-INT SECTION.
       INI-SCR-CLRA-INT.
*% INCLUDE INI_SCR_CLRA_INT OF 'COLBMINT.TXT'
           PERFORM CLR-KEY-INT.
           PERFORM CLR-FLD-INT.
*% INCLUDE FIN_SCR_CLRA_INT OF 'COLBMINT.TXT'
       FIN-SCR-CLRA-INT.
           EXIT.
       
       CLR-KEY-INT SECTION.
       INI-CLR-KEY-INT.
*% INCLUDE INI_CLR_KEY_INT OF 'COLBMINT.TXT'
           MOVE SPACES TO INT-MSC-BCTI IN INT-FLD.
           MOVE SPACES TO INT-MSC-PBTI IN INT-FLD.
           MOVE SPACES TO INT-MSC-BPBT IN INT-FLD.
           MOVE SPACES TO INT-MSC-SPBT IN INT-FLD.
           MOVE SPACES TO INT-COD-CVTI IN INT-FLD.
           MOVE SPACES TO INT-MSC-TFPR IN INT-FLD.
           MOVE ZEROES TO INT-NUM-PREP IN INT-FLD.
           MOVE SPACES TO INT-IND-UTPR IN INT-FLD.
*% INCLUDE FIN_CLR_KEY_INT OF 'COLBMINT.TXT'
       FIN-CLR-KEY-INT.
           EXIT.
       
       CLR-FLD-INT SECTION.
       INI-CLR-FLD-INT.
*% INCLUDE INI_CLR_FLD_INT OF 'COLBMINT.TXT'
           MOVE SPACES TO FRM-GLS-BCTI IN INT-FLD.
           MOVE SPACES TO FRM-GLS-PBTI IN INT-FLD.
           MOVE SPACES TO FRM-GLS-BPBT IN INT-FLD.
           MOVE SPACES TO FRM-GLS-SPBT IN INT-FLD.
           MOVE SPACES TO FRM-GLS-CVTI IN INT-FLD.
           MOVE SPACES TO FRM-GLS-TFPR IN INT-FLD.
           MOVE SPACES TO FRM-GLS-UTPR IN INT-FLD.
           PERFORM CLR-INT-INT.
*% INCLUDE FIN_CLR_FLD_INT OF 'COLBMINT.TXT'
       FIN-CLR-FLD-INT.
           EXIT.
       
       CLR-INT-INT SECTION.
       INI-CLR-INT-INT.
*% INCLUDE INI_CLR_INT_INT OF 'COLBMINT.TXT'
           MOVE SPACES TO INT-CIC-TINT IN INT-FLD.
           MOVE SPACES TO INT-EXT-CTAB IN INT-FLD.
           MOVE SPACES TO INT-GLS-DESC IN INT-FLD.
           MOVE SPACES TO INT-GLS-ABRV IN INT-FLD.
           MOVE SPACES TO INT-GLS-DCOR IN INT-FLD.
           MOVE SPACES TO INT-IND-VIGE IN INT-FLD.
*% RREG = "INT"
*% RREG_FRM = "INT"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO INT-FEC-FTRN IN INT-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_INT_INT OF 'COLBMINT.TXT'
       FIN-CLR-INT-INT.
           EXIT.
       
       FST-KEY-INT SECTION.
       INI-FST-KEY-INT.
*% INCLUDE INI_FST_KEY_INT OF 'COLBMINT.TXT'
           MOVE -1 TO INT-MSC-BCTI-LEN IN INT-FLD.
*% INCLUDE FIN_FST_KEY_INT OF 'COLBMINT.TXT'
       FIN-FST-KEY-INT.
           EXIT.
       
       FST-FLD-INT SECTION.
       INI-FST-FLD-INT.
*% INCLUDE INI_FST_FLD_INT OF 'COLBMINT.TXT'
           MOVE -1 TO INT-CIC-TINT-LEN IN INT-FLD.
*% INCLUDE FIN_FST_FLD_INT OF 'COLBMINT.TXT'
       FIN-FST-FLD-INT.
           EXIT.
       
       PRO-KEY-INT SECTION.
       INI-PRO-KEY-INT.
*% INCLUDE INI_PRO_KEY_INT OF 'COLBMINT.TXT'
           PERFORM PRO-MKY-INT.
*% INCLUDE FIN_PRO_KEY_INT OF 'COLBMINT.TXT'
       FIN-PRO-KEY-INT.
           EXIT.
       
       PRO-MKY-INT SECTION.
       INI-PRO-MKY-INT.
*% INCLUDE INI_PRO_MKY_INT OF 'COLBMINT.TXT'
*% INCLUDE FIN_PRO_MKY_INT OF 'COLBMINT.TXT'
       FIN-PRO-MKY-INT.
           EXIT.
       
       PRO-FLD-INT SECTION.
       INI-PRO-FLD-INT.
*% INCLUDE INI_PRO_FLD_INT OF 'COLBMINT.TXT'
           PERFORM PRO-INT-INT.
*% INCLUDE FIN_PRO_FLD_INT OF 'COLBMINT.TXT'
       FIN-PRO-FLD-INT.
           EXIT.
       
       PRO-INT-INT SECTION.
       INI-PRO-INT-INT.
*% INCLUDE INI_PRO_INT_INT OF 'COLBMINT.TXT'
           MOVE FRM-CPIM-AEY TO INT-CIC-TINT-ATR IN INT-FLD.
           MOVE FRM-CPIM-AEY TO INT-EXT-CTAB-ATR IN INT-FLD.
           MOVE FRM-CPIM-AEY TO INT-GLS-DESC-ATR IN INT-FLD.
           MOVE FRM-CPIM-AEY TO INT-GLS-ABRV-ATR IN INT-FLD.
           MOVE FRM-CPIM-AEY TO INT-GLS-DCOR-ATR IN INT-FLD.
           MOVE FRM-CPIM-AEY TO INT-IND-VIGE-ATR IN INT-FLD.
*% INCLUDE FIN_PRO_INT_INT OF 'COLBMINT.TXT'
       FIN-PRO-INT-INT.
           EXIT.
       
       UNP-KEY-INT SECTION.
       INI-UNP-KEY-INT.
*% INCLUDE INI_UNP_KEY_INT OF 'COLBMINT.TXT'
           PERFORM UNP-IKY-INT.
           PERFORM UNP-AKY-INT.
           PERFORM UNP-MKY-INT.
*% INCLUDE FIN_UNP_KEY_INT OF 'COLBMINT.TXT'
       FIN-UNP-KEY-INT.
           EXIT.
       
       UNP-IKY-INT SECTION.
       INI-UNP-IKY-INT.
*% INCLUDE INI_UNP_IKY_INT OF 'COLBMINT.TXT'
           MOVE FRM-CPIM-UBY TO INT-MSC-BCTI-ATR.
           MOVE FRM-CPIM-UBY TO INT-MSC-PBTI-ATR.
           MOVE FRM-CPIM-UBY TO INT-MSC-BPBT-ATR.
           MOVE FRM-CPIM-UBY TO INT-MSC-SPBT-ATR.
           MOVE FRM-CPIM-UBY TO INT-COD-CVTI-ATR.
           MOVE FRM-CPIM-UBY TO INT-MSC-TFPR-ATR.
           MOVE FRM-CPIM-UBY TO INT-NUM-PREP-ATR.
           MOVE FRM-CPIM-UBY TO INT-IND-UTPR-ATR.
*% INCLUDE FIN_UNP_IKY_INT OF 'COLBMINT.TXT'
       FIN-UNP-IKY-INT.
           EXIT.
       
       UNP-MKY-INT SECTION.
       INI-UNP-MKY-INT.
*% INCLUDE INI_UNP_MKY_INT OF 'COLBMINT.TXT'
*% INCLUDE FIN_UNP_MKY_INT OF 'COLBMINT.TXT'
       FIN-UNP-MKY-INT.
           EXIT.
       
       UNP-AKY-INT SECTION.
       INI-UNP-AKY-INT.
*% INCLUDE INI_UNP_AKY_INT OF 'COLBMINT.TXT'
*% INCLUDE FIN_UNP_AKY_INT OF 'COLBMINT.TXT'
       FIN-UNP-AKY-INT.
           EXIT.
       
       UNP-FLD-INT SECTION.
       INI-UNP-FLD-INT.
*% INCLUDE INI_UNP_FLD_INT OF 'COLBMINT.TXT'
           PERFORM UNP-INT-INT.
*% INCLUDE FIN_UNP_FLD_INT OF 'COLBMINT.TXT'
       FIN-UNP-FLD-INT.
           EXIT.
       
       UNP-INT-INT SECTION.
       INI-UNP-INT-INT.
*% INCLUDE INI_UNP_INT_INT OF 'COLBMINT.TXT'
           MOVE FRM-CPIM-UBY TO INT-CIC-TINT-ATR IN INT-FLD.
           MOVE FRM-CPIM-UBY TO INT-EXT-CTAB-ATR IN INT-FLD.
           MOVE FRM-CPIM-UBY TO INT-GLS-DESC-ATR IN INT-FLD.
           MOVE FRM-CPIM-UBY TO INT-GLS-ABRV-ATR IN INT-FLD.
           MOVE FRM-CPIM-UBY TO INT-GLS-DCOR-ATR IN INT-FLD.
           MOVE FRM-CPIM-UBY TO INT-IND-VIGE-ATR IN INT-FLD.
*% INCLUDE FIN_UNP_INT_INT OF 'COLBMINT.TXT'
       FIN-UNP-INT-INT.
           EXIT.
       
       VAL-MKY-INT SECTION.
       INI-VAL-MKY-INT.
*% INCLUDE INI_VAL_MKY_INT OF 'COLBMINT.TXT'
*% INCLUDE FIN_VAL_MKY_INT OF 'COLBMINT.TXT'
       FIN-VAL-MKY-INT.
           EXIT.
       
       VAL-FLD-INT SECTION.
       INI-VAL-FLD-INT.
*% INCLUDE INI_VAL_FLD_INT OF 'COLBMINT.TXT'
           PERFORM VAL-FRM-GLS-BCTI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-BCTI-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
           PERFORM VAL-FRM-GLS-PBTI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-PBTI-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
           PERFORM VAL-FRM-GLS-BPBT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-BPBT-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
           PERFORM VAL-FRM-GLS-SPBT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-SPBT-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
           PERFORM VAL-FRM-GLS-CVTI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-CVTI-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
           PERFORM VAL-FRM-GLS-TFPR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TFPR-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
           PERFORM VAL-FRM-GLS-UTPR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-UTPR-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
           PERFORM VAL-INT-CIC-TINT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO INT-CIC-TINT-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
           PERFORM VAL-INT-EXT-CTAB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO INT-EXT-CTAB-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
           PERFORM VAL-INT-GLS-DESC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO INT-GLS-DESC-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
           PERFORM VAL-INT-GLS-ABRV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO INT-GLS-ABRV-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
           PERFORM VAL-INT-GLS-DCOR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO INT-GLS-DCOR-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
           PERFORM VAL-INT-IND-VIGE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO INT-IND-VIGE-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
*% RREG = "INT"
*% RREG_FRM = "INT"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-INT-FEC-FTRN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                INT-FEC-FTRN-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% INCLUDE FIN_VAL_FLD_INT OF 'COLBMINT.TXT'
       FIN-VAL-FLD-INT.
           EXIT.
       
       VAL-FRM-GLS-BCTI SECTION.
       INI-VAL-FRM-GLS-BCTI.
*% LBL = "FRM_GLS_BCTI OF 'COLBMINT.TXT'"
*% INCLUDE INI_FRM_GLS_BCTI OF 'COLBMINT.TXT'
*% INCLUDE FRM_GLS_BCTI OF 'COLBMINT.TXT'
*% INCLUDE FIN_FRM_GLS_BCTI OF 'COLBMINT.TXT'
       FIN-VAL-FRM-GLS-BCTI.
           EXIT.
       
       VAL-FRM-GLS-PBTI SECTION.
       INI-VAL-FRM-GLS-PBTI.
*% LBL = "FRM_GLS_PBTI OF 'COLBMINT.TXT'"
*% INCLUDE INI_FRM_GLS_PBTI OF 'COLBMINT.TXT'
*% INCLUDE FRM_GLS_PBTI OF 'COLBMINT.TXT'
*% INCLUDE FIN_FRM_GLS_PBTI OF 'COLBMINT.TXT'
       FIN-VAL-FRM-GLS-PBTI.
           EXIT.
       
       VAL-FRM-GLS-BPBT SECTION.
       INI-VAL-FRM-GLS-BPBT.
*% LBL = "FRM_GLS_BPBT OF 'COLBMINT.TXT'"
*% INCLUDE INI_FRM_GLS_BPBT OF 'COLBMINT.TXT'
*% INCLUDE FRM_GLS_BPBT OF 'COLBMINT.TXT'
*% INCLUDE FIN_FRM_GLS_BPBT OF 'COLBMINT.TXT'
       FIN-VAL-FRM-GLS-BPBT.
           EXIT.
       
       VAL-FRM-GLS-SPBT SECTION.
       INI-VAL-FRM-GLS-SPBT.
*% LBL = "FRM_GLS_SPBT OF 'COLBMINT.TXT'"
*% INCLUDE INI_FRM_GLS_SPBT OF 'COLBMINT.TXT'
*% INCLUDE FRM_GLS_SPBT OF 'COLBMINT.TXT'
*% INCLUDE FIN_FRM_GLS_SPBT OF 'COLBMINT.TXT'
       FIN-VAL-FRM-GLS-SPBT.
           EXIT.
       
       VAL-FRM-GLS-CVTI SECTION.
       INI-VAL-FRM-GLS-CVTI.
*% LBL = "FRM_GLS_CVTI OF 'COLBMINT.TXT'"
*% INCLUDE INI_FRM_GLS_CVTI OF 'COLBMINT.TXT'
*% INCLUDE FRM_GLS_CVTI OF 'COLBMINT.TXT'
*% INCLUDE FIN_FRM_GLS_CVTI OF 'COLBMINT.TXT'
       FIN-VAL-FRM-GLS-CVTI.
           EXIT.
       
       VAL-FRM-GLS-TFPR SECTION.
       INI-VAL-FRM-GLS-TFPR.
*% LBL = "FRM_GLS_TFPR OF 'COLBMINT.TXT'"
*% INCLUDE INI_FRM_GLS_TFPR OF 'COLBMINT.TXT'
*% INCLUDE FRM_GLS_TFPR OF 'COLBMINT.TXT'
*% INCLUDE FIN_FRM_GLS_TFPR OF 'COLBMINT.TXT'
       FIN-VAL-FRM-GLS-TFPR.
           EXIT.
       
