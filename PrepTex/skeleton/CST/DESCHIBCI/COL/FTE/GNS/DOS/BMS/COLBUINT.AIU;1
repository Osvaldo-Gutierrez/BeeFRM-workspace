       INI-VAL-FRM-GLS-UTPR.
*% LBL = "FRM_GLS_UTPR OF 'COLBMINT.TXT'"
*% INCLUDE INI_FRM_GLS_UTPR OF 'COLBMINT.TXT'
*% INCLUDE FRM_GLS_UTPR OF 'COLBMINT.TXT'
*% INCLUDE FIN_FRM_GLS_UTPR OF 'COLBMINT.TXT'
       FIN-VAL-FRM-GLS-UTPR.
           EXIT.
       
       VAL-INT-CIC-TINT SECTION.
       INI-VAL-INT-CIC-TINT.
*% LBL = "INT_CIC_TINT OF 'COLBMINT.TXT'"
*% INCLUDE INI_INT_CIC_TINT OF 'COLBMINT.TXT'
           IF INT-CIC-TINT IN INT-FLD NOT > SPACES
              MOVE SPACES TO INT-CIC-TINT IN INT-FLD.
*% INCLUDE INT_CIC_TINT OF 'COLBMINT.TXT'
*% INCLUDE FIN_INT_CIC_TINT OF 'COLBMINT.TXT'
       FIN-VAL-INT-CIC-TINT.
           EXIT.
       
       VAL-INT-EXT-CTAB SECTION.
       INI-VAL-INT-EXT-CTAB.
*% LBL = "INT_EXT_CTAB OF 'COLBMINT.TXT'"
*% INCLUDE INI_INT_EXT_CTAB OF 'COLBMINT.TXT'
           IF INT-EXT-CTAB IN INT-FLD NOT > SPACES
              MOVE SPACES TO INT-EXT-CTAB IN INT-FLD.
*% INCLUDE INT_EXT_CTAB OF 'COLBMINT.TXT'
*% INCLUDE FIN_INT_EXT_CTAB OF 'COLBMINT.TXT'
       FIN-VAL-INT-EXT-CTAB.
           EXIT.
       
       VAL-INT-GLS-DESC SECTION.
       INI-VAL-INT-GLS-DESC.
*% LBL = "INT_GLS_DESC OF 'COLBMINT.TXT'"
*% INCLUDE INI_INT_GLS_DESC OF 'COLBMINT.TXT'
           IF INT-GLS-DESC IN INT-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-INT-GLS-DESC.
*% INCLUDE INT_GLS_DESC OF 'COLBMINT.TXT'
*% INCLUDE FIN_INT_GLS_DESC OF 'COLBMINT.TXT'
       FIN-VAL-INT-GLS-DESC.
           EXIT.
       
       VAL-INT-GLS-ABRV SECTION.
       INI-VAL-INT-GLS-ABRV.
*% LBL = "INT_GLS_ABRV OF 'COLBMINT.TXT'"
*% INCLUDE INI_INT_GLS_ABRV OF 'COLBMINT.TXT'
           IF INT-GLS-ABRV IN INT-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-INT-GLS-ABRV.
*% INCLUDE INT_GLS_ABRV OF 'COLBMINT.TXT'
*% INCLUDE FIN_INT_GLS_ABRV OF 'COLBMINT.TXT'
       FIN-VAL-INT-GLS-ABRV.
           EXIT.
       
       VAL-INT-GLS-DCOR SECTION.
       INI-VAL-INT-GLS-DCOR.
*% LBL = "INT_GLS_DCOR OF 'COLBMINT.TXT'"
*% INCLUDE INI_INT_GLS_DCOR OF 'COLBMINT.TXT'
           IF INT-GLS-DCOR IN INT-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-INT-GLS-DCOR.
*% INCLUDE INT_GLS_DCOR OF 'COLBMINT.TXT'
*% INCLUDE FIN_INT_GLS_DCOR OF 'COLBMINT.TXT'
       FIN-VAL-INT-GLS-DCOR.
           EXIT.
       
       VAL-INT-IND-VIGE SECTION.
       INI-VAL-INT-IND-VIGE.
*% LBL = "INT_IND_VIGE OF 'COLBMINT.TXT'"
*% INCLUDE INI_INT_IND_VIGE OF 'COLBMINT.TXT'
           IF INT-IND-VIGE IN INT-FLD NOT > SPACES
              MOVE SPACES TO INT-IND-VIGE IN INT-FLD.
*% INCLUDE INT_IND_VIGE OF 'COLBMINT.TXT'
*% BUF = "INT-FLD"
*% SIS = "GNS"
*% TAB = "VIGE"
*% FLD = "INT-IND-VIGE"
*% GLS = ""
*% TIENE_GLOSA = FALSE
*% LGL = 0
*% INCLUDE 'VALIND'
*% INCLUDE FIN_INT_IND_VIGE OF 'COLBMINT.TXT'
       FIN-VAL-INT-IND-VIGE.
           EXIT.
*% RREG = "INT"
*% RREG_FRM = "INT"
*% NOMB = "FTRN"
*% OBL = "S"
*% LBL = "INT_FEC_FTRN OF 'COLBMINT.TXT'"
*% IZR = "S"
*% ZRO = "S"
*% FRM_IFLD = ""
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-INT-FEC-FTRN SECTION.
       INI-VAL-INT-FEC-FTRN.
           IF INT-FEC-FTRN IN INT-FLD  NOT > ZEROES
              MOVE ZEROES TO INT-FEC-FTRN IN INT-FLD.
*% INCLUDE INI_INT_FEC_FTRN OF 'COLBMINT.TXT'
*% INCLUDE INT_FEC_FTRN OF 'COLBMINT.TXT'
*% INCLUDE FIN_INT_FEC_FTRN OF 'COLBMINT.TXT'
       FIN-VAL-INT-FEC-FTRN.
           EXIT.
      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-CON-FLD-INT SECTION.
       INI-VAL-CON-FLD-INT.
*% INCLUDE VAL_CON_FLD_INT OF 'COLBMINT.TXT'
       FIN-VAL-CON-FLD-INT.
           EXIT.
       
       VAL-CON-INT SECTION.
       INI-VAL-CON-INT.
*% INCLUDE VAL_CON_INT OF 'COLBMINT.TXT'
       FIN-VAL-CON-INT.
           EXIT.
       
       VAL-NUM-KEY-INT SECTION.
       INI-VAL-NUM-KEY-INT.
           PERFORM VAL-NUM-IKY-INT.
           PERFORM VAL-NUM-MKY-INT.
           PERFORM VAL-NUM-AKY-INT.
       FIN-VAL-NUM-KEY-INT.
           EXIT.
       
       VAL-NUM-IKY-INT SECTION.
       INI-VAL-NUM-IKY-INT.
*% INCLUDE INI_VAL_NUM_IKY_INT OF 'COLBMINT.TXT'
       VAL-NUM-INT-NUM-PREP.
*% BUF = "INT-FLD"
*% FLD = "INT-NUM-PREP"
*% ENT = "3"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-INT.
*% INCLUDE FIN_VAL_NUM_IKY_INT OF 'COLBMINT.TXT'
       FIN-VAL-NUM-IKY-INT.
           EXIT.
       
       VAL-NUM-MKY-INT SECTION.
       INI-VAL-NUM-MKY-INT.
*% INCLUDE INI_VAL_NUM_MKY_INT OF 'COLBMINT.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_INT OF 'COLBMINT.TXT'
       FIN-VAL-NUM-MKY-INT.
           EXIT.
       
       VAL-NUM-AKY-INT SECTION.
       INI-VAL-NUM-AKY-INT.
*% INCLUDE INI_VAL_NUM_AKY_INT OF 'COLBMINT.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_INT OF 'COLBMINT.TXT'
       FIN-VAL-NUM-AKY-INT.
           EXIT.
       
       VAL-NUM-FLD-INT SECTION.
       INI-VAL-NUM-FLD-INT.
*% INCLUDE INI_VAL_NUM_FLD_INT OF 'COLBMINT.TXT'
*% INCLUDE FIN_VAL_NUM_FLD_INT OF 'COLBMINT.TXT'
       FIN-VAL-NUM-FLD-INT.
           EXIT.
       
       EDT-KEY-INT SECTION.
       INI-EDT-KEY-INT.
           PERFORM EDT-IKY-INT.
           PERFORM EDT-MKY-INT.
           PERFORM EDT-AKY-INT.
*% INCLUDE EDT_KEY_INT OF 'COLBMINT.TXT'
       FIN-EDT-KEY-INT.
           EXIT.
       
       EDT-IKY-INT SECTION.
       INI-EDT-IKY-INT.
*% INCLUDE EDT_IKY_INT OF 'COLBMINT.TXT'
           IF INT-NUM-PREP-LEN IN INT-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO INT-NUM-PREP-LEN IN INT-FLD
                GO TO FIN-EDT-IKY-INT.
           MOVE INT-NUM-PREP IN INT-FLD TO INT-NUM-PREP-EDT IN INT-EDT.
           MOVE INT-NUM-PREP-EDT IN INT-EDT TO 
                INT-NUM-PREP-ALF IN INT-FLD.
*% INCLUDE FIN_EDT_IKY_INT OF 'COLBMINT.TXT'
       FIN-EDT-IKY-INT.
           EXIT.
       
       EDT-MKY-INT SECTION.
       INI-EDT-MKY-INT.
*% INCLUDE EDT_MKY_INT OF 'COLBMINT.TXT'
*% INCLUDE FIN_EDT_MKY_INT OF 'COLBMINT.TXT'
       FIN-EDT-MKY-INT.
           EXIT.
       
       EDT-AKY-INT SECTION.
       INI-EDT-AKY-INT.
*% INCLUDE EDT_AKY_INT OF 'COLBMINT.TXT'
*% INCLUDE FIN_EDT_AKY_INT OF 'COLBMINT.TXT'
       FIN-EDT-AKY-INT.
           EXIT.
       
       EDT-FLD-INT SECTION.
       INI-EDT-FLD-INT.
*% INCLUDE EDT_FLD_INT OF 'COLBMINT.TXT'
*% RREG = "INT"
*% RREG_FRM = "INT"
*% NOMB = "FTRN"
*% INCLUDE FIN_EDT_FLD_INT OF 'COLBMINT.TXT'
       FIN-EDT-FLD-INT.
           EXIT.
*% INCLUDE EOF_BU OF 'COLBMINT.TXT'
*% INCLUDE EOF OF 'COLBMINT.TXT'
*% INCLUDE EOF_IBM OF 'COLBMINT.TXT'
