*% FRM = "TOC"
      *
      * COLBUTOC
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:58 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMTOC.TXT'
       SET-TAG-COLBUTOC SECTION.
       INI-SET-TAG-COLBUTOC.
           MOVE
           '<IDNMOD NOM=$COLBUTOC$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:58 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUTOC.
           EXIT.
           
       RET-ALL-TOC SECTION.
       INI-RET-ALL-TOC.
*% INCLUDE INI_RET_ALL_TOC OF 'COLBMTOC.TXT'
           PERFORM KEY-ALL-TOC.
           PERFORM RET-TOC-TOC.
           PERFORM RET-TIO-TOC.
*% INCLUDE FIN_RET_ALL_TOC OF 'COLBMTOC.TXT'
       FIN-RET-ALL-TOC.
           EXIT.
       
       RET-TOC-TOC SECTION.
       INI-RET-TOC-TOC.
*% INCLUDE INI_RET_TOC_TOC OF 'COLBMTOC.TXT'
           MOVE TOC-MSC-STAT IN TOC-FLD TO TOC-MSC-STAT IN TOC.
           MOVE TOC-MSC-OPEC IN TOC-FLD TO TOC-MSC-OPEC IN TOC.
*% RREG = "TOC"
*% RREG_FRM = "TOC"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE TOC-NUM-DTRN IN TOC-FLD TO TOC-NUM-DTRN IN TOC.
           MOVE TOC-NUM-MTRN IN TOC-FLD TO TOC-NUM-MTRN IN TOC.
           MOVE TOC-NUM-STRN IN TOC-FLD TO TOC-NUM-STRN IN TOC.
           MOVE TOC-NUM-ATRN IN TOC-FLD TO TOC-NUM-ATRN IN TOC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE TOC-MSC-TCOL IN TOC-FLD TO TOC-MSC-TCOL IN TOC.
           MOVE TOC-COD-RECU IN TOC-FLD TO TOC-COD-RECU IN TOC.
*% INCLUDE FIN_RET_TOC_TOC OF 'COLBMTOC.TXT'
       FIN-RET-TOC-TOC.
           EXIT.
       
       RET-TIO-TOC SECTION.
       INI-RET-TIO-TOC.
*% INCLUDE INI_RET_TIO_TOC OF 'COLBMTOC.TXT'
           MOVE TIO-CIC-TIOP IN TOC-FLD TO TIO-CIC-TIOP IN TIO.
           MOVE TIO-EXT-CTAB IN TOC-FLD TO TIO-EXT-CTAB IN TIO.
           MOVE TIO-GLS-DESC IN TOC-FLD TO TIO-GLS-DESC IN TIO.
           MOVE TIO-GLS-DCOR IN TOC-FLD TO TIO-GLS-DCOR IN TIO.
           MOVE TIO-GLS-ABRV IN TOC-FLD TO TIO-GLS-ABRV IN TIO.
*% INCLUDE FIN_RET_TIO_TOC OF 'COLBMTOC.TXT'
       FIN-RET-TIO-TOC.
           EXIT.
       
       KEY-ALL-TOC SECTION.
       INI-KEY-ALL-TOC.
*% INCLUDE INI_KEY_ALL_TOC OF 'COLBMTOC.TXT'
           PERFORM KEY-TOC-TOC.
           PERFORM KEY-TIO-TOC.
*% INCLUDE FIN_KEY_ALL_TOC OF 'COLBMTOC.TXT'
       FIN-KEY-ALL-TOC.
           EXIT.
       
       KEY-TOC-TOC SECTION.
       INI-KEY-TOC-TOC.
*% INCLUDE INI_KEY_TOC_TOC OF 'COLBMTOC.TXT'
           MOVE TOC-COD-TOCR IN TOC-FLD TO TOC-COD-TOCR IN TOC.
*% INCLUDE FIN_KEY_TOC_TOC OF 'COLBMTOC.TXT'
       FIN-KEY-TOC-TOC.
           EXIT.
       
       KEY-TIO-TOC SECTION.
       INI-KEY-TIO-TOC.
*% INCLUDE INI_KEY_TIO_TOC OF 'COLBMTOC.TXT'
*% INCLUDE FIN_KEY_TIO_TOC OF 'COLBMTOC.TXT'
       FIN-KEY-TIO-TOC.
           EXIT.
       
       SCR-CLRA-TOC SECTION.
       INI-SCR-CLRA-TOC.
*% INCLUDE INI_SCR_CLRA_TOC OF 'COLBMTOC.TXT'
           PERFORM CLR-KEY-TOC.
           PERFORM CLR-FLD-TOC.
*% INCLUDE FIN_SCR_CLRA_TOC OF 'COLBMTOC.TXT'
       FIN-SCR-CLRA-TOC.
           EXIT.
       
       CLR-KEY-TOC SECTION.
       INI-CLR-KEY-TOC.
*% INCLUDE INI_CLR_KEY_TOC OF 'COLBMTOC.TXT'
           MOVE SPACES TO TOC-COD-TOCR IN TOC-FLD.
*% INCLUDE FIN_CLR_KEY_TOC OF 'COLBMTOC.TXT'
       FIN-CLR-KEY-TOC.
           EXIT.
       
       CLR-FLD-TOC SECTION.
       INI-CLR-FLD-TOC.
*% INCLUDE INI_CLR_FLD_TOC OF 'COLBMTOC.TXT'
           MOVE SPACES TO FRM-GLS-OPEC IN TOC-FLD.
           MOVE SPACES TO FRM-GLS-TCOL IN TOC-FLD.
           MOVE SPACES TO FRM-GLS-RECU IN TOC-FLD.
           PERFORM CLR-TOC-TOC.
           PERFORM CLR-TIO-TOC.
*% INCLUDE FIN_CLR_FLD_TOC OF 'COLBMTOC.TXT'
       FIN-CLR-FLD-TOC.
           EXIT.
       
       CLR-TOC-TOC SECTION.
       INI-CLR-TOC-TOC.
*% INCLUDE INI_CLR_TOC_TOC OF 'COLBMTOC.TXT'
           MOVE SPACES TO TOC-MSC-STAT IN TOC-FLD.
           MOVE SPACES TO TOC-MSC-OPEC IN TOC-FLD.
*% RREG = "TOC"
*% RREG_FRM = "TOC"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO TOC-FEC-FTRN IN TOC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO TOC-MSC-TCOL IN TOC-FLD.
           MOVE SPACES TO TOC-COD-RECU IN TOC-FLD.
*% INCLUDE FIN_CLR_TOC_TOC OF 'COLBMTOC.TXT'
       FIN-CLR-TOC-TOC.
           EXIT.
       
       CLR-TIO-TOC SECTION.
       INI-CLR-TIO-TOC.
*% INCLUDE INI_CLR_TIO_TOC OF 'COLBMTOC.TXT'
           MOVE SPACES TO TIO-CIC-TIOP IN TOC-FLD.
           MOVE SPACES TO TIO-EXT-CTAB IN TOC-FLD.
           MOVE SPACES TO TIO-GLS-DESC IN TOC-FLD.
           MOVE SPACES TO TIO-GLS-DCOR IN TOC-FLD.
           MOVE SPACES TO TIO-GLS-ABRV IN TOC-FLD.
*% INCLUDE FIN_CLR_TIO_TOC OF 'COLBMTOC.TXT'
       FIN-CLR-TIO-TOC.
           EXIT.
       
       FST-KEY-TOC SECTION.
       INI-FST-KEY-TOC.
*% INCLUDE INI_FST_KEY_TOC OF 'COLBMTOC.TXT'
           MOVE -1 TO TOC-COD-TOCR-LEN IN TOC-FLD.
*% INCLUDE FIN_FST_KEY_TOC OF 'COLBMTOC.TXT'
       FIN-FST-KEY-TOC.
           EXIT.
       
       FST-FLD-TOC SECTION.
       INI-FST-FLD-TOC.
*% INCLUDE INI_FST_FLD_TOC OF 'COLBMTOC.TXT'
           MOVE -1 TO TOC-MSC-STAT-LEN IN TOC-FLD.
*% INCLUDE FIN_FST_FLD_TOC OF 'COLBMTOC.TXT'
       FIN-FST-FLD-TOC.
           EXIT.
       
       PRO-KEY-TOC SECTION.
       INI-PRO-KEY-TOC.
*% INCLUDE INI_PRO_KEY_TOC OF 'COLBMTOC.TXT'
           PERFORM PRO-MKY-TOC.
*% INCLUDE FIN_PRO_KEY_TOC OF 'COLBMTOC.TXT'
       FIN-PRO-KEY-TOC.
           EXIT.
       
       PRO-MKY-TOC SECTION.
       INI-PRO-MKY-TOC.
*% INCLUDE INI_PRO_MKY_TOC OF 'COLBMTOC.TXT'
*% INCLUDE FIN_PRO_MKY_TOC OF 'COLBMTOC.TXT'
       FIN-PRO-MKY-TOC.
           EXIT.
       
       PRO-FLD-TOC SECTION.
       INI-PRO-FLD-TOC.
*% INCLUDE INI_PRO_FLD_TOC OF 'COLBMTOC.TXT'
           PERFORM PRO-TOC-TOC.
           PERFORM PRO-TIO-TOC.
*% INCLUDE FIN_PRO_FLD_TOC OF 'COLBMTOC.TXT'
       FIN-PRO-FLD-TOC.
           EXIT.
       
       PRO-TOC-TOC SECTION.
       INI-PRO-TOC-TOC.
*% INCLUDE INI_PRO_TOC_TOC OF 'COLBMTOC.TXT'
           MOVE FRM-CPIM-AEY TO TOC-MSC-STAT-ATR IN TOC-FLD.
           MOVE FRM-CPIM-AEY TO TOC-MSC-OPEC-ATR IN TOC-FLD.
           MOVE FRM-CPIM-AEY TO TOC-MSC-TCOL-ATR IN TOC-FLD.
           MOVE FRM-CPIM-AEY TO TOC-COD-RECU-ATR IN TOC-FLD.
*% INCLUDE FIN_PRO_TOC_TOC OF 'COLBMTOC.TXT'
       FIN-PRO-TOC-TOC.
           EXIT.
       
       PRO-TIO-TOC SECTION.
       INI-PRO-TIO-TOC.
*% INCLUDE INI_PRO_TIO_TOC OF 'COLBMTOC.TXT'
*% INCLUDE FIN_PRO_TIO_TOC OF 'COLBMTOC.TXT'
       FIN-PRO-TIO-TOC.
           EXIT.
       
       UNP-KEY-TOC SECTION.
       INI-UNP-KEY-TOC.
*% INCLUDE INI_UNP_KEY_TOC OF 'COLBMTOC.TXT'
           PERFORM UNP-IKY-TOC.
           PERFORM UNP-AKY-TOC.
           PERFORM UNP-MKY-TOC.
*% INCLUDE FIN_UNP_KEY_TOC OF 'COLBMTOC.TXT'
       FIN-UNP-KEY-TOC.
           EXIT.
       
       UNP-IKY-TOC SECTION.
       INI-UNP-IKY-TOC.
*% INCLUDE INI_UNP_IKY_TOC OF 'COLBMTOC.TXT'
           MOVE FRM-CPIM-UBY TO TOC-COD-TOCR-ATR.
*% INCLUDE FIN_UNP_IKY_TOC OF 'COLBMTOC.TXT'
       FIN-UNP-IKY-TOC.
           EXIT.
       
       UNP-MKY-TOC SECTION.
       INI-UNP-MKY-TOC.
*% INCLUDE INI_UNP_MKY_TOC OF 'COLBMTOC.TXT'
*% INCLUDE FIN_UNP_MKY_TOC OF 'COLBMTOC.TXT'
       FIN-UNP-MKY-TOC.
           EXIT.
       
       UNP-AKY-TOC SECTION.
       INI-UNP-AKY-TOC.
*% INCLUDE INI_UNP_AKY_TOC OF 'COLBMTOC.TXT'
*% INCLUDE FIN_UNP_AKY_TOC OF 'COLBMTOC.TXT'
       FIN-UNP-AKY-TOC.
           EXIT.
       
       UNP-FLD-TOC SECTION.
       INI-UNP-FLD-TOC.
*% INCLUDE INI_UNP_FLD_TOC OF 'COLBMTOC.TXT'
           PERFORM UNP-TOC-TOC.
           PERFORM UNP-TIO-TOC.
*% INCLUDE FIN_UNP_FLD_TOC OF 'COLBMTOC.TXT'
       FIN-UNP-FLD-TOC.
           EXIT.
       
       UNP-TOC-TOC SECTION.
       INI-UNP-TOC-TOC.
*% INCLUDE INI_UNP_TOC_TOC OF 'COLBMTOC.TXT'
           MOVE FRM-CPIM-UBY TO TOC-MSC-STAT-ATR IN TOC-FLD.
           MOVE FRM-CPIM-UBY TO TOC-MSC-OPEC-ATR IN TOC-FLD.
           MOVE FRM-CPIM-UBY TO TOC-MSC-TCOL-ATR IN TOC-FLD.
           MOVE FRM-CPIM-UBY TO TOC-COD-RECU-ATR IN TOC-FLD.
*% INCLUDE FIN_UNP_TOC_TOC OF 'COLBMTOC.TXT'
       FIN-UNP-TOC-TOC.
           EXIT.
       
       UNP-TIO-TOC SECTION.
       INI-UNP-TIO-TOC.
*% INCLUDE INI_UNP_TIO_TOC OF 'COLBMTOC.TXT'
*% INCLUDE FIN_UNP_TIO_TOC OF 'COLBMTOC.TXT'
       FIN-UNP-TIO-TOC.
           EXIT.
       
       VAL-MKY-TOC SECTION.
       INI-VAL-MKY-TOC.
*% INCLUDE INI_VAL_MKY_TOC OF 'COLBMTOC.TXT'
*% INCLUDE FIN_VAL_MKY_TOC OF 'COLBMTOC.TXT'
       FIN-VAL-MKY-TOC.
           EXIT.
       
       VAL-FLD-TOC SECTION.
       INI-VAL-FLD-TOC.
*% INCLUDE INI_VAL_FLD_TOC OF 'COLBMTOC.TXT'
           PERFORM VAL-TIO-CIC-TIOP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TIO-CIC-TIOP-LEN IN TOC-FLD
                GO TO FIN-VAL-FLD-TOC.
           PERFORM VAL-TIO-EXT-CTAB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TIO-EXT-CTAB-LEN IN TOC-FLD
                GO TO FIN-VAL-FLD-TOC.
           PERFORM VAL-TIO-GLS-DESC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TIO-GLS-DESC-LEN IN TOC-FLD
                GO TO FIN-VAL-FLD-TOC.
           PERFORM VAL-TIO-GLS-DCOR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TIO-GLS-DCOR-LEN IN TOC-FLD
                GO TO FIN-VAL-FLD-TOC.
           PERFORM VAL-TIO-GLS-ABRV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TIO-GLS-ABRV-LEN IN TOC-FLD
                GO TO FIN-VAL-FLD-TOC.
           PERFORM VAL-TOC-MSC-STAT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TOC-MSC-STAT-LEN IN TOC-FLD
                GO TO FIN-VAL-FLD-TOC.
           PERFORM VAL-TOC-MSC-OPEC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TOC-MSC-OPEC-LEN IN TOC-FLD
                GO TO FIN-VAL-FLD-TOC.
*% RREG = "TOC"
*% RREG_FRM = "TOC"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-TOC-FEC-FTRN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                TOC-FEC-FTRN-LEN IN TOC-FLD
                GO TO FIN-VAL-FLD-TOC.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-GLS-OPEC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-OPEC-LEN IN TOC-FLD
                GO TO FIN-VAL-FLD-TOC.
           PERFORM VAL-TOC-MSC-TCOL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TOC-MSC-TCOL-LEN IN TOC-FLD
                GO TO FIN-VAL-FLD-TOC.
           PERFORM VAL-TOC-COD-RECU.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TOC-COD-RECU-LEN IN TOC-FLD
                GO TO FIN-VAL-FLD-TOC.
           PERFORM VAL-FRM-GLS-TCOL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TCOL-LEN IN TOC-FLD
                GO TO FIN-VAL-FLD-TOC.
           PERFORM VAL-FRM-GLS-RECU.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-RECU-LEN IN TOC-FLD
                GO TO FIN-VAL-FLD-TOC.
*% INCLUDE FIN_VAL_FLD_TOC OF 'COLBMTOC.TXT'
       FIN-VAL-FLD-TOC.
           EXIT.
       
       VAL-TIO-CIC-TIOP SECTION.
       INI-VAL-TIO-CIC-TIOP.
*% LBL = "TIO_CIC_TIOP OF 'COLBMTOC.TXT'"
*% INCLUDE INI_TIO_CIC_TIOP OF 'COLBMTOC.TXT'
*% INCLUDE TIO_CIC_TIOP OF 'COLBMTOC.TXT'
*% INCLUDE FIN_TIO_CIC_TIOP OF 'COLBMTOC.TXT'
       FIN-VAL-TIO-CIC-TIOP.
           EXIT.
       
       VAL-TIO-EXT-CTAB SECTION.
       INI-VAL-TIO-EXT-CTAB.
*% LBL = "TIO_EXT_CTAB OF 'COLBMTOC.TXT'"
*% INCLUDE INI_TIO_EXT_CTAB OF 'COLBMTOC.TXT'
*% INCLUDE TIO_EXT_CTAB OF 'COLBMTOC.TXT'
*% INCLUDE FIN_TIO_EXT_CTAB OF 'COLBMTOC.TXT'
       FIN-VAL-TIO-EXT-CTAB.
           EXIT.
       
       VAL-TIO-GLS-DESC SECTION.
       INI-VAL-TIO-GLS-DESC.
*% LBL = "TIO_GLS_DESC OF 'COLBMTOC.TXT'"
*% INCLUDE INI_TIO_GLS_DESC OF 'COLBMTOC.TXT'
*% INCLUDE TIO_GLS_DESC OF 'COLBMTOC.TXT'
*% INCLUDE FIN_TIO_GLS_DESC OF 'COLBMTOC.TXT'
       FIN-VAL-TIO-GLS-DESC.
           EXIT.
       
       VAL-TIO-GLS-DCOR SECTION.
       INI-VAL-TIO-GLS-DCOR.
*% LBL = "TIO_GLS_DCOR OF 'COLBMTOC.TXT'"
*% INCLUDE INI_TIO_GLS_DCOR OF 'COLBMTOC.TXT'
*% INCLUDE TIO_GLS_DCOR OF 'COLBMTOC.TXT'
*% INCLUDE FIN_TIO_GLS_DCOR OF 'COLBMTOC.TXT'
       FIN-VAL-TIO-GLS-DCOR.
           EXIT.
       
       VAL-TIO-GLS-ABRV SECTION.
       INI-VAL-TIO-GLS-ABRV.
*% LBL = "TIO_GLS_ABRV OF 'COLBMTOC.TXT'"
*% INCLUDE INI_TIO_GLS_ABRV OF 'COLBMTOC.TXT'
*% INCLUDE TIO_GLS_ABRV OF 'COLBMTOC.TXT'
*% INCLUDE FIN_TIO_GLS_ABRV OF 'COLBMTOC.TXT'
       FIN-VAL-TIO-GLS-ABRV.
           EXIT.
       
       VAL-TOC-MSC-STAT SECTION.
       INI-VAL-TOC-MSC-STAT.
*% LBL = "TOC_MSC_STAT OF 'COLBMTOC.TXT'"
*% INCLUDE INI_TOC_MSC_STAT OF 'COLBMTOC.TXT'
           IF TOC-MSC-STAT IN TOC-FLD NOT > SPACES
              MOVE SPACES TO TOC-MSC-STAT IN TOC-FLD.
*% INCLUDE TOC_MSC_STAT OF 'COLBMTOC.TXT'
*% BUF = "TOC-FLD"
*% SIS = "GNS"
*% TAB = "VIGE"
*% FLD = "TOC-MSC-STAT"
*% GLS = ""
*% TIENE_GLOSA = FALSE
*% LGL = 0
*% INCLUDE 'VALIND'
*% INCLUDE FIN_TOC_MSC_STAT OF 'COLBMTOC.TXT'
       FIN-VAL-TOC-MSC-STAT.
           EXIT.
       
       VAL-TOC-MSC-OPEC SECTION.
       INI-VAL-TOC-MSC-OPEC.
*% LBL = "TOC_MSC_OPEC OF 'COLBMTOC.TXT'"
*% INCLUDE INI_TOC_MSC_OPEC OF 'COLBMTOC.TXT'
           IF TOC-MSC-OPEC IN TOC-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-TOC-MSC-OPEC.
*% INCLUDE TOC_MSC_OPEC OF 'COLBMTOC.TXT'
*% BUF = "TOC-FLD"
*% SIS = "COL"
*% TAB = "OPEC"
*% FLD = "TOC-MSC-OPEC"
*% GLS = "FRM-GLS-OPEC"
*% TIENE_GLOSA = TRUE
*% LGL = 34
*% INCLUDE 'VALIND'
*% INCLUDE FIN_TOC_MSC_OPEC OF 'COLBMTOC.TXT'
       FIN-VAL-TOC-MSC-OPEC.
           EXIT.
*% RREG = "TOC"
*% RREG_FRM = "TOC"
*% NOMB = "FTRN"
*% OBL = "S"
*% LBL = "TOC_FEC_FTRN OF 'COLBMTOC.TXT'"
*% IZR = "S"
*% ZRO = "S"
*% FRM_IFLD = ""
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-TOC-FEC-FTRN SECTION.
       INI-VAL-TOC-FEC-FTRN.
           IF TOC-FEC-FTRN IN TOC-FLD  NOT > ZEROES
              MOVE ZEROES TO TOC-FEC-FTRN IN TOC-FLD.
*% INCLUDE INI_TOC_FEC_FTRN OF 'COLBMTOC.TXT'
*% INCLUDE TOC_FEC_FTRN OF 'COLBMTOC.TXT'
*% INCLUDE FIN_TOC_FEC_FTRN OF 'COLBMTOC.TXT'
       FIN-VAL-TOC-FEC-FTRN.
           EXIT.
      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-FRM-GLS-OPEC SECTION.
       INI-VAL-FRM-GLS-OPEC.
*% LBL = "FRM_GLS_OPEC OF 'COLBMTOC.TXT'"
*% INCLUDE INI_FRM_GLS_OPEC OF 'COLBMTOC.TXT'
*% INCLUDE FRM_GLS_OPEC OF 'COLBMTOC.TXT'
*% INCLUDE FIN_FRM_GLS_OPEC OF 'COLBMTOC.TXT'
       FIN-VAL-FRM-GLS-OPEC.
           EXIT.
       
       VAL-TOC-MSC-TCOL SECTION.
       INI-VAL-TOC-MSC-TCOL.
*% LBL = "TOC_MSC_TCOL OF 'COLBMTOC.TXT'"
*% INCLUDE INI_TOC_MSC_TCOL OF 'COLBMTOC.TXT'
           IF TOC-MSC-TCOL IN TOC-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-TOC-MSC-TCOL.
*% INCLUDE TOC_MSC_TCOL OF 'COLBMTOC.TXT'
*% BUF = "TOC-FLD"
*% SIS = "COL"
*% TAB = "TCOL"
*% FLD = "TOC-MSC-TCOL"
*% GLS = "FRM-GLS-TCOL"
*% TIENE_GLOSA = TRUE
*% LGL = 34
*% INCLUDE 'VALIND'
*% INCLUDE FIN_TOC_MSC_TCOL OF 'COLBMTOC.TXT'
       FIN-VAL-TOC-MSC-TCOL.
           EXIT.
       
       VAL-TOC-COD-RECU SECTION.
       INI-VAL-TOC-COD-RECU.
*% LBL = "TOC_COD_RECU OF 'COLBMTOC.TXT'"
*% INCLUDE INI_TOC_COD_RECU OF 'COLBMTOC.TXT'
           IF TOC-COD-RECU IN TOC-FLD NOT > SPACES
              MOVE SPACES TO TOC-COD-RECU IN TOC-FLD.
*% INCLUDE TOC_COD_RECU OF 'COLBMTOC.TXT'
*% INCLUDE FIN_TOC_COD_RECU OF 'COLBMTOC.TXT'
       FIN-VAL-TOC-COD-RECU.
           EXIT.
       
       VAL-FRM-GLS-TCOL SECTION.
       INI-VAL-FRM-GLS-TCOL.
*% LBL = "FRM_GLS_TCOL OF 'COLBMTOC.TXT'"
*% INCLUDE INI_FRM_GLS_TCOL OF 'COLBMTOC.TXT'
*% INCLUDE FRM_GLS_TCOL OF 'COLBMTOC.TXT'
*% INCLUDE FIN_FRM_GLS_TCOL OF 'COLBMTOC.TXT'
       FIN-VAL-FRM-GLS-TCOL.
           EXIT.
       
       VAL-FRM-GLS-RECU SECTION.
       INI-VAL-FRM-GLS-RECU.
