*% IZR = "S"
*% ZRO = "S"
*% KO0 = "K"
*% ZRO = "S"
*% LMX = "7"
*% FRM_IFLD = ""
      * IDC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION

       VAL-DBC-NUM-ICLI SECTION.
       INI-VAL-DBC-NUM-ICLI.
*% INCLUDE INI_DBC_NUM_ICLI  OF 'COLBMCDD.TXT'
*% INCLUDE DBC_NUM_ICLI  OF 'COLBMCDD.TXT'
*% INCLUDE FIN_DBC_NUM_ICLI  OF 'COLBMCDD.TXT'
       FIN-VAL-DBC-NUM-ICLI.
           EXIT.

       VAL-DBC-VRF-ICLI SECTION.
       INI-VAL-DBC-VRF-ICLI.
*% INCLUDE INI_DBC_VRF_ICLI  OF 'COLBMCDD.TXT'
           IF DBC-VRF-ICLI IN CDD-FLD NOT > SPACES
              MOVE SPACES TO DBC-VRF-ICLI IN CDD-FLD.
*% INCLUDE 'GNSSRC:SETVRF202.SRC'
*%! INCLUDE 'PROVRF'
*% INCLUDE DBC_VRF_ICLI  OF 'COLBMCDD.TXT'
*% INCLUDE FIN_DBC_VRF_ICLI  OF 'COLBMCDD.TXT'
*% LBL = ""
       FIN-VAL-DBC-VRF-ICLI.
           EXIT.
       VAL-DBC-IND-ICLI SECTION.
       INI-VAL-DBC-IND-ICLI.
*% INCLUDE INI_DBC_IND_ICLI  OF 'COLBMCDD.TXT'
           IF DBC-IND-ICLI IN CDD-FLD NOT > SPACES
              MOVE SPACES TO DBC-IND-ICLI IN CDD-FLD.
*% INCLUDE 'GNSSRC:VALIDC202.SRC'
*% INCLUDE DBC_IND_ICLI  OF 'COLBMCDD.TXT'
*% INCLUDE FIN_DBC_IND_ICLI  OF 'COLBMCDD.TXT'
       FIN-VAL-DBC-IND-ICLI.
           EXIT.

       VAL-DBC-GLS-ICLI SECTION.
       INI-VAL-DBC-GLS-ICLI.
*% LBL = "DBC_GLS_ICLI  OF 'COLBMCDD.TXT'"
*% INCLUDE INI_DBC_GLS_ICLI  OF 'COLBMCDD.TXT'
           IF DBC-GLS-ICLI IN CDD-FLD NOT > IDC-INIC-GLS 
              MOVE IDC-INIC-GLS TO DBC-GLS-ICLI IN CDD-FLD.
      *VAL-GLS para IDC valida glosa ( extension ) de identificador
           MOVE DBC-GLS-ICLI IN CDD-FLD TO IDC-GLS-RUTV.
           PERFORM VAL-GLS.
           MOVE IDC-GLS-RUTV TO DBC-GLS-ICLI IN CDD-FLD.
*% INCLUDE DBC_GLS_ICLI  OF 'COLBMCDD.TXT'
*% INCLUDE FIN_DBC_GLS_ICLI  OF 'COLBMCDD.TXT'
       FIN-VAL-DBC-GLS-ICLI.
           EXIT.

      * IDC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-FRM-VAL-PREP SECTION.
       INI-VAL-FRM-VAL-PREP.
*% LBL = "FRM_VAL_PREP OF 'COLBMCDD.TXT'"
*% INCLUDE INI_FRM_VAL_PREP OF 'COLBMCDD.TXT'
*% INCLUDE FRM_VAL_PREP OF 'COLBMCDD.TXT'
*% INCLUDE FIN_FRM_VAL_PREP OF 'COLBMCDD.TXT'
       FIN-VAL-FRM-VAL-PREP.
           EXIT.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCUR"
*% IZR = "S"
       
       VAL-CON-KEY-CDD SECTION.
       INI-VAL-CON-KEY-CDD.
*% INCLUDE INI_VAL_CON_KEY_CDD OF 'COLBMCDD.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI009.SRCAIU VAL-CON
           IF     DBC-NUM-ICLI IN CDD-FLD NOT > ZEROES
              AND DBC-VRF-ICLI IN CDD-FLD NOT > SPACES
              AND DBC-IND-ICLI IN CDD-FLD NOT > SPACES
              AND DBC-GLS-ICLI IN CDD-FLD NOT > IDC-INIC-GLS
      * IDC-FIN --> BMCHIBCI009.SRCAIU VAL-CON
              AND FRM-VAL-PREP IN CDD-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CDD.
*% INCLUDE FIN_VAL_CON_KEY_CDD OF 'COLBMCDD.TXT'
       FIN-VAL-CON-KEY-CDD.
           EXIT.
       
       VAL-CON-IKY-CDD SECTION.
       INI-VAL-CON-IKY-CDD.
*% INCLUDE INI_VAL_CON_IKY_CDD OF 'COLBMCDD.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI009.SRCAIU VAL-CON
           IF     DBC-NUM-ICLI IN CDD-FLD NOT > ZEROES
              AND DBC-VRF-ICLI IN CDD-FLD NOT > SPACES
              AND DBC-IND-ICLI IN CDD-FLD NOT > SPACES
              AND DBC-GLS-ICLI IN CDD-FLD NOT > IDC-INIC-GLS
      * IDC-FIN --> BMCHIBCI009.SRCAIU VAL-CON
              AND FRM-VAL-PREP IN CDD-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CDD.
*% INCLUDE FIN_VAL_CON_IKY_CDD OF 'COLBMCDD.TXT'
       FIN-VAL-CON-IKY-CDD.
           EXIT.
       
       VAL-NUM-KEY-CDD SECTION.
       INI-VAL-NUM-KEY-CDD.
           PERFORM VAL-NUM-IKY-CDD.
           PERFORM VAL-NUM-MKY-CDD.
           PERFORM VAL-NUM-AKY-CDD.
       FIN-VAL-NUM-KEY-CDD.
           EXIT.
       
       VAL-NUM-IKY-CDD SECTION.
       INI-VAL-NUM-IKY-CDD.
*% INCLUDE INI_VAL_NUM_IKY_CDD OF 'COLBMCDD.TXT'
*% RREG = "DBC"
