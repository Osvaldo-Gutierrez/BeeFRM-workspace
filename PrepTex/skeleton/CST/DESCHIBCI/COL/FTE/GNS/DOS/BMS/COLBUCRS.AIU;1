*% FRM = "CRS"
      *
      * COLBUCRS
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:36 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMCRS.TXT'
       SET-TAG-COLBUCRS SECTION.
       INI-SET-TAG-COLBUCRS.
           MOVE
           '<IDNMOD NOM=$COLBUCRS$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:36 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCRS.
           EXIT.
           
       RET-ALL-CRS SECTION.
       INI-RET-ALL-CRS.
*% INCLUDE INI_RET_ALL_CRS OF 'COLBMCRS.TXT'
           PERFORM KEY-ALL-CRS.
           PERFORM RET-CRS-CRS.
*% INCLUDE FIN_RET_ALL_CRS OF 'COLBMCRS.TXT'
       FIN-RET-ALL-CRS.
           EXIT.
       
       RET-CRS-CRS SECTION.
       INI-RET-CRS-CRS.
*% INCLUDE INI_RET_CRS_CRS OF 'COLBMCRS.TXT'
           MOVE CRS-VAL-VENP IN CRS-FLD TO CRS-VAL-VENP IN CRS.
           MOVE CRS-VAL-VCCP IN CRS-FLD TO CRS-VAL-VCCP IN CRS.
           MOVE CRS-VAL-PAGO IN CRS-FLD TO CRS-VAL-PAGO IN CRS.
           MOVE CRS-VAL-PNEG IN CRS-FLD TO CRS-VAL-PNEG IN CRS.
           MOVE CRS-VAL-VCNC IN CRS-FLD TO CRS-VAL-VCNC IN CRS.
           MOVE CRS-VAL-GNEG IN CRS-FLD TO CRS-VAL-GNEG IN CRS.
           MOVE CRS-VAL-MNEG IN CRS-FLD TO CRS-VAL-MNEG IN CRS.
           MOVE CRS-COD-OFRC IN CRS-FLD TO CRS-COD-OFRC IN CRS.
           MOVE CRS-COD-OFOC IN CRS-FLD TO CRS-COD-OFOC IN CRS.
           MOVE CRS-COD-MONS IN CRS-FLD TO CRS-COD-MONS IN CRS.
           MOVE CRS-NUM-DVCN IN CRS-FLD TO CRS-NUM-DVCN IN CRS.
           MOVE CRS-NUM-MVCN IN CRS-FLD TO CRS-NUM-MVCN IN CRS.
           MOVE CRS-NUM-AVCN IN CRS-FLD TO CRS-NUM-AVCN IN CRS.
           MOVE CRS-NUM-DCAN IN CRS-FLD TO CRS-NUM-DCAN IN CRS.
           MOVE CRS-NUM-MCAN IN CRS-FLD TO CRS-NUM-MCAN IN CRS.
           MOVE CRS-NUM-ACAN IN CRS-FLD TO CRS-NUM-ACAN IN CRS.
           MOVE CRS-NUM-VNEG IN CRS-FLD TO CRS-NUM-VNEG IN CRS.
           MOVE CRS-IND-ACTU IN CRS-FLD TO CRS-IND-ACTU IN CRS.
           MOVE CRS-IND-PNEG IN CRS-FLD TO CRS-IND-PNEG IN CRS.
           MOVE CRS-NUM-IVEN IN CRS-FLD TO CRS-NUM-IVEN IN CRS.
           MOVE CRS-NUM-DVEN IN CRS-FLD TO CRS-NUM-DVEN IN CRS.
           MOVE CRS-NUM-MVEN IN CRS-FLD TO CRS-NUM-MVEN IN CRS.
           MOVE CRS-NUM-AVEN IN CRS-FLD TO CRS-NUM-AVEN IN CRS.
           MOVE CRS-NUM-DIMV IN CRS-FLD TO CRS-NUM-DIMV IN CRS.
           MOVE CRS-NUM-MIMV IN CRS-FLD TO CRS-NUM-MIMV IN CRS.
           MOVE CRS-NUM-AIMV IN CRS-FLD TO CRS-NUM-AIMV IN CRS.
           MOVE CRS-VAL-TASA IN CRS-FLD TO CRS-VAL-TASA IN CRS.
           MOVE CRS-NUM-IDEU IN CRS-FLD TO CRS-NUM-IDEU IN CRS.
           MOVE CRS-IND-IDEU IN CRS-FLD TO CRS-IND-IDEU IN CRS.
           MOVE CRS-GLS-IDEU IN CRS-FLD TO CRS-GLS-IDEU IN CRS.
           MOVE CRS-COD-AUXI IN CRS-FLD TO CRS-COD-AUXI IN CRS.
           MOVE CRS-VAL-MORA IN CRS-FLD TO CRS-VAL-MORA IN CRS.
           MOVE CRS-NUM-DCOL IN CRS-FLD TO CRS-NUM-DCOL IN CRS.
           MOVE CRS-NUM-MCOL IN CRS-FLD TO CRS-NUM-MCOL IN CRS.
           MOVE CRS-NUM-ACOL IN CRS-FLD TO CRS-NUM-ACOL IN CRS.
           MOVE CRS-IND-PLZO IN CRS-FLD TO CRS-IND-PLZO IN CRS.
           MOVE CRS-IND-TCRE IN CRS-FLD TO CRS-IND-TCRE IN CRS.
           MOVE CRS-KEY-IOPC IN CRS-FLD TO CRS-KEY-IOPC IN CRS.
           MOVE CRS-IND-TSIS IN CRS-FLD TO CRS-IND-TSIS IN CRS.
           MOVE CRS-GLS-ICRS IN CRS-FLD TO CRS-GLS-ICRS IN CRS.
           MOVE CRS-MSC-CANC IN CRS-FLD TO CRS-MSC-CANC IN CRS.
           MOVE CRS-VAL-SCAP IN CRS-FLD TO CRS-VAL-SCAP IN CRS.
           MOVE CRS-VAL-SDLC IN CRS-FLD TO CRS-VAL-SDLC IN CRS.
           MOVE CRS-IND-CCOM IN CRS-FLD TO CRS-IND-CCOM IN CRS.
           MOVE CRS-VAL-CRED IN CRS-FLD TO CRS-VAL-CRED IN CRS.
*% INCLUDE FIN_RET_CRS_CRS OF 'COLBMCRS.TXT'
       FIN-RET-CRS-CRS.
           EXIT.
       
       KEY-ALL-CRS SECTION.
       INI-KEY-ALL-CRS.
*% INCLUDE INI_KEY_ALL_CRS OF 'COLBMCRS.TXT'
           PERFORM KEY-CRS-CRS.
*% INCLUDE FIN_KEY_ALL_CRS OF 'COLBMCRS.TXT'
       FIN-KEY-ALL-CRS.
           EXIT.
       
       KEY-CRS-CRS SECTION.
       INI-KEY-CRS-CRS.
*% INCLUDE INI_KEY_CRS_CRS OF 'COLBMCRS.TXT'
*% INCLUDE FIN_KEY_CRS_CRS OF 'COLBMCRS.TXT'
       FIN-KEY-CRS-CRS.
           EXIT.
       
       SCR-CLRA-CRS SECTION.
       INI-SCR-CLRA-CRS.
*% INCLUDE INI_SCR_CLRA_CRS OF 'COLBMCRS.TXT'
           PERFORM CLR-KEY-CRS.
           PERFORM CLR-FLD-CRS.
*% INCLUDE FIN_SCR_CLRA_CRS OF 'COLBMCRS.TXT'
       FIN-SCR-CLRA-CRS.
           EXIT.
       
       CLR-KEY-CRS SECTION.
       INI-CLR-KEY-CRS.
*% INCLUDE INI_CLR_KEY_CRS OF 'COLBMCRS.TXT'
           MOVE SPACES TO FRM-CAI-ICRS IN CRS-FLD.
           MOVE ZEROES TO FRM-IIC-ICRS IN CRS-FLD.
*% INCLUDE FIN_CLR_KEY_CRS OF 'COLBMCRS.TXT'
       FIN-CLR-KEY-CRS.
           EXIT.
       
       CLR-FLD-CRS SECTION.
       INI-CLR-FLD-CRS.
*% INCLUDE INI_CLR_FLD_CRS OF 'COLBMCRS.TXT'
           PERFORM CLR-CRS-CRS.
*% INCLUDE FIN_CLR_FLD_CRS OF 'COLBMCRS.TXT'
       FIN-CLR-FLD-CRS.
           EXIT.
       
       CLR-CRS-CRS SECTION.
       INI-CLR-CRS-CRS.
*% INCLUDE INI_CLR_CRS_CRS OF 'COLBMCRS.TXT'
           MOVE ZEROES TO CRS-VAL-VENP IN CRS-FLD.
           MOVE ZEROES TO CRS-VAL-VCCP IN CRS-FLD.
           MOVE ZEROES TO CRS-VAL-PAGO IN CRS-FLD.
           MOVE ZEROES TO CRS-VAL-PNEG IN CRS-FLD.
           MOVE ZEROES TO CRS-VAL-VCNC IN CRS-FLD.
           MOVE ZEROES TO CRS-VAL-GNEG IN CRS-FLD.
           MOVE ZEROES TO CRS-VAL-MNEG IN CRS-FLD.
           MOVE SPACES TO CRS-COD-OFRC IN CRS-FLD.
           MOVE SPACES TO CRS-COD-OFOC IN CRS-FLD.
           MOVE SPACES TO CRS-COD-MONS IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-DVCN IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-MVCN IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-AVCN IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-DCAN IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-MCAN IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-ACAN IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-VNEG IN CRS-FLD.
           MOVE SPACES TO CRS-IND-ACTU IN CRS-FLD.
           MOVE SPACES TO CRS-IND-PNEG IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-IVEN IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-DVEN IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-MVEN IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-AVEN IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-DIMV IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-MIMV IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-AIMV IN CRS-FLD.
           MOVE ZEROES TO CRS-VAL-TASA IN CRS-FLD.
           MOVE SPACES TO CRS-NUM-IDEU IN CRS-FLD.
           MOVE SPACES TO CRS-IND-IDEU IN CRS-FLD.
           MOVE SPACES TO CRS-GLS-IDEU IN CRS-FLD.
           MOVE SPACES TO CRS-COD-AUXI IN CRS-FLD.
           MOVE ZEROES TO CRS-VAL-MORA IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-DCOL IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-MCOL IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-ACOL IN CRS-FLD.
           MOVE SPACES TO CRS-IND-PLZO IN CRS-FLD.
           MOVE SPACES TO CRS-IND-TCRE IN CRS-FLD.
           MOVE SPACES TO CRS-KEY-IOPC IN CRS-FLD.
           MOVE SPACES TO CRS-IND-TSIS IN CRS-FLD.
           MOVE SPACES TO CRS-GLS-ICRS IN CRS-FLD.
           MOVE SPACES TO CRS-MSC-CANC IN CRS-FLD.
           MOVE ZEROES TO CRS-VAL-SCAP IN CRS-FLD.
           MOVE ZEROES TO CRS-VAL-SDLC IN CRS-FLD.
           MOVE SPACES TO CRS-IND-CCOM IN CRS-FLD.
           MOVE ZEROES TO CRS-VAL-CRED IN CRS-FLD.
*% INCLUDE FIN_CLR_CRS_CRS OF 'COLBMCRS.TXT'
       FIN-CLR-CRS-CRS.
           EXIT.
       
       FST-KEY-CRS SECTION.
       INI-FST-KEY-CRS.
*% INCLUDE INI_FST_KEY_CRS OF 'COLBMCRS.TXT'
           MOVE -1 TO FRM-CAI-ICRS-LEN IN CRS-FLD.
*% INCLUDE FIN_FST_KEY_CRS OF 'COLBMCRS.TXT'
       FIN-FST-KEY-CRS.
           EXIT.
       
       FST-FLD-CRS SECTION.
       INI-FST-FLD-CRS.
*% INCLUDE INI_FST_FLD_CRS OF 'COLBMCRS.TXT'
           MOVE -1 TO CRS-VAL-VENP-LEN IN CRS-FLD.
*% INCLUDE FIN_FST_FLD_CRS OF 'COLBMCRS.TXT'
       FIN-FST-FLD-CRS.
           EXIT.
       
       PRO-KEY-CRS SECTION.
       INI-PRO-KEY-CRS.
*% INCLUDE INI_PRO_KEY_CRS OF 'COLBMCRS.TXT'
           MOVE FRM-CPIM-AEY TO FRM-CAI-ICRS-ATR IN CRS-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IIC-ICRS-ATR IN CRS-FLD.
           PERFORM PRO-MKY-CRS.
*% INCLUDE FIN_PRO_KEY_CRS OF 'COLBMCRS.TXT'
       FIN-PRO-KEY-CRS.
           EXIT.
       
