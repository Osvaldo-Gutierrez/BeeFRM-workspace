       VAL-MKY-CRS SECTION.
       INI-VAL-MKY-CRS.
*% INCLUDE INI_VAL_MKY_CRS OF 'COLBMCRS.TXT'
*% INCLUDE FIN_VAL_MKY_CRS OF 'COLBMCRS.TXT'
       FIN-VAL-MKY-CRS.
           EXIT.
       
       VAL-FLD-CRS SECTION.
       INI-VAL-FLD-CRS.
*% INCLUDE INI_VAL_FLD_CRS OF 'COLBMCRS.TXT'
           PERFORM VAL-CRS-VAL-VENP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CRS-VAL-VENP-LEN IN CRS-FLD
                GO TO FIN-VAL-FLD-CRS.
           PERFORM VAL-CRS-VAL-VCCP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CRS-VAL-VCCP-LEN IN CRS-FLD
                GO TO FIN-VAL-FLD-CRS.
           PERFORM VAL-CRS-VAL-PAGO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CRS-VAL-PAGO-LEN IN CRS-FLD
                GO TO FIN-VAL-FLD-CRS.
