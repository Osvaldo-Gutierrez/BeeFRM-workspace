      *
      * COLBQTOC
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:58 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "TOC"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMTOC.TXT'
       SET-TAG-COLBQTOC SECTION.
       INI-SET-TAG-COLBQTOC.
           MOVE
           '<IDNMOD NOM=$COLBQTOC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:58 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQTOC.
           EXIT.
           
       PUT-ALL-TOC SECTION.
       INI-PUT-ALL-TOC.
*% INCLUDE INI_PUT_ALL_TOC OF 'COLBMTOC.TXT'
           PERFORM PUT-TOC-TOC.
           PERFORM PUT-TIO-TOC.
*% INCLUDE FIN_PUT_ALL_TOC OF 'COLBMTOC.TXT'
       FIN-PUT-ALL-TOC.
           EXIT.
       
       PUT-TOC-TOC SECTION.
       INI-PUT-TOC-TOC.
*% INCLUDE INI_PUT_TOC_TOC OF 'COLBMTOC.TXT'
           IF PGM-STAT-TOC-NUL
               GO TO FIN-PUT-TOC-TOC.
           IF NOT PGM-STAT-TOC-OKS
               PERFORM CLR-TOC-TOC
               GO TO FIN-PUT-TOC-TOC.
           MOVE TOC-COD-TOCR IN TOC TO TOC-COD-TOCR IN TOC-FLD.
           MOVE TOC-MSC-STAT IN TOC TO TOC-MSC-STAT IN TOC-FLD.
           MOVE TOC-MSC-OPEC IN TOC TO TOC-MSC-OPEC IN TOC-FLD.
*% RREG = "TOC"
*% RREG_FRM = "TOC"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE TOC-NUM-DTRN IN TOC TO TOC-NUM-DTRN IN TOC-FLD.
           MOVE TOC-NUM-MTRN IN TOC TO TOC-NUM-MTRN IN TOC-FLD.
           MOVE TOC-NUM-STRN IN TOC TO TOC-NUM-STRN IN TOC-FLD.
           MOVE TOC-NUM-ATRN IN TOC TO TOC-NUM-ATRN IN TOC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE TOC-MSC-TCOL IN TOC TO TOC-MSC-TCOL IN TOC-FLD.
           MOVE TOC-COD-RECU IN TOC TO TOC-COD-RECU IN TOC-FLD.
*% BUF = "TOC-FLD"
*% SIS = "COL"
*% TAB = "OPEC"
*% FLD = "TOC-MSC-OPEC"
*% GLS = "FRM-GLS-OPEC"
*% TIENE_GLOSA = TRUE
*% LGL = 34
*% INCLUDE 'BUSMSC'
*% BUF = "TOC-FLD"
*% SIS = "COL"
*% TAB = "TCOL"
*% FLD = "TOC-MSC-TCOL"
*% GLS = "FRM-GLS-TCOL"
*% TIENE_GLOSA = TRUE
*% LGL = 34
*% INCLUDE 'BUSMSC'
*% INCLUDE FIN_PUT_TOC_TOC OF 'COLBMTOC.TXT'
       FIN-PUT-TOC-TOC.
           EXIT.
       
       PUT-TIO-TOC SECTION.
       INI-PUT-TIO-TOC.
*% INCLUDE INI_PUT_TIO_TOC OF 'COLBMTOC.TXT'
           IF PGM-STAT-TIO-NUL
               GO TO FIN-PUT-TIO-TOC.
           IF NOT PGM-STAT-TIO-OKS
               PERFORM CLR-TIO-TOC
               GO TO FIN-PUT-TIO-TOC.
           MOVE TIO-CIC-TIOP IN TIO TO TIO-CIC-TIOP IN TOC-FLD.
           MOVE TIO-EXT-CTAB IN TIO TO TIO-EXT-CTAB IN TOC-FLD.
           MOVE TIO-GLS-DESC IN TIO TO TIO-GLS-DESC IN TOC-FLD.
           MOVE TIO-GLS-DCOR IN TIO TO TIO-GLS-DCOR IN TOC-FLD.
           MOVE TIO-GLS-ABRV IN TIO TO TIO-GLS-ABRV IN TOC-FLD.
*% INCLUDE FIN_PUT_TIO_TOC OF 'COLBMTOC.TXT'
       FIN-PUT-TIO-TOC.
           EXIT.
       
       KEY-ALL-TOC SECTION.
       INI-KEY-ALL-TOC.
*% INCLUDE INI_KEY_ALL_TOC OF 'COLBMTOC.TXT'
           PERFORM KEY-TOC-TOC.
           PERFORM KEY-TIO-TOC.
*% INCLUDE FIN_KEY_ALL_TOC OF 'COLBMTOC.TXT'
       FIN-KEY-ALL-TOC.
           EXIT.
       
       KEY-TOC-TOC SECTION.
       INI-KEY-TOC-TOC.
*% INCLUDE INI_KEY_TOC_TOC OF 'COLBMTOC.TXT'
           MOVE TOC-COD-TOCR IN TOC-FLD TO TOC-COD-TOCR IN TOC.
*% INCLUDE FIN_KEY_TOC_TOC OF 'COLBMTOC.TXT'
       FIN-KEY-TOC-TOC.
           EXIT.
       
       KEY-TIO-TOC SECTION.
       INI-KEY-TIO-TOC.
*% INCLUDE INI_KEY_TIO_TOC OF 'COLBMTOC.TXT'
*% INCLUDE FIN_KEY_TIO_TOC OF 'COLBMTOC.TXT'
       FIN-KEY-TIO-TOC.
           EXIT.
       
       SCR-CLRA-TOC SECTION.
       INI-SCR-CLRA-TOC.
*% INCLUDE INI_SCR_CLRA_TOC OF 'COLBMTOC.TXT'
           PERFORM CLR-KEY-TOC.
           PERFORM CLR-FLD-TOC.
*% INCLUDE FIN_SCR_CLRA_TOC OF 'COLBMTOC.TXT'
       FIN-SCR-CLRA-TOC.
           EXIT.
       
       CLR-KEY-TOC SECTION.
       INI-CLR-KEY-TOC.
*% INCLUDE INI_CLR_KEY_TOC OF 'COLBMTOC.TXT'
           MOVE SPACES TO TOC-COD-TOCR IN TOC-FLD.
*% INCLUDE FIN_CLR_KEY_TOC OF 'COLBMTOC.TXT'
       FIN-CLR-KEY-TOC.
           EXIT.
       
       CLR-FLD-TOC SECTION.
       INI-CLR-FLD-TOC.
*% INCLUDE INI_CLR_FLD_TOC OF 'COLBMTOC.TXT'
           MOVE SPACES TO FRM-GLS-OPEC IN TOC-FLD.
           MOVE SPACES TO FRM-GLS-TCOL IN TOC-FLD.
           MOVE SPACES TO FRM-GLS-RECU IN TOC-FLD.
           PERFORM CLR-TOC-TOC.
           PERFORM CLR-TIO-TOC.
*% INCLUDE FIN_CLR_FLD_TOC OF 'COLBMTOC.TXT'
       FIN-CLR-FLD-TOC.
           EXIT.
       
       CLR-TOC-TOC SECTION.
       INI-CLR-TOC-TOC.
*% INCLUDE INI_CLR_TOC_TOC OF 'COLBMTOC.TXT'
           MOVE SPACES TO TOC-MSC-STAT IN TOC-FLD.
           MOVE SPACES TO TOC-MSC-OPEC IN TOC-FLD.
*% RREG = "TOC"
*% RREG_FRM = "TOC"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO TOC-FEC-FTRN IN TOC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO TOC-MSC-TCOL IN TOC-FLD.
           MOVE SPACES TO TOC-COD-RECU IN TOC-FLD.
*% INCLUDE FIN_CLR_TOC_TOC OF 'COLBMTOC.TXT'
       FIN-CLR-TOC-TOC.
           EXIT.
       
       CLR-TIO-TOC SECTION.
       INI-CLR-TIO-TOC.
*% INCLUDE INI_CLR_TIO_TOC OF 'COLBMTOC.TXT'
           MOVE SPACES TO TIO-CIC-TIOP IN TOC-FLD.
           MOVE SPACES TO TIO-EXT-CTAB IN TOC-FLD.
           MOVE SPACES TO TIO-GLS-DESC IN TOC-FLD.
           MOVE SPACES TO TIO-GLS-DCOR IN TOC-FLD.
           MOVE SPACES TO TIO-GLS-ABRV IN TOC-FLD.
*% INCLUDE FIN_CLR_TIO_TOC OF 'COLBMTOC.TXT'
       FIN-CLR-TIO-TOC.
           EXIT.
       
       FST-KEY-TOC SECTION.
       INI-FST-KEY-TOC.
*% INCLUDE INI_FST_KEY_TOC OF 'COLBMTOC.TXT'
           MOVE -1 TO TOC-COD-TOCR-LEN IN TOC-FLD.
*% INCLUDE FIN_FST_KEY_TOC OF 'COLBMTOC.TXT'
       FIN-FST-KEY-TOC.
           EXIT.
       
       FST-FLD-TOC SECTION.
       INI-FST-FLD-TOC.
*% INCLUDE INI_FST_FLD_TOC OF 'COLBMTOC.TXT'
           MOVE -1 TO TOC-MSC-STAT-LEN IN TOC-FLD.
*% INCLUDE FIN_FST_FLD_TOC OF 'COLBMTOC.TXT'
       FIN-FST-FLD-TOC.
           EXIT.
       
       PRO-KEY-TOC SECTION.
       INI-PRO-KEY-TOC.
*% INCLUDE INI_PRO_KEY_TOC OF 'COLBMTOC.TXT'
           PERFORM PRO-IKY-TOC.
           PERFORM PRO-AKY-TOC.
           PERFORM PRO-MKY-TOC.
*% INCLUDE FIN_PRO_KEY_TOC OF 'COLBMTOC.TXT'
       FIN-PRO-KEY-TOC.
           EXIT.
       
       PRO-IKY-TOC SECTION.
       INI-PRO-IKY-TOC.
*% INCLUDE INI_PRO_IKY_TOC OF 'COLBMTOC.TXT'
           MOVE FRM-CPIM-AEY TO TOC-COD-TOCR-ATR IN TOC-FLD.
*% INCLUDE FIN_PRO_IKY_TOC OF 'COLBMTOC.TXT'
       FIN-PRO-IKY-TOC.
           EXIT.
       
       PRO-MKY-TOC SECTION.
       INI-PRO-MKY-TOC.
*% INCLUDE INI_PRO_MKY_TOC OF 'COLBMTOC.TXT'
*% INCLUDE FIN_PRO_MKY_TOC OF 'COLBMTOC.TXT'
       FIN-PRO-MKY-TOC.
           EXIT.
       
       PRO-AKY-TOC SECTION.
       INI-PRO-AKY-TOC.
*% INCLUDE INI_PRO_AKY_TOC OF 'COLBMTOC.TXT'
*% INCLUDE FIN_PRO_AKY_TOC OF 'COLBMTOC.TXT'
       FIN-PRO-AKY-TOC.
           EXIT.
       
       UNP-KEY-TOC SECTION.
       INI-UNP-KEY-TOC.
*% INCLUDE INI_UNP_KEY_TOC OF 'COLBMTOC.TXT'
           PERFORM UNP-IKY-TOC.
           PERFORM UNP-AKY-TOC.
           PERFORM UNP-MKY-TOC.
*% INCLUDE FIN_UNP_KEY_TOC OF 'COLBMTOC.TXT'
       FIN-UNP-KEY-TOC.
           EXIT.
       
       UNP-IKY-TOC SECTION.
       INI-UNP-IKY-TOC.
*% INCLUDE INI_UNP_IKY_TOC OF 'COLBMTOC.TXT'
           MOVE FRM-CPIM-UBY TO TOC-COD-TOCR-ATR.
*% INCLUDE FIN_UNP_IKY_TOC OF 'COLBMTOC.TXT'
       FIN-UNP-IKY-TOC.
           EXIT.
       
       UNP-MKY-TOC SECTION.
       INI-UNP-MKY-TOC.
*% INCLUDE INI_UNP_MKY_TOC OF 'COLBMTOC.TXT'
*% INCLUDE FIN_UNP_MKY_TOC OF 'COLBMTOC.TXT'
       FIN-UNP-MKY-TOC.
           EXIT.
       
       UNP-AKY-TOC SECTION.
       INI-UNP-AKY-TOC.
*% INCLUDE INI_UNP_AKY_TOC OF 'COLBMTOC.TXT'
*% INCLUDE FIN_UNP_AKY_TOC OF 'COLBMTOC.TXT'
       FIN-UNP-AKY-TOC.
           EXIT.
       
       UNP-FLD-TOC SECTION.
       INI-UNP-FLD-TOC.
*% INCLUDE INI_UNP_FLD_TOC OF 'COLBMTOC.TXT'
           PERFORM UNP-TOC-TOC.
           PERFORM UNP-TIO-TOC.
*% INCLUDE FIN_UNP_FLD_TOC OF 'COLBMTOC.TXT'
       FIN-UNP-FLD-TOC.
           EXIT.
       
       UNP-TOC-TOC SECTION.
       INI-UNP-TOC-TOC.
*% INCLUDE INI_UNP_TOC_TOC OF 'COLBMTOC.TXT'
           MOVE FRM-CPIM-UBY TO TOC-MSC-STAT-ATR IN TOC-FLD.
           MOVE FRM-CPIM-UBY TO TOC-MSC-OPEC-ATR IN TOC-FLD.
           MOVE FRM-CPIM-UBY TO TOC-MSC-TCOL-ATR IN TOC-FLD.
           MOVE FRM-CPIM-UBY TO TOC-COD-RECU-ATR IN TOC-FLD.
*% INCLUDE FIN_UNP_TOC_TOC OF 'COLBMTOC.TXT'
       FIN-UNP-TOC-TOC.
           EXIT.
       
       UNP-TIO-TOC SECTION.
       INI-UNP-TIO-TOC.
*% INCLUDE INI_UNP_TIO_TOC OF 'COLBMTOC.TXT'
*% INCLUDE FIN_UNP_TIO_TOC OF 'COLBMTOC.TXT'
       FIN-UNP-TIO-TOC.
           EXIT.
       
       VAL-KEY-TOC SECTION.
       INI-VAL-KEY-TOC.
           PERFORM VAL-IKY-TOC.
           PERFORM VAL-MKY-TOC.
           PERFORM VAL-AKY-TOC.
       FIN-VAL-KEY-TOC.
           EXIT.
       
       VAL-IKY-TOC SECTION.
       INI-VAL-IKY-TOC.
*% INCLUDE INI_VAL_IKY_TOC OF 'COLBMTOC.TXT'
           PERFORM VAL-TOC-COD-TOCR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TOC-COD-TOCR-LEN IN TOC-FLD
                GO TO FIN-VAL-IKY-TOC.
*% INCLUDE FIN_VAL_IKY_TOC OF 'COLBMTOC.TXT'
       FIN-VAL-IKY-TOC.
           EXIT.
       
       VAL-MKY-TOC SECTION.
       INI-VAL-MKY-TOC.
*% INCLUDE INI_VAL_MKY_TOC OF 'COLBMTOC.TXT'
*% INCLUDE FIN_VAL_MKY_TOC OF 'COLBMTOC.TXT'
       FIN-VAL-MKY-TOC.
           EXIT.
       
       VAL-AKY-TOC SECTION.
       INI-VAL-AKY-TOC.
*% INCLUDE INI_VAL_AKY_TOC OF 'COLBMTOC.TXT'
*% INCLUDE FIN_VAL_AKY_TOC OF 'COLBMTOC.TXT'
       FIN-VAL-AKY-TOC.
           EXIT.
       
       VAL-TOC-COD-TOCR SECTION.
       INI-VAL-TOC-COD-TOCR.
*% LBL = "TOC_COD_TOCR OF 'COLBMTOC.TXT'"
*% INCLUDE INI_TOC_COD_TOCR OF 'COLBMTOC.TXT'
           IF TOC-COD-TOCR IN TOC-FLD NOT > SPACES
              MOVE SPACES TO TOC-COD-TOCR IN TOC-FLD.
*% INCLUDE TOC_COD_TOCR OF 'COLBMTOC.TXT'
*% BUF = "TOC-FLD"
*% SIS = "TAB"
*% TAB = "TIO "
*% FLD = "TOC-COD-TOCR"
*% GLS = ""
*% TIENE_GLOSA = FALSE
*% LGL = 0
*% INCLUDE 'VALCOD'
*% INCLUDE FIN_TOC_COD_TOCR OF 'COLBMTOC.TXT'
       FIN-VAL-TOC-COD-TOCR.
           EXIT.
*% RREG = "TOC"
*% RREG_FRM = "TOC"
*% NOMB = "FTRN"
*% IZR = "S"
       
       VAL-CON-KEY-TOC SECTION.
       INI-VAL-CON-KEY-TOC.
*% INCLUDE INI_VAL_CON_KEY_TOC OF 'COLBMTOC.TXT'
           IF     TOC-COD-TOCR IN TOC-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-TOC.
*% INCLUDE FIN_VAL_CON_KEY_TOC OF 'COLBMTOC.TXT'
       FIN-VAL-CON-KEY-TOC.
           EXIT.
       
       VAL-CON-IKY-TOC SECTION.
       INI-VAL-CON-IKY-TOC.
*% INCLUDE INI_VAL_CON_IKY_TOC OF 'COLBMTOC.TXT'
           IF     TOC-COD-TOCR IN TOC-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-TOC.
*% INCLUDE FIN_VAL_CON_IKY_TOC OF 'COLBMTOC.TXT'
       FIN-VAL-CON-IKY-TOC.
           EXIT.
       
       VAL-NUM-KEY-TOC SECTION.
       INI-VAL-NUM-KEY-TOC.
           PERFORM VAL-NUM-IKY-TOC.
           PERFORM VAL-NUM-MKY-TOC.
           PERFORM VAL-NUM-AKY-TOC.
       FIN-VAL-NUM-KEY-TOC.
           EXIT.
       
       VAL-NUM-IKY-TOC SECTION.
       INI-VAL-NUM-IKY-TOC.
*% INCLUDE INI_VAL_NUM_IKY_TOC OF 'COLBMTOC.TXT'
*% INCLUDE FIN_VAL_NUM_IKY_TOC OF 'COLBMTOC.TXT'
       FIN-VAL-NUM-IKY-TOC.
           EXIT.
       
       VAL-NUM-MKY-TOC SECTION.
       INI-VAL-NUM-MKY-TOC.
*% INCLUDE INI_VAL_NUM_MKY_TOC OF 'COLBMTOC.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_TOC OF 'COLBMTOC.TXT'
       FIN-VAL-NUM-MKY-TOC.
           EXIT.
       
       VAL-NUM-AKY-TOC SECTION.
       INI-VAL-NUM-AKY-TOC.
*% INCLUDE INI_VAL_NUM_AKY_TOC OF 'COLBMTOC.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_TOC OF 'COLBMTOC.TXT'
       FIN-VAL-NUM-AKY-TOC.
           EXIT.
       
       VAL-NUM-FLD-TOC SECTION.
       INI-VAL-NUM-FLD-TOC.
*% INCLUDE INI_VAL_NUM_FLD_TOC OF 'COLBMTOC.TXT'
*% INCLUDE FIN_VAL_NUM_FLD_TOC OF 'COLBMTOC.TXT'
       FIN-VAL-NUM-FLD-TOC.
           EXIT.
       
       EDT-KEY-TOC SECTION.
       INI-EDT-KEY-TOC.
           PERFORM EDT-IKY-TOC.
           PERFORM EDT-MKY-TOC.
           PERFORM EDT-AKY-TOC.
*% INCLUDE EDT_KEY_TOC OF 'COLBMTOC.TXT'
       FIN-EDT-KEY-TOC.
           EXIT.
       
       EDT-IKY-TOC SECTION.
       INI-EDT-IKY-TOC.
*% INCLUDE EDT_IKY_TOC OF 'COLBMTOC.TXT'
*% INCLUDE FIN_EDT_IKY_TOC OF 'COLBMTOC.TXT'
       FIN-EDT-IKY-TOC.
           EXIT.
       
       EDT-MKY-TOC SECTION.
       INI-EDT-MKY-TOC.
*% INCLUDE EDT_MKY_TOC OF 'COLBMTOC.TXT'
*% INCLUDE FIN_EDT_MKY_TOC OF 'COLBMTOC.TXT'
       FIN-EDT-MKY-TOC.
           EXIT.
       
       EDT-AKY-TOC SECTION.
       INI-EDT-AKY-TOC.
*% INCLUDE EDT_AKY_TOC OF 'COLBMTOC.TXT'
*% INCLUDE FIN_EDT_AKY_TOC OF 'COLBMTOC.TXT'
       FIN-EDT-AKY-TOC.
           EXIT.
       
       EDT-FLD-TOC SECTION.
       INI-EDT-FLD-TOC.
*% INCLUDE EDT_FLD_TOC OF 'COLBMTOC.TXT'
*% RREG = "TOC"
*% RREG_FRM = "TOC"
*% NOMB = "FTRN"
*% INCLUDE FIN_EDT_FLD_TOC OF 'COLBMTOC.TXT'
       FIN-EDT-FLD-TOC.
           EXIT.
*% INCLUDE EOF_BQ OF 'COLBMTOC.TXT'
*% INCLUDE EOF OF 'COLBMTOC.TXT'
*% INCLUDE EOF_IBM OF 'COLBMTOC.TXT'
