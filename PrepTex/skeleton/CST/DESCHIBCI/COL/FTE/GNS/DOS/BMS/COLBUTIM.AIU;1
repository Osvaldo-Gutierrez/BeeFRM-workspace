*% FRM = "TIM"
      *
      * COLBUTIM
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:58 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMTIM.TXT'
       SET-TAG-COLBUTIM SECTION.
       INI-SET-TAG-COLBUTIM.
           MOVE
           '<IDNMOD NOM=$COLBUTIM$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:58 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUTIM.
           EXIT.
           
       RET-ALL-TIM SECTION.
       INI-RET-ALL-TIM.
*% INCLUDE INI_RET_ALL_TIM OF 'COLBMTIM.TXT'
           PERFORM KEY-ALL-TIM.
           PERFORM RET-TIM-TIM.
*% INCLUDE FIN_RET_ALL_TIM OF 'COLBMTIM.TXT'
       FIN-RET-ALL-TIM.
           EXIT.
       
       RET-TIM-TIM SECTION.
       INI-RET-TIM-TIM.
*% INCLUDE INI_RET_TIM_TIM OF 'COLBMTIM.TXT'
           MOVE TIM-CIC-CTAB IN TIM-FLD TO TIM-CIC-CTAB IN TIM.
           MOVE TIM-EXT-CTAB IN TIM-FLD TO TIM-EXT-CTAB IN TIM.
           MOVE TIM-GLS-DESC IN TIM-FLD TO TIM-GLS-DESC IN TIM.
           MOVE TIM-GLS-ABRV IN TIM-FLD TO TIM-GLS-ABRV IN TIM.
           MOVE TIM-GLS-DCOR IN TIM-FLD TO TIM-GLS-DCOR IN TIM.
           MOVE TIM-IND-VIGE IN TIM-FLD TO TIM-IND-VIGE IN TIM.
           MOVE TIM-SGV-TIM1 IN TIM-FLD TO TIM-SGV-TIM1 IN TIM.
           MOVE TIM-SGV-TIM2 IN TIM-FLD TO TIM-SGV-TIM2 IN TIM.
*% INCLUDE FIN_RET_TIM_TIM OF 'COLBMTIM.TXT'
       FIN-RET-TIM-TIM.
           EXIT.
       
       KEY-ALL-TIM SECTION.
       INI-KEY-ALL-TIM.
*% INCLUDE INI_KEY_ALL_TIM OF 'COLBMTIM.TXT'
           PERFORM KEY-TIM-TIM.
*% INCLUDE FIN_KEY_ALL_TIM OF 'COLBMTIM.TXT'
       FIN-KEY-ALL-TIM.
           EXIT.
       
       KEY-TIM-TIM SECTION.
       INI-KEY-TIM-TIM.
*% INCLUDE INI_KEY_TIM_TIM OF 'COLBMTIM.TXT'
           MOVE TIM-COD-CTAB IN TIM-FLD TO TIM-COD-CTAB IN TIM.
*% INCLUDE FIN_KEY_TIM_TIM OF 'COLBMTIM.TXT'
       FIN-KEY-TIM-TIM.
           EXIT.
       
       SCR-CLRA-TIM SECTION.
       INI-SCR-CLRA-TIM.
*% INCLUDE INI_SCR_CLRA_TIM OF 'COLBMTIM.TXT'
           PERFORM CLR-KEY-TIM.
           PERFORM CLR-FLD-TIM.
*% INCLUDE FIN_SCR_CLRA_TIM OF 'COLBMTIM.TXT'
       FIN-SCR-CLRA-TIM.
           EXIT.
       
       CLR-KEY-TIM SECTION.
       INI-CLR-KEY-TIM.
*% INCLUDE INI_CLR_KEY_TIM OF 'COLBMTIM.TXT'
           MOVE SPACES TO TIM-COD-CTAB IN TIM-FLD.
*% INCLUDE FIN_CLR_KEY_TIM OF 'COLBMTIM.TXT'
       FIN-CLR-KEY-TIM.
           EXIT.
       
       CLR-FLD-TIM SECTION.
       INI-CLR-FLD-TIM.
*% INCLUDE INI_CLR_FLD_TIM OF 'COLBMTIM.TXT'
           PERFORM CLR-TIM-TIM.
*% INCLUDE FIN_CLR_FLD_TIM OF 'COLBMTIM.TXT'
       FIN-CLR-FLD-TIM.
           EXIT.
       
       CLR-TIM-TIM SECTION.
       INI-CLR-TIM-TIM.
*% INCLUDE INI_CLR_TIM_TIM OF 'COLBMTIM.TXT'
           MOVE SPACES TO TIM-CIC-CTAB IN TIM-FLD.
           MOVE SPACES TO TIM-EXT-CTAB IN TIM-FLD.
           MOVE SPACES TO TIM-GLS-DESC IN TIM-FLD.
           MOVE SPACES TO TIM-GLS-ABRV IN TIM-FLD.
           MOVE SPACES TO TIM-GLS-DCOR IN TIM-FLD.
           MOVE SPACES TO TIM-IND-VIGE IN TIM-FLD.
           MOVE ZEROES TO TIM-SGV-TIM1 IN TIM-FLD.
           MOVE ZEROES TO TIM-SGV-TIM2 IN TIM-FLD.
*% INCLUDE FIN_CLR_TIM_TIM OF 'COLBMTIM.TXT'
       FIN-CLR-TIM-TIM.
           EXIT.
       
       FST-KEY-TIM SECTION.
       INI-FST-KEY-TIM.
*% INCLUDE INI_FST_KEY_TIM OF 'COLBMTIM.TXT'
           MOVE -1 TO TIM-COD-CTAB-LEN IN TIM-FLD.
*% INCLUDE FIN_FST_KEY_TIM OF 'COLBMTIM.TXT'
       FIN-FST-KEY-TIM.
           EXIT.
       
       FST-FLD-TIM SECTION.
       INI-FST-FLD-TIM.
*% INCLUDE INI_FST_FLD_TIM OF 'COLBMTIM.TXT'
           MOVE -1 TO TIM-CIC-CTAB-LEN IN TIM-FLD.
*% INCLUDE FIN_FST_FLD_TIM OF 'COLBMTIM.TXT'
       FIN-FST-FLD-TIM.
           EXIT.
       
       PRO-KEY-TIM SECTION.
       INI-PRO-KEY-TIM.
*% INCLUDE INI_PRO_KEY_TIM OF 'COLBMTIM.TXT'
           PERFORM PRO-MKY-TIM.
*% INCLUDE FIN_PRO_KEY_TIM OF 'COLBMTIM.TXT'
       FIN-PRO-KEY-TIM.
           EXIT.
       
       PRO-MKY-TIM SECTION.
       INI-PRO-MKY-TIM.
*% INCLUDE INI_PRO_MKY_TIM OF 'COLBMTIM.TXT'
*% INCLUDE FIN_PRO_MKY_TIM OF 'COLBMTIM.TXT'
       FIN-PRO-MKY-TIM.
           EXIT.
       
       PRO-FLD-TIM SECTION.
       INI-PRO-FLD-TIM.
*% INCLUDE INI_PRO_FLD_TIM OF 'COLBMTIM.TXT'
           PERFORM PRO-TIM-TIM.
*% INCLUDE FIN_PRO_FLD_TIM OF 'COLBMTIM.TXT'
       FIN-PRO-FLD-TIM.
           EXIT.
       
       PRO-TIM-TIM SECTION.
       INI-PRO-TIM-TIM.
*% INCLUDE INI_PRO_TIM_TIM OF 'COLBMTIM.TXT'
           MOVE FRM-CPIM-AEY TO TIM-CIC-CTAB-ATR IN TIM-FLD.
           MOVE FRM-CPIM-AEY TO TIM-EXT-CTAB-ATR IN TIM-FLD.
           MOVE FRM-CPIM-AEY TO TIM-GLS-DESC-ATR IN TIM-FLD.
           MOVE FRM-CPIM-AEY TO TIM-GLS-ABRV-ATR IN TIM-FLD.
           MOVE FRM-CPIM-AEY TO TIM-GLS-DCOR-ATR IN TIM-FLD.
           MOVE FRM-CPIM-AEY TO TIM-IND-VIGE-ATR IN TIM-FLD.
           MOVE FRM-CPIM-AEY TO TIM-SGV-TIM1-ATR IN TIM-FLD.
           MOVE FRM-CPIM-AEY TO TIM-SGV-TIM2-ATR IN TIM-FLD.
*% INCLUDE FIN_PRO_TIM_TIM OF 'COLBMTIM.TXT'
       FIN-PRO-TIM-TIM.
           EXIT.
       
       UNP-KEY-TIM SECTION.
       INI-UNP-KEY-TIM.
*% INCLUDE INI_UNP_KEY_TIM OF 'COLBMTIM.TXT'
           PERFORM UNP-IKY-TIM.
           PERFORM UNP-AKY-TIM.
           PERFORM UNP-MKY-TIM.
*% INCLUDE FIN_UNP_KEY_TIM OF 'COLBMTIM.TXT'
       FIN-UNP-KEY-TIM.
           EXIT.
       
       UNP-IKY-TIM SECTION.
       INI-UNP-IKY-TIM.
*% INCLUDE INI_UNP_IKY_TIM OF 'COLBMTIM.TXT'
           MOVE FRM-CPIM-UBY TO TIM-COD-CTAB-ATR.
*% INCLUDE FIN_UNP_IKY_TIM OF 'COLBMTIM.TXT'
       FIN-UNP-IKY-TIM.
           EXIT.
       
       UNP-MKY-TIM SECTION.
       INI-UNP-MKY-TIM.
*% INCLUDE INI_UNP_MKY_TIM OF 'COLBMTIM.TXT'
*% INCLUDE FIN_UNP_MKY_TIM OF 'COLBMTIM.TXT'
       FIN-UNP-MKY-TIM.
           EXIT.
       
       UNP-AKY-TIM SECTION.
       INI-UNP-AKY-TIM.
*% INCLUDE INI_UNP_AKY_TIM OF 'COLBMTIM.TXT'
*% INCLUDE FIN_UNP_AKY_TIM OF 'COLBMTIM.TXT'
       FIN-UNP-AKY-TIM.
           EXIT.
       
       UNP-FLD-TIM SECTION.
       INI-UNP-FLD-TIM.
*% INCLUDE INI_UNP_FLD_TIM OF 'COLBMTIM.TXT'
           PERFORM UNP-TIM-TIM.
*% INCLUDE FIN_UNP_FLD_TIM OF 'COLBMTIM.TXT'
       FIN-UNP-FLD-TIM.
           EXIT.
       
       UNP-TIM-TIM SECTION.
       INI-UNP-TIM-TIM.
*% INCLUDE INI_UNP_TIM_TIM OF 'COLBMTIM.TXT'
           MOVE FRM-CPIM-UBY TO TIM-CIC-CTAB-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-EXT-CTAB-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-GLS-DESC-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-GLS-ABRV-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-GLS-DCOR-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-IND-VIGE-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-SGV-TIM1-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-SGV-TIM2-ATR IN TIM-FLD.
*% INCLUDE FIN_UNP_TIM_TIM OF 'COLBMTIM.TXT'
       FIN-UNP-TIM-TIM.
           EXIT.
       
       VAL-MKY-TIM SECTION.
       INI-VAL-MKY-TIM.
*% INCLUDE INI_VAL_MKY_TIM OF 'COLBMTIM.TXT'
*% INCLUDE FIN_VAL_MKY_TIM OF 'COLBMTIM.TXT'
       FIN-VAL-MKY-TIM.
           EXIT.
       
       VAL-FLD-TIM SECTION.
       INI-VAL-FLD-TIM.
*% INCLUDE INI_VAL_FLD_TIM OF 'COLBMTIM.TXT'
           PERFORM VAL-TIM-CIC-CTAB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TIM-CIC-CTAB-LEN IN TIM-FLD
                GO TO FIN-VAL-FLD-TIM.
           PERFORM VAL-TIM-EXT-CTAB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TIM-EXT-CTAB-LEN IN TIM-FLD
                GO TO FIN-VAL-FLD-TIM.
           PERFORM VAL-TIM-GLS-DESC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TIM-GLS-DESC-LEN IN TIM-FLD
                GO TO FIN-VAL-FLD-TIM.
           PERFORM VAL-TIM-GLS-ABRV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TIM-GLS-ABRV-LEN IN TIM-FLD
                GO TO FIN-VAL-FLD-TIM.
           PERFORM VAL-TIM-GLS-DCOR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TIM-GLS-DCOR-LEN IN TIM-FLD
                GO TO FIN-VAL-FLD-TIM.
           PERFORM VAL-TIM-IND-VIGE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TIM-IND-VIGE-LEN IN TIM-FLD
                GO TO FIN-VAL-FLD-TIM.
           PERFORM VAL-TIM-SGV-TIM1.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TIM-SGV-TIM1-LEN IN TIM-FLD
                GO TO FIN-VAL-FLD-TIM.
           PERFORM VAL-TIM-SGV-TIM2.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TIM-SGV-TIM2-LEN IN TIM-FLD
                GO TO FIN-VAL-FLD-TIM.
*% INCLUDE FIN_VAL_FLD_TIM OF 'COLBMTIM.TXT'
       FIN-VAL-FLD-TIM.
           EXIT.
       
       VAL-TIM-CIC-CTAB SECTION.
       INI-VAL-TIM-CIC-CTAB.
*% LBL = "TIM_CIC_CTAB OF 'COLBMTIM.TXT'"
*% INCLUDE INI_TIM_CIC_CTAB OF 'COLBMTIM.TXT'
           IF TIM-CIC-CTAB IN TIM-FLD NOT > SPACES
              MOVE SPACES TO TIM-CIC-CTAB IN TIM-FLD.
*% INCLUDE TIM_CIC_CTAB OF 'COLBMTIM.TXT'
*% INCLUDE FIN_TIM_CIC_CTAB OF 'COLBMTIM.TXT'
       FIN-VAL-TIM-CIC-CTAB.
           EXIT.
       
       VAL-TIM-EXT-CTAB SECTION.
       INI-VAL-TIM-EXT-CTAB.
*% LBL = "TIM_EXT_CTAB OF 'COLBMTIM.TXT'"
*% INCLUDE INI_TIM_EXT_CTAB OF 'COLBMTIM.TXT'
           IF TIM-EXT-CTAB IN TIM-FLD NOT > SPACES
              MOVE SPACES TO TIM-EXT-CTAB IN TIM-FLD.
*% INCLUDE TIM_EXT_CTAB OF 'COLBMTIM.TXT'
*% INCLUDE FIN_TIM_EXT_CTAB OF 'COLBMTIM.TXT'
       FIN-VAL-TIM-EXT-CTAB.
           EXIT.
       
       VAL-TIM-GLS-DESC SECTION.
       INI-VAL-TIM-GLS-DESC.
*% LBL = "TIM_GLS_DESC OF 'COLBMTIM.TXT'"
*% INCLUDE INI_TIM_GLS_DESC OF 'COLBMTIM.TXT'
           IF TIM-GLS-DESC IN TIM-FLD NOT > SPACES
              MOVE SPACES TO TIM-GLS-DESC IN TIM-FLD.
*% INCLUDE TIM_GLS_DESC OF 'COLBMTIM.TXT'
*% INCLUDE FIN_TIM_GLS_DESC OF 'COLBMTIM.TXT'
       FIN-VAL-TIM-GLS-DESC.
           EXIT.
       
       VAL-TIM-GLS-ABRV SECTION.
       INI-VAL-TIM-GLS-ABRV.
*% LBL = "TIM_GLS_ABRV OF 'COLBMTIM.TXT'"
*% INCLUDE INI_TIM_GLS_ABRV OF 'COLBMTIM.TXT'
           IF TIM-GLS-ABRV IN TIM-FLD NOT > SPACES
              MOVE SPACES TO TIM-GLS-ABRV IN TIM-FLD.
*% INCLUDE TIM_GLS_ABRV OF 'COLBMTIM.TXT'
*% INCLUDE FIN_TIM_GLS_ABRV OF 'COLBMTIM.TXT'
       FIN-VAL-TIM-GLS-ABRV.
           EXIT.
       
       VAL-TIM-GLS-DCOR SECTION.
       INI-VAL-TIM-GLS-DCOR.
*% LBL = "TIM_GLS_DCOR OF 'COLBMTIM.TXT'"
*% INCLUDE INI_TIM_GLS_DCOR OF 'COLBMTIM.TXT'
           IF TIM-GLS-DCOR IN TIM-FLD NOT > SPACES
              MOVE SPACES TO TIM-GLS-DCOR IN TIM-FLD.
*% INCLUDE TIM_GLS_DCOR OF 'COLBMTIM.TXT'
*% INCLUDE FIN_TIM_GLS_DCOR OF 'COLBMTIM.TXT'
       FIN-VAL-TIM-GLS-DCOR.
           EXIT.
       
       VAL-TIM-IND-VIGE SECTION.
       INI-VAL-TIM-IND-VIGE.
*% LBL = "TIM_IND_VIGE OF 'COLBMTIM.TXT'"
*% INCLUDE INI_TIM_IND_VIGE OF 'COLBMTIM.TXT'
           IF TIM-IND-VIGE IN TIM-FLD NOT > SPACES
              MOVE SPACES TO TIM-IND-VIGE IN TIM-FLD.
*% INCLUDE TIM_IND_VIGE OF 'COLBMTIM.TXT'
*% INCLUDE FIN_TIM_IND_VIGE OF 'COLBMTIM.TXT'
       FIN-VAL-TIM-IND-VIGE.
           EXIT.
       
       VAL-TIM-SGV-TIM1 SECTION.
       INI-VAL-TIM-SGV-TIM1.
*% LBL = "TIM_SGV_TIM1 OF 'COLBMTIM.TXT'"
*% INCLUDE INI_TIM_SGV_TIM1 OF 'COLBMTIM.TXT'
*% INCLUDE TIM_SGV_TIM1 OF 'COLBMTIM.TXT'
*% INCLUDE FIN_TIM_SGV_TIM1 OF 'COLBMTIM.TXT'
       FIN-VAL-TIM-SGV-TIM1.
           EXIT.
       
       VAL-TIM-SGV-TIM2 SECTION.
       INI-VAL-TIM-SGV-TIM2.
*% LBL = "TIM_SGV_TIM2 OF 'COLBMTIM.TXT'"
*% INCLUDE INI_TIM_SGV_TIM2 OF 'COLBMTIM.TXT'
*% INCLUDE TIM_SGV_TIM2 OF 'COLBMTIM.TXT'
*% INCLUDE FIN_TIM_SGV_TIM2 OF 'COLBMTIM.TXT'
       FIN-VAL-TIM-SGV-TIM2.
           EXIT.
       
       VAL-CON-FLD-TIM SECTION.
       INI-VAL-CON-FLD-TIM.
*% INCLUDE VAL_CON_FLD_TIM OF 'COLBMTIM.TXT'
       FIN-VAL-CON-FLD-TIM.
           EXIT.
       
       VAL-CON-TIM SECTION.
       INI-VAL-CON-TIM.
*% INCLUDE VAL_CON_TIM OF 'COLBMTIM.TXT'
       FIN-VAL-CON-TIM.
           EXIT.
       
       VAL-NUM-KEY-TIM SECTION.
       INI-VAL-NUM-KEY-TIM.
           PERFORM VAL-NUM-IKY-TIM.
           PERFORM VAL-NUM-MKY-TIM.
           PERFORM VAL-NUM-AKY-TIM.
       FIN-VAL-NUM-KEY-TIM.
           EXIT.
       
       VAL-NUM-IKY-TIM SECTION.
       INI-VAL-NUM-IKY-TIM.
*% INCLUDE INI_VAL_NUM_IKY_TIM OF 'COLBMTIM.TXT'
*% INCLUDE FIN_VAL_NUM_IKY_TIM OF 'COLBMTIM.TXT'
       FIN-VAL-NUM-IKY-TIM.
           EXIT.
       
       VAL-NUM-MKY-TIM SECTION.
       INI-VAL-NUM-MKY-TIM.
*% INCLUDE INI_VAL_NUM_MKY_TIM OF 'COLBMTIM.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_TIM OF 'COLBMTIM.TXT'
       FIN-VAL-NUM-MKY-TIM.
           EXIT.
       
       VAL-NUM-AKY-TIM SECTION.
       INI-VAL-NUM-AKY-TIM.
*% INCLUDE INI_VAL_NUM_AKY_TIM OF 'COLBMTIM.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_TIM OF 'COLBMTIM.TXT'
       FIN-VAL-NUM-AKY-TIM.
           EXIT.
       
       VAL-NUM-FLD-TIM SECTION.
       INI-VAL-NUM-FLD-TIM.
*% INCLUDE INI_VAL_NUM_FLD_TIM OF 'COLBMTIM.TXT'
       VAL-NUM-TIM-SGV-TIM1.
*% BUF = "TIM-FLD"
*% FLD = "TIM-SGV-TIM1"
*% ENT = "3"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-TIM.
       VAL-NUM-TIM-SGV-TIM2.
*% BUF = "TIM-FLD"
*% FLD = "TIM-SGV-TIM2"
*% ENT = "7"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-TIM.
*% INCLUDE FIN_VAL_NUM_FLD_TIM OF 'COLBMTIM.TXT'
       FIN-VAL-NUM-FLD-TIM.
           EXIT.
       
       EDT-KEY-TIM SECTION.
       INI-EDT-KEY-TIM.
           PERFORM EDT-IKY-TIM.
           PERFORM EDT-MKY-TIM.
           PERFORM EDT-AKY-TIM.
*% INCLUDE EDT_KEY_TIM OF 'COLBMTIM.TXT'
       FIN-EDT-KEY-TIM.
           EXIT.
       
       EDT-IKY-TIM SECTION.
       INI-EDT-IKY-TIM.
*% INCLUDE EDT_IKY_TIM OF 'COLBMTIM.TXT'
*% INCLUDE FIN_EDT_IKY_TIM OF 'COLBMTIM.TXT'
       FIN-EDT-IKY-TIM.
           EXIT.
       
       EDT-MKY-TIM SECTION.
       INI-EDT-MKY-TIM.
*% INCLUDE EDT_MKY_TIM OF 'COLBMTIM.TXT'
*% INCLUDE FIN_EDT_MKY_TIM OF 'COLBMTIM.TXT'
       FIN-EDT-MKY-TIM.
           EXIT.
       
       EDT-AKY-TIM SECTION.
       INI-EDT-AKY-TIM.
*% INCLUDE EDT_AKY_TIM OF 'COLBMTIM.TXT'
*% INCLUDE FIN_EDT_AKY_TIM OF 'COLBMTIM.TXT'
       FIN-EDT-AKY-TIM.
           EXIT.
       
       EDT-FLD-TIM SECTION.
       INI-EDT-FLD-TIM.
*% INCLUDE EDT_FLD_TIM OF 'COLBMTIM.TXT'
           IF TIM-SGV-TIM1-LEN IN TIM-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO TIM-SGV-TIM1-LEN IN TIM-FLD
                GO TO FIN-EDT-FLD-TIM.
           MOVE TIM-SGV-TIM1 IN TIM-FLD TO TIM-SGV-TIM1-EDT IN TIM-EDT.
           MOVE TIM-SGV-TIM1-EDT IN TIM-EDT TO 
                TIM-SGV-TIM1-ALF IN TIM-FLD.
           IF TIM-SGV-TIM2-LEN IN TIM-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO TIM-SGV-TIM2-LEN IN TIM-FLD
                GO TO FIN-EDT-FLD-TIM.
           MOVE TIM-SGV-TIM2 IN TIM-FLD TO TIM-SGV-TIM2-EDT IN TIM-EDT.
           MOVE TIM-SGV-TIM2-EDT IN TIM-EDT TO 
                TIM-SGV-TIM2-ALF IN TIM-FLD.
*% INCLUDE FIN_EDT_FLD_TIM OF 'COLBMTIM.TXT'
       FIN-EDT-FLD-TIM.
           EXIT.
*% INCLUDE EOF_BU OF 'COLBMTIM.TXT'
*% INCLUDE EOF OF 'COLBMTIM.TXT'
*% INCLUDE EOF_IBM OF 'COLBMTIM.TXT'
