*% FRM = "CVC"
      *
      * COLBUCVC
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:48 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMCVC.TXT'
       SET-TAG-COLBUCVC SECTION.
       INI-SET-TAG-COLBUCVC.
           MOVE
           '<IDNMOD NOM=$COLBUCVC$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:48 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCVC.
           EXIT.
           
       RET-ALL-CVC SECTION.
       INI-RET-ALL-CVC.
*% INCLUDE INI_RET_ALL_CVC OF 'COLBMCVC.TXT'
           PERFORM KEY-ALL-CVC.
           PERFORM RET-CVC-CVC.
*% INCLUDE FIN_RET_ALL_CVC OF 'COLBMCVC.TXT'
       FIN-RET-ALL-CVC.
           EXIT.
       
       RET-CVC-CVC SECTION.
       INI-RET-CVC-CVC.
*% INCLUDE INI_RET_CVC_CVC OF 'COLBMCVC.TXT'
           MOVE CVC-CIC-TCVC IN CVC-FLD TO CVC-CIC-TCVC IN CVC.
           MOVE CVC-EXT-CTAB IN CVC-FLD TO CVC-EXT-CTAB IN CVC.
           MOVE CVC-GLS-DESC IN CVC-FLD TO CVC-GLS-DESC IN CVC.
           MOVE CVC-GLS-ABRV IN CVC-FLD TO CVC-GLS-ABRV IN CVC.
           MOVE CVC-GLS-DCOR IN CVC-FLD TO CVC-GLS-DCOR IN CVC.
           MOVE CVC-IND-VIGE IN CVC-FLD TO CVC-IND-VIGE IN CVC.
*% RREG = "CVC"
*% RREG_FRM = "CVC"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CVC-NUM-DTRN IN CVC-FLD TO CVC-NUM-DTRN IN CVC.
           MOVE CVC-NUM-MTRN IN CVC-FLD TO CVC-NUM-MTRN IN CVC.
           MOVE CVC-NUM-STRN IN CVC-FLD TO CVC-NUM-STRN IN CVC.
           MOVE CVC-NUM-ATRN IN CVC-FLD TO CVC-NUM-ATRN IN CVC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_RET_CVC_CVC OF 'COLBMCVC.TXT'
       FIN-RET-CVC-CVC.
           EXIT.
       
       KEY-ALL-CVC SECTION.
       INI-KEY-ALL-CVC.
*% INCLUDE INI_KEY_ALL_CVC OF 'COLBMCVC.TXT'
           PERFORM KEY-CVC-CVC.
*% INCLUDE FIN_KEY_ALL_CVC OF 'COLBMCVC.TXT'
       FIN-KEY-ALL-CVC.
           EXIT.
       
       KEY-CVC-CVC SECTION.
       INI-KEY-CVC-CVC.
*% INCLUDE INI_KEY_CVC_CVC OF 'COLBMCVC.TXT'
           MOVE CVC-MSC-BCCR IN CVC-FLD TO CVC-MSC-BCCR IN CVC.
           MOVE CVC-MSC-VICR IN CVC-FLD TO CVC-MSC-VICR IN CVC.
*% INCLUDE FIN_KEY_CVC_CVC OF 'COLBMCVC.TXT'
       FIN-KEY-CVC-CVC.
           EXIT.
       
       SCR-CLRA-CVC SECTION.
       INI-SCR-CLRA-CVC.
*% INCLUDE INI_SCR_CLRA_CVC OF 'COLBMCVC.TXT'
           PERFORM CLR-KEY-CVC.
           PERFORM CLR-FLD-CVC.
*% INCLUDE FIN_SCR_CLRA_CVC OF 'COLBMCVC.TXT'
       FIN-SCR-CLRA-CVC.
           EXIT.
       
       CLR-KEY-CVC SECTION.
       INI-CLR-KEY-CVC.
*% INCLUDE INI_CLR_KEY_CVC OF 'COLBMCVC.TXT'
           MOVE SPACES TO CVC-MSC-BCCR IN CVC-FLD.
           MOVE SPACES TO CVC-MSC-VICR IN CVC-FLD.
*% INCLUDE FIN_CLR_KEY_CVC OF 'COLBMCVC.TXT'
       FIN-CLR-KEY-CVC.
           EXIT.
       
       CLR-FLD-CVC SECTION.
       INI-CLR-FLD-CVC.
*% INCLUDE INI_CLR_FLD_CVC OF 'COLBMCVC.TXT'
           MOVE SPACES TO FRM-GLS-BCCR IN CVC-FLD.
           MOVE SPACES TO FRM-GLS-VICR IN CVC-FLD.
           PERFORM CLR-CVC-CVC.
*% INCLUDE FIN_CLR_FLD_CVC OF 'COLBMCVC.TXT'
       FIN-CLR-FLD-CVC.
           EXIT.
       
       CLR-CVC-CVC SECTION.
       INI-CLR-CVC-CVC.
*% INCLUDE INI_CLR_CVC_CVC OF 'COLBMCVC.TXT'
           MOVE SPACES TO CVC-CIC-TCVC IN CVC-FLD.
           MOVE SPACES TO CVC-EXT-CTAB IN CVC-FLD.
           MOVE SPACES TO CVC-GLS-DESC IN CVC-FLD.
           MOVE SPACES TO CVC-GLS-ABRV IN CVC-FLD.
           MOVE SPACES TO CVC-GLS-DCOR IN CVC-FLD.
           MOVE SPACES TO CVC-IND-VIGE IN CVC-FLD.
*% RREG = "CVC"
*% RREG_FRM = "CVC"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CVC-FEC-FTRN IN CVC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_CVC_CVC OF 'COLBMCVC.TXT'
       FIN-CLR-CVC-CVC.
           EXIT.
       
       FST-KEY-CVC SECTION.
       INI-FST-KEY-CVC.
*% INCLUDE INI_FST_KEY_CVC OF 'COLBMCVC.TXT'
           MOVE -1 TO CVC-MSC-BCCR-LEN IN CVC-FLD.
*% INCLUDE FIN_FST_KEY_CVC OF 'COLBMCVC.TXT'
       FIN-FST-KEY-CVC.
           EXIT.
       
       FST-FLD-CVC SECTION.
       INI-FST-FLD-CVC.
*% INCLUDE INI_FST_FLD_CVC OF 'COLBMCVC.TXT'
           MOVE -1 TO CVC-CIC-TCVC-LEN IN CVC-FLD.
*% INCLUDE FIN_FST_FLD_CVC OF 'COLBMCVC.TXT'
       FIN-FST-FLD-CVC.
           EXIT.
       
       PRO-KEY-CVC SECTION.
       INI-PRO-KEY-CVC.
*% INCLUDE INI_PRO_KEY_CVC OF 'COLBMCVC.TXT'
           PERFORM PRO-MKY-CVC.
*% INCLUDE FIN_PRO_KEY_CVC OF 'COLBMCVC.TXT'
       FIN-PRO-KEY-CVC.
           EXIT.
       
       PRO-MKY-CVC SECTION.
       INI-PRO-MKY-CVC.
*% INCLUDE INI_PRO_MKY_CVC OF 'COLBMCVC.TXT'
*% INCLUDE FIN_PRO_MKY_CVC OF 'COLBMCVC.TXT'
       FIN-PRO-MKY-CVC.
           EXIT.
       
       PRO-FLD-CVC SECTION.
       INI-PRO-FLD-CVC.
*% INCLUDE INI_PRO_FLD_CVC OF 'COLBMCVC.TXT'
           PERFORM PRO-CVC-CVC.
*% INCLUDE FIN_PRO_FLD_CVC OF 'COLBMCVC.TXT'
       FIN-PRO-FLD-CVC.
           EXIT.
       
       PRO-CVC-CVC SECTION.
       INI-PRO-CVC-CVC.
*% INCLUDE INI_PRO_CVC_CVC OF 'COLBMCVC.TXT'
           MOVE FRM-CPIM-AEY TO CVC-CIC-TCVC-ATR IN CVC-FLD.
           MOVE FRM-CPIM-AEY TO CVC-EXT-CTAB-ATR IN CVC-FLD.
           MOVE FRM-CPIM-AEY TO CVC-GLS-DESC-ATR IN CVC-FLD.
           MOVE FRM-CPIM-AEY TO CVC-GLS-ABRV-ATR IN CVC-FLD.
           MOVE FRM-CPIM-AEY TO CVC-GLS-DCOR-ATR IN CVC-FLD.
           MOVE FRM-CPIM-AEY TO CVC-IND-VIGE-ATR IN CVC-FLD.
*% INCLUDE FIN_PRO_CVC_CVC OF 'COLBMCVC.TXT'
       FIN-PRO-CVC-CVC.
           EXIT.
       
       UNP-KEY-CVC SECTION.
       INI-UNP-KEY-CVC.
*% INCLUDE INI_UNP_KEY_CVC OF 'COLBMCVC.TXT'
           PERFORM UNP-IKY-CVC.
           PERFORM UNP-AKY-CVC.
           PERFORM UNP-MKY-CVC.
*% INCLUDE FIN_UNP_KEY_CVC OF 'COLBMCVC.TXT'
       FIN-UNP-KEY-CVC.
           EXIT.
       
       UNP-IKY-CVC SECTION.
       INI-UNP-IKY-CVC.
*% INCLUDE INI_UNP_IKY_CVC OF 'COLBMCVC.TXT'
           MOVE FRM-CPIM-UBY TO CVC-MSC-BCCR-ATR.
           MOVE FRM-CPIM-UBY TO CVC-MSC-VICR-ATR.
*% INCLUDE FIN_UNP_IKY_CVC OF 'COLBMCVC.TXT'
       FIN-UNP-IKY-CVC.
           EXIT.
       
       UNP-MKY-CVC SECTION.
       INI-UNP-MKY-CVC.
*% INCLUDE INI_UNP_MKY_CVC OF 'COLBMCVC.TXT'
*% INCLUDE FIN_UNP_MKY_CVC OF 'COLBMCVC.TXT'
       FIN-UNP-MKY-CVC.
           EXIT.
       
       UNP-AKY-CVC SECTION.
       INI-UNP-AKY-CVC.
*% INCLUDE INI_UNP_AKY_CVC OF 'COLBMCVC.TXT'
*% INCLUDE FIN_UNP_AKY_CVC OF 'COLBMCVC.TXT'
       FIN-UNP-AKY-CVC.
           EXIT.
       
       UNP-FLD-CVC SECTION.
       INI-UNP-FLD-CVC.
*% INCLUDE INI_UNP_FLD_CVC OF 'COLBMCVC.TXT'
           PERFORM UNP-CVC-CVC.
*% INCLUDE FIN_UNP_FLD_CVC OF 'COLBMCVC.TXT'
       FIN-UNP-FLD-CVC.
           EXIT.
       
       UNP-CVC-CVC SECTION.
       INI-UNP-CVC-CVC.
*% INCLUDE INI_UNP_CVC_CVC OF 'COLBMCVC.TXT'
           MOVE FRM-CPIM-UBY TO CVC-CIC-TCVC-ATR IN CVC-FLD.
           MOVE FRM-CPIM-UBY TO CVC-EXT-CTAB-ATR IN CVC-FLD.
           MOVE FRM-CPIM-UBY TO CVC-GLS-DESC-ATR IN CVC-FLD.
           MOVE FRM-CPIM-UBY TO CVC-GLS-ABRV-ATR IN CVC-FLD.
           MOVE FRM-CPIM-UBY TO CVC-GLS-DCOR-ATR IN CVC-FLD.
           MOVE FRM-CPIM-UBY TO CVC-IND-VIGE-ATR IN CVC-FLD.
*% INCLUDE FIN_UNP_CVC_CVC OF 'COLBMCVC.TXT'
       FIN-UNP-CVC-CVC.
           EXIT.
       
       VAL-MKY-CVC SECTION.
       INI-VAL-MKY-CVC.
*% INCLUDE INI_VAL_MKY_CVC OF 'COLBMCVC.TXT'
*% INCLUDE FIN_VAL_MKY_CVC OF 'COLBMCVC.TXT'
       FIN-VAL-MKY-CVC.
           EXIT.
       
       VAL-FLD-CVC SECTION.
       INI-VAL-FLD-CVC.
*% INCLUDE INI_VAL_FLD_CVC OF 'COLBMCVC.TXT'
           PERFORM VAL-FRM-GLS-BCCR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-BCCR-LEN IN CVC-FLD
                GO TO FIN-VAL-FLD-CVC.
           PERFORM VAL-FRM-GLS-VICR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-VICR-LEN IN CVC-FLD
                GO TO FIN-VAL-FLD-CVC.
           PERFORM VAL-CVC-CIC-TCVC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CVC-CIC-TCVC-LEN IN CVC-FLD
                GO TO FIN-VAL-FLD-CVC.
           PERFORM VAL-CVC-EXT-CTAB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CVC-EXT-CTAB-LEN IN CVC-FLD
                GO TO FIN-VAL-FLD-CVC.
           PERFORM VAL-CVC-GLS-DESC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CVC-GLS-DESC-LEN IN CVC-FLD
                GO TO FIN-VAL-FLD-CVC.
           PERFORM VAL-CVC-GLS-ABRV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CVC-GLS-ABRV-LEN IN CVC-FLD
                GO TO FIN-VAL-FLD-CVC.
           PERFORM VAL-CVC-GLS-DCOR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CVC-GLS-DCOR-LEN IN CVC-FLD
                GO TO FIN-VAL-FLD-CVC.
           PERFORM VAL-CVC-IND-VIGE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CVC-IND-VIGE-LEN IN CVC-FLD
                GO TO FIN-VAL-FLD-CVC.
*% RREG = "CVC"
*% RREG_FRM = "CVC"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-CVC-FEC-FTRN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                CVC-FEC-FTRN-LEN IN CVC-FLD
                GO TO FIN-VAL-FLD-CVC.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% INCLUDE FIN_VAL_FLD_CVC OF 'COLBMCVC.TXT'
       FIN-VAL-FLD-CVC.
           EXIT.
       
       VAL-FRM-GLS-BCCR SECTION.
       INI-VAL-FRM-GLS-BCCR.
*% LBL = "FRM_GLS_BCCR OF 'COLBMCVC.TXT'"
*% INCLUDE INI_FRM_GLS_BCCR OF 'COLBMCVC.TXT'
*% INCLUDE FRM_GLS_BCCR OF 'COLBMCVC.TXT'
*% INCLUDE FIN_FRM_GLS_BCCR OF 'COLBMCVC.TXT'
       FIN-VAL-FRM-GLS-BCCR.
           EXIT.
       
       VAL-FRM-GLS-VICR SECTION.
       INI-VAL-FRM-GLS-VICR.
*% LBL = "FRM_GLS_VICR OF 'COLBMCVC.TXT'"
*% INCLUDE INI_FRM_GLS_VICR OF 'COLBMCVC.TXT'
*% INCLUDE FRM_GLS_VICR OF 'COLBMCVC.TXT'
*% INCLUDE FIN_FRM_GLS_VICR OF 'COLBMCVC.TXT'
       FIN-VAL-FRM-GLS-VICR.
           EXIT.
       
       VAL-CVC-CIC-TCVC SECTION.
       INI-VAL-CVC-CIC-TCVC.
*% LBL = "CVC_CIC_TCVC OF 'COLBMCVC.TXT'"
*% INCLUDE INI_CVC_CIC_TCVC OF 'COLBMCVC.TXT'
           IF CVC-CIC-TCVC IN CVC-FLD NOT > SPACES
              MOVE SPACES TO CVC-CIC-TCVC IN CVC-FLD.
*% INCLUDE CVC_CIC_TCVC OF 'COLBMCVC.TXT'
*% INCLUDE FIN_CVC_CIC_TCVC OF 'COLBMCVC.TXT'
       FIN-VAL-CVC-CIC-TCVC.
           EXIT.
       
       VAL-CVC-EXT-CTAB SECTION.
       INI-VAL-CVC-EXT-CTAB.
*% LBL = "CVC_EXT_CTAB OF 'COLBMCVC.TXT'"
*% INCLUDE INI_CVC_EXT_CTAB OF 'COLBMCVC.TXT'
           IF CVC-EXT-CTAB IN CVC-FLD NOT > SPACES
              MOVE SPACES TO CVC-EXT-CTAB IN CVC-FLD.
*% INCLUDE CVC_EXT_CTAB OF 'COLBMCVC.TXT'
*% INCLUDE FIN_CVC_EXT_CTAB OF 'COLBMCVC.TXT'
       FIN-VAL-CVC-EXT-CTAB.
           EXIT.
       
       VAL-CVC-GLS-DESC SECTION.
       INI-VAL-CVC-GLS-DESC.
*% LBL = "CVC_GLS_DESC OF 'COLBMCVC.TXT'"
*% INCLUDE INI_CVC_GLS_DESC OF 'COLBMCVC.TXT'
           IF CVC-GLS-DESC IN CVC-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-CVC-GLS-DESC.
*% INCLUDE CVC_GLS_DESC OF 'COLBMCVC.TXT'
*% INCLUDE FIN_CVC_GLS_DESC OF 'COLBMCVC.TXT'
       FIN-VAL-CVC-GLS-DESC.
           EXIT.
       
       VAL-CVC-GLS-ABRV SECTION.
       INI-VAL-CVC-GLS-ABRV.
*% LBL = "CVC_GLS_ABRV OF 'COLBMCVC.TXT'"
*% INCLUDE INI_CVC_GLS_ABRV OF 'COLBMCVC.TXT'
           IF CVC-GLS-ABRV IN CVC-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-CVC-GLS-ABRV.
*% INCLUDE CVC_GLS_ABRV OF 'COLBMCVC.TXT'
*% INCLUDE FIN_CVC_GLS_ABRV OF 'COLBMCVC.TXT'
       FIN-VAL-CVC-GLS-ABRV.
           EXIT.
       
       VAL-CVC-GLS-DCOR SECTION.
       INI-VAL-CVC-GLS-DCOR.
*% LBL = "CVC_GLS_DCOR OF 'COLBMCVC.TXT'"
*% INCLUDE INI_CVC_GLS_DCOR OF 'COLBMCVC.TXT'
           IF CVC-GLS-DCOR IN CVC-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-CVC-GLS-DCOR.
*% INCLUDE CVC_GLS_DCOR OF 'COLBMCVC.TXT'
*% INCLUDE FIN_CVC_GLS_DCOR OF 'COLBMCVC.TXT'
       FIN-VAL-CVC-GLS-DCOR.
           EXIT.
       
       VAL-CVC-IND-VIGE SECTION.
       INI-VAL-CVC-IND-VIGE.
*% LBL = "CVC_IND_VIGE OF 'COLBMCVC.TXT'"
*% INCLUDE INI_CVC_IND_VIGE OF 'COLBMCVC.TXT'
           IF CVC-IND-VIGE IN CVC-FLD NOT > SPACES
              MOVE SPACES TO CVC-IND-VIGE IN CVC-FLD.
*% INCLUDE CVC_IND_VIGE OF 'COLBMCVC.TXT'
*% BUF = "CVC-FLD"
*% SIS = "GNS"
*% TAB = "VIGE"
*% FLD = "CVC-IND-VIGE"
*% GLS = ""
*% TIENE_GLOSA = FALSE
*% LGL = 0
*% INCLUDE 'VALIND'
*% INCLUDE FIN_CVC_IND_VIGE OF 'COLBMCVC.TXT'
       FIN-VAL-CVC-IND-VIGE.
           EXIT.
*% RREG = "CVC"
*% RREG_FRM = "CVC"
*% NOMB = "FTRN"
*% OBL = "S"
*% LBL = "CVC_FEC_FTRN OF 'COLBMCVC.TXT'"
*% IZR = "S"
*% ZRO = "S"
*% FRM_IFLD = ""
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-CVC-FEC-FTRN SECTION.
       INI-VAL-CVC-FEC-FTRN.
           IF CVC-FEC-FTRN IN CVC-FLD  NOT > ZEROES
              MOVE ZEROES TO CVC-FEC-FTRN IN CVC-FLD.
*% INCLUDE INI_CVC_FEC_FTRN OF 'COLBMCVC.TXT'
*% INCLUDE CVC_FEC_FTRN OF 'COLBMCVC.TXT'
*% INCLUDE FIN_CVC_FEC_FTRN OF 'COLBMCVC.TXT'
       FIN-VAL-CVC-FEC-FTRN.
           EXIT.
      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-CON-FLD-CVC SECTION.
       INI-VAL-CON-FLD-CVC.
*% INCLUDE VAL_CON_FLD_CVC OF 'COLBMCVC.TXT'
       FIN-VAL-CON-FLD-CVC.
           EXIT.
       
       VAL-CON-CVC SECTION.
       INI-VAL-CON-CVC.
*% INCLUDE VAL_CON_CVC OF 'COLBMCVC.TXT'
       FIN-VAL-CON-CVC.
           EXIT.
       
       VAL-NUM-KEY-CVC SECTION.
       INI-VAL-NUM-KEY-CVC.
           PERFORM VAL-NUM-IKY-CVC.
           PERFORM VAL-NUM-MKY-CVC.
           PERFORM VAL-NUM-AKY-CVC.
       FIN-VAL-NUM-KEY-CVC.
           EXIT.
       
       VAL-NUM-IKY-CVC SECTION.
       INI-VAL-NUM-IKY-CVC.
*% INCLUDE INI_VAL_NUM_IKY_CVC OF 'COLBMCVC.TXT'
*% INCLUDE FIN_VAL_NUM_IKY_CVC OF 'COLBMCVC.TXT'
       FIN-VAL-NUM-IKY-CVC.
