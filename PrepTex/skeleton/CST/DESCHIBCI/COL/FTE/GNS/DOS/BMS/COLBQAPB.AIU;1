*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "APB"
*% PGM_BQ = TRUE
       PUT-ALL-APB SECTION.
       INI-PUT-ALL-APB.
*% INCLUDE INI_PUT_ALL_APB OF 'COLBMAPB.TXT'
           PERFORM PUT-APB-APB.
*% INCLUDE FIN_PUT_ALL_APB OF 'COLBMAPB.TXT'
       FIN-PUT-ALL-APB.
           EXIT.
       
       PUT-APB-APB SECTION.
       INI-PUT-APB-APB.
*% INCLUDE INI_PUT_APB_APB OF 'COLBMAPB.TXT'
           IF PGM-STAT-APB-NUL
               GO TO FIN-PUT-APB-APB.
           IF NOT PGM-STAT-APB-OKS
               PERFORM CLR-APB-APB
               GO TO FIN-PUT-APB-APB.
           MOVE APB-COD-PROG IN APB TO APB-COD-PROG IN APB-FLD.
           MOVE APB-NUM-PARA IN APB TO APB-NUM-PARA IN APB-FLD.
*% INCLUDE FIN_PUT_APB_APB OF 'COLBMAPB.TXT'
       FIN-PUT-APB-APB.
           EXIT.
       
       KEY-ALL-APB SECTION.
       INI-KEY-ALL-APB.
*% INCLUDE INI_KEY_ALL_APB OF 'COLBMAPB.TXT'
           PERFORM KEY-APB-APB.
*% INCLUDE FIN_KEY_ALL_APB OF 'COLBMAPB.TXT'
       FIN-KEY-ALL-APB.
           EXIT.
       
       KEY-APB-APB SECTION.
       INI-KEY-APB-APB.
*% INCLUDE INI_KEY_APB_APB OF 'COLBMAPB.TXT'
           MOVE APB-COD-PROG IN APB-FLD TO APB-COD-PROG IN APB.
*% INCLUDE FIN_KEY_APB_APB OF 'COLBMAPB.TXT'
       FIN-KEY-APB-APB.
           EXIT.
       
       SCR-CLRA-APB SECTION.
       INI-SCR-CLRA-APB.
*% INCLUDE INI_SCR_CLRA_APB OF 'COLBMAPB.TXT'
           PERFORM CLR-KEY-APB.
           PERFORM CLR-FLD-APB.
*% INCLUDE FIN_SCR_CLRA_APB OF 'COLBMAPB.TXT'
       FIN-SCR-CLRA-APB.
           EXIT.
       
       CLR-KEY-APB SECTION.
       INI-CLR-KEY-APB.
*% INCLUDE INI_CLR_KEY_APB OF 'COLBMAPB.TXT'
           MOVE SPACES TO APB-COD-PROG IN APB-FLD.
*% INCLUDE FIN_CLR_KEY_APB OF 'COLBMAPB.TXT'
       FIN-CLR-KEY-APB.
           EXIT.
       
       CLR-FLD-APB SECTION.
       INI-CLR-FLD-APB.
*% INCLUDE INI_CLR_FLD_APB OF 'COLBMAPB.TXT'
           MOVE SPACES TO FRM-GLS-PRO1 IN APB-FLD.
           MOVE SPACES TO FRM-GLS-PRO2 IN APB-FLD.
           MOVE SPACES TO FRM-GLS-PARA IN APB-FLD(1).
           MOVE SPACES TO FRM-GLS-PARA IN APB-FLD(2).
           MOVE SPACES TO FRM-GLS-PARA IN APB-FLD(3).
           MOVE SPACES TO FRM-GLS-PARA IN APB-FLD(4).
           MOVE SPACES TO FRM-GLS-PARA IN APB-FLD(5).
           MOVE SPACES TO FRM-GLS-PARA IN APB-FLD(6).
           MOVE SPACES TO FRM-GLS-PARA IN APB-FLD(7).
           MOVE SPACES TO FRM-GLS-PARA IN APB-FLD(8).
           MOVE SPACES TO FRM-GLS-PARA IN APB-FLD(9).
           MOVE SPACES TO FRM-GLS-PARA IN APB-FLD(10).
           MOVE SPACES TO FRM-COD-PARA IN APB-FLD(1).
           MOVE SPACES TO FRM-COD-PARA IN APB-FLD(2).
           MOVE SPACES TO FRM-COD-PARA IN APB-FLD(3).
           MOVE SPACES TO FRM-COD-PARA IN APB-FLD(4).
           MOVE SPACES TO FRM-COD-PARA IN APB-FLD(5).
           MOVE SPACES TO FRM-COD-PARA IN APB-FLD(6).
           MOVE SPACES TO FRM-COD-PARA IN APB-FLD(7).
           MOVE SPACES TO FRM-COD-PARA IN APB-FLD(8).
           MOVE SPACES TO FRM-COD-PARA IN APB-FLD(9).
           MOVE SPACES TO FRM-COD-PARA IN APB-FLD(10).
           PERFORM CLR-APB-APB.
*% INCLUDE FIN_CLR_FLD_APB OF 'COLBMAPB.TXT'
       FIN-CLR-FLD-APB.
           EXIT.
       
       CLR-APB-APB SECTION.
       INI-CLR-APB-APB.
*% INCLUDE INI_CLR_APB_APB OF 'COLBMAPB.TXT'
           MOVE ZEROES TO APB-NUM-PARA IN APB-FLD.
*% INCLUDE FIN_CLR_APB_APB OF 'COLBMAPB.TXT'
       FIN-CLR-APB-APB.
           EXIT.
       
       FST-KEY-APB SECTION.
       INI-FST-KEY-APB.
*% INCLUDE INI_FST_KEY_APB OF 'COLBMAPB.TXT'
           MOVE -1 TO APB-COD-PROG-LEN IN APB-FLD.
*% INCLUDE FIN_FST_KEY_APB OF 'COLBMAPB.TXT'
       FIN-FST-KEY-APB.
           EXIT.
       
       FST-FLD-APB SECTION.
       INI-FST-FLD-APB.
*% INCLUDE INI_FST_FLD_APB OF 'COLBMAPB.TXT'
           MOVE -1 TO FRM-GLS-PARA-LEN IN APB-FLD(1).
*% INCLUDE FIN_FST_FLD_APB OF 'COLBMAPB.TXT'
       FIN-FST-FLD-APB.
           EXIT.
       
       PRO-KEY-APB SECTION.
       INI-PRO-KEY-APB.
*% INCLUDE INI_PRO_KEY_APB OF 'COLBMAPB.TXT'
           PERFORM PRO-IKY-APB.
           PERFORM PRO-AKY-APB.
           PERFORM PRO-MKY-APB.
*% INCLUDE FIN_PRO_KEY_APB OF 'COLBMAPB.TXT'
       FIN-PRO-KEY-APB.
           EXIT.
       
       PRO-IKY-APB SECTION.
       INI-PRO-IKY-APB.
*% INCLUDE INI_PRO_IKY_APB OF 'COLBMAPB.TXT'
           MOVE FRM-CPIM-AEY TO APB-COD-PROG-ATR IN APB-FLD.
*% INCLUDE FIN_PRO_IKY_APB OF 'COLBMAPB.TXT'
       FIN-PRO-IKY-APB.
           EXIT.
       
       PRO-MKY-APB SECTION.
       INI-PRO-MKY-APB.
*% INCLUDE INI_PRO_MKY_APB OF 'COLBMAPB.TXT'
*% INCLUDE FIN_PRO_MKY_APB OF 'COLBMAPB.TXT'
       FIN-PRO-MKY-APB.
           EXIT.
       
       PRO-AKY-APB SECTION.
       INI-PRO-AKY-APB.
*% INCLUDE INI_PRO_AKY_APB OF 'COLBMAPB.TXT'
*% INCLUDE FIN_PRO_AKY_APB OF 'COLBMAPB.TXT'
       FIN-PRO-AKY-APB.
           EXIT.
       
       UNP-KEY-APB SECTION.
       INI-UNP-KEY-APB.
*% INCLUDE INI_UNP_KEY_APB OF 'COLBMAPB.TXT'
           PERFORM UNP-IKY-APB.
           PERFORM UNP-AKY-APB.
           PERFORM UNP-MKY-APB.
*% INCLUDE FIN_UNP_KEY_APB OF 'COLBMAPB.TXT'
       FIN-UNP-KEY-APB.
           EXIT.
       
       UNP-IKY-APB SECTION.
       INI-UNP-IKY-APB.
*% INCLUDE INI_UNP_IKY_APB OF 'COLBMAPB.TXT'
           MOVE FRM-CPIM-UBY TO APB-COD-PROG-ATR.
*% INCLUDE FIN_UNP_IKY_APB OF 'COLBMAPB.TXT'
       FIN-UNP-IKY-APB.
           EXIT.
       
       UNP-MKY-APB SECTION.
       INI-UNP-MKY-APB.
*% INCLUDE INI_UNP_MKY_APB OF 'COLBMAPB.TXT'
*% INCLUDE FIN_UNP_MKY_APB OF 'COLBMAPB.TXT'
       FIN-UNP-MKY-APB.
           EXIT.
       
       UNP-AKY-APB SECTION.
       INI-UNP-AKY-APB.
*% INCLUDE INI_UNP_AKY_APB OF 'COLBMAPB.TXT'
*% INCLUDE FIN_UNP_AKY_APB OF 'COLBMAPB.TXT'
       FIN-UNP-AKY-APB.
           EXIT.
       
       UNP-FLD-APB SECTION.
       INI-UNP-FLD-APB.
*% INCLUDE INI_UNP_FLD_APB OF 'COLBMAPB.TXT'
           MOVE FRM-CPIM-UBY TO FRM-GLS-PARA-ATR IN APB-FLD(1).
           MOVE FRM-CPIM-UBY TO FRM-GLS-PARA-ATR IN APB-FLD(2).
           MOVE FRM-CPIM-UBY TO FRM-GLS-PARA-ATR IN APB-FLD(3).
           MOVE FRM-CPIM-UBY TO FRM-GLS-PARA-ATR IN APB-FLD(4).
           MOVE FRM-CPIM-UBY TO FRM-GLS-PARA-ATR IN APB-FLD(5).
           MOVE FRM-CPIM-UBY TO FRM-GLS-PARA-ATR IN APB-FLD(6).
           MOVE FRM-CPIM-UBY TO FRM-GLS-PARA-ATR IN APB-FLD(7).
           MOVE FRM-CPIM-UBY TO FRM-GLS-PARA-ATR IN APB-FLD(8).
           MOVE FRM-CPIM-UBY TO FRM-GLS-PARA-ATR IN APB-FLD(9).
           MOVE FRM-CPIM-UBY TO FRM-GLS-PARA-ATR IN APB-FLD(10).
           MOVE FRM-CPIM-UBY TO FRM-COD-PARA-ATR IN APB-FLD(1).
           MOVE FRM-CPIM-UBY TO FRM-COD-PARA-ATR IN APB-FLD(2).
           MOVE FRM-CPIM-UBY TO FRM-COD-PARA-ATR IN APB-FLD(3).
           MOVE FRM-CPIM-UBY TO FRM-COD-PARA-ATR IN APB-FLD(4).
           MOVE FRM-CPIM-UBY TO FRM-COD-PARA-ATR IN APB-FLD(5).
           MOVE FRM-CPIM-UBY TO FRM-COD-PARA-ATR IN APB-FLD(6).
           MOVE FRM-CPIM-UBY TO FRM-COD-PARA-ATR IN APB-FLD(7).
           MOVE FRM-CPIM-UBY TO FRM-COD-PARA-ATR IN APB-FLD(8).
           MOVE FRM-CPIM-UBY TO FRM-COD-PARA-ATR IN APB-FLD(9).
           MOVE FRM-CPIM-UBY TO FRM-COD-PARA-ATR IN APB-FLD(10).
           PERFORM UNP-APB-APB.
*% INCLUDE FIN_UNP_FLD_APB OF 'COLBMAPB.TXT'
       FIN-UNP-FLD-APB.
           EXIT.
       
       UNP-APB-APB SECTION.
       INI-UNP-APB-APB.
*% INCLUDE INI_UNP_APB_APB OF 'COLBMAPB.TXT'
*% INCLUDE FIN_UNP_APB_APB OF 'COLBMAPB.TXT'
       FIN-UNP-APB-APB.
           EXIT.
       
       VAL-KEY-APB SECTION.
       INI-VAL-KEY-APB.
           PERFORM VAL-IKY-APB.
           PERFORM VAL-MKY-APB.
           PERFORM VAL-AKY-APB.
       FIN-VAL-KEY-APB.
           EXIT.
       
       VAL-IKY-APB SECTION.
       INI-VAL-IKY-APB.
*% INCLUDE INI_VAL_IKY_APB OF 'COLBMAPB.TXT'
           PERFORM VAL-APB-COD-PROG.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO APB-COD-PROG-LEN IN APB-FLD
                GO TO FIN-VAL-IKY-APB.
*% INCLUDE FIN_VAL_IKY_APB OF 'COLBMAPB.TXT'
       FIN-VAL-IKY-APB.
           EXIT.
       
       VAL-MKY-APB SECTION.
       INI-VAL-MKY-APB.
*% INCLUDE INI_VAL_MKY_APB OF 'COLBMAPB.TXT'
*% INCLUDE FIN_VAL_MKY_APB OF 'COLBMAPB.TXT'
       FIN-VAL-MKY-APB.
           EXIT.
       
       VAL-AKY-APB SECTION.
       INI-VAL-AKY-APB.
*% INCLUDE INI_VAL_AKY_APB OF 'COLBMAPB.TXT'
*% INCLUDE FIN_VAL_AKY_APB OF 'COLBMAPB.TXT'
       FIN-VAL-AKY-APB.
           EXIT.
       
       VAL-APB-COD-PROG SECTION.
       INI-VAL-APB-COD-PROG.
*% LBL = "APB_COD_PROG OF 'COLBMAPB.TXT'"
*% INCLUDE INI_APB_COD_PROG OF 'COLBMAPB.TXT'
           IF APB-COD-PROG IN APB-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-APB-COD-PROG.
*% INCLUDE APB_COD_PROG OF 'COLBMAPB.TXT'
*% INCLUDE FIN_APB_COD_PROG OF 'COLBMAPB.TXT'
       FIN-VAL-APB-COD-PROG.
           EXIT.
       
       VAL-CON-KEY-APB SECTION.
       INI-VAL-CON-KEY-APB.
*% INCLUDE INI_VAL_CON_KEY_APB OF 'COLBMAPB.TXT'
           IF     APB-COD-PROG IN APB-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-APB.
*% INCLUDE FIN_VAL_CON_KEY_APB OF 'COLBMAPB.TXT'
       FIN-VAL-CON-KEY-APB.
           EXIT.
       
       VAL-CON-IKY-APB SECTION.
       INI-VAL-CON-IKY-APB.
*% INCLUDE INI_VAL_CON_IKY_APB OF 'COLBMAPB.TXT'
           IF     APB-COD-PROG IN APB-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-APB.
*% INCLUDE FIN_VAL_CON_IKY_APB OF 'COLBMAPB.TXT'
       FIN-VAL-CON-IKY-APB.
           EXIT.
       
       VAL-NUM-KEY-APB SECTION.
       INI-VAL-NUM-KEY-APB.
           PERFORM VAL-NUM-IKY-APB.
           PERFORM VAL-NUM-MKY-APB.
           PERFORM VAL-NUM-AKY-APB.
       FIN-VAL-NUM-KEY-APB.
           EXIT.
       
       VAL-NUM-IKY-APB SECTION.
       INI-VAL-NUM-IKY-APB.
*% INCLUDE INI_VAL_NUM_IKY_APB OF 'COLBMAPB.TXT'
*% INCLUDE FIN_VAL_NUM_IKY_APB OF 'COLBMAPB.TXT'
       FIN-VAL-NUM-IKY-APB.
           EXIT.
       
       VAL-NUM-MKY-APB SECTION.
       INI-VAL-NUM-MKY-APB.
*% INCLUDE INI_VAL_NUM_MKY_APB OF 'COLBMAPB.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_APB OF 'COLBMAPB.TXT'
       FIN-VAL-NUM-MKY-APB.
           EXIT.
       
       VAL-NUM-AKY-APB SECTION.
       INI-VAL-NUM-AKY-APB.
*% INCLUDE INI_VAL_NUM_AKY_APB OF 'COLBMAPB.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_APB OF 'COLBMAPB.TXT'
       FIN-VAL-NUM-AKY-APB.
           EXIT.
       
       VAL-NUM-FLD-APB SECTION.
       INI-VAL-NUM-FLD-APB.
*% INCLUDE INI_VAL_NUM_FLD_APB OF 'COLBMAPB.TXT'
       VAL-NUM-APB-NUM-PARA.
*% BUF = "APB-FLD"
*% FLD = "APB-NUM-PARA"
*% ENT = "2"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-APB.
*% INCLUDE FIN_VAL_NUM_FLD_APB OF 'COLBMAPB.TXT'
       FIN-VAL-NUM-FLD-APB.
           EXIT.
       
       EDT-KEY-APB SECTION.
       INI-EDT-KEY-APB.
           PERFORM EDT-IKY-APB.
           PERFORM EDT-MKY-APB.
           PERFORM EDT-AKY-APB.
*% INCLUDE EDT_KEY_APB OF 'COLBMAPB.TXT'
       FIN-EDT-KEY-APB.
           EXIT.
       
       EDT-IKY-APB SECTION.
       INI-EDT-IKY-APB.
*% INCLUDE EDT_IKY_APB OF 'COLBMAPB.TXT'
*% INCLUDE FIN_EDT_IKY_APB OF 'COLBMAPB.TXT'
       FIN-EDT-IKY-APB.
           EXIT.
       
       EDT-MKY-APB SECTION.
       INI-EDT-MKY-APB.
*% INCLUDE EDT_MKY_APB OF 'COLBMAPB.TXT'
*% INCLUDE FIN_EDT_MKY_APB OF 'COLBMAPB.TXT'
       FIN-EDT-MKY-APB.
           EXIT.
       
       EDT-AKY-APB SECTION.
       INI-EDT-AKY-APB.
*% INCLUDE EDT_AKY_APB OF 'COLBMAPB.TXT'
*% INCLUDE FIN_EDT_AKY_APB OF 'COLBMAPB.TXT'
       FIN-EDT-AKY-APB.
           EXIT.
       
       EDT-FLD-APB SECTION.
       INI-EDT-FLD-APB.
*% INCLUDE EDT_FLD_APB OF 'COLBMAPB.TXT'
           IF APB-NUM-PARA-LEN IN APB-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO APB-NUM-PARA-LEN IN APB-FLD
                GO TO FIN-EDT-FLD-APB.
           MOVE APB-NUM-PARA IN APB-FLD TO APB-NUM-PARA-EDT IN APB-EDT.
           MOVE APB-NUM-PARA-EDT IN APB-EDT TO 
                APB-NUM-PARA-ALF IN APB-FLD.
*% INCLUDE FIN_EDT_FLD_APB OF 'COLBMAPB.TXT'
       FIN-EDT-FLD-APB.
           EXIT.
*% INCLUDE EOF_BQ OF 'COLBMAPB.TXT'
*% INCLUDE EOF OF 'COLBMAPB.TXT'
*% INCLUDE EOF_IBM OF 'COLBMAPB.TXT'
