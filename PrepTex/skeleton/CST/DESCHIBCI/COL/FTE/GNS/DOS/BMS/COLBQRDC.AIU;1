*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "RDC"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMRDC.TXT'
       SET-TAG-COLBQRDC SECTION.
       INI-SET-TAG-COLBQRDC.
           MOVE
           '<IDNMOD NOM=$COLBQRDC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:55 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQRDC.
           EXIT.
           
       PUT-ALL-RDC SECTION.
       INI-PUT-ALL-RDC.
*% INCLUDE INI_PUT_ALL_RDC OF 'COLBMRDC.TXT'
           PERFORM PUT-OPC-RDC.
           PERFORM PUT-RDC-RDC.
           PERFORM PUT-DBC-RDC.
           PERFORM PUT-RCD-RDC.
*% INCLUDE FIN_PUT_ALL_RDC OF 'COLBMRDC.TXT'
       FIN-PUT-ALL-RDC.
           EXIT.
       
       PUT-OPC-RDC SECTION.
       INI-PUT-OPC-RDC.
*% INCLUDE INI_PUT_OPC_RDC OF 'COLBMRDC.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-RDC.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-RDC
               GO TO FIN-PUT-OPC-RDC.
           MOVE OPC-MSC-TVOC IN OPC TO OPC-MSC-TVOC IN RDC-FLD.
           MOVE OPC-COD-TOOC IN OPC TO OPC-COD-TOOC IN RDC-FLD.
           MOVE OPC-COD-OFOC IN OPC TO OPC-COD-OFOC IN RDC-FLD.
           MOVE OPC-COD-COOC IN OPC TO OPC-COD-COOC IN RDC-FLD.
           MOVE OPC-COD-VCOC IN OPC TO OPC-COD-VCOC IN RDC-FLD.
*% BUF = "RDC-FLD"
*% SIS = "TAB"
*% TAB = "TVAL"
*% FLD = "OPC-MSC-TVOC"
*% GLS = "FRM-GLS-TVOC"
*% TIENE_GLOSA = TRUE
*% LGL = 13
*% INCLUDE 'BUSMSC'
*% BUF = "RDC-FLD"
*% SIS = "TAB"
*% TAB = "TIO "
*% FLD = "OPC-COD-TOOC"
*% GLS = "FRM-GLS-TOOC"
*% TIENE_GLOSA = TRUE
*% LGL = 14
*% INCLUDE 'BUSTAB'
*% BUF = "RDC-FLD"
*% SIS = "TAB"
*% TAB = "OFI "
*% FLD = "OPC-COD-OFOC"
*% GLS = "FRM-GLS-OFOC"
*% TIENE_GLOSA = TRUE
*% LGL = 14
*% INCLUDE 'BUSOFI          '
*% BUF = "RDC-FLD"
*% SIS = "COL"
*% TAB = "AUX "
*% FLD = "OPC-COD-COOC"
*% GLS = "FRM-GLS-COOC"
*% TIENE_GLOSA = TRUE
*% LGL = 23
*% INCLUDE 'BUSTAB'
*% BUF = "RDC-FLD"
*% SIS = "TAB"
*% TAB = "VLR "
*% FLD = "OPC-COD-VCOC"
*% GLS = "FRM-GLS-VCOC"
*% TIENE_GLOSA = TRUE
*% LGL = 23
*% INCLUDE 'BUSTAB'
*% INCLUDE FIN_PUT_OPC_RDC OF 'COLBMRDC.TXT'
       FIN-PUT-OPC-RDC.
           EXIT.
       
       PUT-RDC-RDC SECTION.
       INI-PUT-RDC-RDC.
*% INCLUDE INI_PUT_RDC_RDC OF 'COLBMRDC.TXT'
           IF PGM-STAT-RDC-NUL
               GO TO FIN-PUT-RDC-RDC.
           IF NOT PGM-STAT-RDC-OKS
               PERFORM CLR-RDC-RDC
               GO TO FIN-PUT-RDC-RDC.
           MOVE RDC-CAI-IOPC IN RDC TO RDC-CAI-IOPC IN RDC-FLD.
           MOVE RDC-IIC-IOPC IN RDC TO RDC-IIC-IOPC IN RDC-FLD.
           MOVE RDC-NUM-IDLC IN RDC TO RDC-NUM-IDLC IN RDC-FLD.
           MOVE RDC-CAI-ICLI IN RDC TO RDC-CAI-ICLI IN RDC-FLD.
           MOVE RDC-IIC-ICLI IN RDC TO RDC-IIC-ICLI IN RDC-FLD.
           MOVE RDC-NUM-DIRE IN RDC TO RDC-NUM-DIRE IN RDC-FLD.
           MOVE RDC-VAL-LIMD IN RDC TO RDC-VAL-LIMD IN RDC-FLD.
*% INCLUDE FIN_PUT_RDC_RDC OF 'COLBMRDC.TXT'
       FIN-PUT-RDC-RDC.
           EXIT.
       
       PUT-DBC-RDC SECTION.
       INI-PUT-DBC-RDC.
*% INCLUDE INI_PUT_DBC_RDC OF 'COLBMRDC.TXT'
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-RDC.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-RDC
               GO TO FIN-PUT-DBC-RDC.
           MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN RDC-FLD.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN RDC-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN RDC-FLD.
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN RDC-FLD.
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN RDC-FLD.
      * IDC-FIN --> BMCHIBCI001210.SRCAIU 
*% INCLUDE FIN_PUT_DBC_RDC OF 'COLBMRDC.TXT'
       FIN-PUT-DBC-RDC.
           EXIT.
       
       PUT-RCD-RDC SECTION.
       INI-PUT-RCD-RDC.
*% INCLUDE INI_PUT_RCD_RDC OF 'COLBMRDC.TXT'
           IF PGM-STAT-RCD-NUL
               GO TO FIN-PUT-RCD-RDC.
           IF NOT PGM-STAT-RCD-OKS
               PERFORM CLR-RCD-RDC
               GO TO FIN-PUT-RCD-RDC.
           MOVE RCD-GLS-DIRE IN RCD TO RCD-GLS-DIRE IN RDC-FLD.
           MOVE RCD-COD-LUGA IN RCD TO RCD-COD-LUGA IN RDC-FLD.
           MOVE RCD-GLS-FONO IN RCD TO RCD-GLS-FONO IN RDC-FLD.
*% INCLUDE FIN_PUT_RCD_RDC OF 'COLBMRDC.TXT'
       FIN-PUT-RCD-RDC.
           EXIT.
       
       KEY-ALL-RDC SECTION.
       INI-KEY-ALL-RDC.
*% INCLUDE INI_KEY_ALL_RDC OF 'COLBMRDC.TXT'
           PERFORM KEY-OPC-RDC.
           PERFORM KEY-RDC-RDC.
