      *
      * COLBQEJC
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:59 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "EJC"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMEJC.TXT'
       SET-TAG-COLBQEJC SECTION.
       INI-SET-TAG-COLBQEJC.
           MOVE
           '<IDNMOD NOM=$COLBQEJC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:59 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQEJC.
           EXIT.
           
       PUT-ALL-EJC SECTION.
       INI-PUT-ALL-EJC.
*% INCLUDE INI_PUT_ALL_EJC OF 'COLBMEJC.TXT'
           PERFORM PUT-EJC-EJC.
           PERFORM PUT-USR-EJC.
*% INCLUDE FIN_PUT_ALL_EJC OF 'COLBMEJC.TXT'
       FIN-PUT-ALL-EJC.
           EXIT.
       
       PUT-EJC-EJC SECTION.
       INI-PUT-EJC-EJC.
*% INCLUDE INI_PUT_EJC_EJC OF 'COLBMEJC.TXT'
           IF PGM-STAT-EJC-NUL
               GO TO FIN-PUT-EJC-EJC.
           IF NOT PGM-STAT-EJC-OKS
               PERFORM CLR-EJC-EJC
               GO TO FIN-PUT-EJC-EJC.
           MOVE EJC-COD-EJEC IN EJC TO EJC-COD-EJEC IN EJC-FLD.
*% RREG = "EJC"
*% RREG_FRM = "EJC"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE EJC-NUM-DTRN IN EJC TO EJC-NUM-DTRN IN EJC-FLD.
           MOVE EJC-NUM-MTRN IN EJC TO EJC-NUM-MTRN IN EJC-FLD.
           MOVE EJC-NUM-STRN IN EJC TO EJC-NUM-STRN IN EJC-FLD.
           MOVE EJC-NUM-ATRN IN EJC TO EJC-NUM-ATRN IN EJC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE EJC-COD-ATRI IN EJC TO EJC-COD-ATRI IN EJC-FLD.
           MOVE EJC-VAL-CMAX IN EJC TO EJC-VAL-CMAX IN EJC-FLD.
           MOVE EJC-COD-VCAM IN EJC TO EJC-COD-VCAM IN EJC-FLD.
           MOVE EJC-VAL-DMAX IN EJC TO EJC-VAL-DMAX IN EJC-FLD.
*% BUF = "EJC-FLD"
*% SIS = "TAB"
*% TAB = "CUS "
*% FLD = "EJC-COD-ATRI"
*% GLS = "FRM-GLS-ATRI"
*% TIENE_GLOSA = TRUE
*% LGL = 36
*% INCLUDE 'BUSTAB'
*% BUF = "EJC-FLD"
*% SIS = "TAB"
*% TAB = "VLR "
*% FLD = "EJC-COD-VCAM"
*% GLS = "FRM-GLS-VCAM"
*% TIENE_GLOSA = TRUE
*% LGL = 26
*% INCLUDE 'BUSTAB'
*% INCLUDE FIN_PUT_EJC_EJC OF 'COLBMEJC.TXT'
       FIN-PUT-EJC-EJC.
           EXIT.
       
       PUT-USR-EJC SECTION.
       INI-PUT-USR-EJC.
*% INCLUDE INI_PUT_USR_EJC OF 'COLBMEJC.TXT'
           IF PGM-STAT-USR-NUL
               GO TO FIN-PUT-USR-EJC.
           IF NOT PGM-STAT-USR-OKS
               PERFORM CLR-USR-EJC
               GO TO FIN-PUT-USR-EJC.
           MOVE USR-GLS-DESC IN USR TO USR-GLS-DESC IN EJC-FLD.
*% RREG = "USR"
*% RREG_FRM = "USR"
*% NOMB = "IUSR"
*% RREG = "USR"
*% RREG_FRM = "USR"
*% NOMB = "IUSR"
      * IDC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE USR-NUM-IUSR IN USR TO USR-NUM-IUSR IN EJC-FLD.
           MOVE USR-VRF-IUSR IN USR TO USR-VRF-IUSR IN EJC-FLD.
           MOVE USR-IND-IUSR IN USR TO USR-IND-IUSR IN EJC-FLD.
           MOVE USR-GLS-IUSR IN USR TO USR-GLS-IUSR IN EJC-FLD.
      * IDC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE USR-IND-VIGE IN USR TO USR-IND-VIGE IN EJC-FLD.
           MOVE USR-COD-UBIC IN USR TO USR-COD-UBIC IN EJC-FLD.
*% INCLUDE FIN_PUT_USR_EJC OF 'COLBMEJC.TXT'
       FIN-PUT-USR-EJC.
           EXIT.
       
       KEY-ALL-EJC SECTION.
       INI-KEY-ALL-EJC.
*% INCLUDE INI_KEY_ALL_EJC OF 'COLBMEJC.TXT'
           PERFORM KEY-EJC-EJC.
           PERFORM KEY-USR-EJC.
*% INCLUDE FIN_KEY_ALL_EJC OF 'COLBMEJC.TXT'
       FIN-KEY-ALL-EJC.
           EXIT.
       
       KEY-EJC-EJC SECTION.
       INI-KEY-EJC-EJC.
*% INCLUDE INI_KEY_EJC_EJC OF 'COLBMEJC.TXT'
           MOVE EJC-COD-EJEC IN EJC-FLD TO EJC-COD-EJEC IN EJC.
*% INCLUDE FIN_KEY_EJC_EJC OF 'COLBMEJC.TXT'
       FIN-KEY-EJC-EJC.
           EXIT.
       
       KEY-USR-EJC SECTION.
       INI-KEY-USR-EJC.
*% INCLUDE INI_KEY_USR_EJC OF 'COLBMEJC.TXT'
*% INCLUDE FIN_KEY_USR_EJC OF 'COLBMEJC.TXT'
       FIN-KEY-USR-EJC.
           EXIT.
       
       SCR-CLRA-EJC SECTION.
       INI-SCR-CLRA-EJC.
*% INCLUDE INI_SCR_CLRA_EJC OF 'COLBMEJC.TXT'
           PERFORM CLR-KEY-EJC.
           PERFORM CLR-FLD-EJC.
*% INCLUDE FIN_SCR_CLRA_EJC OF 'COLBMEJC.TXT'
       FIN-SCR-CLRA-EJC.
           EXIT.
       
       CLR-KEY-EJC SECTION.
       INI-CLR-KEY-EJC.
*% INCLUDE INI_CLR_KEY_EJC OF 'COLBMEJC.TXT'
           MOVE SPACES TO EJC-COD-EJEC IN EJC-FLD.
*% INCLUDE FIN_CLR_KEY_EJC OF 'COLBMEJC.TXT'
       FIN-CLR-KEY-EJC.
           EXIT.
       
       CLR-FLD-EJC SECTION.
       INI-CLR-FLD-EJC.
*% INCLUDE INI_CLR_FLD_EJC OF 'COLBMEJC.TXT'
           MOVE SPACES TO FRM-GLS-ATRI IN EJC-FLD.
           MOVE SPACES TO FRM-GLS-UBIC IN EJC-FLD.
           MOVE SPACES TO FRM-GLS-VCAM IN EJC-FLD.
           PERFORM CLR-EJC-EJC.
           PERFORM CLR-USR-EJC.
*% INCLUDE FIN_CLR_FLD_EJC OF 'COLBMEJC.TXT'
       FIN-CLR-FLD-EJC.
           EXIT.
       
       CLR-EJC-EJC SECTION.
       INI-CLR-EJC-EJC.
*% INCLUDE INI_CLR_EJC_EJC OF 'COLBMEJC.TXT'
*% RREG = "EJC"
*% RREG_FRM = "EJC"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO EJC-FEC-FTRN IN EJC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO EJC-COD-ATRI IN EJC-FLD.
           MOVE ZEROES TO EJC-VAL-CMAX IN EJC-FLD.
           MOVE SPACES TO EJC-COD-VCAM IN EJC-FLD.
           MOVE ZEROES TO EJC-VAL-DMAX IN EJC-FLD.
*% INCLUDE FIN_CLR_EJC_EJC OF 'COLBMEJC.TXT'
       FIN-CLR-EJC-EJC.
           EXIT.
       
       CLR-USR-EJC SECTION.
       INI-CLR-USR-EJC.
*% INCLUDE INI_CLR_USR_EJC OF 'COLBMEJC.TXT'
           MOVE SPACES TO USR-GLS-DESC IN EJC-FLD.
*% RREG = "USR"
*% RREG_FRM = "USR"
*% NOMB = "IUSR"
*% RREG = "USR"
*% RREG_FRM = "USR"
*% NOMB = "IUSR"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO USR-NUM-IUSR IN EJC-FLD.
           MOVE SPACES TO USR-VRF-IUSR IN EJC-FLD.
           MOVE SPACES TO USR-IND-IUSR IN EJC-FLD.
           MOVE SPACES TO USR-GLS-IUSR IN EJC-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO USR-IND-VIGE IN EJC-FLD.
           MOVE SPACES TO USR-COD-UBIC IN EJC-FLD.
*% INCLUDE FIN_CLR_USR_EJC OF 'COLBMEJC.TXT'
       FIN-CLR-USR-EJC.
           EXIT.
       
       FST-KEY-EJC SECTION.
       INI-FST-KEY-EJC.
*% INCLUDE INI_FST_KEY_EJC OF 'COLBMEJC.TXT'
           MOVE -1 TO EJC-COD-EJEC-LEN IN EJC-FLD.
*% INCLUDE FIN_FST_KEY_EJC OF 'COLBMEJC.TXT'
       FIN-FST-KEY-EJC.
           EXIT.
       
       FST-FLD-EJC SECTION.
       INI-FST-FLD-EJC.
*% INCLUDE INI_FST_FLD_EJC OF 'COLBMEJC.TXT'
           MOVE -1 TO EJC-COD-ATRI-LEN IN EJC-FLD.
*% INCLUDE FIN_FST_FLD_EJC OF 'COLBMEJC.TXT'
       FIN-FST-FLD-EJC.
           EXIT.
       
       PRO-KEY-EJC SECTION.
       INI-PRO-KEY-EJC.
*% INCLUDE INI_PRO_KEY_EJC OF 'COLBMEJC.TXT'
           PERFORM PRO-IKY-EJC.
           PERFORM PRO-AKY-EJC.
           PERFORM PRO-MKY-EJC.
*% INCLUDE FIN_PRO_KEY_EJC OF 'COLBMEJC.TXT'
       FIN-PRO-KEY-EJC.
           EXIT.
       
       PRO-IKY-EJC SECTION.
       INI-PRO-IKY-EJC.
*% INCLUDE INI_PRO_IKY_EJC OF 'COLBMEJC.TXT'
           MOVE FRM-CPIM-AEY TO EJC-COD-EJEC-ATR IN EJC-FLD.
*% INCLUDE FIN_PRO_IKY_EJC OF 'COLBMEJC.TXT'
       FIN-PRO-IKY-EJC.
           EXIT.
       
       PRO-MKY-EJC SECTION.
       INI-PRO-MKY-EJC.
*% INCLUDE INI_PRO_MKY_EJC OF 'COLBMEJC.TXT'
*% INCLUDE FIN_PRO_MKY_EJC OF 'COLBMEJC.TXT'
       FIN-PRO-MKY-EJC.
           EXIT.
       
       PRO-AKY-EJC SECTION.
       INI-PRO-AKY-EJC.
*% INCLUDE INI_PRO_AKY_EJC OF 'COLBMEJC.TXT'
*% INCLUDE FIN_PRO_AKY_EJC OF 'COLBMEJC.TXT'
       FIN-PRO-AKY-EJC.
           EXIT.
       
       UNP-KEY-EJC SECTION.
       INI-UNP-KEY-EJC.
*% INCLUDE INI_UNP_KEY_EJC OF 'COLBMEJC.TXT'
           PERFORM UNP-IKY-EJC.
           PERFORM UNP-AKY-EJC.
           PERFORM UNP-MKY-EJC.
*% INCLUDE FIN_UNP_KEY_EJC OF 'COLBMEJC.TXT'
       FIN-UNP-KEY-EJC.
           EXIT.
       
       UNP-IKY-EJC SECTION.
       INI-UNP-IKY-EJC.
*% INCLUDE INI_UNP_IKY_EJC OF 'COLBMEJC.TXT'
           MOVE FRM-CPIM-UBY TO EJC-COD-EJEC-ATR.
*% INCLUDE FIN_UNP_IKY_EJC OF 'COLBMEJC.TXT'
       FIN-UNP-IKY-EJC.
           EXIT.
       
       UNP-MKY-EJC SECTION.
       INI-UNP-MKY-EJC.
*% INCLUDE INI_UNP_MKY_EJC OF 'COLBMEJC.TXT'
*% INCLUDE FIN_UNP_MKY_EJC OF 'COLBMEJC.TXT'
       FIN-UNP-MKY-EJC.
           EXIT.
       
       UNP-AKY-EJC SECTION.
       INI-UNP-AKY-EJC.
*% INCLUDE INI_UNP_AKY_EJC OF 'COLBMEJC.TXT'
*% INCLUDE FIN_UNP_AKY_EJC OF 'COLBMEJC.TXT'
       FIN-UNP-AKY-EJC.
           EXIT.
       
       UNP-FLD-EJC SECTION.
       INI-UNP-FLD-EJC.
*% INCLUDE INI_UNP_FLD_EJC OF 'COLBMEJC.TXT'
           PERFORM UNP-EJC-EJC.
           PERFORM UNP-USR-EJC.
*% INCLUDE FIN_UNP_FLD_EJC OF 'COLBMEJC.TXT'
       FIN-UNP-FLD-EJC.
           EXIT.
       
       UNP-EJC-EJC SECTION.
       INI-UNP-EJC-EJC.
*% INCLUDE INI_UNP_EJC_EJC OF 'COLBMEJC.TXT'
           MOVE FRM-CPIM-UBY TO EJC-COD-ATRI-ATR IN EJC-FLD.
           MOVE FRM-CPIM-UBY TO EJC-VAL-CMAX-ATR IN EJC-FLD.
           MOVE FRM-CPIM-UBY TO EJC-COD-VCAM-ATR IN EJC-FLD.
           MOVE FRM-CPIM-UBY TO EJC-VAL-DMAX-ATR IN EJC-FLD.
*% INCLUDE FIN_UNP_EJC_EJC OF 'COLBMEJC.TXT'
       FIN-UNP-EJC-EJC.
           EXIT.
       
       UNP-USR-EJC SECTION.
       INI-UNP-USR-EJC.
*% INCLUDE INI_UNP_USR_EJC OF 'COLBMEJC.TXT'
*% INCLUDE FIN_UNP_USR_EJC OF 'COLBMEJC.TXT'
       FIN-UNP-USR-EJC.
           EXIT.
       
       VAL-KEY-EJC SECTION.
       INI-VAL-KEY-EJC.
           PERFORM VAL-IKY-EJC.
           PERFORM VAL-MKY-EJC.
           PERFORM VAL-AKY-EJC.
       FIN-VAL-KEY-EJC.
           EXIT.
       
       VAL-IKY-EJC SECTION.
       INI-VAL-IKY-EJC.
*% INCLUDE INI_VAL_IKY_EJC OF 'COLBMEJC.TXT'
           PERFORM VAL-EJC-COD-EJEC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EJC-COD-EJEC-LEN IN EJC-FLD
                GO TO FIN-VAL-IKY-EJC.
*% INCLUDE FIN_VAL_IKY_EJC OF 'COLBMEJC.TXT'
       FIN-VAL-IKY-EJC.
           EXIT.
       
       VAL-MKY-EJC SECTION.
       INI-VAL-MKY-EJC.
*% INCLUDE INI_VAL_MKY_EJC OF 'COLBMEJC.TXT'
*% INCLUDE FIN_VAL_MKY_EJC OF 'COLBMEJC.TXT'
       FIN-VAL-MKY-EJC.
           EXIT.
       
       VAL-AKY-EJC SECTION.
       INI-VAL-AKY-EJC.
*% INCLUDE INI_VAL_AKY_EJC OF 'COLBMEJC.TXT'
*% INCLUDE FIN_VAL_AKY_EJC OF 'COLBMEJC.TXT'
       FIN-VAL-AKY-EJC.
           EXIT.
       
       VAL-EJC-COD-EJEC SECTION.
       INI-VAL-EJC-COD-EJEC.
*% LBL = "EJC_COD_EJEC OF 'COLBMEJC.TXT'"
*% INCLUDE INI_EJC_COD_EJEC OF 'COLBMEJC.TXT'
           IF EJC-COD-EJEC IN EJC-FLD NOT > SPACES
              MOVE SPACES TO EJC-COD-EJEC IN EJC-FLD.
*% INCLUDE EJC_COD_EJEC OF 'COLBMEJC.TXT'
*% BUF = "EJC-FLD"
*% SIS = "TAB"
*% TAB = "USR "
*% FLD = "EJC-COD-EJEC"
*% GLS = ""
*% TIENE_GLOSA = FALSE
*% LGL = 0
*% INCLUDE 'VALUSR          '
*% INCLUDE FIN_EJC_COD_EJEC OF 'COLBMEJC.TXT'
       FIN-VAL-EJC-COD-EJEC.
           EXIT.
*% RREG = "USR"
*% RREG_FRM = "USR"
*% NOMB = "IUSR"
*% IZR = "S"
*% RREG = "USR"
*% RREG_FRM = "USR"
*% NOMB = "IUSR"
*% IZR = "S"
*% RREG = "EJC"
*% RREG_FRM = "EJC"
*% NOMB = "FTRN"
*% IZR = "S"
       
       VAL-CON-KEY-EJC SECTION.
       INI-VAL-CON-KEY-EJC.
*% INCLUDE INI_VAL_CON_KEY_EJC OF 'COLBMEJC.TXT'
           IF     EJC-COD-EJEC IN EJC-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-EJC.
*% INCLUDE FIN_VAL_CON_KEY_EJC OF 'COLBMEJC.TXT'
       FIN-VAL-CON-KEY-EJC.
           EXIT.
       
       VAL-CON-IKY-EJC SECTION.
       INI-VAL-CON-IKY-EJC.
*% INCLUDE INI_VAL_CON_IKY_EJC OF 'COLBMEJC.TXT'
           IF     EJC-COD-EJEC IN EJC-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-EJC.
*% INCLUDE FIN_VAL_CON_IKY_EJC OF 'COLBMEJC.TXT'
       FIN-VAL-CON-IKY-EJC.
           EXIT.
       
       VAL-NUM-KEY-EJC SECTION.
       INI-VAL-NUM-KEY-EJC.
           PERFORM VAL-NUM-IKY-EJC.
           PERFORM VAL-NUM-MKY-EJC.
           PERFORM VAL-NUM-AKY-EJC.
       FIN-VAL-NUM-KEY-EJC.
           EXIT.
       
       VAL-NUM-IKY-EJC SECTION.
       INI-VAL-NUM-IKY-EJC.
*% INCLUDE INI_VAL_NUM_IKY_EJC OF 'COLBMEJC.TXT'
*% INCLUDE FIN_VAL_NUM_IKY_EJC OF 'COLBMEJC.TXT'
       FIN-VAL-NUM-IKY-EJC.
           EXIT.
       
       VAL-NUM-MKY-EJC SECTION.
       INI-VAL-NUM-MKY-EJC.
*% INCLUDE INI_VAL_NUM_MKY_EJC OF 'COLBMEJC.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_EJC OF 'COLBMEJC.TXT'
       FIN-VAL-NUM-MKY-EJC.
           EXIT.
       
       VAL-NUM-AKY-EJC SECTION.
       INI-VAL-NUM-AKY-EJC.
*% INCLUDE INI_VAL_NUM_AKY_EJC OF 'COLBMEJC.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_EJC OF 'COLBMEJC.TXT'
       FIN-VAL-NUM-AKY-EJC.
           EXIT.
       
       VAL-NUM-FLD-EJC SECTION.
       INI-VAL-NUM-FLD-EJC.
*% INCLUDE INI_VAL_NUM_FLD_EJC OF 'COLBMEJC.TXT'
*% RREG = "USR"
*% RREG_FRM = "USR"
*% NOMB = "IUSR"
*% IZR = "S"
*% RREG = "USR"
*% RREG_FRM = "USR"
*% NOMB = "IUSR"
*% IZR = "S"
      * IDC-INI --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
*% FLD = "USR-NUM-IUSR"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% BUF = "EJC-FLD"
       VAL-NUM-{FLD}.
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-EJC.
      * IDC-FIN --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
       VAL-NUM-EJC-VAL-CMAX.
*% BUF = "EJC-FLD"
*% FLD = "EJC-VAL-CMAX"
*% ENT = "11"
*% DCM = "4"
*% NMR = "NN9V"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-EJC.
       VAL-NUM-EJC-VAL-DMAX.
*% BUF = "EJC-FLD"
*% FLD = "EJC-VAL-DMAX"
*% ENT = "11"
*% DCM = "4"
*% NMR = "NN9V"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-EJC.
*% INCLUDE FIN_VAL_NUM_FLD_EJC OF 'COLBMEJC.TXT'
       FIN-VAL-NUM-FLD-EJC.
           EXIT.
       
       EDT-KEY-EJC SECTION.
       INI-EDT-KEY-EJC.
           PERFORM EDT-IKY-EJC.
           PERFORM EDT-MKY-EJC.
           PERFORM EDT-AKY-EJC.
*% INCLUDE EDT_KEY_EJC OF 'COLBMEJC.TXT'
       FIN-EDT-KEY-EJC.
           EXIT.
       
       EDT-IKY-EJC SECTION.
       INI-EDT-IKY-EJC.
*% INCLUDE EDT_IKY_EJC OF 'COLBMEJC.TXT'
*% INCLUDE FIN_EDT_IKY_EJC OF 'COLBMEJC.TXT'
       FIN-EDT-IKY-EJC.
           EXIT.
       
       EDT-MKY-EJC SECTION.
       INI-EDT-MKY-EJC.
*% INCLUDE EDT_MKY_EJC OF 'COLBMEJC.TXT'
*% INCLUDE FIN_EDT_MKY_EJC OF 'COLBMEJC.TXT'
       FIN-EDT-MKY-EJC.
           EXIT.
       
       EDT-AKY-EJC SECTION.
       INI-EDT-AKY-EJC.
*% INCLUDE EDT_AKY_EJC OF 'COLBMEJC.TXT'
*% INCLUDE FIN_EDT_AKY_EJC OF 'COLBMEJC.TXT'
       FIN-EDT-AKY-EJC.
           EXIT.
       
       EDT-FLD-EJC SECTION.
       INI-EDT-FLD-EJC.
*% INCLUDE EDT_FLD_EJC OF 'COLBMEJC.TXT'
*% RREG = "USR"
*% RREG_FRM = "USR"
*% NOMB = "IUSR"
*% RREG = "USR"
*% RREG_FRM = "USR"
*% NOMB = "IUSR"
      * IDC-INI --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF USR-NUM-IUSR-LEN IN EJC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO USR-NUM-IUSR-LEN IN EJC-FLD
                GO TO FIN-EDT-FLD-EJC.
           MOVE USR-NUM-IUSR IN EJC-FLD TO
                USR-NUM-IUSR-EDT IN EJC-EDT.
           MOVE USR-NUM-IUSR-EDT IN EJC-EDT TO
                USR-NUM-IUSR-ALF IN EJC-FLD.
      * IDC-FIN --> BMCHIBCI011.SRCAIU EDICION NUMERICA
*% RREG = "EJC"
*% RREG_FRM = "EJC"
*% NOMB = "FTRN"
           IF EJC-VAL-CMAX-LEN IN EJC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EJC-VAL-CMAX-LEN IN EJC-FLD
                GO TO FIN-EDT-FLD-EJC.
           MOVE EJC-VAL-CMAX IN EJC-FLD TO EJC-VAL-CMAX-EDT IN EJC-EDT.
           MOVE EJC-VAL-CMAX-EDT IN EJC-EDT TO 
                EJC-VAL-CMAX-ALF IN EJC-FLD.
           IF EJC-VAL-DMAX-LEN IN EJC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EJC-VAL-DMAX-LEN IN EJC-FLD
                GO TO FIN-EDT-FLD-EJC.
           MOVE EJC-VAL-DMAX IN EJC-FLD TO EJC-VAL-DMAX-EDT IN EJC-EDT.
           MOVE EJC-VAL-DMAX-EDT IN EJC-EDT TO 
                EJC-VAL-DMAX-ALF IN EJC-FLD.
*% INCLUDE FIN_EDT_FLD_EJC OF 'COLBMEJC.TXT'
       FIN-EDT-FLD-EJC.
           EXIT.
*% INCLUDE EOF_BQ OF 'COLBMEJC.TXT'
*% INCLUDE EOF OF 'COLBMEJC.TXT'
*% INCLUDE EOF_IBM OF 'COLBMEJC.TXT'
