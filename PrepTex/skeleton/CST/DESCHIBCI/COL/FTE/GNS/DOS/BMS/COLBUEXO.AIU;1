*% FRM = "EXO"
      *
      * COLBUEXO
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:00 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMEXO.TXT'
       SET-TAG-COLBUEXO SECTION.
       INI-SET-TAG-COLBUEXO.
           MOVE
           '<IDNMOD NOM=$COLBUEXO$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:00 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUEXO.
           EXIT.
           
       RET-ALL-EXO SECTION.
       INI-RET-ALL-EXO.
*% INCLUDE INI_RET_ALL_EXO OF 'COLBMEXO.TXT'
           PERFORM KEY-ALL-EXO.
           PERFORM RET-EXO-EXO.
*% INCLUDE FIN_RET_ALL_EXO OF 'COLBMEXO.TXT'
       FIN-RET-ALL-EXO.
           EXIT.
       
       RET-EXO-EXO SECTION.
       INI-RET-EXO-EXO.
*% INCLUDE INI_RET_EXO_EXO OF 'COLBMEXO.TXT'
           MOVE EXO-COD-TABL IN EXO-FLD TO EXO-COD-TABL IN EXO.
           MOVE EXO-COD-USER IN EXO-FLD TO EXO-COD-USER IN EXO.
           MOVE EXO-GLS-CMTO IN EXO-FLD TO EXO-GLS-CMTO IN EXO.
           MOVE EXO-MSC-STAT IN EXO-FLD TO EXO-MSC-STAT IN EXO.
           MOVE EXO-HRS-FINI IN EXO-FLD TO EXO-HRS-FINI IN EXO.
*% RREG = "EXO"
*% RREG_FRM = "EXO"
*% NOMB = "FINI"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE EXO-NUM-DINI IN EXO-FLD TO EXO-NUM-DINI IN EXO.
           MOVE EXO-NUM-MINI IN EXO-FLD TO EXO-NUM-MINI IN EXO.
           MOVE EXO-NUM-SINI IN EXO-FLD TO EXO-NUM-SINI IN EXO.
           MOVE EXO-NUM-AINI IN EXO-FLD TO EXO-NUM-AINI IN EXO.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE EXO-HRS-FFIN IN EXO-FLD TO EXO-HRS-FFIN IN EXO.
*% RREG = "EXO"
*% RREG_FRM = "EXO"
*% NOMB = "FFIN"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE EXO-NUM-DFIN IN EXO-FLD TO EXO-NUM-DFIN IN EXO.
           MOVE EXO-NUM-MFIN IN EXO-FLD TO EXO-NUM-MFIN IN EXO.
           MOVE EXO-NUM-SFIN IN EXO-FLD TO EXO-NUM-SFIN IN EXO.
           MOVE EXO-NUM-AFIN IN EXO-FLD TO EXO-NUM-AFIN IN EXO.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_RET_EXO_EXO OF 'COLBMEXO.TXT'
       FIN-RET-EXO-EXO.
           EXIT.
       
       KEY-ALL-EXO SECTION.
       INI-KEY-ALL-EXO.
*% INCLUDE INI_KEY_ALL_EXO OF 'COLBMEXO.TXT'
           PERFORM KEY-EXO-EXO.
*% INCLUDE FIN_KEY_ALL_EXO OF 'COLBMEXO.TXT'
       FIN-KEY-ALL-EXO.
           EXIT.
       
       KEY-EXO-EXO SECTION.
       INI-KEY-EXO-EXO.
*% INCLUDE INI_KEY_EXO_EXO OF 'COLBMEXO.TXT'
           MOVE EXO-COD-SIST IN EXO-FLD TO EXO-COD-SIST IN EXO.
           MOVE EXO-COD-PROC IN EXO-FLD TO EXO-COD-PROC IN EXO.
           MOVE EXO-CAI-IEXO IN EXO-FLD TO EXO-CAI-IEXO IN EXO.
           MOVE EXO-IIC-IEXO IN EXO-FLD TO EXO-IIC-IEXO IN EXO.
           MOVE EXO-NUM-IEXO IN EXO-FLD TO EXO-NUM-IEXO IN EXO.
*% INCLUDE FIN_KEY_EXO_EXO OF 'COLBMEXO.TXT'
       FIN-KEY-EXO-EXO.
           EXIT.
       
       SCR-CLRA-EXO SECTION.
       INI-SCR-CLRA-EXO.
*% INCLUDE INI_SCR_CLRA_EXO OF 'COLBMEXO.TXT'
           PERFORM CLR-KEY-EXO.
           PERFORM CLR-FLD-EXO.
*% INCLUDE FIN_SCR_CLRA_EXO OF 'COLBMEXO.TXT'
       FIN-SCR-CLRA-EXO.
           EXIT.
       
       CLR-KEY-EXO SECTION.
       INI-CLR-KEY-EXO.
*% INCLUDE INI_CLR_KEY_EXO OF 'COLBMEXO.TXT'
           MOVE SPACES TO EXO-COD-SIST IN EXO-FLD.
           MOVE SPACES TO EXO-COD-PROC IN EXO-FLD.
           MOVE SPACES TO EXO-CAI-IEXO IN EXO-FLD.
           MOVE ZEROES TO EXO-IIC-IEXO IN EXO-FLD.
           MOVE ZEROES TO EXO-NUM-IEXO IN EXO-FLD.
*% INCLUDE FIN_CLR_KEY_EXO OF 'COLBMEXO.TXT'
       FIN-CLR-KEY-EXO.
           EXIT.
       
       CLR-FLD-EXO SECTION.
       INI-CLR-FLD-EXO.
*% INCLUDE INI_CLR_FLD_EXO OF 'COLBMEXO.TXT'
           PERFORM CLR-EXO-EXO.
*% INCLUDE FIN_CLR_FLD_EXO OF 'COLBMEXO.TXT'
       FIN-CLR-FLD-EXO.
           EXIT.
       
       CLR-EXO-EXO SECTION.
       INI-CLR-EXO-EXO.
*% INCLUDE INI_CLR_EXO_EXO OF 'COLBMEXO.TXT'
           MOVE SPACES TO EXO-COD-TABL IN EXO-FLD.
           MOVE SPACES TO EXO-COD-USER IN EXO-FLD.
           MOVE SPACES TO EXO-GLS-CMTO IN EXO-FLD.
           MOVE SPACES TO EXO-MSC-STAT IN EXO-FLD.
           MOVE SPACES TO EXO-HRS-FINI IN EXO-FLD.
*% RREG = "EXO"
*% RREG_FRM = "EXO"
*% NOMB = "FINI"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO EXO-FEC-FINI IN EXO-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO EXO-HRS-FFIN IN EXO-FLD.
*% RREG = "EXO"
*% RREG_FRM = "EXO"
*% NOMB = "FFIN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO EXO-FEC-FFIN IN EXO-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_EXO_EXO OF 'COLBMEXO.TXT'
       FIN-CLR-EXO-EXO.
           EXIT.
       
       FST-KEY-EXO SECTION.
       INI-FST-KEY-EXO.
*% INCLUDE INI_FST_KEY_EXO OF 'COLBMEXO.TXT'
           MOVE -1 TO EXO-COD-PROC-LEN IN EXO-FLD.
*% INCLUDE FIN_FST_KEY_EXO OF 'COLBMEXO.TXT'
       FIN-FST-KEY-EXO.
           EXIT.
       
       FST-FLD-EXO SECTION.
       INI-FST-FLD-EXO.
*% INCLUDE INI_FST_FLD_EXO OF 'COLBMEXO.TXT'
           MOVE -1 TO EXO-COD-TABL-LEN IN EXO-FLD.
*% INCLUDE FIN_FST_FLD_EXO OF 'COLBMEXO.TXT'
       FIN-FST-FLD-EXO.
           EXIT.
       
       PRO-KEY-EXO SECTION.
       INI-PRO-KEY-EXO.
*% INCLUDE INI_PRO_KEY_EXO OF 'COLBMEXO.TXT'
           PERFORM PRO-MKY-EXO.
*% INCLUDE FIN_PRO_KEY_EXO OF 'COLBMEXO.TXT'
       FIN-PRO-KEY-EXO.
           EXIT.
       
       PRO-MKY-EXO SECTION.
       INI-PRO-MKY-EXO.
*% INCLUDE INI_PRO_MKY_EXO OF 'COLBMEXO.TXT'
*% INCLUDE FIN_PRO_MKY_EXO OF 'COLBMEXO.TXT'
       FIN-PRO-MKY-EXO.
           EXIT.
       
       PRO-FLD-EXO SECTION.
       INI-PRO-FLD-EXO.
*% INCLUDE INI_PRO_FLD_EXO OF 'COLBMEXO.TXT'
           PERFORM PRO-EXO-EXO.
*% INCLUDE FIN_PRO_FLD_EXO OF 'COLBMEXO.TXT'
       FIN-PRO-FLD-EXO.
           EXIT.
       
       PRO-EXO-EXO SECTION.
       INI-PRO-EXO-EXO.
*% INCLUDE INI_PRO_EXO_EXO OF 'COLBMEXO.TXT'
           MOVE FRM-CPIM-AEY TO EXO-COD-TABL-ATR IN EXO-FLD.
           MOVE FRM-CPIM-AEY TO EXO-COD-USER-ATR IN EXO-FLD.
           MOVE FRM-CPIM-AEY TO EXO-GLS-CMTO-ATR IN EXO-FLD.
*% INCLUDE FIN_PRO_EXO_EXO OF 'COLBMEXO.TXT'
       FIN-PRO-EXO-EXO.
           EXIT.
       
       UNP-KEY-EXO SECTION.
       INI-UNP-KEY-EXO.
*% INCLUDE INI_UNP_KEY_EXO OF 'COLBMEXO.TXT'
           PERFORM UNP-IKY-EXO.
           PERFORM UNP-AKY-EXO.
           PERFORM UNP-MKY-EXO.
*% INCLUDE FIN_UNP_KEY_EXO OF 'COLBMEXO.TXT'
       FIN-UNP-KEY-EXO.
           EXIT.
       
       UNP-IKY-EXO SECTION.
       INI-UNP-IKY-EXO.
*% INCLUDE INI_UNP_IKY_EXO OF 'COLBMEXO.TXT'
           MOVE FRM-CPIM-UBY TO EXO-COD-PROC-ATR.
           MOVE FRM-CPIM-UBY TO EXO-CAI-IEXO-ATR.
           MOVE FRM-CPIM-UBY TO EXO-IIC-IEXO-ATR.
           MOVE FRM-CPIM-UBY TO EXO-NUM-IEXO-ATR.
*% INCLUDE FIN_UNP_IKY_EXO OF 'COLBMEXO.TXT'
       FIN-UNP-IKY-EXO.
           EXIT.
       
       UNP-MKY-EXO SECTION.
       INI-UNP-MKY-EXO.
*% INCLUDE INI_UNP_MKY_EXO OF 'COLBMEXO.TXT'
*% INCLUDE FIN_UNP_MKY_EXO OF 'COLBMEXO.TXT'
       FIN-UNP-MKY-EXO.
           EXIT.
       
       UNP-AKY-EXO SECTION.
       INI-UNP-AKY-EXO.
*% INCLUDE INI_UNP_AKY_EXO OF 'COLBMEXO.TXT'
*% INCLUDE FIN_UNP_AKY_EXO OF 'COLBMEXO.TXT'
       FIN-UNP-AKY-EXO.
           EXIT.
       
       UNP-FLD-EXO SECTION.
       INI-UNP-FLD-EXO.
*% INCLUDE INI_UNP_FLD_EXO OF 'COLBMEXO.TXT'
           PERFORM UNP-EXO-EXO.
*% INCLUDE FIN_UNP_FLD_EXO OF 'COLBMEXO.TXT'
       FIN-UNP-FLD-EXO.
           EXIT.
       
       UNP-EXO-EXO SECTION.
       INI-UNP-EXO-EXO.
*% INCLUDE INI_UNP_EXO_EXO OF 'COLBMEXO.TXT'
           MOVE FRM-CPIM-UBY TO EXO-COD-TABL-ATR IN EXO-FLD.
           MOVE FRM-CPIM-UBY TO EXO-COD-USER-ATR IN EXO-FLD.
           MOVE FRM-CPIM-UBY TO EXO-GLS-CMTO-ATR IN EXO-FLD.
*% INCLUDE FIN_UNP_EXO_EXO OF 'COLBMEXO.TXT'
       FIN-UNP-EXO-EXO.
           EXIT.
       
       VAL-MKY-EXO SECTION.
       INI-VAL-MKY-EXO.
*% INCLUDE INI_VAL_MKY_EXO OF 'COLBMEXO.TXT'
*% INCLUDE FIN_VAL_MKY_EXO OF 'COLBMEXO.TXT'
       FIN-VAL-MKY-EXO.
           EXIT.
       
       VAL-FLD-EXO SECTION.
       INI-VAL-FLD-EXO.
*% INCLUDE INI_VAL_FLD_EXO OF 'COLBMEXO.TXT'
           PERFORM VAL-EXO-COD-TABL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EXO-COD-TABL-LEN IN EXO-FLD
                GO TO FIN-VAL-FLD-EXO.
           PERFORM VAL-EXO-COD-USER.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EXO-COD-USER-LEN IN EXO-FLD
                GO TO FIN-VAL-FLD-EXO.
           PERFORM VAL-EXO-GLS-CMTO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EXO-GLS-CMTO-LEN IN EXO-FLD
                GO TO FIN-VAL-FLD-EXO.
           PERFORM VAL-EXO-MSC-STAT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EXO-MSC-STAT-LEN IN EXO-FLD
                GO TO FIN-VAL-FLD-EXO.
           PERFORM VAL-EXO-HRS-FINI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EXO-HRS-FINI-LEN IN EXO-FLD
                GO TO FIN-VAL-FLD-EXO.
*% RREG = "EXO"
*% RREG_FRM = "EXO"
*% NOMB = "FINI"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-EXO-FEC-FINI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                EXO-FEC-FINI-LEN IN EXO-FLD
                GO TO FIN-VAL-FLD-EXO.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-EXO-HRS-FFIN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EXO-HRS-FFIN-LEN IN EXO-FLD
                GO TO FIN-VAL-FLD-EXO.
*% RREG = "EXO"
*% RREG_FRM = "EXO"
*% NOMB = "FFIN"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-EXO-FEC-FFIN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                EXO-FEC-FFIN-LEN IN EXO-FLD
                GO TO FIN-VAL-FLD-EXO.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% INCLUDE FIN_VAL_FLD_EXO OF 'COLBMEXO.TXT'
       FIN-VAL-FLD-EXO.
           EXIT.
       
       VAL-EXO-COD-TABL SECTION.
       INI-VAL-EXO-COD-TABL.
*% LBL = "EXO_COD_TABL OF 'COLBMEXO.TXT'"
*% INCLUDE INI_EXO_COD_TABL OF 'COLBMEXO.TXT'
           IF EXO-COD-TABL IN EXO-FLD NOT > SPACES
              MOVE SPACES TO EXO-COD-TABL IN EXO-FLD.
*% INCLUDE EXO_COD_TABL OF 'COLBMEXO.TXT'
*% INCLUDE FIN_EXO_COD_TABL OF 'COLBMEXO.TXT'
       FIN-VAL-EXO-COD-TABL.
           EXIT.
       
       VAL-EXO-COD-USER SECTION.
       INI-VAL-EXO-COD-USER.
*% LBL = "EXO_COD_USER OF 'COLBMEXO.TXT'"
*% INCLUDE INI_EXO_COD_USER OF 'COLBMEXO.TXT'
           IF EXO-COD-USER IN EXO-FLD NOT > SPACES
              MOVE SPACES TO EXO-COD-USER IN EXO-FLD.
*% INCLUDE EXO_COD_USER OF 'COLBMEXO.TXT'
*% INCLUDE FIN_EXO_COD_USER OF 'COLBMEXO.TXT'
       FIN-VAL-EXO-COD-USER.
           EXIT.
       
       VAL-EXO-GLS-CMTO SECTION.
       INI-VAL-EXO-GLS-CMTO.
*% LBL = "EXO_GLS_CMTO OF 'COLBMEXO.TXT'"
*% INCLUDE INI_EXO_GLS_CMTO OF 'COLBMEXO.TXT'
           IF EXO-GLS-CMTO IN EXO-FLD NOT > SPACES
              MOVE SPACES TO EXO-GLS-CMTO IN EXO-FLD.
*% INCLUDE EXO_GLS_CMTO OF 'COLBMEXO.TXT'
*% INCLUDE FIN_EXO_GLS_CMTO OF 'COLBMEXO.TXT'
       FIN-VAL-EXO-GLS-CMTO.
           EXIT.
       
       VAL-EXO-MSC-STAT SECTION.
       INI-VAL-EXO-MSC-STAT.
*% LBL = "EXO_MSC_STAT OF 'COLBMEXO.TXT'"
*% INCLUDE INI_EXO_MSC_STAT OF 'COLBMEXO.TXT'
*% INCLUDE EXO_MSC_STAT OF 'COLBMEXO.TXT'
*% INCLUDE FIN_EXO_MSC_STAT OF 'COLBMEXO.TXT'
       FIN-VAL-EXO-MSC-STAT.
           EXIT.
       
       VAL-EXO-HRS-FINI SECTION.
       INI-VAL-EXO-HRS-FINI.
*% LBL = "EXO_HRS_FINI OF 'COLBMEXO.TXT'"
*% INCLUDE INI_EXO_HRS_FINI OF 'COLBMEXO.TXT'
*% INCLUDE EXO_HRS_FINI OF 'COLBMEXO.TXT'
*% INCLUDE FIN_EXO_HRS_FINI OF 'COLBMEXO.TXT'
       FIN-VAL-EXO-HRS-FINI.
           EXIT.
*% RREG = "EXO"
*% RREG_FRM = "EXO"
*% NOMB = "FINI"
*% OBL = "S"
*% LBL = "EXO_FEC_FINI OF 'COLBMEXO.TXT'"
*% IZR = "S"
*% ZRO = "S"
*% FRM_IFLD = ""
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-EXO-FEC-FINI SECTION.
       INI-VAL-EXO-FEC-FINI.
           IF EXO-FEC-FINI IN EXO-FLD  NOT > ZEROES
              MOVE ZEROES TO EXO-FEC-FINI IN EXO-FLD.
*% INCLUDE INI_EXO_FEC_FINI OF 'COLBMEXO.TXT'
*% INCLUDE EXO_FEC_FINI OF 'COLBMEXO.TXT'
*% INCLUDE FIN_EXO_FEC_FINI OF 'COLBMEXO.TXT'
       FIN-VAL-EXO-FEC-FINI.
           EXIT.
      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-EXO-HRS-FFIN SECTION.
       INI-VAL-EXO-HRS-FFIN.
*% LBL = "EXO_HRS_FFIN OF 'COLBMEXO.TXT'"
*% INCLUDE INI_EXO_HRS_FFIN OF 'COLBMEXO.TXT'
*% INCLUDE EXO_HRS_FFIN OF 'COLBMEXO.TXT'
*% INCLUDE FIN_EXO_HRS_FFIN OF 'COLBMEXO.TXT'
       FIN-VAL-EXO-HRS-FFIN.
           EXIT.
*% RREG = "EXO"
*% RREG_FRM = "EXO"
*% NOMB = "FFIN"
*% OBL = "S"
*% LBL = "EXO_FEC_FFIN OF 'COLBMEXO.TXT'"
*% IZR = "S"
*% ZRO = "S"
*% FRM_IFLD = ""
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-EXO-FEC-FFIN SECTION.
       INI-VAL-EXO-FEC-FFIN.
           IF EXO-FEC-FFIN IN EXO-FLD  NOT > ZEROES
              MOVE ZEROES TO EXO-FEC-FFIN IN EXO-FLD.
*% INCLUDE INI_EXO_FEC_FFIN OF 'COLBMEXO.TXT'
*% INCLUDE EXO_FEC_FFIN OF 'COLBMEXO.TXT'
*% INCLUDE FIN_EXO_FEC_FFIN OF 'COLBMEXO.TXT'
       FIN-VAL-EXO-FEC-FFIN.
           EXIT.
      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-CON-FLD-EXO SECTION.
       INI-VAL-CON-FLD-EXO.
*% INCLUDE VAL_CON_FLD_EXO OF 'COLBMEXO.TXT'
       FIN-VAL-CON-FLD-EXO.
           EXIT.
       
       VAL-CON-EXO SECTION.
       INI-VAL-CON-EXO.
*% INCLUDE VAL_CON_EXO OF 'COLBMEXO.TXT'
       FIN-VAL-CON-EXO.
           EXIT.
       
       VAL-NUM-KEY-EXO SECTION.
       INI-VAL-NUM-KEY-EXO.
           PERFORM VAL-NUM-IKY-EXO.
           PERFORM VAL-NUM-MKY-EXO.
           PERFORM VAL-NUM-AKY-EXO.
       FIN-VAL-NUM-KEY-EXO.
           EXIT.
       
       VAL-NUM-IKY-EXO SECTION.
       INI-VAL-NUM-IKY-EXO.
*% INCLUDE INI_VAL_NUM_IKY_EXO OF 'COLBMEXO.TXT'
       VAL-NUM-EXO-IIC-IEXO.
*% BUF = "EXO-FLD"
*% FLD = "EXO-IIC-IEXO"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-EXO.
       VAL-NUM-EXO-NUM-IEXO.
*% BUF = "EXO-FLD"
*% FLD = "EXO-NUM-IEXO"
*% ENT = "3"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-EXO.
*% INCLUDE FIN_VAL_NUM_IKY_EXO OF 'COLBMEXO.TXT'
       FIN-VAL-NUM-IKY-EXO.
           EXIT.
       
       VAL-NUM-MKY-EXO SECTION.
       INI-VAL-NUM-MKY-EXO.
*% INCLUDE INI_VAL_NUM_MKY_EXO OF 'COLBMEXO.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_EXO OF 'COLBMEXO.TXT'
       FIN-VAL-NUM-MKY-EXO.
           EXIT.
       
       VAL-NUM-AKY-EXO SECTION.
       INI-VAL-NUM-AKY-EXO.
*% INCLUDE INI_VAL_NUM_AKY_EXO OF 'COLBMEXO.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_EXO OF 'COLBMEXO.TXT'
       FIN-VAL-NUM-AKY-EXO.
           EXIT.
       
       VAL-NUM-FLD-EXO SECTION.
       INI-VAL-NUM-FLD-EXO.
*% INCLUDE INI_VAL_NUM_FLD_EXO OF 'COLBMEXO.TXT'
*% INCLUDE FIN_VAL_NUM_FLD_EXO OF 'COLBMEXO.TXT'
       FIN-VAL-NUM-FLD-EXO.
           EXIT.
       
       EDT-KEY-EXO SECTION.
       INI-EDT-KEY-EXO.
           PERFORM EDT-IKY-EXO.
           PERFORM EDT-MKY-EXO.
           PERFORM EDT-AKY-EXO.
*% INCLUDE EDT_KEY_EXO OF 'COLBMEXO.TXT'
       FIN-EDT-KEY-EXO.
           EXIT.
       
       EDT-IKY-EXO SECTION.
       INI-EDT-IKY-EXO.
*% INCLUDE EDT_IKY_EXO OF 'COLBMEXO.TXT'
           IF EXO-IIC-IEXO-LEN IN EXO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EXO-IIC-IEXO-LEN IN EXO-FLD
                GO TO FIN-EDT-IKY-EXO.
           MOVE EXO-IIC-IEXO IN EXO-FLD TO EXO-IIC-IEXO-EDT IN EXO-EDT.
           MOVE EXO-IIC-IEXO-EDT IN EXO-EDT TO 
                EXO-IIC-IEXO-ALF IN EXO-FLD.
           IF EXO-NUM-IEXO-LEN IN EXO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EXO-NUM-IEXO-LEN IN EXO-FLD
                GO TO FIN-EDT-IKY-EXO.
           MOVE EXO-NUM-IEXO IN EXO-FLD TO EXO-NUM-IEXO-EDT IN EXO-EDT.
           MOVE EXO-NUM-IEXO-EDT IN EXO-EDT TO 
                EXO-NUM-IEXO-ALF IN EXO-FLD.
*% INCLUDE FIN_EDT_IKY_EXO OF 'COLBMEXO.TXT'
       FIN-EDT-IKY-EXO.
           EXIT.
       
       EDT-MKY-EXO SECTION.
       INI-EDT-MKY-EXO.
