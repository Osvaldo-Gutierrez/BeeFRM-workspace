       UNP-DBC-RVO SECTION.
       INI-UNP-DBC-RVO.
*% INCLUDE INI_UNP_DBC_RVO OF 'COLBMRVO.TXT'
*% INCLUDE FIN_UNP_DBC_RVO OF 'COLBMRVO.TXT'
       FIN-UNP-DBC-RVO.
           EXIT.
       
       UNP-RDC-RVO SECTION.
       INI-UNP-RDC-RVO.
*% INCLUDE INI_UNP_RDC_RVO OF 'COLBMRVO.TXT'
*% INCLUDE FIN_UNP_RDC_RVO OF 'COLBMRVO.TXT'
       FIN-UNP-RDC-RVO.
           EXIT.
       
       VAL-MKY-RVO SECTION.
       INI-VAL-MKY-RVO.
*% INCLUDE INI_VAL_MKY_RVO OF 'COLBMRVO.TXT'
           PERFORM VAL-RDC-CAI-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RDC-CAI-ICLI-LEN IN RVO-FLD
                GO TO FIN-VAL-MKY-RVO.
           PERFORM VAL-RDC-IIC-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RDC-IIC-ICLI-LEN IN RVO-FLD
                GO TO FIN-VAL-MKY-RVO.
*% INCLUDE FIN_VAL_MKY_RVO OF 'COLBMRVO.TXT'
       FIN-VAL-MKY-RVO.
           EXIT.
       
       VAL-FLD-RVO SECTION.
       INI-VAL-FLD-RVO.
*% INCLUDE INI_VAL_FLD_RVO OF 'COLBMRVO.TXT'
           PERFORM VAL-DBC-GLS-NOMC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DBC-GLS-NOMC-LEN IN RVO-FLD
                GO TO FIN-VAL-FLD-RVO.
           PERFORM VAL-DBC-IND-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DBC-IND-ICLI-LEN IN RVO-FLD
                GO TO FIN-VAL-FLD-RVO.
           PERFORM VAL-DBC-GLS-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DBC-GLS-ICLI-LEN IN RVO-FLD
                GO TO FIN-VAL-FLD-RVO.
           PERFORM VAL-DBC-NUM-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DBC-NUM-ICLI-LEN IN RVO-FLD
                GO TO FIN-VAL-FLD-RVO.
           PERFORM VAL-DBC-VRF-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DBC-VRF-ICLI-LEN IN RVO-FLD
                GO TO FIN-VAL-FLD-RVO.
           PERFORM VAL-OPC-COD-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPC-COD-TOOC-LEN IN RVO-FLD
                GO TO FIN-VAL-FLD-RVO.
           PERFORM VAL-OPC-COD-AOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPC-COD-AOOC-LEN IN RVO-FLD
                GO TO FIN-VAL-FLD-RVO.
           PERFORM VAL-OPC-COD-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPC-COD-VCOC-LEN IN RVO-FLD
                GO TO FIN-VAL-FLD-RVO.
           PERFORM VAL-FRM-GLS-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-TOOC-LEN IN RVO-FLD
                GO TO FIN-VAL-FLD-RVO.
           PERFORM VAL-FRM-GLS-AOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-AOOC-LEN IN RVO-FLD
                GO TO FIN-VAL-FLD-RVO.
           PERFORM VAL-FRM-GLS-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-VCOC-LEN IN RVO-FLD
                GO TO FIN-VAL-FLD-RVO.
           PERFORM VAL-OPC-FEC-FCOL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPC-FEC-FCOL-LEN IN RVO-FLD
                GO TO FIN-VAL-FLD-RVO.
           PERFORM VAL-OPC-VAL-CRED.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPC-VAL-CRED-LEN IN RVO-FLD
                GO TO FIN-VAL-FLD-RVO.
           PERFORM VAL-FRM-CAI-IVCD
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 18
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE -1 TO FRM-CAI-IVCD-LEN IN RVO-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-RVO.
           PERFORM VAL-FRM-IIC-IVCD
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 18
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE -1 TO FRM-IIC-IVCD-LEN IN RVO-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-RVO.
*% INCLUDE FIN_VAL_FLD_RVO OF 'COLBMRVO.TXT'
       FIN-VAL-FLD-RVO.
           EXIT.
       
