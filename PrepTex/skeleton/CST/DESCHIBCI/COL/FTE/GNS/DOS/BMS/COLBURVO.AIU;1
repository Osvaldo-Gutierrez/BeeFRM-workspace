*% INCLUDE FIN_FST_KEY_RVO OF 'COLBMRVO.TXT'
       FIN-FST-KEY-RVO.
           EXIT.
       
       FST-FLD-RVO SECTION.
       INI-FST-FLD-RVO.
*% INCLUDE INI_FST_FLD_RVO OF 'COLBMRVO.TXT'
           MOVE -1 TO FRM-CAI-IVCD-LEN IN RVO-FLD(1).
*% INCLUDE FIN_FST_FLD_RVO OF 'COLBMRVO.TXT'
       FIN-FST-FLD-RVO.
           EXIT.
       
       PRO-KEY-RVO SECTION.
       INI-PRO-KEY-RVO.
*% INCLUDE INI_PRO_KEY_RVO OF 'COLBMRVO.TXT'
           PERFORM PRO-MKY-RVO.
