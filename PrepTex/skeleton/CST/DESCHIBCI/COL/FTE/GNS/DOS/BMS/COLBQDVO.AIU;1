*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "DVO"
*% PGM_BQ = TRUE
       PUT-ALL-DVO SECTION.
       INI-PUT-ALL-DVO.
*% INCLUDE INI_PUT_ALL_DVO OF 'COLBMDVO.TXT'
           PERFORM PUT-DVO-DVO.
*% INCLUDE FIN_PUT_ALL_DVO OF 'COLBMDVO.TXT'
       FIN-PUT-ALL-DVO.
           EXIT.
       
       PUT-DVO-DVO SECTION.
       INI-PUT-DVO-DVO.
*% INCLUDE INI_PUT_DVO_DVO OF 'COLBMDVO.TXT'
           IF PGM-STAT-DVO-NUL
               GO TO FIN-PUT-DVO-DVO.
           IF NOT PGM-STAT-DVO-OKS
               PERFORM CLR-DVO-DVO
               GO TO FIN-PUT-DVO-DVO.
           MOVE DVO-CAI-IOPC IN DVO TO DVO-CAI-IOPC IN DVO-FLD.
           MOVE DVO-IIC-IOPC IN DVO TO DVO-IIC-IOPC IN DVO-FLD.
           MOVE DVO-NUM-IDLC IN DVO TO DVO-NUM-IDLC IN DVO-FLD.
           MOVE DVO-SGV-ICTB-IPC IN DVO TO DVO-SGV-ICTB-IPC IN DVO-FLD.
           MOVE DVO-SGV-ICTB-ISD IN DVO TO DVO-SGV-ICTB-ISD IN DVO-FLD.
           MOVE DVO-SGV-ICTB-IMD IN DVO TO DVO-SGV-ICTB-IMD IN DVO-FLD.
           MOVE DVO-SGV-ICTB-RPC IN DVO TO DVO-SGV-ICTB-RPC IN DVO-FLD.
           MOVE DVO-SGV-ICTB-RSD IN DVO TO DVO-SGV-ICTB-RSD IN DVO-FLD.
           MOVE DVO-SGV-ICTB-RMD IN DVO TO DVO-SGV-ICTB-RMD IN DVO-FLD.
*% INCLUDE FIN_PUT_DVO_DVO OF 'COLBMDVO.TXT'
       FIN-PUT-DVO-DVO.
           EXIT.
       
       KEY-ALL-DVO SECTION.
       INI-KEY-ALL-DVO.
*% INCLUDE INI_KEY_ALL_DVO OF 'COLBMDVO.TXT'
           PERFORM KEY-DVO-DVO.
*% INCLUDE FIN_KEY_ALL_DVO OF 'COLBMDVO.TXT'
       FIN-KEY-ALL-DVO.
           EXIT.
       
       KEY-DVO-DVO SECTION.
       INI-KEY-DVO-DVO.
*% INCLUDE INI_KEY_DVO_DVO OF 'COLBMDVO.TXT'
           MOVE DVO-CAI-IOPC IN DVO-FLD TO DVO-CAI-IOPC IN DVO.
           MOVE DVO-IIC-IOPC IN DVO-FLD TO DVO-IIC-IOPC IN DVO.
           MOVE DVO-NUM-IDLC IN DVO-FLD TO DVO-NUM-IDLC IN DVO.
*% INCLUDE FIN_KEY_DVO_DVO OF 'COLBMDVO.TXT'
       FIN-KEY-DVO-DVO.
           EXIT.
       
       SCR-CLRA-DVO SECTION.
       INI-SCR-CLRA-DVO.
*% INCLUDE INI_SCR_CLRA_DVO OF 'COLBMDVO.TXT'
           PERFORM CLR-KEY-DVO.
           PERFORM CLR-FLD-DVO.
*% INCLUDE FIN_SCR_CLRA_DVO OF 'COLBMDVO.TXT'
       FIN-SCR-CLRA-DVO.
           EXIT.
       
       CLR-KEY-DVO SECTION.
       INI-CLR-KEY-DVO.
*% INCLUDE INI_CLR_KEY_DVO OF 'COLBMDVO.TXT'
           MOVE SPACES TO DVO-CAI-IOPC IN DVO-FLD.
           MOVE ZEROES TO DVO-IIC-IOPC IN DVO-FLD.
           MOVE ZEROES TO DVO-NUM-IDLC IN DVO-FLD.
*% INCLUDE FIN_CLR_KEY_DVO OF 'COLBMDVO.TXT'
       FIN-CLR-KEY-DVO.
           EXIT.
       
       CLR-FLD-DVO SECTION.
       INI-CLR-FLD-DVO.
*% INCLUDE INI_CLR_FLD_DVO OF 'COLBMDVO.TXT'
           PERFORM CLR-DVO-DVO.
*% INCLUDE FIN_CLR_FLD_DVO OF 'COLBMDVO.TXT'
       FIN-CLR-FLD-DVO.
           EXIT.
       
       CLR-DVO-DVO SECTION.
       INI-CLR-DVO-DVO.
*% INCLUDE INI_CLR_DVO_DVO OF 'COLBMDVO.TXT'
           MOVE ZEROES TO DVO-SGV-ICTB-IPC IN DVO-FLD.
           MOVE ZEROES TO DVO-SGV-ICTB-ISD IN DVO-FLD.
           MOVE ZEROES TO DVO-SGV-ICTB-IMD IN DVO-FLD.
           MOVE ZEROES TO DVO-SGV-ICTB-RPC IN DVO-FLD.
           MOVE ZEROES TO DVO-SGV-ICTB-RSD IN DVO-FLD.
           MOVE ZEROES TO DVO-SGV-ICTB-RMD IN DVO-FLD.
*% INCLUDE FIN_CLR_DVO_DVO OF 'COLBMDVO.TXT'
       FIN-CLR-DVO-DVO.
           EXIT.
       
       FST-KEY-DVO SECTION.
       INI-FST-KEY-DVO.
*% INCLUDE INI_FST_KEY_DVO OF 'COLBMDVO.TXT'
           MOVE -1 TO DVO-CAI-IOPC-LEN IN DVO-FLD.
*% INCLUDE FIN_FST_KEY_DVO OF 'COLBMDVO.TXT'
       FIN-FST-KEY-DVO.
           EXIT.
       
       FST-FLD-DVO SECTION.
       INI-FST-FLD-DVO.
*% INCLUDE INI_FST_FLD_DVO OF 'COLBMDVO.TXT'
           MOVE -1 TO DVO-SGV-ICTB-IPC-LEN IN DVO-FLD.
*% INCLUDE FIN_FST_FLD_DVO OF 'COLBMDVO.TXT'
       FIN-FST-FLD-DVO.
           EXIT.
       
       PRO-KEY-DVO SECTION.
       INI-PRO-KEY-DVO.
*% INCLUDE INI_PRO_KEY_DVO OF 'COLBMDVO.TXT'
           PERFORM PRO-IKY-DVO.
           PERFORM PRO-AKY-DVO.
           PERFORM PRO-MKY-DVO.
*% INCLUDE FIN_PRO_KEY_DVO OF 'COLBMDVO.TXT'
       FIN-PRO-KEY-DVO.
           EXIT.
       
       PRO-IKY-DVO SECTION.
       INI-PRO-IKY-DVO.
*% INCLUDE INI_PRO_IKY_DVO OF 'COLBMDVO.TXT'
           MOVE FRM-CPIM-AEY TO DVO-CAI-IOPC-ATR IN DVO-FLD.
           MOVE FRM-CPIM-AEY TO DVO-IIC-IOPC-ATR IN DVO-FLD.
           MOVE FRM-CPIM-AEY TO DVO-NUM-IDLC-ATR IN DVO-FLD.
*% INCLUDE FIN_PRO_IKY_DVO OF 'COLBMDVO.TXT'
       FIN-PRO-IKY-DVO.
           EXIT.
       
       PRO-MKY-DVO SECTION.
       INI-PRO-MKY-DVO.
*% INCLUDE INI_PRO_MKY_DVO OF 'COLBMDVO.TXT'
*% INCLUDE FIN_PRO_MKY_DVO OF 'COLBMDVO.TXT'
       FIN-PRO-MKY-DVO.
           EXIT.
       
       PRO-AKY-DVO SECTION.
       INI-PRO-AKY-DVO.
*% INCLUDE INI_PRO_AKY_DVO OF 'COLBMDVO.TXT'
*% INCLUDE FIN_PRO_AKY_DVO OF 'COLBMDVO.TXT'
       FIN-PRO-AKY-DVO.
           EXIT.
       
       UNP-KEY-DVO SECTION.
       INI-UNP-KEY-DVO.
*% INCLUDE INI_UNP_KEY_DVO OF 'COLBMDVO.TXT'
           PERFORM UNP-IKY-DVO.
           PERFORM UNP-AKY-DVO.
           PERFORM UNP-MKY-DVO.
*% INCLUDE FIN_UNP_KEY_DVO OF 'COLBMDVO.TXT'
       FIN-UNP-KEY-DVO.
           EXIT.
       
       UNP-IKY-DVO SECTION.
       INI-UNP-IKY-DVO.
*% INCLUDE INI_UNP_IKY_DVO OF 'COLBMDVO.TXT'
           MOVE FRM-CPIM-UBY TO DVO-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO DVO-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO DVO-NUM-IDLC-ATR.
*% INCLUDE FIN_UNP_IKY_DVO OF 'COLBMDVO.TXT'
       FIN-UNP-IKY-DVO.
           EXIT.
       
       UNP-MKY-DVO SECTION.
       INI-UNP-MKY-DVO.
*% INCLUDE INI_UNP_MKY_DVO OF 'COLBMDVO.TXT'
*% INCLUDE FIN_UNP_MKY_DVO OF 'COLBMDVO.TXT'
       FIN-UNP-MKY-DVO.
           EXIT.
       
       UNP-AKY-DVO SECTION.
       INI-UNP-AKY-DVO.
*% INCLUDE INI_UNP_AKY_DVO OF 'COLBMDVO.TXT'
*% INCLUDE FIN_UNP_AKY_DVO OF 'COLBMDVO.TXT'
       FIN-UNP-AKY-DVO.
           EXIT.
       
       UNP-FLD-DVO SECTION.
       INI-UNP-FLD-DVO.
*% INCLUDE INI_UNP_FLD_DVO OF 'COLBMDVO.TXT'
           PERFORM UNP-DVO-DVO.
*% INCLUDE FIN_UNP_FLD_DVO OF 'COLBMDVO.TXT'
       FIN-UNP-FLD-DVO.
           EXIT.
       
       UNP-DVO-DVO SECTION.
       INI-UNP-DVO-DVO.
*% INCLUDE INI_UNP_DVO_DVO OF 'COLBMDVO.TXT'
           MOVE FRM-CPIM-UBY TO DVO-SGV-ICTB-IPC-ATR IN DVO-FLD.
           MOVE FRM-CPIM-UBY TO DVO-SGV-ICTB-ISD-ATR IN DVO-FLD.
           MOVE FRM-CPIM-UBY TO DVO-SGV-ICTB-IMD-ATR IN DVO-FLD.
           MOVE FRM-CPIM-UBY TO DVO-SGV-ICTB-RPC-ATR IN DVO-FLD.
           MOVE FRM-CPIM-UBY TO DVO-SGV-ICTB-RSD-ATR IN DVO-FLD.
           MOVE FRM-CPIM-UBY TO DVO-SGV-ICTB-RMD-ATR IN DVO-FLD.
*% INCLUDE FIN_UNP_DVO_DVO OF 'COLBMDVO.TXT'
       FIN-UNP-DVO-DVO.
           EXIT.
       
       VAL-KEY-DVO SECTION.
       INI-VAL-KEY-DVO.
           PERFORM VAL-IKY-DVO.
           PERFORM VAL-MKY-DVO.
           PERFORM VAL-AKY-DVO.
       FIN-VAL-KEY-DVO.
           EXIT.
       
       VAL-IKY-DVO SECTION.
       INI-VAL-IKY-DVO.
*% INCLUDE INI_VAL_IKY_DVO OF 'COLBMDVO.TXT'
           PERFORM VAL-DVO-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DVO-CAI-IOPC-LEN IN DVO-FLD
                GO TO FIN-VAL-IKY-DVO.
           PERFORM VAL-DVO-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DVO-IIC-IOPC-LEN IN DVO-FLD
                GO TO FIN-VAL-IKY-DVO.
           PERFORM VAL-DVO-NUM-IDLC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DVO-NUM-IDLC-LEN IN DVO-FLD
                GO TO FIN-VAL-IKY-DVO.
*% INCLUDE FIN_VAL_IKY_DVO OF 'COLBMDVO.TXT'
       FIN-VAL-IKY-DVO.
           EXIT.
       
       VAL-MKY-DVO SECTION.
       INI-VAL-MKY-DVO.
*% INCLUDE INI_VAL_MKY_DVO OF 'COLBMDVO.TXT'
*% INCLUDE FIN_VAL_MKY_DVO OF 'COLBMDVO.TXT'
       FIN-VAL-MKY-DVO.
           EXIT.
       
       VAL-AKY-DVO SECTION.
       INI-VAL-AKY-DVO.
*% INCLUDE INI_VAL_AKY_DVO OF 'COLBMDVO.TXT'
*% INCLUDE FIN_VAL_AKY_DVO OF 'COLBMDVO.TXT'
       FIN-VAL-AKY-DVO.
           EXIT.
       
       VAL-DVO-CAI-IOPC SECTION.
       INI-VAL-DVO-CAI-IOPC.
*% LBL = "DVO_CAI_IOPC OF 'COLBMDVO.TXT'"
*% INCLUDE INI_DVO_CAI_IOPC OF 'COLBMDVO.TXT'
           IF DVO-CAI-IOPC IN DVO-FLD NOT > SPACES
              MOVE SPACES TO DVO-CAI-IOPC IN DVO-FLD.
*% INCLUDE DVO_CAI_IOPC OF 'COLBMDVO.TXT'
*% INCLUDE FIN_DVO_CAI_IOPC OF 'COLBMDVO.TXT'
       FIN-VAL-DVO-CAI-IOPC.
           EXIT.
       
       VAL-DVO-IIC-IOPC SECTION.
       INI-VAL-DVO-IIC-IOPC.
*% LBL = "DVO_IIC_IOPC OF 'COLBMDVO.TXT'"
*% INCLUDE INI_DVO_IIC_IOPC OF 'COLBMDVO.TXT'
*% INCLUDE DVO_IIC_IOPC OF 'COLBMDVO.TXT'
*% INCLUDE FIN_DVO_IIC_IOPC OF 'COLBMDVO.TXT'
       FIN-VAL-DVO-IIC-IOPC.
           EXIT.
       
       VAL-DVO-NUM-IDLC SECTION.
       INI-VAL-DVO-NUM-IDLC.
*% LBL = "DVO_NUM_IDLC OF 'COLBMDVO.TXT'"
*% INCLUDE INI_DVO_NUM_IDLC OF 'COLBMDVO.TXT'
*% INCLUDE DVO_NUM_IDLC OF 'COLBMDVO.TXT'
*% INCLUDE FIN_DVO_NUM_IDLC OF 'COLBMDVO.TXT'
       FIN-VAL-DVO-NUM-IDLC.
           EXIT.
       
       VAL-CON-KEY-DVO SECTION.
       INI-VAL-CON-KEY-DVO.
*% INCLUDE INI_VAL_CON_KEY_DVO OF 'COLBMDVO.TXT'
           IF     DVO-CAI-IOPC IN DVO-FLD NOT > SPACES 
              AND DVO-IIC-IOPC IN DVO-FLD NOT > ZEROES 
              AND DVO-NUM-IDLC IN DVO-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-DVO.
*% INCLUDE FIN_VAL_CON_KEY_DVO OF 'COLBMDVO.TXT'
       FIN-VAL-CON-KEY-DVO.
           EXIT.
       
       VAL-CON-IKY-DVO SECTION.
       INI-VAL-CON-IKY-DVO.
*% INCLUDE INI_VAL_CON_IKY_DVO OF 'COLBMDVO.TXT'
           IF     DVO-CAI-IOPC IN DVO-FLD NOT > SPACES 
              AND DVO-IIC-IOPC IN DVO-FLD NOT > ZEROES 
              AND DVO-NUM-IDLC IN DVO-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-DVO.
*% INCLUDE FIN_VAL_CON_IKY_DVO OF 'COLBMDVO.TXT'
       FIN-VAL-CON-IKY-DVO.
           EXIT.
       
       VAL-NUM-KEY-DVO SECTION.
       INI-VAL-NUM-KEY-DVO.
           PERFORM VAL-NUM-IKY-DVO.
           PERFORM VAL-NUM-MKY-DVO.
           PERFORM VAL-NUM-AKY-DVO.
       FIN-VAL-NUM-KEY-DVO.
           EXIT.
       
       VAL-NUM-IKY-DVO SECTION.
       INI-VAL-NUM-IKY-DVO.
*% INCLUDE INI_VAL_NUM_IKY_DVO OF 'COLBMDVO.TXT'
       VAL-NUM-DVO-IIC-IOPC.
*% BUF = "DVO-FLD"
*% FLD = "DVO-IIC-IOPC"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-DVO.
       VAL-NUM-DVO-NUM-IDLC.
*% BUF = "DVO-FLD"
*% FLD = "DVO-NUM-IDLC"
*% ENT = "3"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-DVO.
*% INCLUDE FIN_VAL_NUM_IKY_DVO OF 'COLBMDVO.TXT'
       FIN-VAL-NUM-IKY-DVO.
           EXIT.
       
       VAL-NUM-MKY-DVO SECTION.
       INI-VAL-NUM-MKY-DVO.
*% INCLUDE INI_VAL_NUM_MKY_DVO OF 'COLBMDVO.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_DVO OF 'COLBMDVO.TXT'
       FIN-VAL-NUM-MKY-DVO.
           EXIT.
       
       VAL-NUM-AKY-DVO SECTION.
       INI-VAL-NUM-AKY-DVO.
*% INCLUDE INI_VAL_NUM_AKY_DVO OF 'COLBMDVO.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_DVO OF 'COLBMDVO.TXT'
       FIN-VAL-NUM-AKY-DVO.
           EXIT.
       
       VAL-NUM-FLD-DVO SECTION.
       INI-VAL-NUM-FLD-DVO.
*% INCLUDE INI_VAL_NUM_FLD_DVO OF 'COLBMDVO.TXT'
       VAL-NUM-DVO-SGV-ICTB-IPC.
*% BUF = "DVO-FLD"
*% FLD = "DVO-SGV-ICTB-IPC"
*% ENT = "12"
*% DCM = "2"
*% NMR = "NS9V"
*% ISG = "S"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-DVO.
       VAL-NUM-DVO-SGV-ICTB-ISD.
*% BUF = "DVO-FLD"
*% FLD = "DVO-SGV-ICTB-ISD"
*% ENT = "12"
*% DCM = "2"
*% NMR = "NS9V"
*% ISG = "S"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-DVO.
       VAL-NUM-DVO-SGV-ICTB-IMD.
*% BUF = "DVO-FLD"
*% FLD = "DVO-SGV-ICTB-IMD"
*% ENT = "12"
*% DCM = "2"
*% NMR = "NS9V"
*% ISG = "S"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-DVO.
       VAL-NUM-DVO-SGV-ICTB-RPC.
*% BUF = "DVO-FLD"
*% FLD = "DVO-SGV-ICTB-RPC"
*% ENT = "12"
*% DCM = "2"
*% NMR = "NS9V"
*% ISG = "S"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-DVO.
       VAL-NUM-DVO-SGV-ICTB-RSD.
*% BUF = "DVO-FLD"
*% FLD = "DVO-SGV-ICTB-RSD"
*% ENT = "12"
*% DCM = "2"
*% NMR = "NS9V"
*% ISG = "S"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-DVO.
       VAL-NUM-DVO-SGV-ICTB-RMD.
*% BUF = "DVO-FLD"
*% FLD = "DVO-SGV-ICTB-RMD"
*% ENT = "12"
*% DCM = "2"
*% NMR = "NS9V"
*% ISG = "S"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-DVO.
*% INCLUDE FIN_VAL_NUM_FLD_DVO OF 'COLBMDVO.TXT'
       FIN-VAL-NUM-FLD-DVO.
           EXIT.
       
       EDT-KEY-DVO SECTION.
       INI-EDT-KEY-DVO.
           PERFORM EDT-IKY-DVO.
           PERFORM EDT-MKY-DVO.
           PERFORM EDT-AKY-DVO.
*% INCLUDE EDT_KEY_DVO OF 'COLBMDVO.TXT'
       FIN-EDT-KEY-DVO.
           EXIT.
       
       EDT-IKY-DVO SECTION.
       INI-EDT-IKY-DVO.
*% INCLUDE EDT_IKY_DVO OF 'COLBMDVO.TXT'
           IF DVO-IIC-IOPC-LEN IN DVO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DVO-IIC-IOPC-LEN IN DVO-FLD
                GO TO FIN-EDT-IKY-DVO.
           MOVE DVO-IIC-IOPC IN DVO-FLD TO DVO-IIC-IOPC-EDT IN DVO-EDT.
           MOVE DVO-IIC-IOPC-EDT IN DVO-EDT TO 
                DVO-IIC-IOPC-ALF IN DVO-FLD.
           IF DVO-NUM-IDLC-LEN IN DVO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DVO-NUM-IDLC-LEN IN DVO-FLD
                GO TO FIN-EDT-IKY-DVO.
           MOVE DVO-NUM-IDLC IN DVO-FLD TO DVO-NUM-IDLC-EDT IN DVO-EDT.
           MOVE DVO-NUM-IDLC-EDT IN DVO-EDT TO 
                DVO-NUM-IDLC-ALF IN DVO-FLD.
*% INCLUDE FIN_EDT_IKY_DVO OF 'COLBMDVO.TXT'
       FIN-EDT-IKY-DVO.
           EXIT.
       
       EDT-MKY-DVO SECTION.
       INI-EDT-MKY-DVO.
*% INCLUDE EDT_MKY_DVO OF 'COLBMDVO.TXT'
*% INCLUDE FIN_EDT_MKY_DVO OF 'COLBMDVO.TXT'
       FIN-EDT-MKY-DVO.
           EXIT.
       
       EDT-AKY-DVO SECTION.
       INI-EDT-AKY-DVO.
*% INCLUDE EDT_AKY_DVO OF 'COLBMDVO.TXT'
*% INCLUDE FIN_EDT_AKY_DVO OF 'COLBMDVO.TXT'
       FIN-EDT-AKY-DVO.
           EXIT.
       
       EDT-FLD-DVO SECTION.
       INI-EDT-FLD-DVO.
*% INCLUDE EDT_FLD_DVO OF 'COLBMDVO.TXT'
           IF DVO-SGV-ICTB-IPC-LEN IN DVO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DVO-SGV-ICTB-IPC-LEN IN DVO-FLD
                GO TO FIN-EDT-FLD-DVO.
           MOVE DVO-SGV-ICTB-IPC IN DVO-FLD TO 
                DVO-SGV-ICTB-IPC-EDT IN DVO-EDT.
           MOVE DVO-SGV-ICTB-IPC-EDT IN DVO-EDT TO 
                DVO-SGV-ICTB-IPC-ALF IN DVO-FLD.
           IF DVO-SGV-ICTB-ISD-LEN IN DVO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DVO-SGV-ICTB-ISD-LEN IN DVO-FLD
                GO TO FIN-EDT-FLD-DVO.
           MOVE DVO-SGV-ICTB-ISD IN DVO-FLD TO 
                DVO-SGV-ICTB-ISD-EDT IN DVO-EDT.
           MOVE DVO-SGV-ICTB-ISD-EDT IN DVO-EDT TO 
                DVO-SGV-ICTB-ISD-ALF IN DVO-FLD.
           IF DVO-SGV-ICTB-IMD-LEN IN DVO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DVO-SGV-ICTB-IMD-LEN IN DVO-FLD
                GO TO FIN-EDT-FLD-DVO.
           MOVE DVO-SGV-ICTB-IMD IN DVO-FLD TO 
                DVO-SGV-ICTB-IMD-EDT IN DVO-EDT.
           MOVE DVO-SGV-ICTB-IMD-EDT IN DVO-EDT TO 
                DVO-SGV-ICTB-IMD-ALF IN DVO-FLD.
           IF DVO-SGV-ICTB-RPC-LEN IN DVO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DVO-SGV-ICTB-RPC-LEN IN DVO-FLD
                GO TO FIN-EDT-FLD-DVO.
           MOVE DVO-SGV-ICTB-RPC IN DVO-FLD TO 
                DVO-SGV-ICTB-RPC-EDT IN DVO-EDT.
           MOVE DVO-SGV-ICTB-RPC-EDT IN DVO-EDT TO 
                DVO-SGV-ICTB-RPC-ALF IN DVO-FLD.
           IF DVO-SGV-ICTB-RSD-LEN IN DVO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DVO-SGV-ICTB-RSD-LEN IN DVO-FLD
                GO TO FIN-EDT-FLD-DVO.
           MOVE DVO-SGV-ICTB-RSD IN DVO-FLD TO 
                DVO-SGV-ICTB-RSD-EDT IN DVO-EDT.
           MOVE DVO-SGV-ICTB-RSD-EDT IN DVO-EDT TO 
                DVO-SGV-ICTB-RSD-ALF IN DVO-FLD.
           IF DVO-SGV-ICTB-RMD-LEN IN DVO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DVO-SGV-ICTB-RMD-LEN IN DVO-FLD
                GO TO FIN-EDT-FLD-DVO.
           MOVE DVO-SGV-ICTB-RMD IN DVO-FLD TO 
                DVO-SGV-ICTB-RMD-EDT IN DVO-EDT.
           MOVE DVO-SGV-ICTB-RMD-EDT IN DVO-EDT TO 
                DVO-SGV-ICTB-RMD-ALF IN DVO-FLD.
*% INCLUDE FIN_EDT_FLD_DVO OF 'COLBMDVO.TXT'
       FIN-EDT-FLD-DVO.
           EXIT.
*% INCLUDE EOF_BQ OF 'COLBMDVO.TXT'
*% INCLUDE EOF OF 'COLBMDVO.TXT'
*% INCLUDE EOF_IBM OF 'COLBMDVO.TXT'
