      *
      * COLBQCPT
      * Version 3.0.1 Nov-2005
      * 04-Jul-17 05:55 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CPT"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMCPT.TXT'
       SET-TAG-COLBQCPT SECTION.
       INI-SET-TAG-COLBQCPT.
           MOVE
           '<IDNMOD NOM=$COLBQCPT$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$04-Jul-17 05:55 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCPT.
           EXIT.
           
       PUT-ALL-CPT SECTION.
       INI-PUT-ALL-CPT.
*% INCLUDE INI_PUT_ALL_CPT OF 'COLBMCPT.TXT'
           PERFORM PUT-OPC-CPT.
           PERFORM PUT-DBC-CPT.
           PERFORM PUT-DLC-CPT.
*% INCLUDE FIN_PUT_ALL_CPT OF 'COLBMCPT.TXT'
       FIN-PUT-ALL-CPT.
           EXIT.
       
       PUT-OPC-CPT SECTION.
       INI-PUT-OPC-CPT.
*% INCLUDE INI_PUT_OPC_CPT OF 'COLBMCPT.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-CPT.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-CPT
               GO TO FIN-PUT-OPC-CPT.
           MOVE OPC-CAI-IOPC IN OPC TO OPC-CAI-IOPC IN CPT-FLD.
           MOVE OPC-IIC-IOPC IN OPC TO OPC-IIC-IOPC IN CPT-FLD.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE OPC-NUM-DCOL IN OPC TO OPC-NUM-DCOL IN CPT-FLD.
           MOVE OPC-NUM-MCOL IN OPC TO OPC-NUM-MCOL IN CPT-FLD.
           MOVE OPC-NUM-SCOL IN OPC TO OPC-NUM-SCOL IN CPT-FLD.
           MOVE OPC-NUM-ACOL IN OPC TO OPC-NUM-ACOL IN CPT-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE OPC-COD-VCOC IN OPC TO OPC-COD-VCOC IN CPT-FLD.
           MOVE OPC-NUM-VCOR IN OPC TO OPC-NUM-VCOR IN CPT-FLD.
*% INCLUDE FIN_PUT_OPC_CPT OF 'COLBMCPT.TXT'
       FIN-PUT-OPC-CPT.
           EXIT.
       
       PUT-DBC-CPT SECTION.
       INI-PUT-DBC-CPT.
*% INCLUDE INI_PUT_DBC_CPT OF 'COLBMCPT.TXT'
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-CPT.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-CPT
               GO TO FIN-PUT-DBC-CPT.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN CPT-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN CPT-FLD.
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN CPT-FLD.
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN CPT-FLD.
      * IDC-FIN --> BMCHIBCI001210.SRCAIU 
*% INCLUDE FIN_PUT_DBC_CPT OF 'COLBMCPT.TXT'
       FIN-PUT-DBC-CPT.
           EXIT.
       
       PUT-DLC-CPT SECTION.
       INI-PUT-DLC-CPT.
*% INCLUDE INI_PUT_DLC_CPT OF 'COLBMCPT.TXT'
           IF PGM-STAT-DLC-NUL
               GO TO FIN-PUT-DLC-CPT.
           IF NOT PGM-STAT-DLC-OKS
               PERFORM CLR-DLC-CPT
               GO TO FIN-PUT-DLC-CPT.
*% RREG = "DLC"
*% RREG_FRM = "DLC"
*% NOMB = "FMAV"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DLC-NUM-DMAV IN DLC TO DLC-NUM-DMAV IN CPT-FLD.
           MOVE DLC-NUM-MMAV IN DLC TO DLC-NUM-MMAV IN CPT-FLD.
           MOVE DLC-NUM-SMAV IN DLC TO DLC-NUM-SMAV IN CPT-FLD.
           MOVE DLC-NUM-AMAV IN DLC TO DLC-NUM-AMAV IN CPT-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% INCLUDE FIN_PUT_DLC_CPT OF 'COLBMCPT.TXT'
       FIN-PUT-DLC-CPT.
           EXIT.
       
       KEY-ALL-CPT SECTION.
       INI-KEY-ALL-CPT.
*% INCLUDE INI_KEY_ALL_CPT OF 'COLBMCPT.TXT'
           PERFORM KEY-OPC-CPT.
           PERFORM KEY-DBC-CPT.
           PERFORM KEY-DLC-CPT.
*% INCLUDE FIN_KEY_ALL_CPT OF 'COLBMCPT.TXT'
       FIN-KEY-ALL-CPT.
           EXIT.
       
       KEY-OPC-CPT SECTION.
       INI-KEY-OPC-CPT.
*% INCLUDE INI_KEY_OPC_CPT OF 'COLBMCPT.TXT'
           MOVE OPC-CAI-IOPC IN CPT-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE OPC-IIC-IOPC IN CPT-FLD TO OPC-IIC-IOPC IN OPC.
*% INCLUDE FIN_KEY_OPC_CPT OF 'COLBMCPT.TXT'
       FIN-KEY-OPC-CPT.
           EXIT.
       
       KEY-DBC-CPT SECTION.
       INI-KEY-DBC-CPT.
*% INCLUDE INI_KEY_DBC_CPT OF 'COLBMCPT.TXT'
*% INCLUDE FIN_KEY_DBC_CPT OF 'COLBMCPT.TXT'
       FIN-KEY-DBC-CPT.
           EXIT.
       
       KEY-DLC-CPT SECTION.
       INI-KEY-DLC-CPT.
*% INCLUDE INI_KEY_DLC_CPT OF 'COLBMCPT.TXT'
*% INCLUDE FIN_KEY_DLC_CPT OF 'COLBMCPT.TXT'
       FIN-KEY-DLC-CPT.
           EXIT.
       
       SCR-CLRA-CPT SECTION.
       INI-SCR-CLRA-CPT.
*% INCLUDE INI_SCR_CLRA_CPT OF 'COLBMCPT.TXT'
           PERFORM CLR-KEY-CPT.
           PERFORM CLR-FLD-CPT.
*% INCLUDE FIN_SCR_CLRA_CPT OF 'COLBMCPT.TXT'
       FIN-SCR-CLRA-CPT.
           EXIT.
       
       CLR-KEY-CPT SECTION.
       INI-CLR-KEY-CPT.
*% INCLUDE INI_CLR_KEY_CPT OF 'COLBMCPT.TXT'
           MOVE SPACES TO OPC-CAI-IOPC IN CPT-FLD.
           MOVE ZEROES TO OPC-IIC-IOPC IN CPT-FLD.
           MOVE ZEROES TO FRM-VAL-PREP IN CPT-FLD.
*% INCLUDE FIN_CLR_KEY_CPT OF 'COLBMCPT.TXT'
       FIN-CLR-KEY-CPT.
           EXIT.
       
       CLR-FLD-CPT SECTION.
       INI-CLR-FLD-CPT.
*% INCLUDE INI_CLR_FLD_CPT OF 'COLBMCPT.TXT'
           MOVE SPACES TO FRM-GLS-TCAN IN CPT-FLD.
           MOVE SPACES TO FRM-GLS-TCRE IN CPT-FLD.
           MOVE SPACES TO FRM-GLS-UTSV IN CPT-FLD.
           MOVE SPACES TO FRM-GLS-NOMC IN CPT-FLD.
           MOVE SPACES TO FRM-GLS-MON IN CPT-FLD.
           MOVE ZEROES TO FRM-VAL-SCRE IN CPT-FLD.
           MOVE ZEROES TO FRM-VAL-TASA IN CPT-FLD.
           MOVE SPACES TO FRM-GLS-TASA IN CPT-FLD.
           MOVE SPACES TO FRM-GLS-GLO1 IN CPT-FLD.
           MOVE SPACES TO FRM-GLS-GLO2 IN CPT-FLD.
           MOVE SPACES TO FRM-GLS-GLO3 IN CPT-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPAG"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FPAG IN CPT-FLD(1).
           MOVE ZEROES TO FRM-FEC-FPAG IN CPT-FLD(2).
           MOVE ZEROES TO FRM-FEC-FPAG IN CPT-FLD(3).
           MOVE ZEROES TO FRM-FEC-FPAG IN CPT-FLD(4).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-VAL-CAPR IN CPT-FLD(1).
           MOVE ZEROES TO FRM-VAL-CAPR IN CPT-FLD(2).
           MOVE ZEROES TO FRM-VAL-CAPR IN CPT-FLD(3).
           MOVE ZEROES TO FRM-VAL-CAPR IN CPT-FLD(4).
           MOVE ZEROES TO FRM-VAL-INTE IN CPT-FLD(1).
           MOVE ZEROES TO FRM-VAL-INTE IN CPT-FLD(2).
           MOVE ZEROES TO FRM-VAL-INTE IN CPT-FLD(3).
           MOVE ZEROES TO FRM-VAL-INTE IN CPT-FLD(4).
           MOVE ZEROES TO FRM-VAL-COPR IN CPT-FLD(1).
           MOVE ZEROES TO FRM-VAL-COPR IN CPT-FLD(2).
           MOVE ZEROES TO FRM-VAL-COPR IN CPT-FLD(3).
           MOVE ZEROES TO FRM-VAL-COPR IN CPT-FLD(4).
           MOVE ZEROES TO FRM-VAL-CAAD IN CPT-FLD(1).
           MOVE ZEROES TO FRM-VAL-CAAD IN CPT-FLD(2).
           MOVE ZEROES TO FRM-VAL-CAAD IN CPT-FLD(3).
           MOVE ZEROES TO FRM-VAL-CAAD IN CPT-FLD(4).
           MOVE ZEROES TO FRM-VAL-MDPS IN CPT-FLD(1).
           MOVE ZEROES TO FRM-VAL-MDPS IN CPT-FLD(2).
           MOVE ZEROES TO FRM-VAL-MDPS IN CPT-FLD(3).
           MOVE ZEROES TO FRM-VAL-MDPS IN CPT-FLD(4).
           MOVE ZEROES TO FRM-VAL-REAJ IN CPT-FLD(1).
           MOVE ZEROES TO FRM-VAL-REAJ IN CPT-FLD(2).
           MOVE ZEROES TO FRM-VAL-REAJ IN CPT-FLD(3).
           MOVE ZEROES TO FRM-VAL-REAJ IN CPT-FLD(4).
           MOVE ZEROES TO FRM-VAL-CAMO IN CPT-FLD(1).
           MOVE ZEROES TO FRM-VAL-CAMO IN CPT-FLD(2).
           MOVE ZEROES TO FRM-VAL-CAMO IN CPT-FLD(3).
           MOVE ZEROES TO FRM-VAL-CAMO IN CPT-FLD(4).
           MOVE ZEROES TO FRM-VAL-COFG IN CPT-FLD(1).
           MOVE ZEROES TO FRM-VAL-COFG IN CPT-FLD(2).
           MOVE ZEROES TO FRM-VAL-COFG IN CPT-FLD(3).
           MOVE ZEROES TO FRM-VAL-COFG IN CPT-FLD(4).
           MOVE ZEROES TO FRM-VAL-TPAG IN CPT-FLD(1).
           MOVE ZEROES TO FRM-VAL-TPAG IN CPT-FLD(2).
           MOVE ZEROES TO FRM-VAL-TPAG IN CPT-FLD(3).
           MOVE ZEROES TO FRM-VAL-TPAG IN CPT-FLD(4).
           PERFORM CLR-OPC-CPT.
           PERFORM CLR-DBC-CPT.
           PERFORM CLR-DLC-CPT.
*% INCLUDE FIN_CLR_FLD_CPT OF 'COLBMCPT.TXT'
       FIN-CLR-FLD-CPT.
           EXIT.
       
       CLR-OPC-CPT SECTION.
       INI-CLR-OPC-CPT.
*% INCLUDE INI_CLR_OPC_CPT OF 'COLBMCPT.TXT'
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO OPC-FEC-FCOL IN CPT-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO OPC-COD-VCOC IN CPT-FLD.
           MOVE ZEROES TO OPC-NUM-VCOR IN CPT-FLD.
*% INCLUDE FIN_CLR_OPC_CPT OF 'COLBMCPT.TXT'
       FIN-CLR-OPC-CPT.
           EXIT.
       
       CLR-DBC-CPT SECTION.
       INI-CLR-DBC-CPT.
*% INCLUDE INI_CLR_DBC_CPT OF 'COLBMCPT.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN CPT-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN CPT-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN CPT-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN CPT-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_DBC_CPT OF 'COLBMCPT.TXT'
       FIN-CLR-DBC-CPT.
           EXIT.
       
       CLR-DLC-CPT SECTION.
       INI-CLR-DLC-CPT.
*% INCLUDE INI_CLR_DLC_CPT OF 'COLBMCPT.TXT'
*% RREG = "DLC"
*% RREG_FRM = "DLC"
*% NOMB = "FMAV"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DLC-FEC-FMAV IN CPT-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_DLC_CPT OF 'COLBMCPT.TXT'
       FIN-CLR-DLC-CPT.
           EXIT.
       
       FST-KEY-CPT SECTION.
       INI-FST-KEY-CPT.
*% INCLUDE INI_FST_KEY_CPT OF 'COLBMCPT.TXT'
           MOVE -1 TO OPC-CAI-IOPC-LEN IN CPT-FLD.
*% INCLUDE FIN_FST_KEY_CPT OF 'COLBMCPT.TXT'
       FIN-FST-KEY-CPT.
           EXIT.
       
       FST-FLD-CPT SECTION.
       INI-FST-FLD-CPT.
*% INCLUDE INI_FST_FLD_CPT OF 'COLBMCPT.TXT'
*% INCLUDE FIN_FST_FLD_CPT OF 'COLBMCPT.TXT'
       FIN-FST-FLD-CPT.
           EXIT.
       
       PRO-KEY-CPT SECTION.
       INI-PRO-KEY-CPT.
*% INCLUDE INI_PRO_KEY_CPT OF 'COLBMCPT.TXT'
           MOVE FRM-CPIM-AEY TO FRM-VAL-PREP-ATR IN CPT-FLD.
           PERFORM PRO-IKY-CPT.
           PERFORM PRO-AKY-CPT.
           PERFORM PRO-MKY-CPT.
*% INCLUDE FIN_PRO_KEY_CPT OF 'COLBMCPT.TXT'
       FIN-PRO-KEY-CPT.
           EXIT.
       
       PRO-IKY-CPT SECTION.
       INI-PRO-IKY-CPT.
*% INCLUDE INI_PRO_IKY_CPT OF 'COLBMCPT.TXT'
           MOVE FRM-CPIM-AEY TO OPC-CAI-IOPC-ATR IN CPT-FLD.
           MOVE FRM-CPIM-AEY TO OPC-IIC-IOPC-ATR IN CPT-FLD.
*% INCLUDE FIN_PRO_IKY_CPT OF 'COLBMCPT.TXT'
       FIN-PRO-IKY-CPT.
           EXIT.
       
       PRO-MKY-CPT SECTION.
       INI-PRO-MKY-CPT.
*% INCLUDE INI_PRO_MKY_CPT OF 'COLBMCPT.TXT'
*% INCLUDE FIN_PRO_MKY_CPT OF 'COLBMCPT.TXT'
       FIN-PRO-MKY-CPT.
           EXIT.
       
       PRO-AKY-CPT SECTION.
       INI-PRO-AKY-CPT.
*% INCLUDE INI_PRO_AKY_CPT OF 'COLBMCPT.TXT'
*% INCLUDE FIN_PRO_AKY_CPT OF 'COLBMCPT.TXT'
       FIN-PRO-AKY-CPT.
           EXIT.
       
       UNP-KEY-CPT SECTION.
       INI-UNP-KEY-CPT.
*% INCLUDE INI_UNP_KEY_CPT OF 'COLBMCPT.TXT'
           MOVE FRM-CPIM-UBY TO FRM-VAL-PREP-ATR IN CPT-FLD.
           PERFORM UNP-IKY-CPT.
           PERFORM UNP-AKY-CPT.
           PERFORM UNP-MKY-CPT.
*% INCLUDE FIN_UNP_KEY_CPT OF 'COLBMCPT.TXT'
       FIN-UNP-KEY-CPT.
           EXIT.
       
       UNP-IKY-CPT SECTION.
       INI-UNP-IKY-CPT.
*% INCLUDE INI_UNP_IKY_CPT OF 'COLBMCPT.TXT'
           MOVE FRM-CPIM-UBY TO OPC-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO OPC-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-VAL-PREP-ATR.
*% INCLUDE FIN_UNP_IKY_CPT OF 'COLBMCPT.TXT'
       FIN-UNP-IKY-CPT.
           EXIT.
       
       UNP-MKY-CPT SECTION.
       INI-UNP-MKY-CPT.
*% INCLUDE INI_UNP_MKY_CPT OF 'COLBMCPT.TXT'
*% INCLUDE FIN_UNP_MKY_CPT OF 'COLBMCPT.TXT'
       FIN-UNP-MKY-CPT.
           EXIT.
       
       UNP-AKY-CPT SECTION.
       INI-UNP-AKY-CPT.
*% INCLUDE INI_UNP_AKY_CPT OF 'COLBMCPT.TXT'
*% INCLUDE FIN_UNP_AKY_CPT OF 'COLBMCPT.TXT'
       FIN-UNP-AKY-CPT.
           EXIT.
       
       UNP-FLD-CPT SECTION.
       INI-UNP-FLD-CPT.
*% INCLUDE INI_UNP_FLD_CPT OF 'COLBMCPT.TXT'
           PERFORM UNP-OPC-CPT.
           PERFORM UNP-DBC-CPT.
           PERFORM UNP-DLC-CPT.
*% INCLUDE FIN_UNP_FLD_CPT OF 'COLBMCPT.TXT'
       FIN-UNP-FLD-CPT.
           EXIT.
       
       UNP-OPC-CPT SECTION.
       INI-UNP-OPC-CPT.
*% INCLUDE INI_UNP_OPC_CPT OF 'COLBMCPT.TXT'
*% INCLUDE FIN_UNP_OPC_CPT OF 'COLBMCPT.TXT'
       FIN-UNP-OPC-CPT.
           EXIT.
       
       UNP-DBC-CPT SECTION.
       INI-UNP-DBC-CPT.
*% INCLUDE INI_UNP_DBC_CPT OF 'COLBMCPT.TXT'
*% INCLUDE FIN_UNP_DBC_CPT OF 'COLBMCPT.TXT'
       FIN-UNP-DBC-CPT.
           EXIT.
       
       UNP-DLC-CPT SECTION.
       INI-UNP-DLC-CPT.
*% INCLUDE INI_UNP_DLC_CPT OF 'COLBMCPT.TXT'
*% INCLUDE FIN_UNP_DLC_CPT OF 'COLBMCPT.TXT'
       FIN-UNP-DLC-CPT.
           EXIT.
       
       VAL-KEY-CPT SECTION.
       INI-VAL-KEY-CPT.
           PERFORM VAL-IKY-CPT.
           PERFORM VAL-MKY-CPT.
           PERFORM VAL-AKY-CPT.
       FIN-VAL-KEY-CPT.
           EXIT.
       
       VAL-IKY-CPT SECTION.
       INI-VAL-IKY-CPT.
*% INCLUDE INI_VAL_IKY_CPT OF 'COLBMCPT.TXT'
           PERFORM VAL-OPC-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-CAI-IOPC-LEN IN CPT-FLD
                GO TO FIN-VAL-IKY-CPT.
           PERFORM VAL-OPC-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-IIC-IOPC-LEN IN CPT-FLD
                GO TO FIN-VAL-IKY-CPT.
           PERFORM VAL-FRM-VAL-PREP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-PREP-LEN IN CPT-FLD
                GO TO FIN-VAL-IKY-CPT.
*% INCLUDE FIN_VAL_IKY_CPT OF 'COLBMCPT.TXT'
       FIN-VAL-IKY-CPT.
           EXIT.
       
       VAL-MKY-CPT SECTION.
       INI-VAL-MKY-CPT.
*% INCLUDE INI_VAL_MKY_CPT OF 'COLBMCPT.TXT'
*% INCLUDE FIN_VAL_MKY_CPT OF 'COLBMCPT.TXT'
       FIN-VAL-MKY-CPT.
           EXIT.
       
       VAL-AKY-CPT SECTION.
       INI-VAL-AKY-CPT.
*% INCLUDE INI_VAL_AKY_CPT OF 'COLBMCPT.TXT'
*% INCLUDE FIN_VAL_AKY_CPT OF 'COLBMCPT.TXT'
       FIN-VAL-AKY-CPT.
           EXIT.
       
       VAL-OPC-CAI-IOPC SECTION.
       INI-VAL-OPC-CAI-IOPC.
*% LBL = "OPC_CAI_IOPC OF 'COLBMCPT.TXT'"
*% INCLUDE INI_OPC_CAI_IOPC OF 'COLBMCPT.TXT'
           IF OPC-CAI-IOPC IN CPT-FLD NOT > SPACES
              MOVE SPACES TO OPC-CAI-IOPC IN CPT-FLD.
*% INCLUDE OPC_CAI_IOPC OF 'COLBMCPT.TXT'
*% INCLUDE FIN_OPC_CAI_IOPC OF 'COLBMCPT.TXT'
       FIN-VAL-OPC-CAI-IOPC.
           EXIT.
       
       VAL-OPC-IIC-IOPC SECTION.
       INI-VAL-OPC-IIC-IOPC.
*% LBL = "OPC_IIC_IOPC OF 'COLBMCPT.TXT'"
*% INCLUDE INI_OPC_IIC_IOPC OF 'COLBMCPT.TXT'
*% INCLUDE OPC_IIC_IOPC OF 'COLBMCPT.TXT'
*% INCLUDE FIN_OPC_IIC_IOPC OF 'COLBMCPT.TXT'
       FIN-VAL-OPC-IIC-IOPC.
           EXIT.
       
       VAL-FRM-VAL-PREP SECTION.
       INI-VAL-FRM-VAL-PREP.
*% LBL = "FRM_VAL_PREP OF 'COLBMCPT.TXT'"
*% INCLUDE INI_FRM_VAL_PREP OF 'COLBMCPT.TXT'
*% INCLUDE FRM_VAL_PREP OF 'COLBMCPT.TXT'
*% INCLUDE FIN_FRM_VAL_PREP OF 'COLBMCPT.TXT'
       FIN-VAL-FRM-VAL-PREP.
           EXIT.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% IZR = "S"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% IZR = "S"
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
*% IZR = "S"
*% RREG = "DLC"
*% RREG_FRM = "DLC"
*% NOMB = "FMAV"
*% IZR = "S"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPAG"
*% IZR = "S"
       
       VAL-CON-KEY-CPT SECTION.
       INI-VAL-CON-KEY-CPT.
*% INCLUDE INI_VAL_CON_KEY_CPT OF 'COLBMCPT.TXT'
           IF     OPC-CAI-IOPC IN CPT-FLD NOT > SPACES 
              AND OPC-IIC-IOPC IN CPT-FLD NOT > ZEROES 
              AND FRM-VAL-PREP IN CPT-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CPT.
*% INCLUDE FIN_VAL_CON_KEY_CPT OF 'COLBMCPT.TXT'
       FIN-VAL-CON-KEY-CPT.
           EXIT.
       
       VAL-CON-IKY-CPT SECTION.
       INI-VAL-CON-IKY-CPT.
*% INCLUDE INI_VAL_CON_IKY_CPT OF 'COLBMCPT.TXT'
           IF     OPC-CAI-IOPC IN CPT-FLD NOT > SPACES 
              AND OPC-IIC-IOPC IN CPT-FLD NOT > ZEROES 
              AND FRM-VAL-PREP IN CPT-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CPT.
*% INCLUDE FIN_VAL_CON_IKY_CPT OF 'COLBMCPT.TXT'
       FIN-VAL-CON-IKY-CPT.
           EXIT.
       
       VAL-NUM-KEY-CPT SECTION.
       INI-VAL-NUM-KEY-CPT.
           PERFORM VAL-NUM-IKY-CPT.
           PERFORM VAL-NUM-MKY-CPT.
           PERFORM VAL-NUM-AKY-CPT.
       FIN-VAL-NUM-KEY-CPT.
           EXIT.
       
