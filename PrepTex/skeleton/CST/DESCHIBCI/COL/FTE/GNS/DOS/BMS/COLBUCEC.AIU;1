       EDT-KEY-CEC SECTION.
       INI-EDT-KEY-CEC.
           PERFORM EDT-IKY-CEC.
           PERFORM EDT-MKY-CEC.
           PERFORM EDT-AKY-CEC.
*% INCLUDE EDT_KEY_CEC OF 'COLBMCEC.TXT'
       FIN-EDT-KEY-CEC.
           EXIT.
       
       EDT-IKY-CEC SECTION.
       INI-EDT-IKY-CEC.
*% INCLUDE EDT_IKY_CEC OF 'COLBMCEC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF DBC-NUM-ICLI-LEN IN CEC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DBC-NUM-ICLI-LEN IN CEC-FLD
                GO TO FIN-EDT-IKY-CEC.
           MOVE DBC-NUM-ICLI IN CEC-FLD TO
                DBC-NUM-ICLI-EDT IN CEC-EDT.
           MOVE DBC-NUM-ICLI-EDT IN CEC-EDT TO
                DBC-NUM-ICLI-ALF IN CEC-FLD.
      * IDC-FIN --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF CEC-NUM-IENT-LEN IN CEC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CEC-NUM-IENT-LEN IN CEC-FLD
                GO TO FIN-EDT-IKY-CEC.
           MOVE CEC-NUM-IENT IN CEC-FLD TO CEC-NUM-IENT-EDT IN CEC-EDT.
           MOVE CEC-NUM-IENT-EDT IN CEC-EDT TO 
                CEC-NUM-IENT-ALF IN CEC-FLD.
*% INCLUDE FIN_EDT_IKY_CEC OF 'COLBMCEC.TXT'
       FIN-EDT-IKY-CEC.
           EXIT.
       
