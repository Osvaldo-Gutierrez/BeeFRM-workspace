*% FRM = "CEC"
      *
      * COLBUCEC
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 03:44 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMCEC.TXT'
       SET-TAG-COLBUCEC SECTION.
       INI-SET-TAG-COLBUCEC.
           MOVE
           '<IDNMOD NOM=$COLBUCEC$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 03:44 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCEC.
           EXIT.
           
       RET-ALL-CEC SECTION.
       INI-RET-ALL-CEC.
*% INCLUDE INI_RET_ALL_CEC OF 'COLBMCEC.TXT'
           PERFORM KEY-ALL-CEC.
           PERFORM RET-DBC-CEC.
           PERFORM RET-CEC-CEC.
*% INCLUDE FIN_RET_ALL_CEC OF 'COLBMCEC.TXT'
       FIN-RET-ALL-CEC.
           EXIT.
       
       RET-DBC-CEC SECTION.
       INI-RET-DBC-CEC.
*% INCLUDE INI_RET_DBC_CEC OF 'COLBMCEC.TXT'
*% INCLUDE FIN_RET_DBC_CEC OF 'COLBMCEC.TXT'
       FIN-RET-DBC-CEC.
           EXIT.
       
       RET-CEC-CEC SECTION.
       INI-RET-CEC-CEC.
*% INCLUDE INI_RET_CEC_CEC OF 'COLBMCEC.TXT'
           MOVE CEC-MSC-ESTA IN CEC-FLD TO CEC-MSC-ESTA IN CEC.
           MOVE CEC-CIC-IOPC IN CEC-FLD TO CEC-CIC-IOPC IN CEC.
           MOVE CEC-CIC-IOPV IN CEC-FLD TO CEC-CIC-IOPV IN CEC.
           MOVE CEC-COD-COPR IN CEC-FLD TO CEC-COD-COPR IN CEC.
           MOVE CEC-COD-VCAM IN CEC-FLD TO CEC-COD-VCAM IN CEC.
           MOVE CEC-COD-CCOM IN CEC-FLD TO CEC-COD-CCOM IN CEC.
           MOVE CEC-COD-COD1 IN CEC-FLD TO CEC-COD-COD1 IN CEC.
           MOVE CEC-COD-TTIT IN CEC-FLD TO CEC-COD-TTIT IN CEC.
           MOVE CEC-SGV-ARNS IN CEC-FLD TO CEC-SGV-ARNS IN CEC.
           MOVE CEC-SGV-ARNE IN CEC-FLD TO CEC-SGV-ARNE IN CEC.
           MOVE CEC-COD-TIES IN CEC-FLD TO CEC-COD-TIES IN CEC.
           MOVE CEC-IND-VIGE IN CEC-FLD TO CEC-IND-VIGE IN CEC.
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FCEC"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CEC-NUM-DCEC IN CEC-FLD TO CEC-NUM-DCEC IN CEC.
           MOVE CEC-NUM-MCEC IN CEC-FLD TO CEC-NUM-MCEC IN CEC.
           MOVE CEC-NUM-SCEC IN CEC-FLD TO CEC-NUM-SCEC IN CEC.
           MOVE CEC-NUM-ACEC IN CEC-FLD TO CEC-NUM-ACEC IN CEC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FCUA"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CEC-NUM-DCUA IN CEC-FLD TO CEC-NUM-DCUA IN CEC.
           MOVE CEC-NUM-MCUA IN CEC-FLD TO CEC-NUM-MCUA IN CEC.
           MOVE CEC-NUM-SCUA IN CEC-FLD TO CEC-NUM-SCUA IN CEC.
           MOVE CEC-NUM-ACUA IN CEC-FLD TO CEC-NUM-ACUA IN CEC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FCUC"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CEC-NUM-DCUC IN CEC-FLD TO CEC-NUM-DCUC IN CEC.
           MOVE CEC-NUM-MCUC IN CEC-FLD TO CEC-NUM-MCUC IN CEC.
           MOVE CEC-NUM-SCUC IN CEC-FLD TO CEC-NUM-SCUC IN CEC.
           MOVE CEC-NUM-ACUC IN CEC-FLD TO CEC-NUM-ACUC IN CEC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FFIA"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CEC-NUM-DFIA IN CEC-FLD TO CEC-NUM-DFIA IN CEC.
           MOVE CEC-NUM-MFIA IN CEC-FLD TO CEC-NUM-MFIA IN CEC.
           MOVE CEC-NUM-SFIA IN CEC-FLD TO CEC-NUM-SFIA IN CEC.
           MOVE CEC-NUM-AFIA IN CEC-FLD TO CEC-NUM-AFIA IN CEC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FVES"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CEC-NUM-DVES IN CEC-FLD TO CEC-NUM-DVES IN CEC.
           MOVE CEC-NUM-MVES IN CEC-FLD TO CEC-NUM-MVES IN CEC.
           MOVE CEC-NUM-SVES IN CEC-FLD TO CEC-NUM-SVES IN CEC.
           MOVE CEC-NUM-AVES IN CEC-FLD TO CEC-NUM-AVES IN CEC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FCOI"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CEC-NUM-DCOI IN CEC-FLD TO CEC-NUM-DCOI IN CEC.
           MOVE CEC-NUM-MCOI IN CEC-FLD TO CEC-NUM-MCOI IN CEC.
           MOVE CEC-NUM-SCOI IN CEC-FLD TO CEC-NUM-SCOI IN CEC.
           MOVE CEC-NUM-ACOI IN CEC-FLD TO CEC-NUM-ACOI IN CEC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FTPG"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CEC-NUM-DTPG IN CEC-FLD TO CEC-NUM-DTPG IN CEC.
           MOVE CEC-NUM-MTPG IN CEC-FLD TO CEC-NUM-MTPG IN CEC.
           MOVE CEC-NUM-STPG IN CEC-FLD TO CEC-NUM-STPG IN CEC.
           MOVE CEC-NUM-ATPG IN CEC-FLD TO CEC-NUM-ATPG IN CEC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CEC-NUM-PZOC IN CEC-FLD TO CEC-NUM-PZOC IN CEC.
           MOVE CEC-SGV-TIMP IN CEC-FLD TO CEC-SGV-TIMP IN CEC.
           MOVE CEC-COD-TIOP IN CEC-FLD TO CEC-COD-TIOP IN CEC.
           MOVE CEC-SGV-TINT IN CEC-FLD TO CEC-SGV-TINT IN CEC.
           MOVE CEC-NUM-TADE IN CEC-FLD TO CEC-NUM-TADE IN CEC.
           MOVE CEC-CIC-IBCO IN CEC-FLD TO CEC-CIC-IBCO IN CEC.
           MOVE CEC-SGV-ARNR IN CEC-FLD TO CEC-SGV-ARNR IN CEC.
           MOVE CEC-CIC-IIES IN CEC-FLD TO CEC-CIC-IIES IN CEC.
           MOVE CEC-SGV-MFIA IN CEC-FLD TO CEC-SGV-MFIA IN CEC.
           MOVE CEC-NUM-ANOL IN CEC-FLD TO CEC-NUM-ANOL IN CEC.
           MOVE CEC-NUM-ANOO IN CEC-FLD TO CEC-NUM-ANOO IN CEC.
           MOVE CEC-NUM-ANOI IN CEC-FLD TO CEC-NUM-ANOI IN CEC.
           MOVE CEC-NUM-ANOD IN CEC-FLD TO CEC-NUM-ANOD IN CEC.
           MOVE CEC-NUM-ANOA IN CEC-FLD TO CEC-NUM-ANOA IN CEC.
           MOVE CEC-NUM-CNCA IN CEC-FLD TO CEC-NUM-CNCA IN CEC.
           MOVE CEC-IND-QNTL IN CEC-FLD TO CEC-IND-QNTL IN CEC.
           MOVE CEC-NUM-NIES IN CEC-FLD TO CEC-NUM-NIES IN CEC.
           MOVE CEC-NUM-PGAR IN CEC-FLD TO CEC-NUM-PGAR IN CEC.
*% INCLUDE FIN_RET_CEC_CEC OF 'COLBMCEC.TXT'
       FIN-RET-CEC-CEC.
           EXIT.
       
       KEY-ALL-CEC SECTION.
       INI-KEY-ALL-CEC.
*% INCLUDE INI_KEY_ALL_CEC OF 'COLBMCEC.TXT'
           PERFORM KEY-DBC-CEC.
           PERFORM KEY-CEC-CEC.
*% INCLUDE FIN_KEY_ALL_CEC OF 'COLBMCEC.TXT'
       FIN-KEY-ALL-CEC.
           EXIT.
       
       KEY-DBC-CEC SECTION.
       INI-KEY-DBC-CEC.
*% INCLUDE INI_KEY_DBC_CEC OF 'COLBMCEC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN CEC-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN CEC-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN CEC-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN CEC-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_KEY_DBC_CEC OF 'COLBMCEC.TXT'
       FIN-KEY-DBC-CEC.
           EXIT.
       
       KEY-CEC-CEC SECTION.
       INI-KEY-CEC-CEC.
*% INCLUDE INI_KEY_CEC_CEC OF 'COLBMCEC.TXT'
           MOVE CEC-NUM-IENT IN CEC-FLD TO CEC-NUM-IENT IN CEC.
*% INCLUDE FIN_KEY_CEC_CEC OF 'COLBMCEC.TXT'
       FIN-KEY-CEC-CEC.
           EXIT.
       
       SCR-CLRA-CEC SECTION.
       INI-SCR-CLRA-CEC.
*% INCLUDE INI_SCR_CLRA_CEC OF 'COLBMCEC.TXT'
           PERFORM CLR-KEY-CEC.
           PERFORM CLR-FLD-CEC.
*% INCLUDE FIN_SCR_CLRA_CEC OF 'COLBMCEC.TXT'
       FIN-SCR-CLRA-CEC.
           EXIT.
       
       CLR-KEY-CEC SECTION.
       INI-CLR-KEY-CEC.
*% INCLUDE INI_CLR_KEY_CEC OF 'COLBMCEC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN CEC-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN CEC-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN CEC-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN CEC-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CEC-NUM-IENT IN CEC-FLD.
*% INCLUDE FIN_CLR_KEY_CEC OF 'COLBMCEC.TXT'
       FIN-CLR-KEY-CEC.
           EXIT.
       
       CLR-FLD-CEC SECTION.
       INI-CLR-FLD-CEC.
*% INCLUDE INI_CLR_FLD_CEC OF 'COLBMCEC.TXT'
           MOVE SPACES TO FRM-GLS-ESTA IN CEC-FLD.
           MOVE SPACES TO FRM-GLS-NOMC IN CEC-FLD.
           MOVE SPACES TO FRM-GLS-CON1 IN CEC-FLD.
           PERFORM CLR-DBC-CEC.
           PERFORM CLR-CEC-CEC.
*% INCLUDE FIN_CLR_FLD_CEC OF 'COLBMCEC.TXT'
       FIN-CLR-FLD-CEC.
           EXIT.
       
       CLR-DBC-CEC SECTION.
       INI-CLR-DBC-CEC.
*% INCLUDE INI_CLR_DBC_CEC OF 'COLBMCEC.TXT'
*% INCLUDE FIN_CLR_DBC_CEC OF 'COLBMCEC.TXT'
       FIN-CLR-DBC-CEC.
           EXIT.
       
       CLR-CEC-CEC SECTION.
       INI-CLR-CEC-CEC.
*% INCLUDE INI_CLR_CEC_CEC OF 'COLBMCEC.TXT'
           MOVE SPACES TO CEC-MSC-ESTA IN CEC-FLD.
           MOVE SPACES TO CEC-CIC-IOPC IN CEC-FLD.
           MOVE SPACES TO CEC-CIC-IOPV IN CEC-FLD.
           MOVE SPACES TO CEC-COD-COPR IN CEC-FLD.
           MOVE SPACES TO CEC-COD-VCAM IN CEC-FLD.
           MOVE SPACES TO CEC-COD-CCOM IN CEC-FLD.
           MOVE SPACES TO CEC-COD-COD1 IN CEC-FLD.
           MOVE SPACES TO CEC-COD-TTIT IN CEC-FLD.
           MOVE ZEROES TO CEC-SGV-ARNS IN CEC-FLD.
           MOVE ZEROES TO CEC-SGV-ARNE IN CEC-FLD.
           MOVE SPACES TO CEC-COD-TIES IN CEC-FLD.
           MOVE SPACES TO CEC-IND-VIGE IN CEC-FLD.
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FCEC"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CEC-FEC-FCEC IN CEC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "CEC"
