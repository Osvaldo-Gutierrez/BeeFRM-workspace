      *
      * COLBQCAC
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 03:17 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CAC"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMCAC.TXT'
       SET-TAG-COLBQCAC SECTION.
       INI-SET-TAG-COLBQCAC.
           MOVE
           '<IDNMOD NOM=$COLBQCAC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 03:17 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCAC.
           EXIT.
           
       PUT-ALL-CAC SECTION.
       INI-PUT-ALL-CAC.
*% INCLUDE INI_PUT_ALL_CAC OF 'COLBMCAC.TXT'
           PERFORM PUT-OPC-CAC.
           PERFORM PUT-DBC-CAC.
           PERFORM PUT-ITR-CAC.
           PERFORM PUT-VEN-CAC.
*% INCLUDE FIN_PUT_ALL_CAC OF 'COLBMCAC.TXT'
       FIN-PUT-ALL-CAC.
           EXIT.
       
       PUT-OPC-CAC SECTION.
       INI-PUT-OPC-CAC.
*% INCLUDE INI_PUT_OPC_CAC OF 'COLBMCAC.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-CAC.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-CAC
               GO TO FIN-PUT-OPC-CAC.
           MOVE OPC-CAI-IOPC IN OPC TO OPC-CAI-IOPC IN CAC-FLD.
           MOVE OPC-IIC-IOPC IN OPC TO OPC-IIC-IOPC IN CAC-FLD.
           MOVE OPC-COD-VCOC IN OPC TO OPC-COD-VCOC IN CAC-FLD.
           MOVE OPC-COD-TOOC IN OPC TO OPC-COD-TOOC IN CAC-FLD.
           MOVE OPC-COD-AOOC IN OPC TO OPC-COD-AOOC IN CAC-FLD.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE OPC-NUM-DCOL IN OPC TO OPC-NUM-DCOL IN CAC-FLD.
           MOVE OPC-NUM-MCOL IN OPC TO OPC-NUM-MCOL IN CAC-FLD.
           MOVE OPC-NUM-SCOL IN OPC TO OPC-NUM-SCOL IN CAC-FLD.
           MOVE OPC-NUM-ACOL IN OPC TO OPC-NUM-ACOL IN CAC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE OPC-VAL-CRED IN OPC TO OPC-VAL-CRED IN CAC-FLD.
           MOVE OPC-VAL-SCRE IN OPC TO OPC-VAL-SCRE IN CAC-FLD.
           MOVE OPC-SGV-LIQU IN OPC TO OPC-SGV-LIQU IN CAC-FLD.
*% BUF = "CAC-FLD"
*% SIS = "TAB"
*% TAB = "VLR "
*% FLD = "OPC-COD-VCOC"
*% GLS = "FRM-GLS-VCOC"
*% TIENE_GLOSA = TRUE
*% LGL = 12
*% INCLUDE 'BUSTAB'
*% INCLUDE FIN_PUT_OPC_CAC OF 'COLBMCAC.TXT'
       FIN-PUT-OPC-CAC.
           EXIT.
       
       PUT-DBC-CAC SECTION.
       INI-PUT-DBC-CAC.
*% INCLUDE INI_PUT_DBC_CAC OF 'COLBMCAC.TXT'
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-CAC.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-CAC
               GO TO FIN-PUT-DBC-CAC.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN CAC-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN CAC-FLD.
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN CAC-FLD.
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN CAC-FLD.
      * IDC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN CAC-FLD.
*% INCLUDE FIN_PUT_DBC_CAC OF 'COLBMCAC.TXT'
       FIN-PUT-DBC-CAC.
           EXIT.
       
       PUT-ITR-CAC SECTION.
       INI-PUT-ITR-CAC.
*% INCLUDE INI_PUT_ITR_CAC OF 'COLBMCAC.TXT'
           IF PGM-STAT-ITR-NUL
               GO TO FIN-PUT-ITR-CAC.
           IF NOT PGM-STAT-ITR-OKS
               PERFORM CLR-ITR-CAC
               GO TO FIN-PUT-ITR-CAC.
           MOVE ITR-SGV-TINT IN ITR TO ITR-SGV-TINT IN CAC-FLD.
*% INCLUDE FIN_PUT_ITR_CAC OF 'COLBMCAC.TXT'
       FIN-PUT-ITR-CAC.
           EXIT.
       
       PUT-VEN-CAC SECTION.
       INI-PUT-VEN-CAC.
*% INCLUDE INI_PUT_VEN_CAC OF 'COLBMCAC.TXT'
           IF PGM-STAT-VEN-NUL
               GO TO FIN-PUT-VEN-CAC.
           IF NOT PGM-STAT-VEN-OKS
               PERFORM CLR-VEN-CAC
               GO TO FIN-PUT-VEN-CAC.
           MOVE VEN-NUM-IVCT IN VEN TO VEN-NUM-IVCT IN CAC-FLD.
*% RREG = "VEN"
*% RREG_FRM = "VEN"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE VEN-NUM-DVEN IN VEN TO VEN-NUM-DVEN IN CAC-FLD.
           MOVE VEN-NUM-MVEN IN VEN TO VEN-NUM-MVEN IN CAC-FLD.
           MOVE VEN-NUM-SVEN IN VEN TO VEN-NUM-SVEN IN CAC-FLD.
           MOVE VEN-NUM-AVEN IN VEN TO VEN-NUM-AVEN IN CAC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE VEN-VAL-CAPI IN VEN TO VEN-VAL-CAPI IN CAC-FLD.
           MOVE VEN-VAL-FINA IN VEN TO VEN-VAL-FINA IN CAC-FLD.
*% INCLUDE FIN_PUT_VEN_CAC OF 'COLBMCAC.TXT'
       FIN-PUT-VEN-CAC.
           EXIT.
       
       KEY-ALL-CAC SECTION.
       INI-KEY-ALL-CAC.
*% INCLUDE INI_KEY_ALL_CAC OF 'COLBMCAC.TXT'
           PERFORM KEY-OPC-CAC.
           PERFORM KEY-DBC-CAC.
           PERFORM KEY-ITR-CAC.
           PERFORM KEY-VEN-CAC.
*% INCLUDE FIN_KEY_ALL_CAC OF 'COLBMCAC.TXT'
       FIN-KEY-ALL-CAC.
           EXIT.
       
       KEY-OPC-CAC SECTION.
       INI-KEY-OPC-CAC.
*% INCLUDE INI_KEY_OPC_CAC OF 'COLBMCAC.TXT'
           MOVE OPC-CAI-IOPC IN CAC-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE OPC-IIC-IOPC IN CAC-FLD TO OPC-IIC-IOPC IN OPC.
*% INCLUDE FIN_KEY_OPC_CAC OF 'COLBMCAC.TXT'
       FIN-KEY-OPC-CAC.
           EXIT.
       
       KEY-DBC-CAC SECTION.
       INI-KEY-DBC-CAC.
*% INCLUDE INI_KEY_DBC_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_KEY_DBC_CAC OF 'COLBMCAC.TXT'
       FIN-KEY-DBC-CAC.
           EXIT.
       
       KEY-ITR-CAC SECTION.
       INI-KEY-ITR-CAC.
*% INCLUDE INI_KEY_ITR_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_KEY_ITR_CAC OF 'COLBMCAC.TXT'
       FIN-KEY-ITR-CAC.
           EXIT.
       
       KEY-VEN-CAC SECTION.
       INI-KEY-VEN-CAC.
*% INCLUDE INI_KEY_VEN_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_KEY_VEN_CAC OF 'COLBMCAC.TXT'
       FIN-KEY-VEN-CAC.
           EXIT.
       
       SCR-CLRA-CAC SECTION.
       INI-SCR-CLRA-CAC.
*% INCLUDE INI_SCR_CLRA_CAC OF 'COLBMCAC.TXT'
           PERFORM CLR-KEY-CAC.
           PERFORM CLR-FLD-CAC.
*% INCLUDE FIN_SCR_CLRA_CAC OF 'COLBMCAC.TXT'
       FIN-SCR-CLRA-CAC.
           EXIT.
       
       CLR-KEY-CAC SECTION.
       INI-CLR-KEY-CAC.
*% INCLUDE INI_CLR_KEY_CAC OF 'COLBMCAC.TXT'
           MOVE SPACES TO OPC-CAI-IOPC IN CAC-FLD.
           MOVE ZEROES TO OPC-IIC-IOPC IN CAC-FLD.
           MOVE ZEROES TO FRM-NUM-VDES IN CAC-FLD.
           MOVE ZEROES TO FRM-NUM-VHAS IN CAC-FLD.
*% INCLUDE FIN_CLR_KEY_CAC OF 'COLBMCAC.TXT'
       FIN-CLR-KEY-CAC.
           EXIT.
       
       CLR-FLD-CAC SECTION.
       INI-CLR-FLD-CAC.
*% INCLUDE INI_CLR_FLD_CAC OF 'COLBMCAC.TXT'
           MOVE SPACES TO FRM-GLS-VCOC IN CAC-FLD.
           MOVE SPACES TO FRM-GLS-AOOC IN CAC-FLD.
           MOVE SPACES TO FRM-GLS-TINT IN CAC-FLD.
           MOVE SPACES TO FRM-IND-CANC IN CAC-FLD.
           MOVE ZEROES TO FRM-VAL-INTC IN CAC-FLD.
           MOVE ZEROES TO FRM-NUM-ICAN IN CAC-FLD(1).
           MOVE ZEROES TO FRM-NUM-ICAN IN CAC-FLD(2).
           MOVE ZEROES TO FRM-NUM-ICAN IN CAC-FLD(3).
           MOVE ZEROES TO FRM-NUM-ICAN IN CAC-FLD(4).
           MOVE ZEROES TO FRM-SGV-CAPI IN CAC-FLD(1).
           MOVE ZEROES TO FRM-SGV-CAPI IN CAC-FLD(2).
           MOVE ZEROES TO FRM-SGV-CAPI IN CAC-FLD(3).
           MOVE ZEROES TO FRM-SGV-CAPI IN CAC-FLD(4).
           MOVE ZEROES TO FRM-SGV-INTE IN CAC-FLD(1).
           MOVE ZEROES TO FRM-SGV-INTE IN CAC-FLD(2).
           MOVE ZEROES TO FRM-SGV-INTE IN CAC-FLD(3).
           MOVE ZEROES TO FRM-SGV-INTE IN CAC-FLD(4).
           MOVE ZEROES TO FRM-SGV-INTP IN CAC-FLD(1).
           MOVE ZEROES TO FRM-SGV-INTP IN CAC-FLD(2).
           MOVE ZEROES TO FRM-SGV-INTP IN CAC-FLD(3).
           MOVE ZEROES TO FRM-SGV-INTP IN CAC-FLD(4).
           MOVE SPACES TO FRM-COD-CANC IN CAC-FLD(1).
           MOVE SPACES TO FRM-COD-CANC IN CAC-FLD(2).
           MOVE SPACES TO FRM-COD-CANC IN CAC-FLD(3).
           MOVE SPACES TO FRM-COD-CANC IN CAC-FLD(4).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FCAN IN CAC-FLD(1).
           MOVE ZEROES TO FRM-FEC-FCAN IN CAC-FLD(2).
           MOVE ZEROES TO FRM-FEC-FCAN IN CAC-FLD(3).
           MOVE ZEROES TO FRM-FEC-FCAN IN CAC-FLD(4).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-SGV-TPAG IN CAC-FLD(1).
           MOVE ZEROES TO FRM-SGV-TPAG IN CAC-FLD(2).
           MOVE ZEROES TO FRM-SGV-TPAG IN CAC-FLD(3).
           MOVE ZEROES TO FRM-SGV-TPAG IN CAC-FLD(4).
           MOVE ZEROES TO FRM-SGV-VREN IN CAC-FLD(1).
           MOVE ZEROES TO FRM-SGV-VREN IN CAC-FLD(2).
           MOVE ZEROES TO FRM-SGV-VREN IN CAC-FLD(3).
           MOVE ZEROES TO FRM-SGV-VREN IN CAC-FLD(4).
           MOVE ZEROES TO FRM-SGV-SEGU IN CAC-FLD(1).
           MOVE ZEROES TO FRM-SGV-SEGU IN CAC-FLD(2).
           MOVE ZEROES TO FRM-SGV-SEGU IN CAC-FLD(3).
           MOVE ZEROES TO FRM-SGV-SEGU IN CAC-FLD(4).
           MOVE SPACES TO FRM-COD-STAT IN CAC-FLD(1).
           MOVE SPACES TO FRM-COD-STAT IN CAC-FLD(2).
           MOVE SPACES TO FRM-COD-STAT IN CAC-FLD(3).
           MOVE SPACES TO FRM-COD-STAT IN CAC-FLD(4).
           PERFORM CLR-OPC-CAC.
           PERFORM CLR-DBC-CAC.
           PERFORM CLR-ITR-CAC.
           PERFORM CLR-VEN-CAC.
*% INCLUDE FIN_CLR_FLD_CAC OF 'COLBMCAC.TXT'
       FIN-CLR-FLD-CAC.
           EXIT.
       
       CLR-OPC-CAC SECTION.
       INI-CLR-OPC-CAC.
*% INCLUDE INI_CLR_OPC_CAC OF 'COLBMCAC.TXT'
           MOVE SPACES TO OPC-COD-VCOC IN CAC-FLD.
           MOVE SPACES TO OPC-COD-TOOC IN CAC-FLD.
           MOVE SPACES TO OPC-COD-AOOC IN CAC-FLD.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO OPC-FEC-FCOL IN CAC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO OPC-VAL-CRED IN CAC-FLD.
           MOVE ZEROES TO OPC-VAL-SCRE IN CAC-FLD.
           MOVE ZEROES TO OPC-SGV-LIQU IN CAC-FLD.
*% INCLUDE FIN_CLR_OPC_CAC OF 'COLBMCAC.TXT'
       FIN-CLR-OPC-CAC.
           EXIT.
       
       CLR-DBC-CAC SECTION.
       INI-CLR-DBC-CAC.
*% INCLUDE INI_CLR_DBC_CAC OF 'COLBMCAC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN CAC-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN CAC-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN CAC-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN CAC-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO DBC-GLS-NOMC IN CAC-FLD.
*% INCLUDE FIN_CLR_DBC_CAC OF 'COLBMCAC.TXT'
       FIN-CLR-DBC-CAC.
           EXIT.
       
       CLR-ITR-CAC SECTION.
       INI-CLR-ITR-CAC.
*% INCLUDE INI_CLR_ITR_CAC OF 'COLBMCAC.TXT'
           MOVE ZEROES TO ITR-SGV-TINT IN CAC-FLD.
*% INCLUDE FIN_CLR_ITR_CAC OF 'COLBMCAC.TXT'
       FIN-CLR-ITR-CAC.
           EXIT.
       
       CLR-VEN-CAC SECTION.
       INI-CLR-VEN-CAC.
*% INCLUDE INI_CLR_VEN_CAC OF 'COLBMCAC.TXT'
           MOVE ZEROES TO VEN-NUM-IVCT IN CAC-FLD.
*% RREG = "VEN"
*% RREG_FRM = "VEN"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO VEN-FEC-FVEN IN CAC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO VEN-VAL-CAPI IN CAC-FLD.
           MOVE ZEROES TO VEN-VAL-FINA IN CAC-FLD.
*% INCLUDE FIN_CLR_VEN_CAC OF 'COLBMCAC.TXT'
       FIN-CLR-VEN-CAC.
           EXIT.
       
       FST-KEY-CAC SECTION.
       INI-FST-KEY-CAC.
*% INCLUDE INI_FST_KEY_CAC OF 'COLBMCAC.TXT'
           MOVE -1 TO OPC-CAI-IOPC-LEN IN CAC-FLD.
*% INCLUDE FIN_FST_KEY_CAC OF 'COLBMCAC.TXT'
       FIN-FST-KEY-CAC.
           EXIT.
       
       FST-FLD-CAC SECTION.
       INI-FST-FLD-CAC.
*% INCLUDE INI_FST_FLD_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_FST_FLD_CAC OF 'COLBMCAC.TXT'
       FIN-FST-FLD-CAC.
           EXIT.
       
       PRO-KEY-CAC SECTION.
       INI-PRO-KEY-CAC.
*% INCLUDE INI_PRO_KEY_CAC OF 'COLBMCAC.TXT'
           MOVE FRM-CPIM-AEY TO FRM-NUM-VDES-ATR IN CAC-FLD.
           MOVE FRM-CPIM-AEY TO FRM-NUM-VHAS-ATR IN CAC-FLD.
           PERFORM PRO-IKY-CAC.
           PERFORM PRO-AKY-CAC.
           PERFORM PRO-MKY-CAC.
*% INCLUDE FIN_PRO_KEY_CAC OF 'COLBMCAC.TXT'
       FIN-PRO-KEY-CAC.
           EXIT.
       
       PRO-IKY-CAC SECTION.
       INI-PRO-IKY-CAC.
*% INCLUDE INI_PRO_IKY_CAC OF 'COLBMCAC.TXT'
           MOVE FRM-CPIM-AEY TO OPC-CAI-IOPC-ATR IN CAC-FLD.
           MOVE FRM-CPIM-AEY TO OPC-IIC-IOPC-ATR IN CAC-FLD.
*% INCLUDE FIN_PRO_IKY_CAC OF 'COLBMCAC.TXT'
       FIN-PRO-IKY-CAC.
           EXIT.
       
       PRO-MKY-CAC SECTION.
       INI-PRO-MKY-CAC.
*% INCLUDE INI_PRO_MKY_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_PRO_MKY_CAC OF 'COLBMCAC.TXT'
       FIN-PRO-MKY-CAC.
           EXIT.
       
       PRO-AKY-CAC SECTION.
       INI-PRO-AKY-CAC.
*% INCLUDE INI_PRO_AKY_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_PRO_AKY_CAC OF 'COLBMCAC.TXT'
       FIN-PRO-AKY-CAC.
           EXIT.
       
       UNP-KEY-CAC SECTION.
       INI-UNP-KEY-CAC.
*% INCLUDE INI_UNP_KEY_CAC OF 'COLBMCAC.TXT'
           MOVE FRM-CPIM-UBY TO FRM-NUM-VDES-ATR IN CAC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-NUM-VHAS-ATR IN CAC-FLD.
           PERFORM UNP-IKY-CAC.
           PERFORM UNP-AKY-CAC.
           PERFORM UNP-MKY-CAC.
*% INCLUDE FIN_UNP_KEY_CAC OF 'COLBMCAC.TXT'
       FIN-UNP-KEY-CAC.
           EXIT.
       
       UNP-IKY-CAC SECTION.
       INI-UNP-IKY-CAC.
*% INCLUDE INI_UNP_IKY_CAC OF 'COLBMCAC.TXT'
           MOVE FRM-CPIM-UBY TO OPC-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO OPC-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-NUM-VDES-ATR.
           MOVE FRM-CPIM-UBY TO FRM-NUM-VHAS-ATR.
*% INCLUDE FIN_UNP_IKY_CAC OF 'COLBMCAC.TXT'
       FIN-UNP-IKY-CAC.
           EXIT.
       
       UNP-MKY-CAC SECTION.
       INI-UNP-MKY-CAC.
*% INCLUDE INI_UNP_MKY_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_UNP_MKY_CAC OF 'COLBMCAC.TXT'
       FIN-UNP-MKY-CAC.
           EXIT.
       
       UNP-AKY-CAC SECTION.
       INI-UNP-AKY-CAC.
*% INCLUDE INI_UNP_AKY_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_UNP_AKY_CAC OF 'COLBMCAC.TXT'
       FIN-UNP-AKY-CAC.
           EXIT.
       
       UNP-FLD-CAC SECTION.
       INI-UNP-FLD-CAC.
*% INCLUDE INI_UNP_FLD_CAC OF 'COLBMCAC.TXT'
           PERFORM UNP-OPC-CAC.
           PERFORM UNP-DBC-CAC.
           PERFORM UNP-ITR-CAC.
           PERFORM UNP-VEN-CAC.
*% INCLUDE FIN_UNP_FLD_CAC OF 'COLBMCAC.TXT'
       FIN-UNP-FLD-CAC.
           EXIT.
       
       UNP-OPC-CAC SECTION.
       INI-UNP-OPC-CAC.
*% INCLUDE INI_UNP_OPC_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_UNP_OPC_CAC OF 'COLBMCAC.TXT'
       FIN-UNP-OPC-CAC.
           EXIT.
       
       UNP-DBC-CAC SECTION.
       INI-UNP-DBC-CAC.
*% INCLUDE INI_UNP_DBC_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_UNP_DBC_CAC OF 'COLBMCAC.TXT'
       FIN-UNP-DBC-CAC.
           EXIT.
       
       UNP-ITR-CAC SECTION.
       INI-UNP-ITR-CAC.
*% INCLUDE INI_UNP_ITR_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_UNP_ITR_CAC OF 'COLBMCAC.TXT'
       FIN-UNP-ITR-CAC.
           EXIT.
       
       UNP-VEN-CAC SECTION.
       INI-UNP-VEN-CAC.
*% INCLUDE INI_UNP_VEN_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_UNP_VEN_CAC OF 'COLBMCAC.TXT'
       FIN-UNP-VEN-CAC.
           EXIT.
       
       VAL-KEY-CAC SECTION.
       INI-VAL-KEY-CAC.
           PERFORM VAL-IKY-CAC.
           PERFORM VAL-MKY-CAC.
           PERFORM VAL-AKY-CAC.
       FIN-VAL-KEY-CAC.
           EXIT.
       
       VAL-IKY-CAC SECTION.
       INI-VAL-IKY-CAC.
*% INCLUDE INI_VAL_IKY_CAC OF 'COLBMCAC.TXT'
           PERFORM VAL-OPC-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-CAI-IOPC-LEN IN CAC-FLD
                GO TO FIN-VAL-IKY-CAC.
           PERFORM VAL-OPC-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-IIC-IOPC-LEN IN CAC-FLD
                GO TO FIN-VAL-IKY-CAC.
           PERFORM VAL-FRM-NUM-VDES.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-NUM-VDES-LEN IN CAC-FLD
                GO TO FIN-VAL-IKY-CAC.
           PERFORM VAL-FRM-NUM-VHAS.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-NUM-VHAS-LEN IN CAC-FLD
                GO TO FIN-VAL-IKY-CAC.
*% INCLUDE FIN_VAL_IKY_CAC OF 'COLBMCAC.TXT'
       FIN-VAL-IKY-CAC.
           EXIT.
       
       VAL-MKY-CAC SECTION.
       INI-VAL-MKY-CAC.
*% INCLUDE INI_VAL_MKY_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_VAL_MKY_CAC OF 'COLBMCAC.TXT'
       FIN-VAL-MKY-CAC.
           EXIT.
       
       VAL-AKY-CAC SECTION.
       INI-VAL-AKY-CAC.
*% INCLUDE INI_VAL_AKY_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_VAL_AKY_CAC OF 'COLBMCAC.TXT'
       FIN-VAL-AKY-CAC.
           EXIT.
       
       VAL-OPC-CAI-IOPC SECTION.
       INI-VAL-OPC-CAI-IOPC.
*% LBL = "OPC_CAI_IOPC OF 'COLBMCAC.TXT'"
*% INCLUDE INI_OPC_CAI_IOPC OF 'COLBMCAC.TXT'
           IF OPC-CAI-IOPC IN CAC-FLD NOT > SPACES
              MOVE SPACES TO OPC-CAI-IOPC IN CAC-FLD.
*% INCLUDE OPC_CAI_IOPC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_OPC_CAI_IOPC OF 'COLBMCAC.TXT'
       FIN-VAL-OPC-CAI-IOPC.
           EXIT.
       
       VAL-OPC-IIC-IOPC SECTION.
       INI-VAL-OPC-IIC-IOPC.
