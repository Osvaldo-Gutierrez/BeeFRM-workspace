       EDT-KEY-CAC SECTION.
       INI-EDT-KEY-CAC.
           PERFORM EDT-IKY-CAC.
           PERFORM EDT-MKY-CAC.
           PERFORM EDT-AKY-CAC.
*% INCLUDE EDT_KEY_CAC OF 'COLBMCAC.TXT'
       FIN-EDT-KEY-CAC.
           EXIT.
       
       EDT-IKY-CAC SECTION.
       INI-EDT-IKY-CAC.
*% INCLUDE EDT_IKY_CAC OF 'COLBMCAC.TXT'
           IF OPC-IIC-IOPC-LEN IN CAC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO OPC-IIC-IOPC-LEN IN CAC-FLD
                GO TO FIN-EDT-IKY-CAC.
           MOVE OPC-IIC-IOPC IN CAC-FLD TO OPC-IIC-IOPC-EDT IN CAC-EDT.
           MOVE OPC-IIC-IOPC-EDT IN CAC-EDT TO 
                OPC-IIC-IOPC-ALF IN CAC-FLD.
           IF FRM-NUM-VDES-LEN IN CAC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-VDES-LEN IN CAC-FLD
                GO TO FIN-EDT-IKY-CAC.
           MOVE FRM-NUM-VDES IN CAC-FLD TO FRM-NUM-VDES-EDT IN CAC-EDT.
           MOVE FRM-NUM-VDES-EDT IN CAC-EDT TO 
                FRM-NUM-VDES-ALF IN CAC-FLD.
           IF FRM-NUM-VHAS-LEN IN CAC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-VHAS-LEN IN CAC-FLD
                GO TO FIN-EDT-IKY-CAC.
           MOVE FRM-NUM-VHAS IN CAC-FLD TO FRM-NUM-VHAS-EDT IN CAC-EDT.
           MOVE FRM-NUM-VHAS-EDT IN CAC-EDT TO 
                FRM-NUM-VHAS-ALF IN CAC-FLD.
*% INCLUDE FIN_EDT_IKY_CAC OF 'COLBMCAC.TXT'
       FIN-EDT-IKY-CAC.
           EXIT.
       
       EDT-MKY-CAC SECTION.
       INI-EDT-MKY-CAC.
*% INCLUDE EDT_MKY_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_EDT_MKY_CAC OF 'COLBMCAC.TXT'
       FIN-EDT-MKY-CAC.
           EXIT.
       
       EDT-AKY-CAC SECTION.
       INI-EDT-AKY-CAC.
*% INCLUDE EDT_AKY_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_EDT_AKY_CAC OF 'COLBMCAC.TXT'
       FIN-EDT-AKY-CAC.
           EXIT.
       
       EDT-FLD-CAC SECTION.
       INI-EDT-FLD-CAC.
*% INCLUDE EDT_FLD_CAC OF 'COLBMCAC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF DBC-NUM-ICLI-LEN IN CAC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DBC-NUM-ICLI-LEN IN CAC-FLD
                GO TO FIN-EDT-FLD-CAC.
           MOVE DBC-NUM-ICLI IN CAC-FLD TO
                DBC-NUM-ICLI-EDT IN CAC-EDT.
           MOVE DBC-NUM-ICLI-EDT IN CAC-EDT TO
                DBC-NUM-ICLI-ALF IN CAC-FLD.
      * IDC-FIN --> BMCHIBCI011.SRCAIU EDICION NUMERICA
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
           IF OPC-VAL-CRED-LEN IN CAC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO OPC-VAL-CRED-LEN IN CAC-FLD
                GO TO FIN-EDT-FLD-CAC.
           MOVE OPC-VAL-CRED IN CAC-FLD TO OPC-VAL-CRED-EDT IN CAC-EDT.
           MOVE OPC-VAL-CRED-EDT IN CAC-EDT TO 
                OPC-VAL-CRED-ALF IN CAC-FLD.
           IF OPC-VAL-SCRE-LEN IN CAC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO OPC-VAL-SCRE-LEN IN CAC-FLD
                GO TO FIN-EDT-FLD-CAC.
           MOVE OPC-VAL-SCRE IN CAC-FLD TO OPC-VAL-SCRE-EDT IN CAC-EDT.
           MOVE OPC-VAL-SCRE-EDT IN CAC-EDT TO 
                OPC-VAL-SCRE-ALF IN CAC-FLD.
           IF OPC-SGV-LIQU-LEN IN CAC-FLD = FRM-ERR-NUME
