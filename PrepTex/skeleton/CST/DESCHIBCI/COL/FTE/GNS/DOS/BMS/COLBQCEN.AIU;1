      *
      * COLBQCEN
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:17 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CEN"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMCEN.TXT'
       SET-TAG-COLBQCEN SECTION.
       INI-SET-TAG-COLBQCEN.
           MOVE
           '<IDNMOD NOM=$COLBQCEN$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:17 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCEN.
           EXIT.
           
       PUT-ALL-CEN SECTION.
       INI-PUT-ALL-CEN.
*% INCLUDE INI_PUT_ALL_CEN OF 'COLBMCEN.TXT'
           PERFORM PUT-CEN-CEN.
*% INCLUDE FIN_PUT_ALL_CEN OF 'COLBMCEN.TXT'
       FIN-PUT-ALL-CEN.
           EXIT.
       
       PUT-CEN-CEN SECTION.
       INI-PUT-CEN-CEN.
*% INCLUDE INI_PUT_CEN_CEN OF 'COLBMCEN.TXT'
           IF PGM-STAT-CEN-NUL
               GO TO FIN-PUT-CEN-CEN.
           IF NOT PGM-STAT-CEN-OKS
               PERFORM CLR-CEN-CEN
               GO TO FIN-PUT-CEN-CEN.
           MOVE CEN-COD-ENTI IN CEN TO CEN-COD-ENTI IN CEN-FLD.
           MOVE CEN-CAI-IENT IN CEN TO CEN-CAI-IENT IN CEN-FLD.
           MOVE CEN-IIC-IENT IN CEN TO CEN-IIC-IENT IN CEN-FLD.
*% RREG = "CEN"
*% RREG_FRM = "CEN"
*% NOMB = "FENT"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CEN-NUM-DENT IN CEN TO CEN-NUM-DENT IN CEN-FLD.
           MOVE CEN-NUM-MENT IN CEN TO CEN-NUM-MENT IN CEN-FLD.
           MOVE CEN-NUM-SENT IN CEN TO CEN-NUM-SENT IN CEN-FLD.
           MOVE CEN-NUM-AENT IN CEN TO CEN-NUM-AENT IN CEN-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE CEN-COD-CREN IN CEN TO CEN-COD-CREN IN CEN-FLD.
           MOVE CEN-COD-SREN IN CEN TO CEN-COD-SREN IN CEN-FLD.
           MOVE CEN-NUM-IENT IN CEN TO CEN-NUM-IENT IN CEN-FLD.
           MOVE CEN-COD-CCSE IN CEN TO CEN-COD-CCSE IN CEN-FLD.
           MOVE CEN-CAI-ISEN IN CEN TO CEN-CAI-ISEN IN CEN-FLD.
           MOVE CEN-IIC-ISEN IN CEN TO CEN-IIC-ISEN IN CEN-FLD.
           MOVE CEN-NUM-ISEN IN CEN TO CEN-NUM-ISEN IN CEN-FLD.
           MOVE CEN-MSC-STAT IN CEN TO CEN-MSC-STAT IN CEN-FLD.
           MOVE CEN-COD-TOPR IN CEN TO CEN-COD-TOPR IN CEN-FLD.
           MOVE CEN-COD-AOPR IN CEN TO CEN-COD-AOPR IN CEN-FLD.
           MOVE CEN-COD-VCAM IN CEN TO CEN-COD-VCAM IN CEN-FLD.
           MOVE CEN-COD-COD1 IN CEN TO CEN-COD-COD1 IN CEN-FLD.
           MOVE CEN-IND-IND1 IN CEN TO CEN-IND-IND1 IN CEN-FLD.
           MOVE CEN-COD-COD2 IN CEN TO CEN-COD-COD2 IN CEN-FLD.
           MOVE CEN-IND-IND2 IN CEN TO CEN-IND-IND2 IN CEN-FLD.
           MOVE CEN-COD-COD3 IN CEN TO CEN-COD-COD3 IN CEN-FLD.
           MOVE CEN-IND-IND3 IN CEN TO CEN-IND-IND3 IN CEN-FLD.
           MOVE CEN-COD-COD4 IN CEN TO CEN-COD-COD4 IN CEN-FLD.
           MOVE CEN-IND-IND4 IN CEN TO CEN-IND-IND4 IN CEN-FLD.
*% RREG = "CEN"
*% RREG_FRM = "CEN"
*% NOMB = "FFE1"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CEN-NUM-DFE1 IN CEN TO CEN-NUM-DFE1 IN CEN-FLD.
           MOVE CEN-NUM-MFE1 IN CEN TO CEN-NUM-MFE1 IN CEN-FLD.
           MOVE CEN-NUM-SFE1 IN CEN TO CEN-NUM-SFE1 IN CEN-FLD.
           MOVE CEN-NUM-AFE1 IN CEN TO CEN-NUM-AFE1 IN CEN-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% RREG = "CEN"
*% RREG_FRM = "CEN"
*% NOMB = "FFE2"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CEN-NUM-DFE2 IN CEN TO CEN-NUM-DFE2 IN CEN-FLD.
           MOVE CEN-NUM-MFE2 IN CEN TO CEN-NUM-MFE2 IN CEN-FLD.
           MOVE CEN-NUM-SFE2 IN CEN TO CEN-NUM-SFE2 IN CEN-FLD.
           MOVE CEN-NUM-AFE2 IN CEN TO CEN-NUM-AFE2 IN CEN-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% RREG = "CEN"
*% RREG_FRM = "CEN"
*% NOMB = "FFE3"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CEN-NUM-DFE3 IN CEN TO CEN-NUM-DFE3 IN CEN-FLD.
           MOVE CEN-NUM-MFE3 IN CEN TO CEN-NUM-MFE3 IN CEN-FLD.
           MOVE CEN-NUM-SFE3 IN CEN TO CEN-NUM-SFE3 IN CEN-FLD.
           MOVE CEN-NUM-AFE3 IN CEN TO CEN-NUM-AFE3 IN CEN-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% RREG = "CEN"
*% RREG_FRM = "CEN"
*% NOMB = "FFE4"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CEN-NUM-DFE4 IN CEN TO CEN-NUM-DFE4 IN CEN-FLD.
           MOVE CEN-NUM-MFE4 IN CEN TO CEN-NUM-MFE4 IN CEN-FLD.
           MOVE CEN-NUM-SFE4 IN CEN TO CEN-NUM-SFE4 IN CEN-FLD.
           MOVE CEN-NUM-AFE4 IN CEN TO CEN-NUM-AFE4 IN CEN-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% RREG = "CEN"
*% RREG_FRM = "CEN"
*% NOMB = "FFE5"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CEN-NUM-DFE5 IN CEN TO CEN-NUM-DFE5 IN CEN-FLD.
           MOVE CEN-NUM-MFE5 IN CEN TO CEN-NUM-MFE5 IN CEN-FLD.
           MOVE CEN-NUM-SFE5 IN CEN TO CEN-NUM-SFE5 IN CEN-FLD.
           MOVE CEN-NUM-AFE5 IN CEN TO CEN-NUM-AFE5 IN CEN-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% RREG = "CEN"
*% RREG_FRM = "CEN"
*% NOMB = "FFE6"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CEN-NUM-DFE6 IN CEN TO CEN-NUM-DFE6 IN CEN-FLD.
           MOVE CEN-NUM-MFE6 IN CEN TO CEN-NUM-MFE6 IN CEN-FLD.
           MOVE CEN-NUM-SFE6 IN CEN TO CEN-NUM-SFE6 IN CEN-FLD.
           MOVE CEN-NUM-AFE6 IN CEN TO CEN-NUM-AFE6 IN CEN-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE CEN-SGV-VAL1 IN CEN TO CEN-SGV-VAL1 IN CEN-FLD.
           MOVE CEN-NUM-CON1 IN CEN TO CEN-NUM-CON1 IN CEN-FLD.
           MOVE CEN-SGV-POR1 IN CEN TO CEN-SGV-POR1 IN CEN-FLD.
           MOVE CEN-SGV-VAL2 IN CEN TO CEN-SGV-VAL2 IN CEN-FLD.
           MOVE CEN-NUM-CON2 IN CEN TO CEN-NUM-CON2 IN CEN-FLD.
           MOVE CEN-SGV-POR2 IN CEN TO CEN-SGV-POR2 IN CEN-FLD.
           MOVE CEN-GLS-GLS1 IN CEN TO CEN-GLS-GLS1 IN CEN-FLD.
           MOVE CEN-GLS-GLS2 IN CEN TO CEN-GLS-GLS2 IN CEN-FLD.
           MOVE CEN-GLS-GLS3 IN CEN TO CEN-GLS-GLS3 IN CEN-FLD.
           MOVE CEN-GLS-GLS4 IN CEN TO CEN-GLS-GLS4 IN CEN-FLD.
*% INCLUDE FIN_PUT_CEN_CEN OF 'COLBMCEN.TXT'
       FIN-PUT-CEN-CEN.
           EXIT.
       
       KEY-ALL-CEN SECTION.
       INI-KEY-ALL-CEN.
*% INCLUDE INI_KEY_ALL_CEN OF 'COLBMCEN.TXT'
           PERFORM KEY-CEN-CEN.
*% INCLUDE FIN_KEY_ALL_CEN OF 'COLBMCEN.TXT'
       FIN-KEY-ALL-CEN.
           EXIT.
       
       KEY-CEN-CEN SECTION.
       INI-KEY-CEN-CEN.
*% INCLUDE INI_KEY_CEN_CEN OF 'COLBMCEN.TXT'
           MOVE CEN-COD-ENTI IN CEN-FLD TO CEN-COD-ENTI IN CEN.
           MOVE CEN-CAI-IENT IN CEN-FLD TO CEN-CAI-IENT IN CEN.
           MOVE CEN-IIC-IENT IN CEN-FLD TO CEN-IIC-IENT IN CEN.
*% RREG = "CEN"
*% RREG_FRM = "CEN"
*% NOMB = "FENT"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CEN-NUM-DENT IN CEN-FLD TO CEN-NUM-DENT IN CEN.
           MOVE CEN-NUM-MENT IN CEN-FLD TO CEN-NUM-MENT IN CEN.
           MOVE CEN-NUM-SENT IN CEN-FLD TO CEN-NUM-SENT IN CEN.
           MOVE CEN-NUM-AENT IN CEN-FLD TO CEN-NUM-AENT IN CEN.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CEN-COD-CREN IN CEN-FLD TO CEN-COD-CREN IN CEN.
           MOVE CEN-COD-SREN IN CEN-FLD TO CEN-COD-SREN IN CEN.
           MOVE CEN-NUM-IENT IN CEN-FLD TO CEN-NUM-IENT IN CEN.
           MOVE CEN-COD-CCSE IN CEN-FLD TO CEN-COD-CCSE IN CEN.
           MOVE CEN-CAI-ISEN IN CEN-FLD TO CEN-CAI-ISEN IN CEN.
           MOVE CEN-IIC-ISEN IN CEN-FLD TO CEN-IIC-ISEN IN CEN.
           MOVE CEN-NUM-ISEN IN CEN-FLD TO CEN-NUM-ISEN IN CEN.
*% INCLUDE FIN_KEY_CEN_CEN OF 'COLBMCEN.TXT'
       FIN-KEY-CEN-CEN.
           EXIT.
       
       SCR-CLRA-CEN SECTION.
       INI-SCR-CLRA-CEN.
*% INCLUDE INI_SCR_CLRA_CEN OF 'COLBMCEN.TXT'
           PERFORM CLR-KEY-CEN.
           PERFORM CLR-FLD-CEN.
*% INCLUDE FIN_SCR_CLRA_CEN OF 'COLBMCEN.TXT'
       FIN-SCR-CLRA-CEN.
           EXIT.
       
       CLR-KEY-CEN SECTION.
       INI-CLR-KEY-CEN.
*% INCLUDE INI_CLR_KEY_CEN OF 'COLBMCEN.TXT'
           MOVE SPACES TO CEN-COD-ENTI IN CEN-FLD.
           MOVE SPACES TO CEN-CAI-IENT IN CEN-FLD.
           MOVE ZEROES TO CEN-IIC-IENT IN CEN-FLD.
*% RREG = "CEN"
*% RREG_FRM = "CEN"
*% NOMB = "FENT"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CEN-FEC-FENT IN CEN-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO CEN-COD-CREN IN CEN-FLD.
           MOVE SPACES TO CEN-COD-SREN IN CEN-FLD.
           MOVE ZEROES TO CEN-NUM-IENT IN CEN-FLD.
           MOVE SPACES TO CEN-COD-CCSE IN CEN-FLD.
           MOVE SPACES TO CEN-CAI-ISEN IN CEN-FLD.
           MOVE ZEROES TO CEN-IIC-ISEN IN CEN-FLD.
           MOVE ZEROES TO CEN-NUM-ISEN IN CEN-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-ICLI IN CEN-FLD.
           MOVE SPACES TO FRM-VRF-ICLI IN CEN-FLD.
           MOVE SPACES TO FRM-IND-ICLI IN CEN-FLD.
           MOVE SPACES TO FRM-GLS-ICLI IN CEN-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_KEY_CEN OF 'COLBMCEN.TXT'
       FIN-CLR-KEY-CEN.
           EXIT.
       
       CLR-FLD-CEN SECTION.
       INI-CLR-FLD-CEN.
*% INCLUDE INI_CLR_FLD_CEN OF 'COLBMCEN.TXT'
           MOVE SPACES TO FRM-GLS-ENTI IN CEN-FLD.
           MOVE SPACES TO FRM-GLS-RENT IN CEN-FLD.
           MOVE SPACES TO FRM-GLS-CCSE IN CEN-FLD.
           MOVE SPACES TO FRM-GLS-VCAM IN CEN-FLD.
           PERFORM CLR-CEN-CEN.
*% INCLUDE FIN_CLR_FLD_CEN OF 'COLBMCEN.TXT'
       FIN-CLR-FLD-CEN.
           EXIT.
       
       CLR-CEN-CEN SECTION.
       INI-CLR-CEN-CEN.
*% INCLUDE INI_CLR_CEN_CEN OF 'COLBMCEN.TXT'
           MOVE SPACES TO CEN-MSC-STAT IN CEN-FLD.
           MOVE SPACES TO CEN-COD-TOPR IN CEN-FLD.
           MOVE SPACES TO CEN-COD-AOPR IN CEN-FLD.
           MOVE SPACES TO CEN-COD-VCAM IN CEN-FLD.
           MOVE SPACES TO CEN-COD-COD1 IN CEN-FLD.
           MOVE SPACES TO CEN-IND-IND1 IN CEN-FLD.
           MOVE SPACES TO CEN-COD-COD2 IN CEN-FLD.
           MOVE SPACES TO CEN-IND-IND2 IN CEN-FLD.
           MOVE SPACES TO CEN-COD-COD3 IN CEN-FLD.
           MOVE SPACES TO CEN-IND-IND3 IN CEN-FLD.
           MOVE SPACES TO CEN-COD-COD4 IN CEN-FLD.
           MOVE SPACES TO CEN-IND-IND4 IN CEN-FLD.
*% RREG = "CEN"
*% RREG_FRM = "CEN"
*% NOMB = "FFE1"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CEN-FEC-FFE1 IN CEN-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "CEN"
*% RREG_FRM = "CEN"
*% NOMB = "FFE2"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CEN-FEC-FFE2 IN CEN-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "CEN"
*% RREG_FRM = "CEN"
*% NOMB = "FFE3"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CEN-FEC-FFE3 IN CEN-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "CEN"
*% RREG_FRM = "CEN"
*% NOMB = "FFE4"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CEN-FEC-FFE4 IN CEN-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "CEN"
*% RREG_FRM = "CEN"
*% NOMB = "FFE5"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CEN-FEC-FFE5 IN CEN-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "CEN"
*% RREG_FRM = "CEN"
*% NOMB = "FFE6"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CEN-FEC-FFE6 IN CEN-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CEN-SGV-VAL1 IN CEN-FLD.
           MOVE ZEROES TO CEN-NUM-CON1 IN CEN-FLD.
           MOVE ZEROES TO CEN-SGV-POR1 IN CEN-FLD.
           MOVE ZEROES TO CEN-SGV-VAL2 IN CEN-FLD.
           MOVE ZEROES TO CEN-NUM-CON2 IN CEN-FLD.
           MOVE ZEROES TO CEN-SGV-POR2 IN CEN-FLD.
           MOVE SPACES TO CEN-GLS-GLS1 IN CEN-FLD.
           MOVE SPACES TO CEN-GLS-GLS2 IN CEN-FLD.
           MOVE SPACES TO CEN-GLS-GLS3 IN CEN-FLD.
           MOVE SPACES TO CEN-GLS-GLS4 IN CEN-FLD.
*% INCLUDE FIN_CLR_CEN_CEN OF 'COLBMCEN.TXT'
       FIN-CLR-CEN-CEN.
           EXIT.
       
       FST-KEY-CEN SECTION.
       INI-FST-KEY-CEN.
*% INCLUDE INI_FST_KEY_CEN OF 'COLBMCEN.TXT'
           MOVE -1 TO CEN-COD-ENTI-LEN IN CEN-FLD.
*% INCLUDE FIN_FST_KEY_CEN OF 'COLBMCEN.TXT'
       FIN-FST-KEY-CEN.
           EXIT.
       
       FST-FLD-CEN SECTION.
       INI-FST-FLD-CEN.
*% INCLUDE INI_FST_FLD_CEN OF 'COLBMCEN.TXT'
           MOVE -1 TO CEN-MSC-STAT-LEN IN CEN-FLD.
*% INCLUDE FIN_FST_FLD_CEN OF 'COLBMCEN.TXT'
       FIN-FST-FLD-CEN.
           EXIT.
       
       PRO-KEY-CEN SECTION.
       INI-PRO-KEY-CEN.
*% INCLUDE INI_PRO_KEY_CEN OF 'COLBMCEN.TXT'
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-NUM-ICLI-ATR IN CEN-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VRF-ICLI-ATR IN CEN-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-ICLI-ATR IN CEN-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-ICLI-ATR IN CEN-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           PERFORM PRO-IKY-CEN.
           PERFORM PRO-AKY-CEN.
           PERFORM PRO-MKY-CEN.
*% INCLUDE FIN_PRO_KEY_CEN OF 'COLBMCEN.TXT'
       FIN-PRO-KEY-CEN.
           EXIT.
       
       PRO-IKY-CEN SECTION.
       INI-PRO-IKY-CEN.
*% INCLUDE INI_PRO_IKY_CEN OF 'COLBMCEN.TXT'
           MOVE FRM-CPIM-AEY TO CEN-COD-ENTI-ATR IN CEN-FLD.
           MOVE FRM-CPIM-AEY TO CEN-CAI-IENT-ATR IN CEN-FLD.
           MOVE FRM-CPIM-AEY TO CEN-IIC-IENT-ATR IN CEN-FLD.
*% RREG = "CEN"
*% RREG_FRM = "CEN"
*% NOMB = "FENT"
           MOVE FRM-CPIM-AEY TO CEN-FEC-FENT-ATR IN CEN-FLD.
           MOVE FRM-CPIM-AEY TO CEN-COD-CREN-ATR IN CEN-FLD.
           MOVE FRM-CPIM-AEY TO CEN-COD-SREN-ATR IN CEN-FLD.
           MOVE FRM-CPIM-AEY TO CEN-NUM-IENT-ATR IN CEN-FLD.
           MOVE FRM-CPIM-AEY TO CEN-COD-CCSE-ATR IN CEN-FLD.
           MOVE FRM-CPIM-AEY TO CEN-CAI-ISEN-ATR IN CEN-FLD.
           MOVE FRM-CPIM-AEY TO CEN-IIC-ISEN-ATR IN CEN-FLD.
           MOVE FRM-CPIM-AEY TO CEN-NUM-ISEN-ATR IN CEN-FLD.
*% INCLUDE FIN_PRO_IKY_CEN OF 'COLBMCEN.TXT'
       FIN-PRO-IKY-CEN.
           EXIT.
       
       PRO-MKY-CEN SECTION.
       INI-PRO-MKY-CEN.
*% INCLUDE INI_PRO_MKY_CEN OF 'COLBMCEN.TXT'
*% INCLUDE FIN_PRO_MKY_CEN OF 'COLBMCEN.TXT'
       FIN-PRO-MKY-CEN.
           EXIT.
       
       PRO-AKY-CEN SECTION.
       INI-PRO-AKY-CEN.
*% INCLUDE INI_PRO_AKY_CEN OF 'COLBMCEN.TXT'
*% INCLUDE FIN_PRO_AKY_CEN OF 'COLBMCEN.TXT'
       FIN-PRO-AKY-CEN.
           EXIT.
       
       UNP-KEY-CEN SECTION.
       INI-UNP-KEY-CEN.
*% INCLUDE INI_UNP_KEY_CEN OF 'COLBMCEN.TXT'
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO FRM-NUM-ICLI-ATR IN CEN-FLD.
           MOVE FRM-CPIM-UBY TO FRM-VRF-ICLI-ATR IN CEN-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IND-ICLI-ATR IN CEN-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-ICLI-ATR IN CEN-FLD.
      * IDC-FIN --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           PERFORM UNP-IKY-CEN.
           PERFORM UNP-AKY-CEN.
           PERFORM UNP-MKY-CEN.
*% INCLUDE FIN_UNP_KEY_CEN OF 'COLBMCEN.TXT'
       FIN-UNP-KEY-CEN.
           EXIT.
       
       UNP-IKY-CEN SECTION.
       INI-UNP-IKY-CEN.
*% INCLUDE INI_UNP_IKY_CEN OF 'COLBMCEN.TXT'
           MOVE FRM-CPIM-UBY TO CEN-COD-ENTI-ATR.
           MOVE FRM-CPIM-UBY TO CEN-CAI-IENT-ATR.
           MOVE FRM-CPIM-UBY TO CEN-IIC-IENT-ATR.
*% RREG = "CEN"
*% RREG_FRM = "CEN"
*% NOMB = "FENT"
           MOVE FRM-CPIM-UBY TO CEN-FEC-FENT-ATR IN CEN-FLD.
           MOVE FRM-CPIM-UBY TO CEN-COD-CREN-ATR.
           MOVE FRM-CPIM-UBY TO CEN-COD-SREN-ATR.
           MOVE FRM-CPIM-UBY TO CEN-NUM-IENT-ATR.
           MOVE FRM-CPIM-UBY TO CEN-COD-CCSE-ATR.
           MOVE FRM-CPIM-UBY TO CEN-CAI-ISEN-ATR.
           MOVE FRM-CPIM-UBY TO CEN-IIC-ISEN-ATR.
           MOVE FRM-CPIM-UBY TO CEN-NUM-ISEN-ATR.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO FRM-NUM-ICLI-ATR IN CEN-FLD.
           MOVE FRM-CPIM-UBY TO FRM-VRF-ICLI-ATR IN CEN-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IND-ICLI-ATR IN CEN-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-ICLI-ATR IN CEN-FLD.
