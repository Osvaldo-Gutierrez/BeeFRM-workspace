                FRM-VAL-TOTA-ALF IN PLC-FLD(1).
           IF FRM-VAL-TOTA-LEN IN PLC-FLD(2) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-TOTA-LEN IN PLC-FLD(2)
                GO TO FIN-EDT-FLD-PLC.
           MOVE FRM-VAL-TOTA IN PLC-FLD(2) TO 
                FRM-VAL-TOTA-EDT IN PLC-EDT.
           MOVE FRM-VAL-TOTA-EDT IN PLC-EDT TO 
                FRM-VAL-TOTA-ALF IN PLC-FLD(2).
           IF FRM-VAL-TOTA-LEN IN PLC-FLD(3) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-TOTA-LEN IN PLC-FLD(3)
                GO TO FIN-EDT-FLD-PLC.
           MOVE FRM-VAL-TOTA IN PLC-FLD(3) TO 
                FRM-VAL-TOTA-EDT IN PLC-EDT.
           MOVE FRM-VAL-TOTA-EDT IN PLC-EDT TO 
                FRM-VAL-TOTA-ALF IN PLC-FLD(3).
           IF FRM-VAL-TOTA-LEN IN PLC-FLD(4) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-TOTA-LEN IN PLC-FLD(4)
                GO TO FIN-EDT-FLD-PLC.
           MOVE FRM-VAL-TOTA IN PLC-FLD(4) TO 
                FRM-VAL-TOTA-EDT IN PLC-EDT.
           MOVE FRM-VAL-TOTA-EDT IN PLC-EDT TO 
                FRM-VAL-TOTA-ALF IN PLC-FLD(4).
           IF FRM-VAL-TOTA-LEN IN PLC-FLD(5) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-TOTA-LEN IN PLC-FLD(5)
                GO TO FIN-EDT-FLD-PLC.
           MOVE FRM-VAL-TOTA IN PLC-FLD(5) TO 
                FRM-VAL-TOTA-EDT IN PLC-EDT.
           MOVE FRM-VAL-TOTA-EDT IN PLC-EDT TO 
                FRM-VAL-TOTA-ALF IN PLC-FLD(5).
           IF FRM-VAL-TOTA-LEN IN PLC-FLD(6) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-TOTA-LEN IN PLC-FLD(6)
                GO TO FIN-EDT-FLD-PLC.
           MOVE FRM-VAL-TOTA IN PLC-FLD(6) TO 
                FRM-VAL-TOTA-EDT IN PLC-EDT.
           MOVE FRM-VAL-TOTA-EDT IN PLC-EDT TO 
                FRM-VAL-TOTA-ALF IN PLC-FLD(6).
           IF FRM-VAL-TOTA-LEN IN PLC-FLD(7) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-TOTA-LEN IN PLC-FLD(7)
                GO TO FIN-EDT-FLD-PLC.
           MOVE FRM-VAL-TOTA IN PLC-FLD(7) TO 
                FRM-VAL-TOTA-EDT IN PLC-EDT.
           MOVE FRM-VAL-TOTA-EDT IN PLC-EDT TO 
                FRM-VAL-TOTA-ALF IN PLC-FLD(7).
           IF FRM-VAL-TOTA-LEN IN PLC-FLD(8) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-TOTA-LEN IN PLC-FLD(8)
                GO TO FIN-EDT-FLD-PLC.
           MOVE FRM-VAL-TOTA IN PLC-FLD(8) TO 
                FRM-VAL-TOTA-EDT IN PLC-EDT.
           MOVE FRM-VAL-TOTA-EDT IN PLC-EDT TO 
                FRM-VAL-TOTA-ALF IN PLC-FLD(8).
           IF FRM-VAL-TOTA-LEN IN PLC-FLD(9) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-TOTA-LEN IN PLC-FLD(9)
                GO TO FIN-EDT-FLD-PLC.
           MOVE FRM-VAL-TOTA IN PLC-FLD(9) TO 
                FRM-VAL-TOTA-EDT IN PLC-EDT.
           MOVE FRM-VAL-TOTA-EDT IN PLC-EDT TO 
                FRM-VAL-TOTA-ALF IN PLC-FLD(9).
*% INCLUDE FIN_EDT_FLD_PLC OF 'COLBMPLC.TXT'
       FIN-EDT-FLD-PLC.
           EXIT.
