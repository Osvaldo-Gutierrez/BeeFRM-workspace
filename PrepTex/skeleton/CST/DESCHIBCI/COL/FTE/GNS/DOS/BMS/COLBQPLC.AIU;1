      *
      * COLBQPLC
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:25 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "PLC"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMPLC.TXT'
       SET-TAG-COLBQPLC SECTION.
       INI-SET-TAG-COLBQPLC.
           MOVE
           '<IDNMOD NOM=$COLBQPLC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:25 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQPLC.
           EXIT.
           
       PUT-ALL-PLC SECTION.
       INI-PUT-ALL-PLC.
*% INCLUDE INI_PUT_ALL_PLC OF 'COLBMPLC.TXT'
           PERFORM PUT-OPC-PLC.
           PERFORM PUT-DBC-PLC.
           PERFORM PUT-ITR-PLC.
*% INCLUDE FIN_PUT_ALL_PLC OF 'COLBMPLC.TXT'
       FIN-PUT-ALL-PLC.
           EXIT.
       
       PUT-OPC-PLC SECTION.
       INI-PUT-OPC-PLC.
*% INCLUDE INI_PUT_OPC_PLC OF 'COLBMPLC.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-PLC.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-PLC
               GO TO FIN-PUT-OPC-PLC.
           MOVE OPC-CAI-IOPC IN OPC TO OPC-CAI-IOPC IN PLC-FLD.
           MOVE OPC-IIC-IOPC IN OPC TO OPC-IIC-IOPC IN PLC-FLD.
           MOVE OPC-COD-VCOC IN OPC TO OPC-COD-VCOC IN PLC-FLD.
           MOVE OPC-COD-TOOC IN OPC TO OPC-COD-TOOC IN PLC-FLD.
           MOVE OPC-COD-AOOC IN OPC TO OPC-COD-AOOC IN PLC-FLD.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE OPC-NUM-DCOL IN OPC TO OPC-NUM-DCOL IN PLC-FLD.
           MOVE OPC-NUM-MCOL IN OPC TO OPC-NUM-MCOL IN PLC-FLD.
           MOVE OPC-NUM-SCOL IN OPC TO OPC-NUM-SCOL IN PLC-FLD.
           MOVE OPC-NUM-ACOL IN OPC TO OPC-NUM-ACOL IN PLC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE OPC-VAL-SEGD IN OPC TO OPC-VAL-SEGD IN PLC-FLD.
           MOVE OPC-SGV-LIQU IN OPC TO OPC-SGV-LIQU IN PLC-FLD.
*% INCLUDE FIN_PUT_OPC_PLC OF 'COLBMPLC.TXT'
       FIN-PUT-OPC-PLC.
           EXIT.
       
       PUT-DBC-PLC SECTION.
       INI-PUT-DBC-PLC.
*% INCLUDE INI_PUT_DBC_PLC OF 'COLBMPLC.TXT'
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-PLC.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-PLC
               GO TO FIN-PUT-DBC-PLC.
           MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN PLC-FLD.
*% INCLUDE FIN_PUT_DBC_PLC OF 'COLBMPLC.TXT'
       FIN-PUT-DBC-PLC.
           EXIT.
       
       PUT-ITR-PLC SECTION.
       INI-PUT-ITR-PLC.
*% INCLUDE INI_PUT_ITR_PLC OF 'COLBMPLC.TXT'
           IF PGM-STAT-ITR-NUL
               GO TO FIN-PUT-ITR-PLC.
           IF NOT PGM-STAT-ITR-OKS
               PERFORM CLR-ITR-PLC
               GO TO FIN-PUT-ITR-PLC.
           MOVE ITR-SGV-TINT IN ITR TO ITR-SGV-TINT IN PLC-FLD.
*% INCLUDE FIN_PUT_ITR_PLC OF 'COLBMPLC.TXT'
       FIN-PUT-ITR-PLC.
           EXIT.
       
       KEY-ALL-PLC SECTION.
       INI-KEY-ALL-PLC.
*% INCLUDE INI_KEY_ALL_PLC OF 'COLBMPLC.TXT'
           PERFORM KEY-OPC-PLC.
           PERFORM KEY-DBC-PLC.
           PERFORM KEY-ITR-PLC.
*% INCLUDE FIN_KEY_ALL_PLC OF 'COLBMPLC.TXT'
       FIN-KEY-ALL-PLC.
           EXIT.
       
       KEY-OPC-PLC SECTION.
       INI-KEY-OPC-PLC.
*% INCLUDE INI_KEY_OPC_PLC OF 'COLBMPLC.TXT'
           MOVE OPC-CAI-IOPC IN PLC-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE OPC-IIC-IOPC IN PLC-FLD TO OPC-IIC-IOPC IN OPC.
*% INCLUDE FIN_KEY_OPC_PLC OF 'COLBMPLC.TXT'
       FIN-KEY-OPC-PLC.
           EXIT.
       
       KEY-DBC-PLC SECTION.
       INI-KEY-DBC-PLC.
*% INCLUDE INI_KEY_DBC_PLC OF 'COLBMPLC.TXT'
*% INCLUDE FIN_KEY_DBC_PLC OF 'COLBMPLC.TXT'
       FIN-KEY-DBC-PLC.
           EXIT.
       
       KEY-ITR-PLC SECTION.
       INI-KEY-ITR-PLC.
*% INCLUDE INI_KEY_ITR_PLC OF 'COLBMPLC.TXT'
*% INCLUDE FIN_KEY_ITR_PLC OF 'COLBMPLC.TXT'
       FIN-KEY-ITR-PLC.
           EXIT.
       
       SCR-CLRA-PLC SECTION.
       INI-SCR-CLRA-PLC.
*% INCLUDE INI_SCR_CLRA_PLC OF 'COLBMPLC.TXT'
           PERFORM CLR-KEY-PLC.
           PERFORM CLR-FLD-PLC.
*% INCLUDE FIN_SCR_CLRA_PLC OF 'COLBMPLC.TXT'
       FIN-SCR-CLRA-PLC.
           EXIT.
       
       CLR-KEY-PLC SECTION.
       INI-CLR-KEY-PLC.
*% INCLUDE INI_CLR_KEY_PLC OF 'COLBMPLC.TXT'
           MOVE SPACES TO OPC-CAI-IOPC IN PLC-FLD.
           MOVE ZEROES TO OPC-IIC-IOPC IN PLC-FLD.
*% INCLUDE FIN_CLR_KEY_PLC OF 'COLBMPLC.TXT'
       FIN-CLR-KEY-PLC.
           EXIT.
       
       CLR-FLD-PLC SECTION.
       INI-CLR-FLD-PLC.
*% INCLUDE INI_CLR_FLD_PLC OF 'COLBMPLC.TXT'
           MOVE SPACES TO FRM-GLS-VEXP IN PLC-FLD.
           MOVE ZEROES TO FRM-VAL-CRED IN PLC-FLD.
           MOVE ZEROES TO FRM-VAL-INTE IN PLC-FLD.
           MOVE ZEROES TO FRM-VAL-VCOM IN PLC-FLD.
           MOVE ZEROES TO FRM-VAL-GAST IN PLC-FLD.
           MOVE ZEROES TO FRM-VAL-IMPU IN PLC-FLD.
           MOVE SPACES TO FRM-GLS-TINT IN PLC-FLD.
           MOVE ZEROES TO FRM-NUM-IVCT IN PLC-FLD(1).
           MOVE ZEROES TO FRM-NUM-IVCT IN PLC-FLD(2).
           MOVE ZEROES TO FRM-NUM-IVCT IN PLC-FLD(3).
           MOVE ZEROES TO FRM-NUM-IVCT IN PLC-FLD(4).
           MOVE ZEROES TO FRM-NUM-IVCT IN PLC-FLD(5).
           MOVE ZEROES TO FRM-NUM-IVCT IN PLC-FLD(6).
           MOVE ZEROES TO FRM-NUM-IVCT IN PLC-FLD(7).
           MOVE ZEROES TO FRM-NUM-IVCT IN PLC-FLD(8).
           MOVE ZEROES TO FRM-NUM-IVCT IN PLC-FLD(9).
           MOVE SPACES TO FRM-IND-CANC IN PLC-FLD(1).
           MOVE SPACES TO FRM-IND-CANC IN PLC-FLD(2).
           MOVE SPACES TO FRM-IND-CANC IN PLC-FLD(3).
           MOVE SPACES TO FRM-IND-CANC IN PLC-FLD(4).
           MOVE SPACES TO FRM-IND-CANC IN PLC-FLD(5).
           MOVE SPACES TO FRM-IND-CANC IN PLC-FLD(6).
           MOVE SPACES TO FRM-IND-CANC IN PLC-FLD(7).
           MOVE SPACES TO FRM-IND-CANC IN PLC-FLD(8).
           MOVE SPACES TO FRM-IND-CANC IN PLC-FLD(9).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FVEN IN PLC-FLD(1).
           MOVE ZEROES TO FRM-FEC-FVEN IN PLC-FLD(2).
           MOVE ZEROES TO FRM-FEC-FVEN IN PLC-FLD(3).
           MOVE ZEROES TO FRM-FEC-FVEN IN PLC-FLD(4).
           MOVE ZEROES TO FRM-FEC-FVEN IN PLC-FLD(5).
           MOVE ZEROES TO FRM-FEC-FVEN IN PLC-FLD(6).
           MOVE ZEROES TO FRM-FEC-FVEN IN PLC-FLD(7).
           MOVE ZEROES TO FRM-FEC-FVEN IN PLC-FLD(8).
           MOVE ZEROES TO FRM-FEC-FVEN IN PLC-FLD(9).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-VAL-IMPC IN PLC-FLD(1).
           MOVE ZEROES TO FRM-VAL-IMPC IN PLC-FLD(2).
           MOVE ZEROES TO FRM-VAL-IMPC IN PLC-FLD(3).
           MOVE ZEROES TO FRM-VAL-IMPC IN PLC-FLD(4).
           MOVE ZEROES TO FRM-VAL-IMPC IN PLC-FLD(5).
           MOVE ZEROES TO FRM-VAL-IMPC IN PLC-FLD(6).
           MOVE ZEROES TO FRM-VAL-IMPC IN PLC-FLD(7).
           MOVE ZEROES TO FRM-VAL-IMPC IN PLC-FLD(8).
           MOVE ZEROES TO FRM-VAL-IMPC IN PLC-FLD(9).
           MOVE ZEROES TO FRM-VAL-COMC IN PLC-FLD(1).
           MOVE ZEROES TO FRM-VAL-COMC IN PLC-FLD(2).
           MOVE ZEROES TO FRM-VAL-COMC IN PLC-FLD(3).
           MOVE ZEROES TO FRM-VAL-COMC IN PLC-FLD(4).
           MOVE ZEROES TO FRM-VAL-COMC IN PLC-FLD(5).
           MOVE ZEROES TO FRM-VAL-COMC IN PLC-FLD(6).
           MOVE ZEROES TO FRM-VAL-COMC IN PLC-FLD(7).
           MOVE ZEROES TO FRM-VAL-COMC IN PLC-FLD(8).
           MOVE ZEROES TO FRM-VAL-COMC IN PLC-FLD(9).
           MOVE ZEROES TO FRM-VAL-INTC IN PLC-FLD(1).
           MOVE ZEROES TO FRM-VAL-INTC IN PLC-FLD(2).
           MOVE ZEROES TO FRM-VAL-INTC IN PLC-FLD(3).
           MOVE ZEROES TO FRM-VAL-INTC IN PLC-FLD(4).
           MOVE ZEROES TO FRM-VAL-INTC IN PLC-FLD(5).
           MOVE ZEROES TO FRM-VAL-INTC IN PLC-FLD(6).
           MOVE ZEROES TO FRM-VAL-INTC IN PLC-FLD(7).
           MOVE ZEROES TO FRM-VAL-INTC IN PLC-FLD(8).
           MOVE ZEROES TO FRM-VAL-INTC IN PLC-FLD(9).
           MOVE ZEROES TO FRM-VAL-SCAP IN PLC-FLD(1).
           MOVE ZEROES TO FRM-VAL-SCAP IN PLC-FLD(2).
           MOVE ZEROES TO FRM-VAL-SCAP IN PLC-FLD(3).
           MOVE ZEROES TO FRM-VAL-SCAP IN PLC-FLD(4).
           MOVE ZEROES TO FRM-VAL-SCAP IN PLC-FLD(5).
           MOVE ZEROES TO FRM-VAL-SCAP IN PLC-FLD(6).
           MOVE ZEROES TO FRM-VAL-SCAP IN PLC-FLD(7).
           MOVE ZEROES TO FRM-VAL-SCAP IN PLC-FLD(8).
           MOVE ZEROES TO FRM-VAL-SCAP IN PLC-FLD(9).
           MOVE ZEROES TO FRM-VAL-TOTA IN PLC-FLD(1).
           MOVE ZEROES TO FRM-VAL-TOTA IN PLC-FLD(2).
           MOVE ZEROES TO FRM-VAL-TOTA IN PLC-FLD(3).
           MOVE ZEROES TO FRM-VAL-TOTA IN PLC-FLD(4).
           MOVE ZEROES TO FRM-VAL-TOTA IN PLC-FLD(5).
           MOVE ZEROES TO FRM-VAL-TOTA IN PLC-FLD(6).
           MOVE ZEROES TO FRM-VAL-TOTA IN PLC-FLD(7).
           MOVE ZEROES TO FRM-VAL-TOTA IN PLC-FLD(8).
           MOVE ZEROES TO FRM-VAL-TOTA IN PLC-FLD(9).
           PERFORM CLR-OPC-PLC.
           PERFORM CLR-DBC-PLC.
           PERFORM CLR-ITR-PLC.
*% INCLUDE FIN_CLR_FLD_PLC OF 'COLBMPLC.TXT'
       FIN-CLR-FLD-PLC.
           EXIT.
       
       CLR-OPC-PLC SECTION.
       INI-CLR-OPC-PLC.
*% INCLUDE INI_CLR_OPC_PLC OF 'COLBMPLC.TXT'
           MOVE SPACES TO OPC-COD-VCOC IN PLC-FLD.
           MOVE SPACES TO OPC-COD-TOOC IN PLC-FLD.
           MOVE SPACES TO OPC-COD-AOOC IN PLC-FLD.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO OPC-FEC-FCOL IN PLC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO OPC-VAL-SEGD IN PLC-FLD.
           MOVE ZEROES TO OPC-SGV-LIQU IN PLC-FLD.
*% INCLUDE FIN_CLR_OPC_PLC OF 'COLBMPLC.TXT'
       FIN-CLR-OPC-PLC.
           EXIT.
       
       CLR-DBC-PLC SECTION.
       INI-CLR-DBC-PLC.
*% INCLUDE INI_CLR_DBC_PLC OF 'COLBMPLC.TXT'
           MOVE SPACES TO DBC-GLS-NOMC IN PLC-FLD.
*% INCLUDE FIN_CLR_DBC_PLC OF 'COLBMPLC.TXT'
       FIN-CLR-DBC-PLC.
           EXIT.
       
       CLR-ITR-PLC SECTION.
       INI-CLR-ITR-PLC.
*% INCLUDE INI_CLR_ITR_PLC OF 'COLBMPLC.TXT'
           MOVE ZEROES TO ITR-SGV-TINT IN PLC-FLD.
*% INCLUDE FIN_CLR_ITR_PLC OF 'COLBMPLC.TXT'
       FIN-CLR-ITR-PLC.
           EXIT.
       
       FST-KEY-PLC SECTION.
       INI-FST-KEY-PLC.
*% INCLUDE INI_FST_KEY_PLC OF 'COLBMPLC.TXT'
           MOVE -1 TO OPC-CAI-IOPC-LEN IN PLC-FLD.
*% INCLUDE FIN_FST_KEY_PLC OF 'COLBMPLC.TXT'
       FIN-FST-KEY-PLC.
           EXIT.
       
       FST-FLD-PLC SECTION.
       INI-FST-FLD-PLC.
*% INCLUDE INI_FST_FLD_PLC OF 'COLBMPLC.TXT'
*% INCLUDE FIN_FST_FLD_PLC OF 'COLBMPLC.TXT'
       FIN-FST-FLD-PLC.
           EXIT.
       
       PRO-KEY-PLC SECTION.
       INI-PRO-KEY-PLC.
*% INCLUDE INI_PRO_KEY_PLC OF 'COLBMPLC.TXT'
           PERFORM PRO-IKY-PLC.
           PERFORM PRO-AKY-PLC.
           PERFORM PRO-MKY-PLC.
*% INCLUDE FIN_PRO_KEY_PLC OF 'COLBMPLC.TXT'
       FIN-PRO-KEY-PLC.
           EXIT.
       
       PRO-IKY-PLC SECTION.
       INI-PRO-IKY-PLC.
*% INCLUDE INI_PRO_IKY_PLC OF 'COLBMPLC.TXT'
           MOVE FRM-CPIM-AEY TO OPC-CAI-IOPC-ATR IN PLC-FLD.
           MOVE FRM-CPIM-AEY TO OPC-IIC-IOPC-ATR IN PLC-FLD.
*% INCLUDE FIN_PRO_IKY_PLC OF 'COLBMPLC.TXT'
       FIN-PRO-IKY-PLC.
           EXIT.
       
       PRO-MKY-PLC SECTION.
       INI-PRO-MKY-PLC.
*% INCLUDE INI_PRO_MKY_PLC OF 'COLBMPLC.TXT'
*% INCLUDE FIN_PRO_MKY_PLC OF 'COLBMPLC.TXT'
       FIN-PRO-MKY-PLC.
           EXIT.
       
       PRO-AKY-PLC SECTION.
       INI-PRO-AKY-PLC.
*% INCLUDE INI_PRO_AKY_PLC OF 'COLBMPLC.TXT'
*% INCLUDE FIN_PRO_AKY_PLC OF 'COLBMPLC.TXT'
       FIN-PRO-AKY-PLC.
           EXIT.
       
       UNP-KEY-PLC SECTION.
       INI-UNP-KEY-PLC.
*% INCLUDE INI_UNP_KEY_PLC OF 'COLBMPLC.TXT'
           PERFORM UNP-IKY-PLC.
           PERFORM UNP-AKY-PLC.
           PERFORM UNP-MKY-PLC.
*% INCLUDE FIN_UNP_KEY_PLC OF 'COLBMPLC.TXT'
       FIN-UNP-KEY-PLC.
           EXIT.
       
       UNP-IKY-PLC SECTION.
       INI-UNP-IKY-PLC.
*% INCLUDE INI_UNP_IKY_PLC OF 'COLBMPLC.TXT'
           MOVE FRM-CPIM-UBY TO OPC-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO OPC-IIC-IOPC-ATR.
*% INCLUDE FIN_UNP_IKY_PLC OF 'COLBMPLC.TXT'
       FIN-UNP-IKY-PLC.
           EXIT.
       
       UNP-MKY-PLC SECTION.
       INI-UNP-MKY-PLC.
*% INCLUDE INI_UNP_MKY_PLC OF 'COLBMPLC.TXT'
*% INCLUDE FIN_UNP_MKY_PLC OF 'COLBMPLC.TXT'
       FIN-UNP-MKY-PLC.
           EXIT.
       
       UNP-AKY-PLC SECTION.
       INI-UNP-AKY-PLC.
*% INCLUDE INI_UNP_AKY_PLC OF 'COLBMPLC.TXT'
*% INCLUDE FIN_UNP_AKY_PLC OF 'COLBMPLC.TXT'
       FIN-UNP-AKY-PLC.
           EXIT.
       
       UNP-FLD-PLC SECTION.
       INI-UNP-FLD-PLC.
*% INCLUDE INI_UNP_FLD_PLC OF 'COLBMPLC.TXT'
           PERFORM UNP-OPC-PLC.
           PERFORM UNP-DBC-PLC.
           PERFORM UNP-ITR-PLC.
*% INCLUDE FIN_UNP_FLD_PLC OF 'COLBMPLC.TXT'
       FIN-UNP-FLD-PLC.
           EXIT.
       
       UNP-OPC-PLC SECTION.
       INI-UNP-OPC-PLC.
*% INCLUDE INI_UNP_OPC_PLC OF 'COLBMPLC.TXT'
*% INCLUDE FIN_UNP_OPC_PLC OF 'COLBMPLC.TXT'
       FIN-UNP-OPC-PLC.
           EXIT.
       
       UNP-DBC-PLC SECTION.
       INI-UNP-DBC-PLC.
*% INCLUDE INI_UNP_DBC_PLC OF 'COLBMPLC.TXT'
*% INCLUDE FIN_UNP_DBC_PLC OF 'COLBMPLC.TXT'
       FIN-UNP-DBC-PLC.
           EXIT.
       
       UNP-ITR-PLC SECTION.
       INI-UNP-ITR-PLC.
*% INCLUDE INI_UNP_ITR_PLC OF 'COLBMPLC.TXT'
*% INCLUDE FIN_UNP_ITR_PLC OF 'COLBMPLC.TXT'
       FIN-UNP-ITR-PLC.
           EXIT.
       
       VAL-KEY-PLC SECTION.
       INI-VAL-KEY-PLC.
           PERFORM VAL-IKY-PLC.
           PERFORM VAL-MKY-PLC.
           PERFORM VAL-AKY-PLC.
       FIN-VAL-KEY-PLC.
           EXIT.
       
       VAL-IKY-PLC SECTION.
       INI-VAL-IKY-PLC.
*% INCLUDE INI_VAL_IKY_PLC OF 'COLBMPLC.TXT'
           PERFORM VAL-OPC-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-CAI-IOPC-LEN IN PLC-FLD
                GO TO FIN-VAL-IKY-PLC.
           PERFORM VAL-OPC-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-IIC-IOPC-LEN IN PLC-FLD
                GO TO FIN-VAL-IKY-PLC.
*% INCLUDE FIN_VAL_IKY_PLC OF 'COLBMPLC.TXT'
       FIN-VAL-IKY-PLC.
           EXIT.
       
       VAL-MKY-PLC SECTION.
       INI-VAL-MKY-PLC.
*% INCLUDE INI_VAL_MKY_PLC OF 'COLBMPLC.TXT'
*% INCLUDE FIN_VAL_MKY_PLC OF 'COLBMPLC.TXT'
       FIN-VAL-MKY-PLC.
           EXIT.
       
       VAL-AKY-PLC SECTION.
       INI-VAL-AKY-PLC.
*% INCLUDE INI_VAL_AKY_PLC OF 'COLBMPLC.TXT'
*% INCLUDE FIN_VAL_AKY_PLC OF 'COLBMPLC.TXT'
       FIN-VAL-AKY-PLC.
           EXIT.
       
       VAL-OPC-CAI-IOPC SECTION.
       INI-VAL-OPC-CAI-IOPC.
*% LBL = "OPC_CAI_IOPC OF 'COLBMPLC.TXT'"
*% INCLUDE INI_OPC_CAI_IOPC OF 'COLBMPLC.TXT'
           IF OPC-CAI-IOPC IN PLC-FLD NOT > SPACES
              MOVE SPACES TO OPC-CAI-IOPC IN PLC-FLD.
*% INCLUDE OPC_CAI_IOPC OF 'COLBMPLC.TXT'
*% INCLUDE FIN_OPC_CAI_IOPC OF 'COLBMPLC.TXT'
       FIN-VAL-OPC-CAI-IOPC.
           EXIT.
       
       VAL-OPC-IIC-IOPC SECTION.
       INI-VAL-OPC-IIC-IOPC.
*% LBL = "OPC_IIC_IOPC OF 'COLBMPLC.TXT'"
*% INCLUDE INI_OPC_IIC_IOPC OF 'COLBMPLC.TXT'
*% INCLUDE OPC_IIC_IOPC OF 'COLBMPLC.TXT'
*% INCLUDE FIN_OPC_IIC_IOPC OF 'COLBMPLC.TXT'
       FIN-VAL-OPC-IIC-IOPC.
           EXIT.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
*% IZR = "S"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVEN"
*% IZR = "S"
       
       VAL-CON-KEY-PLC SECTION.
       INI-VAL-CON-KEY-PLC.
*% INCLUDE INI_VAL_CON_KEY_PLC OF 'COLBMPLC.TXT'
           IF     OPC-CAI-IOPC IN PLC-FLD NOT > SPACES 
              AND OPC-IIC-IOPC IN PLC-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-PLC.
*% INCLUDE FIN_VAL_CON_KEY_PLC OF 'COLBMPLC.TXT'
       FIN-VAL-CON-KEY-PLC.
           EXIT.
       
       VAL-CON-IKY-PLC SECTION.
       INI-VAL-CON-IKY-PLC.
*% INCLUDE INI_VAL_CON_IKY_PLC OF 'COLBMPLC.TXT'
           IF     OPC-CAI-IOPC IN PLC-FLD NOT > SPACES 
              AND OPC-IIC-IOPC IN PLC-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-PLC.
*% INCLUDE FIN_VAL_CON_IKY_PLC OF 'COLBMPLC.TXT'
       FIN-VAL-CON-IKY-PLC.
           EXIT.
       
       VAL-NUM-KEY-PLC SECTION.
       INI-VAL-NUM-KEY-PLC.
           PERFORM VAL-NUM-IKY-PLC.
           PERFORM VAL-NUM-MKY-PLC.
           PERFORM VAL-NUM-AKY-PLC.
       FIN-VAL-NUM-KEY-PLC.
           EXIT.
       
       VAL-NUM-IKY-PLC SECTION.
       INI-VAL-NUM-IKY-PLC.
*% INCLUDE INI_VAL_NUM_IKY_PLC OF 'COLBMPLC.TXT'
       VAL-NUM-OPC-IIC-IOPC.
*% BUF = "PLC-FLD"
*% FLD = "OPC-IIC-IOPC"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-PLC.
*% INCLUDE FIN_VAL_NUM_IKY_PLC OF 'COLBMPLC.TXT'
       FIN-VAL-NUM-IKY-PLC.
           EXIT.
       
       VAL-NUM-MKY-PLC SECTION.
       INI-VAL-NUM-MKY-PLC.
*% INCLUDE INI_VAL_NUM_MKY_PLC OF 'COLBMPLC.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_PLC OF 'COLBMPLC.TXT'
       FIN-VAL-NUM-MKY-PLC.
           EXIT.
       
       VAL-NUM-AKY-PLC SECTION.
       INI-VAL-NUM-AKY-PLC.
*% INCLUDE INI_VAL_NUM_AKY_PLC OF 'COLBMPLC.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_PLC OF 'COLBMPLC.TXT'
       FIN-VAL-NUM-AKY-PLC.
           EXIT.
       
       VAL-NUM-FLD-PLC SECTION.
       INI-VAL-NUM-FLD-PLC.
*% INCLUDE INI_VAL_NUM_FLD_PLC OF 'COLBMPLC.TXT'
       VAL-NUM-FRM-VAL-CRED.
*% BUF = "PLC-FLD"
*% FLD = "FRM-VAL-CRED"
*% ENT = "11"
*% DCM = "4"
