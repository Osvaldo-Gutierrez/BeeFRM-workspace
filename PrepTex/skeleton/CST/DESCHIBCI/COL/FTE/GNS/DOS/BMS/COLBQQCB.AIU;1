      *
      * COLBQQCB
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:48 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "QCB"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMQCB.TXT'
       SET-TAG-COLBQQCB SECTION.
       INI-SET-TAG-COLBQQCB.
           MOVE
           '<IDNMOD NOM=$COLBQQCB$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:48 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQQCB.
           EXIT.
           
       PUT-ALL-QCB SECTION.
       INI-PUT-ALL-QCB.
*% INCLUDE INI_PUT_ALL_QCB OF 'COLBMQCB.TXT'
           PERFORM PUT-CCB-QCB.
*% INCLUDE FIN_PUT_ALL_QCB OF 'COLBMQCB.TXT'
       FIN-PUT-ALL-QCB.
           EXIT.
       
       PUT-CCB-QCB SECTION.
       INI-PUT-CCB-QCB.
*% INCLUDE INI_PUT_CCB_QCB OF 'COLBMQCB.TXT'
           IF PGM-STAT-CCB-NUL
               GO TO FIN-PUT-CCB-QCB.
           IF NOT PGM-STAT-CCB-OKS
               PERFORM CLR-CCB-QCB
               GO TO FIN-PUT-CCB-QCB.
           MOVE CCB-CAI-IOPC IN CCB TO CCB-CAI-IOPC IN QCB-FLD.
           MOVE CCB-IIC-IOPC IN CCB TO CCB-IIC-IOPC IN QCB-FLD.
           MOVE CCB-NUM-IDLC IN CCB TO CCB-NUM-IDLC IN QCB-FLD.
*% INCLUDE FIN_PUT_CCB_QCB OF 'COLBMQCB.TXT'
       FIN-PUT-CCB-QCB.
           EXIT.
       
       KEY-ALL-QCB SECTION.
       INI-KEY-ALL-QCB.
*% INCLUDE INI_KEY_ALL_QCB OF 'COLBMQCB.TXT'
           PERFORM KEY-CCB-QCB.
*% INCLUDE FIN_KEY_ALL_QCB OF 'COLBMQCB.TXT'
       FIN-KEY-ALL-QCB.
           EXIT.
       
       KEY-CCB-QCB SECTION.
       INI-KEY-CCB-QCB.
*% INCLUDE INI_KEY_CCB_QCB OF 'COLBMQCB.TXT'
           MOVE CCB-CAI-IOPC IN QCB-FLD TO CCB-CAI-IOPC IN CCB.
           MOVE CCB-IIC-IOPC IN QCB-FLD TO CCB-IIC-IOPC IN CCB.
           MOVE CCB-NUM-IDLC IN QCB-FLD TO CCB-NUM-IDLC IN CCB.
*% INCLUDE FIN_KEY_CCB_QCB OF 'COLBMQCB.TXT'
       FIN-KEY-CCB-QCB.
           EXIT.
       
       SCR-CLRA-QCB SECTION.
       INI-SCR-CLRA-QCB.
*% INCLUDE INI_SCR_CLRA_QCB OF 'COLBMQCB.TXT'
           PERFORM CLR-KEY-QCB.
           PERFORM CLR-FLD-QCB.
*% INCLUDE FIN_SCR_CLRA_QCB OF 'COLBMQCB.TXT'
       FIN-SCR-CLRA-QCB.
           EXIT.
       
       CLR-KEY-QCB SECTION.
       INI-CLR-KEY-QCB.
*% INCLUDE INI_CLR_KEY_QCB OF 'COLBMQCB.TXT'
           MOVE SPACES TO CCB-CAI-IOPC IN QCB-FLD.
           MOVE ZEROES TO CCB-IIC-IOPC IN QCB-FLD.
           MOVE ZEROES TO CCB-NUM-IDLC IN QCB-FLD.
           MOVE SPACES TO FRM-IND-HMAS IN QCB-FLD.
           MOVE SPACES TO FRM-CAI-IOPC IN QCB-FLD.
           MOVE ZEROES TO FRM-IIC-IOPC IN QCB-FLD.
           MOVE ZEROES TO FRM-NUM-IDLC IN QCB-FLD.
           MOVE ZEROES TO FRM-VAL-ICCB IN QCB-FLD.
*% INCLUDE FIN_CLR_KEY_QCB OF 'COLBMQCB.TXT'
       FIN-CLR-KEY-QCB.
           EXIT.
       
       CLR-FLD-QCB SECTION.
       INI-CLR-FLD-QCB.
*% INCLUDE INI_CLR_FLD_QCB OF 'COLBMQCB.TXT'
           MOVE SPACES TO FRM-GLS-VCOC IN QCB-FLD.
           MOVE SPACES TO FRM-GLS-COOC IN QCB-FLD.
           MOVE SPACES TO FRM-GLS-OFOC IN QCB-FLD.
           MOVE ZEROES TO FRM-NUM-ICCB IN QCB-FLD(1).
           MOVE ZEROES TO FRM-NUM-ICCB IN QCB-FLD(2).
           MOVE ZEROES TO FRM-NUM-ICCB IN QCB-FLD(3).
           MOVE ZEROES TO FRM-NUM-ICCB IN QCB-FLD(4).
           MOVE ZEROES TO FRM-NUM-ICCB IN QCB-FLD(5).
           MOVE ZEROES TO FRM-NUM-ICCB IN QCB-FLD(6).
           MOVE ZEROES TO FRM-NUM-ICCB IN QCB-FLD(7).
           MOVE ZEROES TO FRM-NUM-ICCB IN QCB-FLD(8).
           MOVE ZEROES TO FRM-NUM-ICCB IN QCB-FLD(9).
           MOVE ZEROES TO FRM-NUM-ICCB IN QCB-FLD(10).
           MOVE ZEROES TO FRM-NUM-ICCB IN QCB-FLD(11).
           MOVE SPACES TO FRM-GLS-SCBA IN QCB-FLD(1).
           MOVE SPACES TO FRM-GLS-SCBA IN QCB-FLD(2).
           MOVE SPACES TO FRM-GLS-SCBA IN QCB-FLD(3).
           MOVE SPACES TO FRM-GLS-SCBA IN QCB-FLD(4).
           MOVE SPACES TO FRM-GLS-SCBA IN QCB-FLD(5).
           MOVE SPACES TO FRM-GLS-SCBA IN QCB-FLD(6).
           MOVE SPACES TO FRM-GLS-SCBA IN QCB-FLD(7).
           MOVE SPACES TO FRM-GLS-SCBA IN QCB-FLD(8).
           MOVE SPACES TO FRM-GLS-SCBA IN QCB-FLD(9).
           MOVE SPACES TO FRM-GLS-SCBA IN QCB-FLD(10).
           MOVE SPACES TO FRM-GLS-SCBA IN QCB-FLD(11).
           MOVE SPACES TO FRM-GLS-SCBZ IN QCB-FLD(1).
           MOVE SPACES TO FRM-GLS-SCBZ IN QCB-FLD(2).
           MOVE SPACES TO FRM-GLS-SCBZ IN QCB-FLD(3).
           MOVE SPACES TO FRM-GLS-SCBZ IN QCB-FLD(4).
           MOVE SPACES TO FRM-GLS-SCBZ IN QCB-FLD(5).
           MOVE SPACES TO FRM-GLS-SCBZ IN QCB-FLD(6).
           MOVE SPACES TO FRM-GLS-SCBZ IN QCB-FLD(7).
           MOVE SPACES TO FRM-GLS-SCBZ IN QCB-FLD(8).
           MOVE SPACES TO FRM-GLS-SCBZ IN QCB-FLD(9).
           MOVE SPACES TO FRM-GLS-SCBZ IN QCB-FLD(10).
           MOVE SPACES TO FRM-GLS-SCBZ IN QCB-FLD(11).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCCB"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FCCB IN QCB-FLD(1).
           MOVE ZEROES TO FRM-FEC-FCCB IN QCB-FLD(2).
           MOVE ZEROES TO FRM-FEC-FCCB IN QCB-FLD(3).
           MOVE ZEROES TO FRM-FEC-FCCB IN QCB-FLD(4).
           MOVE ZEROES TO FRM-FEC-FCCB IN QCB-FLD(5).
           MOVE ZEROES TO FRM-FEC-FCCB IN QCB-FLD(6).
           MOVE ZEROES TO FRM-FEC-FCCB IN QCB-FLD(7).
           MOVE ZEROES TO FRM-FEC-FCCB IN QCB-FLD(8).
           MOVE ZEROES TO FRM-FEC-FCCB IN QCB-FLD(9).
           MOVE ZEROES TO FRM-FEC-FCCB IN QCB-FLD(10).
           MOVE ZEROES TO FRM-FEC-FCCB IN QCB-FLD(11).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FING"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FING IN QCB-FLD(1).
           MOVE ZEROES TO FRM-FEC-FING IN QCB-FLD(2).
           MOVE ZEROES TO FRM-FEC-FING IN QCB-FLD(3).
           MOVE ZEROES TO FRM-FEC-FING IN QCB-FLD(4).
           MOVE ZEROES TO FRM-FEC-FING IN QCB-FLD(5).
           MOVE ZEROES TO FRM-FEC-FING IN QCB-FLD(6).
           MOVE ZEROES TO FRM-FEC-FING IN QCB-FLD(7).
           MOVE ZEROES TO FRM-FEC-FING IN QCB-FLD(8).
           MOVE ZEROES TO FRM-FEC-FING IN QCB-FLD(9).
           MOVE ZEROES TO FRM-FEC-FING IN QCB-FLD(10).
           MOVE ZEROES TO FRM-FEC-FING IN QCB-FLD(11).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-COD-RCCB IN QCB-FLD(1).
           MOVE SPACES TO FRM-COD-RCCB IN QCB-FLD(2).
           MOVE SPACES TO FRM-COD-RCCB IN QCB-FLD(3).
           MOVE SPACES TO FRM-COD-RCCB IN QCB-FLD(4).
           MOVE SPACES TO FRM-COD-RCCB IN QCB-FLD(5).
           MOVE SPACES TO FRM-COD-RCCB IN QCB-FLD(6).
           MOVE SPACES TO FRM-COD-RCCB IN QCB-FLD(7).
           MOVE SPACES TO FRM-COD-RCCB IN QCB-FLD(8).
           MOVE SPACES TO FRM-COD-RCCB IN QCB-FLD(9).
           MOVE SPACES TO FRM-COD-RCCB IN QCB-FLD(10).
           MOVE SPACES TO FRM-COD-RCCB IN QCB-FLD(11).
           PERFORM CLR-CCB-QCB.
*% INCLUDE FIN_CLR_FLD_QCB OF 'COLBMQCB.TXT'
       FIN-CLR-FLD-QCB.
           EXIT.
       
       CLR-CCB-QCB SECTION.
       INI-CLR-CCB-QCB.
*% INCLUDE INI_CLR_CCB_QCB OF 'COLBMQCB.TXT'
*% INCLUDE FIN_CLR_CCB_QCB OF 'COLBMQCB.TXT'
       FIN-CLR-CCB-QCB.
           EXIT.
       
       FST-KEY-QCB SECTION.
       INI-FST-KEY-QCB.
*% INCLUDE INI_FST_KEY_QCB OF 'COLBMQCB.TXT'
           MOVE -1 TO CCB-CAI-IOPC-LEN IN QCB-FLD.
*% INCLUDE FIN_FST_KEY_QCB OF 'COLBMQCB.TXT'
       FIN-FST-KEY-QCB.
           EXIT.
       
       FST-FLD-QCB SECTION.
       INI-FST-FLD-QCB.
*% INCLUDE INI_FST_FLD_QCB OF 'COLBMQCB.TXT'
*% INCLUDE FIN_FST_FLD_QCB OF 'COLBMQCB.TXT'
       FIN-FST-FLD-QCB.
           EXIT.
       
       PRO-KEY-QCB SECTION.
       INI-PRO-KEY-QCB.
*% INCLUDE INI_PRO_KEY_QCB OF 'COLBMQCB.TXT'
           MOVE FRM-CPIM-AEY TO FRM-IND-HMAS-ATR IN QCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-CAI-IOPC-ATR IN QCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IIC-IOPC-ATR IN QCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-NUM-IDLC-ATR IN QCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VAL-ICCB-ATR IN QCB-FLD.
           PERFORM PRO-IKY-QCB.
           PERFORM PRO-AKY-QCB.
           PERFORM PRO-MKY-QCB.
*% INCLUDE FIN_PRO_KEY_QCB OF 'COLBMQCB.TXT'
       FIN-PRO-KEY-QCB.
           EXIT.
       
       PRO-IKY-QCB SECTION.
       INI-PRO-IKY-QCB.
*% INCLUDE INI_PRO_IKY_QCB OF 'COLBMQCB.TXT'
           MOVE FRM-CPIM-AEY TO CCB-CAI-IOPC-ATR IN QCB-FLD.
           MOVE FRM-CPIM-AEY TO CCB-IIC-IOPC-ATR IN QCB-FLD.
           MOVE FRM-CPIM-AEY TO CCB-NUM-IDLC-ATR IN QCB-FLD.
*% INCLUDE FIN_PRO_IKY_QCB OF 'COLBMQCB.TXT'
       FIN-PRO-IKY-QCB.
           EXIT.
       
       PRO-MKY-QCB SECTION.
       INI-PRO-MKY-QCB.
*% INCLUDE INI_PRO_MKY_QCB OF 'COLBMQCB.TXT'
*% INCLUDE FIN_PRO_MKY_QCB OF 'COLBMQCB.TXT'
       FIN-PRO-MKY-QCB.
           EXIT.
       
       PRO-AKY-QCB SECTION.
       INI-PRO-AKY-QCB.
*% INCLUDE INI_PRO_AKY_QCB OF 'COLBMQCB.TXT'
*% INCLUDE FIN_PRO_AKY_QCB OF 'COLBMQCB.TXT'
       FIN-PRO-AKY-QCB.
           EXIT.
       
       UNP-KEY-QCB SECTION.
       INI-UNP-KEY-QCB.
*% INCLUDE INI_UNP_KEY_QCB OF 'COLBMQCB.TXT'
           MOVE FRM-CPIM-UBY TO FRM-IND-HMAS-ATR IN QCB-FLD.
           MOVE FRM-CPIM-UBY TO FRM-CAI-IOPC-ATR IN QCB-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IIC-IOPC-ATR IN QCB-FLD.
           MOVE FRM-CPIM-UBY TO FRM-NUM-IDLC-ATR IN QCB-FLD.
           MOVE FRM-CPIM-UBY TO FRM-VAL-ICCB-ATR IN QCB-FLD.
           PERFORM UNP-IKY-QCB.
           PERFORM UNP-AKY-QCB.
           PERFORM UNP-MKY-QCB.
*% INCLUDE FIN_UNP_KEY_QCB OF 'COLBMQCB.TXT'
       FIN-UNP-KEY-QCB.
           EXIT.
       
       UNP-IKY-QCB SECTION.
       INI-UNP-IKY-QCB.
*% INCLUDE INI_UNP_IKY_QCB OF 'COLBMQCB.TXT'
           MOVE FRM-CPIM-UBY TO CCB-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO CCB-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO CCB-NUM-IDLC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-IND-HMAS-ATR.
           MOVE FRM-CPIM-UBY TO FRM-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-NUM-IDLC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-VAL-ICCB-ATR.
*% INCLUDE FIN_UNP_IKY_QCB OF 'COLBMQCB.TXT'
       FIN-UNP-IKY-QCB.
           EXIT.
       
       UNP-MKY-QCB SECTION.
       INI-UNP-MKY-QCB.
*% INCLUDE INI_UNP_MKY_QCB OF 'COLBMQCB.TXT'
*% INCLUDE FIN_UNP_MKY_QCB OF 'COLBMQCB.TXT'
       FIN-UNP-MKY-QCB.
           EXIT.
       
       UNP-AKY-QCB SECTION.
       INI-UNP-AKY-QCB.
*% INCLUDE INI_UNP_AKY_QCB OF 'COLBMQCB.TXT'
*% INCLUDE FIN_UNP_AKY_QCB OF 'COLBMQCB.TXT'
       FIN-UNP-AKY-QCB.
           EXIT.
       
       UNP-FLD-QCB SECTION.
       INI-UNP-FLD-QCB.
*% INCLUDE INI_UNP_FLD_QCB OF 'COLBMQCB.TXT'
           PERFORM UNP-CCB-QCB.
*% INCLUDE FIN_UNP_FLD_QCB OF 'COLBMQCB.TXT'
       FIN-UNP-FLD-QCB.
           EXIT.
       
       UNP-CCB-QCB SECTION.
       INI-UNP-CCB-QCB.
*% INCLUDE INI_UNP_CCB_QCB OF 'COLBMQCB.TXT'
*% INCLUDE FIN_UNP_CCB_QCB OF 'COLBMQCB.TXT'
       FIN-UNP-CCB-QCB.
           EXIT.
       
       VAL-KEY-QCB SECTION.
       INI-VAL-KEY-QCB.
           PERFORM VAL-IKY-QCB.
           PERFORM VAL-MKY-QCB.
           PERFORM VAL-AKY-QCB.
       FIN-VAL-KEY-QCB.
           EXIT.
       
       VAL-IKY-QCB SECTION.
       INI-VAL-IKY-QCB.
*% INCLUDE INI_VAL_IKY_QCB OF 'COLBMQCB.TXT'
           PERFORM VAL-CCB-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CCB-CAI-IOPC-LEN IN QCB-FLD
                GO TO FIN-VAL-IKY-QCB.
           PERFORM VAL-CCB-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CCB-IIC-IOPC-LEN IN QCB-FLD
                GO TO FIN-VAL-IKY-QCB.
           PERFORM VAL-CCB-NUM-IDLC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CCB-NUM-IDLC-LEN IN QCB-FLD
                GO TO FIN-VAL-IKY-QCB.
           PERFORM VAL-FRM-IND-HMAS.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IND-HMAS-LEN IN QCB-FLD
                GO TO FIN-VAL-IKY-QCB.
           PERFORM VAL-FRM-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-IOPC-LEN IN QCB-FLD
                GO TO FIN-VAL-IKY-QCB.
           PERFORM VAL-FRM-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-IOPC-LEN IN QCB-FLD
                GO TO FIN-VAL-IKY-QCB.
           PERFORM VAL-FRM-NUM-IDLC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-NUM-IDLC-LEN IN QCB-FLD
                GO TO FIN-VAL-IKY-QCB.
           PERFORM VAL-FRM-VAL-ICCB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-ICCB-LEN IN QCB-FLD
                GO TO FIN-VAL-IKY-QCB.
*% INCLUDE FIN_VAL_IKY_QCB OF 'COLBMQCB.TXT'
       FIN-VAL-IKY-QCB.
           EXIT.
       
       VAL-MKY-QCB SECTION.
       INI-VAL-MKY-QCB.
*% INCLUDE INI_VAL_MKY_QCB OF 'COLBMQCB.TXT'
*% INCLUDE FIN_VAL_MKY_QCB OF 'COLBMQCB.TXT'
       FIN-VAL-MKY-QCB.
           EXIT.
       
       VAL-AKY-QCB SECTION.
       INI-VAL-AKY-QCB.
*% INCLUDE INI_VAL_AKY_QCB OF 'COLBMQCB.TXT'
*% INCLUDE FIN_VAL_AKY_QCB OF 'COLBMQCB.TXT'
       FIN-VAL-AKY-QCB.
           EXIT.
       
       VAL-CCB-CAI-IOPC SECTION.
       INI-VAL-CCB-CAI-IOPC.
*% LBL = "CCB_CAI_IOPC OF 'COLBMQCB.TXT'"
*% INCLUDE INI_CCB_CAI_IOPC OF 'COLBMQCB.TXT'
           IF CCB-CAI-IOPC IN QCB-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-CCB-CAI-IOPC.
*% INCLUDE CCB_CAI_IOPC OF 'COLBMQCB.TXT'
*% INCLUDE FIN_CCB_CAI_IOPC OF 'COLBMQCB.TXT'
       FIN-VAL-CCB-CAI-IOPC.
           EXIT.
       
       VAL-CCB-IIC-IOPC SECTION.
       INI-VAL-CCB-IIC-IOPC.
*% LBL = "CCB_IIC_IOPC OF 'COLBMQCB.TXT'"
*% INCLUDE INI_CCB_IIC_IOPC OF 'COLBMQCB.TXT'
           IF CCB-IIC-IOPC IN QCB-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-CCB-IIC-IOPC.
*% INCLUDE CCB_IIC_IOPC OF 'COLBMQCB.TXT'
*% INCLUDE FIN_CCB_IIC_IOPC OF 'COLBMQCB.TXT'
       FIN-VAL-CCB-IIC-IOPC.
           EXIT.
       
       VAL-CCB-NUM-IDLC SECTION.
       INI-VAL-CCB-NUM-IDLC.
*% LBL = "CCB_NUM_IDLC OF 'COLBMQCB.TXT'"
*% INCLUDE INI_CCB_NUM_IDLC OF 'COLBMQCB.TXT'
           IF CCB-NUM-IDLC IN QCB-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-CCB-NUM-IDLC.
*% INCLUDE CCB_NUM_IDLC OF 'COLBMQCB.TXT'
*% INCLUDE FIN_CCB_NUM_IDLC OF 'COLBMQCB.TXT'
       FIN-VAL-CCB-NUM-IDLC.
           EXIT.
       
       VAL-FRM-IND-HMAS SECTION.
       INI-VAL-FRM-IND-HMAS.
*% LBL = "FRM_IND_HMAS OF 'COLBMQCB.TXT'"
*% INCLUDE INI_FRM_IND_HMAS OF 'COLBMQCB.TXT'
           IF FRM-IND-HMAS IN QCB-FLD NOT > SPACES
              MOVE SPACES TO FRM-IND-HMAS IN QCB-FLD.
*% INCLUDE FRM_IND_HMAS OF 'COLBMQCB.TXT'
*% INCLUDE FIN_FRM_IND_HMAS OF 'COLBMQCB.TXT'
       FIN-VAL-FRM-IND-HMAS.
           EXIT.
       
       VAL-FRM-CAI-IOPC SECTION.
       INI-VAL-FRM-CAI-IOPC.
*% LBL = "FRM_CAI_IOPC OF 'COLBMQCB.TXT'"
*% INCLUDE INI_FRM_CAI_IOPC OF 'COLBMQCB.TXT'
           IF FRM-CAI-IOPC IN QCB-FLD NOT > SPACES
              MOVE SPACES TO FRM-CAI-IOPC IN QCB-FLD.
*% INCLUDE FRM_CAI_IOPC OF 'COLBMQCB.TXT'
*% INCLUDE FIN_FRM_CAI_IOPC OF 'COLBMQCB.TXT'
       FIN-VAL-FRM-CAI-IOPC.
           EXIT.
       
       VAL-FRM-IIC-IOPC SECTION.
       INI-VAL-FRM-IIC-IOPC.
*% LBL = "FRM_IIC_IOPC OF 'COLBMQCB.TXT'"
*% INCLUDE INI_FRM_IIC_IOPC OF 'COLBMQCB.TXT'
*% INCLUDE FRM_IIC_IOPC OF 'COLBMQCB.TXT'
*% INCLUDE FIN_FRM_IIC_IOPC OF 'COLBMQCB.TXT'
       FIN-VAL-FRM-IIC-IOPC.
           EXIT.
       
       VAL-FRM-NUM-IDLC SECTION.
       INI-VAL-FRM-NUM-IDLC.
*% LBL = "FRM_NUM_IDLC OF 'COLBMQCB.TXT'"
*% INCLUDE INI_FRM_NUM_IDLC OF 'COLBMQCB.TXT'
*% INCLUDE FRM_NUM_IDLC OF 'COLBMQCB.TXT'
*% INCLUDE FIN_FRM_NUM_IDLC OF 'COLBMQCB.TXT'
       FIN-VAL-FRM-NUM-IDLC.
           EXIT.
       
       VAL-FRM-VAL-ICCB SECTION.
       INI-VAL-FRM-VAL-ICCB.
*% LBL = "FRM_VAL_ICCB OF 'COLBMQCB.TXT'"
*% INCLUDE INI_FRM_VAL_ICCB OF 'COLBMQCB.TXT'
*% INCLUDE FRM_VAL_ICCB OF 'COLBMQCB.TXT'
*% INCLUDE FIN_FRM_VAL_ICCB OF 'COLBMQCB.TXT'
       FIN-VAL-FRM-VAL-ICCB.
           EXIT.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCCB"
*% IZR = "S"
*% RREG = "FRM"
