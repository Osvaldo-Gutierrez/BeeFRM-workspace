      *
      * COLBQCCB
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 03:20 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CCB"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMCCB.TXT'
       SET-TAG-COLBQCCB SECTION.
       INI-SET-TAG-COLBQCCB.
           MOVE
           '<IDNMOD NOM=$COLBQCCB$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 03:20 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCCB.
           EXIT.
           
       PUT-ALL-CCB SECTION.
       INI-PUT-ALL-CCB.
*% INCLUDE INI_PUT_ALL_CCB OF 'COLBMCCB.TXT'
           PERFORM PUT-OPC-CCB.
           PERFORM PUT-CCB-CCB.
           PERFORM PUT-DLC-CCB.
*% INCLUDE FIN_PUT_ALL_CCB OF 'COLBMCCB.TXT'
       FIN-PUT-ALL-CCB.
           EXIT.
       
       PUT-OPC-CCB SECTION.
       INI-PUT-OPC-CCB.
*% INCLUDE INI_PUT_OPC_CCB OF 'COLBMCCB.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-CCB.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-CCB
               GO TO FIN-PUT-OPC-CCB.
           MOVE OPC-MSC-TVOC IN OPC TO OPC-MSC-TVOC IN CCB-FLD.
           MOVE OPC-COD-TOOC IN OPC TO OPC-COD-TOOC IN CCB-FLD.
           MOVE OPC-COD-OFOC IN OPC TO OPC-COD-OFOC IN CCB-FLD.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE OPC-NUM-DCOL IN OPC TO OPC-NUM-DCOL IN CCB-FLD.
           MOVE OPC-NUM-MCOL IN OPC TO OPC-NUM-MCOL IN CCB-FLD.
           MOVE OPC-NUM-SCOL IN OPC TO OPC-NUM-SCOL IN CCB-FLD.
           MOVE OPC-NUM-ACOL IN OPC TO OPC-NUM-ACOL IN CCB-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE OPC-COD-VCOC IN OPC TO OPC-COD-VCOC IN CCB-FLD.
*% BUF = "CCB-FLD"
*% SIS = "TAB"
*% TAB = "TVAL"
*% FLD = "OPC-MSC-TVOC"
*% GLS = "FRM-GLS-TVOC"
*% TIENE_GLOSA = TRUE
*% LGL = 13
*% INCLUDE 'BUSMSC'
*% BUF = "CCB-FLD"
*% SIS = "TAB"
*% TAB = "TIO "
*% FLD = "OPC-COD-TOOC"
*% GLS = "FRM-GLS-TOOC"
*% TIENE_GLOSA = TRUE
*% LGL = 15
*% INCLUDE 'BUSTAB'
*% BUF = "CCB-FLD"
*% SIS = "TAB"
*% TAB = "OFI "
*% FLD = "OPC-COD-OFOC"
*% GLS = "FRM-GLS-OFOC"
*% TIENE_GLOSA = TRUE
*% LGL = 13
*% INCLUDE 'BUSOFI          '
*% BUF = "CCB-FLD"
*% SIS = "TAB"
*% TAB = "VLR "
*% FLD = "OPC-COD-VCOC"
*% GLS = "FRM-GLS-VCOC"
*% TIENE_GLOSA = TRUE
*% LGL = 29
*% INCLUDE 'BUSTAB'
*% INCLUDE FIN_PUT_OPC_CCB OF 'COLBMCCB.TXT'
       FIN-PUT-OPC-CCB.
           EXIT.
       
       PUT-CCB-CCB SECTION.
       INI-PUT-CCB-CCB.
*% INCLUDE INI_PUT_CCB_CCB OF 'COLBMCCB.TXT'
           IF PGM-STAT-CCB-NUL
               GO TO FIN-PUT-CCB-CCB.
           IF NOT PGM-STAT-CCB-OKS
               PERFORM CLR-CCB-CCB
               GO TO FIN-PUT-CCB-CCB.
           MOVE CCB-CAI-IOPC IN CCB TO CCB-CAI-IOPC IN CCB-FLD.
           MOVE CCB-IIC-IOPC IN CCB TO CCB-IIC-IOPC IN CCB-FLD.
           MOVE CCB-NUM-IDLC IN CCB TO CCB-NUM-IDLC IN CCB-FLD.
           MOVE CCB-NUM-ICCB IN CCB TO CCB-NUM-ICCB IN CCB-FLD.
           MOVE CCB-IND-SCBA IN CCB TO CCB-IND-SCBA IN CCB-FLD.
*% RREG = "CCB"
*% RREG_FRM = "CCB"
*% NOMB = "FCCB"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CCB-NUM-DCCB IN CCB TO CCB-NUM-DCCB IN CCB-FLD.
           MOVE CCB-NUM-MCCB IN CCB TO CCB-NUM-MCCB IN CCB-FLD.
           MOVE CCB-NUM-SCCB IN CCB TO CCB-NUM-SCCB IN CCB-FLD.
           MOVE CCB-NUM-ACCB IN CCB TO CCB-NUM-ACCB IN CCB-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% RREG = "CCB"
*% RREG_FRM = "CCB"
*% NOMB = "FING"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CCB-NUM-DING IN CCB TO CCB-NUM-DING IN CCB-FLD.
           MOVE CCB-NUM-MING IN CCB TO CCB-NUM-MING IN CCB-FLD.
           MOVE CCB-NUM-SING IN CCB TO CCB-NUM-SING IN CCB-FLD.
           MOVE CCB-NUM-AING IN CCB TO CCB-NUM-AING IN CCB-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE CCB-IND-SCBZ IN CCB TO CCB-IND-SCBZ IN CCB-FLD.
           MOVE CCB-COD-RCCB IN CCB TO CCB-COD-RCCB IN CCB-FLD.
           MOVE CCB-SGV-GAST IN CCB TO CCB-SGV-GAST IN CCB-FLD.
*% BUF = "CCB-FLD"
*% SIS = "COL"
*% TAB = "SCBZ"
*% FLD = "CCB-IND-SCBA"
*% GLS = "FRM-GLS-SCBA"
*% TIENE_GLOSA = TRUE
*% LGL = 37
*% INCLUDE 'BUSMSC'
*% BUF = "CCB-FLD"
*% SIS = "COL"
*% TAB = "SCBZ"
*% FLD = "CCB-IND-SCBZ"
*% GLS = "FRM-GLS-SCBZ"
*% TIENE_GLOSA = TRUE
*% LGL = 37
*% INCLUDE 'BUSMSC'
*% BUF = "CCB-FLD"
*% SIS = "TAB"
*% TAB = "USR "
*% FLD = "CCB-COD-RCCB"
*% GLS = "FRM-GLS-RCCB"
*% TIENE_GLOSA = TRUE
*% LGL = 29
*% INCLUDE 'BUSUSR          '
*% INCLUDE FIN_PUT_CCB_CCB OF 'COLBMCCB.TXT'
       FIN-PUT-CCB-CCB.
           EXIT.
       
       PUT-DLC-CCB SECTION.
       INI-PUT-DLC-CCB.
*% INCLUDE INI_PUT_DLC_CCB OF 'COLBMCCB.TXT'
           IF PGM-STAT-DLC-NUL
               GO TO FIN-PUT-DLC-CCB.
           IF NOT PGM-STAT-DLC-OKS
               PERFORM CLR-DLC-CCB
               GO TO FIN-PUT-DLC-CCB.
           MOVE DLC-VAL-SDLC IN DLC TO DLC-VAL-SDLC IN CCB-FLD.
           MOVE DLC-COD-UBFD IN DLC TO DLC-COD-UBFD IN CCB-FLD.
           MOVE DLC-IND-SCTB IN DLC TO DLC-IND-SCTB IN CCB-FLD.
           MOVE DLC-IND-SCBZ IN DLC TO DLC-IND-SCBZ IN CCB-FLD.
*% BUF = "CCB-FLD"
*% SIS = "COL"
*% TAB = "SCTB"
*% FLD = "DLC-IND-SCTB"
*% GLS = "FRM-GLS-SCTB"
*% TIENE_GLOSA = TRUE
*% LGL = 29
*% INCLUDE 'BUSMSC'
*% BUF = "CCB-FLD"
*% SIS = "COL"
*% TAB = "SCBZ"
*% FLD = "DLC-IND-SCBZ"
*% GLS = "FRM-GLS-SCBZ"
*% TIENE_GLOSA = TRUE
*% LGL = 37
*% INCLUDE 'BUSMSC'
*% INCLUDE FIN_PUT_DLC_CCB OF 'COLBMCCB.TXT'
       FIN-PUT-DLC-CCB.
           EXIT.
       
       KEY-ALL-CCB SECTION.
       INI-KEY-ALL-CCB.
*% INCLUDE INI_KEY_ALL_CCB OF 'COLBMCCB.TXT'
           PERFORM KEY-OPC-CCB.
           PERFORM KEY-CCB-CCB.
           PERFORM KEY-DLC-CCB.
*% INCLUDE FIN_KEY_ALL_CCB OF 'COLBMCCB.TXT'
       FIN-KEY-ALL-CCB.
           EXIT.
       
       KEY-OPC-CCB SECTION.
       INI-KEY-OPC-CCB.
*% INCLUDE INI_KEY_OPC_CCB OF 'COLBMCCB.TXT'
*% INCLUDE FIN_KEY_OPC_CCB OF 'COLBMCCB.TXT'
       FIN-KEY-OPC-CCB.
           EXIT.
       
       KEY-CCB-CCB SECTION.
       INI-KEY-CCB-CCB.
*% INCLUDE INI_KEY_CCB_CCB OF 'COLBMCCB.TXT'
           MOVE CCB-CAI-IOPC IN CCB-FLD TO CCB-CAI-IOPC IN CCB.
           MOVE CCB-IIC-IOPC IN CCB-FLD TO CCB-IIC-IOPC IN CCB.
           MOVE CCB-NUM-IDLC IN CCB-FLD TO CCB-NUM-IDLC IN CCB.
           MOVE CCB-NUM-ICCB IN CCB-FLD TO CCB-NUM-ICCB IN CCB.
*% INCLUDE FIN_KEY_CCB_CCB OF 'COLBMCCB.TXT'
       FIN-KEY-CCB-CCB.
           EXIT.
       
       KEY-DLC-CCB SECTION.
       INI-KEY-DLC-CCB.
*% INCLUDE INI_KEY_DLC_CCB OF 'COLBMCCB.TXT'
*% INCLUDE FIN_KEY_DLC_CCB OF 'COLBMCCB.TXT'
       FIN-KEY-DLC-CCB.
           EXIT.
       
       SCR-CLRA-CCB SECTION.
       INI-SCR-CLRA-CCB.
*% INCLUDE INI_SCR_CLRA_CCB OF 'COLBMCCB.TXT'
           PERFORM CLR-KEY-CCB.
           PERFORM CLR-FLD-CCB.
*% INCLUDE FIN_SCR_CLRA_CCB OF 'COLBMCCB.TXT'
       FIN-SCR-CLRA-CCB.
           EXIT.
       
       CLR-KEY-CCB SECTION.
       INI-CLR-KEY-CCB.
*% INCLUDE INI_CLR_KEY_CCB OF 'COLBMCCB.TXT'
           MOVE SPACES TO CCB-CAI-IOPC IN CCB-FLD.
           MOVE ZEROES TO CCB-IIC-IOPC IN CCB-FLD.
           MOVE ZEROES TO CCB-NUM-IDLC IN CCB-FLD.
           MOVE ZEROES TO CCB-NUM-ICCB IN CCB-FLD.
           MOVE SPACES TO FRM-GLS-NOMC IN CCB-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-ICLI IN CCB-FLD.
           MOVE SPACES TO FRM-VRF-ICLI IN CCB-FLD.
           MOVE SPACES TO FRM-IND-ICLI IN CCB-FLD.
           MOVE SPACES TO FRM-GLS-ICLI IN CCB-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-CAI-ICLI IN CCB-FLD.
           MOVE SPACES TO FRM-IIC-ICLI IN CCB-FLD.
           MOVE SPACES TO FRM-GLS-NOMA IN CCB-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-IAVA IN CCB-FLD.
           MOVE SPACES TO FRM-VRF-IAVA IN CCB-FLD.
           MOVE SPACES TO FRM-IND-IAVA IN CCB-FLD.
           MOVE SPACES TO FRM-GLS-IAVA IN CCB-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-CAI-IAVA IN CCB-FLD.
           MOVE SPACES TO FRM-IIC-IAVA IN CCB-FLD.
*% INCLUDE FIN_CLR_KEY_CCB OF 'COLBMCCB.TXT'
       FIN-CLR-KEY-CCB.
           EXIT.
       
       CLR-FLD-CCB SECTION.
       INI-CLR-FLD-CCB.
*% INCLUDE INI_CLR_FLD_CCB OF 'COLBMCCB.TXT'
           MOVE SPACES TO FRM-GLS-TVOC IN CCB-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN CCB-FLD.
           MOVE SPACES TO FRM-GLS-OFOC IN CCB-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN CCB-FLD.
           MOVE SPACES TO FRM-GLS-UBFD IN CCB-FLD.
           MOVE SPACES TO FRM-GLS-SCTB IN CCB-FLD.
           MOVE SPACES TO FRM-GLS-SCBA IN CCB-FLD.
           MOVE SPACES TO FRM-GLS-SCBZ IN CCB-FLD.
           MOVE SPACES TO FRM-GLS-RCCB IN CCB-FLD.
           PERFORM CLR-OPC-CCB.
           PERFORM CLR-CCB-CCB.
           PERFORM CLR-DLC-CCB.
*% INCLUDE FIN_CLR_FLD_CCB OF 'COLBMCCB.TXT'
       FIN-CLR-FLD-CCB.
           EXIT.
       
       CLR-OPC-CCB SECTION.
       INI-CLR-OPC-CCB.
*% INCLUDE INI_CLR_OPC_CCB OF 'COLBMCCB.TXT'
           MOVE SPACES TO OPC-MSC-TVOC IN CCB-FLD.
           MOVE SPACES TO OPC-COD-TOOC IN CCB-FLD.
           MOVE SPACES TO OPC-COD-OFOC IN CCB-FLD.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO OPC-FEC-FCOL IN CCB-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO OPC-COD-VCOC IN CCB-FLD.
*% INCLUDE FIN_CLR_OPC_CCB OF 'COLBMCCB.TXT'
       FIN-CLR-OPC-CCB.
           EXIT.
       
       CLR-CCB-CCB SECTION.
       INI-CLR-CCB-CCB.
*% INCLUDE INI_CLR_CCB_CCB OF 'COLBMCCB.TXT'
           MOVE SPACES TO CCB-IND-SCBA IN CCB-FLD.
*% RREG = "CCB"
*% RREG_FRM = "CCB"
*% NOMB = "FCCB"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CCB-FEC-FCCB IN CCB-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "CCB"
*% RREG_FRM = "CCB"
*% NOMB = "FING"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CCB-FEC-FING IN CCB-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO CCB-IND-SCBZ IN CCB-FLD.
           MOVE SPACES TO CCB-COD-RCCB IN CCB-FLD.
           MOVE ZEROES TO CCB-SGV-GAST IN CCB-FLD.
*% INCLUDE FIN_CLR_CCB_CCB OF 'COLBMCCB.TXT'
       FIN-CLR-CCB-CCB.
           EXIT.
       
       CLR-DLC-CCB SECTION.
       INI-CLR-DLC-CCB.
*% INCLUDE INI_CLR_DLC_CCB OF 'COLBMCCB.TXT'
           MOVE ZEROES TO DLC-VAL-SDLC IN CCB-FLD.
           MOVE SPACES TO DLC-COD-UBFD IN CCB-FLD.
           MOVE SPACES TO DLC-IND-SCTB IN CCB-FLD.
           MOVE SPACES TO DLC-IND-SCBZ IN CCB-FLD.
*% INCLUDE FIN_CLR_DLC_CCB OF 'COLBMCCB.TXT'
       FIN-CLR-DLC-CCB.
           EXIT.
       
       FST-KEY-CCB SECTION.
       INI-FST-KEY-CCB.
*% INCLUDE INI_FST_KEY_CCB OF 'COLBMCCB.TXT'
           MOVE -1 TO CCB-CAI-IOPC-LEN IN CCB-FLD.
*% INCLUDE FIN_FST_KEY_CCB OF 'COLBMCCB.TXT'
       FIN-FST-KEY-CCB.
           EXIT.
       
       FST-FLD-CCB SECTION.
       INI-FST-FLD-CCB.
*% INCLUDE INI_FST_FLD_CCB OF 'COLBMCCB.TXT'
*% RREG = "CCB"
*% RREG_FRM = "CCB"
*% NOMB = "FCCB"
           MOVE FRM-POS-CURS TO CCB-FEC-FCCB-LEN IN CCB-FLD.
*% INCLUDE FIN_FST_FLD_CCB OF 'COLBMCCB.TXT'
       FIN-FST-FLD-CCB.
           EXIT.
       
       PRO-KEY-CCB SECTION.
       INI-PRO-KEY-CCB.
*% INCLUDE INI_PRO_KEY_CCB OF 'COLBMCCB.TXT'
           MOVE FRM-CPIM-AEY TO FRM-GLS-NOMC-ATR IN CCB-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-NUM-ICLI-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VRF-ICLI-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-ICLI-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-ICLI-ATR IN CCB-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-GLS-NOMA-ATR IN CCB-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-NUM-IAVA-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VRF-IAVA-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-IAVA-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-IAVA-ATR IN CCB-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           PERFORM PRO-IKY-CCB.
           PERFORM PRO-AKY-CCB.
           PERFORM PRO-MKY-CCB.
*% INCLUDE FIN_PRO_KEY_CCB OF 'COLBMCCB.TXT'
       FIN-PRO-KEY-CCB.
           EXIT.
       
       PRO-IKY-CCB SECTION.
       INI-PRO-IKY-CCB.
*% INCLUDE INI_PRO_IKY_CCB OF 'COLBMCCB.TXT'
           MOVE FRM-CPIM-AEY TO CCB-CAI-IOPC-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO CCB-IIC-IOPC-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO CCB-NUM-IDLC-ATR IN CCB-FLD.
*% INCLUDE FIN_PRO_IKY_CCB OF 'COLBMCCB.TXT'
       FIN-PRO-IKY-CCB.
           EXIT.
       
       PRO-MKY-CCB SECTION.
       INI-PRO-MKY-CCB.
*% INCLUDE INI_PRO_MKY_CCB OF 'COLBMCCB.TXT'
*% INCLUDE FIN_PRO_MKY_CCB OF 'COLBMCCB.TXT'
       FIN-PRO-MKY-CCB.
           EXIT.
       
       PRO-AKY-CCB SECTION.
       INI-PRO-AKY-CCB.
*% INCLUDE INI_PRO_AKY_CCB OF 'COLBMCCB.TXT'
           MOVE FRM-CPIM-AEY TO CCB-NUM-ICCB-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-NOMC-ATR IN CCB-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-NUM-ICLI-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VRF-ICLI-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-ICLI-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-ICLI-ATR IN CCB-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-GLS-NOMA-ATR IN CCB-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-NUM-IAVA-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VRF-IAVA-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-IAVA-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-IAVA-ATR IN CCB-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
*% INCLUDE FIN_PRO_AKY_CCB OF 'COLBMCCB.TXT'
       FIN-PRO-AKY-CCB.
           EXIT.
       
       UNP-KEY-CCB SECTION.
       INI-UNP-KEY-CCB.
*% INCLUDE INI_UNP_KEY_CCB OF 'COLBMCCB.TXT'
           MOVE FRM-CPIM-UBY TO FRM-GLS-NOMC-ATR IN CCB-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO FRM-NUM-ICLI-ATR IN CCB-FLD.
           MOVE FRM-CPIM-UBY TO FRM-VRF-ICLI-ATR IN CCB-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IND-ICLI-ATR IN CCB-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-ICLI-ATR IN CCB-FLD.
      * IDC-FIN --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO FRM-GLS-NOMA-ATR IN CCB-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
      * IDC-INI --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO FRM-NUM-IAVA-ATR IN CCB-FLD.
           MOVE FRM-CPIM-UBY TO FRM-VRF-IAVA-ATR IN CCB-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IND-IAVA-ATR IN CCB-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-IAVA-ATR IN CCB-FLD.
      * IDC-FIN --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           PERFORM UNP-IKY-CCB.
           PERFORM UNP-AKY-CCB.
           PERFORM UNP-MKY-CCB.
