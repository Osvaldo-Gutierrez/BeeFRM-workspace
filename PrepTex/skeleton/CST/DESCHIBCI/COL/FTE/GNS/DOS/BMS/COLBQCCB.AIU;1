       VAL-IKY-CCB SECTION.
       INI-VAL-IKY-CCB.
*% INCLUDE INI_VAL_IKY_CCB OF 'COLBMCCB.TXT'
           PERFORM VAL-CCB-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CCB-CAI-IOPC-LEN IN CCB-FLD
                GO TO FIN-VAL-IKY-CCB.
           PERFORM VAL-CCB-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CCB-IIC-IOPC-LEN IN CCB-FLD
                GO TO FIN-VAL-IKY-CCB.
           PERFORM VAL-CCB-NUM-IDLC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CCB-NUM-IDLC-LEN IN CCB-FLD
                GO TO FIN-VAL-IKY-CCB.
*% INCLUDE FIN_VAL_IKY_CCB OF 'COLBMCCB.TXT'
       FIN-VAL-IKY-CCB.
           EXIT.
       
       VAL-MKY-CCB SECTION.
       INI-VAL-MKY-CCB.
*% INCLUDE INI_VAL_MKY_CCB OF 'COLBMCCB.TXT'
           PERFORM VAL-FRM-CAI-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-ICLI-LEN IN CCB-FLD
                GO TO FIN-VAL-MKY-CCB.
           PERFORM VAL-FRM-IIC-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-ICLI-LEN IN CCB-FLD
                GO TO FIN-VAL-MKY-CCB.
           PERFORM VAL-FRM-CAI-IAVA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-IAVA-LEN IN CCB-FLD
                GO TO FIN-VAL-MKY-CCB.
           PERFORM VAL-FRM-IIC-IAVA.
