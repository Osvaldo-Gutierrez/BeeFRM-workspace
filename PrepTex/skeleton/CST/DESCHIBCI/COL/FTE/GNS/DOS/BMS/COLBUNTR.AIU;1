*% FRM = "NTR"
      *
      * COLBUNTR
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:11 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMNTR.TXT'
       SET-TAG-COLBUNTR SECTION.
       INI-SET-TAG-COLBUNTR.
           MOVE
           '<IDNMOD NOM=$COLBUNTR$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:11 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUNTR.
           EXIT.
           
       RET-ALL-NTR SECTION.
       INI-RET-ALL-NTR.
*% INCLUDE INI_RET_ALL_NTR OF 'COLBMNTR.TXT'
           PERFORM KEY-ALL-NTR.
           PERFORM RET-NTR-NTR.
*% INCLUDE FIN_RET_ALL_NTR OF 'COLBMNTR.TXT'
       FIN-RET-ALL-NTR.
           EXIT.
       
       RET-NTR-NTR SECTION.
       INI-RET-NTR-NTR.
*% INCLUDE INI_RET_NTR_NTR OF 'COLBMNTR.TXT'
           MOVE NTR-CIC-GNOT IN NTR-FLD TO NTR-CIC-GNOT IN NTR.
           MOVE NTR-IND-RETE IN NTR-FLD TO NTR-IND-RETE IN NTR.
           MOVE NTR-GLS-CCTE IN NTR-FLD TO NTR-GLS-CCTE IN NTR.
*% RREG = "NTR"
*% RREG_FRM = "NTR"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE NTR-NUM-DTRN IN NTR-FLD TO NTR-NUM-DTRN IN NTR.
           MOVE NTR-NUM-MTRN IN NTR-FLD TO NTR-NUM-MTRN IN NTR.
           MOVE NTR-NUM-STRN IN NTR-FLD TO NTR-NUM-STRN IN NTR.
           MOVE NTR-NUM-ATRN IN NTR-FLD TO NTR-NUM-ATRN IN NTR.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE NTR-IND-VIGE IN NTR-FLD TO NTR-IND-VIGE IN NTR.
           MOVE NTR-VAL-GNOT IN NTR-FLD TO NTR-VAL-GNOT IN NTR.
           MOVE NTR-NUM-TR01 IN NTR-FLD TO NTR-NUM-TR01 IN NTR.
           MOVE NTR-VAL-MT01 IN NTR-FLD TO NTR-VAL-MT01 IN NTR.
           MOVE NTR-NUM-TR02 IN NTR-FLD TO NTR-NUM-TR02 IN NTR.
           MOVE NTR-VAL-MT02 IN NTR-FLD TO NTR-VAL-MT02 IN NTR.
           MOVE NTR-NUM-TR03 IN NTR-FLD TO NTR-NUM-TR03 IN NTR.
           MOVE NTR-VAL-MT03 IN NTR-FLD TO NTR-VAL-MT03 IN NTR.
           MOVE NTR-NUM-TR04 IN NTR-FLD TO NTR-NUM-TR04 IN NTR.
           MOVE NTR-VAL-MT04 IN NTR-FLD TO NTR-VAL-MT04 IN NTR.
           MOVE NTR-NUM-TR05 IN NTR-FLD TO NTR-NUM-TR05 IN NTR.
           MOVE NTR-VAL-MT05 IN NTR-FLD TO NTR-VAL-MT05 IN NTR.
           MOVE NTR-NUM-TR06 IN NTR-FLD TO NTR-NUM-TR06 IN NTR.
           MOVE NTR-VAL-MT06 IN NTR-FLD TO NTR-VAL-MT06 IN NTR.
           MOVE NTR-NUM-TR07 IN NTR-FLD TO NTR-NUM-TR07 IN NTR.
           MOVE NTR-VAL-MT07 IN NTR-FLD TO NTR-VAL-MT07 IN NTR.
           MOVE NTR-NUM-TR08 IN NTR-FLD TO NTR-NUM-TR08 IN NTR.
           MOVE NTR-VAL-MT08 IN NTR-FLD TO NTR-VAL-MT08 IN NTR.
           MOVE NTR-NUM-TR09 IN NTR-FLD TO NTR-NUM-TR09 IN NTR.
           MOVE NTR-VAL-MT09 IN NTR-FLD TO NTR-VAL-MT09 IN NTR.
           MOVE NTR-NUM-TR10 IN NTR-FLD TO NTR-NUM-TR10 IN NTR.
           MOVE NTR-VAL-MT10 IN NTR-FLD TO NTR-VAL-MT10 IN NTR.
           MOVE NTR-NUM-TR11 IN NTR-FLD TO NTR-NUM-TR11 IN NTR.
           MOVE NTR-VAL-MT11 IN NTR-FLD TO NTR-VAL-MT11 IN NTR.
           MOVE NTR-NUM-TR12 IN NTR-FLD TO NTR-NUM-TR12 IN NTR.
           MOVE NTR-VAL-MT12 IN NTR-FLD TO NTR-VAL-MT12 IN NTR.
           MOVE NTR-NUM-TR13 IN NTR-FLD TO NTR-NUM-TR13 IN NTR.
           MOVE NTR-VAL-MT13 IN NTR-FLD TO NTR-VAL-MT13 IN NTR.
           MOVE NTR-NUM-TR14 IN NTR-FLD TO NTR-NUM-TR14 IN NTR.
           MOVE NTR-VAL-MT14 IN NTR-FLD TO NTR-VAL-MT14 IN NTR.
*% INCLUDE FIN_RET_NTR_NTR OF 'COLBMNTR.TXT'
       FIN-RET-NTR-NTR.
           EXIT.
       
       KEY-ALL-NTR SECTION.
       INI-KEY-ALL-NTR.
*% INCLUDE INI_KEY_ALL_NTR OF 'COLBMNTR.TXT'
           PERFORM KEY-NTR-NTR.
*% INCLUDE FIN_KEY_ALL_NTR OF 'COLBMNTR.TXT'
       FIN-KEY-ALL-NTR.
           EXIT.
       
       KEY-NTR-NTR SECTION.
       INI-KEY-NTR-NTR.
*% INCLUDE INI_KEY_NTR_NTR OF 'COLBMNTR.TXT'
           MOVE NTR-COD-NOTA IN NTR-FLD TO NTR-COD-NOTA IN NTR.
           MOVE NTR-COD-OFIC IN NTR-FLD TO NTR-COD-OFIC IN NTR.
           MOVE NTR-COD-TDOC IN NTR-FLD TO NTR-COD-TDOC IN NTR.
*% INCLUDE FIN_KEY_NTR_NTR OF 'COLBMNTR.TXT'
       FIN-KEY-NTR-NTR.
           EXIT.
       
       SCR-CLRA-NTR SECTION.
       INI-SCR-CLRA-NTR.
*% INCLUDE INI_SCR_CLRA_NTR OF 'COLBMNTR.TXT'
           PERFORM CLR-KEY-NTR.
           PERFORM CLR-FLD-NTR.
*% INCLUDE FIN_SCR_CLRA_NTR OF 'COLBMNTR.TXT'
       FIN-SCR-CLRA-NTR.
           EXIT.
       
       CLR-KEY-NTR SECTION.
       INI-CLR-KEY-NTR.
*% INCLUDE INI_CLR_KEY_NTR OF 'COLBMNTR.TXT'
           MOVE SPACES TO NTR-COD-NOTA IN NTR-FLD.
           MOVE SPACES TO NTR-COD-OFIC IN NTR-FLD.
           MOVE SPACES TO NTR-COD-TDOC IN NTR-FLD.
*% INCLUDE FIN_CLR_KEY_NTR OF 'COLBMNTR.TXT'
       FIN-CLR-KEY-NTR.
           EXIT.
       
       CLR-FLD-NTR SECTION.
       INI-CLR-FLD-NTR.
*% INCLUDE INI_CLR_FLD_NTR OF 'COLBMNTR.TXT'
           MOVE SPACES TO FRM-GLS-NOTA IN NTR-FLD.
           MOVE SPACES TO FRM-GLS-OFIC IN NTR-FLD.
           MOVE SPACES TO FRM-GLS-TDOC IN NTR-FLD.
           PERFORM CLR-NTR-NTR.
*% INCLUDE FIN_CLR_FLD_NTR OF 'COLBMNTR.TXT'
       FIN-CLR-FLD-NTR.
           EXIT.
       
       CLR-NTR-NTR SECTION.
       INI-CLR-NTR-NTR.
*% INCLUDE INI_CLR_NTR_NTR OF 'COLBMNTR.TXT'
           MOVE SPACES TO NTR-CIC-GNOT IN NTR-FLD.
           MOVE SPACES TO NTR-IND-RETE IN NTR-FLD.
           MOVE SPACES TO NTR-GLS-CCTE IN NTR-FLD.
*% RREG = "NTR"
*% RREG_FRM = "NTR"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO NTR-FEC-FTRN IN NTR-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO NTR-IND-VIGE IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-GNOT IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR01 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT01 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR02 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT02 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR03 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT03 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR04 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT04 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR05 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT05 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR06 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT06 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR07 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT07 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR08 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT08 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR09 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT09 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR10 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT10 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR11 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT11 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR12 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT12 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR13 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT13 IN NTR-FLD.
           MOVE ZEROES TO NTR-NUM-TR14 IN NTR-FLD.
           MOVE ZEROES TO NTR-VAL-MT14 IN NTR-FLD.
*% INCLUDE FIN_CLR_NTR_NTR OF 'COLBMNTR.TXT'
       FIN-CLR-NTR-NTR.
           EXIT.
       
       FST-KEY-NTR SECTION.
       INI-FST-KEY-NTR.
*% INCLUDE INI_FST_KEY_NTR OF 'COLBMNTR.TXT'
           MOVE -1 TO NTR-COD-NOTA-LEN IN NTR-FLD.
*% INCLUDE FIN_FST_KEY_NTR OF 'COLBMNTR.TXT'
       FIN-FST-KEY-NTR.
           EXIT.
       
       FST-FLD-NTR SECTION.
       INI-FST-FLD-NTR.
*% INCLUDE INI_FST_FLD_NTR OF 'COLBMNTR.TXT'
           MOVE -1 TO NTR-CIC-GNOT-LEN IN NTR-FLD.
*% INCLUDE FIN_FST_FLD_NTR OF 'COLBMNTR.TXT'
       FIN-FST-FLD-NTR.
           EXIT.
       
