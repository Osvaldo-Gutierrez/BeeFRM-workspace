       VAL-MKY-NTR SECTION.
       INI-VAL-MKY-NTR.
*% INCLUDE INI_VAL_MKY_NTR OF 'COLBMNTR.TXT'
*% INCLUDE FIN_VAL_MKY_NTR OF 'COLBMNTR.TXT'
       FIN-VAL-MKY-NTR.
           EXIT.
       
       VAL-FLD-NTR SECTION.
       INI-VAL-FLD-NTR.
*% INCLUDE INI_VAL_FLD_NTR OF 'COLBMNTR.TXT'
           PERFORM VAL-FRM-GLS-NOTA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-NOTA-LEN IN NTR-FLD
                GO TO FIN-VAL-FLD-NTR.
           PERFORM VAL-FRM-GLS-OFIC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-OFIC-LEN IN NTR-FLD
                GO TO FIN-VAL-FLD-NTR.
           PERFORM VAL-FRM-GLS-TDOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TDOC-LEN IN NTR-FLD
                GO TO FIN-VAL-FLD-NTR.
           PERFORM VAL-NTR-CIC-GNOT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO NTR-CIC-GNOT-LEN IN NTR-FLD
                GO TO FIN-VAL-FLD-NTR.
           PERFORM VAL-NTR-IND-RETE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO NTR-IND-RETE-LEN IN NTR-FLD
                GO TO FIN-VAL-FLD-NTR.
           PERFORM VAL-NTR-GLS-CCTE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO NTR-GLS-CCTE-LEN IN NTR-FLD
                GO TO FIN-VAL-FLD-NTR.
*% RREG = "NTR"
*% RREG_FRM = "NTR"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-NTR-FEC-FTRN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                NTR-FEC-FTRN-LEN IN NTR-FLD
                GO TO FIN-VAL-FLD-NTR.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-NTR-IND-VIGE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO NTR-IND-VIGE-LEN IN NTR-FLD
                GO TO FIN-VAL-FLD-NTR.
           PERFORM VAL-NTR-VAL-GNOT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO NTR-VAL-GNOT-LEN IN NTR-FLD
                GO TO FIN-VAL-FLD-NTR.
           PERFORM VAL-NTR-NUM-TR01.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO NTR-NUM-TR01-LEN IN NTR-FLD
                GO TO FIN-VAL-FLD-NTR.
           PERFORM VAL-NTR-VAL-MT01.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO NTR-VAL-MT01-LEN IN NTR-FLD
                GO TO FIN-VAL-FLD-NTR.
           PERFORM VAL-NTR-NUM-TR02.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO NTR-NUM-TR02-LEN IN NTR-FLD
                GO TO FIN-VAL-FLD-NTR.
           PERFORM VAL-NTR-VAL-MT02.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO NTR-VAL-MT02-LEN IN NTR-FLD
                GO TO FIN-VAL-FLD-NTR.
           PERFORM VAL-NTR-NUM-TR03.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO NTR-NUM-TR03-LEN IN NTR-FLD
                GO TO FIN-VAL-FLD-NTR.
           PERFORM VAL-NTR-VAL-MT03.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO NTR-VAL-MT03-LEN IN NTR-FLD
                GO TO FIN-VAL-FLD-NTR.
           PERFORM VAL-NTR-NUM-TR04.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO NTR-NUM-TR04-LEN IN NTR-FLD
                GO TO FIN-VAL-FLD-NTR.
           PERFORM VAL-NTR-VAL-MT04.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO NTR-VAL-MT04-LEN IN NTR-FLD
                GO TO FIN-VAL-FLD-NTR.
