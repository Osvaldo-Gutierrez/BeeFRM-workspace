      *
      * COLBQCGC
      * Version 3.0.1 Nov-2005
      * 13-Mar-14 11:20 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CGC"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMCGC.TXT'
       SET-TAG-COLBQCGC SECTION.
       INI-SET-TAG-COLBQCGC.
           MOVE
           '<IDNMOD NOM=$COLBQCGC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$13-Mar-14 11:20 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCGC.
           EXIT.
           
       PUT-ALL-CGC SECTION.
       INI-PUT-ALL-CGC.
*% INCLUDE INI_PUT_ALL_CGC OF 'COLBMCGC.TXT'
           PERFORM PUT-CGC-CGC.
*% INCLUDE FIN_PUT_ALL_CGC OF 'COLBMCGC.TXT'
       FIN-PUT-ALL-CGC.
           EXIT.
       
       PUT-CGC-CGC SECTION.
       INI-PUT-CGC-CGC.
*% INCLUDE INI_PUT_CGC_CGC OF 'COLBMCGC.TXT'
           IF PGM-STAT-CGC-NUL
               GO TO FIN-PUT-CGC-CGC.
           IF NOT PGM-STAT-CGC-OKS
               PERFORM CLR-CGC-CGC
               GO TO FIN-PUT-CGC-CGC.
           MOVE CGC-GLS-CPTO IN CGC TO CGC-GLS-CPTO IN CGC-FLD.
           MOVE CGC-CIC-KEY1 IN CGC TO CGC-CIC-KEY1 IN CGC-FLD.
           MOVE CGC-NUM-KEY1 IN CGC TO CGC-NUM-KEY1 IN CGC-FLD.
           MOVE CGC-CIC-ICLI IN CGC TO CGC-CIC-ICLI IN CGC-FLD.
           MOVE CGC-GLS-KEY3 IN CGC TO CGC-GLS-KEY3 IN CGC-FLD.
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "KEY4"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CGC-NUM-DEY4 IN CGC TO CGC-NUM-DEY4 IN CGC-FLD.
           MOVE CGC-NUM-MEY4 IN CGC TO CGC-NUM-MEY4 IN CGC-FLD.
           MOVE CGC-NUM-SEY4 IN CGC TO CGC-NUM-SEY4 IN CGC-FLD.
           MOVE CGC-NUM-AEY4 IN CGC TO CGC-NUM-AEY4 IN CGC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE CGC-GLS-KEY5 IN CGC TO CGC-GLS-KEY5 IN CGC-FLD.
           MOVE CGC-GLS-KEY6 IN CGC TO CGC-GLS-KEY6 IN CGC-FLD.
           MOVE CGC-GLS-KEY8 IN CGC TO CGC-GLS-KEY8 IN CGC-FLD.
           MOVE CGC-SGV-MON1 IN CGC TO CGC-SGV-MON1 IN CGC-FLD.
           MOVE CGC-SGV-MON2 IN CGC TO CGC-SGV-MON2 IN CGC-FLD.
           MOVE CGC-SGV-MON3 IN CGC TO CGC-SGV-MON3 IN CGC-FLD.
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "AUX4"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CGC-NUM-DUX4 IN CGC TO CGC-NUM-DUX4 IN CGC-FLD.
           MOVE CGC-NUM-MUX4 IN CGC TO CGC-NUM-MUX4 IN CGC-FLD.
           MOVE CGC-NUM-SUX4 IN CGC TO CGC-NUM-SUX4 IN CGC-FLD.
           MOVE CGC-NUM-AUX4 IN CGC TO CGC-NUM-AUX4 IN CGC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "AUX5"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CGC-NUM-DUX5 IN CGC TO CGC-NUM-DUX5 IN CGC-FLD.
           MOVE CGC-NUM-MUX5 IN CGC TO CGC-NUM-MUX5 IN CGC-FLD.
           MOVE CGC-NUM-SUX5 IN CGC TO CGC-NUM-SUX5 IN CGC-FLD.
           MOVE CGC-NUM-AUX5 IN CGC TO CGC-NUM-AUX5 IN CGC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE CGC-GLS-AUX4 IN CGC TO CGC-GLS-AUX4 IN CGC-FLD.
           MOVE CGC-NUM-COR1 IN CGC TO CGC-NUM-COR1 IN CGC-FLD.
*% INCLUDE FIN_PUT_CGC_CGC OF 'COLBMCGC.TXT'
       FIN-PUT-CGC-CGC.
           EXIT.
       
       KEY-ALL-CGC SECTION.
       INI-KEY-ALL-CGC.
*% INCLUDE INI_KEY_ALL_CGC OF 'COLBMCGC.TXT'
           PERFORM KEY-CGC-CGC.
*% INCLUDE FIN_KEY_ALL_CGC OF 'COLBMCGC.TXT'
       FIN-KEY-ALL-CGC.
           EXIT.
       
       KEY-CGC-CGC SECTION.
       INI-KEY-CGC-CGC.
*% INCLUDE INI_KEY_CGC_CGC OF 'COLBMCGC.TXT'
           MOVE CGC-GLS-CPTO IN CGC-FLD TO CGC-GLS-CPTO IN CGC.
           MOVE CGC-CIC-KEY1 IN CGC-FLD TO CGC-CIC-KEY1 IN CGC.
           MOVE CGC-NUM-KEY1 IN CGC-FLD TO CGC-NUM-KEY1 IN CGC.
*% INCLUDE FIN_KEY_CGC_CGC OF 'COLBMCGC.TXT'
       FIN-KEY-CGC-CGC.
           EXIT.
       
       SCR-CLRA-CGC SECTION.
       INI-SCR-CLRA-CGC.
*% INCLUDE INI_SCR_CLRA_CGC OF 'COLBMCGC.TXT'
           PERFORM CLR-KEY-CGC.
           PERFORM CLR-FLD-CGC.
*% INCLUDE FIN_SCR_CLRA_CGC OF 'COLBMCGC.TXT'
       FIN-SCR-CLRA-CGC.
           EXIT.
       
       CLR-KEY-CGC SECTION.
       INI-CLR-KEY-CGC.
*% INCLUDE INI_CLR_KEY_CGC OF 'COLBMCGC.TXT'
           MOVE SPACES TO CGC-GLS-CPTO IN CGC-FLD.
           MOVE SPACES TO CGC-CIC-KEY1 IN CGC-FLD.
           MOVE ZEROES TO CGC-NUM-KEY1 IN CGC-FLD.
*% INCLUDE FIN_CLR_KEY_CGC OF 'COLBMCGC.TXT'
       FIN-CLR-KEY-CGC.
           EXIT.
       
       CLR-FLD-CGC SECTION.
       INI-CLR-FLD-CGC.
*% INCLUDE INI_CLR_FLD_CGC OF 'COLBMCGC.TXT'
           PERFORM CLR-CGC-CGC.
*% INCLUDE FIN_CLR_FLD_CGC OF 'COLBMCGC.TXT'
       FIN-CLR-FLD-CGC.
           EXIT.
       
       CLR-CGC-CGC SECTION.
       INI-CLR-CGC-CGC.
*% INCLUDE INI_CLR_CGC_CGC OF 'COLBMCGC.TXT'
           MOVE SPACES TO CGC-CIC-ICLI IN CGC-FLD.
           MOVE SPACES TO CGC-GLS-KEY3 IN CGC-FLD.
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "KEY4"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CGC-FEC-KEY4 IN CGC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO CGC-GLS-KEY5 IN CGC-FLD.
           MOVE SPACES TO CGC-GLS-KEY6 IN CGC-FLD.
           MOVE SPACES TO CGC-GLS-KEY8 IN CGC-FLD.
           MOVE ZEROES TO CGC-SGV-MON1 IN CGC-FLD.
           MOVE ZEROES TO CGC-SGV-MON2 IN CGC-FLD.
           MOVE ZEROES TO CGC-SGV-MON3 IN CGC-FLD.
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "AUX4"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CGC-FEC-AUX4 IN CGC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "AUX5"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CGC-FEC-AUX5 IN CGC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO CGC-GLS-AUX4 IN CGC-FLD.
           MOVE ZEROES TO CGC-NUM-COR1 IN CGC-FLD.
*% INCLUDE FIN_CLR_CGC_CGC OF 'COLBMCGC.TXT'
       FIN-CLR-CGC-CGC.
           EXIT.
       
       FST-KEY-CGC SECTION.
       INI-FST-KEY-CGC.
*% INCLUDE INI_FST_KEY_CGC OF 'COLBMCGC.TXT'
           MOVE -1 TO CGC-GLS-CPTO-LEN IN CGC-FLD.
*% INCLUDE FIN_FST_KEY_CGC OF 'COLBMCGC.TXT'
       FIN-FST-KEY-CGC.
           EXIT.
       
       FST-FLD-CGC SECTION.
       INI-FST-FLD-CGC.
*% INCLUDE INI_FST_FLD_CGC OF 'COLBMCGC.TXT'
           MOVE -1 TO CGC-CIC-ICLI-LEN IN CGC-FLD.
*% INCLUDE FIN_FST_FLD_CGC OF 'COLBMCGC.TXT'
       FIN-FST-FLD-CGC.
           EXIT.
       
       PRO-KEY-CGC SECTION.
       INI-PRO-KEY-CGC.
*% INCLUDE INI_PRO_KEY_CGC OF 'COLBMCGC.TXT'
           PERFORM PRO-IKY-CGC.
           PERFORM PRO-AKY-CGC.
           PERFORM PRO-MKY-CGC.
*% INCLUDE FIN_PRO_KEY_CGC OF 'COLBMCGC.TXT'
       FIN-PRO-KEY-CGC.
           EXIT.
       
       PRO-IKY-CGC SECTION.
       INI-PRO-IKY-CGC.
*% INCLUDE INI_PRO_IKY_CGC OF 'COLBMCGC.TXT'
           MOVE FRM-CPIM-AEY TO CGC-GLS-CPTO-ATR IN CGC-FLD.
           MOVE FRM-CPIM-AEY TO CGC-CIC-KEY1-ATR IN CGC-FLD.
           MOVE FRM-CPIM-AEY TO CGC-NUM-KEY1-ATR IN CGC-FLD.
*% INCLUDE FIN_PRO_IKY_CGC OF 'COLBMCGC.TXT'
       FIN-PRO-IKY-CGC.
           EXIT.
       
       PRO-MKY-CGC SECTION.
       INI-PRO-MKY-CGC.
*% INCLUDE INI_PRO_MKY_CGC OF 'COLBMCGC.TXT'
*% INCLUDE FIN_PRO_MKY_CGC OF 'COLBMCGC.TXT'
       FIN-PRO-MKY-CGC.
           EXIT.
       
       PRO-AKY-CGC SECTION.
       INI-PRO-AKY-CGC.
*% INCLUDE INI_PRO_AKY_CGC OF 'COLBMCGC.TXT'
*% INCLUDE FIN_PRO_AKY_CGC OF 'COLBMCGC.TXT'
       FIN-PRO-AKY-CGC.
           EXIT.
       
       UNP-KEY-CGC SECTION.
       INI-UNP-KEY-CGC.
*% INCLUDE INI_UNP_KEY_CGC OF 'COLBMCGC.TXT'
           PERFORM UNP-IKY-CGC.
           PERFORM UNP-AKY-CGC.
           PERFORM UNP-MKY-CGC.
*% INCLUDE FIN_UNP_KEY_CGC OF 'COLBMCGC.TXT'
       FIN-UNP-KEY-CGC.
           EXIT.
       
       UNP-IKY-CGC SECTION.
       INI-UNP-IKY-CGC.
*% INCLUDE INI_UNP_IKY_CGC OF 'COLBMCGC.TXT'
           MOVE FRM-CPIM-UBY TO CGC-GLS-CPTO-ATR.
           MOVE FRM-CPIM-UBY TO CGC-CIC-KEY1-ATR.
           MOVE FRM-CPIM-UBY TO CGC-NUM-KEY1-ATR.
*% INCLUDE FIN_UNP_IKY_CGC OF 'COLBMCGC.TXT'
       FIN-UNP-IKY-CGC.
           EXIT.
       
       UNP-MKY-CGC SECTION.
       INI-UNP-MKY-CGC.
*% INCLUDE INI_UNP_MKY_CGC OF 'COLBMCGC.TXT'
*% INCLUDE FIN_UNP_MKY_CGC OF 'COLBMCGC.TXT'
       FIN-UNP-MKY-CGC.
           EXIT.
       
       UNP-AKY-CGC SECTION.
       INI-UNP-AKY-CGC.
*% INCLUDE INI_UNP_AKY_CGC OF 'COLBMCGC.TXT'
*% INCLUDE FIN_UNP_AKY_CGC OF 'COLBMCGC.TXT'
       FIN-UNP-AKY-CGC.
           EXIT.
       
       UNP-FLD-CGC SECTION.
       INI-UNP-FLD-CGC.
*% INCLUDE INI_UNP_FLD_CGC OF 'COLBMCGC.TXT'
           PERFORM UNP-CGC-CGC.
*% INCLUDE FIN_UNP_FLD_CGC OF 'COLBMCGC.TXT'
       FIN-UNP-FLD-CGC.
           EXIT.
       
       UNP-CGC-CGC SECTION.
       INI-UNP-CGC-CGC.
*% INCLUDE INI_UNP_CGC_CGC OF 'COLBMCGC.TXT'
           MOVE FRM-CPIM-UBY TO CGC-CIC-ICLI-ATR IN CGC-FLD.
           MOVE FRM-CPIM-UBY TO CGC-GLS-KEY3-ATR IN CGC-FLD.
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "KEY4"
           MOVE FRM-CPIM-UBY TO CGC-FEC-KEY4-ATR IN CGC-FLD.
           MOVE FRM-CPIM-UBY TO CGC-GLS-KEY5-ATR IN CGC-FLD.
           MOVE FRM-CPIM-UBY TO CGC-GLS-KEY6-ATR IN CGC-FLD.
           MOVE FRM-CPIM-UBY TO CGC-GLS-KEY8-ATR IN CGC-FLD.
           MOVE FRM-CPIM-UBY TO CGC-SGV-MON1-ATR IN CGC-FLD.
           MOVE FRM-CPIM-UBY TO CGC-SGV-MON2-ATR IN CGC-FLD.
           MOVE FRM-CPIM-UBY TO CGC-SGV-MON3-ATR IN CGC-FLD.
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "AUX4"
           MOVE FRM-CPIM-UBY TO CGC-FEC-AUX4-ATR IN CGC-FLD.
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "AUX5"
           MOVE FRM-CPIM-UBY TO CGC-FEC-AUX5-ATR IN CGC-FLD.
           MOVE FRM-CPIM-UBY TO CGC-GLS-AUX4-ATR IN CGC-FLD.
           MOVE FRM-CPIM-UBY TO CGC-NUM-COR1-ATR IN CGC-FLD.
*% INCLUDE FIN_UNP_CGC_CGC OF 'COLBMCGC.TXT'
       FIN-UNP-CGC-CGC.
           EXIT.
       
       VAL-KEY-CGC SECTION.
       INI-VAL-KEY-CGC.
           PERFORM VAL-IKY-CGC.
           PERFORM VAL-MKY-CGC.
           PERFORM VAL-AKY-CGC.
       FIN-VAL-KEY-CGC.
           EXIT.
       
       VAL-IKY-CGC SECTION.
       INI-VAL-IKY-CGC.
*% INCLUDE INI_VAL_IKY_CGC OF 'COLBMCGC.TXT'
           PERFORM VAL-CGC-GLS-CPTO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CGC-GLS-CPTO-LEN IN CGC-FLD
                GO TO FIN-VAL-IKY-CGC.
           PERFORM VAL-CGC-CIC-KEY1.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CGC-CIC-KEY1-LEN IN CGC-FLD
                GO TO FIN-VAL-IKY-CGC.
           PERFORM VAL-CGC-NUM-KEY1.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CGC-NUM-KEY1-LEN IN CGC-FLD
                GO TO FIN-VAL-IKY-CGC.
*% INCLUDE FIN_VAL_IKY_CGC OF 'COLBMCGC.TXT'
       FIN-VAL-IKY-CGC.
           EXIT.
       
       VAL-MKY-CGC SECTION.
       INI-VAL-MKY-CGC.
*% INCLUDE INI_VAL_MKY_CGC OF 'COLBMCGC.TXT'
*% INCLUDE FIN_VAL_MKY_CGC OF 'COLBMCGC.TXT'
       FIN-VAL-MKY-CGC.
           EXIT.
       
       VAL-AKY-CGC SECTION.
       INI-VAL-AKY-CGC.
*% INCLUDE INI_VAL_AKY_CGC OF 'COLBMCGC.TXT'
*% INCLUDE FIN_VAL_AKY_CGC OF 'COLBMCGC.TXT'
       FIN-VAL-AKY-CGC.
           EXIT.
       
       VAL-CGC-GLS-CPTO SECTION.
       INI-VAL-CGC-GLS-CPTO.
*% LBL = "CGC_GLS_CPTO OF 'COLBMCGC.TXT'"
*% INCLUDE INI_CGC_GLS_CPTO OF 'COLBMCGC.TXT'
           IF CGC-GLS-CPTO IN CGC-FLD NOT > SPACES
              MOVE SPACES TO CGC-GLS-CPTO IN CGC-FLD.
*% INCLUDE CGC_GLS_CPTO OF 'COLBMCGC.TXT'
*% INCLUDE FIN_CGC_GLS_CPTO OF 'COLBMCGC.TXT'
       FIN-VAL-CGC-GLS-CPTO.
           EXIT.
       
       VAL-CGC-CIC-KEY1 SECTION.
       INI-VAL-CGC-CIC-KEY1.
*% LBL = "CGC_CIC_KEY1 OF 'COLBMCGC.TXT'"
*% INCLUDE INI_CGC_CIC_KEY1 OF 'COLBMCGC.TXT'
           IF CGC-CIC-KEY1 IN CGC-FLD NOT > SPACES
              MOVE SPACES TO CGC-CIC-KEY1 IN CGC-FLD.
*% INCLUDE CGC_CIC_KEY1 OF 'COLBMCGC.TXT'
*% INCLUDE FIN_CGC_CIC_KEY1 OF 'COLBMCGC.TXT'
       FIN-VAL-CGC-CIC-KEY1.
           EXIT.
       
       VAL-CGC-NUM-KEY1 SECTION.
       INI-VAL-CGC-NUM-KEY1.
*% LBL = "CGC_NUM_KEY1 OF 'COLBMCGC.TXT'"
*% INCLUDE INI_CGC_NUM_KEY1 OF 'COLBMCGC.TXT'
*% INCLUDE CGC_NUM_KEY1 OF 'COLBMCGC.TXT'
*% INCLUDE FIN_CGC_NUM_KEY1 OF 'COLBMCGC.TXT'
       FIN-VAL-CGC-NUM-KEY1.
           EXIT.
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "KEY4"
*% IZR = "S"
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "AUX4"
*% IZR = "S"
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "AUX5"
*% IZR = "S"
       
       VAL-CON-KEY-CGC SECTION.
       INI-VAL-CON-KEY-CGC.
*% INCLUDE INI_VAL_CON_KEY_CGC OF 'COLBMCGC.TXT'
           IF     CGC-GLS-CPTO IN CGC-FLD NOT > SPACES 
              AND CGC-CIC-KEY1 IN CGC-FLD NOT > SPACES 
              AND CGC-NUM-KEY1 IN CGC-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CGC.
*% INCLUDE FIN_VAL_CON_KEY_CGC OF 'COLBMCGC.TXT'
       FIN-VAL-CON-KEY-CGC.
           EXIT.
       
       VAL-CON-IKY-CGC SECTION.
       INI-VAL-CON-IKY-CGC.
*% INCLUDE INI_VAL_CON_IKY_CGC OF 'COLBMCGC.TXT'
           IF     CGC-GLS-CPTO IN CGC-FLD NOT > SPACES 
              AND CGC-CIC-KEY1 IN CGC-FLD NOT > SPACES 
              AND CGC-NUM-KEY1 IN CGC-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CGC.
*% INCLUDE FIN_VAL_CON_IKY_CGC OF 'COLBMCGC.TXT'
       FIN-VAL-CON-IKY-CGC.
           EXIT.
       
       VAL-NUM-KEY-CGC SECTION.
       INI-VAL-NUM-KEY-CGC.
           PERFORM VAL-NUM-IKY-CGC.
           PERFORM VAL-NUM-MKY-CGC.
           PERFORM VAL-NUM-AKY-CGC.
       FIN-VAL-NUM-KEY-CGC.
           EXIT.
       
       VAL-NUM-IKY-CGC SECTION.
       INI-VAL-NUM-IKY-CGC.
*% INCLUDE INI_VAL_NUM_IKY_CGC OF 'COLBMCGC.TXT'
       VAL-NUM-CGC-NUM-KEY1.
*% BUF = "CGC-FLD"
*% FLD = "CGC-NUM-KEY1"
*% ENT = "4"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-CGC.
*% INCLUDE FIN_VAL_NUM_IKY_CGC OF 'COLBMCGC.TXT'
       FIN-VAL-NUM-IKY-CGC.
           EXIT.
       
       VAL-NUM-MKY-CGC SECTION.
       INI-VAL-NUM-MKY-CGC.
*% INCLUDE INI_VAL_NUM_MKY_CGC OF 'COLBMCGC.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_CGC OF 'COLBMCGC.TXT'
       FIN-VAL-NUM-MKY-CGC.
           EXIT.
       
       VAL-NUM-AKY-CGC SECTION.
       INI-VAL-NUM-AKY-CGC.
*% INCLUDE INI_VAL_NUM_AKY_CGC OF 'COLBMCGC.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_CGC OF 'COLBMCGC.TXT'
       FIN-VAL-NUM-AKY-CGC.
           EXIT.
       
       VAL-NUM-FLD-CGC SECTION.
       INI-VAL-NUM-FLD-CGC.
*% INCLUDE INI_VAL_NUM_FLD_CGC OF 'COLBMCGC.TXT'
       VAL-NUM-CGC-SGV-MON1.
*% BUF = "CGC-FLD"
*% FLD = "CGC-SGV-MON1"
*% ENT = "10"
*% DCM = "4"
*% NMR = "NS9V"
*% ISG = "S"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-CGC.
       VAL-NUM-CGC-SGV-MON2.
*% BUF = "CGC-FLD"
*% FLD = "CGC-SGV-MON2"
*% ENT = "7"
*% DCM = "4"
*% NMR = "NS9V"
*% ISG = "S"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-CGC.
       VAL-NUM-CGC-SGV-MON3.
*% BUF = "CGC-FLD"
*% FLD = "CGC-SGV-MON3"
*% ENT = "10"
*% DCM = "4"
*% NMR = "NS9V"
*% ISG = "S"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-CGC.
       VAL-NUM-CGC-NUM-COR1.
*% BUF = "CGC-FLD"
*% FLD = "CGC-NUM-COR1"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-CGC.
*% INCLUDE FIN_VAL_NUM_FLD_CGC OF 'COLBMCGC.TXT'
       FIN-VAL-NUM-FLD-CGC.
           EXIT.
       
       EDT-KEY-CGC SECTION.
       INI-EDT-KEY-CGC.
           PERFORM EDT-IKY-CGC.
           PERFORM EDT-MKY-CGC.
           PERFORM EDT-AKY-CGC.
*% INCLUDE EDT_KEY_CGC OF 'COLBMCGC.TXT'
       FIN-EDT-KEY-CGC.
           EXIT.
       
       EDT-IKY-CGC SECTION.
       INI-EDT-IKY-CGC.
*% INCLUDE EDT_IKY_CGC OF 'COLBMCGC.TXT'
           IF CGC-NUM-KEY1-LEN IN CGC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CGC-NUM-KEY1-LEN IN CGC-FLD
                GO TO FIN-EDT-IKY-CGC.
           MOVE CGC-NUM-KEY1 IN CGC-FLD TO CGC-NUM-KEY1-EDT IN CGC-EDT.
           MOVE CGC-NUM-KEY1-EDT IN CGC-EDT TO 
                CGC-NUM-KEY1-ALF IN CGC-FLD.
*% INCLUDE FIN_EDT_IKY_CGC OF 'COLBMCGC.TXT'
       FIN-EDT-IKY-CGC.
           EXIT.
       
       EDT-MKY-CGC SECTION.
       INI-EDT-MKY-CGC.
*% INCLUDE EDT_MKY_CGC OF 'COLBMCGC.TXT'
*% INCLUDE FIN_EDT_MKY_CGC OF 'COLBMCGC.TXT'
       FIN-EDT-MKY-CGC.
           EXIT.
       
       EDT-AKY-CGC SECTION.
       INI-EDT-AKY-CGC.
*% INCLUDE EDT_AKY_CGC OF 'COLBMCGC.TXT'
*% INCLUDE FIN_EDT_AKY_CGC OF 'COLBMCGC.TXT'
       FIN-EDT-AKY-CGC.
           EXIT.
       
       EDT-FLD-CGC SECTION.
       INI-EDT-FLD-CGC.
*% INCLUDE EDT_FLD_CGC OF 'COLBMCGC.TXT'
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "KEY4"
           IF CGC-SGV-MON1-LEN IN CGC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CGC-SGV-MON1-LEN IN CGC-FLD
                GO TO FIN-EDT-FLD-CGC.
           MOVE CGC-SGV-MON1 IN CGC-FLD TO CGC-SGV-MON1-EDT IN CGC-EDT.
           MOVE CGC-SGV-MON1-EDT IN CGC-EDT TO 
                CGC-SGV-MON1-ALF IN CGC-FLD.
           IF CGC-SGV-MON2-LEN IN CGC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CGC-SGV-MON2-LEN IN CGC-FLD
                GO TO FIN-EDT-FLD-CGC.
           MOVE CGC-SGV-MON2 IN CGC-FLD TO CGC-SGV-MON2-EDT IN CGC-EDT.
           MOVE CGC-SGV-MON2-EDT IN CGC-EDT TO 
                CGC-SGV-MON2-ALF IN CGC-FLD.
           IF CGC-SGV-MON3-LEN IN CGC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CGC-SGV-MON3-LEN IN CGC-FLD
                GO TO FIN-EDT-FLD-CGC.
           MOVE CGC-SGV-MON3 IN CGC-FLD TO CGC-SGV-MON3-EDT IN CGC-EDT.
           MOVE CGC-SGV-MON3-EDT IN CGC-EDT TO 
                CGC-SGV-MON3-ALF IN CGC-FLD.
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "AUX4"
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "AUX5"
           IF CGC-NUM-COR1-LEN IN CGC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CGC-NUM-COR1-LEN IN CGC-FLD
                GO TO FIN-EDT-FLD-CGC.
           MOVE CGC-NUM-COR1 IN CGC-FLD TO CGC-NUM-COR1-EDT IN CGC-EDT.
           MOVE CGC-NUM-COR1-EDT IN CGC-EDT TO 
                CGC-NUM-COR1-ALF IN CGC-FLD.
*% INCLUDE FIN_EDT_FLD_CGC OF 'COLBMCGC.TXT'
       FIN-EDT-FLD-CGC.
           EXIT.
