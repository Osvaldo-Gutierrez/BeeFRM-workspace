       FIN-EDT-KEY-CRP.
           EXIT.
       
       EDT-IKY-CRP SECTION.
       INI-EDT-IKY-CRP.
*% INCLUDE EDT_IKY_CRP OF 'COLBMCRP.TXT'
*% INCLUDE FIN_EDT_IKY_CRP OF 'COLBMCRP.TXT'
       FIN-EDT-IKY-CRP.
           EXIT.
       
       EDT-MKY-CRP SECTION.
       INI-EDT-MKY-CRP.
*% INCLUDE EDT_MKY_CRP OF 'COLBMCRP.TXT'
           IF PLL-IIC-IPLL-LEN IN CRP-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PLL-IIC-IPLL-LEN IN CRP-FLD
                GO TO FIN-EDT-MKY-CRP.
           MOVE PLL-IIC-IPLL IN CRP-FLD TO PLL-IIC-IPLL-EDT IN CRP-EDT.
           MOVE PLL-IIC-IPLL-EDT IN CRP-EDT TO 
                PLL-IIC-IPLL-ALF IN CRP-FLD.
           IF PLL-NUM-IPLL-LEN IN CRP-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PLL-NUM-IPLL-LEN IN CRP-FLD
                GO TO FIN-EDT-MKY-CRP.
           MOVE PLL-NUM-IPLL IN CRP-FLD TO PLL-NUM-IPLL-EDT IN CRP-EDT.
           MOVE PLL-NUM-IPLL-EDT IN CRP-EDT TO 
                PLL-NUM-IPLL-ALF IN CRP-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPIN"
           IF PLL-IIC-ICVN-LEN IN CRP-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PLL-IIC-ICVN-LEN IN CRP-FLD
                GO TO FIN-EDT-MKY-CRP.
           MOVE PLL-IIC-ICVN IN CRP-FLD TO PLL-IIC-ICVN-EDT IN CRP-EDT.
           MOVE PLL-IIC-ICVN-EDT IN CRP-EDT TO 
                PLL-IIC-ICVN-ALF IN CRP-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPTR"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVIN"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVTR"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF DBC-NUM-ICLI-LEN IN CRP-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DBC-NUM-ICLI-LEN IN CRP-FLD
                GO TO FIN-EDT-MKY-CRP.
           MOVE DBC-NUM-ICLI IN CRP-FLD TO
                DBC-NUM-ICLI-EDT IN CRP-EDT.
           MOVE DBC-NUM-ICLI-EDT IN CRP-EDT TO
                DBC-NUM-ICLI-ALF IN CRP-FLD.
      * IDC-FIN --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF FRM-IIC-YPLL-LEN IN CRP-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-IIC-YPLL-LEN IN CRP-FLD
                GO TO FIN-EDT-MKY-CRP.
           MOVE FRM-IIC-YPLL IN CRP-FLD TO FRM-IIC-YPLL-EDT IN CRP-EDT.
           MOVE FRM-IIC-YPLL-EDT IN CRP-EDT TO 
                FRM-IIC-YPLL-ALF IN CRP-FLD.
           IF FRM-IIC-YCVN-LEN IN CRP-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-IIC-YCVN-LEN IN CRP-FLD
                GO TO FIN-EDT-MKY-CRP.
           MOVE FRM-IIC-YCVN IN CRP-FLD TO FRM-IIC-YCVN-EDT IN CRP-EDT.
           MOVE FRM-IIC-YCVN-EDT IN CRP-EDT TO 
                FRM-IIC-YCVN-ALF IN CRP-FLD.
           IF FRM-NUM-YPLL-LEN IN CRP-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-YPLL-LEN IN CRP-FLD
                GO TO FIN-EDT-MKY-CRP.
