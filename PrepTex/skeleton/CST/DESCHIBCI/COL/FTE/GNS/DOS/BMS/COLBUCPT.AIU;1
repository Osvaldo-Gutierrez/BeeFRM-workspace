       EDT-FLD-CPT SECTION.
       INI-EDT-FLD-CPT.
*% INCLUDE EDT_FLD_CPT OF 'COLBMCPT.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF DBC-NUM-ICLI-LEN IN CPT-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DBC-NUM-ICLI-LEN IN CPT-FLD
                GO TO FIN-EDT-FLD-CPT.
           MOVE DBC-NUM-ICLI IN CPT-FLD TO
                DBC-NUM-ICLI-EDT IN CPT-EDT.
           MOVE DBC-NUM-ICLI-EDT IN CPT-EDT TO
                DBC-NUM-ICLI-ALF IN CPT-FLD.
      * IDC-FIN --> BMCHIBCI011.SRCAIU EDICION NUMERICA
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
           IF OPC-NUM-VCOR-LEN IN CPT-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO OPC-NUM-VCOR-LEN IN CPT-FLD
                GO TO FIN-EDT-FLD-CPT.
           MOVE OPC-NUM-VCOR IN CPT-FLD TO OPC-NUM-VCOR-EDT IN CPT-EDT.
           MOVE OPC-NUM-VCOR-EDT IN CPT-EDT TO 
                OPC-NUM-VCOR-ALF IN CPT-FLD.
*% RREG = "DLC"
*% RREG_FRM = "DLC"
*% NOMB = "FMAV"
           IF FRM-VAL-SCRE-LEN IN CPT-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-SCRE-LEN IN CPT-FLD
                GO TO FIN-EDT-FLD-CPT.
           MOVE FRM-VAL-SCRE IN CPT-FLD TO FRM-VAL-SCRE-EDT IN CPT-EDT.
           MOVE FRM-VAL-SCRE-EDT IN CPT-EDT TO 
                FRM-VAL-SCRE-ALF IN CPT-FLD.
           IF FRM-VAL-TASA-LEN IN CPT-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-TASA-LEN IN CPT-FLD
                GO TO FIN-EDT-FLD-CPT.
           MOVE FRM-VAL-TASA IN CPT-FLD TO FRM-VAL-TASA-EDT IN CPT-EDT.
           MOVE FRM-VAL-TASA-EDT IN CPT-EDT TO 
                FRM-VAL-TASA-ALF IN CPT-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPAG"
           IF FRM-VAL-CAPR-LEN IN CPT-FLD(1) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-CAPR-LEN IN CPT-FLD(1)
                GO TO FIN-EDT-FLD-CPT.
           MOVE FRM-VAL-CAPR IN CPT-FLD(1) TO 
                FRM-VAL-CAPR-EDT IN CPT-EDT.
           MOVE FRM-VAL-CAPR-EDT IN CPT-EDT TO 
                FRM-VAL-CAPR-ALF IN CPT-FLD(1).
           IF FRM-VAL-CAPR-LEN IN CPT-FLD(2) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-CAPR-LEN IN CPT-FLD(2)
                GO TO FIN-EDT-FLD-CPT.
           MOVE FRM-VAL-CAPR IN CPT-FLD(2) TO 
                FRM-VAL-CAPR-EDT IN CPT-EDT.
           MOVE FRM-VAL-CAPR-EDT IN CPT-EDT TO 
                FRM-VAL-CAPR-ALF IN CPT-FLD(2).
           IF FRM-VAL-CAPR-LEN IN CPT-FLD(3) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-CAPR-LEN IN CPT-FLD(3)
                GO TO FIN-EDT-FLD-CPT.
           MOVE FRM-VAL-CAPR IN CPT-FLD(3) TO 
                FRM-VAL-CAPR-EDT IN CPT-EDT.
           MOVE FRM-VAL-CAPR-EDT IN CPT-EDT TO 
                FRM-VAL-CAPR-ALF IN CPT-FLD(3).
           IF FRM-VAL-CAPR-LEN IN CPT-FLD(4) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-CAPR-LEN IN CPT-FLD(4)
                GO TO FIN-EDT-FLD-CPT.
           MOVE FRM-VAL-CAPR IN CPT-FLD(4) TO 
                FRM-VAL-CAPR-EDT IN CPT-EDT.
           MOVE FRM-VAL-CAPR-EDT IN CPT-EDT TO 
                FRM-VAL-CAPR-ALF IN CPT-FLD(4).
           IF FRM-VAL-INTE-LEN IN CPT-FLD(1) = FRM-ERR-NUME
