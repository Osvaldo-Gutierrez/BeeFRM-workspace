       INI-CLR-DBC-CPT.
*% INCLUDE INI_CLR_DBC_CPT OF 'COLBMCPT.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN CPT-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN CPT-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN CPT-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN CPT-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_DBC_CPT OF 'COLBMCPT.TXT'
       FIN-CLR-DBC-CPT.
           EXIT.
       
       CLR-DLC-CPT SECTION.
       INI-CLR-DLC-CPT.
*% INCLUDE INI_CLR_DLC_CPT OF 'COLBMCPT.TXT'
*% RREG = "DLC"
*% RREG_FRM = "DLC"
*% NOMB = "FMAV"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DLC-FEC-FMAV IN CPT-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_DLC_CPT OF 'COLBMCPT.TXT'
       FIN-CLR-DLC-CPT.
           EXIT.
       
       FST-KEY-CPT SECTION.
       INI-FST-KEY-CPT.
*% INCLUDE INI_FST_KEY_CPT OF 'COLBMCPT.TXT'
           MOVE -1 TO OPC-CAI-IOPC-LEN IN CPT-FLD.
*% INCLUDE FIN_FST_KEY_CPT OF 'COLBMCPT.TXT'
       FIN-FST-KEY-CPT.
           EXIT.
       
       FST-FLD-CPT SECTION.
       INI-FST-FLD-CPT.
*% INCLUDE INI_FST_FLD_CPT OF 'COLBMCPT.TXT'
*% INCLUDE FIN_FST_FLD_CPT OF 'COLBMCPT.TXT'
       FIN-FST-FLD-CPT.
           EXIT.
       
       PRO-KEY-CPT SECTION.
       INI-PRO-KEY-CPT.
*% INCLUDE INI_PRO_KEY_CPT OF 'COLBMCPT.TXT'
           MOVE FRM-CPIM-AEY TO FRM-VAL-PREP-ATR IN CPT-FLD.
           PERFORM PRO-MKY-CPT.
*% INCLUDE FIN_PRO_KEY_CPT OF 'COLBMCPT.TXT'
       FIN-PRO-KEY-CPT.
           EXIT.
       
       PRO-MKY-CPT SECTION.
       INI-PRO-MKY-CPT.
*% INCLUDE INI_PRO_MKY_CPT OF 'COLBMCPT.TXT'
*% INCLUDE FIN_PRO_MKY_CPT OF 'COLBMCPT.TXT'
       FIN-PRO-MKY-CPT.
           EXIT.
       
       PRO-FLD-CPT SECTION.
       INI-PRO-FLD-CPT.
*% INCLUDE INI_PRO_FLD_CPT OF 'COLBMCPT.TXT'
           PERFORM PRO-OPC-CPT.
           PERFORM PRO-DBC-CPT.
           PERFORM PRO-DLC-CPT.
*% INCLUDE FIN_PRO_FLD_CPT OF 'COLBMCPT.TXT'
       FIN-PRO-FLD-CPT.
           EXIT.
       
       PRO-OPC-CPT SECTION.
       INI-PRO-OPC-CPT.
*% INCLUDE INI_PRO_OPC_CPT OF 'COLBMCPT.TXT'
*% INCLUDE FIN_PRO_OPC_CPT OF 'COLBMCPT.TXT'
       FIN-PRO-OPC-CPT.
           EXIT.
       
       PRO-DBC-CPT SECTION.
       INI-PRO-DBC-CPT.
*% INCLUDE INI_PRO_DBC_CPT OF 'COLBMCPT.TXT'
*% INCLUDE FIN_PRO_DBC_CPT OF 'COLBMCPT.TXT'
       FIN-PRO-DBC-CPT.
           EXIT.
       
