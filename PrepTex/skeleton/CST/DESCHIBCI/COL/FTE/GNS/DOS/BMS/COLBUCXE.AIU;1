*% FRM = "CXE"
      *
      * COLBUCXE
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:49 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMCXE.TXT'
       SET-TAG-COLBUCXE SECTION.
       INI-SET-TAG-COLBUCXE.
           MOVE
           '<IDNMOD NOM=$COLBUCXE$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:49 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCXE.
           EXIT.
           
       RET-ALL-CXE SECTION.
       INI-RET-ALL-CXE.
*% INCLUDE INI_RET_ALL_CXE OF 'COLBMCXE.TXT'
           PERFORM KEY-ALL-CXE.
           PERFORM RET-CXE-CXE.
*% INCLUDE FIN_RET_ALL_CXE OF 'COLBMCXE.TXT'
       FIN-RET-ALL-CXE.
           EXIT.
       
       RET-CXE-CXE SECTION.
       INI-RET-CXE-CXE.
*% INCLUDE INI_RET_CXE_CXE OF 'COLBMCXE.TXT'
           MOVE CXE-COD-TINT IN CXE-FLD TO CXE-COD-TINT IN CXE.
           MOVE CXE-IND-CBRO IN CXE-FLD TO CXE-IND-CBRO IN CXE.
           MOVE CXE-SGV-TINT IN CXE-FLD TO CXE-SGV-TINT IN CXE.
*% RREG = "CXE"
*% RREG_FRM = "CXE"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CXE-NUM-DVEN IN CXE-FLD TO CXE-NUM-DVEN IN CXE.
           MOVE CXE-NUM-MVEN IN CXE-FLD TO CXE-NUM-MVEN IN CXE.
           MOVE CXE-NUM-SVEN IN CXE-FLD TO CXE-NUM-SVEN IN CXE.
           MOVE CXE-NUM-AVEN IN CXE-FLD TO CXE-NUM-AVEN IN CXE.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CXE-SGV-MNTO IN CXE-FLD TO CXE-SGV-MNTO IN CXE.
           MOVE CXE-IND-FOGA IN CXE-FLD TO CXE-IND-FOGA IN CXE.
           MOVE CXE-SGV-MCAL IN CXE-FLD TO CXE-SGV-MCAL IN CXE.
           MOVE CXE-IND-DCTO IN CXE-FLD TO CXE-IND-DCTO IN CXE.
           MOVE CXE-COD-VCOC IN CXE-FLD TO CXE-COD-VCOC IN CXE.
           MOVE CXE-SGV-MCAM IN CXE-FLD TO CXE-SGV-MCAM IN CXE.
           MOVE CXE-CAI-IPLA IN CXE-FLD TO CXE-CAI-IPLA IN CXE.
           MOVE CXE-IIC-IPLA IN CXE-FLD TO CXE-IIC-IPLA IN CXE.
           MOVE CXE-COD-EVEX IN CXE-FLD TO CXE-COD-EVEX IN CXE.
*% RREG = "CXE"
*% RREG_FRM = "CXE"
*% NOMB = "FPLA"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CXE-NUM-DPLA IN CXE-FLD TO CXE-NUM-DPLA IN CXE.
           MOVE CXE-NUM-MPLA IN CXE-FLD TO CXE-NUM-MPLA IN CXE.
           MOVE CXE-NUM-SPLA IN CXE-FLD TO CXE-NUM-SPLA IN CXE.
           MOVE CXE-NUM-APLA IN CXE-FLD TO CXE-NUM-APLA IN CXE.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CXE-HRA-HPLA IN CXE-FLD TO CXE-HRA-HPLA IN CXE.
           MOVE CXE-NUM-IPLA IN CXE-FLD TO CXE-NUM-IPLA IN CXE.
           MOVE CXE-SGV-SALD IN CXE-FLD TO CXE-SGV-SALD IN CXE.
*% INCLUDE FIN_RET_CXE_CXE OF 'COLBMCXE.TXT'
       FIN-RET-CXE-CXE.
           EXIT.
       
       KEY-ALL-CXE SECTION.
       INI-KEY-ALL-CXE.
*% INCLUDE INI_KEY_ALL_CXE OF 'COLBMCXE.TXT'
           PERFORM KEY-CXE-CXE.
*% INCLUDE FIN_KEY_ALL_CXE OF 'COLBMCXE.TXT'
       FIN-KEY-ALL-CXE.
           EXIT.
       
       KEY-CXE-CXE SECTION.
       INI-KEY-CXE-CXE.
*% INCLUDE INI_KEY_CXE_CXE OF 'COLBMCXE.TXT'
           MOVE CXE-CAI-IOPC IN CXE-FLD TO CXE-CAI-IOPC IN CXE.
           MOVE CXE-IIC-IOPC IN CXE-FLD TO CXE-IIC-IOPC IN CXE.
           MOVE CXE-CAI-IOPV IN CXE-FLD TO CXE-CAI-IOPV IN CXE.
           MOVE CXE-IIC-IOPV IN CXE-FLD TO CXE-IIC-IOPV IN CXE.
           MOVE CXE-COD-EVEN IN CXE-FLD TO CXE-COD-EVEN IN CXE.
           MOVE CXE-NUM-IVCT IN CXE-FLD TO CXE-NUM-IVCT IN CXE.
           MOVE CXE-NUM-EVEN IN CXE-FLD TO CXE-NUM-EVEN IN CXE.
           MOVE CXE-COD-CPTO IN CXE-FLD TO CXE-COD-CPTO IN CXE.
*% INCLUDE FIN_KEY_CXE_CXE OF 'COLBMCXE.TXT'
       FIN-KEY-CXE-CXE.
           EXIT.
       
       SCR-CLRA-CXE SECTION.
       INI-SCR-CLRA-CXE.
*% INCLUDE INI_SCR_CLRA_CXE OF 'COLBMCXE.TXT'
           PERFORM CLR-KEY-CXE.
           PERFORM CLR-FLD-CXE.
*% INCLUDE FIN_SCR_CLRA_CXE OF 'COLBMCXE.TXT'
       FIN-SCR-CLRA-CXE.
           EXIT.
       
       CLR-KEY-CXE SECTION.
       INI-CLR-KEY-CXE.
*% INCLUDE INI_CLR_KEY_CXE OF 'COLBMCXE.TXT'
           MOVE SPACES TO CXE-CAI-IOPC IN CXE-FLD.
           MOVE ZEROES TO CXE-IIC-IOPC IN CXE-FLD.
           MOVE SPACES TO CXE-CAI-IOPV IN CXE-FLD.
           MOVE SPACES TO CXE-IIC-IOPV IN CXE-FLD.
           MOVE SPACES TO CXE-COD-EVEN IN CXE-FLD.
           MOVE ZEROES TO CXE-NUM-IVCT IN CXE-FLD.
           MOVE ZEROES TO CXE-NUM-EVEN IN CXE-FLD.
           MOVE SPACES TO CXE-COD-CPTO IN CXE-FLD.
*% INCLUDE FIN_CLR_KEY_CXE OF 'COLBMCXE.TXT'
       FIN-CLR-KEY-CXE.
           EXIT.
       
       CLR-FLD-CXE SECTION.
       INI-CLR-FLD-CXE.
*% INCLUDE INI_CLR_FLD_CXE OF 'COLBMCXE.TXT'
           MOVE SPACES TO FRM-GLS-TINT IN CXE-FLD.
           MOVE SPACES TO FRM-GLS-CBRO IN CXE-FLD.
           MOVE SPACES TO FRM-GLS-TTNT IN CXE-FLD.
           MOVE SPACES TO FRM-GLS-FVEN IN CXE-FLD.
           MOVE SPACES TO FRM-GLS-MNTO IN CXE-FLD.
           MOVE SPACES TO FRM-GLS-FOGA IN CXE-FLD.
           MOVE SPACES TO FRM-GLS-MCAL IN CXE-FLD.
           MOVE SPACES TO FRM-GLS-DCTO IN CXE-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN CXE-FLD.
           MOVE SPACES TO FRM-GLS-MCAM IN CXE-FLD.
           MOVE SPACES TO FRM-GLS-IPLA IN CXE-FLD.
           MOVE SPACES TO FRM-GLS-EVEX IN CXE-FLD.
           MOVE SPACES TO FRM-GLS-FPLA IN CXE-FLD.
           MOVE SPACES TO FRM-GLS-HPLA IN CXE-FLD.
           MOVE SPACES TO FRM-GLS-PLAN IN CXE-FLD.
           MOVE SPACES TO FRM-GLS-SALD IN CXE-FLD.
           PERFORM CLR-CXE-CXE.
*% INCLUDE FIN_CLR_FLD_CXE OF 'COLBMCXE.TXT'
       FIN-CLR-FLD-CXE.
           EXIT.
       
       CLR-CXE-CXE SECTION.
       INI-CLR-CXE-CXE.
*% INCLUDE INI_CLR_CXE_CXE OF 'COLBMCXE.TXT'
           MOVE SPACES TO CXE-COD-TINT IN CXE-FLD.
           MOVE SPACES TO CXE-IND-CBRO IN CXE-FLD.
           MOVE ZEROES TO CXE-SGV-TINT IN CXE-FLD.
*% RREG = "CXE"
*% RREG_FRM = "CXE"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CXE-FEC-FVEN IN CXE-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CXE-SGV-MNTO IN CXE-FLD.
           MOVE SPACES TO CXE-IND-FOGA IN CXE-FLD.
           MOVE ZEROES TO CXE-SGV-MCAL IN CXE-FLD.
           MOVE SPACES TO CXE-IND-DCTO IN CXE-FLD.
           MOVE SPACES TO CXE-COD-VCOC IN CXE-FLD.
           MOVE ZEROES TO CXE-SGV-MCAM IN CXE-FLD.
           MOVE SPACES TO CXE-CAI-IPLA IN CXE-FLD.
           MOVE SPACES TO CXE-IIC-IPLA IN CXE-FLD.
           MOVE SPACES TO CXE-COD-EVEX IN CXE-FLD.
*% RREG = "CXE"
*% RREG_FRM = "CXE"
*% NOMB = "FPLA"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CXE-FEC-FPLA IN CXE-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CXE-HRA-HPLA IN CXE-FLD.
           MOVE ZEROES TO CXE-NUM-IPLA IN CXE-FLD.
           MOVE ZEROES TO CXE-SGV-SALD IN CXE-FLD.
*% INCLUDE FIN_CLR_CXE_CXE OF 'COLBMCXE.TXT'
       FIN-CLR-CXE-CXE.
           EXIT.
       
       FST-KEY-CXE SECTION.
       INI-FST-KEY-CXE.
*% INCLUDE INI_FST_KEY_CXE OF 'COLBMCXE.TXT'
           MOVE -1 TO CXE-CAI-IOPC-LEN IN CXE-FLD.
*% INCLUDE FIN_FST_KEY_CXE OF 'COLBMCXE.TXT'
       FIN-FST-KEY-CXE.
           EXIT.
       
       FST-FLD-CXE SECTION.
       INI-FST-FLD-CXE.
*% INCLUDE INI_FST_FLD_CXE OF 'COLBMCXE.TXT'
           MOVE -1 TO CXE-COD-TINT-LEN IN CXE-FLD.
*% INCLUDE FIN_FST_FLD_CXE OF 'COLBMCXE.TXT'
       FIN-FST-FLD-CXE.
           EXIT.
       
       PRO-KEY-CXE SECTION.
       INI-PRO-KEY-CXE.
*% INCLUDE INI_PRO_KEY_CXE OF 'COLBMCXE.TXT'
           PERFORM PRO-MKY-CXE.
*% INCLUDE FIN_PRO_KEY_CXE OF 'COLBMCXE.TXT'
       FIN-PRO-KEY-CXE.
           EXIT.
       
       PRO-MKY-CXE SECTION.
       INI-PRO-MKY-CXE.
*% INCLUDE INI_PRO_MKY_CXE OF 'COLBMCXE.TXT'
*% INCLUDE FIN_PRO_MKY_CXE OF 'COLBMCXE.TXT'
       FIN-PRO-MKY-CXE.
           EXIT.
       
       PRO-FLD-CXE SECTION.
       INI-PRO-FLD-CXE.
*% INCLUDE INI_PRO_FLD_CXE OF 'COLBMCXE.TXT'
           PERFORM PRO-CXE-CXE.
*% INCLUDE FIN_PRO_FLD_CXE OF 'COLBMCXE.TXT'
       FIN-PRO-FLD-CXE.
           EXIT.
       
       PRO-CXE-CXE SECTION.
       INI-PRO-CXE-CXE.
*% INCLUDE INI_PRO_CXE_CXE OF 'COLBMCXE.TXT'
           MOVE FRM-CPIM-AEY TO CXE-COD-TINT-ATR IN CXE-FLD.
           MOVE FRM-CPIM-AEY TO CXE-IND-CBRO-ATR IN CXE-FLD.
           MOVE FRM-CPIM-AEY TO CXE-SGV-TINT-ATR IN CXE-FLD.
*% RREG = "CXE"
*% RREG_FRM = "CXE"
*% NOMB = "FVEN"
           MOVE FRM-CPIM-AEY TO CXE-FEC-FVEN-ATR IN CXE-FLD.
           MOVE FRM-CPIM-AEY TO CXE-SGV-MNTO-ATR IN CXE-FLD.
           MOVE FRM-CPIM-AEY TO CXE-IND-FOGA-ATR IN CXE-FLD.
           MOVE FRM-CPIM-AEY TO CXE-SGV-MCAL-ATR IN CXE-FLD.
           MOVE FRM-CPIM-AEY TO CXE-IND-DCTO-ATR IN CXE-FLD.
           MOVE FRM-CPIM-AEY TO CXE-COD-VCOC-ATR IN CXE-FLD.
           MOVE FRM-CPIM-AEY TO CXE-SGV-MCAM-ATR IN CXE-FLD.
           MOVE FRM-CPIM-AEY TO CXE-CAI-IPLA-ATR IN CXE-FLD.
           MOVE FRM-CPIM-AEY TO CXE-IIC-IPLA-ATR IN CXE-FLD.
           MOVE FRM-CPIM-AEY TO CXE-COD-EVEX-ATR IN CXE-FLD.
*% RREG = "CXE"
*% RREG_FRM = "CXE"
*% NOMB = "FPLA"
           MOVE FRM-CPIM-AEY TO CXE-FEC-FPLA-ATR IN CXE-FLD.
           MOVE FRM-CPIM-AEY TO CXE-HRA-HPLA-ATR IN CXE-FLD.
           MOVE FRM-CPIM-AEY TO CXE-NUM-IPLA-ATR IN CXE-FLD.
           MOVE FRM-CPIM-AEY TO CXE-SGV-SALD-ATR IN CXE-FLD.
*% INCLUDE FIN_PRO_CXE_CXE OF 'COLBMCXE.TXT'
       FIN-PRO-CXE-CXE.
           EXIT.
       
       UNP-KEY-CXE SECTION.
       INI-UNP-KEY-CXE.
*% INCLUDE INI_UNP_KEY_CXE OF 'COLBMCXE.TXT'
           PERFORM UNP-IKY-CXE.
           PERFORM UNP-AKY-CXE.
           PERFORM UNP-MKY-CXE.
*% INCLUDE FIN_UNP_KEY_CXE OF 'COLBMCXE.TXT'
       FIN-UNP-KEY-CXE.
           EXIT.
       
       UNP-IKY-CXE SECTION.
       INI-UNP-IKY-CXE.
*% INCLUDE INI_UNP_IKY_CXE OF 'COLBMCXE.TXT'
           MOVE FRM-CPIM-UBY TO CXE-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO CXE-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO CXE-CAI-IOPV-ATR.
           MOVE FRM-CPIM-UBY TO CXE-IIC-IOPV-ATR.
           MOVE FRM-CPIM-UBY TO CXE-COD-EVEN-ATR.
           MOVE FRM-CPIM-UBY TO CXE-NUM-IVCT-ATR.
           MOVE FRM-CPIM-UBY TO CXE-NUM-EVEN-ATR.
           MOVE FRM-CPIM-UBY TO CXE-COD-CPTO-ATR.
*% INCLUDE FIN_UNP_IKY_CXE OF 'COLBMCXE.TXT'
       FIN-UNP-IKY-CXE.
           EXIT.
       
       UNP-MKY-CXE SECTION.
       INI-UNP-MKY-CXE.
*% INCLUDE INI_UNP_MKY_CXE OF 'COLBMCXE.TXT'
*% INCLUDE FIN_UNP_MKY_CXE OF 'COLBMCXE.TXT'
       FIN-UNP-MKY-CXE.
           EXIT.
       
       UNP-AKY-CXE SECTION.
       INI-UNP-AKY-CXE.
*% INCLUDE INI_UNP_AKY_CXE OF 'COLBMCXE.TXT'
*% INCLUDE FIN_UNP_AKY_CXE OF 'COLBMCXE.TXT'
       FIN-UNP-AKY-CXE.
           EXIT.
       
       UNP-FLD-CXE SECTION.
       INI-UNP-FLD-CXE.
*% INCLUDE INI_UNP_FLD_CXE OF 'COLBMCXE.TXT'
           PERFORM UNP-CXE-CXE.
*% INCLUDE FIN_UNP_FLD_CXE OF 'COLBMCXE.TXT'
       FIN-UNP-FLD-CXE.
           EXIT.
       
       UNP-CXE-CXE SECTION.
       INI-UNP-CXE-CXE.
*% INCLUDE INI_UNP_CXE_CXE OF 'COLBMCXE.TXT'
           MOVE FRM-CPIM-UBY TO CXE-COD-TINT-ATR IN CXE-FLD.
           MOVE FRM-CPIM-UBY TO CXE-IND-CBRO-ATR IN CXE-FLD.
           MOVE FRM-CPIM-UBY TO CXE-SGV-TINT-ATR IN CXE-FLD.
*% RREG = "CXE"
*% RREG_FRM = "CXE"
*% NOMB = "FVEN"
           MOVE FRM-CPIM-UBY TO CXE-FEC-FVEN-ATR IN CXE-FLD.
           MOVE FRM-CPIM-UBY TO CXE-SGV-MNTO-ATR IN CXE-FLD.
           MOVE FRM-CPIM-UBY TO CXE-IND-FOGA-ATR IN CXE-FLD.
           MOVE FRM-CPIM-UBY TO CXE-SGV-MCAL-ATR IN CXE-FLD.
           MOVE FRM-CPIM-UBY TO CXE-IND-DCTO-ATR IN CXE-FLD.
           MOVE FRM-CPIM-UBY TO CXE-COD-VCOC-ATR IN CXE-FLD.
           MOVE FRM-CPIM-UBY TO CXE-SGV-MCAM-ATR IN CXE-FLD.
           MOVE FRM-CPIM-UBY TO CXE-CAI-IPLA-ATR IN CXE-FLD.
           MOVE FRM-CPIM-UBY TO CXE-IIC-IPLA-ATR IN CXE-FLD.
           MOVE FRM-CPIM-UBY TO CXE-COD-EVEX-ATR IN CXE-FLD.
*% RREG = "CXE"
*% RREG_FRM = "CXE"
*% NOMB = "FPLA"
           MOVE FRM-CPIM-UBY TO CXE-FEC-FPLA-ATR IN CXE-FLD.
           MOVE FRM-CPIM-UBY TO CXE-HRA-HPLA-ATR IN CXE-FLD.
           MOVE FRM-CPIM-UBY TO CXE-NUM-IPLA-ATR IN CXE-FLD.
           MOVE FRM-CPIM-UBY TO CXE-SGV-SALD-ATR IN CXE-FLD.
*% INCLUDE FIN_UNP_CXE_CXE OF 'COLBMCXE.TXT'
       FIN-UNP-CXE-CXE.
           EXIT.
       
       VAL-MKY-CXE SECTION.
       INI-VAL-MKY-CXE.
*% INCLUDE INI_VAL_MKY_CXE OF 'COLBMCXE.TXT'
*% INCLUDE FIN_VAL_MKY_CXE OF 'COLBMCXE.TXT'
       FIN-VAL-MKY-CXE.
           EXIT.
       
       VAL-FLD-CXE SECTION.
       INI-VAL-FLD-CXE.
*% INCLUDE INI_VAL_FLD_CXE OF 'COLBMCXE.TXT'
           PERFORM VAL-FRM-GLS-TINT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TINT-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-CXE-COD-TINT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CXE-COD-TINT-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-FRM-GLS-CBRO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-CBRO-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-CXE-IND-CBRO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CXE-IND-CBRO-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-FRM-GLS-TTNT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TTNT-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-CXE-SGV-TINT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CXE-SGV-TINT-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-FRM-GLS-FVEN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-FVEN-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
*% RREG = "CXE"
*% RREG_FRM = "CXE"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-CXE-FEC-FVEN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                CXE-FEC-FVEN-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-GLS-MNTO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-MNTO-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-CXE-SGV-MNTO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CXE-SGV-MNTO-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-FRM-GLS-FOGA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-FOGA-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-CXE-IND-FOGA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CXE-IND-FOGA-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-FRM-GLS-MCAL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-MCAL-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-CXE-SGV-MCAL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CXE-SGV-MCAL-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-FRM-GLS-DCTO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-DCTO-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-CXE-IND-DCTO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CXE-IND-DCTO-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-FRM-GLS-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-VCOC-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-CXE-COD-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CXE-COD-VCOC-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-FRM-GLS-MCAM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-MCAM-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-CXE-SGV-MCAM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CXE-SGV-MCAM-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-FRM-GLS-IPLA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-IPLA-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-CXE-CAI-IPLA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CXE-CAI-IPLA-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-CXE-IIC-IPLA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CXE-IIC-IPLA-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-FRM-GLS-EVEX.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-EVEX-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-CXE-COD-EVEX.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CXE-COD-EVEX-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-FRM-GLS-FPLA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-FPLA-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
*% RREG = "CXE"
*% RREG_FRM = "CXE"
*% NOMB = "FPLA"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-CXE-FEC-FPLA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                CXE-FEC-FPLA-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-GLS-HPLA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-HPLA-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-CXE-HRA-HPLA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CXE-HRA-HPLA-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-FRM-GLS-PLAN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-PLAN-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-CXE-NUM-IPLA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CXE-NUM-IPLA-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-FRM-GLS-SALD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-SALD-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
           PERFORM VAL-CXE-SGV-SALD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CXE-SGV-SALD-LEN IN CXE-FLD
                GO TO FIN-VAL-FLD-CXE.
*% INCLUDE FIN_VAL_FLD_CXE OF 'COLBMCXE.TXT'
       FIN-VAL-FLD-CXE.
           EXIT.
       
