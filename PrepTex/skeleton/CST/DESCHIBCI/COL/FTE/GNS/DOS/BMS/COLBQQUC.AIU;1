      *
      * COLBQQUC
      * Version 3.0.1 Nov-2005
      * 23-Jan-17 07:16 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "QUC"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMQUC.TXT'
       SET-TAG-COLBQQUC SECTION.
       INI-SET-TAG-COLBQQUC.
           MOVE
           '<IDNMOD NOM=$COLBQQUC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$23-Jan-17 07:16 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQQUC.
           EXIT.
           
       PUT-ALL-QUC SECTION.
       INI-PUT-ALL-QUC.
*% INCLUDE INI_PUT_ALL_QUC OF 'COLBMQUC.TXT'
           PERFORM PUT-OPC-QUC.
           PERFORM PUT-ITR-QUC.
           PERFORM PUT-VEN-QUC.
           PERFORM PUT-AUT-QUC.
           PERFORM PUT-EVC-QUC.
           PERFORM PUT-DLC-QUC.
*% INCLUDE FIN_PUT_ALL_QUC OF 'COLBMQUC.TXT'
       FIN-PUT-ALL-QUC.
           EXIT.
       
       PUT-OPC-QUC SECTION.
       INI-PUT-OPC-QUC.
*% INCLUDE INI_PUT_OPC_QUC OF 'COLBMQUC.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-QUC.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-QUC
               GO TO FIN-PUT-OPC-QUC.
           MOVE OPC-CAI-IOPC IN OPC TO OPC-CAI-IOPC IN QUC-FLD.
           MOVE OPC-IIC-IOPC IN OPC TO OPC-IIC-IOPC IN QUC-FLD.
           MOVE OPC-COD-TOOC IN OPC TO OPC-COD-TOOC IN QUC-FLD.
           MOVE OPC-COD-VCOC IN OPC TO OPC-COD-VCOC IN QUC-FLD.
           MOVE OPC-COD-AOOC IN OPC TO OPC-COD-AOOC IN QUC-FLD.
           MOVE OPC-VAL-CRED IN OPC TO OPC-VAL-CRED IN QUC-FLD.
           MOVE OPC-VAL-SCRE IN OPC TO OPC-VAL-SCRE IN QUC-FLD.
           MOVE OPC-NUM-VCOR IN OPC TO OPC-NUM-VCOR IN QUC-FLD.
           MOVE OPC-NUM-IVEN IN OPC TO OPC-NUM-IVEN IN QUC-FLD.
           MOVE OPC-IND-GARA IN OPC TO OPC-IND-GARA IN QUC-FLD.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE OPC-NUM-DCOL IN OPC TO OPC-NUM-DCOL IN QUC-FLD.
           MOVE OPC-NUM-MCOL IN OPC TO OPC-NUM-MCOL IN QUC-FLD.
           MOVE OPC-NUM-SCOL IN OPC TO OPC-NUM-SCOL IN QUC-FLD.
           MOVE OPC-NUM-ACOL IN OPC TO OPC-NUM-ACOL IN QUC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FING"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE OPC-NUM-DING IN OPC TO OPC-NUM-DING IN QUC-FLD.
           MOVE OPC-NUM-MING IN OPC TO OPC-NUM-MING IN QUC-FLD.
           MOVE OPC-NUM-SING IN OPC TO OPC-NUM-SING IN QUC-FLD.
           MOVE OPC-NUM-AING IN OPC TO OPC-NUM-AING IN QUC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% BUF = "QUC-FLD"
*% SIS = "TAB"
*% TAB = "TIO "
*% FLD = "OPC-COD-TOOC"
*% GLS = "FRM-GLS-TOOC"
*% TIENE_GLOSA = TRUE
*% LGL = 14
*% INCLUDE 'BUSTAB'
*% BUF = "QUC-FLD"
*% SIS = "TAB"
*% TAB = "VLR "
*% FLD = "OPC-COD-VCOC"
*% GLS = "FRM-GLS-VCOC"
*% TIENE_GLOSA = TRUE
*% LGL = 15
*% INCLUDE 'BUSTAB'
*% BUF = "QUC-FLD"
*% SIS = "COL"
*% TAB = "GARA"
*% FLD = "OPC-IND-GARA"
*% GLS = "FRM-GLS-GARA"
*% TIENE_GLOSA = TRUE
*% LGL = 27
*% INCLUDE 'BUSMSC'
*% INCLUDE FIN_PUT_OPC_QUC OF 'COLBMQUC.TXT'
       FIN-PUT-OPC-QUC.
           EXIT.
       
       PUT-ITR-QUC SECTION.
       INI-PUT-ITR-QUC.
*% INCLUDE INI_PUT_ITR_QUC OF 'COLBMQUC.TXT'
           IF PGM-STAT-ITR-NUL
               GO TO FIN-PUT-ITR-QUC.
           IF NOT PGM-STAT-ITR-OKS
               PERFORM CLR-ITR-QUC
               GO TO FIN-PUT-ITR-QUC.
           MOVE ITR-SGV-TINT IN ITR TO ITR-SGV-TINT IN QUC-FLD.
           MOVE ITR-COD-TINT IN ITR TO ITR-COD-TINT IN QUC-FLD.
*% BUF = "QUC-FLD"
*% SIS = "COL"
*% TAB = "INT "
*% FLD = "ITR-COD-TINT"
*% GLS = "FRM-GLS-TINT"
*% TIENE_GLOSA = TRUE
*% LGL = 36
*% INCLUDE 'BUSTAB'
*% INCLUDE FIN_PUT_ITR_QUC OF 'COLBMQUC.TXT'
       FIN-PUT-ITR-QUC.
           EXIT.
       
       PUT-VEN-QUC SECTION.
       INI-PUT-VEN-QUC.
*% INCLUDE INI_PUT_VEN_QUC OF 'COLBMQUC.TXT'
           IF PGM-STAT-VEN-NUL
               GO TO FIN-PUT-VEN-QUC.
           IF NOT PGM-STAT-VEN-OKS
               PERFORM CLR-VEN-QUC
               GO TO FIN-PUT-VEN-QUC.
           MOVE VEN-VAL-SFIN IN VEN TO VEN-VAL-SFIN IN QUC-FLD.
*% RREG = "VEN"
*% RREG_FRM = "VEN"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE VEN-NUM-DVEN IN VEN TO VEN-NUM-DVEN IN QUC-FLD.
           MOVE VEN-NUM-MVEN IN VEN TO VEN-NUM-MVEN IN QUC-FLD.
           MOVE VEN-NUM-SVEN IN VEN TO VEN-NUM-SVEN IN QUC-FLD.
           MOVE VEN-NUM-AVEN IN VEN TO VEN-NUM-AVEN IN QUC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% INCLUDE FIN_PUT_VEN_QUC OF 'COLBMQUC.TXT'
       FIN-PUT-VEN-QUC.
           EXIT.
       
       PUT-AUT-QUC SECTION.
       INI-PUT-AUT-QUC.
*% INCLUDE INI_PUT_AUT_QUC OF 'COLBMQUC.TXT'
           IF PGM-STAT-AUT-NUL
               GO TO FIN-PUT-AUT-QUC.
           IF NOT PGM-STAT-AUT-OKS
               PERFORM CLR-AUT-QUC
               GO TO FIN-PUT-AUT-QUC.
           MOVE AUT-NUM-ICAN IN AUT TO AUT-NUM-ICAN IN QUC-FLD.
*% INCLUDE FIN_PUT_AUT_QUC OF 'COLBMQUC.TXT'
       FIN-PUT-AUT-QUC.
           EXIT.
       
       PUT-EVC-QUC SECTION.
       INI-PUT-EVC-QUC.
*% INCLUDE INI_PUT_EVC_QUC OF 'COLBMQUC.TXT'
           IF PGM-STAT-EVC-NUL
               GO TO FIN-PUT-EVC-QUC.
           IF NOT PGM-STAT-EVC-OKS
               PERFORM CLR-EVC-QUC
               GO TO FIN-PUT-EVC-QUC.
           MOVE EVC-NUM-SEVE IN EVC TO EVC-NUM-SEVE IN QUC-FLD.
           MOVE EVC-IND-UTSV IN EVC TO EVC-IND-UTSV IN QUC-FLD.
*% INCLUDE FIN_PUT_EVC_QUC OF 'COLBMQUC.TXT'
       FIN-PUT-EVC-QUC.
           EXIT.
       
       PUT-DLC-QUC SECTION.
       INI-PUT-DLC-QUC.
*% INCLUDE INI_PUT_DLC_QUC OF 'COLBMQUC.TXT'
           IF PGM-STAT-DLC-NUL
               GO TO FIN-PUT-DLC-QUC.
           IF NOT PGM-STAT-DLC-OKS
               PERFORM CLR-DLC-QUC
               GO TO FIN-PUT-DLC-QUC.
*% RREG = "DLC"
*% RREG_FRM = "DLC"
*% NOMB = "FMAV"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DLC-NUM-DMAV IN DLC TO DLC-NUM-DMAV IN QUC-FLD.
           MOVE DLC-NUM-MMAV IN DLC TO DLC-NUM-MMAV IN QUC-FLD.
           MOVE DLC-NUM-SMAV IN DLC TO DLC-NUM-SMAV IN QUC-FLD.
           MOVE DLC-NUM-AMAV IN DLC TO DLC-NUM-AMAV IN QUC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% RREG = "DLC"
*% RREG_FRM = "DLC"
*% NOMB = "FDEV"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DLC-NUM-DDEV IN DLC TO DLC-NUM-DDEV IN QUC-FLD.
           MOVE DLC-NUM-MDEV IN DLC TO DLC-NUM-MDEV IN QUC-FLD.
           MOVE DLC-NUM-SDEV IN DLC TO DLC-NUM-SDEV IN QUC-FLD.
           MOVE DLC-NUM-ADEV IN DLC TO DLC-NUM-ADEV IN QUC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% INCLUDE FIN_PUT_DLC_QUC OF 'COLBMQUC.TXT'
       FIN-PUT-DLC-QUC.
           EXIT.
       
       KEY-ALL-QUC SECTION.
       INI-KEY-ALL-QUC.
*% INCLUDE INI_KEY_ALL_QUC OF 'COLBMQUC.TXT'
           PERFORM KEY-OPC-QUC.
           PERFORM KEY-ITR-QUC.
           PERFORM KEY-VEN-QUC.
           PERFORM KEY-AUT-QUC.
           PERFORM KEY-EVC-QUC.
           PERFORM KEY-DLC-QUC.
*% INCLUDE FIN_KEY_ALL_QUC OF 'COLBMQUC.TXT'
       FIN-KEY-ALL-QUC.
           EXIT.
       
       KEY-OPC-QUC SECTION.
       INI-KEY-OPC-QUC.
*% INCLUDE INI_KEY_OPC_QUC OF 'COLBMQUC.TXT'
           MOVE OPC-CAI-IOPC IN QUC-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE OPC-IIC-IOPC IN QUC-FLD TO OPC-IIC-IOPC IN OPC.
*% INCLUDE FIN_KEY_OPC_QUC OF 'COLBMQUC.TXT'
       FIN-KEY-OPC-QUC.
           EXIT.
       
       KEY-ITR-QUC SECTION.
       INI-KEY-ITR-QUC.
*% INCLUDE INI_KEY_ITR_QUC OF 'COLBMQUC.TXT'
*% INCLUDE FIN_KEY_ITR_QUC OF 'COLBMQUC.TXT'
       FIN-KEY-ITR-QUC.
           EXIT.
       
       KEY-VEN-QUC SECTION.
       INI-KEY-VEN-QUC.
*% INCLUDE INI_KEY_VEN_QUC OF 'COLBMQUC.TXT'
*% INCLUDE FIN_KEY_VEN_QUC OF 'COLBMQUC.TXT'
       FIN-KEY-VEN-QUC.
           EXIT.
       
       KEY-AUT-QUC SECTION.
       INI-KEY-AUT-QUC.
*% INCLUDE INI_KEY_AUT_QUC OF 'COLBMQUC.TXT'
*% INCLUDE FIN_KEY_AUT_QUC OF 'COLBMQUC.TXT'
       FIN-KEY-AUT-QUC.
           EXIT.
       
