*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMCPP.TXT'
       SET-TAG-COLBUCPP SECTION.
       INI-SET-TAG-COLBUCPP.
           MOVE
           '<IDNMOD NOM=$COLBUCPP$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:31 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCPP.
           EXIT.
           
       RET-ALL-CPP SECTION.
       INI-RET-ALL-CPP.
*% INCLUDE INI_RET_ALL_CPP OF 'COLBMCPP.TXT'
           PERFORM KEY-ALL-CPP.
           PERFORM RET-CPP-CPP.
*% INCLUDE FIN_RET_ALL_CPP OF 'COLBMCPP.TXT'
       FIN-RET-ALL-CPP.
           EXIT.
       
       RET-CPP-CPP SECTION.
       INI-RET-CPP-CPP.
