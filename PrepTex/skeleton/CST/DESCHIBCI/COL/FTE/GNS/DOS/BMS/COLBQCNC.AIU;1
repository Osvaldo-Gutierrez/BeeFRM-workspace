      *
      * COLBQCNC
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:28 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CNC"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMCNC.TXT'
       SET-TAG-COLBQCNC SECTION.
       INI-SET-TAG-COLBQCNC.
           MOVE
           '<IDNMOD NOM=$COLBQCNC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:28 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCNC.
           EXIT.
           
       PUT-ALL-CNC SECTION.
       INI-PUT-ALL-CNC.
*% INCLUDE INI_PUT_ALL_CNC OF 'COLBMCNC.TXT'
           PERFORM PUT-CNC-CNC.
*% INCLUDE FIN_PUT_ALL_CNC OF 'COLBMCNC.TXT'
       FIN-PUT-ALL-CNC.
           EXIT.
       
       PUT-CNC-CNC SECTION.
       INI-PUT-CNC-CNC.
*% INCLUDE INI_PUT_CNC_CNC OF 'COLBMCNC.TXT'
           IF PGM-STAT-CNC-NUL
               GO TO FIN-PUT-CNC-CNC.
           IF NOT PGM-STAT-CNC-OKS
               PERFORM CLR-CNC-CNC
               GO TO FIN-PUT-CNC-CNC.
           MOVE CNC-MSC-TCAN IN CNC TO CNC-MSC-TCAN IN CNC-FLD.
           MOVE CNC-MSC-FCAN IN CNC TO CNC-MSC-FCAN IN CNC-FLD.
           MOVE CNC-MSC-ECAN IN CNC TO CNC-MSC-ECAN IN CNC-FLD.
           MOVE CNC-CIC-CANC IN CNC TO CNC-CIC-CANC IN CNC-FLD.
           MOVE CNC-EXT-CTAB IN CNC TO CNC-EXT-CTAB IN CNC-FLD.
           MOVE CNC-GLS-DESC IN CNC TO CNC-GLS-DESC IN CNC-FLD.
           MOVE CNC-GLS-ABRV IN CNC TO CNC-GLS-ABRV IN CNC-FLD.
           MOVE CNC-GLS-DCOR IN CNC TO CNC-GLS-DCOR IN CNC-FLD.
           MOVE CNC-IND-VIGE IN CNC TO CNC-IND-VIGE IN CNC-FLD.
*% RREG = "CNC"
*% RREG_FRM = "CNC"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CNC-NUM-DTRN IN CNC TO CNC-NUM-DTRN IN CNC-FLD.
           MOVE CNC-NUM-MTRN IN CNC TO CNC-NUM-MTRN IN CNC-FLD.
           MOVE CNC-NUM-STRN IN CNC TO CNC-NUM-STRN IN CNC-FLD.
           MOVE CNC-NUM-ATRN IN CNC TO CNC-NUM-ATRN IN CNC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% BUF = "CNC-FLD"
*% SIS = "COL"
*% TAB = "TCAN"
*% FLD = "CNC-MSC-TCAN"
*% GLS = "FRM-GLS-TCAN"
*% TIENE_GLOSA = TRUE
*% LGL = 34
*% INCLUDE 'BUSMSC'
*% BUF = "CNC-FLD"
*% SIS = "COL"
*% TAB = "FCAN"
*% FLD = "CNC-MSC-FCAN"
*% GLS = "FRM-GLS-FCAN"
*% TIENE_GLOSA = TRUE
*% LGL = 34
*% INCLUDE 'BUSMSC'
*% BUF = "CNC-FLD"
*% SIS = "COL"
*% TAB = "CCAN"
*% FLD = "CNC-MSC-ECAN"
*% GLS = "FRM-GLS-ECAN"
*% TIENE_GLOSA = TRUE
*% LGL = 34
*% INCLUDE 'BUSMSC'
*% INCLUDE FIN_PUT_CNC_CNC OF 'COLBMCNC.TXT'
       FIN-PUT-CNC-CNC.
           EXIT.
       
       KEY-ALL-CNC SECTION.
       INI-KEY-ALL-CNC.
*% INCLUDE INI_KEY_ALL_CNC OF 'COLBMCNC.TXT'
           PERFORM KEY-CNC-CNC.
*% INCLUDE FIN_KEY_ALL_CNC OF 'COLBMCNC.TXT'
       FIN-KEY-ALL-CNC.
           EXIT.
       
       KEY-CNC-CNC SECTION.
       INI-KEY-CNC-CNC.
*% INCLUDE INI_KEY_CNC_CNC OF 'COLBMCNC.TXT'
           MOVE CNC-MSC-TCAN IN CNC-FLD TO CNC-MSC-TCAN IN CNC.
           MOVE CNC-MSC-FCAN IN CNC-FLD TO CNC-MSC-FCAN IN CNC.
           MOVE CNC-MSC-ECAN IN CNC-FLD TO CNC-MSC-ECAN IN CNC.
*% INCLUDE FIN_KEY_CNC_CNC OF 'COLBMCNC.TXT'
       FIN-KEY-CNC-CNC.
           EXIT.
       
       SCR-CLRA-CNC SECTION.
       INI-SCR-CLRA-CNC.
*% INCLUDE INI_SCR_CLRA_CNC OF 'COLBMCNC.TXT'
           PERFORM CLR-KEY-CNC.
           PERFORM CLR-FLD-CNC.
*% INCLUDE FIN_SCR_CLRA_CNC OF 'COLBMCNC.TXT'
       FIN-SCR-CLRA-CNC.
           EXIT.
       
       CLR-KEY-CNC SECTION.
       INI-CLR-KEY-CNC.
*% INCLUDE INI_CLR_KEY_CNC OF 'COLBMCNC.TXT'
           MOVE SPACES TO CNC-MSC-TCAN IN CNC-FLD.
           MOVE SPACES TO CNC-MSC-FCAN IN CNC-FLD.
           MOVE SPACES TO CNC-MSC-ECAN IN CNC-FLD.
*% INCLUDE FIN_CLR_KEY_CNC OF 'COLBMCNC.TXT'
       FIN-CLR-KEY-CNC.
           EXIT.
       
       CLR-FLD-CNC SECTION.
       INI-CLR-FLD-CNC.
*% INCLUDE INI_CLR_FLD_CNC OF 'COLBMCNC.TXT'
           MOVE SPACES TO FRM-GLS-TCAN IN CNC-FLD.
           MOVE SPACES TO FRM-GLS-FCAN IN CNC-FLD.
           MOVE SPACES TO FRM-GLS-ECAN IN CNC-FLD.
           PERFORM CLR-CNC-CNC.
*% INCLUDE FIN_CLR_FLD_CNC OF 'COLBMCNC.TXT'
       FIN-CLR-FLD-CNC.
           EXIT.
       
       CLR-CNC-CNC SECTION.
       INI-CLR-CNC-CNC.
*% INCLUDE INI_CLR_CNC_CNC OF 'COLBMCNC.TXT'
           MOVE SPACES TO CNC-CIC-CANC IN CNC-FLD.
           MOVE SPACES TO CNC-EXT-CTAB IN CNC-FLD.
           MOVE SPACES TO CNC-GLS-DESC IN CNC-FLD.
           MOVE SPACES TO CNC-GLS-ABRV IN CNC-FLD.
           MOVE SPACES TO CNC-GLS-DCOR IN CNC-FLD.
           MOVE SPACES TO CNC-IND-VIGE IN CNC-FLD.
*% RREG = "CNC"
*% RREG_FRM = "CNC"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CNC-FEC-FTRN IN CNC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_CNC_CNC OF 'COLBMCNC.TXT'
       FIN-CLR-CNC-CNC.
           EXIT.
       
       FST-KEY-CNC SECTION.
       INI-FST-KEY-CNC.
*% INCLUDE INI_FST_KEY_CNC OF 'COLBMCNC.TXT'
           MOVE -1 TO CNC-MSC-TCAN-LEN IN CNC-FLD.
*% INCLUDE FIN_FST_KEY_CNC OF 'COLBMCNC.TXT'
       FIN-FST-KEY-CNC.
           EXIT.
       
       FST-FLD-CNC SECTION.
       INI-FST-FLD-CNC.
*% INCLUDE INI_FST_FLD_CNC OF 'COLBMCNC.TXT'
           MOVE -1 TO CNC-CIC-CANC-LEN IN CNC-FLD.
*% INCLUDE FIN_FST_FLD_CNC OF 'COLBMCNC.TXT'
       FIN-FST-FLD-CNC.
           EXIT.
       
       PRO-KEY-CNC SECTION.
       INI-PRO-KEY-CNC.
*% INCLUDE INI_PRO_KEY_CNC OF 'COLBMCNC.TXT'
           PERFORM PRO-IKY-CNC.
           PERFORM PRO-AKY-CNC.
           PERFORM PRO-MKY-CNC.
*% INCLUDE FIN_PRO_KEY_CNC OF 'COLBMCNC.TXT'
       FIN-PRO-KEY-CNC.
           EXIT.
       
       PRO-IKY-CNC SECTION.
       INI-PRO-IKY-CNC.
*% INCLUDE INI_PRO_IKY_CNC OF 'COLBMCNC.TXT'
           MOVE FRM-CPIM-AEY TO CNC-MSC-TCAN-ATR IN CNC-FLD.
           MOVE FRM-CPIM-AEY TO CNC-MSC-FCAN-ATR IN CNC-FLD.
           MOVE FRM-CPIM-AEY TO CNC-MSC-ECAN-ATR IN CNC-FLD.
*% INCLUDE FIN_PRO_IKY_CNC OF 'COLBMCNC.TXT'
       FIN-PRO-IKY-CNC.
           EXIT.
       
       PRO-MKY-CNC SECTION.
       INI-PRO-MKY-CNC.
*% INCLUDE INI_PRO_MKY_CNC OF 'COLBMCNC.TXT'
*% INCLUDE FIN_PRO_MKY_CNC OF 'COLBMCNC.TXT'
       FIN-PRO-MKY-CNC.
           EXIT.
       
       PRO-AKY-CNC SECTION.
       INI-PRO-AKY-CNC.
*% INCLUDE INI_PRO_AKY_CNC OF 'COLBMCNC.TXT'
*% INCLUDE FIN_PRO_AKY_CNC OF 'COLBMCNC.TXT'
       FIN-PRO-AKY-CNC.
           EXIT.
       
       UNP-KEY-CNC SECTION.
       INI-UNP-KEY-CNC.
*% INCLUDE INI_UNP_KEY_CNC OF 'COLBMCNC.TXT'
           PERFORM UNP-IKY-CNC.
           PERFORM UNP-AKY-CNC.
           PERFORM UNP-MKY-CNC.
*% INCLUDE FIN_UNP_KEY_CNC OF 'COLBMCNC.TXT'
       FIN-UNP-KEY-CNC.
           EXIT.
       
       UNP-IKY-CNC SECTION.
       INI-UNP-IKY-CNC.
*% INCLUDE INI_UNP_IKY_CNC OF 'COLBMCNC.TXT'
           MOVE FRM-CPIM-UBY TO CNC-MSC-TCAN-ATR.
           MOVE FRM-CPIM-UBY TO CNC-MSC-FCAN-ATR.
           MOVE FRM-CPIM-UBY TO CNC-MSC-ECAN-ATR.
*% INCLUDE FIN_UNP_IKY_CNC OF 'COLBMCNC.TXT'
       FIN-UNP-IKY-CNC.
           EXIT.
       
       UNP-MKY-CNC SECTION.
       INI-UNP-MKY-CNC.
*% INCLUDE INI_UNP_MKY_CNC OF 'COLBMCNC.TXT'
*% INCLUDE FIN_UNP_MKY_CNC OF 'COLBMCNC.TXT'
       FIN-UNP-MKY-CNC.
           EXIT.
       
       UNP-AKY-CNC SECTION.
       INI-UNP-AKY-CNC.
*% INCLUDE INI_UNP_AKY_CNC OF 'COLBMCNC.TXT'
*% INCLUDE FIN_UNP_AKY_CNC OF 'COLBMCNC.TXT'
       FIN-UNP-AKY-CNC.
           EXIT.
       
       UNP-FLD-CNC SECTION.
       INI-UNP-FLD-CNC.
*% INCLUDE INI_UNP_FLD_CNC OF 'COLBMCNC.TXT'
           PERFORM UNP-CNC-CNC.
*% INCLUDE FIN_UNP_FLD_CNC OF 'COLBMCNC.TXT'
       FIN-UNP-FLD-CNC.
           EXIT.
       
       UNP-CNC-CNC SECTION.
       INI-UNP-CNC-CNC.
*% INCLUDE INI_UNP_CNC_CNC OF 'COLBMCNC.TXT'
           MOVE FRM-CPIM-UBY TO CNC-CIC-CANC-ATR IN CNC-FLD.
           MOVE FRM-CPIM-UBY TO CNC-EXT-CTAB-ATR IN CNC-FLD.
           MOVE FRM-CPIM-UBY TO CNC-GLS-DESC-ATR IN CNC-FLD.
           MOVE FRM-CPIM-UBY TO CNC-GLS-ABRV-ATR IN CNC-FLD.
           MOVE FRM-CPIM-UBY TO CNC-GLS-DCOR-ATR IN CNC-FLD.
           MOVE FRM-CPIM-UBY TO CNC-IND-VIGE-ATR IN CNC-FLD.
*% INCLUDE FIN_UNP_CNC_CNC OF 'COLBMCNC.TXT'
       FIN-UNP-CNC-CNC.
           EXIT.
       
       VAL-KEY-CNC SECTION.
       INI-VAL-KEY-CNC.
           PERFORM VAL-IKY-CNC.
           PERFORM VAL-MKY-CNC.
           PERFORM VAL-AKY-CNC.
       FIN-VAL-KEY-CNC.
           EXIT.
       
       VAL-IKY-CNC SECTION.
       INI-VAL-IKY-CNC.
*% INCLUDE INI_VAL_IKY_CNC OF 'COLBMCNC.TXT'
           PERFORM VAL-CNC-MSC-TCAN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CNC-MSC-TCAN-LEN IN CNC-FLD
                GO TO FIN-VAL-IKY-CNC.
           PERFORM VAL-CNC-MSC-FCAN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CNC-MSC-FCAN-LEN IN CNC-FLD
                GO TO FIN-VAL-IKY-CNC.
           PERFORM VAL-CNC-MSC-ECAN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CNC-MSC-ECAN-LEN IN CNC-FLD
                GO TO FIN-VAL-IKY-CNC.
*% INCLUDE FIN_VAL_IKY_CNC OF 'COLBMCNC.TXT'
       FIN-VAL-IKY-CNC.
           EXIT.
       
       VAL-MKY-CNC SECTION.
       INI-VAL-MKY-CNC.
*% INCLUDE INI_VAL_MKY_CNC OF 'COLBMCNC.TXT'
*% INCLUDE FIN_VAL_MKY_CNC OF 'COLBMCNC.TXT'
       FIN-VAL-MKY-CNC.
           EXIT.
       
       VAL-AKY-CNC SECTION.
       INI-VAL-AKY-CNC.
*% INCLUDE INI_VAL_AKY_CNC OF 'COLBMCNC.TXT'
*% INCLUDE FIN_VAL_AKY_CNC OF 'COLBMCNC.TXT'
       FIN-VAL-AKY-CNC.
           EXIT.
       
       VAL-CNC-MSC-TCAN SECTION.
       INI-VAL-CNC-MSC-TCAN.
*% LBL = "CNC_MSC_TCAN OF 'COLBMCNC.TXT'"
*% INCLUDE INI_CNC_MSC_TCAN OF 'COLBMCNC.TXT'
           IF CNC-MSC-TCAN IN CNC-FLD NOT > SPACES
              MOVE SPACES TO CNC-MSC-TCAN IN CNC-FLD.
*% INCLUDE CNC_MSC_TCAN OF 'COLBMCNC.TXT'
*% BUF = "CNC-FLD"
*% SIS = "COL"
*% TAB = "TCAN"
*% FLD = "CNC-MSC-TCAN"
*% GLS = "FRM-GLS-TCAN"
*% TIENE_GLOSA = TRUE
*% LGL = 34
*% INCLUDE 'VALIND'
*% INCLUDE FIN_CNC_MSC_TCAN OF 'COLBMCNC.TXT'
       FIN-VAL-CNC-MSC-TCAN.
           EXIT.
       
       VAL-CNC-MSC-FCAN SECTION.
       INI-VAL-CNC-MSC-FCAN.
*% LBL = "CNC_MSC_FCAN OF 'COLBMCNC.TXT'"
*% INCLUDE INI_CNC_MSC_FCAN OF 'COLBMCNC.TXT'
           IF CNC-MSC-FCAN IN CNC-FLD NOT > SPACES
              MOVE SPACES TO CNC-MSC-FCAN IN CNC-FLD.
*% INCLUDE CNC_MSC_FCAN OF 'COLBMCNC.TXT'
*% BUF = "CNC-FLD"
*% SIS = "COL"
*% TAB = "FCAN"
*% FLD = "CNC-MSC-FCAN"
*% GLS = "FRM-GLS-FCAN"
*% TIENE_GLOSA = TRUE
*% LGL = 34
*% INCLUDE 'VALIND'
*% INCLUDE FIN_CNC_MSC_FCAN OF 'COLBMCNC.TXT'
       FIN-VAL-CNC-MSC-FCAN.
           EXIT.
       
       VAL-CNC-MSC-ECAN SECTION.
       INI-VAL-CNC-MSC-ECAN.
*% LBL = "CNC_MSC_ECAN OF 'COLBMCNC.TXT'"
*% INCLUDE INI_CNC_MSC_ECAN OF 'COLBMCNC.TXT'
           IF CNC-MSC-ECAN IN CNC-FLD NOT > SPACES
              MOVE SPACES TO CNC-MSC-ECAN IN CNC-FLD.
*% INCLUDE CNC_MSC_ECAN OF 'COLBMCNC.TXT'
*% BUF = "CNC-FLD"
*% SIS = "COL"
*% TAB = "CCAN"
*% FLD = "CNC-MSC-ECAN"
*% GLS = "FRM-GLS-ECAN"
*% TIENE_GLOSA = TRUE
*% LGL = 34
*% INCLUDE 'VALIND'
*% INCLUDE FIN_CNC_MSC_ECAN OF 'COLBMCNC.TXT'
       FIN-VAL-CNC-MSC-ECAN.
           EXIT.
*% RREG = "CNC"
*% RREG_FRM = "CNC"
*% NOMB = "FTRN"
*% IZR = "S"
       
       VAL-CON-KEY-CNC SECTION.
       INI-VAL-CON-KEY-CNC.
*% INCLUDE INI_VAL_CON_KEY_CNC OF 'COLBMCNC.TXT'
           IF     CNC-MSC-TCAN IN CNC-FLD NOT > SPACES 
              AND CNC-MSC-FCAN IN CNC-FLD NOT > SPACES 
              AND CNC-MSC-ECAN IN CNC-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CNC.
*% INCLUDE FIN_VAL_CON_KEY_CNC OF 'COLBMCNC.TXT'
       FIN-VAL-CON-KEY-CNC.
           EXIT.
       
       VAL-CON-IKY-CNC SECTION.
       INI-VAL-CON-IKY-CNC.
*% INCLUDE INI_VAL_CON_IKY_CNC OF 'COLBMCNC.TXT'
           IF     CNC-MSC-TCAN IN CNC-FLD NOT > SPACES 
              AND CNC-MSC-FCAN IN CNC-FLD NOT > SPACES 
              AND CNC-MSC-ECAN IN CNC-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CNC.
*% INCLUDE FIN_VAL_CON_IKY_CNC OF 'COLBMCNC.TXT'
       FIN-VAL-CON-IKY-CNC.
           EXIT.
       
       VAL-NUM-KEY-CNC SECTION.
       INI-VAL-NUM-KEY-CNC.
           PERFORM VAL-NUM-IKY-CNC.
           PERFORM VAL-NUM-MKY-CNC.
           PERFORM VAL-NUM-AKY-CNC.
       FIN-VAL-NUM-KEY-CNC.
           EXIT.
       
       VAL-NUM-IKY-CNC SECTION.
       INI-VAL-NUM-IKY-CNC.
*% INCLUDE INI_VAL_NUM_IKY_CNC OF 'COLBMCNC.TXT'
*% INCLUDE FIN_VAL_NUM_IKY_CNC OF 'COLBMCNC.TXT'
       FIN-VAL-NUM-IKY-CNC.
           EXIT.
       
       VAL-NUM-MKY-CNC SECTION.
       INI-VAL-NUM-MKY-CNC.
*% INCLUDE INI_VAL_NUM_MKY_CNC OF 'COLBMCNC.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_CNC OF 'COLBMCNC.TXT'
       FIN-VAL-NUM-MKY-CNC.
           EXIT.
       
       VAL-NUM-AKY-CNC SECTION.
       INI-VAL-NUM-AKY-CNC.
*% INCLUDE INI_VAL_NUM_AKY_CNC OF 'COLBMCNC.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_CNC OF 'COLBMCNC.TXT'
