*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "QBC"
*% PGM_BQ = TRUE
       PUT-ALL-QBC SECTION.
       INI-PUT-ALL-QBC.
*% INCLUDE INI_PUT_ALL_QBC OF 'COLBMQBC.TXT'
           PERFORM PUT-OPC-QBC.
           PERFORM PUT-DLC-QBC.
*% INCLUDE FIN_PUT_ALL_QBC OF 'COLBMQBC.TXT'
       FIN-PUT-ALL-QBC.
           EXIT.
       
       PUT-OPC-QBC SECTION.
       INI-PUT-OPC-QBC.
*% INCLUDE INI_PUT_OPC_QBC OF 'COLBMQBC.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-QBC.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-QBC
               GO TO FIN-PUT-OPC-QBC.
           MOVE OPC-COD-VCOC IN OPC TO OPC-COD-VCOC IN QBC-FLD.
           MOVE OPC-COD-TOOC IN OPC TO OPC-COD-TOOC IN QBC-FLD.
           MOVE OPC-COD-OFOC IN OPC TO OPC-COD-OFOC IN QBC-FLD.
*% BUF = "QBC-FLD"
*% SIS = "TAB"
*% TAB = "VLR "
*% FLD = "OPC-COD-VCOC"
*% GLS = "FRM-GLS-VCOC"
*% TIENE_GLOSA = TRUE
*% LGL = 13
*% INCLUDE 'BUSTAB'
*% BUF = "QBC-FLD"
*% SIS = "TAB"
*% TAB = "TIO "
*% FLD = "OPC-COD-TOOC"
*% GLS = "FRM-GLS-TOOC"
*% TIENE_GLOSA = TRUE
*% LGL = 14
*% INCLUDE 'BUSTAB'
*% BUF = "QBC-FLD"
*% SIS = "TAB"
*% TAB = "OFI "
*% FLD = "OPC-COD-OFOC"
*% GLS = "FRM-GLS-OFOC"
*% TIENE_GLOSA = TRUE
*% LGL = 14
*% INCLUDE 'BUSOFI          '
*% INCLUDE FIN_PUT_OPC_QBC OF 'COLBMQBC.TXT'
       FIN-PUT-OPC-QBC.
           EXIT.
       
       PUT-DLC-QBC SECTION.
       INI-PUT-DLC-QBC.
*% INCLUDE INI_PUT_DLC_QBC OF 'COLBMQBC.TXT'
           IF PGM-STAT-DLC-NUL
               GO TO FIN-PUT-DLC-QBC.
           IF NOT PGM-STAT-DLC-OKS
               PERFORM CLR-DLC-QBC
               GO TO FIN-PUT-DLC-QBC.
           MOVE DLC-CAI-IOPC IN DLC TO DLC-CAI-IOPC IN QBC-FLD.
           MOVE DLC-IIC-IOPC IN DLC TO DLC-IIC-IOPC IN QBC-FLD.
           MOVE DLC-NUM-IDLC IN DLC TO DLC-NUM-IDLC IN QBC-FLD.
*% INCLUDE FIN_PUT_DLC_QBC OF 'COLBMQBC.TXT'
       FIN-PUT-DLC-QBC.
           EXIT.
       
       KEY-ALL-QBC SECTION.
       INI-KEY-ALL-QBC.
*% INCLUDE INI_KEY_ALL_QBC OF 'COLBMQBC.TXT'
           PERFORM KEY-OPC-QBC.
           PERFORM KEY-DLC-QBC.
*% INCLUDE FIN_KEY_ALL_QBC OF 'COLBMQBC.TXT'
       FIN-KEY-ALL-QBC.
           EXIT.
       
       KEY-OPC-QBC SECTION.
       INI-KEY-OPC-QBC.
*% INCLUDE INI_KEY_OPC_QBC OF 'COLBMQBC.TXT'
*% INCLUDE FIN_KEY_OPC_QBC OF 'COLBMQBC.TXT'
       FIN-KEY-OPC-QBC.
           EXIT.
       
       KEY-DLC-QBC SECTION.
       INI-KEY-DLC-QBC.
*% INCLUDE INI_KEY_DLC_QBC OF 'COLBMQBC.TXT'
           MOVE DLC-CAI-IOPC IN QBC-FLD TO DLC-CAI-IOPC IN DLC.
           MOVE DLC-IIC-IOPC IN QBC-FLD TO DLC-IIC-IOPC IN DLC.
           MOVE DLC-NUM-IDLC IN QBC-FLD TO DLC-NUM-IDLC IN DLC.
*% INCLUDE FIN_KEY_DLC_QBC OF 'COLBMQBC.TXT'
       FIN-KEY-DLC-QBC.
           EXIT.
       
       SCR-CLRA-QBC SECTION.
       INI-SCR-CLRA-QBC.
*% INCLUDE INI_SCR_CLRA_QBC OF 'COLBMQBC.TXT'
           PERFORM CLR-KEY-QBC.
           PERFORM CLR-FLD-QBC.
*% INCLUDE FIN_SCR_CLRA_QBC OF 'COLBMQBC.TXT'
       FIN-SCR-CLRA-QBC.
           EXIT.
       
       CLR-KEY-QBC SECTION.
       INI-CLR-KEY-QBC.
*% INCLUDE INI_CLR_KEY_QBC OF 'COLBMQBC.TXT'
           MOVE SPACES TO DLC-CAI-IOPC IN QBC-FLD.
           MOVE ZEROES TO DLC-IIC-IOPC IN QBC-FLD.
           MOVE ZEROES TO DLC-NUM-IDLC IN QBC-FLD.
*% INCLUDE FIN_CLR_KEY_QBC OF 'COLBMQBC.TXT'
       FIN-CLR-KEY-QBC.
           EXIT.
       
       CLR-FLD-QBC SECTION.
       INI-CLR-FLD-QBC.
*% INCLUDE INI_CLR_FLD_QBC OF 'COLBMQBC.TXT'
           MOVE SPACES TO FRM-GLS-VCOC IN QBC-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN QBC-FLD.
           MOVE SPACES TO FRM-GLS-OFOC IN QBC-FLD.
           MOVE SPACES TO FRM-CIC-GTIA IN QBC-FLD(1).
           MOVE SPACES TO FRM-CIC-GTIA IN QBC-FLD(2).
           MOVE SPACES TO FRM-CIC-GTIA IN QBC-FLD(3).
           MOVE SPACES TO FRM-CIC-GTIA IN QBC-FLD(4).
           MOVE SPACES TO FRM-CIC-GTIA IN QBC-FLD(5).
           MOVE SPACES TO FRM-CIC-GTIA IN QBC-FLD(6).
           MOVE ZEROES TO FRM-NUM-IGES IN QBC-FLD(1).
           MOVE ZEROES TO FRM-NUM-IGES IN QBC-FLD(2).
           MOVE ZEROES TO FRM-NUM-IGES IN QBC-FLD(3).
           MOVE ZEROES TO FRM-NUM-IGES IN QBC-FLD(4).
           MOVE ZEROES TO FRM-NUM-IGES IN QBC-FLD(5).
           MOVE ZEROES TO FRM-NUM-IGES IN QBC-FLD(6).
           MOVE SPACES TO FRM-GLS-TOPE IN QBC-FLD(1).
           MOVE SPACES TO FRM-GLS-TOPE IN QBC-FLD(2).
           MOVE SPACES TO FRM-GLS-TOPE IN QBC-FLD(3).
           MOVE SPACES TO FRM-GLS-TOPE IN QBC-FLD(4).
           MOVE SPACES TO FRM-GLS-TOPE IN QBC-FLD(5).
           MOVE SPACES TO FRM-GLS-TOPE IN QBC-FLD(6).
           MOVE SPACES TO FRM-GLS-SOPE IN QBC-FLD(1).
           MOVE SPACES TO FRM-GLS-SOPE IN QBC-FLD(2).
           MOVE SPACES TO FRM-GLS-SOPE IN QBC-FLD(3).
           MOVE SPACES TO FRM-GLS-SOPE IN QBC-FLD(4).
           MOVE SPACES TO FRM-GLS-SOPE IN QBC-FLD(5).
           MOVE SPACES TO FRM-GLS-SOPE IN QBC-FLD(6).
           MOVE SPACES TO FRM-IND-TREA IN QBC-FLD(1).
           MOVE SPACES TO FRM-IND-TREA IN QBC-FLD(2).
           MOVE SPACES TO FRM-IND-TREA IN QBC-FLD(3).
           MOVE SPACES TO FRM-IND-TREA IN QBC-FLD(4).
           MOVE SPACES TO FRM-IND-TREA IN QBC-FLD(5).
           MOVE SPACES TO FRM-IND-TREA IN QBC-FLD(6).
           MOVE SPACES TO FRM-COD-LGTA IN QBC-FLD(1).
           MOVE SPACES TO FRM-COD-LGTA IN QBC-FLD(2).
           MOVE SPACES TO FRM-COD-LGTA IN QBC-FLD(3).
           MOVE SPACES TO FRM-COD-LGTA IN QBC-FLD(4).
           MOVE SPACES TO FRM-COD-LGTA IN QBC-FLD(5).
           MOVE SPACES TO FRM-COD-LGTA IN QBC-FLD(6).
           MOVE SPACES TO FRM-COD-GGTA IN QBC-FLD(1).
           MOVE SPACES TO FRM-COD-GGTA IN QBC-FLD(2).
           MOVE SPACES TO FRM-COD-GGTA IN QBC-FLD(3).
           MOVE SPACES TO FRM-COD-GGTA IN QBC-FLD(4).
           MOVE SPACES TO FRM-COD-GGTA IN QBC-FLD(5).
           MOVE SPACES TO FRM-COD-GGTA IN QBC-FLD(6).
           MOVE SPACES TO FRM-COD-EST2 IN QBC-FLD(1).
           MOVE SPACES TO FRM-COD-EST2 IN QBC-FLD(2).
           MOVE SPACES TO FRM-COD-EST2 IN QBC-FLD(3).
           MOVE SPACES TO FRM-COD-EST2 IN QBC-FLD(4).
           MOVE SPACES TO FRM-COD-EST2 IN QBC-FLD(5).
           MOVE SPACES TO FRM-COD-EST2 IN QBC-FLD(6).
           MOVE SPACES TO FRM-GLS-EST1 IN QBC-FLD(1).
           MOVE SPACES TO FRM-GLS-EST1 IN QBC-FLD(2).
           MOVE SPACES TO FRM-GLS-EST1 IN QBC-FLD(3).
           MOVE SPACES TO FRM-GLS-EST1 IN QBC-FLD(4).
           MOVE SPACES TO FRM-GLS-EST1 IN QBC-FLD(5).
           MOVE SPACES TO FRM-GLS-EST1 IN QBC-FLD(6).
           MOVE ZEROES TO FRM-SGV-VCTS IN QBC-FLD(1).
           MOVE ZEROES TO FRM-SGV-VCTS IN QBC-FLD(2).
           MOVE ZEROES TO FRM-SGV-VCTS IN QBC-FLD(3).
           MOVE ZEROES TO FRM-SGV-VCTS IN QBC-FLD(4).
           MOVE ZEROES TO FRM-SGV-VCTS IN QBC-FLD(5).
           MOVE ZEROES TO FRM-SGV-VCTS IN QBC-FLD(6).
           MOVE ZEROES TO FRM-SGV-LTSN IN QBC-FLD(1).
           MOVE ZEROES TO FRM-SGV-LTSN IN QBC-FLD(2).
           MOVE ZEROES TO FRM-SGV-LTSN IN QBC-FLD(3).
           MOVE ZEROES TO FRM-SGV-LTSN IN QBC-FLD(4).
           MOVE ZEROES TO FRM-SGV-LTSN IN QBC-FLD(5).
           MOVE ZEROES TO FRM-SGV-LTSN IN QBC-FLD(6).
           MOVE ZEROES TO FRM-SGV-VPND IN QBC-FLD(1).
           MOVE ZEROES TO FRM-SGV-VPND IN QBC-FLD(2).
           MOVE ZEROES TO FRM-SGV-VPND IN QBC-FLD(3).
           MOVE ZEROES TO FRM-SGV-VPND IN QBC-FLD(4).
           MOVE ZEROES TO FRM-SGV-VPND IN QBC-FLD(5).
           MOVE ZEROES TO FRM-SGV-VPND IN QBC-FLD(6).
*% RREG = "FRM"
*% NOMB = "FVTO"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FVTO IN QBC-FLD(1).
           MOVE ZEROES TO FRM-FEC-FVTO IN QBC-FLD(2).
           MOVE ZEROES TO FRM-FEC-FVTO IN QBC-FLD(3).
           MOVE ZEROES TO FRM-FEC-FVTO IN QBC-FLD(4).
           MOVE ZEROES TO FRM-FEC-FVTO IN QBC-FLD(5).
           MOVE ZEROES TO FRM-FEC-FVTO IN QBC-FLD(6).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           PERFORM CLR-OPC-QBC.
           PERFORM CLR-DLC-QBC.
*% INCLUDE FIN_CLR_FLD_QBC OF 'COLBMQBC.TXT'
       FIN-CLR-FLD-QBC.
           EXIT.
       
       CLR-OPC-QBC SECTION.
       INI-CLR-OPC-QBC.
*% INCLUDE INI_CLR_OPC_QBC OF 'COLBMQBC.TXT'
           MOVE SPACES TO OPC-COD-VCOC IN QBC-FLD.
           MOVE SPACES TO OPC-COD-TOOC IN QBC-FLD.
           MOVE SPACES TO OPC-COD-OFOC IN QBC-FLD.
*% INCLUDE FIN_CLR_OPC_QBC OF 'COLBMQBC.TXT'
       FIN-CLR-OPC-QBC.
           EXIT.
       
       CLR-DLC-QBC SECTION.
       INI-CLR-DLC-QBC.
*% INCLUDE INI_CLR_DLC_QBC OF 'COLBMQBC.TXT'
*% INCLUDE FIN_CLR_DLC_QBC OF 'COLBMQBC.TXT'
       FIN-CLR-DLC-QBC.
           EXIT.
       
       FST-KEY-QBC SECTION.
       INI-FST-KEY-QBC.
*% INCLUDE INI_FST_KEY_QBC OF 'COLBMQBC.TXT'
           MOVE -1 TO DLC-CAI-IOPC-LEN IN QBC-FLD.
*% INCLUDE FIN_FST_KEY_QBC OF 'COLBMQBC.TXT'
       FIN-FST-KEY-QBC.
           EXIT.
       
       FST-FLD-QBC SECTION.
       INI-FST-FLD-QBC.
*% INCLUDE INI_FST_FLD_QBC OF 'COLBMQBC.TXT'
*% INCLUDE FIN_FST_FLD_QBC OF 'COLBMQBC.TXT'
       FIN-FST-FLD-QBC.
           EXIT.
       
       PRO-KEY-QBC SECTION.
       INI-PRO-KEY-QBC.
*% INCLUDE INI_PRO_KEY_QBC OF 'COLBMQBC.TXT'
           PERFORM PRO-IKY-QBC.
           PERFORM PRO-AKY-QBC.
           PERFORM PRO-MKY-QBC.
*% INCLUDE FIN_PRO_KEY_QBC OF 'COLBMQBC.TXT'
       FIN-PRO-KEY-QBC.
           EXIT.
       
       PRO-IKY-QBC SECTION.
       INI-PRO-IKY-QBC.
*% INCLUDE INI_PRO_IKY_QBC OF 'COLBMQBC.TXT'
           MOVE FRM-CPIM-AEY TO DLC-CAI-IOPC-ATR IN QBC-FLD.
           MOVE FRM-CPIM-AEY TO DLC-IIC-IOPC-ATR IN QBC-FLD.
*% INCLUDE FIN_PRO_IKY_QBC OF 'COLBMQBC.TXT'
       FIN-PRO-IKY-QBC.
           EXIT.
       
       PRO-MKY-QBC SECTION.
       INI-PRO-MKY-QBC.
*% INCLUDE INI_PRO_MKY_QBC OF 'COLBMQBC.TXT'
*% INCLUDE FIN_PRO_MKY_QBC OF 'COLBMQBC.TXT'
       FIN-PRO-MKY-QBC.
           EXIT.
       
       PRO-AKY-QBC SECTION.
       INI-PRO-AKY-QBC.
*% INCLUDE INI_PRO_AKY_QBC OF 'COLBMQBC.TXT'
           MOVE FRM-CPIM-AEY TO DLC-NUM-IDLC-ATR IN QBC-FLD.
*% INCLUDE FIN_PRO_AKY_QBC OF 'COLBMQBC.TXT'
       FIN-PRO-AKY-QBC.
           EXIT.
       
       UNP-KEY-QBC SECTION.
       INI-UNP-KEY-QBC.
*% INCLUDE INI_UNP_KEY_QBC OF 'COLBMQBC.TXT'
           PERFORM UNP-IKY-QBC.
           PERFORM UNP-AKY-QBC.
           PERFORM UNP-MKY-QBC.
*% INCLUDE FIN_UNP_KEY_QBC OF 'COLBMQBC.TXT'
       FIN-UNP-KEY-QBC.
           EXIT.
       
       UNP-IKY-QBC SECTION.
       INI-UNP-IKY-QBC.
*% INCLUDE INI_UNP_IKY_QBC OF 'COLBMQBC.TXT'
           MOVE FRM-CPIM-UBY TO DLC-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO DLC-IIC-IOPC-ATR.
*% INCLUDE FIN_UNP_IKY_QBC OF 'COLBMQBC.TXT'
       FIN-UNP-IKY-QBC.
           EXIT.
       
       UNP-MKY-QBC SECTION.
       INI-UNP-MKY-QBC.
*% INCLUDE INI_UNP_MKY_QBC OF 'COLBMQBC.TXT'
*% INCLUDE FIN_UNP_MKY_QBC OF 'COLBMQBC.TXT'
       FIN-UNP-MKY-QBC.
           EXIT.
       
       UNP-AKY-QBC SECTION.
       INI-UNP-AKY-QBC.
*% INCLUDE INI_UNP_AKY_QBC OF 'COLBMQBC.TXT'
           MOVE FRM-CPIM-UBY TO DLC-NUM-IDLC-ATR.
*% INCLUDE FIN_UNP_AKY_QBC OF 'COLBMQBC.TXT'
       FIN-UNP-AKY-QBC.
           EXIT.
       
       UNP-FLD-QBC SECTION.
       INI-UNP-FLD-QBC.
*% INCLUDE INI_UNP_FLD_QBC OF 'COLBMQBC.TXT'
           PERFORM UNP-OPC-QBC.
           PERFORM UNP-DLC-QBC.
*% INCLUDE FIN_UNP_FLD_QBC OF 'COLBMQBC.TXT'
       FIN-UNP-FLD-QBC.
           EXIT.
       
       UNP-OPC-QBC SECTION.
       INI-UNP-OPC-QBC.
*% INCLUDE INI_UNP_OPC_QBC OF 'COLBMQBC.TXT'
*% INCLUDE FIN_UNP_OPC_QBC OF 'COLBMQBC.TXT'
       FIN-UNP-OPC-QBC.
           EXIT.
       
       UNP-DLC-QBC SECTION.
       INI-UNP-DLC-QBC.
*% INCLUDE INI_UNP_DLC_QBC OF 'COLBMQBC.TXT'
*% INCLUDE FIN_UNP_DLC_QBC OF 'COLBMQBC.TXT'
       FIN-UNP-DLC-QBC.
           EXIT.
       
       VAL-KEY-QBC SECTION.
       INI-VAL-KEY-QBC.
           PERFORM VAL-IKY-QBC.
           PERFORM VAL-MKY-QBC.
           PERFORM VAL-AKY-QBC.
       FIN-VAL-KEY-QBC.
           EXIT.
       
       VAL-IKY-QBC SECTION.
       INI-VAL-IKY-QBC.
*% INCLUDE INI_VAL_IKY_QBC OF 'COLBMQBC.TXT'
           PERFORM VAL-DLC-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DLC-CAI-IOPC-LEN IN QBC-FLD
                GO TO FIN-VAL-IKY-QBC.
           PERFORM VAL-DLC-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DLC-IIC-IOPC-LEN IN QBC-FLD
                GO TO FIN-VAL-IKY-QBC.
*% INCLUDE FIN_VAL_IKY_QBC OF 'COLBMQBC.TXT'
       FIN-VAL-IKY-QBC.
           EXIT.
       
       VAL-MKY-QBC SECTION.
       INI-VAL-MKY-QBC.
*% INCLUDE INI_VAL_MKY_QBC OF 'COLBMQBC.TXT'
*% INCLUDE FIN_VAL_MKY_QBC OF 'COLBMQBC.TXT'
       FIN-VAL-MKY-QBC.
           EXIT.
       
       VAL-AKY-QBC SECTION.
       INI-VAL-AKY-QBC.
*% INCLUDE INI_VAL_AKY_QBC OF 'COLBMQBC.TXT'
           PERFORM VAL-DLC-NUM-IDLC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DLC-NUM-IDLC-LEN IN QBC-FLD
                GO TO FIN-VAL-AKY-QBC.
*% INCLUDE FIN_VAL_AKY_QBC OF 'COLBMQBC.TXT'
       FIN-VAL-AKY-QBC.
           EXIT.
       
       VAL-DLC-CAI-IOPC SECTION.
       INI-VAL-DLC-CAI-IOPC.
*% LBL = "DLC_CAI_IOPC OF 'COLBMQBC.TXT'"
*% INCLUDE INI_DLC_CAI_IOPC OF 'COLBMQBC.TXT'
           IF DLC-CAI-IOPC IN QBC-FLD NOT > SPACES
              MOVE SPACES TO DLC-CAI-IOPC IN QBC-FLD.
*% INCLUDE DLC_CAI_IOPC OF 'COLBMQBC.TXT'
*% INCLUDE FIN_DLC_CAI_IOPC OF 'COLBMQBC.TXT'
       FIN-VAL-DLC-CAI-IOPC.
           EXIT.
       
       VAL-DLC-IIC-IOPC SECTION.
       INI-VAL-DLC-IIC-IOPC.
*% LBL = "DLC_IIC_IOPC OF 'COLBMQBC.TXT'"
*% INCLUDE INI_DLC_IIC_IOPC OF 'COLBMQBC.TXT'
*% INCLUDE DLC_IIC_IOPC OF 'COLBMQBC.TXT'
*% INCLUDE FIN_DLC_IIC_IOPC OF 'COLBMQBC.TXT'
       FIN-VAL-DLC-IIC-IOPC.
           EXIT.
       
       VAL-DLC-NUM-IDLC SECTION.
       INI-VAL-DLC-NUM-IDLC.
*% LBL = "DLC_NUM_IDLC OF 'COLBMQBC.TXT'"
*% INCLUDE INI_DLC_NUM_IDLC OF 'COLBMQBC.TXT'
*% INCLUDE DLC_NUM_IDLC OF 'COLBMQBC.TXT'
*% INCLUDE FIN_DLC_NUM_IDLC OF 'COLBMQBC.TXT'
       FIN-VAL-DLC-NUM-IDLC.
           EXIT.
*% RREG = "FRM"
*% NOMB = "FVTO"
*% IZR = "S"
       
       VAL-CON-KEY-QBC SECTION.
       INI-VAL-CON-KEY-QBC.
*% INCLUDE INI_VAL_CON_KEY_QBC OF 'COLBMQBC.TXT'
           IF     DLC-CAI-IOPC IN QBC-FLD NOT > SPACES 
              AND DLC-IIC-IOPC IN QBC-FLD NOT > ZEROES 
              AND DLC-NUM-IDLC IN QBC-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-QBC.
*% INCLUDE FIN_VAL_CON_KEY_QBC OF 'COLBMQBC.TXT'
       FIN-VAL-CON-KEY-QBC.
           EXIT.
       
       VAL-CON-IKY-QBC SECTION.
       INI-VAL-CON-IKY-QBC.
*% INCLUDE INI_VAL_CON_IKY_QBC OF 'COLBMQBC.TXT'
           IF     DLC-CAI-IOPC IN QBC-FLD NOT > SPACES 
              AND DLC-IIC-IOPC IN QBC-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-QBC.
*% INCLUDE FIN_VAL_CON_IKY_QBC OF 'COLBMQBC.TXT'
       FIN-VAL-CON-IKY-QBC.
           EXIT.
       
       VAL-NUM-KEY-QBC SECTION.
       INI-VAL-NUM-KEY-QBC.
           PERFORM VAL-NUM-IKY-QBC.
           PERFORM VAL-NUM-MKY-QBC.
           PERFORM VAL-NUM-AKY-QBC.
       FIN-VAL-NUM-KEY-QBC.
           EXIT.
       
       VAL-NUM-IKY-QBC SECTION.
       INI-VAL-NUM-IKY-QBC.
*% INCLUDE INI_VAL_NUM_IKY_QBC OF 'COLBMQBC.TXT'
       VAL-NUM-DLC-IIC-IOPC.
*% BUF = "QBC-FLD"
*% FLD = "DLC-IIC-IOPC"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-QBC.
*% INCLUDE FIN_VAL_NUM_IKY_QBC OF 'COLBMQBC.TXT'
       FIN-VAL-NUM-IKY-QBC.
           EXIT.
       
       VAL-NUM-MKY-QBC SECTION.
       INI-VAL-NUM-MKY-QBC.
*% INCLUDE INI_VAL_NUM_MKY_QBC OF 'COLBMQBC.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_QBC OF 'COLBMQBC.TXT'
       FIN-VAL-NUM-MKY-QBC.
           EXIT.
       
       VAL-NUM-AKY-QBC SECTION.
       INI-VAL-NUM-AKY-QBC.
*% INCLUDE INI_VAL_NUM_AKY_QBC OF 'COLBMQBC.TXT'
       VAL-NUM-DLC-NUM-IDLC.
*% BUF = "QBC-FLD"
*% FLD = "DLC-NUM-IDLC"
*% ENT = "3"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-AKY-QBC.
*% INCLUDE FIN_VAL_NUM_AKY_QBC OF 'COLBMQBC.TXT'
       FIN-VAL-NUM-AKY-QBC.
           EXIT.
       
       VAL-NUM-FLD-QBC SECTION.
