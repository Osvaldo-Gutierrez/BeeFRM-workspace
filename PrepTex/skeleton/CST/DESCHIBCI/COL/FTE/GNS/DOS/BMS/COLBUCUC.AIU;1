       INI-PRO-MKY-CUC.
*% INCLUDE INI_PRO_MKY_CUC OF 'COLBMCUC.TXT'
*% INCLUDE FIN_PRO_MKY_CUC OF 'COLBMCUC.TXT'
       FIN-PRO-MKY-CUC.
           EXIT.
       
       PRO-FLD-CUC SECTION.
       INI-PRO-FLD-CUC.
*% INCLUDE INI_PRO_FLD_CUC OF 'COLBMCUC.TXT'
           PERFORM PRO-CUC-CUC.
           PERFORM PRO-OFI-CUC.
*% INCLUDE FIN_PRO_FLD_CUC OF 'COLBMCUC.TXT'
       FIN-PRO-FLD-CUC.
           EXIT.
       
       PRO-CUC-CUC SECTION.
       INI-PRO-CUC-CUC.
*% INCLUDE INI_PRO_CUC_CUC OF 'COLBMCUC.TXT'
*% RREG = "CUC"
*% RREG_FRM = "CUC"
*% NOMB = "CUA"
           MOVE FRM-CPIM-AEY TO CUC-FEC-CUA-ATR IN CUC-FLD.
           MOVE FRM-CPIM-AEY TO CUC-HH-CUA-ATR IN CUC-FLD.
           MOVE FRM-CPIM-AEY TO CUC-MM-CUA-ATR IN CUC-FLD.
           MOVE FRM-CPIM-AEY TO CUC-SS-CUA-ATR IN CUC-FLD.
           MOVE FRM-CPIM-AEY TO CUC-DRS-CAR-ABO-ATR IN CUC-FLD(1).
           MOVE FRM-CPIM-AEY TO CUC-DRS-CAR-ABO-ATR IN CUC-FLD(2).
           MOVE FRM-CPIM-AEY TO CUC-DRS-CAR-ABO-ATR IN CUC-FLD(3).
           MOVE FRM-CPIM-AEY TO CUC-DRS-CAR-ABO-ATR IN CUC-FLD(4).
           MOVE FRM-CPIM-AEY TO CUC-DRS-CAR-ABO-ATR IN CUC-FLD(5).
           MOVE FRM-CPIM-AEY TO CUC-CTD-CUR-ATR IN CUC-FLD(1).
           MOVE FRM-CPIM-AEY TO CUC-CTD-CUR-ATR IN CUC-FLD(2).
           MOVE FRM-CPIM-AEY TO CUC-CTD-CUR-ATR IN CUC-FLD(3).
           MOVE FRM-CPIM-AEY TO CUC-CTD-CUR-ATR IN CUC-FLD(4).
           MOVE FRM-CPIM-AEY TO CUC-CTD-CUR-ATR IN CUC-FLD(5).
           MOVE FRM-CPIM-AEY TO CUC-VAL-LIQ-CUR-ATR IN CUC-FLD(1).
           MOVE FRM-CPIM-AEY TO CUC-VAL-LIQ-CUR-ATR IN CUC-FLD(2).
           MOVE FRM-CPIM-AEY TO CUC-VAL-LIQ-CUR-ATR IN CUC-FLD(3).
           MOVE FRM-CPIM-AEY TO CUC-VAL-LIQ-CUR-ATR IN CUC-FLD(4).
           MOVE FRM-CPIM-AEY TO CUC-VAL-LIQ-CUR-ATR IN CUC-FLD(5).
           MOVE FRM-CPIM-AEY TO CUC-VAL-CAN-ATR IN CUC-FLD(1).
           MOVE FRM-CPIM-AEY TO CUC-VAL-CAN-ATR IN CUC-FLD(2).
           MOVE FRM-CPIM-AEY TO CUC-VAL-CAN-ATR IN CUC-FLD(3).
           MOVE FRM-CPIM-AEY TO CUC-VAL-CAN-ATR IN CUC-FLD(4).
           MOVE FRM-CPIM-AEY TO CUC-VAL-CAN-ATR IN CUC-FLD(5).
           MOVE FRM-CPIM-AEY TO CUC-CTD-CAN-ATR IN CUC-FLD(1).
           MOVE FRM-CPIM-AEY TO CUC-CTD-CAN-ATR IN CUC-FLD(2).
           MOVE FRM-CPIM-AEY TO CUC-CTD-CAN-ATR IN CUC-FLD(3).
           MOVE FRM-CPIM-AEY TO CUC-CTD-CAN-ATR IN CUC-FLD(4).
           MOVE FRM-CPIM-AEY TO CUC-CTD-CAN-ATR IN CUC-FLD(5).
*% INCLUDE FIN_PRO_CUC_CUC OF 'COLBMCUC.TXT'
       FIN-PRO-CUC-CUC.
           EXIT.
       
       PRO-OFI-CUC SECTION.
       INI-PRO-OFI-CUC.
*% INCLUDE INI_PRO_OFI_CUC OF 'COLBMCUC.TXT'
           MOVE FRM-CPIM-AEY TO OFI-GLS-DESC-ATR IN CUC-FLD.
*% INCLUDE FIN_PRO_OFI_CUC OF 'COLBMCUC.TXT'
       FIN-PRO-OFI-CUC.
           EXIT.
       
       UNP-KEY-CUC SECTION.
       INI-UNP-KEY-CUC.
*% INCLUDE INI_UNP_KEY_CUC OF 'COLBMCUC.TXT'
           PERFORM UNP-IKY-CUC.
           PERFORM UNP-AKY-CUC.
           PERFORM UNP-MKY-CUC.
*% INCLUDE FIN_UNP_KEY_CUC OF 'COLBMCUC.TXT'
       FIN-UNP-KEY-CUC.
           EXIT.
       
       UNP-IKY-CUC SECTION.
       INI-UNP-IKY-CUC.
*% INCLUDE INI_UNP_IKY_CUC OF 'COLBMCUC.TXT'
           MOVE FRM-CPIM-UBY TO CUC-COD-OFI-CUA-ATR.
*% INCLUDE FIN_UNP_IKY_CUC OF 'COLBMCUC.TXT'
       FIN-UNP-IKY-CUC.
           EXIT.
       
       UNP-MKY-CUC SECTION.
       INI-UNP-MKY-CUC.
*% INCLUDE INI_UNP_MKY_CUC OF 'COLBMCUC.TXT'
*% INCLUDE FIN_UNP_MKY_CUC OF 'COLBMCUC.TXT'
       FIN-UNP-MKY-CUC.
           EXIT.
       
       UNP-AKY-CUC SECTION.
       INI-UNP-AKY-CUC.
*% INCLUDE INI_UNP_AKY_CUC OF 'COLBMCUC.TXT'
*% INCLUDE FIN_UNP_AKY_CUC OF 'COLBMCUC.TXT'
       FIN-UNP-AKY-CUC.
           EXIT.
       
       UNP-FLD-CUC SECTION.
       INI-UNP-FLD-CUC.
*% INCLUDE INI_UNP_FLD_CUC OF 'COLBMCUC.TXT'
           PERFORM UNP-CUC-CUC.
           PERFORM UNP-OFI-CUC.
*% INCLUDE FIN_UNP_FLD_CUC OF 'COLBMCUC.TXT'
       FIN-UNP-FLD-CUC.
           EXIT.
       
       UNP-CUC-CUC SECTION.
       INI-UNP-CUC-CUC.
*% INCLUDE INI_UNP_CUC_CUC OF 'COLBMCUC.TXT'
*% RREG = "CUC"
*% RREG_FRM = "CUC"
*% NOMB = "CUA"
           MOVE FRM-CPIM-UBY TO CUC-FEC-CUA-ATR IN CUC-FLD.
           MOVE FRM-CPIM-UBY TO CUC-HH-CUA-ATR IN CUC-FLD.
           MOVE FRM-CPIM-UBY TO CUC-MM-CUA-ATR IN CUC-FLD.
           MOVE FRM-CPIM-UBY TO CUC-SS-CUA-ATR IN CUC-FLD.
           MOVE FRM-CPIM-UBY TO CUC-DRS-CAR-ABO-ATR IN CUC-FLD(1).
           MOVE FRM-CPIM-UBY TO CUC-DRS-CAR-ABO-ATR IN CUC-FLD(2).
           MOVE FRM-CPIM-UBY TO CUC-DRS-CAR-ABO-ATR IN CUC-FLD(3).
           MOVE FRM-CPIM-UBY TO CUC-DRS-CAR-ABO-ATR IN CUC-FLD(4).
           MOVE FRM-CPIM-UBY TO CUC-DRS-CAR-ABO-ATR IN CUC-FLD(5).
           MOVE FRM-CPIM-UBY TO CUC-CTD-CUR-ATR IN CUC-FLD(1).
           MOVE FRM-CPIM-UBY TO CUC-CTD-CUR-ATR IN CUC-FLD(2).
           MOVE FRM-CPIM-UBY TO CUC-CTD-CUR-ATR IN CUC-FLD(3).
           MOVE FRM-CPIM-UBY TO CUC-CTD-CUR-ATR IN CUC-FLD(4).
           MOVE FRM-CPIM-UBY TO CUC-CTD-CUR-ATR IN CUC-FLD(5).
           MOVE FRM-CPIM-UBY TO CUC-VAL-LIQ-CUR-ATR IN CUC-FLD(1).
           MOVE FRM-CPIM-UBY TO CUC-VAL-LIQ-CUR-ATR IN CUC-FLD(2).
           MOVE FRM-CPIM-UBY TO CUC-VAL-LIQ-CUR-ATR IN CUC-FLD(3).
           MOVE FRM-CPIM-UBY TO CUC-VAL-LIQ-CUR-ATR IN CUC-FLD(4).
           MOVE FRM-CPIM-UBY TO CUC-VAL-LIQ-CUR-ATR IN CUC-FLD(5).
           MOVE FRM-CPIM-UBY TO CUC-VAL-CAN-ATR IN CUC-FLD(1).
           MOVE FRM-CPIM-UBY TO CUC-VAL-CAN-ATR IN CUC-FLD(2).
           MOVE FRM-CPIM-UBY TO CUC-VAL-CAN-ATR IN CUC-FLD(3).
           MOVE FRM-CPIM-UBY TO CUC-VAL-CAN-ATR IN CUC-FLD(4).
           MOVE FRM-CPIM-UBY TO CUC-VAL-CAN-ATR IN CUC-FLD(5).
           MOVE FRM-CPIM-UBY TO CUC-CTD-CAN-ATR IN CUC-FLD(1).
           MOVE FRM-CPIM-UBY TO CUC-CTD-CAN-ATR IN CUC-FLD(2).
           MOVE FRM-CPIM-UBY TO CUC-CTD-CAN-ATR IN CUC-FLD(3).
           MOVE FRM-CPIM-UBY TO CUC-CTD-CAN-ATR IN CUC-FLD(4).
           MOVE FRM-CPIM-UBY TO CUC-CTD-CAN-ATR IN CUC-FLD(5).
*% INCLUDE FIN_UNP_CUC_CUC OF 'COLBMCUC.TXT'
       FIN-UNP-CUC-CUC.
           EXIT.
       
       UNP-OFI-CUC SECTION.
       INI-UNP-OFI-CUC.
*% INCLUDE INI_UNP_OFI_CUC OF 'COLBMCUC.TXT'
           MOVE FRM-CPIM-UBY TO OFI-GLS-DESC-ATR IN CUC-FLD.
*% INCLUDE FIN_UNP_OFI_CUC OF 'COLBMCUC.TXT'
       FIN-UNP-OFI-CUC.
           EXIT.
       
       VAL-MKY-CUC SECTION.
       INI-VAL-MKY-CUC.
*% INCLUDE INI_VAL_MKY_CUC OF 'COLBMCUC.TXT'
*% INCLUDE FIN_VAL_MKY_CUC OF 'COLBMCUC.TXT'
       FIN-VAL-MKY-CUC.
           EXIT.
       
       VAL-FLD-CUC SECTION.
       INI-VAL-FLD-CUC.
*% INCLUDE INI_VAL_FLD_CUC OF 'COLBMCUC.TXT'
           PERFORM VAL-OFI-GLS-DESC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OFI-GLS-DESC-LEN IN CUC-FLD
                GO TO FIN-VAL-FLD-CUC.
*% RREG = "CUC"
*% RREG_FRM = "CUC"
*% NOMB = "CUA"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-CUC-FEC-CUA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                CUC-FEC-CUA-LEN IN CUC-FLD
                GO TO FIN-VAL-FLD-CUC.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-CUC-HH-CUA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CUC-HH-CUA-LEN IN CUC-FLD
                GO TO FIN-VAL-FLD-CUC.
           PERFORM VAL-CUC-MM-CUA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CUC-MM-CUA-LEN IN CUC-FLD
                GO TO FIN-VAL-FLD-CUC.
           PERFORM VAL-CUC-SS-CUA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CUC-SS-CUA-LEN IN CUC-FLD
                GO TO FIN-VAL-FLD-CUC.
           PERFORM VAL-CUC-DRS-CAR-ABO
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                CUC-DRS-CAR-ABO-LEN IN CUC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CUC.
           PERFORM VAL-CUC-CTD-CUR
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                CUC-CTD-CUR-LEN IN CUC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CUC.
           PERFORM VAL-CUC-VAL-LIQ-CUR
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                CUC-VAL-LIQ-CUR-LEN IN CUC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CUC.
           PERFORM VAL-CUC-VAL-CAN
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                CUC-VAL-CAN-LEN IN CUC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CUC.
           PERFORM VAL-CUC-CTD-CAN
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                CUC-CTD-CAN-LEN IN CUC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CUC.
*% INCLUDE FIN_VAL_FLD_CUC OF 'COLBMCUC.TXT'
       FIN-VAL-FLD-CUC.
           EXIT.
       
       VAL-OFI-GLS-DESC SECTION.
       INI-VAL-OFI-GLS-DESC.
*% LBL = "OFI_GLS_DESC OF 'COLBMCUC.TXT'"
*% INCLUDE INI_OFI_GLS_DESC OF 'COLBMCUC.TXT'
