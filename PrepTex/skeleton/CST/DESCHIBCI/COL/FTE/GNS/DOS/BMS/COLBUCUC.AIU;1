*% FRM = "CUC"
      *
      * COLBUCUC
      * Version 3.0.1 Nov-2005
      * 09-Jan-13 01:08 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMCUC.TXT'
       SET-TAG-COLBUCUC SECTION.
       INI-SET-TAG-COLBUCUC.
           MOVE
           '<IDNMOD NOM=$COLBUCUC$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$09-Jan-13 01:08 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCUC.
           EXIT.
           
       RET-ALL-CUC SECTION.
       INI-RET-ALL-CUC.
*% INCLUDE INI_RET_ALL_CUC OF 'COLBMCUC.TXT'
           PERFORM KEY-ALL-CUC.
           PERFORM RET-CUC-CUC.
           PERFORM RET-OFI-CUC.
*% INCLUDE FIN_RET_ALL_CUC OF 'COLBMCUC.TXT'
       FIN-RET-ALL-CUC.
           EXIT.
       
       RET-CUC-CUC SECTION.
       INI-RET-CUC-CUC.
*% INCLUDE INI_RET_CUC_CUC OF 'COLBMCUC.TXT'
*% RREG = "CUC"
*% RREG_FRM = "CUC"
*% NOMB = "CUA"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CUC-NUM-DUA IN CUC-FLD TO CUC-NUM-DUA IN CUC.
           MOVE CUC-NUM-MUA IN CUC-FLD TO CUC-NUM-MUA IN CUC.
           MOVE CUC-NUM-SUA IN CUC-FLD TO CUC-NUM-SUA IN CUC.
           MOVE CUC-NUM-AUA IN CUC-FLD TO CUC-NUM-AUA IN CUC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CUC-HH-CUA IN CUC-FLD TO CUC-HH-CUA IN CUC.
           MOVE CUC-MM-CUA IN CUC-FLD TO CUC-MM-CUA IN CUC.
           MOVE CUC-SS-CUA IN CUC-FLD TO CUC-SS-CUA IN CUC.
      *
      *    CAMPO INDEXADO   CUC-DRS-CAR-ABO
           MOVE CUC-DRS-CAR-ABO IN CUC-FLD(1) TO 
                CUC-DRS-CAR-ABO IN CUC(1).
           MOVE CUC-DRS-CAR-ABO IN CUC-FLD(2) TO 
                CUC-DRS-CAR-ABO IN CUC(2).
           MOVE CUC-DRS-CAR-ABO IN CUC-FLD(3) TO 
                CUC-DRS-CAR-ABO IN CUC(3).
           MOVE CUC-DRS-CAR-ABO IN CUC-FLD(4) TO 
                CUC-DRS-CAR-ABO IN CUC(4).
           MOVE CUC-DRS-CAR-ABO IN CUC-FLD(5) TO 
                CUC-DRS-CAR-ABO IN CUC(5).
      *
      *    CAMPO INDEXADO   CUC-CTD-CUR
           MOVE CUC-CTD-CUR IN CUC-FLD(1) TO CUC-CTD-CUR IN CUC(1).
           MOVE CUC-CTD-CUR IN CUC-FLD(2) TO CUC-CTD-CUR IN CUC(2).
           MOVE CUC-CTD-CUR IN CUC-FLD(3) TO CUC-CTD-CUR IN CUC(3).
           MOVE CUC-CTD-CUR IN CUC-FLD(4) TO CUC-CTD-CUR IN CUC(4).
           MOVE CUC-CTD-CUR IN CUC-FLD(5) TO CUC-CTD-CUR IN CUC(5).
      *
      *    CAMPO INDEXADO   CUC-VAL-LIQ-CUR
           MOVE CUC-VAL-LIQ-CUR IN CUC-FLD(1) TO 
                CUC-VAL-LIQ-CUR IN CUC(1).
           MOVE CUC-VAL-LIQ-CUR IN CUC-FLD(2) TO 
                CUC-VAL-LIQ-CUR IN CUC(2).
           MOVE CUC-VAL-LIQ-CUR IN CUC-FLD(3) TO 
                CUC-VAL-LIQ-CUR IN CUC(3).
           MOVE CUC-VAL-LIQ-CUR IN CUC-FLD(4) TO 
                CUC-VAL-LIQ-CUR IN CUC(4).
           MOVE CUC-VAL-LIQ-CUR IN CUC-FLD(5) TO 
                CUC-VAL-LIQ-CUR IN CUC(5).
      *
      *    CAMPO INDEXADO   CUC-VAL-CAN
           MOVE CUC-VAL-CAN IN CUC-FLD(1) TO CUC-VAL-CAN IN CUC(1).
           MOVE CUC-VAL-CAN IN CUC-FLD(2) TO CUC-VAL-CAN IN CUC(2).
           MOVE CUC-VAL-CAN IN CUC-FLD(3) TO CUC-VAL-CAN IN CUC(3).
           MOVE CUC-VAL-CAN IN CUC-FLD(4) TO CUC-VAL-CAN IN CUC(4).
           MOVE CUC-VAL-CAN IN CUC-FLD(5) TO CUC-VAL-CAN IN CUC(5).
      *
      *    CAMPO INDEXADO   CUC-CTD-CAN
           MOVE CUC-CTD-CAN IN CUC-FLD(1) TO CUC-CTD-CAN IN CUC(1).
           MOVE CUC-CTD-CAN IN CUC-FLD(2) TO CUC-CTD-CAN IN CUC(2).
           MOVE CUC-CTD-CAN IN CUC-FLD(3) TO CUC-CTD-CAN IN CUC(3).
           MOVE CUC-CTD-CAN IN CUC-FLD(4) TO CUC-CTD-CAN IN CUC(4).
           MOVE CUC-CTD-CAN IN CUC-FLD(5) TO CUC-CTD-CAN IN CUC(5).
*% INCLUDE FIN_RET_CUC_CUC OF 'COLBMCUC.TXT'
       FIN-RET-CUC-CUC.
           EXIT.
       
       RET-OFI-CUC SECTION.
       INI-RET-OFI-CUC.
*% INCLUDE INI_RET_OFI_CUC OF 'COLBMCUC.TXT'
           MOVE OFI-GLS-DESC IN CUC-FLD TO OFI-GLS-DESC IN OFI.
*% INCLUDE FIN_RET_OFI_CUC OF 'COLBMCUC.TXT'
       FIN-RET-OFI-CUC.
           EXIT.
       
       KEY-ALL-CUC SECTION.
       INI-KEY-ALL-CUC.
*% INCLUDE INI_KEY_ALL_CUC OF 'COLBMCUC.TXT'
           PERFORM KEY-CUC-CUC.
           PERFORM KEY-OFI-CUC.
*% INCLUDE FIN_KEY_ALL_CUC OF 'COLBMCUC.TXT'
       FIN-KEY-ALL-CUC.
           EXIT.
       
       KEY-CUC-CUC SECTION.
       INI-KEY-CUC-CUC.
*% INCLUDE INI_KEY_CUC_CUC OF 'COLBMCUC.TXT'
           MOVE CUC-COD-OFI-CUA IN CUC-FLD TO CUC-COD-OFI-CUA IN CUC.
*% INCLUDE FIN_KEY_CUC_CUC OF 'COLBMCUC.TXT'
       FIN-KEY-CUC-CUC.
           EXIT.
       
       KEY-OFI-CUC SECTION.
       INI-KEY-OFI-CUC.
*% INCLUDE INI_KEY_OFI_CUC OF 'COLBMCUC.TXT'
*% INCLUDE FIN_KEY_OFI_CUC OF 'COLBMCUC.TXT'
       FIN-KEY-OFI-CUC.
           EXIT.
       
       SCR-CLRA-CUC SECTION.
       INI-SCR-CLRA-CUC.
*% INCLUDE INI_SCR_CLRA_CUC OF 'COLBMCUC.TXT'
           PERFORM CLR-KEY-CUC.
           PERFORM CLR-FLD-CUC.
*% INCLUDE FIN_SCR_CLRA_CUC OF 'COLBMCUC.TXT'
       FIN-SCR-CLRA-CUC.
           EXIT.
       
       CLR-KEY-CUC SECTION.
       INI-CLR-KEY-CUC.
*% INCLUDE INI_CLR_KEY_CUC OF 'COLBMCUC.TXT'
           MOVE ZEROES TO CUC-COD-OFI-CUA IN CUC-FLD.
*% INCLUDE FIN_CLR_KEY_CUC OF 'COLBMCUC.TXT'
       FIN-CLR-KEY-CUC.
           EXIT.
       
       CLR-FLD-CUC SECTION.
       INI-CLR-FLD-CUC.
*% INCLUDE INI_CLR_FLD_CUC OF 'COLBMCUC.TXT'
           PERFORM CLR-CUC-CUC.
           PERFORM CLR-OFI-CUC.
*% INCLUDE FIN_CLR_FLD_CUC OF 'COLBMCUC.TXT'
       FIN-CLR-FLD-CUC.
           EXIT.
       
       CLR-CUC-CUC SECTION.
       INI-CLR-CUC-CUC.
*% INCLUDE INI_CLR_CUC_CUC OF 'COLBMCUC.TXT'
*% RREG = "CUC"
*% RREG_FRM = "CUC"
*% NOMB = "CUA"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CUC-FEC-CUA IN CUC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CUC-HH-CUA IN CUC-FLD.
           MOVE ZEROES TO CUC-MM-CUA IN CUC-FLD.
           MOVE ZEROES TO CUC-SS-CUA IN CUC-FLD.
           MOVE SPACES TO CUC-DRS-CAR-ABO IN CUC-FLD(1).
           MOVE SPACES TO CUC-DRS-CAR-ABO IN CUC-FLD(2).
           MOVE SPACES TO CUC-DRS-CAR-ABO IN CUC-FLD(3).
           MOVE SPACES TO CUC-DRS-CAR-ABO IN CUC-FLD(4).
           MOVE SPACES TO CUC-DRS-CAR-ABO IN CUC-FLD(5).
           MOVE ZEROES TO CUC-CTD-CUR IN CUC-FLD(1).
           MOVE ZEROES TO CUC-CTD-CUR IN CUC-FLD(2).
           MOVE ZEROES TO CUC-CTD-CUR IN CUC-FLD(3).
           MOVE ZEROES TO CUC-CTD-CUR IN CUC-FLD(4).
           MOVE ZEROES TO CUC-CTD-CUR IN CUC-FLD(5).
           MOVE ZEROES TO CUC-VAL-LIQ-CUR IN CUC-FLD(1).
           MOVE ZEROES TO CUC-VAL-LIQ-CUR IN CUC-FLD(2).
           MOVE ZEROES TO CUC-VAL-LIQ-CUR IN CUC-FLD(3).
           MOVE ZEROES TO CUC-VAL-LIQ-CUR IN CUC-FLD(4).
           MOVE ZEROES TO CUC-VAL-LIQ-CUR IN CUC-FLD(5).
           MOVE ZEROES TO CUC-VAL-CAN IN CUC-FLD(1).
           MOVE ZEROES TO CUC-VAL-CAN IN CUC-FLD(2).
           MOVE ZEROES TO CUC-VAL-CAN IN CUC-FLD(3).
           MOVE ZEROES TO CUC-VAL-CAN IN CUC-FLD(4).
           MOVE ZEROES TO CUC-VAL-CAN IN CUC-FLD(5).
           MOVE ZEROES TO CUC-CTD-CAN IN CUC-FLD(1).
           MOVE ZEROES TO CUC-CTD-CAN IN CUC-FLD(2).
           MOVE ZEROES TO CUC-CTD-CAN IN CUC-FLD(3).
           MOVE ZEROES TO CUC-CTD-CAN IN CUC-FLD(4).
           MOVE ZEROES TO CUC-CTD-CAN IN CUC-FLD(5).
*% INCLUDE FIN_CLR_CUC_CUC OF 'COLBMCUC.TXT'
       FIN-CLR-CUC-CUC.
           EXIT.
       
       CLR-OFI-CUC SECTION.
       INI-CLR-OFI-CUC.
*% INCLUDE INI_CLR_OFI_CUC OF 'COLBMCUC.TXT'
           MOVE SPACES TO OFI-GLS-DESC IN CUC-FLD.
*% INCLUDE FIN_CLR_OFI_CUC OF 'COLBMCUC.TXT'
       FIN-CLR-OFI-CUC.
           EXIT.
       
       FST-KEY-CUC SECTION.
       INI-FST-KEY-CUC.
*% INCLUDE INI_FST_KEY_CUC OF 'COLBMCUC.TXT'
           MOVE -1 TO CUC-COD-OFI-CUA-LEN IN CUC-FLD.
*% INCLUDE FIN_FST_KEY_CUC OF 'COLBMCUC.TXT'
       FIN-FST-KEY-CUC.
           EXIT.
       
       FST-FLD-CUC SECTION.
       INI-FST-FLD-CUC.
*% INCLUDE INI_FST_FLD_CUC OF 'COLBMCUC.TXT'
           MOVE -1 TO OFI-GLS-DESC-LEN IN CUC-FLD.
*% INCLUDE FIN_FST_FLD_CUC OF 'COLBMCUC.TXT'
       FIN-FST-FLD-CUC.
           EXIT.
       
       PRO-KEY-CUC SECTION.
       INI-PRO-KEY-CUC.
*% INCLUDE INI_PRO_KEY_CUC OF 'COLBMCUC.TXT'
           PERFORM PRO-MKY-CUC.
*% INCLUDE FIN_PRO_KEY_CUC OF 'COLBMCUC.TXT'
       FIN-PRO-KEY-CUC.
           EXIT.
       
