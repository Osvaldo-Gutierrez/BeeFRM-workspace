       EDT-IKY-CNO SECTION.
       INI-EDT-IKY-CNO.
*% INCLUDE EDT_IKY_CNO OF 'COLBMCNO.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF DBC-NUM-ICLI-LEN IN CNO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DBC-NUM-ICLI-LEN IN CNO-FLD
                GO TO FIN-EDT-IKY-CNO.
           MOVE DBC-NUM-ICLI IN CNO-FLD TO
                DBC-NUM-ICLI-EDT IN CNO-EDT.
           MOVE DBC-NUM-ICLI-EDT IN CNO-EDT TO
                DBC-NUM-ICLI-ALF IN CNO-FLD.
      * IDC-FIN --> BMCHIBCI011.SRCAIU EDICION NUMERICA
*% RREG = "NGA"
*% RREG_FRM = "NGA"
*% NOMB = "AUX2"
           IF FRM-NUM-NVEN-LEN IN CNO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-NVEN-LEN IN CNO-FLD
                GO TO FIN-EDT-IKY-CNO.
           MOVE FRM-NUM-NVEN IN CNO-FLD TO FRM-NUM-NVEN-EDT IN CNO-EDT.
