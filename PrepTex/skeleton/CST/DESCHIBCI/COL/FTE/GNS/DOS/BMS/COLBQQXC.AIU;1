      * IDC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION

       VAL-DBC-NUM-ICLI SECTION.
       INI-VAL-DBC-NUM-ICLI.
*% INCLUDE INI_DBC_NUM_ICLI  OF 'COLBMQXC.TXT'
*% INCLUDE DBC_NUM_ICLI  OF 'COLBMQXC.TXT'
*% INCLUDE FIN_DBC_NUM_ICLI  OF 'COLBMQXC.TXT'
       FIN-VAL-DBC-NUM-ICLI.
           EXIT.

       VAL-DBC-VRF-ICLI SECTION.
       INI-VAL-DBC-VRF-ICLI.
*% INCLUDE INI_DBC_VRF_ICLI  OF 'COLBMQXC.TXT'
           IF DBC-VRF-ICLI IN QXC-FLD NOT > SPACES
              MOVE SPACES TO DBC-VRF-ICLI IN QXC-FLD.
*% INCLUDE 'GNSSRC:SETVRF202.SRC'
*%! INCLUDE 'PROVRF'
*% INCLUDE DBC_VRF_ICLI  OF 'COLBMQXC.TXT'
*% INCLUDE FIN_DBC_VRF_ICLI  OF 'COLBMQXC.TXT'
*% LBL = ""
       FIN-VAL-DBC-VRF-ICLI.
           EXIT.
       VAL-DBC-IND-ICLI SECTION.
       INI-VAL-DBC-IND-ICLI.
*% INCLUDE INI_DBC_IND_ICLI  OF 'COLBMQXC.TXT'
           IF DBC-IND-ICLI IN QXC-FLD NOT > SPACES
              MOVE SPACES TO DBC-IND-ICLI IN QXC-FLD.
*% INCLUDE 'GNSSRC:VALIDC202.SRC'
*% INCLUDE DBC_IND_ICLI  OF 'COLBMQXC.TXT'
*% INCLUDE FIN_DBC_IND_ICLI  OF 'COLBMQXC.TXT'
       FIN-VAL-DBC-IND-ICLI.
           EXIT.

       VAL-DBC-GLS-ICLI SECTION.
       INI-VAL-DBC-GLS-ICLI.
*% LBL = "DBC_GLS_ICLI  OF 'COLBMQXC.TXT'"
*% INCLUDE INI_DBC_GLS_ICLI  OF 'COLBMQXC.TXT'
           IF DBC-GLS-ICLI IN QXC-FLD NOT > IDC-INIC-GLS 
              MOVE IDC-INIC-GLS TO DBC-GLS-ICLI IN QXC-FLD.
      *VAL-GLS para IDC valida glosa ( extension ) de identificador
           MOVE DBC-GLS-ICLI IN QXC-FLD TO IDC-GLS-RUTV.
           PERFORM VAL-GLS.
           MOVE IDC-GLS-RUTV TO DBC-GLS-ICLI IN QXC-FLD.
*% INCLUDE DBC_GLS_ICLI  OF 'COLBMQXC.TXT'
*% INCLUDE FIN_DBC_GLS_ICLI  OF 'COLBMQXC.TXT'
       FIN-VAL-DBC-GLS-ICLI.
           EXIT.

      * IDC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-FRM-CAI-IOPI SECTION.
       INI-VAL-FRM-CAI-IOPI.
*% LBL = "FRM_CAI_IOPI OF 'COLBMQXC.TXT'"
*% INCLUDE INI_FRM_CAI_IOPI OF 'COLBMQXC.TXT'
           IF FRM-CAI-IOPI IN QXC-FLD NOT > SPACES
              MOVE SPACES TO FRM-CAI-IOPI IN QXC-FLD.
*% INCLUDE FRM_CAI_IOPI OF 'COLBMQXC.TXT'
*% INCLUDE FIN_FRM_CAI_IOPI OF 'COLBMQXC.TXT'
       FIN-VAL-FRM-CAI-IOPI.
           EXIT.
       
       VAL-FRM-IIC-IOPI SECTION.
       INI-VAL-FRM-IIC-IOPI.
*% LBL = "FRM_IIC_IOPI OF 'COLBMQXC.TXT'"
*% INCLUDE INI_FRM_IIC_IOPI OF 'COLBMQXC.TXT'
*% INCLUDE FRM_IIC_IOPI OF 'COLBMQXC.TXT'
*% INCLUDE FIN_FRM_IIC_IOPI OF 'COLBMQXC.TXT'
       FIN-VAL-FRM-IIC-IOPI.
           EXIT.
