       UNP-FLD-QXC SECTION.
       INI-UNP-FLD-QXC.
*% INCLUDE INI_UNP_FLD_QXC OF 'COLBMQXC.TXT'
           PERFORM UNP-DBC-QXC.
*% INCLUDE FIN_UNP_FLD_QXC OF 'COLBMQXC.TXT'
       FIN-UNP-FLD-QXC.
           EXIT.
       
       UNP-DBC-QXC SECTION.
       INI-UNP-DBC-QXC.
*% INCLUDE INI_UNP_DBC_QXC OF 'COLBMQXC.TXT'
*% INCLUDE FIN_UNP_DBC_QXC OF 'COLBMQXC.TXT'
       FIN-UNP-DBC-QXC.
           EXIT.
       
       VAL-KEY-QXC SECTION.
       INI-VAL-KEY-QXC.
           PERFORM VAL-IKY-QXC.
           PERFORM VAL-MKY-QXC.
           PERFORM VAL-AKY-QXC.
       FIN-VAL-KEY-QXC.
           EXIT.
       
       VAL-IKY-QXC SECTION.
       INI-VAL-IKY-QXC.
*% INCLUDE INI_VAL_IKY_QXC OF 'COLBMQXC.TXT'
           PERFORM VAL-FRM-CAI-IOPI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-IOPI-LEN IN QXC-FLD
                GO TO FIN-VAL-IKY-QXC.
           PERFORM VAL-FRM-IIC-IOPI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-IOPI-LEN IN QXC-FLD
                GO TO FIN-VAL-IKY-QXC.
*% INCLUDE FIN_VAL_IKY_QXC OF 'COLBMQXC.TXT'
       FIN-VAL-IKY-QXC.
           EXIT.
       
       VAL-MKY-QXC SECTION.
       INI-VAL-MKY-QXC.
*% INCLUDE INI_VAL_MKY_QXC OF 'COLBMQXC.TXT'
*% INCLUDE FIN_VAL_MKY_QXC OF 'COLBMQXC.TXT'
       FIN-VAL-MKY-QXC.
           EXIT.
       
       VAL-AKY-QXC SECTION.
       INI-VAL-AKY-QXC.
*% INCLUDE INI_VAL_AKY_QXC OF 'COLBMQXC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * VRF-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * VRF-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DBC-IND-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-IND-ICLI-LEN IN QXC-FLD
                GO TO FIN-VAL-AKY-QXC.
           PERFORM VAL-DBC-GLS-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-GLS-ICLI-LEN IN QXC-FLD
                GO TO FIN-VAL-AKY-QXC.
           PERFORM VAL-DBC-NUM-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-NUM-ICLI-LEN IN QXC-FLD
                GO TO FIN-VAL-AKY-QXC.
           PERFORM VAL-DBC-VRF-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-VRF-ICLI-LEN IN QXC-FLD
                GO TO FIN-VAL-AKY-QXC.
      * IDC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% INCLUDE FIN_VAL_AKY_QXC OF 'COLBMQXC.TXT'
       FIN-VAL-AKY-QXC.
           EXIT.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% OBL = "S"
*% LBL = "DBC_VRF_ICLI OF 'COLBMQXC.TXT'"
*% IZR = "S"
*% ZRO = "S"
*% KO0 = "K"
*% ZRO = "S"
*% LMX = "7"
*% FRM_IFLD = ""
      * VRF-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
      * VRF-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
