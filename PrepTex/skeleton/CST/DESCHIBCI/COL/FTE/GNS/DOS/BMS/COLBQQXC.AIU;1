      *
      * COLBQQXC
      * Version 3.0.1 Nov-2005
      * 24-Jan-17 10:55 AM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "QXC"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMQXC.TXT'
       SET-TAG-COLBQQXC SECTION.
       INI-SET-TAG-COLBQQXC.
           MOVE
           '<IDNMOD NOM=$COLBQQXC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$24-Jan-17 10:55 AM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQQXC.
           EXIT.
           
       PUT-ALL-QXC SECTION.
       INI-PUT-ALL-QXC.
*% INCLUDE INI_PUT_ALL_QXC OF 'COLBMQXC.TXT'
           PERFORM PUT-DBC-QXC.
*% INCLUDE FIN_PUT_ALL_QXC OF 'COLBMQXC.TXT'
       FIN-PUT-ALL-QXC.
           EXIT.
       
       PUT-DBC-QXC SECTION.
       INI-PUT-DBC-QXC.
*% INCLUDE INI_PUT_DBC_QXC OF 'COLBMQXC.TXT'
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-QXC.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-QXC
               GO TO FIN-PUT-DBC-QXC.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN QXC-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN QXC-FLD.
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN QXC-FLD.
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN QXC-FLD.
      * IDC-FIN --> BMCHIBCI001210.SRCAIU 
*% INCLUDE FIN_PUT_DBC_QXC OF 'COLBMQXC.TXT'
       FIN-PUT-DBC-QXC.
           EXIT.
       
       KEY-ALL-QXC SECTION.
       INI-KEY-ALL-QXC.
*% INCLUDE INI_KEY_ALL_QXC OF 'COLBMQXC.TXT'
           PERFORM KEY-DBC-QXC.
*% INCLUDE FIN_KEY_ALL_QXC OF 'COLBMQXC.TXT'
       FIN-KEY-ALL-QXC.
           EXIT.
       
       KEY-DBC-QXC SECTION.
       INI-KEY-DBC-QXC.
*% INCLUDE INI_KEY_DBC_QXC OF 'COLBMQXC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN QXC-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN QXC-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN QXC-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN QXC-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_KEY_DBC_QXC OF 'COLBMQXC.TXT'
       FIN-KEY-DBC-QXC.
           EXIT.
       
       SCR-CLRA-QXC SECTION.
       INI-SCR-CLRA-QXC.
*% INCLUDE INI_SCR_CLRA_QXC OF 'COLBMQXC.TXT'
           PERFORM CLR-KEY-QXC.
           PERFORM CLR-FLD-QXC.
*% INCLUDE FIN_SCR_CLRA_QXC OF 'COLBMQXC.TXT'
       FIN-SCR-CLRA-QXC.
           EXIT.
       
       CLR-KEY-QXC SECTION.
       INI-CLR-KEY-QXC.
*% INCLUDE INI_CLR_KEY_QXC OF 'COLBMQXC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN QXC-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN QXC-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN QXC-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN QXC-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-CAI-IOPI IN QXC-FLD.
           MOVE ZEROES TO FRM-IIC-IOPI IN QXC-FLD.
*% INCLUDE FIN_CLR_KEY_QXC OF 'COLBMQXC.TXT'
       FIN-CLR-KEY-QXC.
           EXIT.
       
       CLR-FLD-QXC SECTION.
       INI-CLR-FLD-QXC.
*% INCLUDE INI_CLR_FLD_QXC OF 'COLBMQXC.TXT'
           MOVE SPACES TO FRM-GLS-NOMC IN QXC-FLD.
           MOVE SPACES TO FRM-IND-HMAS IN QXC-FLD.
           MOVE ZEROES TO FRM-NUM-TOCC IN QXC-FLD.
           MOVE SPACES TO FRM-CAI-IOPC IN QXC-FLD.
           MOVE ZEROES TO FRM-IIC-IOPC IN QXC-FLD.
           MOVE ZEROES TO FRM-SGV-TINT IN QXC-FLD.
           MOVE SPACES TO FRM-COD-TINT IN QXC-FLD.
           MOVE SPACES TO FRM-GLS-TINT IN QXC-FLD.
           MOVE SPACES TO FRM-COD-COOC IN QXC-FLD.
           MOVE SPACES TO FRM-GLS-COOC IN QXC-FLD.
           MOVE SPACES TO FRM-COD-MONE IN QXC-FLD.
           MOVE SPACES TO FRM-GLS-MONE IN QXC-FLD.
           MOVE ZEROES TO FRM-VAL-CRED IN QXC-FLD.
           MOVE ZEROES TO FRM-NUM-VCAN IN QXC-FLD.
           MOVE ZEROES TO FRM-NUM-NCXV IN QXC-FLD.
           MOVE SPACES TO FRM-COD-TDEU IN QXC-FLD.
           MOVE SPACES TO FRM-GLS-TDEU IN QXC-FLD.
           MOVE ZEROES TO FRM-VAL-SCRE IN QXC-FLD.
           MOVE ZEROES TO FRM-NUM-VCOR IN QXC-FLD.
           MOVE ZEROES TO FRM-VAL-SFIN IN QXC-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FVEN IN QXC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FMAV"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FMAV IN QXC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           PERFORM CLR-DBC-QXC.
*% INCLUDE FIN_CLR_FLD_QXC OF 'COLBMQXC.TXT'
       FIN-CLR-FLD-QXC.
           EXIT.
       
       CLR-DBC-QXC SECTION.
       INI-CLR-DBC-QXC.
*% INCLUDE INI_CLR_DBC_QXC OF 'COLBMQXC.TXT'
*% INCLUDE FIN_CLR_DBC_QXC OF 'COLBMQXC.TXT'
       FIN-CLR-DBC-QXC.
           EXIT.
       
       FST-KEY-QXC SECTION.
       INI-FST-KEY-QXC.
*% INCLUDE INI_FST_KEY_QXC OF 'COLBMQXC.TXT'
           MOVE -1 TO FRM-CAI-IOPI-LEN IN QXC-FLD.
*% INCLUDE FIN_FST_KEY_QXC OF 'COLBMQXC.TXT'
       FIN-FST-KEY-QXC.
           EXIT.
       
       FST-FLD-QXC SECTION.
       INI-FST-FLD-QXC.
*% INCLUDE INI_FST_FLD_QXC OF 'COLBMQXC.TXT'
*% INCLUDE FIN_FST_FLD_QXC OF 'COLBMQXC.TXT'
       FIN-FST-FLD-QXC.
           EXIT.
       
       PRO-KEY-QXC SECTION.
       INI-PRO-KEY-QXC.
*% INCLUDE INI_PRO_KEY_QXC OF 'COLBMQXC.TXT'
           MOVE FRM-CPIM-AEY TO FRM-CAI-IOPI-ATR IN QXC-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IIC-IOPI-ATR IN QXC-FLD.
           PERFORM PRO-IKY-QXC.
           PERFORM PRO-AKY-QXC.
           PERFORM PRO-MKY-QXC.
*% INCLUDE FIN_PRO_KEY_QXC OF 'COLBMQXC.TXT'
       FIN-PRO-KEY-QXC.
           EXIT.
       
       PRO-IKY-QXC SECTION.
       INI-PRO-IKY-QXC.
*% INCLUDE INI_PRO_IKY_QXC OF 'COLBMQXC.TXT'
*% INCLUDE FIN_PRO_IKY_QXC OF 'COLBMQXC.TXT'
       FIN-PRO-IKY-QXC.
           EXIT.
       
       PRO-MKY-QXC SECTION.
       INI-PRO-MKY-QXC.
*% INCLUDE INI_PRO_MKY_QXC OF 'COLBMQXC.TXT'
*% INCLUDE FIN_PRO_MKY_QXC OF 'COLBMQXC.TXT'
       FIN-PRO-MKY-QXC.
           EXIT.
       
       PRO-AKY-QXC SECTION.
       INI-PRO-AKY-QXC.
*% INCLUDE INI_PRO_AKY_QXC OF 'COLBMQXC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO DBC-NUM-ICLI-ATR IN QXC-FLD.
           MOVE FRM-CPIM-AEY TO DBC-VRF-ICLI-ATR IN QXC-FLD.
           MOVE FRM-CPIM-AEY TO DBC-IND-ICLI-ATR IN QXC-FLD.
           MOVE FRM-CPIM-AEY TO DBC-GLS-ICLI-ATR IN QXC-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
*% INCLUDE FIN_PRO_AKY_QXC OF 'COLBMQXC.TXT'
