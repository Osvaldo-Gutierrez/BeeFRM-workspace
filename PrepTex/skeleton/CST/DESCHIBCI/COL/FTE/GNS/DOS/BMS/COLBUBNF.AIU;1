*% FRM = "BNF"
      *
      * COLBUBNF
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 03:11 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMBNF.TXT'
       SET-TAG-COLBUBNF SECTION.
       INI-SET-TAG-COLBUBNF.
           MOVE
           '<IDNMOD NOM=$COLBUBNF$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 03:11 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUBNF.
           EXIT.
           
       RET-ALL-BNF SECTION.
       INI-RET-ALL-BNF.
*% INCLUDE INI_RET_ALL_BNF OF 'COLBMBNF.TXT'
           PERFORM KEY-ALL-BNF.
           PERFORM RET-RCO-BNF.
           PERFORM RET-DBC-BNF.
           PERFORM RET-OPC-BNF.
           PERFORM RET-RCD-BNF.
*% INCLUDE FIN_RET_ALL_BNF OF 'COLBMBNF.TXT'
       FIN-RET-ALL-BNF.
           EXIT.
       
       RET-RCO-BNF SECTION.
       INI-RET-RCO-BNF.
*% INCLUDE INI_RET_RCO_BNF OF 'COLBMBNF.TXT'
           MOVE RCO-TIP-BENF IN BNF-FLD TO RCO-TIP-BENF IN RCO.
*% INCLUDE FIN_RET_RCO_BNF OF 'COLBMBNF.TXT'
       FIN-RET-RCO-BNF.
           EXIT.
       
       RET-DBC-BNF SECTION.
       INI-RET-DBC-BNF.
*% INCLUDE INI_RET_DBC_BNF OF 'COLBMBNF.TXT'
           MOVE DBC-IND-TICL IN BNF-FLD TO DBC-IND-TICL IN DBC.
           MOVE DBC-GLS-APAT IN BNF-FLD TO DBC-GLS-APAT IN DBC.
           MOVE DBC-GLS-AMAT IN BNF-FLD TO DBC-GLS-AMAT IN DBC.
           MOVE DBC-GLS-NOMB IN BNF-FLD TO DBC-GLS-NOMB IN DBC.
*% INCLUDE FIN_RET_DBC_BNF OF 'COLBMBNF.TXT'
       FIN-RET-DBC-BNF.
           EXIT.
       
       RET-OPC-BNF SECTION.
       INI-RET-OPC-BNF.
*% INCLUDE INI_RET_OPC_BNF OF 'COLBMBNF.TXT'
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE OPC-NUM-DCOL IN BNF-FLD TO OPC-NUM-DCOL IN OPC.
           MOVE OPC-NUM-MCOL IN BNF-FLD TO OPC-NUM-MCOL IN OPC.
           MOVE OPC-NUM-SCOL IN BNF-FLD TO OPC-NUM-SCOL IN OPC.
           MOVE OPC-NUM-ACOL IN BNF-FLD TO OPC-NUM-ACOL IN OPC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_RET_OPC_BNF OF 'COLBMBNF.TXT'
       FIN-RET-OPC-BNF.
           EXIT.
       
       RET-RCD-BNF SECTION.
       INI-RET-RCD-BNF.
*% INCLUDE INI_RET_RCD_BNF OF 'COLBMBNF.TXT'
           MOVE RCD-GLS-DIRE IN BNF-FLD TO RCD-GLS-DIRE IN RCD.
           MOVE RCD-COD-LUGA IN BNF-FLD TO RCD-COD-LUGA IN RCD.
           MOVE RCD-GLS-FONO IN BNF-FLD TO RCD-GLS-FONO IN RCD.
*% INCLUDE FIN_RET_RCD_BNF OF 'COLBMBNF.TXT'
       FIN-RET-RCD-BNF.
           EXIT.
       
       KEY-ALL-BNF SECTION.
       INI-KEY-ALL-BNF.
*% INCLUDE INI_KEY_ALL_BNF OF 'COLBMBNF.TXT'
           PERFORM KEY-RCO-BNF.
           PERFORM KEY-DBC-BNF.
           PERFORM KEY-OPC-BNF.
           PERFORM KEY-RCD-BNF.
*% INCLUDE FIN_KEY_ALL_BNF OF 'COLBMBNF.TXT'
       FIN-KEY-ALL-BNF.
           EXIT.
       
       KEY-RCO-BNF SECTION.
       INI-KEY-RCO-BNF.
*% INCLUDE INI_KEY_RCO_BNF OF 'COLBMBNF.TXT'
           MOVE RCO-CAI-IOPE IN BNF-FLD TO RCO-CAI-IOPE IN RCO.
           MOVE RCO-IIC-IOPE IN BNF-FLD TO RCO-IIC-IOPE IN RCO.
           MOVE RCO-COD-TREL IN BNF-FLD TO RCO-COD-TREL IN RCO.
*% INCLUDE FIN_KEY_RCO_BNF OF 'COLBMBNF.TXT'
       FIN-KEY-RCO-BNF.
           EXIT.
       
       KEY-DBC-BNF SECTION.
       INI-KEY-DBC-BNF.
*% INCLUDE INI_KEY_DBC_BNF OF 'COLBMBNF.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN BNF-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN BNF-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN BNF-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN BNF-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_KEY_DBC_BNF OF 'COLBMBNF.TXT'
       FIN-KEY-DBC-BNF.
           EXIT.
       
       KEY-OPC-BNF SECTION.
       INI-KEY-OPC-BNF.
*% INCLUDE INI_KEY_OPC_BNF OF 'COLBMBNF.TXT'
*% INCLUDE FIN_KEY_OPC_BNF OF 'COLBMBNF.TXT'
       FIN-KEY-OPC-BNF.
           EXIT.
       
       KEY-RCD-BNF SECTION.
       INI-KEY-RCD-BNF.
*% INCLUDE INI_KEY_RCD_BNF OF 'COLBMBNF.TXT'
*% INCLUDE FIN_KEY_RCD_BNF OF 'COLBMBNF.TXT'
       FIN-KEY-RCD-BNF.
           EXIT.
       
       SCR-CLRA-BNF SECTION.
       INI-SCR-CLRA-BNF.
*% INCLUDE INI_SCR_CLRA_BNF OF 'COLBMBNF.TXT'
           PERFORM CLR-KEY-BNF.
           PERFORM CLR-FLD-BNF.
*% INCLUDE FIN_SCR_CLRA_BNF OF 'COLBMBNF.TXT'
       FIN-SCR-CLRA-BNF.
           EXIT.
       
       CLR-KEY-BNF SECTION.
       INI-CLR-KEY-BNF.
*% INCLUDE INI_CLR_KEY_BNF OF 'COLBMBNF.TXT'
           MOVE SPACES TO RCO-CAI-IOPE IN BNF-FLD.
           MOVE ZEROES TO RCO-IIC-IOPE IN BNF-FLD.
           MOVE SPACES TO RCO-COD-TREL IN BNF-FLD.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN BNF-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN BNF-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN BNF-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN BNF-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_KEY_BNF OF 'COLBMBNF.TXT'
       FIN-CLR-KEY-BNF.
           EXIT.
       
       CLR-FLD-BNF SECTION.
       INI-CLR-FLD-BNF.
*% INCLUDE INI_CLR_FLD_BNF OF 'COLBMBNF.TXT'
           MOVE SPACES TO FRM-GLS-TREL IN BNF-FLD.
           MOVE SPACES TO FRM-GLS-BENF IN BNF-FLD.
           MOVE SPACES TO FRM-GLS-LUGA IN BNF-FLD.
           MOVE SPACES TO FRM-GLS-DIRC IN BNF-FLD.
           MOVE SPACES TO FRM-GLS-TDIR IN BNF-FLD.
           MOVE SPACES TO FRM-GLS-NDIR IN BNF-FLD.
           MOVE SPACES TO FRM-GLS-CONT IN BNF-FLD.
           MOVE SPACES TO FRM-GLS-GLS1 IN BNF-FLD.
           MOVE SPACES TO FRM-GLS-GLS2 IN BNF-FLD.
           MOVE ZEROES TO FRM-NUM-DOCU IN BNF-FLD.
           PERFORM CLR-RCO-BNF.
           PERFORM CLR-DBC-BNF.
           PERFORM CLR-OPC-BNF.
           PERFORM CLR-RCD-BNF.
*% INCLUDE FIN_CLR_FLD_BNF OF 'COLBMBNF.TXT'
       FIN-CLR-FLD-BNF.
           EXIT.
       
       CLR-RCO-BNF SECTION.
       INI-CLR-RCO-BNF.
*% INCLUDE INI_CLR_RCO_BNF OF 'COLBMBNF.TXT'
           MOVE SPACES TO RCO-TIP-BENF IN BNF-FLD.
*% INCLUDE FIN_CLR_RCO_BNF OF 'COLBMBNF.TXT'
       FIN-CLR-RCO-BNF.
           EXIT.
       
       CLR-DBC-BNF SECTION.
       INI-CLR-DBC-BNF.
*% INCLUDE INI_CLR_DBC_BNF OF 'COLBMBNF.TXT'
           MOVE SPACES TO DBC-IND-TICL IN BNF-FLD.
           MOVE SPACES TO DBC-GLS-APAT IN BNF-FLD.
           MOVE SPACES TO DBC-GLS-AMAT IN BNF-FLD.
           MOVE SPACES TO DBC-GLS-NOMB IN BNF-FLD.
*% INCLUDE FIN_CLR_DBC_BNF OF 'COLBMBNF.TXT'
       FIN-CLR-DBC-BNF.
           EXIT.
       
       CLR-OPC-BNF SECTION.
       INI-CLR-OPC-BNF.
*% INCLUDE INI_CLR_OPC_BNF OF 'COLBMBNF.TXT'
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO OPC-FEC-FCOL IN BNF-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_OPC_BNF OF 'COLBMBNF.TXT'
       FIN-CLR-OPC-BNF.
           EXIT.
       
       CLR-RCD-BNF SECTION.
       INI-CLR-RCD-BNF.
*% INCLUDE INI_CLR_RCD_BNF OF 'COLBMBNF.TXT'
           MOVE SPACES TO RCD-GLS-DIRE IN BNF-FLD.
           MOVE SPACES TO RCD-COD-LUGA IN BNF-FLD.
           MOVE SPACES TO RCD-GLS-FONO IN BNF-FLD.
*% INCLUDE FIN_CLR_RCD_BNF OF 'COLBMBNF.TXT'
       FIN-CLR-RCD-BNF.
           EXIT.
       
       FST-KEY-BNF SECTION.
       INI-FST-KEY-BNF.
*% INCLUDE INI_FST_KEY_BNF OF 'COLBMBNF.TXT'
           MOVE -1 TO RCO-CAI-IOPE-LEN IN BNF-FLD.
*% INCLUDE FIN_FST_KEY_BNF OF 'COLBMBNF.TXT'
       FIN-FST-KEY-BNF.
           EXIT.
       
       FST-FLD-BNF SECTION.
       INI-FST-FLD-BNF.
*% INCLUDE INI_FST_FLD_BNF OF 'COLBMBNF.TXT'
           MOVE -1 TO DBC-IND-TICL-LEN IN BNF-FLD.
*% INCLUDE FIN_FST_FLD_BNF OF 'COLBMBNF.TXT'
       FIN-FST-FLD-BNF.
           EXIT.
       
       PRO-KEY-BNF SECTION.
       INI-PRO-KEY-BNF.
*% INCLUDE INI_PRO_KEY_BNF OF 'COLBMBNF.TXT'
           PERFORM PRO-MKY-BNF.
*% INCLUDE FIN_PRO_KEY_BNF OF 'COLBMBNF.TXT'
       FIN-PRO-KEY-BNF.
           EXIT.
       
       PRO-MKY-BNF SECTION.
       INI-PRO-MKY-BNF.
*% INCLUDE INI_PRO_MKY_BNF OF 'COLBMBNF.TXT'
*% INCLUDE FIN_PRO_MKY_BNF OF 'COLBMBNF.TXT'
       FIN-PRO-MKY-BNF.
           EXIT.
       
       PRO-FLD-BNF SECTION.
       INI-PRO-FLD-BNF.
*% INCLUDE INI_PRO_FLD_BNF OF 'COLBMBNF.TXT'
           MOVE FRM-CPIM-AEY TO FRM-GLS-DIRC-ATR IN BNF-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-NDIR-ATR IN BNF-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-CONT-ATR IN BNF-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-GLS1-ATR IN BNF-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-GLS2-ATR IN BNF-FLD.
           MOVE FRM-CPIM-AEY TO FRM-NUM-DOCU-ATR IN BNF-FLD.
           PERFORM PRO-RCO-BNF.
           PERFORM PRO-DBC-BNF.
           PERFORM PRO-OPC-BNF.
           PERFORM PRO-RCD-BNF.
*% INCLUDE FIN_PRO_FLD_BNF OF 'COLBMBNF.TXT'
       FIN-PRO-FLD-BNF.
           EXIT.
       
       PRO-RCO-BNF SECTION.
       INI-PRO-RCO-BNF.
*% INCLUDE INI_PRO_RCO_BNF OF 'COLBMBNF.TXT'
           MOVE FRM-CPIM-AEY TO RCO-TIP-BENF-ATR IN BNF-FLD.
*% INCLUDE FIN_PRO_RCO_BNF OF 'COLBMBNF.TXT'
       FIN-PRO-RCO-BNF.
           EXIT.
       
       PRO-DBC-BNF SECTION.
       INI-PRO-DBC-BNF.
*% INCLUDE INI_PRO_DBC_BNF OF 'COLBMBNF.TXT'
           MOVE FRM-CPIM-AEY TO DBC-IND-TICL-ATR IN BNF-FLD.
           MOVE FRM-CPIM-AEY TO DBC-GLS-APAT-ATR IN BNF-FLD.
           MOVE FRM-CPIM-AEY TO DBC-GLS-AMAT-ATR IN BNF-FLD.
           MOVE FRM-CPIM-AEY TO DBC-GLS-NOMB-ATR IN BNF-FLD.
*% INCLUDE FIN_PRO_DBC_BNF OF 'COLBMBNF.TXT'
       FIN-PRO-DBC-BNF.
           EXIT.
       
       PRO-OPC-BNF SECTION.
       INI-PRO-OPC-BNF.
*% INCLUDE INI_PRO_OPC_BNF OF 'COLBMBNF.TXT'
*% INCLUDE FIN_PRO_OPC_BNF OF 'COLBMBNF.TXT'
       FIN-PRO-OPC-BNF.
           EXIT.
       
       PRO-RCD-BNF SECTION.
       INI-PRO-RCD-BNF.
*% INCLUDE INI_PRO_RCD_BNF OF 'COLBMBNF.TXT'
           MOVE FRM-CPIM-AEY TO RCD-GLS-DIRE-ATR IN BNF-FLD.
           MOVE FRM-CPIM-AEY TO RCD-COD-LUGA-ATR IN BNF-FLD.
           MOVE FRM-CPIM-AEY TO RCD-GLS-FONO-ATR IN BNF-FLD.
*% INCLUDE FIN_PRO_RCD_BNF OF 'COLBMBNF.TXT'
       FIN-PRO-RCD-BNF.
           EXIT.
       
       UNP-KEY-BNF SECTION.
       INI-UNP-KEY-BNF.
*% INCLUDE INI_UNP_KEY_BNF OF 'COLBMBNF.TXT'
           PERFORM UNP-IKY-BNF.
           PERFORM UNP-AKY-BNF.
           PERFORM UNP-MKY-BNF.
*% INCLUDE FIN_UNP_KEY_BNF OF 'COLBMBNF.TXT'
       FIN-UNP-KEY-BNF.
           EXIT.
       
       UNP-IKY-BNF SECTION.
       INI-UNP-IKY-BNF.
*% INCLUDE INI_UNP_IKY_BNF OF 'COLBMBNF.TXT'
           MOVE FRM-CPIM-UBY TO RCO-CAI-IOPE-ATR.
           MOVE FRM-CPIM-UBY TO RCO-IIC-IOPE-ATR.
           MOVE FRM-CPIM-UBY TO RCO-COD-TREL-ATR.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO DBC-NUM-ICLI-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO DBC-VRF-ICLI-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO DBC-IND-ICLI-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO DBC-GLS-ICLI-ATR IN BNF-FLD.
      * IDC-FIN --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
*% INCLUDE FIN_UNP_IKY_BNF OF 'COLBMBNF.TXT'
       FIN-UNP-IKY-BNF.
           EXIT.
       
       UNP-MKY-BNF SECTION.
       INI-UNP-MKY-BNF.
*% INCLUDE INI_UNP_MKY_BNF OF 'COLBMBNF.TXT'
*% INCLUDE FIN_UNP_MKY_BNF OF 'COLBMBNF.TXT'
       FIN-UNP-MKY-BNF.
           EXIT.
       
       UNP-AKY-BNF SECTION.
       INI-UNP-AKY-BNF.
*% INCLUDE INI_UNP_AKY_BNF OF 'COLBMBNF.TXT'
*% INCLUDE FIN_UNP_AKY_BNF OF 'COLBMBNF.TXT'
       FIN-UNP-AKY-BNF.
           EXIT.
       
       UNP-FLD-BNF SECTION.
       INI-UNP-FLD-BNF.
*% INCLUDE INI_UNP_FLD_BNF OF 'COLBMBNF.TXT'
           MOVE FRM-CPIM-UBY TO FRM-GLS-DIRC-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-NDIR-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-CONT-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-GLS1-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-GLS2-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO FRM-NUM-DOCU-ATR IN BNF-FLD.
           PERFORM UNP-RCO-BNF.
           PERFORM UNP-DBC-BNF.
           PERFORM UNP-OPC-BNF.
           PERFORM UNP-RCD-BNF.
*% INCLUDE FIN_UNP_FLD_BNF OF 'COLBMBNF.TXT'
       FIN-UNP-FLD-BNF.
           EXIT.
       
       UNP-RCO-BNF SECTION.
       INI-UNP-RCO-BNF.
*% INCLUDE INI_UNP_RCO_BNF OF 'COLBMBNF.TXT'
           MOVE FRM-CPIM-UBY TO RCO-TIP-BENF-ATR IN BNF-FLD.
*% INCLUDE FIN_UNP_RCO_BNF OF 'COLBMBNF.TXT'
       FIN-UNP-RCO-BNF.
           EXIT.
       
       UNP-DBC-BNF SECTION.
       INI-UNP-DBC-BNF.
*% INCLUDE INI_UNP_DBC_BNF OF 'COLBMBNF.TXT'
           MOVE FRM-CPIM-UBY TO DBC-IND-TICL-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO DBC-GLS-APAT-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO DBC-GLS-AMAT-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO DBC-GLS-NOMB-ATR IN BNF-FLD.
*% INCLUDE FIN_UNP_DBC_BNF OF 'COLBMBNF.TXT'
       FIN-UNP-DBC-BNF.
           EXIT.
       
       UNP-OPC-BNF SECTION.
       INI-UNP-OPC-BNF.
*% INCLUDE INI_UNP_OPC_BNF OF 'COLBMBNF.TXT'
*% INCLUDE FIN_UNP_OPC_BNF OF 'COLBMBNF.TXT'
       FIN-UNP-OPC-BNF.
           EXIT.
       
       UNP-RCD-BNF SECTION.
       INI-UNP-RCD-BNF.
*% INCLUDE INI_UNP_RCD_BNF OF 'COLBMBNF.TXT'
           MOVE FRM-CPIM-UBY TO RCD-GLS-DIRE-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO RCD-COD-LUGA-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO RCD-GLS-FONO-ATR IN BNF-FLD.
*% INCLUDE FIN_UNP_RCD_BNF OF 'COLBMBNF.TXT'
       FIN-UNP-RCD-BNF.
           EXIT.
       
       VAL-MKY-BNF SECTION.
       INI-VAL-MKY-BNF.
*% INCLUDE INI_VAL_MKY_BNF OF 'COLBMBNF.TXT'
*% INCLUDE FIN_VAL_MKY_BNF OF 'COLBMBNF.TXT'
       FIN-VAL-MKY-BNF.
           EXIT.
       
       VAL-FLD-BNF SECTION.
       INI-VAL-FLD-BNF.
*% INCLUDE INI_VAL_FLD_BNF OF 'COLBMBNF.TXT'
           PERFORM VAL-FRM-GLS-TREL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TREL-LEN IN BNF-FLD
                GO TO FIN-VAL-FLD-BNF.
           PERFORM VAL-DBC-IND-TICL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DBC-IND-TICL-LEN IN BNF-FLD
                GO TO FIN-VAL-FLD-BNF.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-OPC-FEC-FCOL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                OPC-FEC-FCOL-LEN IN BNF-FLD
                GO TO FIN-VAL-FLD-BNF.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-RCO-TIP-BENF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RCO-TIP-BENF-LEN IN BNF-FLD
                GO TO FIN-VAL-FLD-BNF.
           PERFORM VAL-FRM-GLS-BENF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-BENF-LEN IN BNF-FLD
                GO TO FIN-VAL-FLD-BNF.
           PERFORM VAL-DBC-GLS-APAT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DBC-GLS-APAT-LEN IN BNF-FLD
                GO TO FIN-VAL-FLD-BNF.
           PERFORM VAL-DBC-GLS-AMAT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DBC-GLS-AMAT-LEN IN BNF-FLD
                GO TO FIN-VAL-FLD-BNF.
           PERFORM VAL-DBC-GLS-NOMB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DBC-GLS-NOMB-LEN IN BNF-FLD
                GO TO FIN-VAL-FLD-BNF.
           PERFORM VAL-RCD-GLS-DIRE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RCD-GLS-DIRE-LEN IN BNF-FLD
                GO TO FIN-VAL-FLD-BNF.
           PERFORM VAL-RCD-COD-LUGA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RCD-COD-LUGA-LEN IN BNF-FLD
                GO TO FIN-VAL-FLD-BNF.
           PERFORM VAL-RCD-GLS-FONO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RCD-GLS-FONO-LEN IN BNF-FLD
                GO TO FIN-VAL-FLD-BNF.
           PERFORM VAL-FRM-GLS-LUGA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-LUGA-LEN IN BNF-FLD
                GO TO FIN-VAL-FLD-BNF.
           PERFORM VAL-FRM-GLS-DIRC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-DIRC-LEN IN BNF-FLD
                GO TO FIN-VAL-FLD-BNF.
           PERFORM VAL-FRM-GLS-TDIR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TDIR-LEN IN BNF-FLD
                GO TO FIN-VAL-FLD-BNF.
           PERFORM VAL-FRM-GLS-NDIR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-NDIR-LEN IN BNF-FLD
                GO TO FIN-VAL-FLD-BNF.
           PERFORM VAL-FRM-GLS-CONT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-CONT-LEN IN BNF-FLD
                GO TO FIN-VAL-FLD-BNF.
           PERFORM VAL-FRM-GLS-GLS1.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-GLS1-LEN IN BNF-FLD
                GO TO FIN-VAL-FLD-BNF.
           PERFORM VAL-FRM-GLS-GLS2.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-GLS2-LEN IN BNF-FLD
                GO TO FIN-VAL-FLD-BNF.
           PERFORM VAL-FRM-NUM-DOCU.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-NUM-DOCU-LEN IN BNF-FLD
                GO TO FIN-VAL-FLD-BNF.
*% INCLUDE FIN_VAL_FLD_BNF OF 'COLBMBNF.TXT'
       FIN-VAL-FLD-BNF.
           EXIT.
