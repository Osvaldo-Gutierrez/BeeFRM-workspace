       FIN-VAL-KEY-OPC.
           EXIT.
       
       VAL-IKY-OPC SECTION.
       INI-VAL-IKY-OPC.
*% INCLUDE INI_VAL_IKY_OPC OF 'COLBMOPC.TXT'
           PERFORM VAL-OPC-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-CAI-IOPC-LEN IN OPC-FLD
                GO TO FIN-VAL-IKY-OPC.
           PERFORM VAL-OPC-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-IIC-IOPC-LEN IN OPC-FLD
                GO TO FIN-VAL-IKY-OPC.
           PERFORM VAL-CAN-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAN-CAI-IOPC-LEN IN OPC-FLD
                GO TO FIN-VAL-IKY-OPC.
           PERFORM VAL-CAN-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAN-IIC-IOPC-LEN IN OPC-FLD
                GO TO FIN-VAL-IKY-OPC.
           PERFORM VAL-CAN-NUM-ICAN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAN-NUM-ICAN-LEN IN OPC-FLD
                GO TO FIN-VAL-IKY-OPC.
*% INCLUDE FIN_VAL_IKY_OPC OF 'COLBMOPC.TXT'
       FIN-VAL-IKY-OPC.
           EXIT.
       
       VAL-MKY-OPC SECTION.
       INI-VAL-MKY-OPC.
*% INCLUDE INI_VAL_MKY_OPC OF 'COLBMOPC.TXT'
*% RREG = "DBC"
*% NOMB = "ICLI"
      * VRF-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * VRF-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% RREG = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DBC-IND-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-IND-ICLI-LEN IN OPC-FLD
                GO TO FIN-VAL-MKY-OPC.
           PERFORM VAL-DBC-GLS-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-GLS-ICLI-LEN IN OPC-FLD
                GO TO FIN-VAL-MKY-OPC.
           PERFORM VAL-DBC-NUM-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-NUM-ICLI-LEN IN OPC-FLD
                GO TO FIN-VAL-MKY-OPC.
           PERFORM VAL-DBC-VRF-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-VRF-ICLI-LEN IN OPC-FLD
                GO TO FIN-VAL-MKY-OPC.
      * IDC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-RDC-CAI-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RDC-CAI-ICLI-LEN IN OPC-FLD
                GO TO FIN-VAL-MKY-OPC.
           PERFORM VAL-RDC-IIC-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RDC-IIC-ICLI-LEN IN OPC-FLD
                GO TO FIN-VAL-MKY-OPC.
