*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "OPC"
*% PGM_BQ = TRUE
       PUT-ALL-OPC SECTION.
       INI-PUT-ALL-OPC.
*% INCLUDE INI_PUT_ALL_OPC OF 'COLBMOPC.TXT'
           PERFORM PUT-OPC-OPC.
           PERFORM PUT-CAN-OPC.
           PERFORM PUT-DBC-OPC.
           PERFORM PUT-RDC-OPC.
           PERFORM PUT-ROV-OPC.
           PERFORM PUT-ITR-OPC.
           PERFORM PUT-COM-OPC.
           PERFORM PUT-CYA-OPC.
           PERFORM PUT-AUT-OPC.
*% INCLUDE FIN_PUT_ALL_OPC OF 'COLBMOPC.TXT'
       FIN-PUT-ALL-OPC.
           EXIT.
       
       PUT-OPC-OPC SECTION.
       INI-PUT-OPC-OPC.
*% INCLUDE INI_PUT_OPC_OPC OF 'COLBMOPC.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-OPC.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-OPC
               GO TO FIN-PUT-OPC-OPC.
           MOVE OPC-MSC-TVOC IN OPC TO OPC-MSC-TVOC IN OPC-FLD.
           MOVE OPC-COD-TOOC IN OPC TO OPC-COD-TOOC IN OPC-FLD.
           MOVE OPC-COD-OFOC IN OPC TO OPC-COD-OFOC IN OPC-FLD.
           MOVE OPC-CAI-IOPC IN OPC TO OPC-CAI-IOPC IN OPC-FLD.
           MOVE OPC-IIC-IOPC IN OPC TO OPC-IIC-IOPC IN OPC-FLD.
           MOVE OPC-COD-VCOC IN OPC TO OPC-COD-VCOC IN OPC-FLD.
           MOVE OPC-COD-AOOC IN OPC TO OPC-COD-AOOC IN OPC-FLD.
           MOVE OPC-VAL-CRED IN OPC TO OPC-VAL-CRED IN OPC-FLD.
           MOVE OPC-CAI-ILDC IN OPC TO OPC-CAI-ILDC IN OPC-FLD.
           MOVE OPC-IIC-ILDC IN OPC TO OPC-IIC-ILDC IN OPC-FLD.
           MOVE OPC-VAL-SCRE IN OPC TO OPC-VAL-SCRE IN OPC-FLD.
           MOVE OPC-NUM-VCVG IN OPC TO OPC-NUM-VCVG IN OPC-FLD.
           MOVE OPC-IND-CTBL IN OPC TO OPC-IND-CTBL IN OPC-FLD.
           MOVE OPC-IND-RENO IN OPC TO OPC-IND-RENO IN OPC-FLD.
           MOVE OPC-COD-SEGD IN OPC TO OPC-COD-SEGD IN OPC-FLD.
           MOVE OPC-CAI-ICEN IN OPC TO OPC-CAI-ICEN IN OPC-FLD.
           MOVE OPC-IIC-ICEN IN OPC TO OPC-IIC-ICEN IN OPC-FLD.
           MOVE OPC-VAL-SEGD IN OPC TO OPC-VAL-SEGD IN OPC-FLD.
           MOVE OPC-VAL-SEGC IN OPC TO OPC-VAL-SEGC IN OPC-FLD.
*% RREG = "OPC"
*% NOMB = "FINP"
      * FEC-INI --> BMCHIBCI001.SRCAIU 
           MOVE OPC-NUM-DINP IN OPC TO OPC-NUM-DINP IN OPC-FLD.
           MOVE OPC-NUM-MINP IN OPC TO OPC-NUM-MINP IN OPC-FLD.
           MOVE OPC-NUM-SINP IN OPC TO OPC-NUM-SINP IN OPC-FLD.
           MOVE OPC-NUM-AINP IN OPC TO OPC-NUM-AINP IN OPC-FLD.
      * FEC-FIN --> BMCHIBCI001.SRCAIU 
*% RREG = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI001.SRCAIU 
           MOVE OPC-NUM-DCOL IN OPC TO OPC-NUM-DCOL IN OPC-FLD.
           MOVE OPC-NUM-MCOL IN OPC TO OPC-NUM-MCOL IN OPC-FLD.
           MOVE OPC-NUM-SCOL IN OPC TO OPC-NUM-SCOL IN OPC-FLD.
           MOVE OPC-NUM-ACOL IN OPC TO OPC-NUM-ACOL IN OPC-FLD.
      * FEC-FIN --> BMCHIBCI001.SRCAIU 
*% RREG = "OPC"
*% NOMB = "FING"
      * FEC-INI --> BMCHIBCI001.SRCAIU 
           MOVE OPC-NUM-DING IN OPC TO OPC-NUM-DING IN OPC-FLD.
           MOVE OPC-NUM-MING IN OPC TO OPC-NUM-MING IN OPC-FLD.
           MOVE OPC-NUM-SING IN OPC TO OPC-NUM-SING IN OPC-FLD.
           MOVE OPC-NUM-AING IN OPC TO OPC-NUM-AING IN OPC-FLD.
      * FEC-FIN --> BMCHIBCI001.SRCAIU 
           MOVE OPC-VAL-SEGP IN OPC TO OPC-VAL-SEGP IN OPC-FLD.
           MOVE OPC-COD-CVTA IN OPC TO OPC-COD-CVTA IN OPC-FLD.
           MOVE OPC-IND-GARA IN OPC TO OPC-IND-GARA IN OPC-FLD.
           MOVE OPC-IND-VSTR IN OPC TO OPC-IND-VSTR IN OPC-FLD.
           MOVE OPC-IND-AFER IN OPC TO OPC-IND-AFER IN OPC-FLD.
           MOVE OPC-SGV-VCOM IN OPC TO OPC-SGV-VCOM IN OPC-FLD.
           MOVE OPC-MSC-MCVF IN OPC TO OPC-MSC-MCVF IN OPC-FLD.
           MOVE OPC-COD-INSI IN OPC TO OPC-COD-INSI IN OPC-FLD.
           MOVE OPC-VAL-GAST IN OPC TO OPC-VAL-GAST IN OPC-FLD.
           MOVE OPC-VAL-TCMB IN OPC TO OPC-VAL-TCMB IN OPC-FLD.
           MOVE OPC-SGV-LIQU IN OPC TO OPC-SGV-LIQU IN OPC-FLD.
           MOVE OPC-COD-RMOC IN OPC TO OPC-COD-RMOC IN OPC-FLD.
           MOVE OPC-COD-CDOC IN OPC TO OPC-COD-CDOC IN OPC-FLD.
           MOVE OPC-COD-PLCO IN OPC TO OPC-COD-PLCO IN OPC-FLD.
           MOVE OPC-COD-PLGA IN OPC TO OPC-COD-PLGA IN OPC-FLD.
