*% FRM = "CNC"
      *
      * COLBUCNC
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:28 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMCNC.TXT'
       SET-TAG-COLBUCNC SECTION.
       INI-SET-TAG-COLBUCNC.
           MOVE
           '<IDNMOD NOM=$COLBUCNC$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:28 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCNC.
           EXIT.
           
       RET-ALL-CNC SECTION.
       INI-RET-ALL-CNC.
*% INCLUDE INI_RET_ALL_CNC OF 'COLBMCNC.TXT'
           PERFORM KEY-ALL-CNC.
           PERFORM RET-CNC-CNC.
*% INCLUDE FIN_RET_ALL_CNC OF 'COLBMCNC.TXT'
       FIN-RET-ALL-CNC.
           EXIT.
       
       RET-CNC-CNC SECTION.
       INI-RET-CNC-CNC.
*% INCLUDE INI_RET_CNC_CNC OF 'COLBMCNC.TXT'
           MOVE CNC-CIC-CANC IN CNC-FLD TO CNC-CIC-CANC IN CNC.
           MOVE CNC-EXT-CTAB IN CNC-FLD TO CNC-EXT-CTAB IN CNC.
           MOVE CNC-GLS-DESC IN CNC-FLD TO CNC-GLS-DESC IN CNC.
           MOVE CNC-GLS-ABRV IN CNC-FLD TO CNC-GLS-ABRV IN CNC.
           MOVE CNC-GLS-DCOR IN CNC-FLD TO CNC-GLS-DCOR IN CNC.
           MOVE CNC-IND-VIGE IN CNC-FLD TO CNC-IND-VIGE IN CNC.
*% RREG = "CNC"
*% RREG_FRM = "CNC"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CNC-NUM-DTRN IN CNC-FLD TO CNC-NUM-DTRN IN CNC.
           MOVE CNC-NUM-MTRN IN CNC-FLD TO CNC-NUM-MTRN IN CNC.
           MOVE CNC-NUM-STRN IN CNC-FLD TO CNC-NUM-STRN IN CNC.
           MOVE CNC-NUM-ATRN IN CNC-FLD TO CNC-NUM-ATRN IN CNC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_RET_CNC_CNC OF 'COLBMCNC.TXT'
       FIN-RET-CNC-CNC.
           EXIT.
       
       KEY-ALL-CNC SECTION.
       INI-KEY-ALL-CNC.
*% INCLUDE INI_KEY_ALL_CNC OF 'COLBMCNC.TXT'
           PERFORM KEY-CNC-CNC.
*% INCLUDE FIN_KEY_ALL_CNC OF 'COLBMCNC.TXT'
       FIN-KEY-ALL-CNC.
           EXIT.
       
       KEY-CNC-CNC SECTION.
       INI-KEY-CNC-CNC.
*% INCLUDE INI_KEY_CNC_CNC OF 'COLBMCNC.TXT'
           MOVE CNC-MSC-TCAN IN CNC-FLD TO CNC-MSC-TCAN IN CNC.
           MOVE CNC-MSC-FCAN IN CNC-FLD TO CNC-MSC-FCAN IN CNC.
           MOVE CNC-MSC-ECAN IN CNC-FLD TO CNC-MSC-ECAN IN CNC.
*% INCLUDE FIN_KEY_CNC_CNC OF 'COLBMCNC.TXT'
       FIN-KEY-CNC-CNC.
           EXIT.
       
       SCR-CLRA-CNC SECTION.
       INI-SCR-CLRA-CNC.
*% INCLUDE INI_SCR_CLRA_CNC OF 'COLBMCNC.TXT'
           PERFORM CLR-KEY-CNC.
           PERFORM CLR-FLD-CNC.
*% INCLUDE FIN_SCR_CLRA_CNC OF 'COLBMCNC.TXT'
       FIN-SCR-CLRA-CNC.
           EXIT.
       
       CLR-KEY-CNC SECTION.
       INI-CLR-KEY-CNC.
*% INCLUDE INI_CLR_KEY_CNC OF 'COLBMCNC.TXT'
           MOVE SPACES TO CNC-MSC-TCAN IN CNC-FLD.
           MOVE SPACES TO CNC-MSC-FCAN IN CNC-FLD.
           MOVE SPACES TO CNC-MSC-ECAN IN CNC-FLD.
*% INCLUDE FIN_CLR_KEY_CNC OF 'COLBMCNC.TXT'
       FIN-CLR-KEY-CNC.
           EXIT.
       
       CLR-FLD-CNC SECTION.
       INI-CLR-FLD-CNC.
*% INCLUDE INI_CLR_FLD_CNC OF 'COLBMCNC.TXT'
           MOVE SPACES TO FRM-GLS-TCAN IN CNC-FLD.
           MOVE SPACES TO FRM-GLS-FCAN IN CNC-FLD.
           MOVE SPACES TO FRM-GLS-ECAN IN CNC-FLD.
           PERFORM CLR-CNC-CNC.
*% INCLUDE FIN_CLR_FLD_CNC OF 'COLBMCNC.TXT'
       FIN-CLR-FLD-CNC.
           EXIT.
       
       CLR-CNC-CNC SECTION.
       INI-CLR-CNC-CNC.
*% INCLUDE INI_CLR_CNC_CNC OF 'COLBMCNC.TXT'
           MOVE SPACES TO CNC-CIC-CANC IN CNC-FLD.
           MOVE SPACES TO CNC-EXT-CTAB IN CNC-FLD.
           MOVE SPACES TO CNC-GLS-DESC IN CNC-FLD.
           MOVE SPACES TO CNC-GLS-ABRV IN CNC-FLD.
           MOVE SPACES TO CNC-GLS-DCOR IN CNC-FLD.
           MOVE SPACES TO CNC-IND-VIGE IN CNC-FLD.
*% RREG = "CNC"
*% RREG_FRM = "CNC"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CNC-FEC-FTRN IN CNC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_CNC_CNC OF 'COLBMCNC.TXT'
       FIN-CLR-CNC-CNC.
           EXIT.
       
       FST-KEY-CNC SECTION.
       INI-FST-KEY-CNC.
*% INCLUDE INI_FST_KEY_CNC OF 'COLBMCNC.TXT'
           MOVE -1 TO CNC-MSC-TCAN-LEN IN CNC-FLD.
*% INCLUDE FIN_FST_KEY_CNC OF 'COLBMCNC.TXT'
       FIN-FST-KEY-CNC.
           EXIT.
       
       FST-FLD-CNC SECTION.
       INI-FST-FLD-CNC.
*% INCLUDE INI_FST_FLD_CNC OF 'COLBMCNC.TXT'
           MOVE -1 TO CNC-CIC-CANC-LEN IN CNC-FLD.
*% INCLUDE FIN_FST_FLD_CNC OF 'COLBMCNC.TXT'
       FIN-FST-FLD-CNC.
           EXIT.
       
       PRO-KEY-CNC SECTION.
       INI-PRO-KEY-CNC.
*% INCLUDE INI_PRO_KEY_CNC OF 'COLBMCNC.TXT'
           PERFORM PRO-MKY-CNC.
*% INCLUDE FIN_PRO_KEY_CNC OF 'COLBMCNC.TXT'
       FIN-PRO-KEY-CNC.
           EXIT.
       
       PRO-MKY-CNC SECTION.
       INI-PRO-MKY-CNC.
*% INCLUDE INI_PRO_MKY_CNC OF 'COLBMCNC.TXT'
*% INCLUDE FIN_PRO_MKY_CNC OF 'COLBMCNC.TXT'
       FIN-PRO-MKY-CNC.
           EXIT.
       
       PRO-FLD-CNC SECTION.
       INI-PRO-FLD-CNC.
*% INCLUDE INI_PRO_FLD_CNC OF 'COLBMCNC.TXT'
           PERFORM PRO-CNC-CNC.
*% INCLUDE FIN_PRO_FLD_CNC OF 'COLBMCNC.TXT'
       FIN-PRO-FLD-CNC.
           EXIT.
       
       PRO-CNC-CNC SECTION.
       INI-PRO-CNC-CNC.
*% INCLUDE INI_PRO_CNC_CNC OF 'COLBMCNC.TXT'
           MOVE FRM-CPIM-AEY TO CNC-CIC-CANC-ATR IN CNC-FLD.
           MOVE FRM-CPIM-AEY TO CNC-EXT-CTAB-ATR IN CNC-FLD.
           MOVE FRM-CPIM-AEY TO CNC-GLS-DESC-ATR IN CNC-FLD.
           MOVE FRM-CPIM-AEY TO CNC-GLS-ABRV-ATR IN CNC-FLD.
           MOVE FRM-CPIM-AEY TO CNC-GLS-DCOR-ATR IN CNC-FLD.
           MOVE FRM-CPIM-AEY TO CNC-IND-VIGE-ATR IN CNC-FLD.
*% INCLUDE FIN_PRO_CNC_CNC OF 'COLBMCNC.TXT'
       FIN-PRO-CNC-CNC.
           EXIT.
       
       UNP-KEY-CNC SECTION.
       INI-UNP-KEY-CNC.
*% INCLUDE INI_UNP_KEY_CNC OF 'COLBMCNC.TXT'
           PERFORM UNP-IKY-CNC.
           PERFORM UNP-AKY-CNC.
           PERFORM UNP-MKY-CNC.
*% INCLUDE FIN_UNP_KEY_CNC OF 'COLBMCNC.TXT'
       FIN-UNP-KEY-CNC.
           EXIT.
       
       UNP-IKY-CNC SECTION.
       INI-UNP-IKY-CNC.
*% INCLUDE INI_UNP_IKY_CNC OF 'COLBMCNC.TXT'
           MOVE FRM-CPIM-UBY TO CNC-MSC-TCAN-ATR.
           MOVE FRM-CPIM-UBY TO CNC-MSC-FCAN-ATR.
           MOVE FRM-CPIM-UBY TO CNC-MSC-ECAN-ATR.
*% INCLUDE FIN_UNP_IKY_CNC OF 'COLBMCNC.TXT'
       FIN-UNP-IKY-CNC.
           EXIT.
       
       UNP-MKY-CNC SECTION.
       INI-UNP-MKY-CNC.
*% INCLUDE INI_UNP_MKY_CNC OF 'COLBMCNC.TXT'
*% INCLUDE FIN_UNP_MKY_CNC OF 'COLBMCNC.TXT'
       FIN-UNP-MKY-CNC.
           EXIT.
       
       UNP-AKY-CNC SECTION.
       INI-UNP-AKY-CNC.
*% INCLUDE INI_UNP_AKY_CNC OF 'COLBMCNC.TXT'
*% INCLUDE FIN_UNP_AKY_CNC OF 'COLBMCNC.TXT'
       FIN-UNP-AKY-CNC.
           EXIT.
       
       UNP-FLD-CNC SECTION.
       INI-UNP-FLD-CNC.
*% INCLUDE INI_UNP_FLD_CNC OF 'COLBMCNC.TXT'
           PERFORM UNP-CNC-CNC.
*% INCLUDE FIN_UNP_FLD_CNC OF 'COLBMCNC.TXT'
       FIN-UNP-FLD-CNC.
           EXIT.
       
       UNP-CNC-CNC SECTION.
       INI-UNP-CNC-CNC.
*% INCLUDE INI_UNP_CNC_CNC OF 'COLBMCNC.TXT'
           MOVE FRM-CPIM-UBY TO CNC-CIC-CANC-ATR IN CNC-FLD.
           MOVE FRM-CPIM-UBY TO CNC-EXT-CTAB-ATR IN CNC-FLD.
           MOVE FRM-CPIM-UBY TO CNC-GLS-DESC-ATR IN CNC-FLD.
           MOVE FRM-CPIM-UBY TO CNC-GLS-ABRV-ATR IN CNC-FLD.
           MOVE FRM-CPIM-UBY TO CNC-GLS-DCOR-ATR IN CNC-FLD.
           MOVE FRM-CPIM-UBY TO CNC-IND-VIGE-ATR IN CNC-FLD.
*% INCLUDE FIN_UNP_CNC_CNC OF 'COLBMCNC.TXT'
       FIN-UNP-CNC-CNC.
           EXIT.
       
       VAL-MKY-CNC SECTION.
       INI-VAL-MKY-CNC.
*% INCLUDE INI_VAL_MKY_CNC OF 'COLBMCNC.TXT'
*% INCLUDE FIN_VAL_MKY_CNC OF 'COLBMCNC.TXT'
       FIN-VAL-MKY-CNC.
           EXIT.
       
       VAL-FLD-CNC SECTION.
       INI-VAL-FLD-CNC.
*% INCLUDE INI_VAL_FLD_CNC OF 'COLBMCNC.TXT'
           PERFORM VAL-FRM-GLS-TCAN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TCAN-LEN IN CNC-FLD
                GO TO FIN-VAL-FLD-CNC.
           PERFORM VAL-FRM-GLS-FCAN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-FCAN-LEN IN CNC-FLD
                GO TO FIN-VAL-FLD-CNC.
           PERFORM VAL-FRM-GLS-ECAN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-ECAN-LEN IN CNC-FLD
                GO TO FIN-VAL-FLD-CNC.
           PERFORM VAL-CNC-CIC-CANC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CNC-CIC-CANC-LEN IN CNC-FLD
                GO TO FIN-VAL-FLD-CNC.
           PERFORM VAL-CNC-EXT-CTAB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CNC-EXT-CTAB-LEN IN CNC-FLD
                GO TO FIN-VAL-FLD-CNC.
           PERFORM VAL-CNC-GLS-DESC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CNC-GLS-DESC-LEN IN CNC-FLD
                GO TO FIN-VAL-FLD-CNC.
           PERFORM VAL-CNC-GLS-ABRV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CNC-GLS-ABRV-LEN IN CNC-FLD
                GO TO FIN-VAL-FLD-CNC.
           PERFORM VAL-CNC-GLS-DCOR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CNC-GLS-DCOR-LEN IN CNC-FLD
                GO TO FIN-VAL-FLD-CNC.
           PERFORM VAL-CNC-IND-VIGE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CNC-IND-VIGE-LEN IN CNC-FLD
                GO TO FIN-VAL-FLD-CNC.
*% RREG = "CNC"
*% RREG_FRM = "CNC"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-CNC-FEC-FTRN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                CNC-FEC-FTRN-LEN IN CNC-FLD
                GO TO FIN-VAL-FLD-CNC.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% INCLUDE FIN_VAL_FLD_CNC OF 'COLBMCNC.TXT'
       FIN-VAL-FLD-CNC.
           EXIT.
       
       VAL-FRM-GLS-TCAN SECTION.
       INI-VAL-FRM-GLS-TCAN.
*% LBL = "FRM_GLS_TCAN OF 'COLBMCNC.TXT'"
*% INCLUDE INI_FRM_GLS_TCAN OF 'COLBMCNC.TXT'
*% INCLUDE FRM_GLS_TCAN OF 'COLBMCNC.TXT'
*% INCLUDE FIN_FRM_GLS_TCAN OF 'COLBMCNC.TXT'
       FIN-VAL-FRM-GLS-TCAN.
           EXIT.
       
       VAL-FRM-GLS-FCAN SECTION.
       INI-VAL-FRM-GLS-FCAN.
*% LBL = "FRM_GLS_FCAN OF 'COLBMCNC.TXT'"
*% INCLUDE INI_FRM_GLS_FCAN OF 'COLBMCNC.TXT'
*% INCLUDE FRM_GLS_FCAN OF 'COLBMCNC.TXT'
*% INCLUDE FIN_FRM_GLS_FCAN OF 'COLBMCNC.TXT'
       FIN-VAL-FRM-GLS-FCAN.
           EXIT.
       
       VAL-FRM-GLS-ECAN SECTION.
       INI-VAL-FRM-GLS-ECAN.
*% LBL = "FRM_GLS_ECAN OF 'COLBMCNC.TXT'"
*% INCLUDE INI_FRM_GLS_ECAN OF 'COLBMCNC.TXT'
*% INCLUDE FRM_GLS_ECAN OF 'COLBMCNC.TXT'
*% INCLUDE FIN_FRM_GLS_ECAN OF 'COLBMCNC.TXT'
       FIN-VAL-FRM-GLS-ECAN.
           EXIT.
       
       VAL-CNC-CIC-CANC SECTION.
       INI-VAL-CNC-CIC-CANC.
*% LBL = "CNC_CIC_CANC OF 'COLBMCNC.TXT'"
*% INCLUDE INI_CNC_CIC_CANC OF 'COLBMCNC.TXT'
           IF CNC-CIC-CANC IN CNC-FLD NOT > SPACES
              MOVE SPACES TO CNC-CIC-CANC IN CNC-FLD.
*% INCLUDE CNC_CIC_CANC OF 'COLBMCNC.TXT'
*% INCLUDE FIN_CNC_CIC_CANC OF 'COLBMCNC.TXT'
       FIN-VAL-CNC-CIC-CANC.
           EXIT.
       
       VAL-CNC-EXT-CTAB SECTION.
       INI-VAL-CNC-EXT-CTAB.
*% LBL = "CNC_EXT_CTAB OF 'COLBMCNC.TXT'"
*% INCLUDE INI_CNC_EXT_CTAB OF 'COLBMCNC.TXT'
           IF CNC-EXT-CTAB IN CNC-FLD NOT > SPACES
              MOVE SPACES TO CNC-EXT-CTAB IN CNC-FLD.
*% INCLUDE CNC_EXT_CTAB OF 'COLBMCNC.TXT'
*% INCLUDE FIN_CNC_EXT_CTAB OF 'COLBMCNC.TXT'
       FIN-VAL-CNC-EXT-CTAB.
           EXIT.
       
       VAL-CNC-GLS-DESC SECTION.
       INI-VAL-CNC-GLS-DESC.
*% LBL = "CNC_GLS_DESC OF 'COLBMCNC.TXT'"
*% INCLUDE INI_CNC_GLS_DESC OF 'COLBMCNC.TXT'
           IF CNC-GLS-DESC IN CNC-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-CNC-GLS-DESC.
*% INCLUDE CNC_GLS_DESC OF 'COLBMCNC.TXT'
*% INCLUDE FIN_CNC_GLS_DESC OF 'COLBMCNC.TXT'
       FIN-VAL-CNC-GLS-DESC.
           EXIT.
       
       VAL-CNC-GLS-ABRV SECTION.
       INI-VAL-CNC-GLS-ABRV.
*% LBL = "CNC_GLS_ABRV OF 'COLBMCNC.TXT'"
*% INCLUDE INI_CNC_GLS_ABRV OF 'COLBMCNC.TXT'
           IF CNC-GLS-ABRV IN CNC-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-CNC-GLS-ABRV.
*% INCLUDE CNC_GLS_ABRV OF 'COLBMCNC.TXT'
*% INCLUDE FIN_CNC_GLS_ABRV OF 'COLBMCNC.TXT'
       FIN-VAL-CNC-GLS-ABRV.
           EXIT.
       
