*% INCLUDE FIN_FRM_GLS_VCOC OF 'COLBMQCB.TXT'
       FIN-VAL-FRM-GLS-VCOC.
           EXIT.
       
       VAL-FRM-GLS-COOC SECTION.
       INI-VAL-FRM-GLS-COOC.
*% LBL = "FRM_GLS_COOC OF 'COLBMQCB.TXT'"
*% INCLUDE INI_FRM_GLS_COOC OF 'COLBMQCB.TXT'
*% INCLUDE FRM_GLS_COOC OF 'COLBMQCB.TXT'
*% INCLUDE FIN_FRM_GLS_COOC OF 'COLBMQCB.TXT'
       FIN-VAL-FRM-GLS-COOC.
           EXIT.
       
       VAL-FRM-GLS-OFOC SECTION.
       INI-VAL-FRM-GLS-OFOC.
*% LBL = "FRM_GLS_OFOC OF 'COLBMQCB.TXT'"
*% INCLUDE INI_FRM_GLS_OFOC OF 'COLBMQCB.TXT'
*% INCLUDE FRM_GLS_OFOC OF 'COLBMQCB.TXT'
*% INCLUDE FIN_FRM_GLS_OFOC OF 'COLBMQCB.TXT'
       FIN-VAL-FRM-GLS-OFOC.
           EXIT.
       
       VAL-FRM-NUM-ICCB SECTION.
       INI-VAL-FRM-NUM-ICCB.
*% LBL = "FRM_NUM_ICCB OF 'COLBMQCB.TXT'"
*% INCLUDE INI_FRM_NUM_ICCB OF 'COLBMQCB.TXT'
*% INCLUDE FRM_NUM_ICCB OF 'COLBMQCB.TXT'
*% INCLUDE FIN_FRM_NUM_ICCB OF 'COLBMQCB.TXT'
       FIN-VAL-FRM-NUM-ICCB.
           EXIT.
       
       VAL-FRM-GLS-SCBA SECTION.
       INI-VAL-FRM-GLS-SCBA.
*% LBL = "FRM_GLS_SCBA OF 'COLBMQCB.TXT'"
*% INCLUDE INI_FRM_GLS_SCBA OF 'COLBMQCB.TXT'
*% INCLUDE FRM_GLS_SCBA OF 'COLBMQCB.TXT'
*% INCLUDE FIN_FRM_GLS_SCBA OF 'COLBMQCB.TXT'
       FIN-VAL-FRM-GLS-SCBA.
           EXIT.
       
       VAL-FRM-GLS-SCBZ SECTION.
       INI-VAL-FRM-GLS-SCBZ.
*% LBL = "FRM_GLS_SCBZ OF 'COLBMQCB.TXT'"
*% INCLUDE INI_FRM_GLS_SCBZ OF 'COLBMQCB.TXT'
*% INCLUDE FRM_GLS_SCBZ OF 'COLBMQCB.TXT'
*% INCLUDE FIN_FRM_GLS_SCBZ OF 'COLBMQCB.TXT'
       FIN-VAL-FRM-GLS-SCBZ.
           EXIT.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCCB"
*% OBL = "S"
*% LBL = "FRM_FEC_FCCB OF 'COLBMQCB.TXT'"
*% IZR = "S"
*% ZRO = "S"
*% FRM_IFLD = "(FRM-IFLD)"
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-FRM-FEC-FCCB SECTION.
       INI-VAL-FRM-FEC-FCCB.
           IF FRM-FEC-FCCB IN QCB-FLD(FRM-IFLD)  NOT > ZEROES
              MOVE ZEROES TO FRM-FEC-FCCB IN QCB-FLD(FRM-IFLD).
*% INCLUDE INI_FRM_FEC_FCCB OF 'COLBMQCB.TXT'
*% INCLUDE FRM_FEC_FCCB OF 'COLBMQCB.TXT'
*% INCLUDE FIN_FRM_FEC_FCCB OF 'COLBMQCB.TXT'
       FIN-VAL-FRM-FEC-FCCB.
           EXIT.
      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FING"
*% OBL = "S"
