      *
      * COLBQSCL
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:56 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "SCL"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMSCL.TXT'
       SET-TAG-COLBQSCL SECTION.
       INI-SET-TAG-COLBQSCL.
           MOVE
           '<IDNMOD NOM=$COLBQSCL$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:56 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQSCL.
           EXIT.
           
       PUT-ALL-SCL SECTION.
       INI-PUT-ALL-SCL.
*% INCLUDE INI_PUT_ALL_SCL OF 'COLBMSCL.TXT'
*% INCLUDE FIN_PUT_ALL_SCL OF 'COLBMSCL.TXT'
       FIN-PUT-ALL-SCL.
           EXIT.
       
       KEY-ALL-SCL SECTION.
       INI-KEY-ALL-SCL.
*% INCLUDE INI_KEY_ALL_SCL OF 'COLBMSCL.TXT'
*% INCLUDE FIN_KEY_ALL_SCL OF 'COLBMSCL.TXT'
       FIN-KEY-ALL-SCL.
           EXIT.
       
       SCR-CLRA-SCL SECTION.
       INI-SCR-CLRA-SCL.
*% INCLUDE INI_SCR_CLRA_SCL OF 'COLBMSCL.TXT'
           PERFORM CLR-KEY-SCL.
           PERFORM CLR-FLD-SCL.
*% INCLUDE FIN_SCR_CLRA_SCL OF 'COLBMSCL.TXT'
       FIN-SCR-CLRA-SCL.
           EXIT.
       
       CLR-KEY-SCL SECTION.
       INI-CLR-KEY-SCL.
*% INCLUDE INI_CLR_KEY_SCL OF 'COLBMSCL.TXT'
           MOVE SPACES TO FRM-GLS-CRIT IN SCL-FLD.
           MOVE SPACES TO FRM-COD-FORM IN SCL-FLD.
           MOVE ZEROES TO FRM-NUM-PAGI IN SCL-FLD.
*% INCLUDE FIN_CLR_KEY_SCL OF 'COLBMSCL.TXT'
       FIN-CLR-KEY-SCL.
           EXIT.
       
       CLR-FLD-SCL SECTION.
       INI-CLR-FLD-SCL.
*% INCLUDE INI_CLR_FLD_SCL OF 'COLBMSCL.TXT'
           MOVE SPACES TO FRM-COD-IDEN IN SCL-FLD(1).
           MOVE SPACES TO FRM-COD-IDEN IN SCL-FLD(2).
           MOVE SPACES TO FRM-COD-IDEN IN SCL-FLD(3).
           MOVE SPACES TO FRM-COD-IDEN IN SCL-FLD(4).
           MOVE SPACES TO FRM-COD-IDEN IN SCL-FLD(5).
           MOVE SPACES TO FRM-COD-IDEN IN SCL-FLD(6).
           MOVE SPACES TO FRM-COD-IDEN IN SCL-FLD(7).
           MOVE SPACES TO FRM-COD-IDEN IN SCL-FLD(8).
           MOVE SPACES TO FRM-COD-IDEN IN SCL-FLD(9).
           MOVE SPACES TO FRM-COD-IDEN IN SCL-FLD(10).
           MOVE SPACES TO FRM-COD-IDEN IN SCL-FLD(11).
           MOVE SPACES TO FRM-COD-IDEN IN SCL-FLD(12).
           MOVE SPACES TO FRM-COD-IDEN IN SCL-FLD(13).
           MOVE SPACES TO FRM-COD-IDEN IN SCL-FLD(14).
           MOVE SPACES TO FRM-COD-IDEN IN SCL-FLD(15).
           MOVE SPACES TO FRM-COD-IDEN IN SCL-FLD(16).
           MOVE SPACES TO FRM-IND-CONT IN SCL-FLD(1).
           MOVE SPACES TO FRM-IND-CONT IN SCL-FLD(2).
           MOVE SPACES TO FRM-IND-CONT IN SCL-FLD(3).
           MOVE SPACES TO FRM-IND-CONT IN SCL-FLD(4).
           MOVE SPACES TO FRM-IND-CONT IN SCL-FLD(5).
           MOVE SPACES TO FRM-IND-CONT IN SCL-FLD(6).
           MOVE SPACES TO FRM-IND-CONT IN SCL-FLD(7).
           MOVE SPACES TO FRM-IND-CONT IN SCL-FLD(8).
           MOVE SPACES TO FRM-IND-CONT IN SCL-FLD(9).
           MOVE SPACES TO FRM-IND-CONT IN SCL-FLD(10).
           MOVE SPACES TO FRM-IND-CONT IN SCL-FLD(11).
           MOVE SPACES TO FRM-IND-CONT IN SCL-FLD(12).
           MOVE SPACES TO FRM-IND-CONT IN SCL-FLD(13).
           MOVE SPACES TO FRM-IND-CONT IN SCL-FLD(14).
           MOVE SPACES TO FRM-IND-CONT IN SCL-FLD(15).
           MOVE SPACES TO FRM-IND-CONT IN SCL-FLD(16).
           MOVE SPACES TO FRM-GLS-TEXT IN SCL-FLD(1).
           MOVE SPACES TO FRM-GLS-TEXT IN SCL-FLD(2).
           MOVE SPACES TO FRM-GLS-TEXT IN SCL-FLD(3).
           MOVE SPACES TO FRM-GLS-TEXT IN SCL-FLD(4).
           MOVE SPACES TO FRM-GLS-TEXT IN SCL-FLD(5).
           MOVE SPACES TO FRM-GLS-TEXT IN SCL-FLD(6).
           MOVE SPACES TO FRM-GLS-TEXT IN SCL-FLD(7).
           MOVE SPACES TO FRM-GLS-TEXT IN SCL-FLD(8).
           MOVE SPACES TO FRM-GLS-TEXT IN SCL-FLD(9).
           MOVE SPACES TO FRM-GLS-TEXT IN SCL-FLD(10).
           MOVE SPACES TO FRM-GLS-TEXT IN SCL-FLD(11).
           MOVE SPACES TO FRM-GLS-TEXT IN SCL-FLD(12).
           MOVE SPACES TO FRM-GLS-TEXT IN SCL-FLD(13).
           MOVE SPACES TO FRM-GLS-TEXT IN SCL-FLD(14).
           MOVE SPACES TO FRM-GLS-TEXT IN SCL-FLD(15).
           MOVE SPACES TO FRM-GLS-TEXT IN SCL-FLD(16).
*% INCLUDE FIN_CLR_FLD_SCL OF 'COLBMSCL.TXT'
       FIN-CLR-FLD-SCL.
           EXIT.
       
       FST-KEY-SCL SECTION.
       INI-FST-KEY-SCL.
*% INCLUDE INI_FST_KEY_SCL OF 'COLBMSCL.TXT'
           MOVE -1 TO FRM-GLS-CRIT-LEN IN SCL-FLD.
*% INCLUDE FIN_FST_KEY_SCL OF 'COLBMSCL.TXT'
       FIN-FST-KEY-SCL.
           EXIT.
       
       FST-FLD-SCL SECTION.
       INI-FST-FLD-SCL.
*% INCLUDE INI_FST_FLD_SCL OF 'COLBMSCL.TXT'
*% INCLUDE FIN_FST_FLD_SCL OF 'COLBMSCL.TXT'
       FIN-FST-FLD-SCL.
           EXIT.
       
       PRO-KEY-SCL SECTION.
       INI-PRO-KEY-SCL.
*% INCLUDE INI_PRO_KEY_SCL OF 'COLBMSCL.TXT'
           MOVE FRM-CPIM-AEY TO FRM-GLS-CRIT-ATR IN SCL-FLD.
           MOVE FRM-CPIM-AEY TO FRM-COD-FORM-ATR IN SCL-FLD.
           PERFORM PRO-IKY-SCL.
           PERFORM PRO-AKY-SCL.
           PERFORM PRO-MKY-SCL.
*% INCLUDE FIN_PRO_KEY_SCL OF 'COLBMSCL.TXT'
       FIN-PRO-KEY-SCL.
           EXIT.
       
       PRO-IKY-SCL SECTION.
       INI-PRO-IKY-SCL.
*% INCLUDE INI_PRO_IKY_SCL OF 'COLBMSCL.TXT'
*% INCLUDE FIN_PRO_IKY_SCL OF 'COLBMSCL.TXT'
       FIN-PRO-IKY-SCL.
           EXIT.
       
       PRO-MKY-SCL SECTION.
       INI-PRO-MKY-SCL.
*% INCLUDE INI_PRO_MKY_SCL OF 'COLBMSCL.TXT'
*% INCLUDE FIN_PRO_MKY_SCL OF 'COLBMSCL.TXT'
       FIN-PRO-MKY-SCL.
           EXIT.
       
       PRO-AKY-SCL SECTION.
       INI-PRO-AKY-SCL.
*% INCLUDE INI_PRO_AKY_SCL OF 'COLBMSCL.TXT'
*% INCLUDE FIN_PRO_AKY_SCL OF 'COLBMSCL.TXT'
       FIN-PRO-AKY-SCL.
           EXIT.
       
       UNP-KEY-SCL SECTION.
       INI-UNP-KEY-SCL.
*% INCLUDE INI_UNP_KEY_SCL OF 'COLBMSCL.TXT'
           MOVE FRM-CPIM-UBY TO FRM-GLS-CRIT-ATR IN SCL-FLD.
           MOVE FRM-CPIM-UBY TO FRM-COD-FORM-ATR IN SCL-FLD.
           PERFORM UNP-IKY-SCL.
           PERFORM UNP-AKY-SCL.
           PERFORM UNP-MKY-SCL.
*% INCLUDE FIN_UNP_KEY_SCL OF 'COLBMSCL.TXT'
       FIN-UNP-KEY-SCL.
           EXIT.
       
       UNP-IKY-SCL SECTION.
       INI-UNP-IKY-SCL.
*% INCLUDE INI_UNP_IKY_SCL OF 'COLBMSCL.TXT'
           MOVE FRM-CPIM-UBY TO FRM-GLS-CRIT-ATR.
           MOVE FRM-CPIM-UBY TO FRM-COD-FORM-ATR.
*% INCLUDE FIN_UNP_IKY_SCL OF 'COLBMSCL.TXT'
       FIN-UNP-IKY-SCL.
           EXIT.
       
       UNP-MKY-SCL SECTION.
       INI-UNP-MKY-SCL.
*% INCLUDE INI_UNP_MKY_SCL OF 'COLBMSCL.TXT'
*% INCLUDE FIN_UNP_MKY_SCL OF 'COLBMSCL.TXT'
       FIN-UNP-MKY-SCL.
           EXIT.
       
       UNP-AKY-SCL SECTION.
       INI-UNP-AKY-SCL.
*% INCLUDE INI_UNP_AKY_SCL OF 'COLBMSCL.TXT'
*% INCLUDE FIN_UNP_AKY_SCL OF 'COLBMSCL.TXT'
       FIN-UNP-AKY-SCL.
           EXIT.
       
       UNP-FLD-SCL SECTION.
       INI-UNP-FLD-SCL.
*% INCLUDE INI_UNP_FLD_SCL OF 'COLBMSCL.TXT'
*% INCLUDE FIN_UNP_FLD_SCL OF 'COLBMSCL.TXT'
       FIN-UNP-FLD-SCL.
           EXIT.
       
       VAL-KEY-SCL SECTION.
       INI-VAL-KEY-SCL.
           PERFORM VAL-IKY-SCL.
           PERFORM VAL-MKY-SCL.
           PERFORM VAL-AKY-SCL.
       FIN-VAL-KEY-SCL.
           EXIT.
       
       VAL-IKY-SCL SECTION.
       INI-VAL-IKY-SCL.
*% INCLUDE INI_VAL_IKY_SCL OF 'COLBMSCL.TXT'
           PERFORM VAL-FRM-GLS-CRIT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-CRIT-LEN IN SCL-FLD
                GO TO FIN-VAL-IKY-SCL.
           PERFORM VAL-FRM-COD-FORM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-FORM-LEN IN SCL-FLD
                GO TO FIN-VAL-IKY-SCL.
           PERFORM VAL-FRM-NUM-PAGI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-NUM-PAGI-LEN IN SCL-FLD
                GO TO FIN-VAL-IKY-SCL.
*% INCLUDE FIN_VAL_IKY_SCL OF 'COLBMSCL.TXT'
       FIN-VAL-IKY-SCL.
           EXIT.
       
       VAL-MKY-SCL SECTION.
       INI-VAL-MKY-SCL.
*% INCLUDE INI_VAL_MKY_SCL OF 'COLBMSCL.TXT'
*% INCLUDE FIN_VAL_MKY_SCL OF 'COLBMSCL.TXT'
       FIN-VAL-MKY-SCL.
           EXIT.
       
       VAL-AKY-SCL SECTION.
       INI-VAL-AKY-SCL.
*% INCLUDE INI_VAL_AKY_SCL OF 'COLBMSCL.TXT'
*% INCLUDE FIN_VAL_AKY_SCL OF 'COLBMSCL.TXT'
       FIN-VAL-AKY-SCL.
           EXIT.
       
       VAL-FRM-GLS-CRIT SECTION.
       INI-VAL-FRM-GLS-CRIT.
*% LBL = "FRM_GLS_CRIT OF 'COLBMSCL.TXT'"
*% INCLUDE INI_FRM_GLS_CRIT OF 'COLBMSCL.TXT'
           IF FRM-GLS-CRIT IN SCL-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-FRM-GLS-CRIT.
*% INCLUDE FRM_GLS_CRIT OF 'COLBMSCL.TXT'
*% INCLUDE FIN_FRM_GLS_CRIT OF 'COLBMSCL.TXT'
       FIN-VAL-FRM-GLS-CRIT.
           EXIT.
       
       VAL-FRM-COD-FORM SECTION.
       INI-VAL-FRM-COD-FORM.
*% LBL = "FRM_COD_FORM OF 'COLBMSCL.TXT'"
*% INCLUDE INI_FRM_COD_FORM OF 'COLBMSCL.TXT'
           IF FRM-COD-FORM IN SCL-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-FRM-COD-FORM.
*% INCLUDE FRM_COD_FORM OF 'COLBMSCL.TXT'
*% INCLUDE FIN_FRM_COD_FORM OF 'COLBMSCL.TXT'
       FIN-VAL-FRM-COD-FORM.
           EXIT.
       
       VAL-FRM-NUM-PAGI SECTION.
       INI-VAL-FRM-NUM-PAGI.
*% LBL = "FRM_NUM_PAGI OF 'COLBMSCL.TXT'"
*% INCLUDE INI_FRM_NUM_PAGI OF 'COLBMSCL.TXT'
*% INCLUDE FRM_NUM_PAGI OF 'COLBMSCL.TXT'
*% INCLUDE FIN_FRM_NUM_PAGI OF 'COLBMSCL.TXT'
       FIN-VAL-FRM-NUM-PAGI.
           EXIT.
       
       VAL-CON-KEY-SCL SECTION.
       INI-VAL-CON-KEY-SCL.
*% INCLUDE INI_VAL_CON_KEY_SCL OF 'COLBMSCL.TXT'
           IF     FRM-GLS-CRIT IN SCL-FLD NOT > SPACES 
              AND FRM-COD-FORM IN SCL-FLD NOT > SPACES 
              AND FRM-NUM-PAGI IN SCL-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-SCL.
*% INCLUDE FIN_VAL_CON_KEY_SCL OF 'COLBMSCL.TXT'
       FIN-VAL-CON-KEY-SCL.
           EXIT.
       
       VAL-CON-IKY-SCL SECTION.
       INI-VAL-CON-IKY-SCL.
*% INCLUDE INI_VAL_CON_IKY_SCL OF 'COLBMSCL.TXT'
           IF     FRM-GLS-CRIT IN SCL-FLD NOT > SPACES 
              AND FRM-COD-FORM IN SCL-FLD NOT > SPACES 
              AND FRM-NUM-PAGI IN SCL-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-SCL.
*% INCLUDE FIN_VAL_CON_IKY_SCL OF 'COLBMSCL.TXT'
       FIN-VAL-CON-IKY-SCL.
           EXIT.
       
       VAL-NUM-KEY-SCL SECTION.
       INI-VAL-NUM-KEY-SCL.
           PERFORM VAL-NUM-IKY-SCL.
           PERFORM VAL-NUM-MKY-SCL.
           PERFORM VAL-NUM-AKY-SCL.
       FIN-VAL-NUM-KEY-SCL.
           EXIT.
       
       VAL-NUM-IKY-SCL SECTION.
       INI-VAL-NUM-IKY-SCL.
*% INCLUDE INI_VAL_NUM_IKY_SCL OF 'COLBMSCL.TXT'
       VAL-NUM-FRM-NUM-PAGI.
*% BUF = "SCL-FLD"
*% FLD = "FRM-NUM-PAGI"
*% ENT = "6"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-SCL.
*% INCLUDE FIN_VAL_NUM_IKY_SCL OF 'COLBMSCL.TXT'
       FIN-VAL-NUM-IKY-SCL.
           EXIT.
       
       VAL-NUM-MKY-SCL SECTION.
       INI-VAL-NUM-MKY-SCL.
*% INCLUDE INI_VAL_NUM_MKY_SCL OF 'COLBMSCL.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_SCL OF 'COLBMSCL.TXT'
       FIN-VAL-NUM-MKY-SCL.
           EXIT.
       
       VAL-NUM-AKY-SCL SECTION.
       INI-VAL-NUM-AKY-SCL.
*% INCLUDE INI_VAL_NUM_AKY_SCL OF 'COLBMSCL.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_SCL OF 'COLBMSCL.TXT'
       FIN-VAL-NUM-AKY-SCL.
           EXIT.
       
       VAL-NUM-FLD-SCL SECTION.
       INI-VAL-NUM-FLD-SCL.
*% INCLUDE INI_VAL_NUM_FLD_SCL OF 'COLBMSCL.TXT'
*% INCLUDE FIN_VAL_NUM_FLD_SCL OF 'COLBMSCL.TXT'
       FIN-VAL-NUM-FLD-SCL.
           EXIT.
       
       EDT-KEY-SCL SECTION.
       INI-EDT-KEY-SCL.
           PERFORM EDT-IKY-SCL.
           PERFORM EDT-MKY-SCL.
           PERFORM EDT-AKY-SCL.
*% INCLUDE EDT_KEY_SCL OF 'COLBMSCL.TXT'
       FIN-EDT-KEY-SCL.
           EXIT.
       
       EDT-IKY-SCL SECTION.
       INI-EDT-IKY-SCL.
*% INCLUDE EDT_IKY_SCL OF 'COLBMSCL.TXT'
           IF FRM-NUM-PAGI-LEN IN SCL-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-PAGI-LEN IN SCL-FLD
                GO TO FIN-EDT-IKY-SCL.
           MOVE FRM-NUM-PAGI IN SCL-FLD TO FRM-NUM-PAGI-EDT IN SCL-EDT.
           MOVE FRM-NUM-PAGI-EDT IN SCL-EDT TO 
                FRM-NUM-PAGI-ALF IN SCL-FLD.
*% INCLUDE FIN_EDT_IKY_SCL OF 'COLBMSCL.TXT'
       FIN-EDT-IKY-SCL.
           EXIT.
       
       EDT-MKY-SCL SECTION.
       INI-EDT-MKY-SCL.
*% INCLUDE EDT_MKY_SCL OF 'COLBMSCL.TXT'
*% INCLUDE FIN_EDT_MKY_SCL OF 'COLBMSCL.TXT'
       FIN-EDT-MKY-SCL.
           EXIT.
       
       EDT-AKY-SCL SECTION.
       INI-EDT-AKY-SCL.
*% INCLUDE EDT_AKY_SCL OF 'COLBMSCL.TXT'
*% INCLUDE FIN_EDT_AKY_SCL OF 'COLBMSCL.TXT'
       FIN-EDT-AKY-SCL.
           EXIT.
       
       EDT-FLD-SCL SECTION.
       INI-EDT-FLD-SCL.
*% INCLUDE EDT_FLD_SCL OF 'COLBMSCL.TXT'
*% INCLUDE FIN_EDT_FLD_SCL OF 'COLBMSCL.TXT'
       FIN-EDT-FLD-SCL.
           EXIT.
*% INCLUDE EOF_BQ OF 'COLBMSCL.TXT'
*% INCLUDE EOF OF 'COLBMSCL.TXT'
*% INCLUDE EOF_IBM OF 'COLBMSCL.TXT'
