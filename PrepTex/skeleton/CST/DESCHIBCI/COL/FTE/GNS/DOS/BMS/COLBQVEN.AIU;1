      *
      * COLBQVEN
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:59 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "VEN"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMVEN.TXT'
       SET-TAG-COLBQVEN SECTION.
       INI-SET-TAG-COLBQVEN.
           MOVE
           '<IDNMOD NOM=$COLBQVEN$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:59 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQVEN.
           EXIT.
           
       PUT-ALL-VEN SECTION.
       INI-PUT-ALL-VEN.
*% INCLUDE INI_PUT_ALL_VEN OF 'COLBMVEN.TXT'
           PERFORM PUT-OPC-VEN.
           PERFORM PUT-VEN-VEN.
*% INCLUDE FIN_PUT_ALL_VEN OF 'COLBMVEN.TXT'
       FIN-PUT-ALL-VEN.
           EXIT.
       
       PUT-OPC-VEN SECTION.
       INI-PUT-OPC-VEN.
*% INCLUDE INI_PUT_OPC_VEN OF 'COLBMVEN.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-VEN.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-VEN
               GO TO FIN-PUT-OPC-VEN.
           MOVE OPC-MSC-TVOC IN OPC TO OPC-MSC-TVOC IN VEN-FLD.
           MOVE OPC-COD-TOOC IN OPC TO OPC-COD-TOOC IN VEN-FLD.
           MOVE OPC-COD-OFOC IN OPC TO OPC-COD-OFOC IN VEN-FLD.
           MOVE OPC-COD-VCOC IN OPC TO OPC-COD-VCOC IN VEN-FLD.
*% BUF = "VEN-FLD"
*% SIS = "TAB"
*% TAB = "TVAL"
*% FLD = "OPC-MSC-TVOC"
*% GLS = "FRM-GLS-TVOC"
*% TIENE_GLOSA = TRUE
*% LGL = 13
*% INCLUDE 'BUSMSC'
*% BUF = "VEN-FLD"
*% SIS = "TAB"
*% TAB = "TIO "
*% FLD = "OPC-COD-TOOC"
*% GLS = "FRM-GLS-TOOC"
*% TIENE_GLOSA = TRUE
*% LGL = 14
*% INCLUDE 'BUSTAB'
*% BUF = "VEN-FLD"
*% SIS = "TAB"
*% TAB = "OFI "
*% FLD = "OPC-COD-OFOC"
*% GLS = "FRM-GLS-OFOC"
*% TIENE_GLOSA = TRUE
*% LGL = 14
*% INCLUDE 'BUSOFI          '
*% BUF = "VEN-FLD"
*% SIS = "TAB"
*% TAB = "VLR "
*% FLD = "OPC-COD-VCOC"
*% GLS = "FRM-GLS-VCOC"
*% TIENE_GLOSA = TRUE
*% LGL = 30
*% INCLUDE 'BUSTAB'
*% INCLUDE FIN_PUT_OPC_VEN OF 'COLBMVEN.TXT'
       FIN-PUT-OPC-VEN.
           EXIT.
       
       PUT-VEN-VEN SECTION.
       INI-PUT-VEN-VEN.
*% INCLUDE INI_PUT_VEN_VEN OF 'COLBMVEN.TXT'
           IF PGM-STAT-VEN-NUL
               GO TO FIN-PUT-VEN-VEN.
           IF NOT PGM-STAT-VEN-OKS
               PERFORM CLR-VEN-VEN
               GO TO FIN-PUT-VEN-VEN.
           MOVE VEN-CAI-IOPC IN VEN TO VEN-CAI-IOPC IN VEN-FLD.
           MOVE VEN-IIC-IOPC IN VEN TO VEN-IIC-IOPC IN VEN-FLD.
           MOVE VEN-NUM-IVCT IN VEN TO VEN-NUM-IVCT IN VEN-FLD.
           MOVE VEN-NUM-IVEN IN VEN TO VEN-NUM-IVEN IN VEN-FLD.
           MOVE VEN-NUM-IDLC IN VEN TO VEN-NUM-IDLC IN VEN-FLD.
*% RREG = "VEN"
*% RREG_FRM = "VEN"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE VEN-NUM-DVEN IN VEN TO VEN-NUM-DVEN IN VEN-FLD.
           MOVE VEN-NUM-MVEN IN VEN TO VEN-NUM-MVEN IN VEN-FLD.
           MOVE VEN-NUM-SVEN IN VEN TO VEN-NUM-SVEN IN VEN-FLD.
           MOVE VEN-NUM-AVEN IN VEN TO VEN-NUM-AVEN IN VEN-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE VEN-VAL-CAPI IN VEN TO VEN-VAL-CAPI IN VEN-FLD.
*% RREG = "VEN"
*% RREG_FRM = "VEN"
*% NOMB = "FIMV"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE VEN-NUM-DIMV IN VEN TO VEN-NUM-DIMV IN VEN-FLD.
           MOVE VEN-NUM-MIMV IN VEN TO VEN-NUM-MIMV IN VEN-FLD.
           MOVE VEN-NUM-SIMV IN VEN TO VEN-NUM-SIMV IN VEN-FLD.
           MOVE VEN-NUM-AIMV IN VEN TO VEN-NUM-AIMV IN VEN-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE VEN-VAL-SCAP IN VEN TO VEN-VAL-SCAP IN VEN-FLD.
           MOVE VEN-IND-SCTB IN VEN TO VEN-IND-SCTB IN VEN-FLD.
           MOVE VEN-VAL-IDNI IN VEN TO VEN-VAL-IDNI IN VEN-FLD.
           MOVE VEN-IND-SCBZ IN VEN TO VEN-IND-SCBZ IN VEN-FLD.
           MOVE VEN-VAL-FINA IN VEN TO VEN-VAL-FINA IN VEN-FLD.
           MOVE VEN-IND-CAUT IN VEN TO VEN-IND-CAUT IN VEN-FLD.
           MOVE VEN-VAL-SFIN IN VEN TO VEN-VAL-SFIN IN VEN-FLD.
*% INCLUDE FIN_PUT_VEN_VEN OF 'COLBMVEN.TXT'
       FIN-PUT-VEN-VEN.
           EXIT.
       
       KEY-ALL-VEN SECTION.
       INI-KEY-ALL-VEN.
*% INCLUDE INI_KEY_ALL_VEN OF 'COLBMVEN.TXT'
           PERFORM KEY-OPC-VEN.
           PERFORM KEY-VEN-VEN.
*% INCLUDE FIN_KEY_ALL_VEN OF 'COLBMVEN.TXT'
       FIN-KEY-ALL-VEN.
           EXIT.
       
       KEY-OPC-VEN SECTION.
       INI-KEY-OPC-VEN.
*% INCLUDE INI_KEY_OPC_VEN OF 'COLBMVEN.TXT'
*% INCLUDE FIN_KEY_OPC_VEN OF 'COLBMVEN.TXT'
       FIN-KEY-OPC-VEN.
           EXIT.
       
       KEY-VEN-VEN SECTION.
       INI-KEY-VEN-VEN.
*% INCLUDE INI_KEY_VEN_VEN OF 'COLBMVEN.TXT'
           MOVE VEN-CAI-IOPC IN VEN-FLD TO VEN-CAI-IOPC IN VEN.
           MOVE VEN-IIC-IOPC IN VEN-FLD TO VEN-IIC-IOPC IN VEN.
           MOVE VEN-NUM-IVCT IN VEN-FLD TO VEN-NUM-IVCT IN VEN.
           MOVE VEN-NUM-IVEN IN VEN-FLD TO VEN-NUM-IVEN IN VEN.
*% INCLUDE FIN_KEY_VEN_VEN OF 'COLBMVEN.TXT'
       FIN-KEY-VEN-VEN.
           EXIT.
       
       SCR-CLRA-VEN SECTION.
       INI-SCR-CLRA-VEN.
*% INCLUDE INI_SCR_CLRA_VEN OF 'COLBMVEN.TXT'
           PERFORM CLR-KEY-VEN.
           PERFORM CLR-FLD-VEN.
*% INCLUDE FIN_SCR_CLRA_VEN OF 'COLBMVEN.TXT'
       FIN-SCR-CLRA-VEN.
           EXIT.
       
       CLR-KEY-VEN SECTION.
       INI-CLR-KEY-VEN.
*% INCLUDE INI_CLR_KEY_VEN OF 'COLBMVEN.TXT'
           MOVE SPACES TO VEN-CAI-IOPC IN VEN-FLD.
           MOVE ZEROES TO VEN-IIC-IOPC IN VEN-FLD.
           MOVE ZEROES TO VEN-NUM-IVCT IN VEN-FLD.
           MOVE ZEROES TO VEN-NUM-IVEN IN VEN-FLD.
*% INCLUDE FIN_CLR_KEY_VEN OF 'COLBMVEN.TXT'
       FIN-CLR-KEY-VEN.
           EXIT.
       
       CLR-FLD-VEN SECTION.
       INI-CLR-FLD-VEN.
*% INCLUDE INI_CLR_FLD_VEN OF 'COLBMVEN.TXT'
           MOVE SPACES TO FRM-GLS-TVOC IN VEN-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN VEN-FLD.
           MOVE SPACES TO FRM-GLS-OFOC IN VEN-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN VEN-FLD.
           PERFORM CLR-OPC-VEN.
           PERFORM CLR-VEN-VEN.
*% INCLUDE FIN_CLR_FLD_VEN OF 'COLBMVEN.TXT'
       FIN-CLR-FLD-VEN.
           EXIT.
       
       CLR-OPC-VEN SECTION.
       INI-CLR-OPC-VEN.
*% INCLUDE INI_CLR_OPC_VEN OF 'COLBMVEN.TXT'
           MOVE SPACES TO OPC-MSC-TVOC IN VEN-FLD.
           MOVE SPACES TO OPC-COD-TOOC IN VEN-FLD.
           MOVE SPACES TO OPC-COD-OFOC IN VEN-FLD.
           MOVE SPACES TO OPC-COD-VCOC IN VEN-FLD.
*% INCLUDE FIN_CLR_OPC_VEN OF 'COLBMVEN.TXT'
       FIN-CLR-OPC-VEN.
           EXIT.
       
       CLR-VEN-VEN SECTION.
       INI-CLR-VEN-VEN.
*% INCLUDE INI_CLR_VEN_VEN OF 'COLBMVEN.TXT'
           MOVE ZEROES TO VEN-NUM-IDLC IN VEN-FLD.
*% RREG = "VEN"
*% RREG_FRM = "VEN"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO VEN-FEC-FVEN IN VEN-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO VEN-VAL-CAPI IN VEN-FLD.
*% RREG = "VEN"
*% RREG_FRM = "VEN"
*% NOMB = "FIMV"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO VEN-FEC-FIMV IN VEN-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO VEN-VAL-SCAP IN VEN-FLD.
           MOVE SPACES TO VEN-IND-SCTB IN VEN-FLD.
           MOVE ZEROES TO VEN-VAL-IDNI IN VEN-FLD.
           MOVE SPACES TO VEN-IND-SCBZ IN VEN-FLD.
           MOVE ZEROES TO VEN-VAL-FINA IN VEN-FLD.
           MOVE SPACES TO VEN-IND-CAUT IN VEN-FLD.
           MOVE ZEROES TO VEN-VAL-SFIN IN VEN-FLD.
*% INCLUDE FIN_CLR_VEN_VEN OF 'COLBMVEN.TXT'
       FIN-CLR-VEN-VEN.
           EXIT.
       
       FST-KEY-VEN SECTION.
       INI-FST-KEY-VEN.
*% INCLUDE INI_FST_KEY_VEN OF 'COLBMVEN.TXT'
           MOVE -1 TO VEN-CAI-IOPC-LEN IN VEN-FLD.
*% INCLUDE FIN_FST_KEY_VEN OF 'COLBMVEN.TXT'
       FIN-FST-KEY-VEN.
           EXIT.
       
       FST-FLD-VEN SECTION.
       INI-FST-FLD-VEN.
*% INCLUDE INI_FST_FLD_VEN OF 'COLBMVEN.TXT'
           MOVE -1 TO VEN-NUM-IDLC-LEN IN VEN-FLD.
*% INCLUDE FIN_FST_FLD_VEN OF 'COLBMVEN.TXT'
       FIN-FST-FLD-VEN.
           EXIT.
       
       PRO-KEY-VEN SECTION.
       INI-PRO-KEY-VEN.
*% INCLUDE INI_PRO_KEY_VEN OF 'COLBMVEN.TXT'
           PERFORM PRO-IKY-VEN.
           PERFORM PRO-AKY-VEN.
           PERFORM PRO-MKY-VEN.
*% INCLUDE FIN_PRO_KEY_VEN OF 'COLBMVEN.TXT'
       FIN-PRO-KEY-VEN.
           EXIT.
       
       PRO-IKY-VEN SECTION.
       INI-PRO-IKY-VEN.
*% INCLUDE INI_PRO_IKY_VEN OF 'COLBMVEN.TXT'
           MOVE FRM-CPIM-AEY TO VEN-CAI-IOPC-ATR IN VEN-FLD.
           MOVE FRM-CPIM-AEY TO VEN-IIC-IOPC-ATR IN VEN-FLD.
*% INCLUDE FIN_PRO_IKY_VEN OF 'COLBMVEN.TXT'
       FIN-PRO-IKY-VEN.
           EXIT.
       
       PRO-MKY-VEN SECTION.
       INI-PRO-MKY-VEN.
*% INCLUDE INI_PRO_MKY_VEN OF 'COLBMVEN.TXT'
*% INCLUDE FIN_PRO_MKY_VEN OF 'COLBMVEN.TXT'
       FIN-PRO-MKY-VEN.
           EXIT.
       
       PRO-AKY-VEN SECTION.
       INI-PRO-AKY-VEN.
*% INCLUDE INI_PRO_AKY_VEN OF 'COLBMVEN.TXT'
           MOVE FRM-CPIM-AEY TO VEN-NUM-IVCT-ATR IN VEN-FLD.
*% INCLUDE FIN_PRO_AKY_VEN OF 'COLBMVEN.TXT'
       FIN-PRO-AKY-VEN.
           EXIT.
       
       UNP-KEY-VEN SECTION.
       INI-UNP-KEY-VEN.
*% INCLUDE INI_UNP_KEY_VEN OF 'COLBMVEN.TXT'
           PERFORM UNP-IKY-VEN.
           PERFORM UNP-AKY-VEN.
           PERFORM UNP-MKY-VEN.
*% INCLUDE FIN_UNP_KEY_VEN OF 'COLBMVEN.TXT'
       FIN-UNP-KEY-VEN.
           EXIT.
       
       UNP-IKY-VEN SECTION.
       INI-UNP-IKY-VEN.
*% INCLUDE INI_UNP_IKY_VEN OF 'COLBMVEN.TXT'
           MOVE FRM-CPIM-UBY TO VEN-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO VEN-IIC-IOPC-ATR.
*% INCLUDE FIN_UNP_IKY_VEN OF 'COLBMVEN.TXT'
       FIN-UNP-IKY-VEN.
           EXIT.
       
       UNP-MKY-VEN SECTION.
       INI-UNP-MKY-VEN.
*% INCLUDE INI_UNP_MKY_VEN OF 'COLBMVEN.TXT'
*% INCLUDE FIN_UNP_MKY_VEN OF 'COLBMVEN.TXT'
       FIN-UNP-MKY-VEN.
           EXIT.
       
       UNP-AKY-VEN SECTION.
       INI-UNP-AKY-VEN.
*% INCLUDE INI_UNP_AKY_VEN OF 'COLBMVEN.TXT'
           MOVE FRM-CPIM-UBY TO VEN-NUM-IVCT-ATR.
*% INCLUDE FIN_UNP_AKY_VEN OF 'COLBMVEN.TXT'
       FIN-UNP-AKY-VEN.
           EXIT.
       
       UNP-FLD-VEN SECTION.
       INI-UNP-FLD-VEN.
*% INCLUDE INI_UNP_FLD_VEN OF 'COLBMVEN.TXT'
           PERFORM UNP-OPC-VEN.
           PERFORM UNP-VEN-VEN.
*% INCLUDE FIN_UNP_FLD_VEN OF 'COLBMVEN.TXT'
       FIN-UNP-FLD-VEN.
           EXIT.
       
       UNP-OPC-VEN SECTION.
       INI-UNP-OPC-VEN.
*% INCLUDE INI_UNP_OPC_VEN OF 'COLBMVEN.TXT'
*% INCLUDE FIN_UNP_OPC_VEN OF 'COLBMVEN.TXT'
       FIN-UNP-OPC-VEN.
           EXIT.
       
       UNP-VEN-VEN SECTION.
       INI-UNP-VEN-VEN.
*% INCLUDE INI_UNP_VEN_VEN OF 'COLBMVEN.TXT'
           MOVE FRM-CPIM-UBY TO VEN-NUM-IDLC-ATR IN VEN-FLD.
*% RREG = "VEN"
*% RREG_FRM = "VEN"
*% NOMB = "FVEN"
           MOVE FRM-CPIM-UBY TO VEN-FEC-FVEN-ATR IN VEN-FLD.
           MOVE FRM-CPIM-UBY TO VEN-VAL-CAPI-ATR IN VEN-FLD.
*% RREG = "VEN"
*% RREG_FRM = "VEN"
*% NOMB = "FIMV"
           MOVE FRM-CPIM-UBY TO VEN-FEC-FIMV-ATR IN VEN-FLD.
           MOVE FRM-CPIM-UBY TO VEN-IND-SCTB-ATR IN VEN-FLD.
           MOVE FRM-CPIM-UBY TO VEN-VAL-IDNI-ATR IN VEN-FLD.
           MOVE FRM-CPIM-UBY TO VEN-IND-SCBZ-ATR IN VEN-FLD.
           MOVE FRM-CPIM-UBY TO VEN-VAL-FINA-ATR IN VEN-FLD.
           MOVE FRM-CPIM-UBY TO VEN-IND-CAUT-ATR IN VEN-FLD.
           MOVE FRM-CPIM-UBY TO VEN-VAL-SFIN-ATR IN VEN-FLD.
*% INCLUDE FIN_UNP_VEN_VEN OF 'COLBMVEN.TXT'
       FIN-UNP-VEN-VEN.
           EXIT.
       
       VAL-KEY-VEN SECTION.
       INI-VAL-KEY-VEN.
           PERFORM VAL-IKY-VEN.
           PERFORM VAL-MKY-VEN.
           PERFORM VAL-AKY-VEN.
       FIN-VAL-KEY-VEN.
           EXIT.
       
       VAL-IKY-VEN SECTION.
       INI-VAL-IKY-VEN.
*% INCLUDE INI_VAL_IKY_VEN OF 'COLBMVEN.TXT'
           PERFORM VAL-VEN-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO VEN-CAI-IOPC-LEN IN VEN-FLD
                GO TO FIN-VAL-IKY-VEN.
           PERFORM VAL-VEN-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO VEN-IIC-IOPC-LEN IN VEN-FLD
                GO TO FIN-VAL-IKY-VEN.
*% INCLUDE FIN_VAL_IKY_VEN OF 'COLBMVEN.TXT'
       FIN-VAL-IKY-VEN.
           EXIT.
       
       VAL-MKY-VEN SECTION.
       INI-VAL-MKY-VEN.
*% INCLUDE INI_VAL_MKY_VEN OF 'COLBMVEN.TXT'
*% INCLUDE FIN_VAL_MKY_VEN OF 'COLBMVEN.TXT'
       FIN-VAL-MKY-VEN.
           EXIT.
       
       VAL-AKY-VEN SECTION.
       INI-VAL-AKY-VEN.
*% INCLUDE INI_VAL_AKY_VEN OF 'COLBMVEN.TXT'
           PERFORM VAL-VEN-NUM-IVCT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO VEN-NUM-IVCT-LEN IN VEN-FLD
                GO TO FIN-VAL-AKY-VEN.
           PERFORM VAL-VEN-NUM-IVEN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO VEN-NUM-IVEN-LEN IN VEN-FLD
                GO TO FIN-VAL-AKY-VEN.
*% INCLUDE FIN_VAL_AKY_VEN OF 'COLBMVEN.TXT'
       FIN-VAL-AKY-VEN.
           EXIT.
       
       VAL-VEN-CAI-IOPC SECTION.
       INI-VAL-VEN-CAI-IOPC.
*% LBL = "VEN_CAI_IOPC OF 'COLBMVEN.TXT'"
*% INCLUDE INI_VEN_CAI_IOPC OF 'COLBMVEN.TXT'
           IF VEN-CAI-IOPC IN VEN-FLD NOT > SPACES
              MOVE SPACES TO VEN-CAI-IOPC IN VEN-FLD.
*% INCLUDE VEN_CAI_IOPC OF 'COLBMVEN.TXT'
*% INCLUDE FIN_VEN_CAI_IOPC OF 'COLBMVEN.TXT'
       FIN-VAL-VEN-CAI-IOPC.
           EXIT.
       
       VAL-VEN-IIC-IOPC SECTION.
       INI-VAL-VEN-IIC-IOPC.
*% LBL = "VEN_IIC_IOPC OF 'COLBMVEN.TXT'"
*% INCLUDE INI_VEN_IIC_IOPC OF 'COLBMVEN.TXT'
*% INCLUDE VEN_IIC_IOPC OF 'COLBMVEN.TXT'
*% INCLUDE FIN_VEN_IIC_IOPC OF 'COLBMVEN.TXT'
       FIN-VAL-VEN-IIC-IOPC.
           EXIT.
       
       VAL-VEN-NUM-IVCT SECTION.
       INI-VAL-VEN-NUM-IVCT.
*% LBL = "VEN_NUM_IVCT OF 'COLBMVEN.TXT'"
*% INCLUDE INI_VEN_NUM_IVCT OF 'COLBMVEN.TXT'
*% INCLUDE VEN_NUM_IVCT OF 'COLBMVEN.TXT'
*% INCLUDE FIN_VEN_NUM_IVCT OF 'COLBMVEN.TXT'
       FIN-VAL-VEN-NUM-IVCT.
           EXIT.
       
       VAL-VEN-NUM-IVEN SECTION.
       INI-VAL-VEN-NUM-IVEN.
*% LBL = "VEN_NUM_IVEN OF 'COLBMVEN.TXT'"
*% INCLUDE INI_VEN_NUM_IVEN OF 'COLBMVEN.TXT'
*% INCLUDE VEN_NUM_IVEN OF 'COLBMVEN.TXT'
*% INCLUDE FIN_VEN_NUM_IVEN OF 'COLBMVEN.TXT'
       FIN-VAL-VEN-NUM-IVEN.
           EXIT.
*% RREG = "VEN"
*% RREG_FRM = "VEN"
*% NOMB = "FVEN"
*% IZR = "S"
*% RREG = "VEN"
*% RREG_FRM = "VEN"
*% NOMB = "FIMV"
*% IZR = "S"
       
       VAL-CON-KEY-VEN SECTION.
       INI-VAL-CON-KEY-VEN.
*% INCLUDE INI_VAL_CON_KEY_VEN OF 'COLBMVEN.TXT'
           IF     VEN-CAI-IOPC IN VEN-FLD NOT > SPACES 
              AND VEN-IIC-IOPC IN VEN-FLD NOT > ZEROES 
              AND VEN-NUM-IVCT IN VEN-FLD NOT > ZEROES 
              AND VEN-NUM-IVEN IN VEN-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-VEN.
*% INCLUDE FIN_VAL_CON_KEY_VEN OF 'COLBMVEN.TXT'
       FIN-VAL-CON-KEY-VEN.
           EXIT.
       
       VAL-CON-IKY-VEN SECTION.
       INI-VAL-CON-IKY-VEN.
*% INCLUDE INI_VAL_CON_IKY_VEN OF 'COLBMVEN.TXT'
           IF     VEN-CAI-IOPC IN VEN-FLD NOT > SPACES 
              AND VEN-IIC-IOPC IN VEN-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-VEN.
*% INCLUDE FIN_VAL_CON_IKY_VEN OF 'COLBMVEN.TXT'
       FIN-VAL-CON-IKY-VEN.
           EXIT.
       
       VAL-NUM-KEY-VEN SECTION.
       INI-VAL-NUM-KEY-VEN.
           PERFORM VAL-NUM-IKY-VEN.
           PERFORM VAL-NUM-MKY-VEN.
           PERFORM VAL-NUM-AKY-VEN.
       FIN-VAL-NUM-KEY-VEN.
           EXIT.
       
       VAL-NUM-IKY-VEN SECTION.
       INI-VAL-NUM-IKY-VEN.
*% INCLUDE INI_VAL_NUM_IKY_VEN OF 'COLBMVEN.TXT'
       VAL-NUM-VEN-IIC-IOPC.
*% BUF = "VEN-FLD"
*% FLD = "VEN-IIC-IOPC"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-VEN.
*% INCLUDE FIN_VAL_NUM_IKY_VEN OF 'COLBMVEN.TXT'
       FIN-VAL-NUM-IKY-VEN.
           EXIT.
       
       VAL-NUM-MKY-VEN SECTION.
       INI-VAL-NUM-MKY-VEN.
*% INCLUDE INI_VAL_NUM_MKY_VEN OF 'COLBMVEN.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_VEN OF 'COLBMVEN.TXT'
       FIN-VAL-NUM-MKY-VEN.
           EXIT.
       
       VAL-NUM-AKY-VEN SECTION.
       INI-VAL-NUM-AKY-VEN.
*% INCLUDE INI_VAL_NUM_AKY_VEN OF 'COLBMVEN.TXT'
       VAL-NUM-VEN-NUM-IVCT.
*% BUF = "VEN-FLD"
*% FLD = "VEN-NUM-IVCT"
*% ENT = "3"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-AKY-VEN.
       VAL-NUM-VEN-NUM-IVEN.
*% BUF = "VEN-FLD"
*% FLD = "VEN-NUM-IVEN"
*% ENT = "3"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-AKY-VEN.
*% INCLUDE FIN_VAL_NUM_AKY_VEN OF 'COLBMVEN.TXT'
       FIN-VAL-NUM-AKY-VEN.
           EXIT.
       
       VAL-NUM-FLD-VEN SECTION.
       INI-VAL-NUM-FLD-VEN.
*% INCLUDE INI_VAL_NUM_FLD_VEN OF 'COLBMVEN.TXT'
       VAL-NUM-VEN-NUM-IDLC.
*% BUF = "VEN-FLD"
*% FLD = "VEN-NUM-IDLC"
