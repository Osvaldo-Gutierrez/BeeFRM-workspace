*% FRM = "CCC"
      *
      * COLBUCCC
      * Version 3.0.1 Nov-2005
      * 20-Apr-12 11:40 AM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMCCC.TXT'
       SET-TAG-COLBUCCC SECTION.
       INI-SET-TAG-COLBUCCC.
           MOVE
           '<IDNMOD NOM=$COLBUCCC$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$20-Apr-12 11:40 AM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCCC.
           EXIT.
           
       RET-ALL-CCC SECTION.
       INI-RET-ALL-CCC.
*% INCLUDE INI_RET_ALL_CCC OF 'COLBMCCC.TXT'
           PERFORM KEY-ALL-CCC.
           PERFORM RET-DBC-CCC.
           PERFORM RET-RDC-CCC.
           PERFORM RET-OPC-CCC.
*% INCLUDE FIN_RET_ALL_CCC OF 'COLBMCCC.TXT'
       FIN-RET-ALL-CCC.
           EXIT.
       
       RET-DBC-CCC SECTION.
       INI-RET-DBC-CCC.
*% INCLUDE INI_RET_DBC_CCC OF 'COLBMCCC.TXT'
           MOVE DBC-COD-EJEC IN CCC-FLD TO DBC-COD-EJEC IN DBC.
           MOVE DBC-CAI-ICLI IN CCC-FLD TO DBC-CAI-ICLI IN DBC.
           MOVE DBC-IIC-ICLI IN CCC-FLD TO DBC-IIC-ICLI IN DBC.
*% INCLUDE FIN_RET_DBC_CCC OF 'COLBMCCC.TXT'
       FIN-RET-DBC-CCC.
           EXIT.
       
       RET-RDC-CCC SECTION.
       INI-RET-RDC-CCC.
*% INCLUDE INI_RET_RDC_CCC OF 'COLBMCCC.TXT'
           MOVE RDC-CIC-ICLI IN CCC-FLD TO RDC-CIC-ICLI IN RDC.
*% INCLUDE FIN_RET_RDC_CCC OF 'COLBMCCC.TXT'
       FIN-RET-RDC-CCC.
           EXIT.
       
       RET-OPC-CCC SECTION.
       INI-RET-OPC-CCC.
*% INCLUDE INI_RET_OPC_CCC OF 'COLBMCCC.TXT'
*% INCLUDE FIN_RET_OPC_CCC OF 'COLBMCCC.TXT'
       FIN-RET-OPC-CCC.
           EXIT.
       
       KEY-ALL-CCC SECTION.
       INI-KEY-ALL-CCC.
*% INCLUDE INI_KEY_ALL_CCC OF 'COLBMCCC.TXT'
           PERFORM KEY-DBC-CCC.
           PERFORM KEY-RDC-CCC.
           PERFORM KEY-OPC-CCC.
*% INCLUDE FIN_KEY_ALL_CCC OF 'COLBMCCC.TXT'
       FIN-KEY-ALL-CCC.
           EXIT.
       
       KEY-DBC-CCC SECTION.
       INI-KEY-DBC-CCC.
*% INCLUDE INI_KEY_DBC_CCC OF 'COLBMCCC.TXT'
           MOVE DBC-GLS-NOMC IN CCC-FLD TO DBC-GLS-NOMC IN DBC.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN CCC-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN CCC-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN CCC-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN CCC-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_KEY_DBC_CCC OF 'COLBMCCC.TXT'
       FIN-KEY-DBC-CCC.
           EXIT.
       
       KEY-RDC-CCC SECTION.
       INI-KEY-RDC-CCC.
*% INCLUDE INI_KEY_RDC_CCC OF 'COLBMCCC.TXT'
*% INCLUDE FIN_KEY_RDC_CCC OF 'COLBMCCC.TXT'
       FIN-KEY-RDC-CCC.
           EXIT.
       
       KEY-OPC-CCC SECTION.
       INI-KEY-OPC-CCC.
*% INCLUDE INI_KEY_OPC_CCC OF 'COLBMCCC.TXT'
           MOVE OPC-IND-RENO IN CCC-FLD TO OPC-IND-RENO IN OPC.
*% INCLUDE FIN_KEY_OPC_CCC OF 'COLBMCCC.TXT'
       FIN-KEY-OPC-CCC.
           EXIT.
       
       SCR-CLRA-CCC SECTION.
       INI-SCR-CLRA-CCC.
*% INCLUDE INI_SCR_CLRA_CCC OF 'COLBMCCC.TXT'
           PERFORM CLR-KEY-CCC.
           PERFORM CLR-FLD-CCC.
*% INCLUDE FIN_SCR_CLRA_CCC OF 'COLBMCCC.TXT'
       FIN-SCR-CLRA-CCC.
           EXIT.
       
       CLR-KEY-CCC SECTION.
       INI-CLR-KEY-CCC.
*% INCLUDE INI_CLR_KEY_CCC OF 'COLBMCCC.TXT'
           MOVE SPACES TO DBC-GLS-NOMC IN CCC-FLD.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN CCC-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN CCC-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN CCC-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN CCC-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-COD-TOOC IN CCC-FLD.
           MOVE SPACES TO FRM-COD-AOOC IN CCC-FLD.
           MOVE SPACES TO FRM-COD-VCOC IN CCC-FLD.
           MOVE SPACES TO FRM-IND-TDEU IN CCC-FLD.
           MOVE SPACES TO FRM-COD-STAT IN CCC-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FINI"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FINI IN CCC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FTER"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FTER IN CCC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-IND-IOPC IN CCC-FLD.
           MOVE SPACES TO OPC-IND-RENO IN CCC-FLD.
           MOVE SPACES TO FRM-COD-ICNL IN CCC-FLD.
*% INCLUDE FIN_CLR_KEY_CCC OF 'COLBMCCC.TXT'
       FIN-CLR-KEY-CCC.
           EXIT.
       
       CLR-FLD-CCC SECTION.
       INI-CLR-FLD-CCC.
*% INCLUDE INI_CLR_FLD_CCC OF 'COLBMCCC.TXT'
           MOVE SPACES TO FRM-GLS-EJEC IN CCC-FLD.
           MOVE ZEROES TO FRM-NUM-TDLC IN CCC-FLD.
           MOVE ZEROES TO FRM-VAL-TDLC IN CCC-FLD.
           MOVE SPACES TO FRM-GLS-TDEU IN CCC-FLD.
           MOVE SPACES TO FRM-GLS-CDEU IN CCC-FLD.
           MOVE SPACES TO FRM-CAI-IOPC IN CCC-FLD.
           MOVE SPACES TO FRM-IIC-IOPC IN CCC-FLD.
           MOVE SPACES TO FRM-NUM-IDLC IN CCC-FLD.
           MOVE SPACES TO FRM-COD-COOC IN CCC-FLD.
           MOVE SPACES TO FRM-VAL-NDLC IN CCC-FLD.
           MOVE SPACES TO FRM-GLS-NOMC IN CCC-FLD.
           MOVE SPACES TO FRM-GLS-COOC IN CCC-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN CCC-FLD.
           MOVE SPACES TO FRM-GLS-OFOC IN CCC-FLD.
           MOVE SPACES TO FRM-COD-TDEU IN CCC-FLD.
           MOVE ZEROES TO FRM-VAL-CRED IN CCC-FLD.
           MOVE ZEROES TO FRM-VAL-SCRE IN CCC-FLD.
           MOVE SPACES TO FRM-IND-RENO IN CCC-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FTRN IN CCC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-HRA-HRTR IN CCC-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FCOL IN CCC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-VCOR IN CCC-FLD.
           MOVE ZEROES TO FRM-NUM-VCAN IN CCC-FLD.
           MOVE SPACES TO FRM-COD-CANL IN CCC-FLD.
           MOVE SPACES TO FRM-GLS-CANL IN CCC-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FVEN IN CCC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-VAL-SFIN IN CCC-FLD.
           MOVE ZEROES TO FRM-VAL-SCAP IN CCC-FLD.
           MOVE ZEROES TO FRM-VAL-INTV IN CCC-FLD.
           MOVE ZEROES TO FRM-SGV-TINT IN CCC-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FMAV"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FMAV IN CCC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-COD-OFOC IN CCC-FLD.
           MOVE SPACES TO FRM-GLS-STAT IN CCC-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ACEP"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ACEP"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-ACEP IN CCC-FLD.
           MOVE SPACES TO FRM-VRF-ACEP IN CCC-FLD.
           MOVE SPACES TO FRM-IND-ACEP IN CCC-FLD.
           MOVE SPACES TO FRM-GLS-ACEP IN CCC-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-COD-NUNI IN CCC-FLD.
           MOVE SPACES TO FRM-COD-MONE IN CCC-FLD.
           PERFORM CLR-DBC-CCC.
           PERFORM CLR-RDC-CCC.
           PERFORM CLR-OPC-CCC.
*% INCLUDE FIN_CLR_FLD_CCC OF 'COLBMCCC.TXT'
       FIN-CLR-FLD-CCC.
           EXIT.
       
       CLR-DBC-CCC SECTION.
       INI-CLR-DBC-CCC.
*% INCLUDE INI_CLR_DBC_CCC OF 'COLBMCCC.TXT'
