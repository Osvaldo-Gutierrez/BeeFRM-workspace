       INI-PRO-OPC-CCC.
*% INCLUDE INI_PRO_OPC_CCC OF 'COLBMCCC.TXT'
*% INCLUDE FIN_PRO_OPC_CCC OF 'COLBMCCC.TXT'
       FIN-PRO-OPC-CCC.
           EXIT.
       
       UNP-KEY-CCC SECTION.
       INI-UNP-KEY-CCC.
*% INCLUDE INI_UNP_KEY_CCC OF 'COLBMCCC.TXT'
           MOVE FRM-CPIM-UBY TO FRM-COD-TOOC-ATR IN CCC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-COD-AOOC-ATR IN CCC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-COD-VCOC-ATR IN CCC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IND-TDEU-ATR IN CCC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-COD-STAT-ATR IN CCC-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FINI"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FINI-ATR IN CCC-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FTER"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FTER-ATR IN CCC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IND-IOPC-ATR IN CCC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-COD-ICNL-ATR IN CCC-FLD.
           PERFORM UNP-IKY-CCC.
           PERFORM UNP-AKY-CCC.
           PERFORM UNP-MKY-CCC.
*% INCLUDE FIN_UNP_KEY_CCC OF 'COLBMCCC.TXT'
       FIN-UNP-KEY-CCC.
           EXIT.
       
       UNP-IKY-CCC SECTION.
       INI-UNP-IKY-CCC.
*% INCLUDE INI_UNP_IKY_CCC OF 'COLBMCCC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO DBC-NUM-ICLI-ATR IN CCC-FLD.
           MOVE FRM-CPIM-UBY TO DBC-VRF-ICLI-ATR IN CCC-FLD.
           MOVE FRM-CPIM-UBY TO DBC-IND-ICLI-ATR IN CCC-FLD.
           MOVE FRM-CPIM-UBY TO DBC-GLS-ICLI-ATR IN CCC-FLD.
      * IDC-FIN --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO FRM-COD-TOOC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-COD-AOOC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-COD-VCOC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-IND-TDEU-ATR.
           MOVE FRM-CPIM-UBY TO FRM-COD-STAT-ATR.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FINI"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FINI-ATR IN CCC-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FTER"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FTER-ATR IN CCC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IND-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO OPC-IND-RENO-ATR.
           MOVE FRM-CPIM-UBY TO FRM-COD-ICNL-ATR.
*% INCLUDE FIN_UNP_IKY_CCC OF 'COLBMCCC.TXT'
       FIN-UNP-IKY-CCC.
           EXIT.
       
       UNP-MKY-CCC SECTION.
       INI-UNP-MKY-CCC.
*% INCLUDE INI_UNP_MKY_CCC OF 'COLBMCCC.TXT'
*% INCLUDE FIN_UNP_MKY_CCC OF 'COLBMCCC.TXT'
       FIN-UNP-MKY-CCC.
           EXIT.
       
       UNP-AKY-CCC SECTION.
       INI-UNP-AKY-CCC.
*% INCLUDE INI_UNP_AKY_CCC OF 'COLBMCCC.TXT'
           MOVE FRM-CPIM-UBY TO DBC-GLS-NOMC-ATR.
*% INCLUDE FIN_UNP_AKY_CCC OF 'COLBMCCC.TXT'
       FIN-UNP-AKY-CCC.
           EXIT.
       
       UNP-FLD-CCC SECTION.
       INI-UNP-FLD-CCC.
*% INCLUDE INI_UNP_FLD_CCC OF 'COLBMCCC.TXT'
           PERFORM UNP-DBC-CCC.
           PERFORM UNP-RDC-CCC.
           PERFORM UNP-OPC-CCC.
*% INCLUDE FIN_UNP_FLD_CCC OF 'COLBMCCC.TXT'
       FIN-UNP-FLD-CCC.
           EXIT.
       
       UNP-DBC-CCC SECTION.
       INI-UNP-DBC-CCC.
*% INCLUDE INI_UNP_DBC_CCC OF 'COLBMCCC.TXT'
*% INCLUDE FIN_UNP_DBC_CCC OF 'COLBMCCC.TXT'
       FIN-UNP-DBC-CCC.
           EXIT.
       
       UNP-RDC-CCC SECTION.
       INI-UNP-RDC-CCC.
*% INCLUDE INI_UNP_RDC_CCC OF 'COLBMCCC.TXT'
*% INCLUDE FIN_UNP_RDC_CCC OF 'COLBMCCC.TXT'
       FIN-UNP-RDC-CCC.
           EXIT.
       
       UNP-OPC-CCC SECTION.
       INI-UNP-OPC-CCC.
*% INCLUDE INI_UNP_OPC_CCC OF 'COLBMCCC.TXT'
*% INCLUDE FIN_UNP_OPC_CCC OF 'COLBMCCC.TXT'
       FIN-UNP-OPC-CCC.
           EXIT.
       
       VAL-MKY-CCC SECTION.
       INI-VAL-MKY-CCC.
*% INCLUDE INI_VAL_MKY_CCC OF 'COLBMCCC.TXT'
*% INCLUDE FIN_VAL_MKY_CCC OF 'COLBMCCC.TXT'
       FIN-VAL-MKY-CCC.
           EXIT.
       
       VAL-FLD-CCC SECTION.
       INI-VAL-FLD-CCC.
*% INCLUDE INI_VAL_FLD_CCC OF 'COLBMCCC.TXT'
           PERFORM VAL-DBC-COD-EJEC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DBC-COD-EJEC-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-FRM-GLS-EJEC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-EJEC-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-FRM-NUM-TDLC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-NUM-TDLC-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-FRM-VAL-TDLC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-TDLC-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-FRM-GLS-TDEU.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TDEU-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-DBC-CAI-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DBC-CAI-ICLI-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-DBC-IIC-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DBC-IIC-ICLI-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-RDC-CIC-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RDC-CIC-ICLI-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-FRM-GLS-CDEU.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-CDEU-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-FRM-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-IOPC-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-FRM-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-IOPC-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-FRM-NUM-IDLC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-NUM-IDLC-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-FRM-COD-COOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-COOC-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-FRM-VAL-NDLC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-NDLC-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-FRM-GLS-NOMC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-NOMC-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
