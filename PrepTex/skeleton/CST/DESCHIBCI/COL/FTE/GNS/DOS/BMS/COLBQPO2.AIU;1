           EXIT.
       
       EDT-KEY-PO2 SECTION.
       INI-EDT-KEY-PO2.
           PERFORM EDT-IKY-PO2.
           PERFORM EDT-MKY-PO2.
           PERFORM EDT-AKY-PO2.
*% INCLUDE EDT_KEY_PO2 OF 'COLBMPO2.TXT'
       FIN-EDT-KEY-PO2.
           EXIT.
       
       EDT-IKY-PO2 SECTION.
       INI-EDT-IKY-PO2.
*% INCLUDE EDT_IKY_PO2 OF 'COLBMPO2.TXT'
*% INCLUDE FIN_EDT_IKY_PO2 OF 'COLBMPO2.TXT'
