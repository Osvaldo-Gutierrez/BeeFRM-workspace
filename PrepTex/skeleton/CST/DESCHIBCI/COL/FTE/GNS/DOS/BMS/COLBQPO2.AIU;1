*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "PO2"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMPO2.TXT'
       SET-TAG-COLBQPO2 SECTION.
       INI-SET-TAG-COLBQPO2.
           MOVE
           '<IDNMOD NOM=$COLBQPO2$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:40 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQPO2.
           EXIT.
           
       PUT-ALL-PO2 SECTION.
       INI-PUT-ALL-PO2.
*% INCLUDE INI_PUT_ALL_PO2 OF 'COLBMPO2.TXT'
           PERFORM PUT-LAP-PO2.
           PERFORM PUT-POC-PO2.
*% INCLUDE FIN_PUT_ALL_PO2 OF 'COLBMPO2.TXT'
       FIN-PUT-ALL-PO2.
           EXIT.
       
       PUT-LAP-PO2 SECTION.
       INI-PUT-LAP-PO2.
*% INCLUDE INI_PUT_LAP_PO2 OF 'COLBMPO2.TXT'
           IF PGM-STAT-LAP-NUL
               GO TO FIN-PUT-LAP-PO2.
           IF NOT PGM-STAT-LAP-OKS
               PERFORM CLR-LAP-PO2
               GO TO FIN-PUT-LAP-PO2.
           MOVE LAP-COD-COME IN LAP TO LAP-COD-COME IN PO2-FLD.
*% INCLUDE FIN_PUT_LAP_PO2 OF 'COLBMPO2.TXT'
       FIN-PUT-LAP-PO2.
           EXIT.
       
       PUT-POC-PO2 SECTION.
       INI-PUT-POC-PO2.
*% INCLUDE INI_PUT_POC_PO2 OF 'COLBMPO2.TXT'
           IF PGM-STAT-POC-NUL
               GO TO FIN-PUT-POC-PO2.
           IF NOT PGM-STAT-POC-OKS
               PERFORM CLR-POC-PO2
               GO TO FIN-PUT-POC-PO2.
           MOVE POC-MSC-STAT IN POC TO POC-MSC-STAT IN PO2-FLD.
           MOVE POC-MSC-MIPL IN POC TO POC-MSC-MIPL IN PO2-FLD.
           MOVE POC-COD-CDOC IN POC TO POC-COD-CDOC IN PO2-FLD.
           MOVE POC-SIT-CTB1 IN POC TO POC-SIT-CTB1 IN PO2-FLD.
           MOVE POC-SIT-CTB2 IN POC TO POC-SIT-CTB2 IN PO2-FLD.
           MOVE POC-SIT-CTB3 IN POC TO POC-SIT-CTB3 IN PO2-FLD.
           MOVE POC-SIT-CRT1 IN POC TO POC-SIT-CRT1 IN PO2-FLD.
           MOVE POC-SIT-CRT2 IN POC TO POC-SIT-CRT2 IN PO2-FLD.
           MOVE POC-SIT-CRT3 IN POC TO POC-SIT-CRT3 IN PO2-FLD.
           MOVE POC-IND-GARA IN POC TO POC-IND-GARA IN PO2-FLD.
           MOVE POC-SIT-CBR1 IN POC TO POC-SIT-CBR1 IN PO2-FLD.
           MOVE POC-SIT-CBR2 IN POC TO POC-SIT-CBR2 IN PO2-FLD.
           MOVE POC-IND-GNTR IN POC TO POC-IND-GNTR IN PO2-FLD.
           MOVE POC-SIT-CBR3 IN POC TO POC-SIT-CBR3 IN PO2-FLD.
           MOVE POC-MSC-TCVE IN POC TO POC-MSC-TCVE IN PO2-FLD.
           MOVE POC-IND-AFER IN POC TO POC-IND-AFER IN PO2-FLD.
           MOVE POC-MSC-MCVF IN POC TO POC-MSC-MCVF IN PO2-FLD.
           MOVE POC-COD-ACTI IN POC TO POC-COD-ACTI IN PO2-FLD.
           MOVE POC-IND-CAUT IN POC TO POC-IND-CAUT IN PO2-FLD.
           MOVE POC-COD-COTO IN POC TO POC-COD-COTO IN PO2-FLD.
           MOVE POC-IND-TABO IN POC TO POC-IND-TABO IN PO2-FLD.
           MOVE POC-NUM-MINC IN POC TO POC-NUM-MINC IN PO2-FLD.
           MOVE POC-NUM-MINI IN POC TO POC-NUM-MINI IN PO2-FLD.
           MOVE POC-IND-MINC IN POC TO POC-IND-MINC IN PO2-FLD.
           MOVE POC-IND-MINI IN POC TO POC-IND-MINI IN PO2-FLD.
           MOVE POC-NUM-MAXC IN POC TO POC-NUM-MAXC IN PO2-FLD.
           MOVE POC-NUM-MAXI IN POC TO POC-NUM-MAXI IN PO2-FLD.
           MOVE POC-IND-MAXC IN POC TO POC-IND-MAXC IN PO2-FLD.
           MOVE POC-IND-MAXI IN POC TO POC-IND-MAXI IN PO2-FLD.
           MOVE POC-IND-LIQI IN POC TO POC-IND-LIQI IN PO2-FLD.
           MOVE POC-IND-ECDC IN POC TO POC-IND-ECDC IN PO2-FLD.
           MOVE POC-COD-IPVC IN POC TO POC-COD-IPVC IN PO2-FLD.
           MOVE POC-COD-TPAG IN POC TO POC-COD-TPAG IN PO2-FLD.
*% BUF = "PO2-FLD"
*% SIS = "COL"
*% TAB = "MIPL"
*% FLD = "POC-MSC-MIPL"
