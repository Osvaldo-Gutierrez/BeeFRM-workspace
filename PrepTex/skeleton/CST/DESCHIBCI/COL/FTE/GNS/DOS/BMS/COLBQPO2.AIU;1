           PERFORM VAL-NUM-MKY-PO2.
           PERFORM VAL-NUM-AKY-PO2.
       FIN-VAL-NUM-KEY-PO2.
           EXIT.
       
       VAL-NUM-IKY-PO2 SECTION.
       INI-VAL-NUM-IKY-PO2.
*% INCLUDE INI_VAL_NUM_IKY_PO2 OF 'COLBMPO2.TXT'
*% INCLUDE FIN_VAL_NUM_IKY_PO2 OF 'COLBMPO2.TXT'
       FIN-VAL-NUM-IKY-PO2.
           EXIT.
       
       VAL-NUM-MKY-PO2 SECTION.
       INI-VAL-NUM-MKY-PO2.
*% INCLUDE INI_VAL_NUM_MKY_PO2 OF 'COLBMPO2.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_PO2 OF 'COLBMPO2.TXT'
       FIN-VAL-NUM-MKY-PO2.
           EXIT.
       
