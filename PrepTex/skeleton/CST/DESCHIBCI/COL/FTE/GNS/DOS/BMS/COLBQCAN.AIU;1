       FIN-PUT-CYA-CAN.
           EXIT.
       
       KEY-ALL-CAN SECTION.
       INI-KEY-ALL-CAN.
*% INCLUDE INI_KEY_ALL_CAN OF 'COLBMCAN.TXT'
           PERFORM KEY-OPC-CAN.
           PERFORM KEY-CAN-CAN.
           PERFORM KEY-DBC-CAN.
           PERFORM KEY-VEN-CAN.
           PERFORM KEY-RCV-CAN.
           PERFORM KEY-ROV-CAN.
           PERFORM KEY-ICG-CAN.
           PERFORM KEY-COM-CAN.
           PERFORM KEY-CYA-CAN.
*% INCLUDE FIN_KEY_ALL_CAN OF 'COLBMCAN.TXT'
       FIN-KEY-ALL-CAN.
           EXIT.
       
       KEY-OPC-CAN SECTION.
       INI-KEY-OPC-CAN.
*% INCLUDE INI_KEY_OPC_CAN OF 'COLBMCAN.TXT'
*% INCLUDE FIN_KEY_OPC_CAN OF 'COLBMCAN.TXT'
       FIN-KEY-OPC-CAN.
           EXIT.
       
       KEY-CAN-CAN SECTION.
       INI-KEY-CAN-CAN.
*% INCLUDE INI_KEY_CAN_CAN OF 'COLBMCAN.TXT'
           MOVE CAN-CAI-IOPC IN CAN-FLD TO CAN-CAI-IOPC IN CAN.
           MOVE CAN-IIC-IOPC IN CAN-FLD TO CAN-IIC-IOPC IN CAN.
           MOVE CAN-NUM-ICAN IN CAN-FLD TO CAN-NUM-ICAN IN CAN.
*% INCLUDE FIN_KEY_CAN_CAN OF 'COLBMCAN.TXT'
       FIN-KEY-CAN-CAN.
           EXIT.
       
       KEY-DBC-CAN SECTION.
       INI-KEY-DBC-CAN.
*% INCLUDE INI_KEY_DBC_CAN OF 'COLBMCAN.TXT'
*% INCLUDE FIN_KEY_DBC_CAN OF 'COLBMCAN.TXT'
       FIN-KEY-DBC-CAN.
           EXIT.
       
       KEY-VEN-CAN SECTION.
       INI-KEY-VEN-CAN.
*% INCLUDE INI_KEY_VEN_CAN OF 'COLBMCAN.TXT'
           MOVE VEN-NUM-IVCT IN CAN-FLD TO VEN-NUM-IVCT IN VEN.
*% INCLUDE FIN_KEY_VEN_CAN OF 'COLBMCAN.TXT'
       FIN-KEY-VEN-CAN.
           EXIT.
       
