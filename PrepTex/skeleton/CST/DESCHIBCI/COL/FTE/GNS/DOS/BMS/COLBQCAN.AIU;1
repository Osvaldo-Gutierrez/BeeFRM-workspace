*% INCLUDE INI_CLR_VEN_CAN OF 'COLBMCAN.TXT'
*% RREG = "VEN"
*% RREG_FRM = "VEN"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO VEN-FEC-FVEN IN CAN-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "VEN"
*% RREG_FRM = "VEN"
*% NOMB = "FIMV"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO VEN-FEC-FIMV IN CAN-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO VEN-IND-SCTB IN CAN-FLD.
           MOVE ZEROES TO VEN-NUM-IVEN IN CAN-FLD.
           MOVE ZEROES TO VEN-VAL-SCAP IN CAN-FLD.
           MOVE ZEROES TO VEN-VAL-SFIN IN CAN-FLD.
*% INCLUDE FIN_CLR_VEN_CAN OF 'COLBMCAN.TXT'
       FIN-CLR-VEN-CAN.
           EXIT.
       
       CLR-RCV-CAN SECTION.
       INI-CLR-RCV-CAN.
*% INCLUDE INI_CLR_RCV_CAN OF 'COLBMCAN.TXT'
           MOVE SPACES TO RCV-MSC-MCAN IN CAN-FLD.
           MOVE ZEROES TO RCV-SGV-CAPI IN CAN-FLD.
           MOVE ZEROES TO RCV-SGV-FINA IN CAN-FLD.
*% INCLUDE FIN_CLR_RCV_CAN OF 'COLBMCAN.TXT'
       FIN-CLR-RCV-CAN.
           EXIT.
       
       CLR-ROV-CAN SECTION.
       INI-CLR-ROV-CAN.
*% INCLUDE INI_CLR_ROV_CAN OF 'COLBMCAN.TXT'
           MOVE SPACES TO ROV-CAI-IOPC IN CAN-FLD.
           MOVE ZEROES TO ROV-IIC-IOPC IN CAN-FLD.
*% INCLUDE FIN_CLR_ROV_CAN OF 'COLBMCAN.TXT'
       FIN-CLR-ROV-CAN.
           EXIT.
       
       CLR-ICG-CAN SECTION.
       INI-CLR-ICG-CAN.
*% INCLUDE INI_CLR_ICG_CAN OF 'COLBMCAN.TXT'
           MOVE ZEROES TO ICG-SGV-TINT IN CAN-FLD.
*% INCLUDE FIN_CLR_ICG_CAN OF 'COLBMCAN.TXT'
       FIN-CLR-ICG-CAN.
           EXIT.
       
       CLR-COM-CAN SECTION.
       INI-CLR-COM-CAN.
*% INCLUDE INI_CLR_COM_CAN OF 'COLBMCAN.TXT'
           MOVE ZEROES TO COM-SGV-TINT IN CAN-FLD.
*% INCLUDE FIN_CLR_COM_CAN OF 'COLBMCAN.TXT'
       FIN-CLR-COM-CAN.
           EXIT.
       
       CLR-CYA-CAN SECTION.
       INI-CLR-CYA-CAN.
*% INCLUDE INI_CLR_CYA_CAN OF 'COLBMCAN.TXT'
           MOVE SPACES TO CYA-IND-TCYA IN CAN-FLD.
           MOVE SPACES TO CYA-CAI-ICYA IN CAN-FLD.
           MOVE ZEROES TO CYA-IIC-ICYA IN CAN-FLD.
*% INCLUDE FIN_CLR_CYA_CAN OF 'COLBMCAN.TXT'
       FIN-CLR-CYA-CAN.
           EXIT.
       
       FST-KEY-CAN SECTION.
       INI-FST-KEY-CAN.
*% INCLUDE INI_FST_KEY_CAN OF 'COLBMCAN.TXT'
           MOVE -1 TO CAN-CAI-IOPC-LEN IN CAN-FLD.
*% INCLUDE FIN_FST_KEY_CAN OF 'COLBMCAN.TXT'
       FIN-FST-KEY-CAN.
           EXIT.
       
       FST-FLD-CAN SECTION.
       INI-FST-FLD-CAN.
*% INCLUDE INI_FST_FLD_CAN OF 'COLBMCAN.TXT'
           MOVE -1 TO CAN-MSC-CANC-LEN IN CAN-FLD.
*% INCLUDE FIN_FST_FLD_CAN OF 'COLBMCAN.TXT'
       FIN-FST-FLD-CAN.
           EXIT.
       
       PRO-KEY-CAN SECTION.
       INI-PRO-KEY-CAN.
*% INCLUDE INI_PRO_KEY_CAN OF 'COLBMCAN.TXT'
           PERFORM PRO-IKY-CAN.
           PERFORM PRO-AKY-CAN.
           PERFORM PRO-MKY-CAN.
*% INCLUDE FIN_PRO_KEY_CAN OF 'COLBMCAN.TXT'
       FIN-PRO-KEY-CAN.
           EXIT.
       
       PRO-IKY-CAN SECTION.
       INI-PRO-IKY-CAN.
*% INCLUDE INI_PRO_IKY_CAN OF 'COLBMCAN.TXT'
           MOVE FRM-CPIM-AEY TO CAN-CAI-IOPC-ATR IN CAN-FLD.
           MOVE FRM-CPIM-AEY TO CAN-IIC-IOPC-ATR IN CAN-FLD.
           MOVE FRM-CPIM-AEY TO VEN-NUM-IVCT-ATR IN CAN-FLD.
*% INCLUDE FIN_PRO_IKY_CAN OF 'COLBMCAN.TXT'
       FIN-PRO-IKY-CAN.
           EXIT.
       
       PRO-MKY-CAN SECTION.
       INI-PRO-MKY-CAN.
*% INCLUDE INI_PRO_MKY_CAN OF 'COLBMCAN.TXT'
*% INCLUDE FIN_PRO_MKY_CAN OF 'COLBMCAN.TXT'
       FIN-PRO-MKY-CAN.
           EXIT.
       
       PRO-AKY-CAN SECTION.
       INI-PRO-AKY-CAN.
*% INCLUDE INI_PRO_AKY_CAN OF 'COLBMCAN.TXT'
           MOVE FRM-CPIM-AEY TO CAN-NUM-ICAN-ATR IN CAN-FLD.
*% INCLUDE FIN_PRO_AKY_CAN OF 'COLBMCAN.TXT'
       FIN-PRO-AKY-CAN.
           EXIT.
       
       UNP-KEY-CAN SECTION.
       INI-UNP-KEY-CAN.
*% INCLUDE INI_UNP_KEY_CAN OF 'COLBMCAN.TXT'
           PERFORM UNP-IKY-CAN.
           PERFORM UNP-AKY-CAN.
           PERFORM UNP-MKY-CAN.
*% INCLUDE FIN_UNP_KEY_CAN OF 'COLBMCAN.TXT'
       FIN-UNP-KEY-CAN.
           EXIT.
       
       UNP-IKY-CAN SECTION.
       INI-UNP-IKY-CAN.
*% INCLUDE INI_UNP_IKY_CAN OF 'COLBMCAN.TXT'
           MOVE FRM-CPIM-UBY TO CAN-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO CAN-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO VEN-NUM-IVCT-ATR.
*% INCLUDE FIN_UNP_IKY_CAN OF 'COLBMCAN.TXT'
       FIN-UNP-IKY-CAN.
           EXIT.
       
