       INI-UNP-CYA-CAN.
*% INCLUDE INI_UNP_CYA_CAN OF 'COLBMCAN.TXT'
           MOVE FRM-CPIM-UBY TO CYA-IND-TCYA-ATR IN CAN-FLD.
           MOVE FRM-CPIM-UBY TO CYA-CAI-ICYA-ATR IN CAN-FLD.
           MOVE FRM-CPIM-UBY TO CYA-IIC-ICYA-ATR IN CAN-FLD.
*% INCLUDE FIN_UNP_CYA_CAN OF 'COLBMCAN.TXT'
       FIN-UNP-CYA-CAN.
           EXIT.
       
       VAL-KEY-CAN SECTION.
       INI-VAL-KEY-CAN.
           PERFORM VAL-IKY-CAN.
           PERFORM VAL-MKY-CAN.
           PERFORM VAL-AKY-CAN.
       FIN-VAL-KEY-CAN.
           EXIT.
       
       VAL-IKY-CAN SECTION.
       INI-VAL-IKY-CAN.
*% INCLUDE INI_VAL_IKY_CAN OF 'COLBMCAN.TXT'
           PERFORM VAL-CAN-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAN-CAI-IOPC-LEN IN CAN-FLD
                GO TO FIN-VAL-IKY-CAN.
           PERFORM VAL-CAN-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAN-IIC-IOPC-LEN IN CAN-FLD
                GO TO FIN-VAL-IKY-CAN.
           PERFORM VAL-VEN-NUM-IVCT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO VEN-NUM-IVCT-LEN IN CAN-FLD
                GO TO FIN-VAL-IKY-CAN.
*% INCLUDE FIN_VAL_IKY_CAN OF 'COLBMCAN.TXT'
       FIN-VAL-IKY-CAN.
           EXIT.
       
       VAL-MKY-CAN SECTION.
       INI-VAL-MKY-CAN.
*% INCLUDE INI_VAL_MKY_CAN OF 'COLBMCAN.TXT'
*% INCLUDE FIN_VAL_MKY_CAN OF 'COLBMCAN.TXT'
       FIN-VAL-MKY-CAN.
           EXIT.
       
       VAL-AKY-CAN SECTION.
       INI-VAL-AKY-CAN.
*% INCLUDE INI_VAL_AKY_CAN OF 'COLBMCAN.TXT'
           PERFORM VAL-CAN-NUM-ICAN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAN-NUM-ICAN-LEN IN CAN-FLD
                GO TO FIN-VAL-AKY-CAN.
*% INCLUDE FIN_VAL_AKY_CAN OF 'COLBMCAN.TXT'
       FIN-VAL-AKY-CAN.
           EXIT.
       
       VAL-CAN-CAI-IOPC SECTION.
       INI-VAL-CAN-CAI-IOPC.
*% LBL = "CAN_CAI_IOPC OF 'COLBMCAN.TXT'"
*% INCLUDE INI_CAN_CAI_IOPC OF 'COLBMCAN.TXT'
           IF CAN-CAI-IOPC IN CAN-FLD NOT > SPACES
              MOVE SPACES TO CAN-CAI-IOPC IN CAN-FLD.
*% INCLUDE CAN_CAI_IOPC OF 'COLBMCAN.TXT'
*% INCLUDE FIN_CAN_CAI_IOPC OF 'COLBMCAN.TXT'
       FIN-VAL-CAN-CAI-IOPC.
           EXIT.
       
       VAL-CAN-IIC-IOPC SECTION.
       INI-VAL-CAN-IIC-IOPC.
*% LBL = "CAN_IIC_IOPC OF 'COLBMCAN.TXT'"
