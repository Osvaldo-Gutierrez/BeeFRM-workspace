       INI-PRO-OPC-TEF.
*% INCLUDE INI_PRO_OPC_TEF OF 'COLBMTEF.TXT'
*% INCLUDE FIN_PRO_OPC_TEF OF 'COLBMTEF.TXT'
       FIN-PRO-OPC-TEF.
           EXIT.
       
       PRO-ICG-TEF SECTION.
       INI-PRO-ICG-TEF.
*% INCLUDE INI_PRO_ICG_TEF OF 'COLBMTEF.TXT'
           MOVE FRM-CPIM-AEY TO ICG-MSC-BCTI-ATR IN TEF-FLD.
           MOVE FRM-CPIM-AEY TO ICG-MSC-PBTI-ATR IN TEF-FLD.
           MOVE FRM-CPIM-AEY TO ICG-MSC-BPBT-ATR IN TEF-FLD.
           MOVE FRM-CPIM-AEY TO ICG-MSC-SPBT-ATR IN TEF-FLD.
           MOVE FRM-CPIM-AEY TO ICG-COD-CVTI-ATR IN TEF-FLD.
           MOVE FRM-CPIM-AEY TO ICG-MSC-TFPR-ATR IN TEF-FLD.
           MOVE FRM-CPIM-AEY TO ICG-NUM-PREP-ATR IN TEF-FLD.
           MOVE FRM-CPIM-AEY TO ICG-MSC-UTPR-ATR IN TEF-FLD.
*% RREG = "ICG"
*% RREG_FRM = "ICG"
*% NOMB = "FTER"
           MOVE FRM-CPIM-AEY TO ICG-FEC-FTER-ATR IN TEF-FLD.
           MOVE FRM-CPIM-AEY TO ICG-SGV-SPRD-ATR IN TEF-FLD.
           MOVE FRM-CPIM-AEY TO ICG-SGV-TINT-ATR IN TEF-FLD.
*% INCLUDE FIN_PRO_ICG_TEF OF 'COLBMTEF.TXT'
       FIN-PRO-ICG-TEF.
           EXIT.
       
       UNP-KEY-TEF SECTION.
       INI-UNP-KEY-TEF.
*% INCLUDE INI_UNP_KEY_TEF OF 'COLBMTEF.TXT'
           PERFORM UNP-IKY-TEF.
           PERFORM UNP-AKY-TEF.
           PERFORM UNP-MKY-TEF.
*% INCLUDE FIN_UNP_KEY_TEF OF 'COLBMTEF.TXT'
       FIN-UNP-KEY-TEF.
           EXIT.
       
       UNP-IKY-TEF SECTION.
       INI-UNP-IKY-TEF.
*% INCLUDE INI_UNP_IKY_TEF OF 'COLBMTEF.TXT'
           MOVE FRM-CPIM-UBY TO ICG-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO ICG-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO ICG-NUM-ICAN-ATR.
           MOVE FRM-CPIM-UBY TO ICG-NUM-IVCT-ATR.
*% RREG = "ICG"
*% RREG_FRM = "ICG"
*% NOMB = "FINI"
           MOVE FRM-CPIM-UBY TO ICG-FEC-FINI-ATR IN TEF-FLD.
           MOVE FRM-CPIM-UBY TO ICG-CAI-IOPV-ATR.
           MOVE FRM-CPIM-UBY TO ICG-IIC-IOPV-ATR.
           MOVE FRM-CPIM-UBY TO ICG-IND-TTAS-ATR.
*% INCLUDE FIN_UNP_IKY_TEF OF 'COLBMTEF.TXT'
       FIN-UNP-IKY-TEF.
           EXIT.
       
       UNP-MKY-TEF SECTION.
       INI-UNP-MKY-TEF.
*% INCLUDE INI_UNP_MKY_TEF OF 'COLBMTEF.TXT'
*% INCLUDE FIN_UNP_MKY_TEF OF 'COLBMTEF.TXT'
       FIN-UNP-MKY-TEF.
           EXIT.
       
       UNP-AKY-TEF SECTION.
       INI-UNP-AKY-TEF.
*% INCLUDE INI_UNP_AKY_TEF OF 'COLBMTEF.TXT'
*% INCLUDE FIN_UNP_AKY_TEF OF 'COLBMTEF.TXT'
       FIN-UNP-AKY-TEF.
           EXIT.
       
       UNP-FLD-TEF SECTION.
       INI-UNP-FLD-TEF.
*% INCLUDE INI_UNP_FLD_TEF OF 'COLBMTEF.TXT'
           PERFORM UNP-OPC-TEF.
           PERFORM UNP-ICG-TEF.
*% INCLUDE FIN_UNP_FLD_TEF OF 'COLBMTEF.TXT'
