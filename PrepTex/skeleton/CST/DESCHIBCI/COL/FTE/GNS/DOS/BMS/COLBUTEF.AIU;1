       INI-UNP-OPC-TEF.
*% INCLUDE INI_UNP_OPC_TEF OF 'COLBMTEF.TXT'
*% INCLUDE FIN_UNP_OPC_TEF OF 'COLBMTEF.TXT'
       FIN-UNP-OPC-TEF.
           EXIT.
       
       UNP-ICG-TEF SECTION.
       INI-UNP-ICG-TEF.
*% INCLUDE INI_UNP_ICG_TEF OF 'COLBMTEF.TXT'
           MOVE FRM-CPIM-UBY TO ICG-MSC-BCTI-ATR IN TEF-FLD.
           MOVE FRM-CPIM-UBY TO ICG-MSC-PBTI-ATR IN TEF-FLD.
           MOVE FRM-CPIM-UBY TO ICG-MSC-BPBT-ATR IN TEF-FLD.
           MOVE FRM-CPIM-UBY TO ICG-MSC-SPBT-ATR IN TEF-FLD.
           MOVE FRM-CPIM-UBY TO ICG-COD-CVTI-ATR IN TEF-FLD.
           MOVE FRM-CPIM-UBY TO ICG-MSC-TFPR-ATR IN TEF-FLD.
           MOVE FRM-CPIM-UBY TO ICG-NUM-PREP-ATR IN TEF-FLD.
           MOVE FRM-CPIM-UBY TO ICG-MSC-UTPR-ATR IN TEF-FLD.
*% RREG = "ICG"
*% RREG_FRM = "ICG"
*% NOMB = "FTER"
           MOVE FRM-CPIM-UBY TO ICG-FEC-FTER-ATR IN TEF-FLD.
           MOVE FRM-CPIM-UBY TO ICG-SGV-SPRD-ATR IN TEF-FLD.
           MOVE FRM-CPIM-UBY TO ICG-SGV-TINT-ATR IN TEF-FLD.
*% INCLUDE FIN_UNP_ICG_TEF OF 'COLBMTEF.TXT'
       FIN-UNP-ICG-TEF.
           EXIT.
       
       VAL-MKY-TEF SECTION.
       INI-VAL-MKY-TEF.
*% INCLUDE INI_VAL_MKY_TEF OF 'COLBMTEF.TXT'
*% INCLUDE FIN_VAL_MKY_TEF OF 'COLBMTEF.TXT'
       FIN-VAL-MKY-TEF.
           EXIT.
       
       VAL-FLD-TEF SECTION.
       INI-VAL-FLD-TEF.
*% INCLUDE INI_VAL_FLD_TEF OF 'COLBMTEF.TXT'
           PERFORM VAL-OPC-MSC-TVOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-MSC-TVOC-LEN IN TEF-FLD
                GO TO FIN-VAL-FLD-TEF.
           PERFORM VAL-OPC-COD-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-TOOC-LEN IN TEF-FLD
                GO TO FIN-VAL-FLD-TEF.
           PERFORM VAL-OPC-COD-OFOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-OFOC-LEN IN TEF-FLD
                GO TO FIN-VAL-FLD-TEF.
           PERFORM VAL-FRM-GLS-TVOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TVOC-LEN IN TEF-FLD
                GO TO FIN-VAL-FLD-TEF.
           PERFORM VAL-FRM-GLS-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TOOC-LEN IN TEF-FLD
                GO TO FIN-VAL-FLD-TEF.
           PERFORM VAL-FRM-GLS-OFOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-OFOC-LEN IN TEF-FLD
                GO TO FIN-VAL-FLD-TEF.
           PERFORM VAL-ICG-MSC-BCTI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO ICG-MSC-BCTI-LEN IN TEF-FLD
                GO TO FIN-VAL-FLD-TEF.
           PERFORM VAL-ICG-MSC-PBTI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO ICG-MSC-PBTI-LEN IN TEF-FLD
                GO TO FIN-VAL-FLD-TEF.
           PERFORM VAL-FRM-GLS-BCTI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-BCTI-LEN IN TEF-FLD
                GO TO FIN-VAL-FLD-TEF.
           PERFORM VAL-FRM-GLS-PBTI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-PBTI-LEN IN TEF-FLD
                GO TO FIN-VAL-FLD-TEF.
           PERFORM VAL-ICG-MSC-BPBT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO ICG-MSC-BPBT-LEN IN TEF-FLD
                GO TO FIN-VAL-FLD-TEF.
           PERFORM VAL-ICG-MSC-SPBT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO ICG-MSC-SPBT-LEN IN TEF-FLD
                GO TO FIN-VAL-FLD-TEF.
           PERFORM VAL-FRM-GLS-BPBT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-BPBT-LEN IN TEF-FLD
                GO TO FIN-VAL-FLD-TEF.
           PERFORM VAL-FRM-GLS-SPBT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-SPBT-LEN IN TEF-FLD
                GO TO FIN-VAL-FLD-TEF.
           PERFORM VAL-ICG-COD-CVTI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO ICG-COD-CVTI-LEN IN TEF-FLD
                GO TO FIN-VAL-FLD-TEF.
           PERFORM VAL-ICG-MSC-TFPR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO ICG-MSC-TFPR-LEN IN TEF-FLD
                GO TO FIN-VAL-FLD-TEF.
           PERFORM VAL-FRM-GLS-CVTI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-CVTI-LEN IN TEF-FLD
                GO TO FIN-VAL-FLD-TEF.
           PERFORM VAL-FRM-GLS-TFPR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TFPR-LEN IN TEF-FLD
                GO TO FIN-VAL-FLD-TEF.
           PERFORM VAL-ICG-NUM-PREP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO ICG-NUM-PREP-LEN IN TEF-FLD
                GO TO FIN-VAL-FLD-TEF.
           PERFORM VAL-ICG-MSC-UTPR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO ICG-MSC-UTPR-LEN IN TEF-FLD
                GO TO FIN-VAL-FLD-TEF.
           PERFORM VAL-FRM-GLS-UTPR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-UTPR-LEN IN TEF-FLD
                GO TO FIN-VAL-FLD-TEF.
*% RREG = "ICG"
*% RREG_FRM = "ICG"
*% NOMB = "FTER"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-ICG-FEC-FTER.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                ICG-FEC-FTER-LEN IN TEF-FLD
                GO TO FIN-VAL-FLD-TEF.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-ICG-SGV-SPRD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO ICG-SGV-SPRD-LEN IN TEF-FLD
                GO TO FIN-VAL-FLD-TEF.
           PERFORM VAL-ICG-SGV-TINT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO ICG-SGV-TINT-LEN IN TEF-FLD
                GO TO FIN-VAL-FLD-TEF.
*% INCLUDE FIN_VAL_FLD_TEF OF 'COLBMTEF.TXT'
       FIN-VAL-FLD-TEF.
           EXIT.
       
