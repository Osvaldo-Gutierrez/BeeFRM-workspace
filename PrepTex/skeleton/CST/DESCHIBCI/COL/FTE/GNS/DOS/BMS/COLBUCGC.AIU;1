*% FRM = "CGC"
      *
      * COLBUCGC
      * Version 3.0.1 Nov-2005
      * 13-Mar-14 11:20 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMCGC.TXT'
       SET-TAG-COLBUCGC SECTION.
       INI-SET-TAG-COLBUCGC.
           MOVE
           '<IDNMOD NOM=$COLBUCGC$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$13-Mar-14 11:20 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCGC.
           EXIT.
           
       RET-ALL-CGC SECTION.
       INI-RET-ALL-CGC.
*% INCLUDE INI_RET_ALL_CGC OF 'COLBMCGC.TXT'
           PERFORM KEY-ALL-CGC.
           PERFORM RET-CGC-CGC.
*% INCLUDE FIN_RET_ALL_CGC OF 'COLBMCGC.TXT'
       FIN-RET-ALL-CGC.
           EXIT.
       
       RET-CGC-CGC SECTION.
       INI-RET-CGC-CGC.
*% INCLUDE INI_RET_CGC_CGC OF 'COLBMCGC.TXT'
           MOVE CGC-CIC-ICLI IN CGC-FLD TO CGC-CIC-ICLI IN CGC.
           MOVE CGC-GLS-KEY3 IN CGC-FLD TO CGC-GLS-KEY3 IN CGC.
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "KEY4"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CGC-NUM-DEY4 IN CGC-FLD TO CGC-NUM-DEY4 IN CGC.
           MOVE CGC-NUM-MEY4 IN CGC-FLD TO CGC-NUM-MEY4 IN CGC.
           MOVE CGC-NUM-SEY4 IN CGC-FLD TO CGC-NUM-SEY4 IN CGC.
           MOVE CGC-NUM-AEY4 IN CGC-FLD TO CGC-NUM-AEY4 IN CGC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CGC-GLS-KEY5 IN CGC-FLD TO CGC-GLS-KEY5 IN CGC.
           MOVE CGC-GLS-KEY6 IN CGC-FLD TO CGC-GLS-KEY6 IN CGC.
           MOVE CGC-GLS-KEY8 IN CGC-FLD TO CGC-GLS-KEY8 IN CGC.
           MOVE CGC-SGV-MON1 IN CGC-FLD TO CGC-SGV-MON1 IN CGC.
           MOVE CGC-SGV-MON2 IN CGC-FLD TO CGC-SGV-MON2 IN CGC.
           MOVE CGC-SGV-MON3 IN CGC-FLD TO CGC-SGV-MON3 IN CGC.
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "AUX4"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CGC-NUM-DUX4 IN CGC-FLD TO CGC-NUM-DUX4 IN CGC.
           MOVE CGC-NUM-MUX4 IN CGC-FLD TO CGC-NUM-MUX4 IN CGC.
           MOVE CGC-NUM-SUX4 IN CGC-FLD TO CGC-NUM-SUX4 IN CGC.
           MOVE CGC-NUM-AUX4 IN CGC-FLD TO CGC-NUM-AUX4 IN CGC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "AUX5"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CGC-NUM-DUX5 IN CGC-FLD TO CGC-NUM-DUX5 IN CGC.
           MOVE CGC-NUM-MUX5 IN CGC-FLD TO CGC-NUM-MUX5 IN CGC.
           MOVE CGC-NUM-SUX5 IN CGC-FLD TO CGC-NUM-SUX5 IN CGC.
           MOVE CGC-NUM-AUX5 IN CGC-FLD TO CGC-NUM-AUX5 IN CGC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CGC-GLS-AUX4 IN CGC-FLD TO CGC-GLS-AUX4 IN CGC.
           MOVE CGC-NUM-COR1 IN CGC-FLD TO CGC-NUM-COR1 IN CGC.
*% INCLUDE FIN_RET_CGC_CGC OF 'COLBMCGC.TXT'
       FIN-RET-CGC-CGC.
           EXIT.
       
       KEY-ALL-CGC SECTION.
       INI-KEY-ALL-CGC.
*% INCLUDE INI_KEY_ALL_CGC OF 'COLBMCGC.TXT'
           PERFORM KEY-CGC-CGC.
*% INCLUDE FIN_KEY_ALL_CGC OF 'COLBMCGC.TXT'
       FIN-KEY-ALL-CGC.
           EXIT.
       
       KEY-CGC-CGC SECTION.
       INI-KEY-CGC-CGC.
*% INCLUDE INI_KEY_CGC_CGC OF 'COLBMCGC.TXT'
           MOVE CGC-GLS-CPTO IN CGC-FLD TO CGC-GLS-CPTO IN CGC.
           MOVE CGC-CIC-KEY1 IN CGC-FLD TO CGC-CIC-KEY1 IN CGC.
           MOVE CGC-NUM-KEY1 IN CGC-FLD TO CGC-NUM-KEY1 IN CGC.
*% INCLUDE FIN_KEY_CGC_CGC OF 'COLBMCGC.TXT'
       FIN-KEY-CGC-CGC.
           EXIT.
       
       SCR-CLRA-CGC SECTION.
       INI-SCR-CLRA-CGC.
*% INCLUDE INI_SCR_CLRA_CGC OF 'COLBMCGC.TXT'
           PERFORM CLR-KEY-CGC.
           PERFORM CLR-FLD-CGC.
*% INCLUDE FIN_SCR_CLRA_CGC OF 'COLBMCGC.TXT'
       FIN-SCR-CLRA-CGC.
           EXIT.
       
       CLR-KEY-CGC SECTION.
       INI-CLR-KEY-CGC.
*% INCLUDE INI_CLR_KEY_CGC OF 'COLBMCGC.TXT'
           MOVE SPACES TO CGC-GLS-CPTO IN CGC-FLD.
           MOVE SPACES TO CGC-CIC-KEY1 IN CGC-FLD.
           MOVE ZEROES TO CGC-NUM-KEY1 IN CGC-FLD.
*% INCLUDE FIN_CLR_KEY_CGC OF 'COLBMCGC.TXT'
       FIN-CLR-KEY-CGC.
           EXIT.
       
       CLR-FLD-CGC SECTION.
       INI-CLR-FLD-CGC.
*% INCLUDE INI_CLR_FLD_CGC OF 'COLBMCGC.TXT'
           PERFORM CLR-CGC-CGC.
*% INCLUDE FIN_CLR_FLD_CGC OF 'COLBMCGC.TXT'
       FIN-CLR-FLD-CGC.
           EXIT.
       
       CLR-CGC-CGC SECTION.
       INI-CLR-CGC-CGC.
*% INCLUDE INI_CLR_CGC_CGC OF 'COLBMCGC.TXT'
           MOVE SPACES TO CGC-CIC-ICLI IN CGC-FLD.
           MOVE SPACES TO CGC-GLS-KEY3 IN CGC-FLD.
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "KEY4"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CGC-FEC-KEY4 IN CGC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO CGC-GLS-KEY5 IN CGC-FLD.
           MOVE SPACES TO CGC-GLS-KEY6 IN CGC-FLD.
           MOVE SPACES TO CGC-GLS-KEY8 IN CGC-FLD.
           MOVE ZEROES TO CGC-SGV-MON1 IN CGC-FLD.
           MOVE ZEROES TO CGC-SGV-MON2 IN CGC-FLD.
           MOVE ZEROES TO CGC-SGV-MON3 IN CGC-FLD.
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "AUX4"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CGC-FEC-AUX4 IN CGC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "AUX5"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CGC-FEC-AUX5 IN CGC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO CGC-GLS-AUX4 IN CGC-FLD.
           MOVE ZEROES TO CGC-NUM-COR1 IN CGC-FLD.
*% INCLUDE FIN_CLR_CGC_CGC OF 'COLBMCGC.TXT'
       FIN-CLR-CGC-CGC.
           EXIT.
       
       FST-KEY-CGC SECTION.
       INI-FST-KEY-CGC.
*% INCLUDE INI_FST_KEY_CGC OF 'COLBMCGC.TXT'
           MOVE -1 TO CGC-GLS-CPTO-LEN IN CGC-FLD.
*% INCLUDE FIN_FST_KEY_CGC OF 'COLBMCGC.TXT'
       FIN-FST-KEY-CGC.
           EXIT.
       
       FST-FLD-CGC SECTION.
       INI-FST-FLD-CGC.
*% INCLUDE INI_FST_FLD_CGC OF 'COLBMCGC.TXT'
           MOVE -1 TO CGC-CIC-ICLI-LEN IN CGC-FLD.
*% INCLUDE FIN_FST_FLD_CGC OF 'COLBMCGC.TXT'
       FIN-FST-FLD-CGC.
           EXIT.
       
       PRO-KEY-CGC SECTION.
       INI-PRO-KEY-CGC.
*% INCLUDE INI_PRO_KEY_CGC OF 'COLBMCGC.TXT'
           PERFORM PRO-MKY-CGC.
*% INCLUDE FIN_PRO_KEY_CGC OF 'COLBMCGC.TXT'
       FIN-PRO-KEY-CGC.
           EXIT.
       
       PRO-MKY-CGC SECTION.
       INI-PRO-MKY-CGC.
*% INCLUDE INI_PRO_MKY_CGC OF 'COLBMCGC.TXT'
*% INCLUDE FIN_PRO_MKY_CGC OF 'COLBMCGC.TXT'
       FIN-PRO-MKY-CGC.
           EXIT.
       
       PRO-FLD-CGC SECTION.
       INI-PRO-FLD-CGC.
*% INCLUDE INI_PRO_FLD_CGC OF 'COLBMCGC.TXT'
           PERFORM PRO-CGC-CGC.
*% INCLUDE FIN_PRO_FLD_CGC OF 'COLBMCGC.TXT'
       FIN-PRO-FLD-CGC.
           EXIT.
       
       PRO-CGC-CGC SECTION.
       INI-PRO-CGC-CGC.
*% INCLUDE INI_PRO_CGC_CGC OF 'COLBMCGC.TXT'
           MOVE FRM-CPIM-AEY TO CGC-CIC-ICLI-ATR IN CGC-FLD.
           MOVE FRM-CPIM-AEY TO CGC-GLS-KEY3-ATR IN CGC-FLD.
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "KEY4"
           MOVE FRM-CPIM-AEY TO CGC-FEC-KEY4-ATR IN CGC-FLD.
           MOVE FRM-CPIM-AEY TO CGC-GLS-KEY5-ATR IN CGC-FLD.
           MOVE FRM-CPIM-AEY TO CGC-GLS-KEY6-ATR IN CGC-FLD.
           MOVE FRM-CPIM-AEY TO CGC-GLS-KEY8-ATR IN CGC-FLD.
           MOVE FRM-CPIM-AEY TO CGC-SGV-MON1-ATR IN CGC-FLD.
           MOVE FRM-CPIM-AEY TO CGC-SGV-MON2-ATR IN CGC-FLD.
           MOVE FRM-CPIM-AEY TO CGC-SGV-MON3-ATR IN CGC-FLD.
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "AUX4"
           MOVE FRM-CPIM-AEY TO CGC-FEC-AUX4-ATR IN CGC-FLD.
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "AUX5"
           MOVE FRM-CPIM-AEY TO CGC-FEC-AUX5-ATR IN CGC-FLD.
           MOVE FRM-CPIM-AEY TO CGC-GLS-AUX4-ATR IN CGC-FLD.
           MOVE FRM-CPIM-AEY TO CGC-NUM-COR1-ATR IN CGC-FLD.
*% INCLUDE FIN_PRO_CGC_CGC OF 'COLBMCGC.TXT'
       FIN-PRO-CGC-CGC.
           EXIT.
       
       UNP-KEY-CGC SECTION.
       INI-UNP-KEY-CGC.
*% INCLUDE INI_UNP_KEY_CGC OF 'COLBMCGC.TXT'
           PERFORM UNP-IKY-CGC.
           PERFORM UNP-AKY-CGC.
           PERFORM UNP-MKY-CGC.
*% INCLUDE FIN_UNP_KEY_CGC OF 'COLBMCGC.TXT'
       FIN-UNP-KEY-CGC.
           EXIT.
       
       UNP-IKY-CGC SECTION.
       INI-UNP-IKY-CGC.
*% INCLUDE INI_UNP_IKY_CGC OF 'COLBMCGC.TXT'
           MOVE FRM-CPIM-UBY TO CGC-GLS-CPTO-ATR.
           MOVE FRM-CPIM-UBY TO CGC-CIC-KEY1-ATR.
           MOVE FRM-CPIM-UBY TO CGC-NUM-KEY1-ATR.
*% INCLUDE FIN_UNP_IKY_CGC OF 'COLBMCGC.TXT'
       FIN-UNP-IKY-CGC.
           EXIT.
       
       UNP-MKY-CGC SECTION.
       INI-UNP-MKY-CGC.
*% INCLUDE INI_UNP_MKY_CGC OF 'COLBMCGC.TXT'
*% INCLUDE FIN_UNP_MKY_CGC OF 'COLBMCGC.TXT'
       FIN-UNP-MKY-CGC.
           EXIT.
       
       UNP-AKY-CGC SECTION.
       INI-UNP-AKY-CGC.
*% INCLUDE INI_UNP_AKY_CGC OF 'COLBMCGC.TXT'
*% INCLUDE FIN_UNP_AKY_CGC OF 'COLBMCGC.TXT'
       FIN-UNP-AKY-CGC.
           EXIT.
       
       UNP-FLD-CGC SECTION.
       INI-UNP-FLD-CGC.
*% INCLUDE INI_UNP_FLD_CGC OF 'COLBMCGC.TXT'
           PERFORM UNP-CGC-CGC.
*% INCLUDE FIN_UNP_FLD_CGC OF 'COLBMCGC.TXT'
       FIN-UNP-FLD-CGC.
           EXIT.
       
       UNP-CGC-CGC SECTION.
       INI-UNP-CGC-CGC.
*% INCLUDE INI_UNP_CGC_CGC OF 'COLBMCGC.TXT'
           MOVE FRM-CPIM-UBY TO CGC-CIC-ICLI-ATR IN CGC-FLD.
           MOVE FRM-CPIM-UBY TO CGC-GLS-KEY3-ATR IN CGC-FLD.
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "KEY4"
           MOVE FRM-CPIM-UBY TO CGC-FEC-KEY4-ATR IN CGC-FLD.
           MOVE FRM-CPIM-UBY TO CGC-GLS-KEY5-ATR IN CGC-FLD.
           MOVE FRM-CPIM-UBY TO CGC-GLS-KEY6-ATR IN CGC-FLD.
           MOVE FRM-CPIM-UBY TO CGC-GLS-KEY8-ATR IN CGC-FLD.
           MOVE FRM-CPIM-UBY TO CGC-SGV-MON1-ATR IN CGC-FLD.
           MOVE FRM-CPIM-UBY TO CGC-SGV-MON2-ATR IN CGC-FLD.
           MOVE FRM-CPIM-UBY TO CGC-SGV-MON3-ATR IN CGC-FLD.
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "AUX4"
           MOVE FRM-CPIM-UBY TO CGC-FEC-AUX4-ATR IN CGC-FLD.
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "AUX5"
           MOVE FRM-CPIM-UBY TO CGC-FEC-AUX5-ATR IN CGC-FLD.
           MOVE FRM-CPIM-UBY TO CGC-GLS-AUX4-ATR IN CGC-FLD.
           MOVE FRM-CPIM-UBY TO CGC-NUM-COR1-ATR IN CGC-FLD.
*% INCLUDE FIN_UNP_CGC_CGC OF 'COLBMCGC.TXT'
       FIN-UNP-CGC-CGC.
           EXIT.
       
       VAL-MKY-CGC SECTION.
       INI-VAL-MKY-CGC.
*% INCLUDE INI_VAL_MKY_CGC OF 'COLBMCGC.TXT'
*% INCLUDE FIN_VAL_MKY_CGC OF 'COLBMCGC.TXT'
       FIN-VAL-MKY-CGC.
           EXIT.
       
       VAL-FLD-CGC SECTION.
       INI-VAL-FLD-CGC.
*% INCLUDE INI_VAL_FLD_CGC OF 'COLBMCGC.TXT'
           PERFORM VAL-CGC-CIC-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CGC-CIC-ICLI-LEN IN CGC-FLD
                GO TO FIN-VAL-FLD-CGC.
           PERFORM VAL-CGC-GLS-KEY3.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CGC-GLS-KEY3-LEN IN CGC-FLD
                GO TO FIN-VAL-FLD-CGC.
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "KEY4"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-CGC-FEC-KEY4.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                CGC-FEC-KEY4-LEN IN CGC-FLD
                GO TO FIN-VAL-FLD-CGC.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-CGC-GLS-KEY5.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CGC-GLS-KEY5-LEN IN CGC-FLD
                GO TO FIN-VAL-FLD-CGC.
           PERFORM VAL-CGC-GLS-KEY6.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CGC-GLS-KEY6-LEN IN CGC-FLD
                GO TO FIN-VAL-FLD-CGC.
           PERFORM VAL-CGC-GLS-KEY8.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CGC-GLS-KEY8-LEN IN CGC-FLD
                GO TO FIN-VAL-FLD-CGC.
           PERFORM VAL-CGC-SGV-MON1.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CGC-SGV-MON1-LEN IN CGC-FLD
                GO TO FIN-VAL-FLD-CGC.
           PERFORM VAL-CGC-SGV-MON2.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CGC-SGV-MON2-LEN IN CGC-FLD
                GO TO FIN-VAL-FLD-CGC.
           PERFORM VAL-CGC-SGV-MON3.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CGC-SGV-MON3-LEN IN CGC-FLD
                GO TO FIN-VAL-FLD-CGC.
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "AUX4"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-CGC-FEC-AUX4.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                CGC-FEC-AUX4-LEN IN CGC-FLD
                GO TO FIN-VAL-FLD-CGC.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "AUX5"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-CGC-FEC-AUX5.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                CGC-FEC-AUX5-LEN IN CGC-FLD
                GO TO FIN-VAL-FLD-CGC.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-CGC-GLS-AUX4.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CGC-GLS-AUX4-LEN IN CGC-FLD
                GO TO FIN-VAL-FLD-CGC.
           PERFORM VAL-CGC-NUM-COR1.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CGC-NUM-COR1-LEN IN CGC-FLD
                GO TO FIN-VAL-FLD-CGC.
*% INCLUDE FIN_VAL_FLD_CGC OF 'COLBMCGC.TXT'
       FIN-VAL-FLD-CGC.
           EXIT.
       
       VAL-CGC-CIC-ICLI SECTION.
       INI-VAL-CGC-CIC-ICLI.
*% LBL = "CGC_CIC_ICLI OF 'COLBMCGC.TXT'"
*% INCLUDE INI_CGC_CIC_ICLI OF 'COLBMCGC.TXT'
           IF CGC-CIC-ICLI IN CGC-FLD NOT > SPACES
              MOVE SPACES TO CGC-CIC-ICLI IN CGC-FLD.
*% INCLUDE CGC_CIC_ICLI OF 'COLBMCGC.TXT'
*% INCLUDE FIN_CGC_CIC_ICLI OF 'COLBMCGC.TXT'
       FIN-VAL-CGC-CIC-ICLI.
           EXIT.
       
       VAL-CGC-GLS-KEY3 SECTION.
       INI-VAL-CGC-GLS-KEY3.
*% LBL = "CGC_GLS_KEY3 OF 'COLBMCGC.TXT'"
*% INCLUDE INI_CGC_GLS_KEY3 OF 'COLBMCGC.TXT'
           IF CGC-GLS-KEY3 IN CGC-FLD NOT > SPACES
              MOVE SPACES TO CGC-GLS-KEY3 IN CGC-FLD.
*% INCLUDE CGC_GLS_KEY3 OF 'COLBMCGC.TXT'
*% INCLUDE FIN_CGC_GLS_KEY3 OF 'COLBMCGC.TXT'
       FIN-VAL-CGC-GLS-KEY3.
           EXIT.
*% RREG = "CGC"
*% RREG_FRM = "CGC"
*% NOMB = "KEY4"
*% OBL = "S"
*% LBL = "CGC_FEC_KEY4 OF 'COLBMCGC.TXT'"
*% IZR = "S"
*% ZRO = "S"
*% BUF = "CGC-FLD"
*% FLD = "CGC-FEC-KEY4"
*% SN  = "S"
*% HOY = "LE"
*% HBL = "S"
*% TIPO = "FEC"
*% FRM_IFLD = ""
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-CGC-FEC-KEY4 SECTION.
       INI-VAL-CGC-FEC-KEY4.
           IF CGC-FEC-KEY4 IN CGC-FLD  NOT > ZEROES
              MOVE ZEROES TO CGC-FEC-KEY4 IN CGC-FLD.
*% INCLUDE 'SETFEC'
*% INCLUDE INI_CGC_FEC_KEY4 OF 'COLBMCGC.TXT'
*% INCLUDE 'PROFEC'
*% INCLUDE CGC_FEC_KEY4 OF 'COLBMCGC.TXT'
*% INCLUDE FIN_CGC_FEC_KEY4 OF 'COLBMCGC.TXT'
       FIN-VAL-CGC-FEC-KEY4.
           EXIT.
      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-CGC-GLS-KEY5 SECTION.
       INI-VAL-CGC-GLS-KEY5.
*% LBL = "CGC_GLS_KEY5 OF 'COLBMCGC.TXT'"
*% INCLUDE INI_CGC_GLS_KEY5 OF 'COLBMCGC.TXT'
           IF CGC-GLS-KEY5 IN CGC-FLD NOT > SPACES
              MOVE SPACES TO CGC-GLS-KEY5 IN CGC-FLD.
*% INCLUDE CGC_GLS_KEY5 OF 'COLBMCGC.TXT'
*% INCLUDE FIN_CGC_GLS_KEY5 OF 'COLBMCGC.TXT'
       FIN-VAL-CGC-GLS-KEY5.
           EXIT.
       
       VAL-CGC-GLS-KEY6 SECTION.
       INI-VAL-CGC-GLS-KEY6.
*% LBL = "CGC_GLS_KEY6 OF 'COLBMCGC.TXT'"
*% INCLUDE INI_CGC_GLS_KEY6 OF 'COLBMCGC.TXT'
           IF CGC-GLS-KEY6 IN CGC-FLD NOT > SPACES
              MOVE SPACES TO CGC-GLS-KEY6 IN CGC-FLD.
*% INCLUDE CGC_GLS_KEY6 OF 'COLBMCGC.TXT'
*% INCLUDE FIN_CGC_GLS_KEY6 OF 'COLBMCGC.TXT'
       FIN-VAL-CGC-GLS-KEY6.
           EXIT.
       
       VAL-CGC-GLS-KEY8 SECTION.
       INI-VAL-CGC-GLS-KEY8.
*% LBL = "CGC_GLS_KEY8 OF 'COLBMCGC.TXT'"
*% INCLUDE INI_CGC_GLS_KEY8 OF 'COLBMCGC.TXT'
