      *
      * COLBQLV1
      * Version 3.0.1 Nov-2005
      * 17-Nov-11 05:38 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "LV1"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMLV1.TXT'
       SET-TAG-COLBQLV1 SECTION.
       INI-SET-TAG-COLBQLV1.
           MOVE
           '<IDNMOD NOM=$COLBQLV1$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$17-Nov-11 05:38 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQLV1.
           EXIT.
           
       PUT-ALL-LV1 SECTION.
       INI-PUT-ALL-LV1.
*% INCLUDE INI_PUT_ALL_LV1 OF 'COLBMLV1.TXT'
           PERFORM PUT-LAP-LV1.
           PERFORM PUT-PVC-LV1.
*% INCLUDE FIN_PUT_ALL_LV1 OF 'COLBMLV1.TXT'
       FIN-PUT-ALL-LV1.
           EXIT.
       
       PUT-LAP-LV1 SECTION.
       INI-PUT-LAP-LV1.
*% INCLUDE INI_PUT_LAP_LV1 OF 'COLBMLV1.TXT'
           IF PGM-STAT-LAP-NUL
               GO TO FIN-PUT-LAP-LV1.
           IF NOT PGM-STAT-LAP-OKS
               PERFORM CLR-LAP-LV1
               GO TO FIN-PUT-LAP-LV1.
*% RREG = "LAP"
*% RREG_FRM = "LAP"
*% NOMB = "FLAP"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE LAP-NUM-DLAP IN LAP TO LAP-NUM-DLAP IN LV1-FLD.
           MOVE LAP-NUM-MLAP IN LAP TO LAP-NUM-MLAP IN LV1-FLD.
           MOVE LAP-NUM-SLAP IN LAP TO LAP-NUM-SLAP IN LV1-FLD.
           MOVE LAP-NUM-ALAP IN LAP TO LAP-NUM-ALAP IN LV1-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE LAP-HRA-HLAP IN LAP TO LAP-HRA-HLAP IN LV1-FLD.
           MOVE LAP-COD-KLAP IN LAP TO LAP-COD-KLAP IN LV1-FLD.
           MOVE LAP-COD-AUTO IN LAP TO LAP-COD-AUTO IN LV1-FLD.
           MOVE LAP-COD-COME IN LAP TO LAP-COD-COME IN LV1-FLD.
           MOVE LAP-COD-CMND IN LAP TO LAP-COD-CMND IN LV1-FLD.
           MOVE LAP-COD-OTRN IN LAP TO LAP-COD-OTRN IN LV1-FLD.
           MOVE LAP-NUM-CLAP IN LAP TO LAP-NUM-CLAP IN LV1-FLD.
*% INCLUDE FIN_PUT_LAP_LV1 OF 'COLBMLV1.TXT'
       FIN-PUT-LAP-LV1.
           EXIT.
       
       PUT-PVC-LV1 SECTION.
       INI-PUT-PVC-LV1.
*% INCLUDE INI_PUT_PVC_LV1 OF 'COLBMLV1.TXT'
           IF PGM-STAT-PVC-NUL
               GO TO FIN-PUT-PVC-LV1.
           IF NOT PGM-STAT-PVC-OKS
               PERFORM CLR-PVC-LV1
               GO TO FIN-PUT-PVC-LV1.
           MOVE PVC-KEY-IPVC IN PVC TO PVC-KEY-IPVC IN LV1-FLD.
           MOVE PVC-MSC-TCVE IN PVC TO PVC-MSC-TCVE IN LV1-FLD.
           MOVE PVC-NUM-VIMI IN PVC TO PVC-NUM-VIMI IN LV1-FLD.
           MOVE PVC-NUM-VIMA IN PVC TO PVC-NUM-VIMA IN LV1-FLD.
           MOVE PVC-NUM-SVMI IN PVC TO PVC-NUM-SVMI IN LV1-FLD.
           MOVE PVC-IND-SVMI IN PVC TO PVC-IND-SVMI IN LV1-FLD.
           MOVE PVC-NUM-SVMA IN PVC TO PVC-NUM-SVMA IN LV1-FLD.
           MOVE PVC-IND-SVMA IN PVC TO PVC-IND-SVMA IN LV1-FLD.
           MOVE PVC-NUM-DVMI IN PVC TO PVC-NUM-DVMI IN LV1-FLD.
           MOVE PVC-IND-DVMI IN PVC TO PVC-IND-DVMI IN LV1-FLD.
           MOVE PVC-NUM-DVMA IN PVC TO PVC-NUM-DVMA IN LV1-FLD.
           MOVE PVC-IND-DVMA IN PVC TO PVC-IND-DVMA IN LV1-FLD.
           MOVE PVC-VAL-VVMI IN PVC TO PVC-VAL-VVMI IN LV1-FLD.
           MOVE PVC-VAL-VVMA IN PVC TO PVC-VAL-VVMA IN LV1-FLD.
           MOVE PVC-NUM-SPMI IN PVC TO PVC-NUM-SPMI IN LV1-FLD.
           MOVE PVC-IND-SPMI IN PVC TO PVC-IND-SPMI IN LV1-FLD.
           MOVE PVC-NUM-SPMA IN PVC TO PVC-NUM-SPMA IN LV1-FLD.
           MOVE PVC-IND-SPMA IN PVC TO PVC-IND-SPMA IN LV1-FLD.
           MOVE PVC-SGV-FMMI IN PVC TO PVC-SGV-FMMI IN LV1-FLD.
           MOVE PVC-SGV-FMMA IN PVC TO PVC-SGV-FMMA IN LV1-FLD.
           MOVE PVC-SGV-FPMI IN PVC TO PVC-SGV-FPMI IN LV1-FLD.
           MOVE PVC-SGV-FPMA IN PVC TO PVC-SGV-FPMA IN LV1-FLD.
           MOVE PVC-COD-VCOC IN PVC TO PVC-COD-VCOC IN LV1-FLD.
           MOVE PVC-NUM-DESD IN PVC TO PVC-NUM-DESD IN LV1-FLD.
*% INCLUDE FIN_PUT_PVC_LV1 OF 'COLBMLV1.TXT'
       FIN-PUT-PVC-LV1.
           EXIT.
       
       KEY-ALL-LV1 SECTION.
       INI-KEY-ALL-LV1.
*% INCLUDE INI_KEY_ALL_LV1 OF 'COLBMLV1.TXT'
           PERFORM KEY-LAP-LV1.
           PERFORM KEY-PVC-LV1.
*% INCLUDE FIN_KEY_ALL_LV1 OF 'COLBMLV1.TXT'
       FIN-KEY-ALL-LV1.
           EXIT.
       
       KEY-LAP-LV1 SECTION.
       INI-KEY-LAP-LV1.
*% INCLUDE INI_KEY_LAP_LV1 OF 'COLBMLV1.TXT'
*% RREG = "LAP"
*% RREG_FRM = "LAP"
*% NOMB = "FLAP"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE LAP-NUM-DLAP IN LV1-FLD TO LAP-NUM-DLAP IN LAP.
           MOVE LAP-NUM-MLAP IN LV1-FLD TO LAP-NUM-MLAP IN LAP.
           MOVE LAP-NUM-SLAP IN LV1-FLD TO LAP-NUM-SLAP IN LAP.
           MOVE LAP-NUM-ALAP IN LV1-FLD TO LAP-NUM-ALAP IN LAP.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE LAP-HRA-HLAP IN LV1-FLD TO LAP-HRA-HLAP IN LAP.
           MOVE LAP-COD-KLAP IN LV1-FLD TO LAP-COD-KLAP IN LAP.
           MOVE LAP-COD-AUTO IN LV1-FLD TO LAP-COD-AUTO IN LAP.
           MOVE LAP-NUM-CLAP IN LV1-FLD TO LAP-NUM-CLAP IN LAP.
*% INCLUDE FIN_KEY_LAP_LV1 OF 'COLBMLV1.TXT'
       FIN-KEY-LAP-LV1.
           EXIT.
       
       KEY-PVC-LV1 SECTION.
       INI-KEY-PVC-LV1.
*% INCLUDE INI_KEY_PVC_LV1 OF 'COLBMLV1.TXT'
*% INCLUDE FIN_KEY_PVC_LV1 OF 'COLBMLV1.TXT'
       FIN-KEY-PVC-LV1.
           EXIT.
       
       SCR-CLRA-LV1 SECTION.
       INI-SCR-CLRA-LV1.
*% INCLUDE INI_SCR_CLRA_LV1 OF 'COLBMLV1.TXT'
           PERFORM CLR-KEY-LV1.
           PERFORM CLR-FLD-LV1.
*% INCLUDE FIN_SCR_CLRA_LV1 OF 'COLBMLV1.TXT'
       FIN-SCR-CLRA-LV1.
           EXIT.
       
       CLR-KEY-LV1 SECTION.
       INI-CLR-KEY-LV1.
*% INCLUDE INI_CLR_KEY_LV1 OF 'COLBMLV1.TXT'
           MOVE SPACES TO FRM-COD-CREB IN LV1-FLD.
           MOVE SPACES TO FRM-IND-NEXT IN LV1-FLD.
*% RREG = "LAP"
*% RREG_FRM = "LAP"
*% NOMB = "FLAP"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO LAP-FEC-FLAP IN LV1-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO LAP-HRA-HLAP IN LV1-FLD.
           MOVE SPACES TO LAP-COD-KLAP IN LV1-FLD.
           MOVE SPACES TO LAP-COD-AUTO IN LV1-FLD.
           MOVE ZEROES TO LAP-NUM-CLAP IN LV1-FLD.
*% INCLUDE FIN_CLR_KEY_LV1 OF 'COLBMLV1.TXT'
       FIN-CLR-KEY-LV1.
           EXIT.
       
       CLR-FLD-LV1 SECTION.
       INI-CLR-FLD-LV1.
*% INCLUDE INI_CLR_FLD_LV1 OF 'COLBMLV1.TXT'
           MOVE SPACES TO FRM-GLS-INST IN LV1-FLD.
           MOVE SPACES TO FRM-GLS-TX01 IN LV1-FLD.
           MOVE SPACES TO FRM-GLS-TX03 IN LV1-FLD.
           MOVE SPACES TO FRM-GLS-TX05 IN LV1-FLD.
           MOVE SPACES TO FRM-GLS-TX07 IN LV1-FLD.
           MOVE SPACES TO FRM-GLS-TX09 IN LV1-FLD.
           MOVE SPACES TO FRM-GLS-TX11 IN LV1-FLD.
           MOVE SPACES TO FRM-GLS-TX13 IN LV1-FLD.
           MOVE SPACES TO FRM-GLS-TX15 IN LV1-FLD.
           MOVE SPACES TO FRM-GLS-TX17 IN LV1-FLD.
           MOVE SPACES TO FRM-GLS-TX02 IN LV1-FLD.
           MOVE SPACES TO FRM-GLS-TX04 IN LV1-FLD.
           MOVE SPACES TO FRM-GLS-TX06 IN LV1-FLD.
           MOVE SPACES TO FRM-GLS-TX08 IN LV1-FLD.
           MOVE SPACES TO FRM-GLS-TX10 IN LV1-FLD.
           MOVE SPACES TO FRM-GLS-TX12 IN LV1-FLD.
           MOVE SPACES TO FRM-GLS-TX14 IN LV1-FLD.
           MOVE SPACES TO FRM-GLS-TX16 IN LV1-FLD.
           MOVE SPACES TO FRM-GLS-TX18 IN LV1-FLD.
           PERFORM CLR-LAP-LV1.
           PERFORM CLR-PVC-LV1.
*% INCLUDE FIN_CLR_FLD_LV1 OF 'COLBMLV1.TXT'
       FIN-CLR-FLD-LV1.
           EXIT.
       
       CLR-LAP-LV1 SECTION.
       INI-CLR-LAP-LV1.
*% INCLUDE INI_CLR_LAP_LV1 OF 'COLBMLV1.TXT'
           MOVE SPACES TO LAP-COD-COME IN LV1-FLD.
           MOVE SPACES TO LAP-COD-CMND IN LV1-FLD.
           MOVE SPACES TO LAP-COD-OTRN IN LV1-FLD.
*% INCLUDE FIN_CLR_LAP_LV1 OF 'COLBMLV1.TXT'
       FIN-CLR-LAP-LV1.
           EXIT.
       
       CLR-PVC-LV1 SECTION.
       INI-CLR-PVC-LV1.
*% INCLUDE INI_CLR_PVC_LV1 OF 'COLBMLV1.TXT'
           MOVE SPACES TO PVC-KEY-IPVC IN LV1-FLD.
           MOVE SPACES TO PVC-MSC-TCVE IN LV1-FLD.
           MOVE ZEROES TO PVC-NUM-VIMI IN LV1-FLD.
           MOVE ZEROES TO PVC-NUM-VIMA IN LV1-FLD.
           MOVE ZEROES TO PVC-NUM-SVMI IN LV1-FLD.
           MOVE SPACES TO PVC-IND-SVMI IN LV1-FLD.
           MOVE ZEROES TO PVC-NUM-SVMA IN LV1-FLD.
           MOVE SPACES TO PVC-IND-SVMA IN LV1-FLD.
           MOVE ZEROES TO PVC-NUM-DVMI IN LV1-FLD.
           MOVE SPACES TO PVC-IND-DVMI IN LV1-FLD.
           MOVE ZEROES TO PVC-NUM-DVMA IN LV1-FLD.
           MOVE SPACES TO PVC-IND-DVMA IN LV1-FLD.
           MOVE ZEROES TO PVC-VAL-VVMI IN LV1-FLD.
           MOVE ZEROES TO PVC-VAL-VVMA IN LV1-FLD.
           MOVE ZEROES TO PVC-NUM-SPMI IN LV1-FLD.
           MOVE SPACES TO PVC-IND-SPMI IN LV1-FLD.
           MOVE ZEROES TO PVC-NUM-SPMA IN LV1-FLD.
           MOVE SPACES TO PVC-IND-SPMA IN LV1-FLD.
           MOVE ZEROES TO PVC-SGV-FMMI IN LV1-FLD.
           MOVE ZEROES TO PVC-SGV-FMMA IN LV1-FLD.
           MOVE ZEROES TO PVC-SGV-FPMI IN LV1-FLD.
           MOVE ZEROES TO PVC-SGV-FPMA IN LV1-FLD.
           MOVE SPACES TO PVC-COD-VCOC IN LV1-FLD.
           MOVE ZEROES TO PVC-NUM-DESD IN LV1-FLD.
*% INCLUDE FIN_CLR_PVC_LV1 OF 'COLBMLV1.TXT'
       FIN-CLR-PVC-LV1.
           EXIT.
       
       FST-KEY-LV1 SECTION.
       INI-FST-KEY-LV1.
*% INCLUDE INI_FST_KEY_LV1 OF 'COLBMLV1.TXT'
           MOVE -1 TO FRM-COD-CREB-LEN IN LV1-FLD.
*% INCLUDE FIN_FST_KEY_LV1 OF 'COLBMLV1.TXT'
       FIN-FST-KEY-LV1.
           EXIT.
       
       FST-FLD-LV1 SECTION.
       INI-FST-FLD-LV1.
*% INCLUDE INI_FST_FLD_LV1 OF 'COLBMLV1.TXT'
*% INCLUDE FIN_FST_FLD_LV1 OF 'COLBMLV1.TXT'
       FIN-FST-FLD-LV1.
           EXIT.
       
       PRO-KEY-LV1 SECTION.
       INI-PRO-KEY-LV1.
*% INCLUDE INI_PRO_KEY_LV1 OF 'COLBMLV1.TXT'
           MOVE FRM-CPIM-AEY TO FRM-COD-CREB-ATR IN LV1-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-NEXT-ATR IN LV1-FLD.
           PERFORM PRO-IKY-LV1.
           PERFORM PRO-AKY-LV1.
           PERFORM PRO-MKY-LV1.
*% INCLUDE FIN_PRO_KEY_LV1 OF 'COLBMLV1.TXT'
       FIN-PRO-KEY-LV1.
           EXIT.
       
       PRO-IKY-LV1 SECTION.
       INI-PRO-IKY-LV1.
*% INCLUDE INI_PRO_IKY_LV1 OF 'COLBMLV1.TXT'
*% RREG = "LAP"
*% RREG_FRM = "LAP"
*% NOMB = "FLAP"
           MOVE FRM-CPIM-AEY TO LAP-FEC-FLAP-ATR IN LV1-FLD.
           MOVE FRM-CPIM-AEY TO LAP-HRA-HLAP-ATR IN LV1-FLD.
           MOVE FRM-CPIM-AEY TO LAP-COD-KLAP-ATR IN LV1-FLD.
           MOVE FRM-CPIM-AEY TO LAP-COD-AUTO-ATR IN LV1-FLD.
*% INCLUDE FIN_PRO_IKY_LV1 OF 'COLBMLV1.TXT'
       FIN-PRO-IKY-LV1.
           EXIT.
       
       PRO-MKY-LV1 SECTION.
       INI-PRO-MKY-LV1.
*% INCLUDE INI_PRO_MKY_LV1 OF 'COLBMLV1.TXT'
*% INCLUDE FIN_PRO_MKY_LV1 OF 'COLBMLV1.TXT'
       FIN-PRO-MKY-LV1.
           EXIT.
       
       PRO-AKY-LV1 SECTION.
       INI-PRO-AKY-LV1.
*% INCLUDE INI_PRO_AKY_LV1 OF 'COLBMLV1.TXT'
*% INCLUDE FIN_PRO_AKY_LV1 OF 'COLBMLV1.TXT'
       FIN-PRO-AKY-LV1.
           EXIT.
       
       UNP-KEY-LV1 SECTION.
       INI-UNP-KEY-LV1.
*% INCLUDE INI_UNP_KEY_LV1 OF 'COLBMLV1.TXT'
           MOVE FRM-CPIM-UBY TO FRM-COD-CREB-ATR IN LV1-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IND-NEXT-ATR IN LV1-FLD.
           PERFORM UNP-IKY-LV1.
           PERFORM UNP-AKY-LV1.
           PERFORM UNP-MKY-LV1.
*% INCLUDE FIN_UNP_KEY_LV1 OF 'COLBMLV1.TXT'
       FIN-UNP-KEY-LV1.
           EXIT.
       
       UNP-IKY-LV1 SECTION.
       INI-UNP-IKY-LV1.
*% INCLUDE INI_UNP_IKY_LV1 OF 'COLBMLV1.TXT'
           MOVE FRM-CPIM-UBY TO FRM-COD-CREB-ATR.
           MOVE FRM-CPIM-UBY TO FRM-IND-NEXT-ATR.
*% RREG = "LAP"
*% RREG_FRM = "LAP"
*% NOMB = "FLAP"
           MOVE FRM-CPIM-UBY TO LAP-FEC-FLAP-ATR IN LV1-FLD.
           MOVE FRM-CPIM-UBY TO LAP-HRA-HLAP-ATR.
           MOVE FRM-CPIM-UBY TO LAP-COD-KLAP-ATR.
           MOVE FRM-CPIM-UBY TO LAP-COD-AUTO-ATR.
*% INCLUDE FIN_UNP_IKY_LV1 OF 'COLBMLV1.TXT'
       FIN-UNP-IKY-LV1.
           EXIT.
       
       UNP-MKY-LV1 SECTION.
       INI-UNP-MKY-LV1.
*% INCLUDE INI_UNP_MKY_LV1 OF 'COLBMLV1.TXT'
*% INCLUDE FIN_UNP_MKY_LV1 OF 'COLBMLV1.TXT'
       FIN-UNP-MKY-LV1.
           EXIT.
       
