       FIN-UNP-AKY-LV1.
           EXIT.
       
       UNP-FLD-LV1 SECTION.
       INI-UNP-FLD-LV1.
*% INCLUDE INI_UNP_FLD_LV1 OF 'COLBMLV1.TXT'
           PERFORM UNP-LAP-LV1.
           PERFORM UNP-PVC-LV1.
*% INCLUDE FIN_UNP_FLD_LV1 OF 'COLBMLV1.TXT'
       FIN-UNP-FLD-LV1.
           EXIT.
       
       UNP-LAP-LV1 SECTION.
       INI-UNP-LAP-LV1.
*% INCLUDE INI_UNP_LAP_LV1 OF 'COLBMLV1.TXT'
*% INCLUDE FIN_UNP_LAP_LV1 OF 'COLBMLV1.TXT'
       FIN-UNP-LAP-LV1.
           EXIT.
       
       UNP-PVC-LV1 SECTION.
       INI-UNP-PVC-LV1.
*% INCLUDE INI_UNP_PVC_LV1 OF 'COLBMLV1.TXT'
*% INCLUDE FIN_UNP_PVC_LV1 OF 'COLBMLV1.TXT'
       FIN-UNP-PVC-LV1.
           EXIT.
       
       VAL-KEY-LV1 SECTION.
       INI-VAL-KEY-LV1.
           PERFORM VAL-IKY-LV1.
           PERFORM VAL-MKY-LV1.
           PERFORM VAL-AKY-LV1.
       FIN-VAL-KEY-LV1.
           EXIT.
       
       VAL-IKY-LV1 SECTION.
       INI-VAL-IKY-LV1.
*% INCLUDE INI_VAL_IKY_LV1 OF 'COLBMLV1.TXT'
           PERFORM VAL-FRM-COD-CREB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-CREB-LEN IN LV1-FLD
                GO TO FIN-VAL-IKY-LV1.
           PERFORM VAL-FRM-IND-NEXT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IND-NEXT-LEN IN LV1-FLD
                GO TO FIN-VAL-IKY-LV1.
*% RREG = "LAP"
*% RREG_FRM = "LAP"
*% NOMB = "FLAP"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-LAP-FEC-FLAP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                LAP-FEC-FLAP-LEN IN LV1-FLD
                GO TO FIN-VAL-IKY-LV1.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-LAP-HRA-HLAP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO LAP-HRA-HLAP-LEN IN LV1-FLD
                GO TO FIN-VAL-IKY-LV1.
           PERFORM VAL-LAP-COD-KLAP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO LAP-COD-KLAP-LEN IN LV1-FLD
                GO TO FIN-VAL-IKY-LV1.
           PERFORM VAL-LAP-COD-AUTO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO LAP-COD-AUTO-LEN IN LV1-FLD
                GO TO FIN-VAL-IKY-LV1.
           PERFORM VAL-LAP-NUM-CLAP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO LAP-NUM-CLAP-LEN IN LV1-FLD
                GO TO FIN-VAL-IKY-LV1.
*% INCLUDE FIN_VAL_IKY_LV1 OF 'COLBMLV1.TXT'
       FIN-VAL-IKY-LV1.
           EXIT.
       
       VAL-MKY-LV1 SECTION.
       INI-VAL-MKY-LV1.
*% INCLUDE INI_VAL_MKY_LV1 OF 'COLBMLV1.TXT'
*% INCLUDE FIN_VAL_MKY_LV1 OF 'COLBMLV1.TXT'
       FIN-VAL-MKY-LV1.
           EXIT.
       
       VAL-AKY-LV1 SECTION.
       INI-VAL-AKY-LV1.
*% INCLUDE INI_VAL_AKY_LV1 OF 'COLBMLV1.TXT'
*% INCLUDE FIN_VAL_AKY_LV1 OF 'COLBMLV1.TXT'
       FIN-VAL-AKY-LV1.
           EXIT.
       
       VAL-FRM-COD-CREB SECTION.
       INI-VAL-FRM-COD-CREB.
*% LBL = "FRM_COD_CREB OF 'COLBMLV1.TXT'"
*% INCLUDE INI_FRM_COD_CREB OF 'COLBMLV1.TXT'
           IF FRM-COD-CREB IN LV1-FLD NOT > SPACES
              MOVE SPACES TO FRM-COD-CREB IN LV1-FLD.
*% INCLUDE FRM_COD_CREB OF 'COLBMLV1.TXT'
*% INCLUDE FIN_FRM_COD_CREB OF 'COLBMLV1.TXT'
       FIN-VAL-FRM-COD-CREB.
           EXIT.
       
       VAL-FRM-IND-NEXT SECTION.
       INI-VAL-FRM-IND-NEXT.
*% LBL = "FRM_IND_NEXT OF 'COLBMLV1.TXT'"
*% INCLUDE INI_FRM_IND_NEXT OF 'COLBMLV1.TXT'
           IF FRM-IND-NEXT IN LV1-FLD NOT > SPACES
              MOVE SPACES TO FRM-IND-NEXT IN LV1-FLD.
*% INCLUDE FRM_IND_NEXT OF 'COLBMLV1.TXT'
*% INCLUDE FIN_FRM_IND_NEXT OF 'COLBMLV1.TXT'
       FIN-VAL-FRM-IND-NEXT.
           EXIT.
*% RREG = "LAP"
*% RREG_FRM = "LAP"
*% NOMB = "FLAP"
*% OBL = "S"
*% LBL = "LAP_FEC_FLAP OF 'COLBMLV1.TXT'"
*% IZR = "S"
*% ZRO = "S"
*% BUF = "LV1-FLD"
*% FLD = "LAP-FEC-FLAP"
*% SN  = "S"
*% HOY = "LE"
*% HBL = "S"
*% TIPO = "FEC"
*% FRM_IFLD = ""
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-LAP-FEC-FLAP SECTION.
       INI-VAL-LAP-FEC-FLAP.
           IF LAP-FEC-FLAP IN LV1-FLD  NOT > ZEROES
              MOVE ZEROES TO LAP-FEC-FLAP IN LV1-FLD.
*% INCLUDE 'SETFEC'
*% INCLUDE INI_LAP_FEC_FLAP OF 'COLBMLV1.TXT'
*% INCLUDE 'PROFEC'
*% INCLUDE LAP_FEC_FLAP OF 'COLBMLV1.TXT'
*% INCLUDE FIN_LAP_FEC_FLAP OF 'COLBMLV1.TXT'
       FIN-VAL-LAP-FEC-FLAP.
           EXIT.
      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-LAP-HRA-HLAP SECTION.
       INI-VAL-LAP-HRA-HLAP.
