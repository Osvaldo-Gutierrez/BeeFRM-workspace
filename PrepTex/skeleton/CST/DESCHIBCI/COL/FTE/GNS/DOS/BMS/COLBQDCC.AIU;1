      *
      * COLBQDCC
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:50 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "DCC"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMDCC.TXT'
       SET-TAG-COLBQDCC SECTION.
       INI-SET-TAG-COLBQDCC.
           MOVE
           '<IDNMOD NOM=$COLBQDCC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:50 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQDCC.
           EXIT.
           
       PUT-ALL-DCC SECTION.
       INI-PUT-ALL-DCC.
*% INCLUDE INI_PUT_ALL_DCC OF 'COLBMDCC.TXT'
           PERFORM PUT-DCC-DCC.
           PERFORM PUT-TAB-DCC.
*% INCLUDE FIN_PUT_ALL_DCC OF 'COLBMDCC.TXT'
       FIN-PUT-ALL-DCC.
           EXIT.
       
       PUT-DCC-DCC SECTION.
       INI-PUT-DCC-DCC.
*% INCLUDE INI_PUT_DCC_DCC OF 'COLBMDCC.TXT'
           IF PGM-STAT-DCC-NUL
               GO TO FIN-PUT-DCC-DCC.
           IF NOT PGM-STAT-DCC-OKS
               PERFORM CLR-DCC-DCC
               GO TO FIN-PUT-DCC-DCC.
           MOVE DCC-MSC-TCCT IN DCC TO DCC-MSC-TCCT IN DCC-FLD.
           MOVE DCC-COD-VCAM IN DCC TO DCC-COD-VCAM IN DCC-FLD.
           MOVE DCC-COD-COCR IN DCC TO DCC-COD-COCR IN DCC-FLD.
           MOVE DCC-COD-PLZO IN DCC TO DCC-COD-PLZO IN DCC-FLD.
           MOVE DCC-IND-SCRT IN DCC TO DCC-IND-SCRT IN DCC-FLD.
           MOVE DCC-IND-SCTB IN DCC TO DCC-IND-SCTB IN DCC-FLD.
           MOVE DCC-IND-SCBZ IN DCC TO DCC-IND-SCBZ IN DCC-FLD.
           MOVE DCC-CIC-CCTB IN DCC TO DCC-CIC-CCTB IN DCC-FLD.
           MOVE DCC-EXT-CTAB IN DCC TO DCC-EXT-CTAB IN DCC-FLD.
           MOVE DCC-GLS-CCSB IN DCC TO DCC-GLS-CCSB IN DCC-FLD.
           MOVE DCC-IND-VIGE IN DCC TO DCC-IND-VIGE IN DCC-FLD.
*% RREG = "DCC"
*% RREG_FRM = "DCC"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DCC-NUM-DTRN IN DCC TO DCC-NUM-DTRN IN DCC-FLD.
           MOVE DCC-NUM-MTRN IN DCC TO DCC-NUM-MTRN IN DCC-FLD.
           MOVE DCC-NUM-STRN IN DCC TO DCC-NUM-STRN IN DCC-FLD.
           MOVE DCC-NUM-ATRN IN DCC TO DCC-NUM-ATRN IN DCC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE DCC-GLS-DISP IN DCC TO DCC-GLS-DISP IN DCC-FLD.
*% BUF = "DCC-FLD"
*% SIS = "COL"
*% TAB = "TCCT"
*% FLD = "DCC-MSC-TCCT"
*% GLS = "FRM-GLS-TCCT"
*% TIENE_GLOSA = TRUE
*% LGL = 30
*% INCLUDE 'BUSMSC'
*% BUF = "DCC-FLD"
*% SIS = "TAB"
*% TAB = "VLR "
*% FLD = "DCC-COD-VCAM"
*% GLS = "FRM-GLS-VCAM"
*% TIENE_GLOSA = TRUE
*% LGL = 30
*% INCLUDE 'BUSTAB'
*% BUF = "DCC-FLD"
*% SIS = "COL"
*% TAB = "AUX "
*% FLD = "DCC-COD-COCR"
*% GLS = "FRM-GLS-COCR"
*% TIENE_GLOSA = TRUE
*% LGL = 30
*% INCLUDE 'BUSTAB'
*% BUF = "DCC-FLD"
*% SIS = "TAB"
*% TAB = "PZO "
*% FLD = "DCC-COD-PLZO"
*% GLS = "FRM-GLS-PLZO"
*% TIENE_GLOSA = TRUE
*% LGL = 30
*% INCLUDE 'BUSTAB'
*% BUF = "DCC-FLD"
*% SIS = "COL"
*% TAB = "SCRT"
*% FLD = "DCC-IND-SCRT"
*% GLS = "FRM-GLS-SCRT"
*% TIENE_GLOSA = TRUE
*% LGL = 30
*% INCLUDE 'BUSMSC'
*% BUF = "DCC-FLD"
*% SIS = "COL"
*% TAB = "SCTB"
*% FLD = "DCC-IND-SCTB"
*% GLS = "FRM-GLS-SCTB"
*% TIENE_GLOSA = TRUE
*% LGL = 30
*% INCLUDE 'BUSMSC'
*% BUF = "DCC-FLD"
*% SIS = "COL"
*% TAB = "SCBZ"
*% FLD = "DCC-IND-SCBZ"
*% GLS = "FRM-GLS-SCBZ"
*% TIENE_GLOSA = TRUE
*% LGL = 30
*% INCLUDE 'BUSMSC'
*% INCLUDE FIN_PUT_DCC_DCC OF 'COLBMDCC.TXT'
       FIN-PUT-DCC-DCC.
           EXIT.
       
       PUT-TAB-DCC SECTION.
       INI-PUT-TAB-DCC.
*% INCLUDE INI_PUT_TAB_DCC OF 'COLBMDCC.TXT'
           IF PGM-STAT-TAB-NUL
               GO TO FIN-PUT-TAB-DCC.
           IF NOT PGM-STAT-TAB-OKS
               PERFORM CLR-TAB-DCC
               GO TO FIN-PUT-TAB-DCC.
           MOVE TAB-CIC-CTAB IN TAB TO TAB-CIC-CTAB IN DCC-FLD.
           MOVE TAB-GLS-DESC IN TAB TO TAB-GLS-DESC IN DCC-FLD.
           MOVE TAB-GLS-DATA IN TAB TO TAB-GLS-DATA IN DCC-FLD.
           MOVE TAB-GLS-ABRV IN TAB TO TAB-GLS-ABRV IN DCC-FLD.
           MOVE TAB-GLS-DCOR IN TAB TO TAB-GLS-DCOR IN DCC-FLD.
*% INCLUDE FIN_PUT_TAB_DCC OF 'COLBMDCC.TXT'
       FIN-PUT-TAB-DCC.
           EXIT.
       
       KEY-ALL-DCC SECTION.
       INI-KEY-ALL-DCC.
*% INCLUDE INI_KEY_ALL_DCC OF 'COLBMDCC.TXT'
           PERFORM KEY-DCC-DCC.
           PERFORM KEY-TAB-DCC.
*% INCLUDE FIN_KEY_ALL_DCC OF 'COLBMDCC.TXT'
       FIN-KEY-ALL-DCC.
           EXIT.
       
       KEY-DCC-DCC SECTION.
       INI-KEY-DCC-DCC.
*% INCLUDE INI_KEY_DCC_DCC OF 'COLBMDCC.TXT'
           MOVE DCC-MSC-TCCT IN DCC-FLD TO DCC-MSC-TCCT IN DCC.
           MOVE DCC-COD-VCAM IN DCC-FLD TO DCC-COD-VCAM IN DCC.
           MOVE DCC-COD-COCR IN DCC-FLD TO DCC-COD-COCR IN DCC.
           MOVE DCC-COD-PLZO IN DCC-FLD TO DCC-COD-PLZO IN DCC.
           MOVE DCC-IND-SCRT IN DCC-FLD TO DCC-IND-SCRT IN DCC.
           MOVE DCC-IND-SCTB IN DCC-FLD TO DCC-IND-SCTB IN DCC.
           MOVE DCC-IND-SCBZ IN DCC-FLD TO DCC-IND-SCBZ IN DCC.
*% INCLUDE FIN_KEY_DCC_DCC OF 'COLBMDCC.TXT'
       FIN-KEY-DCC-DCC.
           EXIT.
       
       KEY-TAB-DCC SECTION.
       INI-KEY-TAB-DCC.
*% INCLUDE INI_KEY_TAB_DCC OF 'COLBMDCC.TXT'
*% INCLUDE FIN_KEY_TAB_DCC OF 'COLBMDCC.TXT'
       FIN-KEY-TAB-DCC.
           EXIT.
       
       SCR-CLRA-DCC SECTION.
       INI-SCR-CLRA-DCC.
*% INCLUDE INI_SCR_CLRA_DCC OF 'COLBMDCC.TXT'
           PERFORM CLR-KEY-DCC.
           PERFORM CLR-FLD-DCC.
*% INCLUDE FIN_SCR_CLRA_DCC OF 'COLBMDCC.TXT'
       FIN-SCR-CLRA-DCC.
           EXIT.
       
       CLR-KEY-DCC SECTION.
       INI-CLR-KEY-DCC.
*% INCLUDE INI_CLR_KEY_DCC OF 'COLBMDCC.TXT'
           MOVE SPACES TO DCC-MSC-TCCT IN DCC-FLD.
           MOVE SPACES TO DCC-COD-VCAM IN DCC-FLD.
           MOVE SPACES TO DCC-COD-COCR IN DCC-FLD.
           MOVE SPACES TO DCC-COD-PLZO IN DCC-FLD.
           MOVE SPACES TO DCC-IND-SCRT IN DCC-FLD.
           MOVE SPACES TO DCC-IND-SCTB IN DCC-FLD.
           MOVE SPACES TO DCC-IND-SCBZ IN DCC-FLD.
*% INCLUDE FIN_CLR_KEY_DCC OF 'COLBMDCC.TXT'
       FIN-CLR-KEY-DCC.
           EXIT.
       
       CLR-FLD-DCC SECTION.
       INI-CLR-FLD-DCC.
*% INCLUDE INI_CLR_FLD_DCC OF 'COLBMDCC.TXT'
           MOVE SPACES TO FRM-GLS-TCCT IN DCC-FLD.
           MOVE SPACES TO FRM-GLS-VCAM IN DCC-FLD.
           MOVE SPACES TO FRM-GLS-COCR IN DCC-FLD.
           MOVE SPACES TO FRM-GLS-PLZO IN DCC-FLD.
           MOVE SPACES TO FRM-GLS-SCRT IN DCC-FLD.
           MOVE SPACES TO FRM-GLS-SCTB IN DCC-FLD.
           MOVE SPACES TO FRM-GLS-SCBZ IN DCC-FLD.
           PERFORM CLR-DCC-DCC.
           PERFORM CLR-TAB-DCC.
*% INCLUDE FIN_CLR_FLD_DCC OF 'COLBMDCC.TXT'
       FIN-CLR-FLD-DCC.
           EXIT.
       
       CLR-DCC-DCC SECTION.
       INI-CLR-DCC-DCC.
*% INCLUDE INI_CLR_DCC_DCC OF 'COLBMDCC.TXT'
           MOVE SPACES TO DCC-CIC-CCTB IN DCC-FLD.
           MOVE SPACES TO DCC-EXT-CTAB IN DCC-FLD.
           MOVE SPACES TO DCC-GLS-CCSB IN DCC-FLD.
           MOVE SPACES TO DCC-IND-VIGE IN DCC-FLD.
*% RREG = "DCC"
*% RREG_FRM = "DCC"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DCC-FEC-FTRN IN DCC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO DCC-GLS-DISP IN DCC-FLD.
*% INCLUDE FIN_CLR_DCC_DCC OF 'COLBMDCC.TXT'
       FIN-CLR-DCC-DCC.
           EXIT.
       
       CLR-TAB-DCC SECTION.
       INI-CLR-TAB-DCC.
*% INCLUDE INI_CLR_TAB_DCC OF 'COLBMDCC.TXT'
           MOVE SPACES TO TAB-CIC-CTAB IN DCC-FLD.
           MOVE SPACES TO TAB-GLS-DESC IN DCC-FLD.
           MOVE SPACES TO TAB-GLS-DATA IN DCC-FLD.
           MOVE SPACES TO TAB-GLS-ABRV IN DCC-FLD.
           MOVE SPACES TO TAB-GLS-DCOR IN DCC-FLD.
*% INCLUDE FIN_CLR_TAB_DCC OF 'COLBMDCC.TXT'
       FIN-CLR-TAB-DCC.
           EXIT.
       
       FST-KEY-DCC SECTION.
       INI-FST-KEY-DCC.
*% INCLUDE INI_FST_KEY_DCC OF 'COLBMDCC.TXT'
           MOVE -1 TO DCC-MSC-TCCT-LEN IN DCC-FLD.
*% INCLUDE FIN_FST_KEY_DCC OF 'COLBMDCC.TXT'
       FIN-FST-KEY-DCC.
           EXIT.
       
       FST-FLD-DCC SECTION.
       INI-FST-FLD-DCC.
*% INCLUDE INI_FST_FLD_DCC OF 'COLBMDCC.TXT'
           MOVE -1 TO DCC-CIC-CCTB-LEN IN DCC-FLD.
*% INCLUDE FIN_FST_FLD_DCC OF 'COLBMDCC.TXT'
       FIN-FST-FLD-DCC.
           EXIT.
       
       PRO-KEY-DCC SECTION.
       INI-PRO-KEY-DCC.
*% INCLUDE INI_PRO_KEY_DCC OF 'COLBMDCC.TXT'
           PERFORM PRO-IKY-DCC.
           PERFORM PRO-AKY-DCC.
           PERFORM PRO-MKY-DCC.
*% INCLUDE FIN_PRO_KEY_DCC OF 'COLBMDCC.TXT'
       FIN-PRO-KEY-DCC.
           EXIT.
       
       PRO-IKY-DCC SECTION.
       INI-PRO-IKY-DCC.
*% INCLUDE INI_PRO_IKY_DCC OF 'COLBMDCC.TXT'
           MOVE FRM-CPIM-AEY TO DCC-MSC-TCCT-ATR IN DCC-FLD.
           MOVE FRM-CPIM-AEY TO DCC-COD-VCAM-ATR IN DCC-FLD.
           MOVE FRM-CPIM-AEY TO DCC-COD-COCR-ATR IN DCC-FLD.
           MOVE FRM-CPIM-AEY TO DCC-COD-PLZO-ATR IN DCC-FLD.
           MOVE FRM-CPIM-AEY TO DCC-IND-SCRT-ATR IN DCC-FLD.
           MOVE FRM-CPIM-AEY TO DCC-IND-SCTB-ATR IN DCC-FLD.
           MOVE FRM-CPIM-AEY TO DCC-IND-SCBZ-ATR IN DCC-FLD.
*% INCLUDE FIN_PRO_IKY_DCC OF 'COLBMDCC.TXT'
       FIN-PRO-IKY-DCC.
           EXIT.
       
       PRO-MKY-DCC SECTION.
       INI-PRO-MKY-DCC.
*% INCLUDE INI_PRO_MKY_DCC OF 'COLBMDCC.TXT'
*% INCLUDE FIN_PRO_MKY_DCC OF 'COLBMDCC.TXT'
       FIN-PRO-MKY-DCC.
           EXIT.
       
       PRO-AKY-DCC SECTION.
       INI-PRO-AKY-DCC.
*% INCLUDE INI_PRO_AKY_DCC OF 'COLBMDCC.TXT'
*% INCLUDE FIN_PRO_AKY_DCC OF 'COLBMDCC.TXT'
       FIN-PRO-AKY-DCC.
           EXIT.
       
       UNP-KEY-DCC SECTION.
       INI-UNP-KEY-DCC.
*% INCLUDE INI_UNP_KEY_DCC OF 'COLBMDCC.TXT'
           PERFORM UNP-IKY-DCC.
           PERFORM UNP-AKY-DCC.
           PERFORM UNP-MKY-DCC.
*% INCLUDE FIN_UNP_KEY_DCC OF 'COLBMDCC.TXT'
       FIN-UNP-KEY-DCC.
           EXIT.
       
       UNP-IKY-DCC SECTION.
       INI-UNP-IKY-DCC.
*% INCLUDE INI_UNP_IKY_DCC OF 'COLBMDCC.TXT'
           MOVE FRM-CPIM-UBY TO DCC-MSC-TCCT-ATR.
           MOVE FRM-CPIM-UBY TO DCC-COD-VCAM-ATR.
           MOVE FRM-CPIM-UBY TO DCC-COD-COCR-ATR.
           MOVE FRM-CPIM-UBY TO DCC-COD-PLZO-ATR.
           MOVE FRM-CPIM-UBY TO DCC-IND-SCRT-ATR.
           MOVE FRM-CPIM-UBY TO DCC-IND-SCTB-ATR.
           MOVE FRM-CPIM-UBY TO DCC-IND-SCBZ-ATR.
*% INCLUDE FIN_UNP_IKY_DCC OF 'COLBMDCC.TXT'
       FIN-UNP-IKY-DCC.
           EXIT.
       
       UNP-MKY-DCC SECTION.
       INI-UNP-MKY-DCC.
*% INCLUDE INI_UNP_MKY_DCC OF 'COLBMDCC.TXT'
*% INCLUDE FIN_UNP_MKY_DCC OF 'COLBMDCC.TXT'
       FIN-UNP-MKY-DCC.
           EXIT.
       
       UNP-AKY-DCC SECTION.
       INI-UNP-AKY-DCC.
*% INCLUDE INI_UNP_AKY_DCC OF 'COLBMDCC.TXT'
