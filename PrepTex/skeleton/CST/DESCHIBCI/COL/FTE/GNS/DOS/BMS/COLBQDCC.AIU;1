           EXIT.
       
       UNP-FLD-DCC SECTION.
       INI-UNP-FLD-DCC.
*% INCLUDE INI_UNP_FLD_DCC OF 'COLBMDCC.TXT'
           PERFORM UNP-DCC-DCC.
           PERFORM UNP-TAB-DCC.
*% INCLUDE FIN_UNP_FLD_DCC OF 'COLBMDCC.TXT'
       FIN-UNP-FLD-DCC.
           EXIT.
       
       UNP-DCC-DCC SECTION.
       INI-UNP-DCC-DCC.
*% INCLUDE INI_UNP_DCC_DCC OF 'COLBMDCC.TXT'
           MOVE FRM-CPIM-UBY TO DCC-CIC-CCTB-ATR IN DCC-FLD.
           MOVE FRM-CPIM-UBY TO DCC-EXT-CTAB-ATR IN DCC-FLD.
           MOVE FRM-CPIM-UBY TO DCC-GLS-CCSB-ATR IN DCC-FLD.
           MOVE FRM-CPIM-UBY TO DCC-IND-VIGE-ATR IN DCC-FLD.
           MOVE FRM-CPIM-UBY TO DCC-GLS-DISP-ATR IN DCC-FLD.
*% INCLUDE FIN_UNP_DCC_DCC OF 'COLBMDCC.TXT'
       FIN-UNP-DCC-DCC.
           EXIT.
       
       UNP-TAB-DCC SECTION.
       INI-UNP-TAB-DCC.
*% INCLUDE INI_UNP_TAB_DCC OF 'COLBMDCC.TXT'
*% INCLUDE FIN_UNP_TAB_DCC OF 'COLBMDCC.TXT'
       FIN-UNP-TAB-DCC.
           EXIT.
       
       VAL-KEY-DCC SECTION.
       INI-VAL-KEY-DCC.
           PERFORM VAL-IKY-DCC.
           PERFORM VAL-MKY-DCC.
           PERFORM VAL-AKY-DCC.
       FIN-VAL-KEY-DCC.
           EXIT.
       
       VAL-IKY-DCC SECTION.
       INI-VAL-IKY-DCC.
*% INCLUDE INI_VAL_IKY_DCC OF 'COLBMDCC.TXT'
           PERFORM VAL-DCC-MSC-TCCT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCC-MSC-TCCT-LEN IN DCC-FLD
                GO TO FIN-VAL-IKY-DCC.
           PERFORM VAL-DCC-COD-VCAM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCC-COD-VCAM-LEN IN DCC-FLD
                GO TO FIN-VAL-IKY-DCC.
           PERFORM VAL-DCC-COD-COCR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCC-COD-COCR-LEN IN DCC-FLD
                GO TO FIN-VAL-IKY-DCC.
           PERFORM VAL-DCC-COD-PLZO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCC-COD-PLZO-LEN IN DCC-FLD
                GO TO FIN-VAL-IKY-DCC.
           PERFORM VAL-DCC-IND-SCRT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCC-IND-SCRT-LEN IN DCC-FLD
                GO TO FIN-VAL-IKY-DCC.
           PERFORM VAL-DCC-IND-SCTB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCC-IND-SCTB-LEN IN DCC-FLD
                GO TO FIN-VAL-IKY-DCC.
           PERFORM VAL-DCC-IND-SCBZ.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCC-IND-SCBZ-LEN IN DCC-FLD
                GO TO FIN-VAL-IKY-DCC.
*% INCLUDE FIN_VAL_IKY_DCC OF 'COLBMDCC.TXT'
       FIN-VAL-IKY-DCC.
           EXIT.
       
       VAL-MKY-DCC SECTION.
       INI-VAL-MKY-DCC.
*% INCLUDE INI_VAL_MKY_DCC OF 'COLBMDCC.TXT'
