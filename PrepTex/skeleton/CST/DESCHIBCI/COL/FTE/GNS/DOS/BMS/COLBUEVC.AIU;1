                GO TO FIN-VAL-NUM-FLD-EVC.
*% INCLUDE FIN_VAL_NUM_FLD_EVC OF 'COLBMEVC.TXT'
       FIN-VAL-NUM-FLD-EVC.
           EXIT.
       
       EDT-KEY-EVC SECTION.
       INI-EDT-KEY-EVC.
           PERFORM EDT-IKY-EVC.
           PERFORM EDT-MKY-EVC.
           PERFORM EDT-AKY-EVC.
*% INCLUDE EDT_KEY_EVC OF 'COLBMEVC.TXT'
       FIN-EDT-KEY-EVC.
           EXIT.
       
       EDT-IKY-EVC SECTION.
       INI-EDT-IKY-EVC.
*% INCLUDE EDT_IKY_EVC OF 'COLBMEVC.TXT'
           IF EVC-IIC-IOPC-LEN IN EVC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EVC-IIC-IOPC-LEN IN EVC-FLD
                GO TO FIN-EDT-IKY-EVC.
           MOVE EVC-IIC-IOPC IN EVC-FLD TO EVC-IIC-IOPC-EDT IN EVC-EDT.
           MOVE EVC-IIC-IOPC-EDT IN EVC-EDT TO 
                EVC-IIC-IOPC-ALF IN EVC-FLD.
*% INCLUDE FIN_EDT_IKY_EVC OF 'COLBMEVC.TXT'
       FIN-EDT-IKY-EVC.
           EXIT.
       
       EDT-MKY-EVC SECTION.
       INI-EDT-MKY-EVC.
*% INCLUDE EDT_MKY_EVC OF 'COLBMEVC.TXT'
*% INCLUDE FIN_EDT_MKY_EVC OF 'COLBMEVC.TXT'
       FIN-EDT-MKY-EVC.
           EXIT.
       
       EDT-AKY-EVC SECTION.
       INI-EDT-AKY-EVC.
*% INCLUDE EDT_AKY_EVC OF 'COLBMEVC.TXT'
           IF EVC-NUM-IEVC-LEN IN EVC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EVC-NUM-IEVC-LEN IN EVC-FLD
                GO TO FIN-EDT-AKY-EVC.
           MOVE EVC-NUM-IEVC IN EVC-FLD TO EVC-NUM-IEVC-EDT IN EVC-EDT.
           MOVE EVC-NUM-IEVC-EDT IN EVC-EDT TO 
                EVC-NUM-IEVC-ALF IN EVC-FLD.
*% INCLUDE FIN_EDT_AKY_EVC OF 'COLBMEVC.TXT'
       FIN-EDT-AKY-EVC.
           EXIT.
       
       EDT-FLD-EVC SECTION.
       INI-EDT-FLD-EVC.
*% INCLUDE EDT_FLD_EVC OF 'COLBMEVC.TXT'
           IF FRM-NUM-DURV-LEN IN EVC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-DURV-LEN IN EVC-FLD
                GO TO FIN-EDT-FLD-EVC.
           MOVE FRM-NUM-DURV IN EVC-FLD TO FRM-NUM-DURV-EDT IN EVC-EDT.
           MOVE FRM-NUM-DURV-EDT IN EVC-EDT TO 
                FRM-NUM-DURV-ALF IN EVC-FLD.
           IF EVC-NUM-IDLC-LEN IN EVC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EVC-NUM-IDLC-LEN IN EVC-FLD
                GO TO FIN-EDT-FLD-EVC.
           MOVE EVC-NUM-IDLC IN EVC-FLD TO EVC-NUM-IDLC-EDT IN EVC-EDT.
           MOVE EVC-NUM-IDLC-EDT IN EVC-EDT TO 
                EVC-NUM-IDLC-ALF IN EVC-FLD.
           IF EVC-NUM-VIGU-LEN IN EVC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EVC-NUM-VIGU-LEN IN EVC-FLD
                GO TO FIN-EDT-FLD-EVC.
           MOVE EVC-NUM-VIGU IN EVC-FLD TO EVC-NUM-VIGU-EDT IN EVC-EDT.
           MOVE EVC-NUM-VIGU-EDT IN EVC-EDT TO 
                EVC-NUM-VIGU-ALF IN EVC-FLD.
*% RREG = "EVC"
*% RREG_FRM = "EVC"
*% NOMB = "FPVC"
           IF EVC-NUM-DPVE-LEN IN EVC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EVC-NUM-DPVE-LEN IN EVC-FLD
                GO TO FIN-EDT-FLD-EVC.
           MOVE EVC-NUM-DPVE IN EVC-FLD TO EVC-NUM-DPVE-EDT IN EVC-EDT.
           MOVE EVC-NUM-DPVE-EDT IN EVC-EDT TO 
                EVC-NUM-DPVE-ALF IN EVC-FLD.
           IF FRM-NUM-SPVE-LEN IN EVC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-SPVE-LEN IN EVC-FLD
                GO TO FIN-EDT-FLD-EVC.
           MOVE FRM-NUM-SPVE IN EVC-FLD TO FRM-NUM-SPVE-EDT IN EVC-EDT.
           MOVE FRM-NUM-SPVE-EDT IN EVC-EDT TO 
                FRM-NUM-SPVE-ALF IN EVC-FLD.
           IF EVC-NUM-DESD-LEN IN EVC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EVC-NUM-DESD-LEN IN EVC-FLD
                GO TO FIN-EDT-FLD-EVC.
           MOVE EVC-NUM-DESD IN EVC-FLD TO EVC-NUM-DESD-EDT IN EVC-EDT.
           MOVE EVC-NUM-DESD-EDT IN EVC-EDT TO 
                EVC-NUM-DESD-ALF IN EVC-FLD.
           IF EVC-VAL-VENC-LEN IN EVC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EVC-VAL-VENC-LEN IN EVC-FLD
                GO TO FIN-EDT-FLD-EVC.
           MOVE EVC-VAL-VENC IN EVC-FLD TO EVC-VAL-VENC-EDT IN EVC-EDT.
           MOVE EVC-VAL-VENC-EDT IN EVC-EDT TO 
                EVC-VAL-VENC-ALF IN EVC-FLD.
           IF EVC-NUM-SEVE-LEN IN EVC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EVC-NUM-SEVE-LEN IN EVC-FLD
                GO TO FIN-EDT-FLD-EVC.
           MOVE EVC-NUM-SEVE IN EVC-FLD TO EVC-NUM-SEVE-EDT IN EVC-EDT.
           MOVE EVC-NUM-SEVE-EDT IN EVC-EDT TO 
                EVC-NUM-SEVE-ALF IN EVC-FLD.
           IF EVC-SGV-FVAR-LEN IN EVC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EVC-SGV-FVAR-LEN IN EVC-FLD
                GO TO FIN-EDT-FLD-EVC.
           MOVE EVC-SGV-FVAR IN EVC-FLD TO EVC-SGV-FVAR-EDT IN EVC-EDT.
           MOVE EVC-SGV-FVAR-EDT IN EVC-EDT TO 
                EVC-SGV-FVAR-ALF IN EVC-FLD.
*% INCLUDE FIN_EDT_FLD_EVC OF 'COLBMEVC.TXT'
       FIN-EDT-FLD-EVC.
           EXIT.
