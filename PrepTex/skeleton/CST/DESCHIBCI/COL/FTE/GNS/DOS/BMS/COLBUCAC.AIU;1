*% FRM = "CAC"
      *
      * COLBUCAC
      * Version 3.0.1 Nov-2005
      * 30-Mar-12 12:23 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMCAC.TXT'
       SET-TAG-COLBUCAC SECTION.
       INI-SET-TAG-COLBUCAC.
           MOVE
           '<IDNMOD NOM=$COLBUCAC$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$30-Mar-12 12:23 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCAC.
           EXIT.
           
       RET-ALL-CAC SECTION.
       INI-RET-ALL-CAC.
*% INCLUDE INI_RET_ALL_CAC OF 'COLBMCAC.TXT'
           PERFORM KEY-ALL-CAC.
           PERFORM RET-OPC-CAC.
           PERFORM RET-DBC-CAC.
           PERFORM RET-ITR-CAC.
           PERFORM RET-VEN-CAC.
*% INCLUDE FIN_RET_ALL_CAC OF 'COLBMCAC.TXT'
       FIN-RET-ALL-CAC.
           EXIT.
       
       RET-OPC-CAC SECTION.
       INI-RET-OPC-CAC.
*% INCLUDE INI_RET_OPC_CAC OF 'COLBMCAC.TXT'
           MOVE OPC-COD-VCOC IN CAC-FLD TO OPC-COD-VCOC IN OPC.
           MOVE OPC-COD-TOOC IN CAC-FLD TO OPC-COD-TOOC IN OPC.
           MOVE OPC-COD-AOOC IN CAC-FLD TO OPC-COD-AOOC IN OPC.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE OPC-NUM-DCOL IN CAC-FLD TO OPC-NUM-DCOL IN OPC.
           MOVE OPC-NUM-MCOL IN CAC-FLD TO OPC-NUM-MCOL IN OPC.
           MOVE OPC-NUM-SCOL IN CAC-FLD TO OPC-NUM-SCOL IN OPC.
           MOVE OPC-NUM-ACOL IN CAC-FLD TO OPC-NUM-ACOL IN OPC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE OPC-VAL-CRED IN CAC-FLD TO OPC-VAL-CRED IN OPC.
           MOVE OPC-VAL-SCRE IN CAC-FLD TO OPC-VAL-SCRE IN OPC.
           MOVE OPC-SGV-LIQU IN CAC-FLD TO OPC-SGV-LIQU IN OPC.
*% INCLUDE FIN_RET_OPC_CAC OF 'COLBMCAC.TXT'
       FIN-RET-OPC-CAC.
           EXIT.
       
       RET-DBC-CAC SECTION.
       INI-RET-DBC-CAC.
*% INCLUDE INI_RET_DBC_CAC OF 'COLBMCAC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN CAC-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN CAC-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN CAC-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN CAC-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-GLS-NOMC IN CAC-FLD TO DBC-GLS-NOMC IN DBC.
*% INCLUDE FIN_RET_DBC_CAC OF 'COLBMCAC.TXT'
       FIN-RET-DBC-CAC.
           EXIT.
       
       RET-ITR-CAC SECTION.
       INI-RET-ITR-CAC.
*% INCLUDE INI_RET_ITR_CAC OF 'COLBMCAC.TXT'
           MOVE ITR-SGV-TINT IN CAC-FLD TO ITR-SGV-TINT IN ITR.
*% INCLUDE FIN_RET_ITR_CAC OF 'COLBMCAC.TXT'
       FIN-RET-ITR-CAC.
           EXIT.
       
       RET-VEN-CAC SECTION.
       INI-RET-VEN-CAC.
*% INCLUDE INI_RET_VEN_CAC OF 'COLBMCAC.TXT'
           MOVE VEN-NUM-IVCT IN CAC-FLD TO VEN-NUM-IVCT IN VEN.
*% RREG = "VEN"
*% RREG_FRM = "VEN"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE VEN-NUM-DVEN IN CAC-FLD TO VEN-NUM-DVEN IN VEN.
           MOVE VEN-NUM-MVEN IN CAC-FLD TO VEN-NUM-MVEN IN VEN.
           MOVE VEN-NUM-SVEN IN CAC-FLD TO VEN-NUM-SVEN IN VEN.
           MOVE VEN-NUM-AVEN IN CAC-FLD TO VEN-NUM-AVEN IN VEN.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE VEN-VAL-CAPI IN CAC-FLD TO VEN-VAL-CAPI IN VEN.
           MOVE VEN-VAL-FINA IN CAC-FLD TO VEN-VAL-FINA IN VEN.
*% INCLUDE FIN_RET_VEN_CAC OF 'COLBMCAC.TXT'
       FIN-RET-VEN-CAC.
           EXIT.
       
       KEY-ALL-CAC SECTION.
       INI-KEY-ALL-CAC.
*% INCLUDE INI_KEY_ALL_CAC OF 'COLBMCAC.TXT'
           PERFORM KEY-OPC-CAC.
           PERFORM KEY-DBC-CAC.
           PERFORM KEY-ITR-CAC.
           PERFORM KEY-VEN-CAC.
*% INCLUDE FIN_KEY_ALL_CAC OF 'COLBMCAC.TXT'
       FIN-KEY-ALL-CAC.
           EXIT.
       
       KEY-OPC-CAC SECTION.
       INI-KEY-OPC-CAC.
*% INCLUDE INI_KEY_OPC_CAC OF 'COLBMCAC.TXT'
           MOVE OPC-CAI-IOPC IN CAC-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE OPC-IIC-IOPC IN CAC-FLD TO OPC-IIC-IOPC IN OPC.
*% INCLUDE FIN_KEY_OPC_CAC OF 'COLBMCAC.TXT'
       FIN-KEY-OPC-CAC.
           EXIT.
       
       KEY-DBC-CAC SECTION.
       INI-KEY-DBC-CAC.
*% INCLUDE INI_KEY_DBC_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_KEY_DBC_CAC OF 'COLBMCAC.TXT'
       FIN-KEY-DBC-CAC.
           EXIT.
       
       KEY-ITR-CAC SECTION.
       INI-KEY-ITR-CAC.
*% INCLUDE INI_KEY_ITR_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_KEY_ITR_CAC OF 'COLBMCAC.TXT'
       FIN-KEY-ITR-CAC.
           EXIT.
       
       KEY-VEN-CAC SECTION.
       INI-KEY-VEN-CAC.
*% INCLUDE INI_KEY_VEN_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_KEY_VEN_CAC OF 'COLBMCAC.TXT'
       FIN-KEY-VEN-CAC.
           EXIT.
       
       SCR-CLRA-CAC SECTION.
       INI-SCR-CLRA-CAC.
*% INCLUDE INI_SCR_CLRA_CAC OF 'COLBMCAC.TXT'
           PERFORM CLR-KEY-CAC.
           PERFORM CLR-FLD-CAC.
*% INCLUDE FIN_SCR_CLRA_CAC OF 'COLBMCAC.TXT'
       FIN-SCR-CLRA-CAC.
           EXIT.
       
       CLR-KEY-CAC SECTION.
       INI-CLR-KEY-CAC.
*% INCLUDE INI_CLR_KEY_CAC OF 'COLBMCAC.TXT'
           MOVE SPACES TO OPC-CAI-IOPC IN CAC-FLD.
           MOVE ZEROES TO OPC-IIC-IOPC IN CAC-FLD.
           MOVE ZEROES TO FRM-NUM-VDES IN CAC-FLD.
           MOVE ZEROES TO FRM-NUM-VHAS IN CAC-FLD.
*% INCLUDE FIN_CLR_KEY_CAC OF 'COLBMCAC.TXT'
       FIN-CLR-KEY-CAC.
           EXIT.
       
       CLR-FLD-CAC SECTION.
       INI-CLR-FLD-CAC.
*% INCLUDE INI_CLR_FLD_CAC OF 'COLBMCAC.TXT'
           MOVE SPACES TO FRM-GLS-VCOC IN CAC-FLD.
           MOVE SPACES TO FRM-GLS-AOOC IN CAC-FLD.
           MOVE SPACES TO FRM-GLS-TINT IN CAC-FLD.
           MOVE SPACES TO FRM-IND-CANC IN CAC-FLD.
           MOVE ZEROES TO FRM-VAL-INTC IN CAC-FLD.
           MOVE ZEROES TO FRM-NUM-ICAN IN CAC-FLD(1).
           MOVE ZEROES TO FRM-NUM-ICAN IN CAC-FLD(2).
           MOVE ZEROES TO FRM-NUM-ICAN IN CAC-FLD(3).
           MOVE ZEROES TO FRM-NUM-ICAN IN CAC-FLD(4).
           MOVE ZEROES TO FRM-SGV-CAPI IN CAC-FLD(1).
           MOVE ZEROES TO FRM-SGV-CAPI IN CAC-FLD(2).
           MOVE ZEROES TO FRM-SGV-CAPI IN CAC-FLD(3).
           MOVE ZEROES TO FRM-SGV-CAPI IN CAC-FLD(4).
           MOVE ZEROES TO FRM-SGV-INTE IN CAC-FLD(1).
           MOVE ZEROES TO FRM-SGV-INTE IN CAC-FLD(2).
           MOVE ZEROES TO FRM-SGV-INTE IN CAC-FLD(3).
           MOVE ZEROES TO FRM-SGV-INTE IN CAC-FLD(4).
           MOVE ZEROES TO FRM-SGV-INTP IN CAC-FLD(1).
           MOVE ZEROES TO FRM-SGV-INTP IN CAC-FLD(2).
           MOVE ZEROES TO FRM-SGV-INTP IN CAC-FLD(3).
           MOVE ZEROES TO FRM-SGV-INTP IN CAC-FLD(4).
           MOVE SPACES TO FRM-COD-CANC IN CAC-FLD(1).
           MOVE SPACES TO FRM-COD-CANC IN CAC-FLD(2).
           MOVE SPACES TO FRM-COD-CANC IN CAC-FLD(3).
           MOVE SPACES TO FRM-COD-CANC IN CAC-FLD(4).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FCAN IN CAC-FLD(1).
           MOVE ZEROES TO FRM-FEC-FCAN IN CAC-FLD(2).
           MOVE ZEROES TO FRM-FEC-FCAN IN CAC-FLD(3).
           MOVE ZEROES TO FRM-FEC-FCAN IN CAC-FLD(4).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-SGV-TPAG IN CAC-FLD(1).
           MOVE ZEROES TO FRM-SGV-TPAG IN CAC-FLD(2).
           MOVE ZEROES TO FRM-SGV-TPAG IN CAC-FLD(3).
           MOVE ZEROES TO FRM-SGV-TPAG IN CAC-FLD(4).
           MOVE ZEROES TO FRM-SGV-VREN IN CAC-FLD(1).
           MOVE ZEROES TO FRM-SGV-VREN IN CAC-FLD(2).
           MOVE ZEROES TO FRM-SGV-VREN IN CAC-FLD(3).
           MOVE ZEROES TO FRM-SGV-VREN IN CAC-FLD(4).
           MOVE ZEROES TO FRM-SGV-SEGU IN CAC-FLD(1).
           MOVE ZEROES TO FRM-SGV-SEGU IN CAC-FLD(2).
           MOVE ZEROES TO FRM-SGV-SEGU IN CAC-FLD(3).
           MOVE ZEROES TO FRM-SGV-SEGU IN CAC-FLD(4).
           MOVE SPACES TO FRM-COD-STAT IN CAC-FLD(1).
           MOVE SPACES TO FRM-COD-STAT IN CAC-FLD(2).
           MOVE SPACES TO FRM-COD-STAT IN CAC-FLD(3).
           MOVE SPACES TO FRM-COD-STAT IN CAC-FLD(4).
           PERFORM CLR-OPC-CAC.
           PERFORM CLR-DBC-CAC.
           PERFORM CLR-ITR-CAC.
           PERFORM CLR-VEN-CAC.
*% INCLUDE FIN_CLR_FLD_CAC OF 'COLBMCAC.TXT'
       FIN-CLR-FLD-CAC.
           EXIT.
       
       CLR-OPC-CAC SECTION.
       INI-CLR-OPC-CAC.
*% INCLUDE INI_CLR_OPC_CAC OF 'COLBMCAC.TXT'
           MOVE SPACES TO OPC-COD-VCOC IN CAC-FLD.
           MOVE SPACES TO OPC-COD-TOOC IN CAC-FLD.
           MOVE SPACES TO OPC-COD-AOOC IN CAC-FLD.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO OPC-FEC-FCOL IN CAC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO OPC-VAL-CRED IN CAC-FLD.
           MOVE ZEROES TO OPC-VAL-SCRE IN CAC-FLD.
           MOVE ZEROES TO OPC-SGV-LIQU IN CAC-FLD.
*% INCLUDE FIN_CLR_OPC_CAC OF 'COLBMCAC.TXT'
       FIN-CLR-OPC-CAC.
           EXIT.
       
       CLR-DBC-CAC SECTION.
       INI-CLR-DBC-CAC.
*% INCLUDE INI_CLR_DBC_CAC OF 'COLBMCAC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN CAC-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN CAC-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN CAC-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN CAC-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO DBC-GLS-NOMC IN CAC-FLD.
*% INCLUDE FIN_CLR_DBC_CAC OF 'COLBMCAC.TXT'
       FIN-CLR-DBC-CAC.
           EXIT.
       
       CLR-ITR-CAC SECTION.
       INI-CLR-ITR-CAC.
*% INCLUDE INI_CLR_ITR_CAC OF 'COLBMCAC.TXT'
           MOVE ZEROES TO ITR-SGV-TINT IN CAC-FLD.
*% INCLUDE FIN_CLR_ITR_CAC OF 'COLBMCAC.TXT'
       FIN-CLR-ITR-CAC.
           EXIT.
       
       CLR-VEN-CAC SECTION.
       INI-CLR-VEN-CAC.
*% INCLUDE INI_CLR_VEN_CAC OF 'COLBMCAC.TXT'
           MOVE ZEROES TO VEN-NUM-IVCT IN CAC-FLD.
*% RREG = "VEN"
*% RREG_FRM = "VEN"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO VEN-FEC-FVEN IN CAC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO VEN-VAL-CAPI IN CAC-FLD.
           MOVE ZEROES TO VEN-VAL-FINA IN CAC-FLD.
*% INCLUDE FIN_CLR_VEN_CAC OF 'COLBMCAC.TXT'
       FIN-CLR-VEN-CAC.
           EXIT.
       
       FST-KEY-CAC SECTION.
       INI-FST-KEY-CAC.
*% INCLUDE INI_FST_KEY_CAC OF 'COLBMCAC.TXT'
           MOVE -1 TO OPC-CAI-IOPC-LEN IN CAC-FLD.
*% INCLUDE FIN_FST_KEY_CAC OF 'COLBMCAC.TXT'
       FIN-FST-KEY-CAC.
           EXIT.
       
       FST-FLD-CAC SECTION.
       INI-FST-FLD-CAC.
*% INCLUDE INI_FST_FLD_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_FST_FLD_CAC OF 'COLBMCAC.TXT'
       FIN-FST-FLD-CAC.
           EXIT.
       
       PRO-KEY-CAC SECTION.
       INI-PRO-KEY-CAC.
*% INCLUDE INI_PRO_KEY_CAC OF 'COLBMCAC.TXT'
           MOVE FRM-CPIM-AEY TO FRM-NUM-VDES-ATR IN CAC-FLD.
           MOVE FRM-CPIM-AEY TO FRM-NUM-VHAS-ATR IN CAC-FLD.
           PERFORM PRO-MKY-CAC.
*% INCLUDE FIN_PRO_KEY_CAC OF 'COLBMCAC.TXT'
       FIN-PRO-KEY-CAC.
           EXIT.
       
       PRO-MKY-CAC SECTION.
       INI-PRO-MKY-CAC.
*% INCLUDE INI_PRO_MKY_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_PRO_MKY_CAC OF 'COLBMCAC.TXT'
       FIN-PRO-MKY-CAC.
           EXIT.
       
       PRO-FLD-CAC SECTION.
       INI-PRO-FLD-CAC.
*% INCLUDE INI_PRO_FLD_CAC OF 'COLBMCAC.TXT'
           PERFORM PRO-OPC-CAC.
           PERFORM PRO-DBC-CAC.
           PERFORM PRO-ITR-CAC.
           PERFORM PRO-VEN-CAC.
*% INCLUDE FIN_PRO_FLD_CAC OF 'COLBMCAC.TXT'
       FIN-PRO-FLD-CAC.
           EXIT.
       
       PRO-OPC-CAC SECTION.
       INI-PRO-OPC-CAC.
*% INCLUDE INI_PRO_OPC_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_PRO_OPC_CAC OF 'COLBMCAC.TXT'
       FIN-PRO-OPC-CAC.
           EXIT.
       
