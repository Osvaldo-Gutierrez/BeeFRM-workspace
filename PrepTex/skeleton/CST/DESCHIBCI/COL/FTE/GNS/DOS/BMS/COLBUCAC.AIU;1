       INI-PRO-DBC-CAC.
*% INCLUDE INI_PRO_DBC_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_PRO_DBC_CAC OF 'COLBMCAC.TXT'
       FIN-PRO-DBC-CAC.
           EXIT.
       
       PRO-ITR-CAC SECTION.
       INI-PRO-ITR-CAC.
*% INCLUDE INI_PRO_ITR_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_PRO_ITR_CAC OF 'COLBMCAC.TXT'
       FIN-PRO-ITR-CAC.
           EXIT.
       
       PRO-VEN-CAC SECTION.
       INI-PRO-VEN-CAC.
*% INCLUDE INI_PRO_VEN_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_PRO_VEN_CAC OF 'COLBMCAC.TXT'
       FIN-PRO-VEN-CAC.
           EXIT.
       
       UNP-KEY-CAC SECTION.
       INI-UNP-KEY-CAC.
*% INCLUDE INI_UNP_KEY_CAC OF 'COLBMCAC.TXT'
           MOVE FRM-CPIM-UBY TO FRM-NUM-VDES-ATR IN CAC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-NUM-VHAS-ATR IN CAC-FLD.
           PERFORM UNP-IKY-CAC.
           PERFORM UNP-AKY-CAC.
           PERFORM UNP-MKY-CAC.
*% INCLUDE FIN_UNP_KEY_CAC OF 'COLBMCAC.TXT'
       FIN-UNP-KEY-CAC.
           EXIT.
       
       UNP-IKY-CAC SECTION.
       INI-UNP-IKY-CAC.
*% INCLUDE INI_UNP_IKY_CAC OF 'COLBMCAC.TXT'
           MOVE FRM-CPIM-UBY TO OPC-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO OPC-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-NUM-VDES-ATR.
           MOVE FRM-CPIM-UBY TO FRM-NUM-VHAS-ATR.
*% INCLUDE FIN_UNP_IKY_CAC OF 'COLBMCAC.TXT'
       FIN-UNP-IKY-CAC.
           EXIT.
       
       UNP-MKY-CAC SECTION.
       INI-UNP-MKY-CAC.
*% INCLUDE INI_UNP_MKY_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_UNP_MKY_CAC OF 'COLBMCAC.TXT'
       FIN-UNP-MKY-CAC.
           EXIT.
       
       UNP-AKY-CAC SECTION.
       INI-UNP-AKY-CAC.
*% INCLUDE INI_UNP_AKY_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_UNP_AKY_CAC OF 'COLBMCAC.TXT'
       FIN-UNP-AKY-CAC.
           EXIT.
       
       UNP-FLD-CAC SECTION.
       INI-UNP-FLD-CAC.
*% INCLUDE INI_UNP_FLD_CAC OF 'COLBMCAC.TXT'
           PERFORM UNP-OPC-CAC.
           PERFORM UNP-DBC-CAC.
           PERFORM UNP-ITR-CAC.
           PERFORM UNP-VEN-CAC.
*% INCLUDE FIN_UNP_FLD_CAC OF 'COLBMCAC.TXT'
       FIN-UNP-FLD-CAC.
           EXIT.
       
       UNP-OPC-CAC SECTION.
       INI-UNP-OPC-CAC.
*% INCLUDE INI_UNP_OPC_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_UNP_OPC_CAC OF 'COLBMCAC.TXT'
       FIN-UNP-OPC-CAC.
           EXIT.
       
       UNP-DBC-CAC SECTION.
       INI-UNP-DBC-CAC.
*% INCLUDE INI_UNP_DBC_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_UNP_DBC_CAC OF 'COLBMCAC.TXT'
       FIN-UNP-DBC-CAC.
           EXIT.
       
       UNP-ITR-CAC SECTION.
       INI-UNP-ITR-CAC.
*% INCLUDE INI_UNP_ITR_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_UNP_ITR_CAC OF 'COLBMCAC.TXT'
       FIN-UNP-ITR-CAC.
           EXIT.
       
       UNP-VEN-CAC SECTION.
       INI-UNP-VEN-CAC.
*% INCLUDE INI_UNP_VEN_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_UNP_VEN_CAC OF 'COLBMCAC.TXT'
       FIN-UNP-VEN-CAC.
           EXIT.
       
       VAL-MKY-CAC SECTION.
       INI-VAL-MKY-CAC.
*% INCLUDE INI_VAL_MKY_CAC OF 'COLBMCAC.TXT'
*% INCLUDE FIN_VAL_MKY_CAC OF 'COLBMCAC.TXT'
       FIN-VAL-MKY-CAC.
           EXIT.
       
       VAL-FLD-CAC SECTION.
       INI-VAL-FLD-CAC.
*% INCLUDE INI_VAL_FLD_CAC OF 'COLBMCAC.TXT'
           PERFORM VAL-OPC-COD-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-VCOC-LEN IN CAC-FLD
                GO TO FIN-VAL-FLD-CAC.
           PERFORM VAL-OPC-COD-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-TOOC-LEN IN CAC-FLD
                GO TO FIN-VAL-FLD-CAC.
           PERFORM VAL-OPC-COD-AOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-AOOC-LEN IN CAC-FLD
                GO TO FIN-VAL-FLD-CAC.
           PERFORM VAL-FRM-GLS-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-VCOC-LEN IN CAC-FLD
                GO TO FIN-VAL-FLD-CAC.
           PERFORM VAL-FRM-GLS-AOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-AOOC-LEN IN CAC-FLD
                GO TO FIN-VAL-FLD-CAC.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * VRF-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * VRF-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DBC-IND-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-IND-ICLI-LEN IN CAC-FLD
                GO TO FIN-VAL-FLD-CAC.
           PERFORM VAL-DBC-GLS-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-GLS-ICLI-LEN IN CAC-FLD
                GO TO FIN-VAL-FLD-CAC.
           PERFORM VAL-DBC-NUM-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-NUM-ICLI-LEN IN CAC-FLD
                GO TO FIN-VAL-FLD-CAC.
           PERFORM VAL-DBC-VRF-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-VRF-ICLI-LEN IN CAC-FLD
                GO TO FIN-VAL-FLD-CAC.
      * IDC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DBC-GLS-NOMC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DBC-GLS-NOMC-LEN IN CAC-FLD
                GO TO FIN-VAL-FLD-CAC.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-OPC-FEC-FCOL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                OPC-FEC-FCOL-LEN IN CAC-FLD
                GO TO FIN-VAL-FLD-CAC.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-OPC-VAL-CRED.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-VAL-CRED-LEN IN CAC-FLD
                GO TO FIN-VAL-FLD-CAC.
           PERFORM VAL-OPC-VAL-SCRE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-VAL-SCRE-LEN IN CAC-FLD
                GO TO FIN-VAL-FLD-CAC.
           PERFORM VAL-OPC-SGV-LIQU.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-SGV-LIQU-LEN IN CAC-FLD
                GO TO FIN-VAL-FLD-CAC.
           PERFORM VAL-FRM-GLS-TINT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TINT-LEN IN CAC-FLD
                GO TO FIN-VAL-FLD-CAC.
           PERFORM VAL-ITR-SGV-TINT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO ITR-SGV-TINT-LEN IN CAC-FLD
                GO TO FIN-VAL-FLD-CAC.
           PERFORM VAL-VEN-NUM-IVCT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO VEN-NUM-IVCT-LEN IN CAC-FLD
                GO TO FIN-VAL-FLD-CAC.
           PERFORM VAL-FRM-IND-CANC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IND-CANC-LEN IN CAC-FLD
                GO TO FIN-VAL-FLD-CAC.
*% RREG = "VEN"
*% RREG_FRM = "VEN"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-VEN-FEC-FVEN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                VEN-FEC-FVEN-LEN IN CAC-FLD
                GO TO FIN-VAL-FLD-CAC.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-VAL-INTC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-INTC-LEN IN CAC-FLD
                GO TO FIN-VAL-FLD-CAC.
           PERFORM VAL-VEN-VAL-CAPI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO VEN-VAL-CAPI-LEN IN CAC-FLD
                GO TO FIN-VAL-FLD-CAC.
           PERFORM VAL-VEN-VAL-FINA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO VEN-VAL-FINA-LEN IN CAC-FLD
                GO TO FIN-VAL-FLD-CAC.
           PERFORM VAL-FRM-NUM-ICAN
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 4
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-NUM-ICAN-LEN IN CAC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CAC.
           PERFORM VAL-FRM-SGV-CAPI
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 4
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-CAPI-LEN IN CAC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CAC.
           PERFORM VAL-FRM-SGV-INTE
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 4
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-INTE-LEN IN CAC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CAC.
           PERFORM VAL-FRM-SGV-INTP
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 4
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-INTP-LEN IN CAC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CAC.
           PERFORM VAL-FRM-COD-CANC
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 4
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-COD-CANC-LEN IN CAC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CAC.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAN"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FCAN VARYING FRM-IFLD
                FROM 1 BY 1 UNTIL FRM-IFLD > 4 OR
                FRM-SUAR = FRM-SUAR-MAL OR
                FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FCAN-LEN IN CAC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CAC.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-SGV-TPAG
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 4
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-TPAG-LEN IN CAC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CAC.
           PERFORM VAL-FRM-SGV-VREN
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 4
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-VREN-LEN IN CAC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CAC.
           PERFORM VAL-FRM-SGV-SEGU
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 4
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-SEGU-LEN IN CAC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CAC.
           PERFORM VAL-FRM-COD-STAT
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 4
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-COD-STAT-LEN IN CAC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CAC.
*% INCLUDE FIN_VAL_FLD_CAC OF 'COLBMCAC.TXT'
       FIN-VAL-FLD-CAC.
           EXIT.
       
