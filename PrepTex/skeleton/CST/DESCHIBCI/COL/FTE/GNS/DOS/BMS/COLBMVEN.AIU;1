      * 23-Mar-94 12:51 PM
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
       PUT-ALL-VEN SECTION.
       INI-PUT-ALL-VEN.
*% INCLUDE INI_PUT_ALL_VEN OF 'COLBMVEN.TXT'
           PERFORM PUT-OPC-VEN.
           PERFORM PUT-VEN-VEN.
*% INCLUDE FIN_PUT_ALL_VEN OF 'COLBMVEN.TXT'
       FIN-PUT-ALL-VEN.
           EXIT.
       
       PUT-OPC-VEN SECTION.
       INI-PUT-OPC-VEN.
*% INCLUDE INI_PUT_OPC_VEN OF 'COLBMVEN.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-VEN.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-VEN
               GO TO FIN-PUT-OPC-VEN.
           MOVE OPC-MSC-TVOC IN OPC TO OPC-MSC-TVOC IN VEN-FLD.
           MOVE OPC-COD-TOOC IN OPC TO OPC-COD-TOOC IN VEN-FLD.
           MOVE OPC-COD-OFOC IN OPC TO OPC-COD-OFOC IN VEN-FLD.
           MOVE OPC-COD-VCOC IN OPC TO OPC-COD-VCOC IN VEN-FLD.
*% BUF = "VEN-FLD"
*% SIS = "TAB"
*% TAB = "TVAL"
*% FLD = "OPC-MSC-TVOC"
*% GLS = "FRM-GLS-TVOC"
*% TIENE_GLOSA = TRUE
*% LGL = 13
*% INCLUDE 'BUSMSC'
*% BUF = "VEN-FLD"
*% SIS = "TAB"
*% TAB = "TIO "
*% FLD = "OPC-COD-TOOC"
*% GLS = "FRM-GLS-TOOC"
*% TIENE_GLOSA = TRUE
*% LGL = 14
*% INCLUDE 'BUSTAB'
*% BUF = "VEN-FLD"
*% SIS = "TAB"
*% TAB = "OFI "
*% FLD = "OPC-COD-OFOC"
*% GLS = "FRM-GLS-OFOC"
*% TIENE_GLOSA = TRUE
*% LGL = 14
*% INCLUDE 'BUSOFI          '
*% BUF = "VEN-FLD"
*% SIS = "TAB"
*% TAB = "VLR "
*% FLD = "OPC-COD-VCOC"
*% GLS = "FRM-GLS-VCOC"
*% TIENE_GLOSA = TRUE
*% LGL = 30
*% INCLUDE 'BUSTAB'
*% INCLUDE FIN_PUT_OPC_VEN OF 'COLBMVEN.TXT'
       FIN-PUT-OPC-VEN.
           EXIT.
       
       PUT-VEN-VEN SECTION.
       INI-PUT-VEN-VEN.
*% INCLUDE INI_PUT_VEN_VEN OF 'COLBMVEN.TXT'
           IF PGM-STAT-VEN-NUL
               GO TO FIN-PUT-VEN-VEN.
           IF NOT PGM-STAT-VEN-OKS
               PERFORM CLR-VEN-VEN
               GO TO FIN-PUT-VEN-VEN.
           MOVE VEN-CAI-IOPC IN VEN TO VEN-CAI-IOPC IN VEN-FLD.
           MOVE VEN-IIC-IOPC IN VEN TO VEN-IIC-IOPC IN VEN-FLD.
           MOVE VEN-NUM-IVCT IN VEN TO VEN-NUM-IVCT IN VEN-FLD.
           MOVE VEN-NUM-IVEN IN VEN TO VEN-NUM-IVEN IN VEN-FLD.
           MOVE VEN-NUM-IDLC IN VEN TO VEN-NUM-IDLC IN VEN-FLD.
           MOVE VEN-NUM-DVEN IN VEN TO VEN-NUM-DVEN IN VEN-FLD.
           MOVE VEN-NUM-MVEN IN VEN TO VEN-NUM-MVEN IN VEN-FLD.
           MOVE VEN-NUM-SVEN IN VEN TO VEN-NUM-SVEN IN VEN-FLD.
           MOVE VEN-NUM-AVEN IN VEN TO VEN-NUM-AVEN IN VEN-FLD.
           MOVE VEN-VAL-CAPI IN VEN TO VEN-VAL-CAPI IN VEN-FLD.
           MOVE VEN-NUM-DIMV IN VEN TO VEN-NUM-DIMV IN VEN-FLD.
           MOVE VEN-NUM-MIMV IN VEN TO VEN-NUM-MIMV IN VEN-FLD.
           MOVE VEN-NUM-SIMV IN VEN TO VEN-NUM-SIMV IN VEN-FLD.
           MOVE VEN-NUM-AIMV IN VEN TO VEN-NUM-AIMV IN VEN-FLD.
           MOVE VEN-VAL-SCAP IN VEN TO VEN-VAL-SCAP IN VEN-FLD.
           MOVE VEN-IND-SCTB IN VEN TO VEN-IND-SCTB IN VEN-FLD.
           MOVE VEN-VAL-IDNI IN VEN TO VEN-VAL-IDNI IN VEN-FLD.
           MOVE VEN-IND-SCBZ IN VEN TO VEN-IND-SCBZ IN VEN-FLD.
           MOVE VEN-VAL-FINA IN VEN TO VEN-VAL-FINA IN VEN-FLD.
           MOVE VEN-IND-CAUT IN VEN TO VEN-IND-CAUT IN VEN-FLD.
           MOVE VEN-VAL-SFIN IN VEN TO VEN-VAL-SFIN IN VEN-FLD.
*% INCLUDE FIN_PUT_VEN_VEN OF 'COLBMVEN.TXT'
       FIN-PUT-VEN-VEN.
           EXIT.
      * 23-Mar-94 12:51 PM
*% INI_LBL = TRUE
*% PGM_BU = TRUE
       RET-ALL-VEN SECTION.
       INI-RET-ALL-VEN.
*% INCLUDE INI_RET_ALL_VEN OF 'COLBMVEN.TXT'
           PERFORM KEY-ALL-VEN.
           PERFORM RET-OPC-VEN.
           PERFORM RET-VEN-VEN.
*% INCLUDE FIN_RET_ALL_VEN OF 'COLBMVEN.TXT'
       FIN-RET-ALL-VEN.
           EXIT.
       
       RET-OPC-VEN SECTION.
       INI-RET-OPC-VEN.
*% INCLUDE INI_RET_OPC_VEN OF 'COLBMVEN.TXT'
           MOVE OPC-MSC-TVOC IN VEN-FLD TO OPC-MSC-TVOC IN OPC.
           MOVE OPC-COD-TOOC IN VEN-FLD TO OPC-COD-TOOC IN OPC.
           MOVE OPC-COD-OFOC IN VEN-FLD TO OPC-COD-OFOC IN OPC.
           MOVE OPC-COD-VCOC IN VEN-FLD TO OPC-COD-VCOC IN OPC.
*% INCLUDE FIN_RET_OPC_VEN OF 'COLBMVEN.TXT'
       FIN-RET-OPC-VEN.
           EXIT.
       
       RET-VEN-VEN SECTION.
       INI-RET-VEN-VEN.
*% INCLUDE INI_RET_VEN_VEN OF 'COLBMVEN.TXT'
           MOVE VEN-NUM-IDLC IN VEN-FLD TO VEN-NUM-IDLC IN VEN.
           MOVE VEN-NUM-DVEN IN VEN-FLD TO VEN-NUM-DVEN IN VEN.
           MOVE VEN-NUM-MVEN IN VEN-FLD TO VEN-NUM-MVEN IN VEN.
           MOVE VEN-NUM-SVEN IN VEN-FLD TO VEN-NUM-SVEN IN VEN.
           MOVE VEN-NUM-AVEN IN VEN-FLD TO VEN-NUM-AVEN IN VEN.
           MOVE VEN-VAL-CAPI IN VEN-FLD TO VEN-VAL-CAPI IN VEN.
           MOVE VEN-NUM-DIMV IN VEN-FLD TO VEN-NUM-DIMV IN VEN.
           MOVE VEN-NUM-MIMV IN VEN-FLD TO VEN-NUM-MIMV IN VEN.
           MOVE VEN-NUM-SIMV IN VEN-FLD TO VEN-NUM-SIMV IN VEN.
           MOVE VEN-NUM-AIMV IN VEN-FLD TO VEN-NUM-AIMV IN VEN.
           MOVE VEN-VAL-SCAP IN VEN-FLD TO VEN-VAL-SCAP IN VEN.
           MOVE VEN-IND-SCTB IN VEN-FLD TO VEN-IND-SCTB IN VEN.
           MOVE VEN-VAL-IDNI IN VEN-FLD TO VEN-VAL-IDNI IN VEN.
           MOVE VEN-IND-SCBZ IN VEN-FLD TO VEN-IND-SCBZ IN VEN.
           MOVE VEN-VAL-FINA IN VEN-FLD TO VEN-VAL-FINA IN VEN.
           MOVE VEN-IND-CAUT IN VEN-FLD TO VEN-IND-CAUT IN VEN.
           MOVE VEN-VAL-SFIN IN VEN-FLD TO VEN-VAL-SFIN IN VEN.
*% INCLUDE FIN_RET_VEN_VEN OF 'COLBMVEN.TXT'
       FIN-RET-VEN-VEN.
           EXIT.
       
       KEY-ALL-VEN SECTION.
       INI-KEY-ALL-VEN.
*% INCLUDE INI_KEY_ALL_VEN OF 'COLBMVEN.TXT'
           PERFORM KEY-OPC-VEN.
           PERFORM KEY-VEN-VEN.
*% INCLUDE FIN_KEY_ALL_VEN OF 'COLBMVEN.TXT'
       FIN-KEY-ALL-VEN.
           EXIT.
       
       KEY-OPC-VEN SECTION.
       INI-KEY-OPC-VEN.
*% INCLUDE INI_KEY_OPC_VEN OF 'COLBMVEN.TXT'
*% INCLUDE FIN_KEY_OPC_VEN OF 'COLBMVEN.TXT'
       FIN-KEY-OPC-VEN.
           EXIT.
       
       KEY-VEN-VEN SECTION.
       INI-KEY-VEN-VEN.
*% INCLUDE INI_KEY_VEN_VEN OF 'COLBMVEN.TXT'
           MOVE VEN-CAI-IOPC IN VEN-FLD TO VEN-CAI-IOPC IN VEN.
           MOVE VEN-IIC-IOPC IN VEN-FLD TO VEN-IIC-IOPC IN VEN.
           MOVE VEN-NUM-IVCT IN VEN-FLD TO VEN-NUM-IVCT IN VEN.
           MOVE VEN-NUM-IVEN IN VEN-FLD TO VEN-NUM-IVEN IN VEN.
*% INCLUDE FIN_KEY_VEN_VEN OF 'COLBMVEN.TXT'
       FIN-KEY-VEN-VEN.
           EXIT.
       
       SCR-CLRA-VEN SECTION.
       INI-SCR-CLRA-VEN.
*% INCLUDE INI_SCR_CLRA_VEN OF 'COLBMVEN.TXT'
           PERFORM CLR-KEY-VEN.
           PERFORM CLR-FLD-VEN.
*% INCLUDE FIN_SCR_CLRA_VEN OF 'COLBMVEN.TXT'
       FIN-SCR-CLRA-VEN.
           EXIT.
       
       CLR-KEY-VEN SECTION.
       INI-CLR-KEY-VEN.
*% INCLUDE INI_CLR_KEY_VEN OF 'COLBMVEN.TXT'
           MOVE SPACES TO VEN-CAI-IOPC IN VEN-FLD.
           MOVE ZEROES TO VEN-IIC-IOPC IN VEN-FLD.
           MOVE ZEROES TO VEN-NUM-IVCT IN VEN-FLD.
           MOVE ZEROES TO VEN-NUM-IVEN IN VEN-FLD.
*% INCLUDE FIN_CLR_KEY_VEN OF 'COLBMVEN.TXT'
       FIN-CLR-KEY-VEN.
           EXIT.
       
       CLR-FLD-VEN SECTION.
       INI-CLR-FLD-VEN.
*% INCLUDE INI_CLR_FLD_VEN OF 'COLBMVEN.TXT'
           MOVE SPACES TO FRM-GLS-TVOC IN VEN-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN VEN-FLD.
           MOVE SPACES TO FRM-GLS-OFOC IN VEN-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN VEN-FLD.
           PERFORM CLR-OPC-VEN.
           PERFORM CLR-VEN-VEN.
*% INCLUDE FIN_CLR_FLD_VEN OF 'COLBMVEN.TXT'
       FIN-CLR-FLD-VEN.
           EXIT.
       
       CLR-OPC-VEN SECTION.
       INI-CLR-OPC-VEN.
*% INCLUDE INI_CLR_OPC_VEN OF 'COLBMVEN.TXT'
           MOVE SPACES TO OPC-MSC-TVOC IN VEN-FLD.
           MOVE SPACES TO OPC-COD-TOOC IN VEN-FLD.
           MOVE SPACES TO OPC-COD-OFOC IN VEN-FLD.
           MOVE SPACES TO OPC-COD-VCOC IN VEN-FLD.
*% INCLUDE FIN_CLR_OPC_VEN OF 'COLBMVEN.TXT'
       FIN-CLR-OPC-VEN.
           EXIT.
       
       CLR-VEN-VEN SECTION.
       INI-CLR-VEN-VEN.
*% INCLUDE INI_CLR_VEN_VEN OF 'COLBMVEN.TXT'
           MOVE ZEROES TO VEN-NUM-IDLC IN VEN-FLD.
           MOVE ZEROES TO VEN-FEC-FVEN IN VEN-FLD.
           MOVE ZEROES TO VEN-VAL-CAPI IN VEN-FLD.
           MOVE ZEROES TO VEN-FEC-FIMV IN VEN-FLD.
           MOVE ZEROES TO VEN-VAL-SCAP IN VEN-FLD.
           MOVE SPACES TO VEN-IND-SCTB IN VEN-FLD.
           MOVE ZEROES TO VEN-VAL-IDNI IN VEN-FLD.
           MOVE SPACES TO VEN-IND-SCBZ IN VEN-FLD.
           MOVE ZEROES TO VEN-VAL-FINA IN VEN-FLD.
           MOVE SPACES TO VEN-IND-CAUT IN VEN-FLD.
           MOVE ZEROES TO VEN-VAL-SFIN IN VEN-FLD.
*% INCLUDE FIN_CLR_VEN_VEN OF 'COLBMVEN.TXT'
       FIN-CLR-VEN-VEN.
           EXIT.
       
       FST-KEY-VEN SECTION.
       INI-FST-KEY-VEN.
*% INCLUDE INI_FST_KEY_VEN OF 'COLBMVEN.TXT'
           MOVE -1 TO VEN-CAI-IOPC-LEN IN VEN-FLD.
*% INCLUDE FIN_FST_KEY_VEN OF 'COLBMVEN.TXT'
       FIN-FST-KEY-VEN.
           EXIT.
       
       FST-FLD-VEN SECTION.
       INI-FST-FLD-VEN.
*% INCLUDE INI_FST_FLD_VEN OF 'COLBMVEN.TXT'
