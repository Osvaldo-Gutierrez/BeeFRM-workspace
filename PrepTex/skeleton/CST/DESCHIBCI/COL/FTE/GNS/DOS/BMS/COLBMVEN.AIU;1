       VAL-KEY-VEN SECTION.
       INI-VAL-KEY-VEN.
           PERFORM VAL-IKY-VEN.
           PERFORM VAL-MKY-VEN.
           PERFORM VAL-AKY-VEN.
       FIN-VAL-KEY-VEN.
           EXIT.
       
       VAL-IKY-VEN SECTION.
       INI-VAL-IKY-VEN.
*% INCLUDE INI_VAL_IKY_VEN OF 'COLBMVEN.TXT'
           PERFORM VAL-VEN-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO VEN-CAI-IOPC-LEN IN VEN-FLD
                GO TO FIN-VAL-IKY-VEN.
           PERFORM VAL-VEN-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO VEN-IIC-IOPC-LEN IN VEN-FLD
                GO TO FIN-VAL-IKY-VEN.
*% INCLUDE FIN_VAL_IKY_VEN OF 'COLBMVEN.TXT'
       FIN-VAL-IKY-VEN.
           EXIT.
       
       VAL-MKY-VEN SECTION.
       INI-VAL-MKY-VEN.
*% INCLUDE INI_VAL_MKY_VEN OF 'COLBMVEN.TXT'
*% INCLUDE FIN_VAL_MKY_VEN OF 'COLBMVEN.TXT'
       FIN-VAL-MKY-VEN.
           EXIT.
       
       VAL-AKY-VEN SECTION.
       INI-VAL-AKY-VEN.
*% INCLUDE INI_VAL_AKY_VEN OF 'COLBMVEN.TXT'
           PERFORM VAL-VEN-NUM-IVCT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO VEN-NUM-IVCT-LEN IN VEN-FLD
                GO TO FIN-VAL-AKY-VEN.
           PERFORM VAL-VEN-NUM-IVEN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO VEN-NUM-IVEN-LEN IN VEN-FLD
                GO TO FIN-VAL-AKY-VEN.
*% INCLUDE FIN_VAL_AKY_VEN OF 'COLBMVEN.TXT'
       FIN-VAL-AKY-VEN.
           EXIT.
       
       VAL-FLD-VEN SECTION.
       INI-VAL-FLD-VEN.
*% INCLUDE INI_VAL_FLD_VEN OF 'COLBMVEN.TXT'
           PERFORM VAL-OPC-MSC-TVOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPC-MSC-TVOC-LEN IN VEN-FLD
                GO TO FIN-VAL-FLD-VEN.
           PERFORM VAL-OPC-COD-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPC-COD-TOOC-LEN IN VEN-FLD
                GO TO FIN-VAL-FLD-VEN.
           PERFORM VAL-OPC-COD-OFOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPC-COD-OFOC-LEN IN VEN-FLD
                GO TO FIN-VAL-FLD-VEN.
           PERFORM VAL-FRM-GLS-TVOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-TVOC-LEN IN VEN-FLD
                GO TO FIN-VAL-FLD-VEN.
           PERFORM VAL-FRM-GLS-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-TOOC-LEN IN VEN-FLD
                GO TO FIN-VAL-FLD-VEN.
           PERFORM VAL-FRM-GLS-OFOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-OFOC-LEN IN VEN-FLD
                GO TO FIN-VAL-FLD-VEN.
           PERFORM VAL-OPC-COD-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPC-COD-VCOC-LEN IN VEN-FLD
                GO TO FIN-VAL-FLD-VEN.
           PERFORM VAL-VEN-NUM-IDLC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO VEN-NUM-IDLC-LEN IN VEN-FLD
                GO TO FIN-VAL-FLD-VEN.
           PERFORM VAL-FRM-GLS-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-VCOC-LEN IN VEN-FLD
                GO TO FIN-VAL-FLD-VEN.
           PERFORM VAL-VEN-FEC-FVEN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO VEN-FEC-FVEN-LEN IN VEN-FLD
                GO TO FIN-VAL-FLD-VEN.
           PERFORM VAL-VEN-VAL-CAPI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO VEN-VAL-CAPI-LEN IN VEN-FLD
                GO TO FIN-VAL-FLD-VEN.
           PERFORM VAL-VEN-FEC-FIMV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO VEN-FEC-FIMV-LEN IN VEN-FLD
                GO TO FIN-VAL-FLD-VEN.
           PERFORM VAL-VEN-VAL-SCAP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO VEN-VAL-SCAP-LEN IN VEN-FLD
                GO TO FIN-VAL-FLD-VEN.
           PERFORM VAL-VEN-IND-SCTB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO VEN-IND-SCTB-LEN IN VEN-FLD
                GO TO FIN-VAL-FLD-VEN.
           PERFORM VAL-VEN-VAL-IDNI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO VEN-VAL-IDNI-LEN IN VEN-FLD
                GO TO FIN-VAL-FLD-VEN.
           PERFORM VAL-VEN-IND-SCBZ.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO VEN-IND-SCBZ-LEN IN VEN-FLD
                GO TO FIN-VAL-FLD-VEN.
           PERFORM VAL-VEN-VAL-FINA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO VEN-VAL-FINA-LEN IN VEN-FLD
                GO TO FIN-VAL-FLD-VEN.
           PERFORM VAL-VEN-IND-CAUT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO VEN-IND-CAUT-LEN IN VEN-FLD
                GO TO FIN-VAL-FLD-VEN.
           PERFORM VAL-VEN-VAL-SFIN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO VEN-VAL-SFIN-LEN IN VEN-FLD
                GO TO FIN-VAL-FLD-VEN.
*% INCLUDE FIN_VAL_FLD_VEN OF 'COLBMVEN.TXT'
       FIN-VAL-FLD-VEN.
           EXIT.
       
