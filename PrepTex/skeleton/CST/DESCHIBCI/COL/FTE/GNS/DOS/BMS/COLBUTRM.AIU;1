*% FRM = "TRM"
      *
      * COLBUTRM
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:59 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMTRM.TXT'
       SET-TAG-COLBUTRM SECTION.
       INI-SET-TAG-COLBUTRM.
           MOVE
           '<IDNMOD NOM=$COLBUTRM$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:59 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUTRM.
           EXIT.
           
       RET-ALL-TRM SECTION.
       INI-RET-ALL-TRM.
*% INCLUDE INI_RET_ALL_TRM OF 'COLBMTRM.TXT'
           PERFORM KEY-ALL-TRM.
           PERFORM RET-OPC-TRM.
           PERFORM RET-VEN-TRM.
           PERFORM RET-DBC-TRM.
           PERFORM RET-DLC-TRM.
*% INCLUDE FIN_RET_ALL_TRM OF 'COLBMTRM.TXT'
       FIN-RET-ALL-TRM.
           EXIT.
       
       RET-OPC-TRM SECTION.
       INI-RET-OPC-TRM.
*% INCLUDE INI_RET_OPC_TRM OF 'COLBMTRM.TXT'
           MOVE OPC-MSC-TVOC IN TRM-FLD TO OPC-MSC-TVOC IN OPC.
           MOVE OPC-COD-TOOC IN TRM-FLD TO OPC-COD-TOOC IN OPC.
           MOVE OPC-COD-OFOC IN TRM-FLD TO OPC-COD-OFOC IN OPC.
           MOVE OPC-COD-VCOC IN TRM-FLD TO OPC-COD-VCOC IN OPC.
*% INCLUDE FIN_RET_OPC_TRM OF 'COLBMTRM.TXT'
       FIN-RET-OPC-TRM.
           EXIT.
       
       RET-VEN-TRM SECTION.
       INI-RET-VEN-TRM.
*% INCLUDE INI_RET_VEN_TRM OF 'COLBMTRM.TXT'
           MOVE VEN-VAL-SCAP IN TRM-FLD TO VEN-VAL-SCAP IN VEN.
*% RREG = "VEN"
*% RREG_FRM = "VEN"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE VEN-NUM-DVEN IN TRM-FLD TO VEN-NUM-DVEN IN VEN.
           MOVE VEN-NUM-MVEN IN TRM-FLD TO VEN-NUM-MVEN IN VEN.
           MOVE VEN-NUM-SVEN IN TRM-FLD TO VEN-NUM-SVEN IN VEN.
           MOVE VEN-NUM-AVEN IN TRM-FLD TO VEN-NUM-AVEN IN VEN.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE VEN-VAL-SFIN IN TRM-FLD TO VEN-VAL-SFIN IN VEN.
*% INCLUDE FIN_RET_VEN_TRM OF 'COLBMTRM.TXT'
       FIN-RET-VEN-TRM.
           EXIT.
       
       RET-DBC-TRM SECTION.
       INI-RET-DBC-TRM.
*% INCLUDE INI_RET_DBC_TRM OF 'COLBMTRM.TXT'
           MOVE DBC-GLS-NOMC IN TRM-FLD TO DBC-GLS-NOMC IN DBC.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN TRM-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN TRM-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN TRM-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN TRM-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_RET_DBC_TRM OF 'COLBMTRM.TXT'
       FIN-RET-DBC-TRM.
           EXIT.
       
       RET-DLC-TRM SECTION.
       INI-RET-DLC-TRM.
*% INCLUDE INI_RET_DLC_TRM OF 'COLBMTRM.TXT'
           MOVE DLC-VAL-SDLC IN TRM-FLD TO DLC-VAL-SDLC IN DLC.
           MOVE DLC-IND-SCTB IN TRM-FLD TO DLC-IND-SCTB IN DLC.
           MOVE DLC-IND-SCBZ IN TRM-FLD TO DLC-IND-SCBZ IN DLC.
*% INCLUDE FIN_RET_DLC_TRM OF 'COLBMTRM.TXT'
       FIN-RET-DLC-TRM.
           EXIT.
       
       KEY-ALL-TRM SECTION.
       INI-KEY-ALL-TRM.
*% INCLUDE INI_KEY_ALL_TRM OF 'COLBMTRM.TXT'
           PERFORM KEY-OPC-TRM.
           PERFORM KEY-VEN-TRM.
           PERFORM KEY-DBC-TRM.
           PERFORM KEY-DLC-TRM.
*% INCLUDE FIN_KEY_ALL_TRM OF 'COLBMTRM.TXT'
       FIN-KEY-ALL-TRM.
           EXIT.
       
       KEY-OPC-TRM SECTION.
       INI-KEY-OPC-TRM.
*% INCLUDE INI_KEY_OPC_TRM OF 'COLBMTRM.TXT'
           MOVE OPC-CAI-IOPC IN TRM-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE OPC-IIC-IOPC IN TRM-FLD TO OPC-IIC-IOPC IN OPC.
*% INCLUDE FIN_KEY_OPC_TRM OF 'COLBMTRM.TXT'
       FIN-KEY-OPC-TRM.
           EXIT.
       
       KEY-VEN-TRM SECTION.
       INI-KEY-VEN-TRM.
*% INCLUDE INI_KEY_VEN_TRM OF 'COLBMTRM.TXT'
           MOVE VEN-NUM-IVCT IN TRM-FLD TO VEN-NUM-IVCT IN VEN.
*% INCLUDE FIN_KEY_VEN_TRM OF 'COLBMTRM.TXT'
       FIN-KEY-VEN-TRM.
           EXIT.
       
       KEY-DBC-TRM SECTION.
       INI-KEY-DBC-TRM.
*% INCLUDE INI_KEY_DBC_TRM OF 'COLBMTRM.TXT'
*% INCLUDE FIN_KEY_DBC_TRM OF 'COLBMTRM.TXT'
       FIN-KEY-DBC-TRM.
           EXIT.
       
       KEY-DLC-TRM SECTION.
       INI-KEY-DLC-TRM.
*% INCLUDE INI_KEY_DLC_TRM OF 'COLBMTRM.TXT'
*% INCLUDE FIN_KEY_DLC_TRM OF 'COLBMTRM.TXT'
       FIN-KEY-DLC-TRM.
           EXIT.
       
       SCR-CLRA-TRM SECTION.
       INI-SCR-CLRA-TRM.
*% INCLUDE INI_SCR_CLRA_TRM OF 'COLBMTRM.TXT'
           PERFORM CLR-KEY-TRM.
           PERFORM CLR-FLD-TRM.
*% INCLUDE FIN_SCR_CLRA_TRM OF 'COLBMTRM.TXT'
       FIN-SCR-CLRA-TRM.
           EXIT.
       
       CLR-KEY-TRM SECTION.
       INI-CLR-KEY-TRM.
*% INCLUDE INI_CLR_KEY_TRM OF 'COLBMTRM.TXT'
           MOVE SPACES TO OPC-CAI-IOPC IN TRM-FLD.
           MOVE ZEROES TO OPC-IIC-IOPC IN TRM-FLD.
           MOVE ZEROES TO VEN-NUM-IVCT IN TRM-FLD.
*% INCLUDE FIN_CLR_KEY_TRM OF 'COLBMTRM.TXT'
       FIN-CLR-KEY-TRM.
           EXIT.
       
       CLR-FLD-TRM SECTION.
       INI-CLR-FLD-TRM.
*% INCLUDE INI_CLR_FLD_TRM OF 'COLBMTRM.TXT'
           MOVE SPACES TO FRM-GLS-TVOC IN TRM-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN TRM-FLD.
           MOVE SPACES TO FRM-GLS-OFOC IN TRM-FLD.
           MOVE SPACES TO FRM-GLS-TRAS IN TRM-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCCT"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FCCT IN TRM-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-GLS-SCTB IN TRM-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN TRM-FLD.
           MOVE SPACES TO FRM-GLS-SCBZ IN TRM-FLD.
           PERFORM CLR-OPC-TRM.
           PERFORM CLR-VEN-TRM.
           PERFORM CLR-DBC-TRM.
           PERFORM CLR-DLC-TRM.
*% INCLUDE FIN_CLR_FLD_TRM OF 'COLBMTRM.TXT'
       FIN-CLR-FLD-TRM.
           EXIT.
       
       CLR-OPC-TRM SECTION.
       INI-CLR-OPC-TRM.
*% INCLUDE INI_CLR_OPC_TRM OF 'COLBMTRM.TXT'
           MOVE SPACES TO OPC-MSC-TVOC IN TRM-FLD.
           MOVE SPACES TO OPC-COD-TOOC IN TRM-FLD.
           MOVE SPACES TO OPC-COD-OFOC IN TRM-FLD.
           MOVE SPACES TO OPC-COD-VCOC IN TRM-FLD.
*% INCLUDE FIN_CLR_OPC_TRM OF 'COLBMTRM.TXT'
       FIN-CLR-OPC-TRM.
           EXIT.
       
       CLR-VEN-TRM SECTION.
       INI-CLR-VEN-TRM.
*% INCLUDE INI_CLR_VEN_TRM OF 'COLBMTRM.TXT'
           MOVE ZEROES TO VEN-VAL-SCAP IN TRM-FLD.
*% RREG = "VEN"
*% RREG_FRM = "VEN"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO VEN-FEC-FVEN IN TRM-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO VEN-VAL-SFIN IN TRM-FLD.
*% INCLUDE FIN_CLR_VEN_TRM OF 'COLBMTRM.TXT'
       FIN-CLR-VEN-TRM.
           EXIT.
       
       CLR-DBC-TRM SECTION.
       INI-CLR-DBC-TRM.
*% INCLUDE INI_CLR_DBC_TRM OF 'COLBMTRM.TXT'
           MOVE SPACES TO DBC-GLS-NOMC IN TRM-FLD.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN TRM-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN TRM-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN TRM-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN TRM-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_DBC_TRM OF 'COLBMTRM.TXT'
       FIN-CLR-DBC-TRM.
           EXIT.
       
       CLR-DLC-TRM SECTION.
       INI-CLR-DLC-TRM.
*% INCLUDE INI_CLR_DLC_TRM OF 'COLBMTRM.TXT'
           MOVE ZEROES TO DLC-VAL-SDLC IN TRM-FLD.
           MOVE SPACES TO DLC-IND-SCTB IN TRM-FLD.
           MOVE SPACES TO DLC-IND-SCBZ IN TRM-FLD.
*% INCLUDE FIN_CLR_DLC_TRM OF 'COLBMTRM.TXT'
       FIN-CLR-DLC-TRM.
           EXIT.
       
       FST-KEY-TRM SECTION.
       INI-FST-KEY-TRM.
*% INCLUDE INI_FST_KEY_TRM OF 'COLBMTRM.TXT'
           MOVE -1 TO OPC-CAI-IOPC-LEN IN TRM-FLD.
*% INCLUDE FIN_FST_KEY_TRM OF 'COLBMTRM.TXT'
       FIN-FST-KEY-TRM.
           EXIT.
       
       FST-FLD-TRM SECTION.
       INI-FST-FLD-TRM.
*% INCLUDE INI_FST_FLD_TRM OF 'COLBMTRM.TXT'
*% INCLUDE FIN_FST_FLD_TRM OF 'COLBMTRM.TXT'
       FIN-FST-FLD-TRM.
           EXIT.
       
       PRO-KEY-TRM SECTION.
       INI-PRO-KEY-TRM.
*% INCLUDE INI_PRO_KEY_TRM OF 'COLBMTRM.TXT'
           PERFORM PRO-MKY-TRM.
*% INCLUDE FIN_PRO_KEY_TRM OF 'COLBMTRM.TXT'
       FIN-PRO-KEY-TRM.
           EXIT.
       
       PRO-MKY-TRM SECTION.
       INI-PRO-MKY-TRM.
*% INCLUDE INI_PRO_MKY_TRM OF 'COLBMTRM.TXT'
*% INCLUDE FIN_PRO_MKY_TRM OF 'COLBMTRM.TXT'
       FIN-PRO-MKY-TRM.
           EXIT.
       
