       INI-PRO-DLC-TRM.
*% INCLUDE INI_PRO_DLC_TRM OF 'COLBMTRM.TXT'
*% INCLUDE FIN_PRO_DLC_TRM OF 'COLBMTRM.TXT'
       FIN-PRO-DLC-TRM.
           EXIT.
       
       UNP-KEY-TRM SECTION.
       INI-UNP-KEY-TRM.
*% INCLUDE INI_UNP_KEY_TRM OF 'COLBMTRM.TXT'
           PERFORM UNP-IKY-TRM.
           PERFORM UNP-AKY-TRM.
           PERFORM UNP-MKY-TRM.
*% INCLUDE FIN_UNP_KEY_TRM OF 'COLBMTRM.TXT'
       FIN-UNP-KEY-TRM.
           EXIT.
       
       UNP-IKY-TRM SECTION.
       INI-UNP-IKY-TRM.
*% INCLUDE INI_UNP_IKY_TRM OF 'COLBMTRM.TXT'
           MOVE FRM-CPIM-UBY TO OPC-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO OPC-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO VEN-NUM-IVCT-ATR.
*% INCLUDE FIN_UNP_IKY_TRM OF 'COLBMTRM.TXT'
       FIN-UNP-IKY-TRM.
           EXIT.
       
       UNP-MKY-TRM SECTION.
       INI-UNP-MKY-TRM.
*% INCLUDE INI_UNP_MKY_TRM OF 'COLBMTRM.TXT'
*% INCLUDE FIN_UNP_MKY_TRM OF 'COLBMTRM.TXT'
       FIN-UNP-MKY-TRM.
           EXIT.
       
       UNP-AKY-TRM SECTION.
       INI-UNP-AKY-TRM.
*% INCLUDE INI_UNP_AKY_TRM OF 'COLBMTRM.TXT'
*% INCLUDE FIN_UNP_AKY_TRM OF 'COLBMTRM.TXT'
       FIN-UNP-AKY-TRM.
           EXIT.
       
       UNP-FLD-TRM SECTION.
       INI-UNP-FLD-TRM.
*% INCLUDE INI_UNP_FLD_TRM OF 'COLBMTRM.TXT'
           PERFORM UNP-OPC-TRM.
           PERFORM UNP-VEN-TRM.
           PERFORM UNP-DBC-TRM.
           PERFORM UNP-DLC-TRM.
*% INCLUDE FIN_UNP_FLD_TRM OF 'COLBMTRM.TXT'
       FIN-UNP-FLD-TRM.
           EXIT.
       
