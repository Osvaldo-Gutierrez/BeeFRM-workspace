*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "TIP"
*% PGM_BQ = TRUE
       PUT-ALL-TIP SECTION.
       INI-PUT-ALL-TIP.
*% INCLUDE INI_PUT_ALL_TIP OF 'COLBMTIP.TXT'
           PERFORM PUT-TIP-TIP.
*% INCLUDE FIN_PUT_ALL_TIP OF 'COLBMTIP.TXT'
       FIN-PUT-ALL-TIP.
           EXIT.
       
       PUT-TIP-TIP SECTION.
       INI-PUT-TIP-TIP.
*% INCLUDE INI_PUT_TIP_TIP OF 'COLBMTIP.TXT'
           IF PGM-STAT-TIP-NUL
               GO TO FIN-PUT-TIP-TIP.
           IF NOT PGM-STAT-TIP-OKS
               PERFORM CLR-TIP-TIP
               GO TO FIN-PUT-TIP-TIP.
*% RREG = "TIP"
*% NOMB = "FUNO"
      * FEC-INI --> BMCHIBCI001.SRCAIU 
           MOVE TIP-NUM-DUNO IN TIP TO TIP-NUM-DUNO IN TIP-FLD.
           MOVE TIP-NUM-MUNO IN TIP TO TIP-NUM-MUNO IN TIP-FLD.
           MOVE TIP-NUM-SUNO IN TIP TO TIP-NUM-SUNO IN TIP-FLD.
           MOVE TIP-NUM-AUNO IN TIP TO TIP-NUM-AUNO IN TIP-FLD.
      * FEC-FIN --> BMCHIBCI001.SRCAIU 
*% RREG = "TIP"
*% NOMB = "FDOS"
      * FEC-INI --> BMCHIBCI001.SRCAIU 
           MOVE TIP-NUM-DDOS IN TIP TO TIP-NUM-DDOS IN TIP-FLD.
           MOVE TIP-NUM-MDOS IN TIP TO TIP-NUM-MDOS IN TIP-FLD.
           MOVE TIP-NUM-SDOS IN TIP TO TIP-NUM-SDOS IN TIP-FLD.
           MOVE TIP-NUM-ADOS IN TIP TO TIP-NUM-ADOS IN TIP-FLD.
      * FEC-FIN --> BMCHIBCI001.SRCAIU 
           MOVE TIP-COD-VCAM IN TIP TO TIP-COD-VCAM IN TIP-FLD.
*% RREG = "TIP"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI001.SRCAIU 
           MOVE TIP-NUM-DCOL IN TIP TO TIP-NUM-DCOL IN TIP-FLD.
           MOVE TIP-NUM-MCOL IN TIP TO TIP-NUM-MCOL IN TIP-FLD.
           MOVE TIP-NUM-SCOL IN TIP TO TIP-NUM-SCOL IN TIP-FLD.
           MOVE TIP-NUM-ACOL IN TIP TO TIP-NUM-ACOL IN TIP-FLD.
      * FEC-FIN --> BMCHIBCI001.SRCAIU 
*% RREG = "TIP"
*% NOMB = "FBTV"
      * FEC-INI --> BMCHIBCI001.SRCAIU 
           MOVE TIP-NUM-DBTV IN TIP TO TIP-NUM-DBTV IN TIP-FLD.
           MOVE TIP-NUM-MBTV IN TIP TO TIP-NUM-MBTV IN TIP-FLD.
           MOVE TIP-NUM-SBTV IN TIP TO TIP-NUM-SBTV IN TIP-FLD.
           MOVE TIP-NUM-ABTV IN TIP TO TIP-NUM-ABTV IN TIP-FLD.
      * FEC-FIN --> BMCHIBCI001.SRCAIU 
           MOVE TIP-SGV-TIPE IN TIP TO TIP-SGV-TIPE IN TIP-FLD.
           MOVE TIP-COD-CMSG IN TIP TO TIP-COD-CMSG IN TIP-FLD.
           MOVE TIP-GLS-MENS IN TIP TO TIP-GLS-MENS IN TIP-FLD.
*% INCLUDE FIN_PUT_TIP_TIP OF 'COLBMTIP.TXT'
       FIN-PUT-TIP-TIP.
           EXIT.
       
       KEY-ALL-TIP SECTION.
       INI-KEY-ALL-TIP.
*% INCLUDE INI_KEY_ALL_TIP OF 'COLBMTIP.TXT'
           PERFORM KEY-TIP-TIP.
*% INCLUDE FIN_KEY_ALL_TIP OF 'COLBMTIP.TXT'
       FIN-KEY-ALL-TIP.
           EXIT.
       
       KEY-TIP-TIP SECTION.
       INI-KEY-TIP-TIP.
*% INCLUDE INI_KEY_TIP_TIP OF 'COLBMTIP.TXT'
*% RREG = "TIP"
*% NOMB = "FUNO"
      * FEC-INI --> BMCHIBCI002.SRCAIU RET-ALL Y KEY-ALL
           MOVE TIP-NUM-DBTV IN TIP-FLD TO TIP-NUM-DBTV IN TIP.
           MOVE TIP-NUM-MBTV IN TIP-FLD TO TIP-NUM-MBTV IN TIP.
           MOVE TIP-NUM-SBTV IN TIP-FLD TO TIP-NUM-SBTV IN TIP.
           MOVE TIP-NUM-ABTV IN TIP-FLD TO TIP-NUM-ABTV IN TIP.
      * FEC-FIN --> BMCHIBCI002.SRCAIU RET-ALL Y KEY-ALL
*% RREG = "TIP"
*% NOMB = "FDOS"
      * FEC-INI --> BMCHIBCI002.SRCAIU RET-ALL Y KEY-ALL
           MOVE TIP-NUM-DBTV IN TIP-FLD TO TIP-NUM-DBTV IN TIP.
           MOVE TIP-NUM-MBTV IN TIP-FLD TO TIP-NUM-MBTV IN TIP.
           MOVE TIP-NUM-SBTV IN TIP-FLD TO TIP-NUM-SBTV IN TIP.
           MOVE TIP-NUM-ABTV IN TIP-FLD TO TIP-NUM-ABTV IN TIP.
      * FEC-FIN --> BMCHIBCI002.SRCAIU RET-ALL Y KEY-ALL
           MOVE TIP-COD-VCAM IN TIP-FLD TO TIP-COD-VCAM IN TIP.
*% RREG = "TIP"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI002.SRCAIU RET-ALL Y KEY-ALL
           MOVE TIP-NUM-DBTV IN TIP-FLD TO TIP-NUM-DBTV IN TIP.
           MOVE TIP-NUM-MBTV IN TIP-FLD TO TIP-NUM-MBTV IN TIP.
           MOVE TIP-NUM-SBTV IN TIP-FLD TO TIP-NUM-SBTV IN TIP.
           MOVE TIP-NUM-ABTV IN TIP-FLD TO TIP-NUM-ABTV IN TIP.
      * FEC-FIN --> BMCHIBCI002.SRCAIU RET-ALL Y KEY-ALL
*% RREG = "TIP"
*% NOMB = "FBTV"
      * FEC-INI --> BMCHIBCI002.SRCAIU RET-ALL Y KEY-ALL
           MOVE TIP-NUM-DBTV IN TIP-FLD TO TIP-NUM-DBTV IN TIP.
           MOVE TIP-NUM-MBTV IN TIP-FLD TO TIP-NUM-MBTV IN TIP.
           MOVE TIP-NUM-SBTV IN TIP-FLD TO TIP-NUM-SBTV IN TIP.
           MOVE TIP-NUM-ABTV IN TIP-FLD TO TIP-NUM-ABTV IN TIP.
      * FEC-FIN --> BMCHIBCI002.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_KEY_TIP_TIP OF 'COLBMTIP.TXT'
       FIN-KEY-TIP-TIP.
           EXIT.
       
       SCR-CLRA-TIP SECTION.
       INI-SCR-CLRA-TIP.
*% INCLUDE INI_SCR_CLRA_TIP OF 'COLBMTIP.TXT'
           PERFORM CLR-KEY-TIP.
           PERFORM CLR-FLD-TIP.
*% INCLUDE FIN_SCR_CLRA_TIP OF 'COLBMTIP.TXT'
       FIN-SCR-CLRA-TIP.
           EXIT.
       
       CLR-KEY-TIP SECTION.
       INI-CLR-KEY-TIP.
*% INCLUDE INI_CLR_KEY_TIP OF 'COLBMTIP.TXT'
*% RREG = "TIP"
*% NOMB = "FUNO"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO TIP-FEC-FUNO IN TIP-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "TIP"
*% NOMB = "FDOS"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO TIP-FEC-FDOS IN TIP-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO TIP-COD-VCAM IN TIP-FLD.
*% RREG = "TIP"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO TIP-FEC-FCOL IN TIP-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "TIP"
*% NOMB = "FBTV"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO TIP-FEC-FBTV IN TIP-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_KEY_TIP OF 'COLBMTIP.TXT'
       FIN-CLR-KEY-TIP.
           EXIT.
       
       CLR-FLD-TIP SECTION.
