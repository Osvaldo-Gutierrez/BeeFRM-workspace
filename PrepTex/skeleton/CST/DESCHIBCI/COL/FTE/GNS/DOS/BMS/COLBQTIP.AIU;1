      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-CON-KEY-TIP SECTION.
       INI-VAL-CON-KEY-TIP.
*% INCLUDE INI_VAL_CON_KEY_TIP OF 'COLBMTIP.TXT'
*% RREG = "TIP"
*% NOMB = "FUNO"
           IF     TIP-FEC-FUNO IN TIP-FLD NOT > ZEROES
*% RREG = "TIP"
*% NOMB = "FDOS"
           IF     TIP-FEC-FDOS IN TIP-FLD NOT > ZEROES
              AND TIP-COD-VCAM IN TIP-FLD NOT > SPACES 
*% RREG = "TIP"
*% NOMB = "FCOL"
              AND TIP-FEC-FCOL IN TIP-FLD NOT > ZEROES
*% RREG = "TIP"
*% NOMB = "FBTV"
              AND TIP-FEC-FBTV IN TIP-FLD NOT > ZEROES
                PERFORM ERR-KEY
                PERFORM FST-KEY-TIP.
*% INCLUDE FIN_VAL_CON_KEY_TIP OF 'COLBMTIP.TXT'
       FIN-VAL-CON-KEY-TIP.
           EXIT.
       
       VAL-CON-IKY-TIP SECTION.
       INI-VAL-CON-IKY-TIP.
*% INCLUDE INI_VAL_CON_IKY_TIP OF 'COLBMTIP.TXT'
*% RREG = "TIP"
*% NOMB = "FUNO"
           IF     TIP-FEC-FUNO IN TIP-FLD NOT > ZEROES
*% RREG = "TIP"
*% NOMB = "FDOS"
           IF     TIP-FEC-FDOS IN TIP-FLD NOT > ZEROES
              AND TIP-COD-VCAM IN TIP-FLD NOT > SPACES 
*% RREG = "TIP"
*% NOMB = "FCOL"
              AND TIP-FEC-FCOL IN TIP-FLD NOT > ZEROES
*% RREG = "TIP"
*% NOMB = "FBTV"
              AND TIP-FEC-FBTV IN TIP-FLD NOT > ZEROES
                PERFORM ERR-KEY
                PERFORM FST-KEY-TIP.
*% INCLUDE FIN_VAL_CON_IKY_TIP OF 'COLBMTIP.TXT'
       FIN-VAL-CON-IKY-TIP.
           EXIT.
       
       VAL-NUM-KEY-TIP SECTION.
       INI-VAL-NUM-KEY-TIP.
           PERFORM VAL-NUM-IKY-TIP.
           PERFORM VAL-NUM-MKY-TIP.
           PERFORM VAL-NUM-AKY-TIP.
       FIN-VAL-NUM-KEY-TIP.
           EXIT.
       
       VAL-NUM-IKY-TIP SECTION.
       INI-VAL-NUM-IKY-TIP.
*% INCLUDE INI_VAL_NUM_IKY_TIP OF 'COLBMTIP.TXT'
*% INCLUDE FIN_VAL_NUM_IKY_TIP OF 'COLBMTIP.TXT'
       FIN-VAL-NUM-IKY-TIP.
           EXIT.
       
       VAL-NUM-MKY-TIP SECTION.
       INI-VAL-NUM-MKY-TIP.
*% INCLUDE INI_VAL_NUM_MKY_TIP OF 'COLBMTIP.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_TIP OF 'COLBMTIP.TXT'
       FIN-VAL-NUM-MKY-TIP.
           EXIT.
       
       VAL-NUM-AKY-TIP SECTION.
       INI-VAL-NUM-AKY-TIP.
*% INCLUDE INI_VAL_NUM_AKY_TIP OF 'COLBMTIP.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_TIP OF 'COLBMTIP.TXT'
       FIN-VAL-NUM-AKY-TIP.
           EXIT.
       
       VAL-NUM-FLD-TIP SECTION.
       INI-VAL-NUM-FLD-TIP.
*% INCLUDE INI_VAL_NUM_FLD_TIP OF 'COLBMTIP.TXT'
       VAL-NUM-TIP-SGV-TIPE.
*% BUF = "TIP-FLD"
*% FLD = "TIP-SGV-TIPE"
*% ENT = "3"
*% DCM = "11"
*% NMR = "NN9V"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-TIP.
*% INCLUDE FIN_VAL_NUM_FLD_TIP OF 'COLBMTIP.TXT'
       FIN-VAL-NUM-FLD-TIP.
           EXIT.
       
       EDT-KEY-TIP SECTION.
       INI-EDT-KEY-TIP.
           PERFORM EDT-IKY-TIP.
           PERFORM EDT-MKY-TIP.
           PERFORM EDT-AKY-TIP.
*% INCLUDE EDT_KEY_TIP OF 'COLBMTIP.TXT'
       FIN-EDT-KEY-TIP.
           EXIT.
       
       EDT-IKY-TIP SECTION.
       INI-EDT-IKY-TIP.
*% INCLUDE EDT_IKY_TIP OF 'COLBMTIP.TXT'
*% RREG = "TIP"
*% NOMB = "FUNO"
*% RREG = "TIP"
*% NOMB = "FDOS"
*% RREG = "TIP"
*% NOMB = "FCOL"
*% RREG = "TIP"
*% NOMB = "FBTV"
*% INCLUDE FIN_EDT_IKY_TIP OF 'COLBMTIP.TXT'
       FIN-EDT-IKY-TIP.
           EXIT.
       
       EDT-MKY-TIP SECTION.
       INI-EDT-MKY-TIP.
*% INCLUDE EDT_MKY_TIP OF 'COLBMTIP.TXT'
*% INCLUDE FIN_EDT_MKY_TIP OF 'COLBMTIP.TXT'
       FIN-EDT-MKY-TIP.
           EXIT.
       
       EDT-AKY-TIP SECTION.
       INI-EDT-AKY-TIP.
*% INCLUDE EDT_AKY_TIP OF 'COLBMTIP.TXT'
*% INCLUDE FIN_EDT_AKY_TIP OF 'COLBMTIP.TXT'
       FIN-EDT-AKY-TIP.
           EXIT.
       
       EDT-FLD-TIP SECTION.
       INI-EDT-FLD-TIP.
*% INCLUDE EDT_FLD_TIP OF 'COLBMTIP.TXT'
           IF TIP-SGV-TIPE-LEN IN TIP-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO TIP-SGV-TIPE-LEN IN TIP-FLD
                GO TO FIN-EDT-FLD-TIP.
           MOVE TIP-SGV-TIPE IN TIP-FLD TO TIP-SGV-TIPE-EDT IN TIP-EDT.
           MOVE TIP-SGV-TIPE-EDT IN TIP-EDT TO 
                TIP-SGV-TIPE-ALF IN TIP-FLD.
*% INCLUDE FIN_EDT_FLD_TIP OF 'COLBMTIP.TXT'
       FIN-EDT-FLD-TIP.
           EXIT.
*% INCLUDE EOF_BQ OF 'COLBMTIP.TXT'
*% INCLUDE EOF OF 'COLBMTIP.TXT'
*% INCLUDE EOF_IBM OF 'COLBMTIP.TXT'
