       INI-PRO-DBC-QPI.
*% INCLUDE INI_PRO_DBC_QPI OF 'COLBMQPI.TXT'
*% INCLUDE FIN_PRO_DBC_QPI OF 'COLBMQPI.TXT'
       FIN-PRO-DBC-QPI.
           EXIT.
       
       UNP-KEY-QPI SECTION.
       INI-UNP-KEY-QPI.
*% INCLUDE INI_UNP_KEY_QPI OF 'COLBMQPI.TXT'
           PERFORM UNP-IKY-QPI.
           PERFORM UNP-AKY-QPI.
           PERFORM UNP-MKY-QPI.
*% INCLUDE FIN_UNP_KEY_QPI OF 'COLBMQPI.TXT'
       FIN-UNP-KEY-QPI.
           EXIT.
       
       UNP-IKY-QPI SECTION.
       INI-UNP-IKY-QPI.
*% INCLUDE INI_UNP_IKY_QPI OF 'COLBMQPI.TXT'
           MOVE FRM-CPIM-UBY TO IPT-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO IPT-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO IPT-COD-EVEN-ATR.
           MOVE FRM-CPIM-UBY TO IPT-COD-CPTO-ATR.
*% RREG = "IPT"
*% RREG_FRM = "IPT"
*% NOMB = "FINI"
           MOVE FRM-CPIM-UBY TO IPT-FEC-FINI-ATR IN QPI-FLD.
*% RREG = "IPT"
*% RREG_FRM = "IPT"
*% NOMB = "FTER"
           MOVE FRM-CPIM-UBY TO IPT-FEC-FTER-ATR IN QPI-FLD.
*% INCLUDE FIN_UNP_IKY_QPI OF 'COLBMQPI.TXT'
       FIN-UNP-IKY-QPI.
           EXIT.
       
       UNP-MKY-QPI SECTION.
       INI-UNP-MKY-QPI.
*% INCLUDE INI_UNP_MKY_QPI OF 'COLBMQPI.TXT'
*% INCLUDE FIN_UNP_MKY_QPI OF 'COLBMQPI.TXT'
       FIN-UNP-MKY-QPI.
           EXIT.
       
       UNP-AKY-QPI SECTION.
       INI-UNP-AKY-QPI.
*% INCLUDE INI_UNP_AKY_QPI OF 'COLBMQPI.TXT'
*% INCLUDE FIN_UNP_AKY_QPI OF 'COLBMQPI.TXT'
       FIN-UNP-AKY-QPI.
           EXIT.
       
       UNP-FLD-QPI SECTION.
       INI-UNP-FLD-QPI.
*% INCLUDE INI_UNP_FLD_QPI OF 'COLBMQPI.TXT'
           PERFORM UNP-IPT-QPI.
           PERFORM UNP-DBC-QPI.
*% INCLUDE FIN_UNP_FLD_QPI OF 'COLBMQPI.TXT'
       FIN-UNP-FLD-QPI.
           EXIT.
       
       UNP-IPT-QPI SECTION.
       INI-UNP-IPT-QPI.
*% INCLUDE INI_UNP_IPT_QPI OF 'COLBMQPI.TXT'
*% INCLUDE FIN_UNP_IPT_QPI OF 'COLBMQPI.TXT'
       FIN-UNP-IPT-QPI.
           EXIT.
       
       UNP-DBC-QPI SECTION.
       INI-UNP-DBC-QPI.
*% INCLUDE INI_UNP_DBC_QPI OF 'COLBMQPI.TXT'
*% INCLUDE FIN_UNP_DBC_QPI OF 'COLBMQPI.TXT'
       FIN-UNP-DBC-QPI.
           EXIT.
       
       VAL-MKY-QPI SECTION.
       INI-VAL-MKY-QPI.
*% INCLUDE INI_VAL_MKY_QPI OF 'COLBMQPI.TXT'
*% INCLUDE FIN_VAL_MKY_QPI OF 'COLBMQPI.TXT'
       FIN-VAL-MKY-QPI.
           EXIT.
       
       VAL-FLD-QPI SECTION.
       INI-VAL-FLD-QPI.
*% INCLUDE INI_VAL_FLD_QPI OF 'COLBMQPI.TXT'
           PERFORM VAL-FRM-GLS-NOMC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-NOMC-LEN IN QPI-FLD
                GO TO FIN-VAL-FLD-QPI.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * VRF-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * VRF-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DBC-IND-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-IND-ICLI-LEN IN QPI-FLD
                GO TO FIN-VAL-FLD-QPI.
           PERFORM VAL-DBC-GLS-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-GLS-ICLI-LEN IN QPI-FLD
                GO TO FIN-VAL-FLD-QPI.
           PERFORM VAL-DBC-NUM-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-NUM-ICLI-LEN IN QPI-FLD
                GO TO FIN-VAL-FLD-QPI.
           PERFORM VAL-DBC-VRF-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-VRF-ICLI-LEN IN QPI-FLD
                GO TO FIN-VAL-FLD-QPI.
      * IDC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-VAL-MCRE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-MCRE-LEN IN QPI-FLD
                GO TO FIN-VAL-FLD-QPI.
           PERFORM VAL-FRM-COD-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-VCOC-LEN IN QPI-FLD
                GO TO FIN-VAL-FLD-QPI.
           PERFORM VAL-FRM-GLS-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-VCOC-LEN IN QPI-FLD
                GO TO FIN-VAL-FLD-QPI.
           PERFORM VAL-FRM-GLS-EVEN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-EVEN-LEN IN QPI-FLD
                GO TO FIN-VAL-FLD-QPI.
           PERFORM VAL-FRM-GLS-CPTO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-CPTO-LEN IN QPI-FLD
                GO TO FIN-VAL-FLD-QPI.
           PERFORM VAL-FRM-VAL-SCRE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-SCRE-LEN IN QPI-FLD
                GO TO FIN-VAL-FLD-QPI.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FCOL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FCOL-LEN IN QPI-FLD
                GO TO FIN-VAL-FLD-QPI.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-SGV-MNTO
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 4
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-MNTO-LEN IN QPI-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-QPI.
           PERFORM VAL-FRM-SGV-VTOT
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 4
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-VTOT-LEN IN QPI-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-QPI.
           PERFORM VAL-FRM-SGV-TOPE
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 4
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-TOPE-LEN IN QPI-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-QPI.
           PERFORM VAL-FRM-NUM-VDES
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 4
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-NUM-VDES-LEN IN QPI-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-QPI.
           PERFORM VAL-FRM-SGV-TINI
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 4
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-TINI-LEN IN QPI-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-QPI.
