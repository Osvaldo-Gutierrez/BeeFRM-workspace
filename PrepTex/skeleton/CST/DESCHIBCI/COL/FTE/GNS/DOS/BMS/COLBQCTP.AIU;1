      *
      * COLBQCTP
      * Version 3.0.1 Nov-2005
      * 30-Oct-12 04:55 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CTP"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMCTP.TXT'
       SET-TAG-COLBQCTP SECTION.
       INI-SET-TAG-COLBQCTP.
           MOVE
           '<IDNMOD NOM=$COLBQCTP$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$30-Oct-12 04:55 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCTP.
           EXIT.
           
       PUT-ALL-CTP SECTION.
       INI-PUT-ALL-CTP.
*% INCLUDE INI_PUT_ALL_CTP OF 'COLBMCTP.TXT'
           PERFORM PUT-POC-CTP.
*% INCLUDE FIN_PUT_ALL_CTP OF 'COLBMCTP.TXT'
       FIN-PUT-ALL-CTP.
           EXIT.
       
       PUT-POC-CTP SECTION.
       INI-PUT-POC-CTP.
*% INCLUDE INI_PUT_POC_CTP OF 'COLBMCTP.TXT'
           IF PGM-STAT-POC-NUL
               GO TO FIN-PUT-POC-CTP.
           IF NOT PGM-STAT-POC-OKS
               PERFORM CLR-POC-CTP
               GO TO FIN-PUT-POC-CTP.
           MOVE POC-COD-VCOC IN POC TO POC-COD-VCOC IN CTP-FLD.
           MOVE POC-COD-TOOC IN POC TO POC-COD-TOOC IN CTP-FLD.
           MOVE POC-COD-AOOC IN POC TO POC-COD-AOOC IN CTP-FLD.
           MOVE POC-NUM-IPOC IN POC TO POC-NUM-IPOC IN CTP-FLD.
*% INCLUDE FIN_PUT_POC_CTP OF 'COLBMCTP.TXT'
       FIN-PUT-POC-CTP.
           EXIT.
       
       KEY-ALL-CTP SECTION.
       INI-KEY-ALL-CTP.
*% INCLUDE INI_KEY_ALL_CTP OF 'COLBMCTP.TXT'
           PERFORM KEY-POC-CTP.
*% INCLUDE FIN_KEY_ALL_CTP OF 'COLBMCTP.TXT'
       FIN-KEY-ALL-CTP.
           EXIT.
       
       KEY-POC-CTP SECTION.
       INI-KEY-POC-CTP.
*% INCLUDE INI_KEY_POC_CTP OF 'COLBMCTP.TXT'
           MOVE POC-COD-VCOC IN CTP-FLD TO POC-COD-VCOC IN POC.
           MOVE POC-COD-TOOC IN CTP-FLD TO POC-COD-TOOC IN POC.
           MOVE POC-COD-AOOC IN CTP-FLD TO POC-COD-AOOC IN POC.
           MOVE POC-NUM-IPOC IN CTP-FLD TO POC-NUM-IPOC IN POC.
*% INCLUDE FIN_KEY_POC_CTP OF 'COLBMCTP.TXT'
       FIN-KEY-POC-CTP.
           EXIT.
       
       SCR-CLRA-CTP SECTION.
       INI-SCR-CLRA-CTP.
*% INCLUDE INI_SCR_CLRA_CTP OF 'COLBMCTP.TXT'
           PERFORM CLR-KEY-CTP.
           PERFORM CLR-FLD-CTP.
*% INCLUDE FIN_SCR_CLRA_CTP OF 'COLBMCTP.TXT'
       FIN-SCR-CLRA-CTP.
           EXIT.
       
       CLR-KEY-CTP SECTION.
       INI-CLR-KEY-CTP.
*% INCLUDE INI_CLR_KEY_CTP OF 'COLBMCTP.TXT'
           MOVE SPACES TO POC-COD-VCOC IN CTP-FLD.
           MOVE SPACES TO POC-COD-TOOC IN CTP-FLD.
           MOVE SPACES TO POC-COD-AOOC IN CTP-FLD.
           MOVE ZEROES TO POC-NUM-IPOC IN CTP-FLD.
           MOVE SPACES TO FRM-COD-NEXT IN CTP-FLD.
*% INCLUDE FIN_CLR_KEY_CTP OF 'COLBMCTP.TXT'
       FIN-CLR-KEY-CTP.
           EXIT.
       
       CLR-FLD-CTP SECTION.
       INI-CLR-FLD-CTP.
*% INCLUDE INI_CLR_FLD_CTP OF 'COLBMCTP.TXT'
           MOVE SPACES TO FRM-IND-HMAS IN CTP-FLD.
           MOVE ZEROES TO FRM-NUM-TOCC IN CTP-FLD.
           MOVE SPACES TO FRM-COD-VCOC IN CTP-FLD(1).
           MOVE SPACES TO FRM-COD-VCOC IN CTP-FLD(2).
           MOVE SPACES TO FRM-COD-VCOC IN CTP-FLD(3).
           MOVE SPACES TO FRM-COD-VCOC IN CTP-FLD(4).
           MOVE SPACES TO FRM-COD-VCOC IN CTP-FLD(5).
           MOVE SPACES TO FRM-COD-VCOC IN CTP-FLD(6).
           MOVE SPACES TO FRM-COD-VCOC IN CTP-FLD(7).
           MOVE SPACES TO FRM-COD-VCOC IN CTP-FLD(8).
           MOVE SPACES TO FRM-COD-VCOC IN CTP-FLD(9).
           MOVE SPACES TO FRM-COD-VCOC IN CTP-FLD(10).
           MOVE SPACES TO FRM-COD-VCOC IN CTP-FLD(11).
           MOVE SPACES TO FRM-COD-VCOC IN CTP-FLD(12).
           MOVE SPACES TO FRM-COD-VCOC IN CTP-FLD(13).
           MOVE SPACES TO FRM-COD-VCOC IN CTP-FLD(14).
           MOVE SPACES TO FRM-COD-VCOC IN CTP-FLD(15).
           MOVE SPACES TO FRM-COD-VCOC IN CTP-FLD(16).
           MOVE SPACES TO FRM-COD-VCOC IN CTP-FLD(17).
           MOVE SPACES TO FRM-COD-TOOC IN CTP-FLD(1).
           MOVE SPACES TO FRM-COD-TOOC IN CTP-FLD(2).
           MOVE SPACES TO FRM-COD-TOOC IN CTP-FLD(3).
           MOVE SPACES TO FRM-COD-TOOC IN CTP-FLD(4).
           MOVE SPACES TO FRM-COD-TOOC IN CTP-FLD(5).
           MOVE SPACES TO FRM-COD-TOOC IN CTP-FLD(6).
           MOVE SPACES TO FRM-COD-TOOC IN CTP-FLD(7).
           MOVE SPACES TO FRM-COD-TOOC IN CTP-FLD(8).
           MOVE SPACES TO FRM-COD-TOOC IN CTP-FLD(9).
           MOVE SPACES TO FRM-COD-TOOC IN CTP-FLD(10).
           MOVE SPACES TO FRM-COD-TOOC IN CTP-FLD(11).
           MOVE SPACES TO FRM-COD-TOOC IN CTP-FLD(12).
           MOVE SPACES TO FRM-COD-TOOC IN CTP-FLD(13).
           MOVE SPACES TO FRM-COD-TOOC IN CTP-FLD(14).
           MOVE SPACES TO FRM-COD-TOOC IN CTP-FLD(15).
           MOVE SPACES TO FRM-COD-TOOC IN CTP-FLD(16).
           MOVE SPACES TO FRM-COD-TOOC IN CTP-FLD(17).
           MOVE SPACES TO FRM-COD-AOOC IN CTP-FLD(1).
           MOVE SPACES TO FRM-COD-AOOC IN CTP-FLD(2).
           MOVE SPACES TO FRM-COD-AOOC IN CTP-FLD(3).
           MOVE SPACES TO FRM-COD-AOOC IN CTP-FLD(4).
           MOVE SPACES TO FRM-COD-AOOC IN CTP-FLD(5).
           MOVE SPACES TO FRM-COD-AOOC IN CTP-FLD(6).
           MOVE SPACES TO FRM-COD-AOOC IN CTP-FLD(7).
           MOVE SPACES TO FRM-COD-AOOC IN CTP-FLD(8).
           MOVE SPACES TO FRM-COD-AOOC IN CTP-FLD(9).
           MOVE SPACES TO FRM-COD-AOOC IN CTP-FLD(10).
           MOVE SPACES TO FRM-COD-AOOC IN CTP-FLD(11).
           MOVE SPACES TO FRM-COD-AOOC IN CTP-FLD(12).
           MOVE SPACES TO FRM-COD-AOOC IN CTP-FLD(13).
           MOVE SPACES TO FRM-COD-AOOC IN CTP-FLD(14).
           MOVE SPACES TO FRM-COD-AOOC IN CTP-FLD(15).
           MOVE SPACES TO FRM-COD-AOOC IN CTP-FLD(16).
           MOVE SPACES TO FRM-COD-AOOC IN CTP-FLD(17).
           MOVE ZEROES TO FRM-NUM-IPOC IN CTP-FLD(1).
           MOVE ZEROES TO FRM-NUM-IPOC IN CTP-FLD(2).
           MOVE ZEROES TO FRM-NUM-IPOC IN CTP-FLD(3).
           MOVE ZEROES TO FRM-NUM-IPOC IN CTP-FLD(4).
           MOVE ZEROES TO FRM-NUM-IPOC IN CTP-FLD(5).
           MOVE ZEROES TO FRM-NUM-IPOC IN CTP-FLD(6).
           MOVE ZEROES TO FRM-NUM-IPOC IN CTP-FLD(7).
           MOVE ZEROES TO FRM-NUM-IPOC IN CTP-FLD(8).
           MOVE ZEROES TO FRM-NUM-IPOC IN CTP-FLD(9).
           MOVE ZEROES TO FRM-NUM-IPOC IN CTP-FLD(10).
           MOVE ZEROES TO FRM-NUM-IPOC IN CTP-FLD(11).
           MOVE ZEROES TO FRM-NUM-IPOC IN CTP-FLD(12).
           MOVE ZEROES TO FRM-NUM-IPOC IN CTP-FLD(13).
           MOVE ZEROES TO FRM-NUM-IPOC IN CTP-FLD(14).
           MOVE ZEROES TO FRM-NUM-IPOC IN CTP-FLD(15).
           MOVE ZEROES TO FRM-NUM-IPOC IN CTP-FLD(16).
           MOVE ZEROES TO FRM-NUM-IPOC IN CTP-FLD(17).
           PERFORM CLR-POC-CTP.
*% INCLUDE FIN_CLR_FLD_CTP OF 'COLBMCTP.TXT'
       FIN-CLR-FLD-CTP.
           EXIT.
       
       CLR-POC-CTP SECTION.
       INI-CLR-POC-CTP.
*% INCLUDE INI_CLR_POC_CTP OF 'COLBMCTP.TXT'
*% INCLUDE FIN_CLR_POC_CTP OF 'COLBMCTP.TXT'
       FIN-CLR-POC-CTP.
           EXIT.
       
       FST-KEY-CTP SECTION.
       INI-FST-KEY-CTP.
*% INCLUDE INI_FST_KEY_CTP OF 'COLBMCTP.TXT'
           MOVE -1 TO POC-COD-VCOC-LEN IN CTP-FLD.
*% INCLUDE FIN_FST_KEY_CTP OF 'COLBMCTP.TXT'
       FIN-FST-KEY-CTP.
           EXIT.
       
       FST-FLD-CTP SECTION.
       INI-FST-FLD-CTP.
*% INCLUDE INI_FST_FLD_CTP OF 'COLBMCTP.TXT'
*% INCLUDE FIN_FST_FLD_CTP OF 'COLBMCTP.TXT'
       FIN-FST-FLD-CTP.
           EXIT.
       
       PRO-KEY-CTP SECTION.
       INI-PRO-KEY-CTP.
*% INCLUDE INI_PRO_KEY_CTP OF 'COLBMCTP.TXT'
           MOVE FRM-CPIM-AEY TO FRM-COD-NEXT-ATR IN CTP-FLD.
           PERFORM PRO-IKY-CTP.
           PERFORM PRO-AKY-CTP.
           PERFORM PRO-MKY-CTP.
*% INCLUDE FIN_PRO_KEY_CTP OF 'COLBMCTP.TXT'
       FIN-PRO-KEY-CTP.
           EXIT.
       
       PRO-IKY-CTP SECTION.
       INI-PRO-IKY-CTP.
*% INCLUDE INI_PRO_IKY_CTP OF 'COLBMCTP.TXT'
           MOVE FRM-CPIM-AEY TO POC-COD-VCOC-ATR IN CTP-FLD.
           MOVE FRM-CPIM-AEY TO POC-COD-TOOC-ATR IN CTP-FLD.
           MOVE FRM-CPIM-AEY TO POC-COD-AOOC-ATR IN CTP-FLD.
           MOVE FRM-CPIM-AEY TO POC-NUM-IPOC-ATR IN CTP-FLD.
*% INCLUDE FIN_PRO_IKY_CTP OF 'COLBMCTP.TXT'
       FIN-PRO-IKY-CTP.
           EXIT.
       
       PRO-MKY-CTP SECTION.
       INI-PRO-MKY-CTP.
*% INCLUDE INI_PRO_MKY_CTP OF 'COLBMCTP.TXT'
*% INCLUDE FIN_PRO_MKY_CTP OF 'COLBMCTP.TXT'
       FIN-PRO-MKY-CTP.
           EXIT.
       
       PRO-AKY-CTP SECTION.
       INI-PRO-AKY-CTP.
*% INCLUDE INI_PRO_AKY_CTP OF 'COLBMCTP.TXT'
*% INCLUDE FIN_PRO_AKY_CTP OF 'COLBMCTP.TXT'
       FIN-PRO-AKY-CTP.
           EXIT.
       
       UNP-KEY-CTP SECTION.
       INI-UNP-KEY-CTP.
*% INCLUDE INI_UNP_KEY_CTP OF 'COLBMCTP.TXT'
           MOVE FRM-CPIM-UBY TO FRM-COD-NEXT-ATR IN CTP-FLD.
           PERFORM UNP-IKY-CTP.
           PERFORM UNP-AKY-CTP.
           PERFORM UNP-MKY-CTP.
*% INCLUDE FIN_UNP_KEY_CTP OF 'COLBMCTP.TXT'
       FIN-UNP-KEY-CTP.
           EXIT.
       
       UNP-IKY-CTP SECTION.
       INI-UNP-IKY-CTP.
*% INCLUDE INI_UNP_IKY_CTP OF 'COLBMCTP.TXT'
           MOVE FRM-CPIM-UBY TO POC-COD-VCOC-ATR.
           MOVE FRM-CPIM-UBY TO POC-COD-TOOC-ATR.
           MOVE FRM-CPIM-UBY TO POC-COD-AOOC-ATR.
           MOVE FRM-CPIM-UBY TO POC-NUM-IPOC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-COD-NEXT-ATR.
*% INCLUDE FIN_UNP_IKY_CTP OF 'COLBMCTP.TXT'
       FIN-UNP-IKY-CTP.
           EXIT.
       
       UNP-MKY-CTP SECTION.
       INI-UNP-MKY-CTP.
*% INCLUDE INI_UNP_MKY_CTP OF 'COLBMCTP.TXT'
*% INCLUDE FIN_UNP_MKY_CTP OF 'COLBMCTP.TXT'
       FIN-UNP-MKY-CTP.
           EXIT.
       
       UNP-AKY-CTP SECTION.
       INI-UNP-AKY-CTP.
*% INCLUDE INI_UNP_AKY_CTP OF 'COLBMCTP.TXT'
*% INCLUDE FIN_UNP_AKY_CTP OF 'COLBMCTP.TXT'
       FIN-UNP-AKY-CTP.
           EXIT.
       
       UNP-FLD-CTP SECTION.
       INI-UNP-FLD-CTP.
*% INCLUDE INI_UNP_FLD_CTP OF 'COLBMCTP.TXT'
           PERFORM UNP-POC-CTP.
*% INCLUDE FIN_UNP_FLD_CTP OF 'COLBMCTP.TXT'
       FIN-UNP-FLD-CTP.
           EXIT.
       
       UNP-POC-CTP SECTION.
       INI-UNP-POC-CTP.
*% INCLUDE INI_UNP_POC_CTP OF 'COLBMCTP.TXT'
*% INCLUDE FIN_UNP_POC_CTP OF 'COLBMCTP.TXT'
       FIN-UNP-POC-CTP.
           EXIT.
       
       VAL-KEY-CTP SECTION.
       INI-VAL-KEY-CTP.
           PERFORM VAL-IKY-CTP.
           PERFORM VAL-MKY-CTP.
           PERFORM VAL-AKY-CTP.
       FIN-VAL-KEY-CTP.
           EXIT.
       
       VAL-IKY-CTP SECTION.
       INI-VAL-IKY-CTP.
*% INCLUDE INI_VAL_IKY_CTP OF 'COLBMCTP.TXT'
           PERFORM VAL-POC-COD-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO POC-COD-VCOC-LEN IN CTP-FLD
                GO TO FIN-VAL-IKY-CTP.
           PERFORM VAL-POC-COD-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO POC-COD-TOOC-LEN IN CTP-FLD
                GO TO FIN-VAL-IKY-CTP.
           PERFORM VAL-POC-COD-AOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO POC-COD-AOOC-LEN IN CTP-FLD
                GO TO FIN-VAL-IKY-CTP.
           PERFORM VAL-POC-NUM-IPOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO POC-NUM-IPOC-LEN IN CTP-FLD
                GO TO FIN-VAL-IKY-CTP.
           PERFORM VAL-FRM-COD-NEXT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-NEXT-LEN IN CTP-FLD
                GO TO FIN-VAL-IKY-CTP.
*% INCLUDE FIN_VAL_IKY_CTP OF 'COLBMCTP.TXT'
       FIN-VAL-IKY-CTP.
           EXIT.
       
       VAL-MKY-CTP SECTION.
       INI-VAL-MKY-CTP.
*% INCLUDE INI_VAL_MKY_CTP OF 'COLBMCTP.TXT'
*% INCLUDE FIN_VAL_MKY_CTP OF 'COLBMCTP.TXT'
       FIN-VAL-MKY-CTP.
           EXIT.
       
       VAL-AKY-CTP SECTION.
       INI-VAL-AKY-CTP.
*% INCLUDE INI_VAL_AKY_CTP OF 'COLBMCTP.TXT'
*% INCLUDE FIN_VAL_AKY_CTP OF 'COLBMCTP.TXT'
       FIN-VAL-AKY-CTP.
           EXIT.
       
       VAL-POC-COD-VCOC SECTION.
       INI-VAL-POC-COD-VCOC.
*% LBL = "POC_COD_VCOC OF 'COLBMCTP.TXT'"
*% INCLUDE INI_POC_COD_VCOC OF 'COLBMCTP.TXT'
           IF POC-COD-VCOC IN CTP-FLD NOT > SPACES
              MOVE SPACES TO POC-COD-VCOC IN CTP-FLD.
*% INCLUDE POC_COD_VCOC OF 'COLBMCTP.TXT'
*% BUF = "CTP-FLD"
*% SIS = "TAB"
*% TAB = "VLR "
*% FLD = "POC-COD-VCOC"
*% GLS = ""
*% TIENE_GLOSA = FALSE
*% LGL = 0
*% INCLUDE 'VALCOD'
*% INCLUDE FIN_POC_COD_VCOC OF 'COLBMCTP.TXT'
       FIN-VAL-POC-COD-VCOC.
           EXIT.
       
       VAL-POC-COD-TOOC SECTION.
       INI-VAL-POC-COD-TOOC.
*% LBL = "POC_COD_TOOC OF 'COLBMCTP.TXT'"
*% INCLUDE INI_POC_COD_TOOC OF 'COLBMCTP.TXT'
           IF POC-COD-TOOC IN CTP-FLD NOT > SPACES
              MOVE SPACES TO POC-COD-TOOC IN CTP-FLD.
*% INCLUDE POC_COD_TOOC OF 'COLBMCTP.TXT'
*% BUF = "CTP-FLD"
*% SIS = "TAB"
*% TAB = "TIO "
*% FLD = "POC-COD-TOOC"
*% GLS = ""
*% TIENE_GLOSA = FALSE
*% LGL = 0
*% INCLUDE 'VALCOD'
*% INCLUDE FIN_POC_COD_TOOC OF 'COLBMCTP.TXT'
       FIN-VAL-POC-COD-TOOC.
           EXIT.
       
       VAL-POC-COD-AOOC SECTION.
       INI-VAL-POC-COD-AOOC.
*% LBL = "POC_COD_AOOC OF 'COLBMCTP.TXT'"
*% INCLUDE INI_POC_COD_AOOC OF 'COLBMCTP.TXT'
           IF POC-COD-AOOC IN CTP-FLD NOT > SPACES
              MOVE SPACES TO POC-COD-AOOC IN CTP-FLD.
*% INCLUDE POC_COD_AOOC OF 'COLBMCTP.TXT'
*% INCLUDE FIN_POC_COD_AOOC OF 'COLBMCTP.TXT'
       FIN-VAL-POC-COD-AOOC.
           EXIT.
       
       VAL-POC-NUM-IPOC SECTION.
       INI-VAL-POC-NUM-IPOC.
*% LBL = "POC_NUM_IPOC OF 'COLBMCTP.TXT'"
*% INCLUDE INI_POC_NUM_IPOC OF 'COLBMCTP.TXT'
*% INCLUDE POC_NUM_IPOC OF 'COLBMCTP.TXT'
*% INCLUDE FIN_POC_NUM_IPOC OF 'COLBMCTP.TXT'
       FIN-VAL-POC-NUM-IPOC.
           EXIT.
       
       VAL-FRM-COD-NEXT SECTION.
       INI-VAL-FRM-COD-NEXT.
*% LBL = "FRM_COD_NEXT OF 'COLBMCTP.TXT'"
*% INCLUDE INI_FRM_COD_NEXT OF 'COLBMCTP.TXT'
           IF FRM-COD-NEXT IN CTP-FLD NOT > SPACES
              MOVE SPACES TO FRM-COD-NEXT IN CTP-FLD.
*% INCLUDE FRM_COD_NEXT OF 'COLBMCTP.TXT'
*% INCLUDE FIN_FRM_COD_NEXT OF 'COLBMCTP.TXT'
       FIN-VAL-FRM-COD-NEXT.
           EXIT.
       
       VAL-CON-KEY-CTP SECTION.
       INI-VAL-CON-KEY-CTP.
*% INCLUDE INI_VAL_CON_KEY_CTP OF 'COLBMCTP.TXT'
           IF     POC-COD-VCOC IN CTP-FLD NOT > SPACES 
              AND POC-COD-TOOC IN CTP-FLD NOT > SPACES 
              AND POC-COD-AOOC IN CTP-FLD NOT > SPACES 
              AND POC-NUM-IPOC IN CTP-FLD NOT > ZEROES 
              AND FRM-COD-NEXT IN CTP-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CTP.
*% INCLUDE FIN_VAL_CON_KEY_CTP OF 'COLBMCTP.TXT'
       FIN-VAL-CON-KEY-CTP.
           EXIT.
       
       VAL-CON-IKY-CTP SECTION.
       INI-VAL-CON-IKY-CTP.
*% INCLUDE INI_VAL_CON_IKY_CTP OF 'COLBMCTP.TXT'
           IF     POC-COD-VCOC IN CTP-FLD NOT > SPACES 
              AND POC-COD-TOOC IN CTP-FLD NOT > SPACES 
              AND POC-COD-AOOC IN CTP-FLD NOT > SPACES 
              AND POC-NUM-IPOC IN CTP-FLD NOT > ZEROES 
              AND FRM-COD-NEXT IN CTP-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CTP.
*% INCLUDE FIN_VAL_CON_IKY_CTP OF 'COLBMCTP.TXT'
       FIN-VAL-CON-IKY-CTP.
           EXIT.
       
       VAL-NUM-KEY-CTP SECTION.
       INI-VAL-NUM-KEY-CTP.
           PERFORM VAL-NUM-IKY-CTP.
           PERFORM VAL-NUM-MKY-CTP.
           PERFORM VAL-NUM-AKY-CTP.
       FIN-VAL-NUM-KEY-CTP.
           EXIT.
       
       VAL-NUM-IKY-CTP SECTION.
       INI-VAL-NUM-IKY-CTP.
*% INCLUDE INI_VAL_NUM_IKY_CTP OF 'COLBMCTP.TXT'
       VAL-NUM-POC-NUM-IPOC.
*% BUF = "CTP-FLD"
*% FLD = "POC-NUM-IPOC"
*% ENT = "1"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-CTP.
*% INCLUDE FIN_VAL_NUM_IKY_CTP OF 'COLBMCTP.TXT'
       FIN-VAL-NUM-IKY-CTP.
           EXIT.
       
       VAL-NUM-MKY-CTP SECTION.
       INI-VAL-NUM-MKY-CTP.
*% INCLUDE INI_VAL_NUM_MKY_CTP OF 'COLBMCTP.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_CTP OF 'COLBMCTP.TXT'
       FIN-VAL-NUM-MKY-CTP.
           EXIT.
       
       VAL-NUM-AKY-CTP SECTION.
       INI-VAL-NUM-AKY-CTP.
*% INCLUDE INI_VAL_NUM_AKY_CTP OF 'COLBMCTP.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_CTP OF 'COLBMCTP.TXT'
       FIN-VAL-NUM-AKY-CTP.
           EXIT.
       
       VAL-NUM-FLD-CTP SECTION.
       INI-VAL-NUM-FLD-CTP.
*% INCLUDE INI_VAL_NUM_FLD_CTP OF 'COLBMCTP.TXT'
       VAL-NUM-FRM-NUM-TOCC.
*% BUF = "CTP-FLD"
*% FLD = "FRM-NUM-TOCC"
*% ENT = "4"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-CTP.
       VAL-NUM-FRM-NUM-IPOC-1.
*% BUF = "CTP-FLD(1)"
*% FLD = "FRM-NUM-IPOC"
*% ENT = "1"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
