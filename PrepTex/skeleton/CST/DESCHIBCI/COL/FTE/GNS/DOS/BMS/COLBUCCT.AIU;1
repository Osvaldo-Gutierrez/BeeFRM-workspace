*% FRM = "CCT"
      *
      * COLBUCCT
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 03:24 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMCCT.TXT'
       SET-TAG-COLBUCCT SECTION.
       INI-SET-TAG-COLBUCCT.
           MOVE
           '<IDNMOD NOM=$COLBUCCT$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 03:24 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCCT.
           EXIT.
           
       RET-ALL-CCT SECTION.
       INI-RET-ALL-CCT.
*% INCLUDE INI_RET_ALL_CCT OF 'COLBMCCT.TXT'
           PERFORM KEY-ALL-CCT.
           PERFORM RET-OPC-CCT.
           PERFORM RET-CCT-CCT.
           PERFORM RET-VEN-CCT.
           PERFORM RET-DLC-CCT.
*% INCLUDE FIN_RET_ALL_CCT OF 'COLBMCCT.TXT'
       FIN-RET-ALL-CCT.
           EXIT.
       
       RET-OPC-CCT SECTION.
       INI-RET-OPC-CCT.
*% INCLUDE INI_RET_OPC_CCT OF 'COLBMCCT.TXT'
           MOVE OPC-MSC-TVOC IN CCT-FLD TO OPC-MSC-TVOC IN OPC.
           MOVE OPC-COD-TOOC IN CCT-FLD TO OPC-COD-TOOC IN OPC.
           MOVE OPC-COD-OFOC IN CCT-FLD TO OPC-COD-OFOC IN OPC.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE OPC-NUM-DCOL IN CCT-FLD TO OPC-NUM-DCOL IN OPC.
           MOVE OPC-NUM-MCOL IN CCT-FLD TO OPC-NUM-MCOL IN OPC.
           MOVE OPC-NUM-SCOL IN CCT-FLD TO OPC-NUM-SCOL IN OPC.
           MOVE OPC-NUM-ACOL IN CCT-FLD TO OPC-NUM-ACOL IN OPC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE OPC-COD-VCOC IN CCT-FLD TO OPC-COD-VCOC IN OPC.
*% INCLUDE FIN_RET_OPC_CCT OF 'COLBMCCT.TXT'
       FIN-RET-OPC-CCT.
           EXIT.
       
       RET-CCT-CCT SECTION.
       INI-RET-CCT-CCT.
*% INCLUDE INI_RET_CCT_CCT OF 'COLBMCCT.TXT'
           MOVE CCT-IND-SCTA IN CCT-FLD TO CCT-IND-SCTA IN CCT.
*% RREG = "CCT"
*% RREG_FRM = "CCT"
*% NOMB = "FCCT"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CCT-NUM-DCCT IN CCT-FLD TO CCT-NUM-DCCT IN CCT.
           MOVE CCT-NUM-MCCT IN CCT-FLD TO CCT-NUM-MCCT IN CCT.
           MOVE CCT-NUM-SCCT IN CCT-FLD TO CCT-NUM-SCCT IN CCT.
           MOVE CCT-NUM-ACCT IN CCT-FLD TO CCT-NUM-ACCT IN CCT.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CCT-IND-SCTB IN CCT-FLD TO CCT-IND-SCTB IN CCT.
           MOVE CCT-COD-RCCT IN CCT-FLD TO CCT-COD-RCCT IN CCT.
           MOVE CCT-IND-ACEL IN CCT-FLD TO CCT-IND-ACEL IN CCT.
*% INCLUDE FIN_RET_CCT_CCT OF 'COLBMCCT.TXT'
       FIN-RET-CCT-CCT.
           EXIT.
       
       RET-VEN-CCT SECTION.
       INI-RET-VEN-CCT.
*% INCLUDE INI_RET_VEN_CCT OF 'COLBMCCT.TXT'
           MOVE VEN-IND-SCTB IN CCT-FLD TO VEN-IND-SCTB IN VEN.
           MOVE VEN-VAL-SCAP IN CCT-FLD TO VEN-VAL-SCAP IN VEN.
*% RREG = "VEN"
*% RREG_FRM = "VEN"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE VEN-NUM-DVEN IN CCT-FLD TO VEN-NUM-DVEN IN VEN.
           MOVE VEN-NUM-MVEN IN CCT-FLD TO VEN-NUM-MVEN IN VEN.
           MOVE VEN-NUM-SVEN IN CCT-FLD TO VEN-NUM-SVEN IN VEN.
           MOVE VEN-NUM-AVEN IN CCT-FLD TO VEN-NUM-AVEN IN VEN.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE VEN-VAL-SFIN IN CCT-FLD TO VEN-VAL-SFIN IN VEN.
*% RREG = "VEN"
*% RREG_FRM = "VEN"
*% NOMB = "FIMV"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE VEN-NUM-DIMV IN CCT-FLD TO VEN-NUM-DIMV IN VEN.
           MOVE VEN-NUM-MIMV IN CCT-FLD TO VEN-NUM-MIMV IN VEN.
           MOVE VEN-NUM-SIMV IN CCT-FLD TO VEN-NUM-SIMV IN VEN.
           MOVE VEN-NUM-AIMV IN CCT-FLD TO VEN-NUM-AIMV IN VEN.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE VEN-IND-SCBZ IN CCT-FLD TO VEN-IND-SCBZ IN VEN.
*% INCLUDE FIN_RET_VEN_CCT OF 'COLBMCCT.TXT'
       FIN-RET-VEN-CCT.
           EXIT.
       
       RET-DLC-CCT SECTION.
       INI-RET-DLC-CCT.
*% INCLUDE INI_RET_DLC_CCT OF 'COLBMCCT.TXT'
           MOVE DLC-IND-SCTB IN CCT-FLD TO DLC-IND-SCTB IN DLC.
           MOVE DLC-VAL-SDLC IN CCT-FLD TO DLC-VAL-SDLC IN DLC.
           MOVE DLC-COD-UBFD IN CCT-FLD TO DLC-COD-UBFD IN DLC.
*% INCLUDE FIN_RET_DLC_CCT OF 'COLBMCCT.TXT'
       FIN-RET-DLC-CCT.
           EXIT.
       
       KEY-ALL-CCT SECTION.
       INI-KEY-ALL-CCT.
*% INCLUDE INI_KEY_ALL_CCT OF 'COLBMCCT.TXT'
           PERFORM KEY-OPC-CCT.
           PERFORM KEY-CCT-CCT.
           PERFORM KEY-VEN-CCT.
           PERFORM KEY-DLC-CCT.
*% INCLUDE FIN_KEY_ALL_CCT OF 'COLBMCCT.TXT'
       FIN-KEY-ALL-CCT.
           EXIT.
       
       KEY-OPC-CCT SECTION.
       INI-KEY-OPC-CCT.
*% INCLUDE INI_KEY_OPC_CCT OF 'COLBMCCT.TXT'
*% INCLUDE FIN_KEY_OPC_CCT OF 'COLBMCCT.TXT'
       FIN-KEY-OPC-CCT.
           EXIT.
       
       KEY-CCT-CCT SECTION.
       INI-KEY-CCT-CCT.
*% INCLUDE INI_KEY_CCT_CCT OF 'COLBMCCT.TXT'
           MOVE CCT-CAI-IOPC IN CCT-FLD TO CCT-CAI-IOPC IN CCT.
           MOVE CCT-IIC-IOPC IN CCT-FLD TO CCT-IIC-IOPC IN CCT.
           MOVE CCT-NUM-IDLC IN CCT-FLD TO CCT-NUM-IDLC IN CCT.
           MOVE CCT-NUM-ICCT IN CCT-FLD TO CCT-NUM-ICCT IN CCT.
*% INCLUDE FIN_KEY_CCT_CCT OF 'COLBMCCT.TXT'
       FIN-KEY-CCT-CCT.
           EXIT.
       
       KEY-VEN-CCT SECTION.
       INI-KEY-VEN-CCT.
*% INCLUDE INI_KEY_VEN_CCT OF 'COLBMCCT.TXT'
           MOVE VEN-NUM-IVCT IN CCT-FLD TO VEN-NUM-IVCT IN VEN.
           MOVE VEN-NUM-IVEN IN CCT-FLD TO VEN-NUM-IVEN IN VEN.
*% INCLUDE FIN_KEY_VEN_CCT OF 'COLBMCCT.TXT'
       FIN-KEY-VEN-CCT.
           EXIT.
       
       KEY-DLC-CCT SECTION.
       INI-KEY-DLC-CCT.
*% INCLUDE INI_KEY_DLC_CCT OF 'COLBMCCT.TXT'
*% INCLUDE FIN_KEY_DLC_CCT OF 'COLBMCCT.TXT'
       FIN-KEY-DLC-CCT.
           EXIT.
       
       SCR-CLRA-CCT SECTION.
       INI-SCR-CLRA-CCT.
*% INCLUDE INI_SCR_CLRA_CCT OF 'COLBMCCT.TXT'
           PERFORM CLR-KEY-CCT.
           PERFORM CLR-FLD-CCT.
*% INCLUDE FIN_SCR_CLRA_CCT OF 'COLBMCCT.TXT'
       FIN-SCR-CLRA-CCT.
           EXIT.
       
       CLR-KEY-CCT SECTION.
       INI-CLR-KEY-CCT.
*% INCLUDE INI_CLR_KEY_CCT OF 'COLBMCCT.TXT'
           MOVE SPACES TO CCT-CAI-IOPC IN CCT-FLD.
           MOVE ZEROES TO CCT-IIC-IOPC IN CCT-FLD.
           MOVE ZEROES TO VEN-NUM-IVCT IN CCT-FLD.
           MOVE ZEROES TO CCT-NUM-IDLC IN CCT-FLD.
           MOVE ZEROES TO VEN-NUM-IVEN IN CCT-FLD.
           MOVE ZEROES TO CCT-NUM-ICCT IN CCT-FLD.
           MOVE SPACES TO FRM-GLS-NOMC IN CCT-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-ICLI IN CCT-FLD.
           MOVE SPACES TO FRM-VRF-ICLI IN CCT-FLD.
           MOVE SPACES TO FRM-IND-ICLI IN CCT-FLD.
           MOVE SPACES TO FRM-GLS-ICLI IN CCT-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-CAI-ICLI IN CCT-FLD.
           MOVE SPACES TO FRM-IIC-ICLI IN CCT-FLD.
           MOVE SPACES TO FRM-GLS-NOMA IN CCT-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-IAVA IN CCT-FLD.
           MOVE SPACES TO FRM-VRF-IAVA IN CCT-FLD.
           MOVE SPACES TO FRM-IND-IAVA IN CCT-FLD.
           MOVE SPACES TO FRM-GLS-IAVA IN CCT-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-CAI-IAVA IN CCT-FLD.
           MOVE SPACES TO FRM-IIC-IAVA IN CCT-FLD.
*% INCLUDE FIN_CLR_KEY_CCT OF 'COLBMCCT.TXT'
       FIN-CLR-KEY-CCT.
           EXIT.
       
       CLR-FLD-CCT SECTION.
       INI-CLR-FLD-CCT.
*% INCLUDE INI_CLR_FLD_CCT OF 'COLBMCCT.TXT'
           MOVE SPACES TO FRM-GLS-TVOC IN CCT-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN CCT-FLD.
           MOVE SPACES TO FRM-GLS-OFOC IN CCT-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN CCT-FLD.
           MOVE SPACES TO FRM-GLS-UBFD IN CCT-FLD.
           MOVE SPACES TO FRM-GLS-SCBZ IN CCT-FLD.
           MOVE SPACES TO FRM-GLS-SCTA IN CCT-FLD.
           MOVE SPACES TO FRM-GLS-SCTB IN CCT-FLD.
           MOVE SPACES TO FRM-GLS-RCCT IN CCT-FLD.
           PERFORM CLR-OPC-CCT.
           PERFORM CLR-CCT-CCT.
           PERFORM CLR-VEN-CCT.
           PERFORM CLR-DLC-CCT.
*% INCLUDE FIN_CLR_FLD_CCT OF 'COLBMCCT.TXT'
       FIN-CLR-FLD-CCT.
           EXIT.
       
       CLR-OPC-CCT SECTION.
       INI-CLR-OPC-CCT.
*% INCLUDE INI_CLR_OPC_CCT OF 'COLBMCCT.TXT'
           MOVE SPACES TO OPC-MSC-TVOC IN CCT-FLD.
           MOVE SPACES TO OPC-COD-TOOC IN CCT-FLD.
           MOVE SPACES TO OPC-COD-OFOC IN CCT-FLD.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO OPC-FEC-FCOL IN CCT-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO OPC-COD-VCOC IN CCT-FLD.
*% INCLUDE FIN_CLR_OPC_CCT OF 'COLBMCCT.TXT'
       FIN-CLR-OPC-CCT.
           EXIT.
       
