           MOVE CRS-NUM-MCOL-EDT IN CRS-EDT TO 
                CRS-NUM-MCOL-ALF IN CRS-FLD.
           IF CRS-NUM-ACOL-LEN IN CRS-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CRS-NUM-ACOL-LEN IN CRS-FLD
                GO TO FIN-EDT-FLD-CRS.
           MOVE CRS-NUM-ACOL IN CRS-FLD TO CRS-NUM-ACOL-EDT IN CRS-EDT.
           MOVE CRS-NUM-ACOL-EDT IN CRS-EDT TO 
                CRS-NUM-ACOL-ALF IN CRS-FLD.
           IF CRS-VAL-SCAP-LEN IN CRS-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CRS-VAL-SCAP-LEN IN CRS-FLD
                GO TO FIN-EDT-FLD-CRS.
           MOVE CRS-VAL-SCAP IN CRS-FLD TO CRS-VAL-SCAP-EDT IN CRS-EDT.
           MOVE CRS-VAL-SCAP-EDT IN CRS-EDT TO 
                CRS-VAL-SCAP-ALF IN CRS-FLD.
           IF CRS-VAL-SDLC-LEN IN CRS-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CRS-VAL-SDLC-LEN IN CRS-FLD
                GO TO FIN-EDT-FLD-CRS.
           MOVE CRS-VAL-SDLC IN CRS-FLD TO CRS-VAL-SDLC-EDT IN CRS-EDT.
           MOVE CRS-VAL-SDLC-EDT IN CRS-EDT TO 
                CRS-VAL-SDLC-ALF IN CRS-FLD.
           IF CRS-VAL-CRED-LEN IN CRS-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CRS-VAL-CRED-LEN IN CRS-FLD
                GO TO FIN-EDT-FLD-CRS.
           MOVE CRS-VAL-CRED IN CRS-FLD TO CRS-VAL-CRED-EDT IN CRS-EDT.
           MOVE CRS-VAL-CRED-EDT IN CRS-EDT TO 
                CRS-VAL-CRED-ALF IN CRS-FLD.
*% INCLUDE FIN_EDT_FLD_CRS OF 'COLBMCRS.TXT'
       FIN-EDT-FLD-CRS.
           EXIT.
*% INCLUDE EOF_BQ OF 'COLBMCRS.TXT'
*% INCLUDE EOF OF 'COLBMCRS.TXT'
*% INCLUDE EOF_IBM OF 'COLBMCRS.TXT'
