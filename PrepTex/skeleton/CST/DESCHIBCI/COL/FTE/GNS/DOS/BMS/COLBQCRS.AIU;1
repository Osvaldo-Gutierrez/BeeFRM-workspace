      *
      * COLBQCRS
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:36 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CRS"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMCRS.TXT'
       SET-TAG-COLBQCRS SECTION.
       INI-SET-TAG-COLBQCRS.
           MOVE
           '<IDNMOD NOM=$COLBQCRS$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:36 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCRS.
           EXIT.
           
       PUT-ALL-CRS SECTION.
       INI-PUT-ALL-CRS.
*% INCLUDE INI_PUT_ALL_CRS OF 'COLBMCRS.TXT'
           PERFORM PUT-CRS-CRS.
*% INCLUDE FIN_PUT_ALL_CRS OF 'COLBMCRS.TXT'
       FIN-PUT-ALL-CRS.
           EXIT.
       
       PUT-CRS-CRS SECTION.
       INI-PUT-CRS-CRS.
*% INCLUDE INI_PUT_CRS_CRS OF 'COLBMCRS.TXT'
           IF PGM-STAT-CRS-NUL
               GO TO FIN-PUT-CRS-CRS.
           IF NOT PGM-STAT-CRS-OKS
               PERFORM CLR-CRS-CRS
               GO TO FIN-PUT-CRS-CRS.
           MOVE CRS-VAL-VENP IN CRS TO CRS-VAL-VENP IN CRS-FLD.
           MOVE CRS-VAL-VCCP IN CRS TO CRS-VAL-VCCP IN CRS-FLD.
           MOVE CRS-VAL-PAGO IN CRS TO CRS-VAL-PAGO IN CRS-FLD.
           MOVE CRS-VAL-PNEG IN CRS TO CRS-VAL-PNEG IN CRS-FLD.
           MOVE CRS-VAL-VCNC IN CRS TO CRS-VAL-VCNC IN CRS-FLD.
           MOVE CRS-VAL-GNEG IN CRS TO CRS-VAL-GNEG IN CRS-FLD.
           MOVE CRS-VAL-MNEG IN CRS TO CRS-VAL-MNEG IN CRS-FLD.
           MOVE CRS-COD-OFRC IN CRS TO CRS-COD-OFRC IN CRS-FLD.
           MOVE CRS-COD-OFOC IN CRS TO CRS-COD-OFOC IN CRS-FLD.
           MOVE CRS-COD-MONS IN CRS TO CRS-COD-MONS IN CRS-FLD.
           MOVE CRS-NUM-DVCN IN CRS TO CRS-NUM-DVCN IN CRS-FLD.
           MOVE CRS-NUM-MVCN IN CRS TO CRS-NUM-MVCN IN CRS-FLD.
           MOVE CRS-NUM-AVCN IN CRS TO CRS-NUM-AVCN IN CRS-FLD.
           MOVE CRS-NUM-DCAN IN CRS TO CRS-NUM-DCAN IN CRS-FLD.
           MOVE CRS-NUM-MCAN IN CRS TO CRS-NUM-MCAN IN CRS-FLD.
           MOVE CRS-NUM-ACAN IN CRS TO CRS-NUM-ACAN IN CRS-FLD.
           MOVE CRS-NUM-VNEG IN CRS TO CRS-NUM-VNEG IN CRS-FLD.
           MOVE CRS-IND-ACTU IN CRS TO CRS-IND-ACTU IN CRS-FLD.
           MOVE CRS-IND-PNEG IN CRS TO CRS-IND-PNEG IN CRS-FLD.
           MOVE CRS-NUM-IVEN IN CRS TO CRS-NUM-IVEN IN CRS-FLD.
           MOVE CRS-NUM-DVEN IN CRS TO CRS-NUM-DVEN IN CRS-FLD.
           MOVE CRS-NUM-MVEN IN CRS TO CRS-NUM-MVEN IN CRS-FLD.
           MOVE CRS-NUM-AVEN IN CRS TO CRS-NUM-AVEN IN CRS-FLD.
           MOVE CRS-NUM-DIMV IN CRS TO CRS-NUM-DIMV IN CRS-FLD.
           MOVE CRS-NUM-MIMV IN CRS TO CRS-NUM-MIMV IN CRS-FLD.
           MOVE CRS-NUM-AIMV IN CRS TO CRS-NUM-AIMV IN CRS-FLD.
           MOVE CRS-VAL-TASA IN CRS TO CRS-VAL-TASA IN CRS-FLD.
           MOVE CRS-NUM-IDEU IN CRS TO CRS-NUM-IDEU IN CRS-FLD.
           MOVE CRS-IND-IDEU IN CRS TO CRS-IND-IDEU IN CRS-FLD.
           MOVE CRS-GLS-IDEU IN CRS TO CRS-GLS-IDEU IN CRS-FLD.
           MOVE CRS-COD-AUXI IN CRS TO CRS-COD-AUXI IN CRS-FLD.
           MOVE CRS-VAL-MORA IN CRS TO CRS-VAL-MORA IN CRS-FLD.
           MOVE CRS-NUM-DCOL IN CRS TO CRS-NUM-DCOL IN CRS-FLD.
           MOVE CRS-NUM-MCOL IN CRS TO CRS-NUM-MCOL IN CRS-FLD.
           MOVE CRS-NUM-ACOL IN CRS TO CRS-NUM-ACOL IN CRS-FLD.
           MOVE CRS-IND-PLZO IN CRS TO CRS-IND-PLZO IN CRS-FLD.
           MOVE CRS-IND-TCRE IN CRS TO CRS-IND-TCRE IN CRS-FLD.
           MOVE CRS-KEY-IOPC IN CRS TO CRS-KEY-IOPC IN CRS-FLD.
           MOVE CRS-IND-TSIS IN CRS TO CRS-IND-TSIS IN CRS-FLD.
           MOVE CRS-GLS-ICRS IN CRS TO CRS-GLS-ICRS IN CRS-FLD.
           MOVE CRS-MSC-CANC IN CRS TO CRS-MSC-CANC IN CRS-FLD.
           MOVE CRS-VAL-SCAP IN CRS TO CRS-VAL-SCAP IN CRS-FLD.
           MOVE CRS-VAL-SDLC IN CRS TO CRS-VAL-SDLC IN CRS-FLD.
           MOVE CRS-IND-CCOM IN CRS TO CRS-IND-CCOM IN CRS-FLD.
           MOVE CRS-VAL-CRED IN CRS TO CRS-VAL-CRED IN CRS-FLD.
*% INCLUDE FIN_PUT_CRS_CRS OF 'COLBMCRS.TXT'
       FIN-PUT-CRS-CRS.
           EXIT.
       
       KEY-ALL-CRS SECTION.
       INI-KEY-ALL-CRS.
*% INCLUDE INI_KEY_ALL_CRS OF 'COLBMCRS.TXT'
           PERFORM KEY-CRS-CRS.
*% INCLUDE FIN_KEY_ALL_CRS OF 'COLBMCRS.TXT'
       FIN-KEY-ALL-CRS.
           EXIT.
       
       KEY-CRS-CRS SECTION.
       INI-KEY-CRS-CRS.
*% INCLUDE INI_KEY_CRS_CRS OF 'COLBMCRS.TXT'
*% INCLUDE FIN_KEY_CRS_CRS OF 'COLBMCRS.TXT'
       FIN-KEY-CRS-CRS.
           EXIT.
       
       SCR-CLRA-CRS SECTION.
       INI-SCR-CLRA-CRS.
*% INCLUDE INI_SCR_CLRA_CRS OF 'COLBMCRS.TXT'
           PERFORM CLR-KEY-CRS.
           PERFORM CLR-FLD-CRS.
*% INCLUDE FIN_SCR_CLRA_CRS OF 'COLBMCRS.TXT'
       FIN-SCR-CLRA-CRS.
           EXIT.
       
       CLR-KEY-CRS SECTION.
       INI-CLR-KEY-CRS.
*% INCLUDE INI_CLR_KEY_CRS OF 'COLBMCRS.TXT'
           MOVE SPACES TO FRM-CAI-ICRS IN CRS-FLD.
           MOVE ZEROES TO FRM-IIC-ICRS IN CRS-FLD.
*% INCLUDE FIN_CLR_KEY_CRS OF 'COLBMCRS.TXT'
       FIN-CLR-KEY-CRS.
           EXIT.
       
       CLR-FLD-CRS SECTION.
       INI-CLR-FLD-CRS.
*% INCLUDE INI_CLR_FLD_CRS OF 'COLBMCRS.TXT'
           PERFORM CLR-CRS-CRS.
*% INCLUDE FIN_CLR_FLD_CRS OF 'COLBMCRS.TXT'
       FIN-CLR-FLD-CRS.
           EXIT.
       
       CLR-CRS-CRS SECTION.
       INI-CLR-CRS-CRS.
*% INCLUDE INI_CLR_CRS_CRS OF 'COLBMCRS.TXT'
           MOVE ZEROES TO CRS-VAL-VENP IN CRS-FLD.
           MOVE ZEROES TO CRS-VAL-VCCP IN CRS-FLD.
           MOVE ZEROES TO CRS-VAL-PAGO IN CRS-FLD.
           MOVE ZEROES TO CRS-VAL-PNEG IN CRS-FLD.
           MOVE ZEROES TO CRS-VAL-VCNC IN CRS-FLD.
           MOVE ZEROES TO CRS-VAL-GNEG IN CRS-FLD.
           MOVE ZEROES TO CRS-VAL-MNEG IN CRS-FLD.
           MOVE SPACES TO CRS-COD-OFRC IN CRS-FLD.
           MOVE SPACES TO CRS-COD-OFOC IN CRS-FLD.
           MOVE SPACES TO CRS-COD-MONS IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-DVCN IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-MVCN IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-AVCN IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-DCAN IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-MCAN IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-ACAN IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-VNEG IN CRS-FLD.
           MOVE SPACES TO CRS-IND-ACTU IN CRS-FLD.
           MOVE SPACES TO CRS-IND-PNEG IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-IVEN IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-DVEN IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-MVEN IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-AVEN IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-DIMV IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-MIMV IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-AIMV IN CRS-FLD.
           MOVE ZEROES TO CRS-VAL-TASA IN CRS-FLD.
           MOVE SPACES TO CRS-NUM-IDEU IN CRS-FLD.
           MOVE SPACES TO CRS-IND-IDEU IN CRS-FLD.
           MOVE SPACES TO CRS-GLS-IDEU IN CRS-FLD.
           MOVE SPACES TO CRS-COD-AUXI IN CRS-FLD.
           MOVE ZEROES TO CRS-VAL-MORA IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-DCOL IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-MCOL IN CRS-FLD.
           MOVE ZEROES TO CRS-NUM-ACOL IN CRS-FLD.
           MOVE SPACES TO CRS-IND-PLZO IN CRS-FLD.
           MOVE SPACES TO CRS-IND-TCRE IN CRS-FLD.
           MOVE SPACES TO CRS-KEY-IOPC IN CRS-FLD.
           MOVE SPACES TO CRS-IND-TSIS IN CRS-FLD.
           MOVE SPACES TO CRS-GLS-ICRS IN CRS-FLD.
           MOVE SPACES TO CRS-MSC-CANC IN CRS-FLD.
           MOVE ZEROES TO CRS-VAL-SCAP IN CRS-FLD.
           MOVE ZEROES TO CRS-VAL-SDLC IN CRS-FLD.
           MOVE SPACES TO CRS-IND-CCOM IN CRS-FLD.
           MOVE ZEROES TO CRS-VAL-CRED IN CRS-FLD.
*% INCLUDE FIN_CLR_CRS_CRS OF 'COLBMCRS.TXT'
       FIN-CLR-CRS-CRS.
           EXIT.
       
       FST-KEY-CRS SECTION.
       INI-FST-KEY-CRS.
*% INCLUDE INI_FST_KEY_CRS OF 'COLBMCRS.TXT'
           MOVE -1 TO FRM-CAI-ICRS-LEN IN CRS-FLD.
*% INCLUDE FIN_FST_KEY_CRS OF 'COLBMCRS.TXT'
       FIN-FST-KEY-CRS.
           EXIT.
       
       FST-FLD-CRS SECTION.
       INI-FST-FLD-CRS.
*% INCLUDE INI_FST_FLD_CRS OF 'COLBMCRS.TXT'
           MOVE -1 TO CRS-VAL-VENP-LEN IN CRS-FLD.
*% INCLUDE FIN_FST_FLD_CRS OF 'COLBMCRS.TXT'
       FIN-FST-FLD-CRS.
           EXIT.
       
       PRO-KEY-CRS SECTION.
       INI-PRO-KEY-CRS.
*% INCLUDE INI_PRO_KEY_CRS OF 'COLBMCRS.TXT'
           MOVE FRM-CPIM-AEY TO FRM-CAI-ICRS-ATR IN CRS-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IIC-ICRS-ATR IN CRS-FLD.
           PERFORM PRO-IKY-CRS.
           PERFORM PRO-AKY-CRS.
           PERFORM PRO-MKY-CRS.
*% INCLUDE FIN_PRO_KEY_CRS OF 'COLBMCRS.TXT'
       FIN-PRO-KEY-CRS.
           EXIT.
       
       PRO-IKY-CRS SECTION.
       INI-PRO-IKY-CRS.
*% INCLUDE INI_PRO_IKY_CRS OF 'COLBMCRS.TXT'
*% INCLUDE FIN_PRO_IKY_CRS OF 'COLBMCRS.TXT'
       FIN-PRO-IKY-CRS.
           EXIT.
       
       PRO-MKY-CRS SECTION.
       INI-PRO-MKY-CRS.
*% INCLUDE INI_PRO_MKY_CRS OF 'COLBMCRS.TXT'
*% INCLUDE FIN_PRO_MKY_CRS OF 'COLBMCRS.TXT'
       FIN-PRO-MKY-CRS.
           EXIT.
       
       PRO-AKY-CRS SECTION.
       INI-PRO-AKY-CRS.
*% INCLUDE INI_PRO_AKY_CRS OF 'COLBMCRS.TXT'
*% INCLUDE FIN_PRO_AKY_CRS OF 'COLBMCRS.TXT'
       FIN-PRO-AKY-CRS.
           EXIT.
       
       UNP-KEY-CRS SECTION.
       INI-UNP-KEY-CRS.
*% INCLUDE INI_UNP_KEY_CRS OF 'COLBMCRS.TXT'
           MOVE FRM-CPIM-UBY TO FRM-CAI-ICRS-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IIC-ICRS-ATR IN CRS-FLD.
           PERFORM UNP-IKY-CRS.
           PERFORM UNP-AKY-CRS.
           PERFORM UNP-MKY-CRS.
*% INCLUDE FIN_UNP_KEY_CRS OF 'COLBMCRS.TXT'
       FIN-UNP-KEY-CRS.
           EXIT.
       
       UNP-IKY-CRS SECTION.
       INI-UNP-IKY-CRS.
*% INCLUDE INI_UNP_IKY_CRS OF 'COLBMCRS.TXT'
           MOVE FRM-CPIM-UBY TO FRM-CAI-ICRS-ATR.
           MOVE FRM-CPIM-UBY TO FRM-IIC-ICRS-ATR.
*% INCLUDE FIN_UNP_IKY_CRS OF 'COLBMCRS.TXT'
       FIN-UNP-IKY-CRS.
           EXIT.
       
       UNP-MKY-CRS SECTION.
       INI-UNP-MKY-CRS.
*% INCLUDE INI_UNP_MKY_CRS OF 'COLBMCRS.TXT'
*% INCLUDE FIN_UNP_MKY_CRS OF 'COLBMCRS.TXT'
       FIN-UNP-MKY-CRS.
           EXIT.
       
       UNP-AKY-CRS SECTION.
       INI-UNP-AKY-CRS.
*% INCLUDE INI_UNP_AKY_CRS OF 'COLBMCRS.TXT'
*% INCLUDE FIN_UNP_AKY_CRS OF 'COLBMCRS.TXT'
       FIN-UNP-AKY-CRS.
           EXIT.
       
       UNP-FLD-CRS SECTION.
       INI-UNP-FLD-CRS.
*% INCLUDE INI_UNP_FLD_CRS OF 'COLBMCRS.TXT'
           PERFORM UNP-CRS-CRS.
*% INCLUDE FIN_UNP_FLD_CRS OF 'COLBMCRS.TXT'
       FIN-UNP-FLD-CRS.
           EXIT.
       
       UNP-CRS-CRS SECTION.
       INI-UNP-CRS-CRS.
*% INCLUDE INI_UNP_CRS_CRS OF 'COLBMCRS.TXT'
           MOVE FRM-CPIM-UBY TO CRS-VAL-VENP-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-VAL-VCCP-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-VAL-PAGO-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-VAL-PNEG-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-VAL-VCNC-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-VAL-GNEG-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-VAL-MNEG-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-COD-OFRC-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-COD-OFOC-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-COD-MONS-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-NUM-DVCN-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-NUM-MVCN-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-NUM-AVCN-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-NUM-DCAN-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-NUM-MCAN-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-NUM-ACAN-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-NUM-VNEG-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-IND-ACTU-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-IND-PNEG-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-NUM-IVEN-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-NUM-DVEN-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-NUM-MVEN-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-NUM-AVEN-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-NUM-DIMV-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-NUM-MIMV-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-NUM-AIMV-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-VAL-TASA-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-NUM-IDEU-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-IND-IDEU-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-GLS-IDEU-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-COD-AUXI-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-VAL-MORA-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-NUM-DCOL-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-NUM-MCOL-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-NUM-ACOL-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-IND-PLZO-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-IND-TCRE-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-KEY-IOPC-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-IND-TSIS-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-GLS-ICRS-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-MSC-CANC-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-VAL-SCAP-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-VAL-SDLC-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-IND-CCOM-ATR IN CRS-FLD.
           MOVE FRM-CPIM-UBY TO CRS-VAL-CRED-ATR IN CRS-FLD.
*% INCLUDE FIN_UNP_CRS_CRS OF 'COLBMCRS.TXT'
       FIN-UNP-CRS-CRS.
           EXIT.
       
       VAL-KEY-CRS SECTION.
       INI-VAL-KEY-CRS.
           PERFORM VAL-IKY-CRS.
           PERFORM VAL-MKY-CRS.
           PERFORM VAL-AKY-CRS.
       FIN-VAL-KEY-CRS.
           EXIT.
       
       VAL-IKY-CRS SECTION.
       INI-VAL-IKY-CRS.
*% INCLUDE INI_VAL_IKY_CRS OF 'COLBMCRS.TXT'
           PERFORM VAL-FRM-CAI-ICRS.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-ICRS-LEN IN CRS-FLD
                GO TO FIN-VAL-IKY-CRS.
           PERFORM VAL-FRM-IIC-ICRS.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-ICRS-LEN IN CRS-FLD
                GO TO FIN-VAL-IKY-CRS.
*% INCLUDE FIN_VAL_IKY_CRS OF 'COLBMCRS.TXT'
       FIN-VAL-IKY-CRS.
           EXIT.
       
       VAL-MKY-CRS SECTION.
       INI-VAL-MKY-CRS.
*% INCLUDE INI_VAL_MKY_CRS OF 'COLBMCRS.TXT'
*% INCLUDE FIN_VAL_MKY_CRS OF 'COLBMCRS.TXT'
       FIN-VAL-MKY-CRS.
           EXIT.
       
       VAL-AKY-CRS SECTION.
       INI-VAL-AKY-CRS.
*% INCLUDE INI_VAL_AKY_CRS OF 'COLBMCRS.TXT'
*% INCLUDE FIN_VAL_AKY_CRS OF 'COLBMCRS.TXT'
       FIN-VAL-AKY-CRS.
           EXIT.
       
       VAL-FRM-CAI-ICRS SECTION.
       INI-VAL-FRM-CAI-ICRS.
*% LBL = "FRM_CAI_ICRS OF 'COLBMCRS.TXT'"
*% INCLUDE INI_FRM_CAI_ICRS OF 'COLBMCRS.TXT'
           IF FRM-CAI-ICRS IN CRS-FLD NOT > SPACES
              MOVE SPACES TO FRM-CAI-ICRS IN CRS-FLD.
*% INCLUDE FRM_CAI_ICRS OF 'COLBMCRS.TXT'
*% INCLUDE FIN_FRM_CAI_ICRS OF 'COLBMCRS.TXT'
       FIN-VAL-FRM-CAI-ICRS.
           EXIT.
       
       VAL-FRM-IIC-ICRS SECTION.
       INI-VAL-FRM-IIC-ICRS.
*% LBL = "FRM_IIC_ICRS OF 'COLBMCRS.TXT'"
*% INCLUDE INI_FRM_IIC_ICRS OF 'COLBMCRS.TXT'
*% INCLUDE FRM_IIC_ICRS OF 'COLBMCRS.TXT'
*% INCLUDE FIN_FRM_IIC_ICRS OF 'COLBMCRS.TXT'
       FIN-VAL-FRM-IIC-ICRS.
           EXIT.
       
       VAL-CON-KEY-CRS SECTION.
       INI-VAL-CON-KEY-CRS.
*% INCLUDE INI_VAL_CON_KEY_CRS OF 'COLBMCRS.TXT'
           IF     FRM-CAI-ICRS IN CRS-FLD NOT > SPACES 
              AND FRM-IIC-ICRS IN CRS-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CRS.
*% INCLUDE FIN_VAL_CON_KEY_CRS OF 'COLBMCRS.TXT'
       FIN-VAL-CON-KEY-CRS.
           EXIT.
       
       VAL-CON-IKY-CRS SECTION.
       INI-VAL-CON-IKY-CRS.
*% INCLUDE INI_VAL_CON_IKY_CRS OF 'COLBMCRS.TXT'
           IF     FRM-CAI-ICRS IN CRS-FLD NOT > SPACES 
              AND FRM-IIC-ICRS IN CRS-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CRS.
*% INCLUDE FIN_VAL_CON_IKY_CRS OF 'COLBMCRS.TXT'
       FIN-VAL-CON-IKY-CRS.
           EXIT.
       
       VAL-NUM-KEY-CRS SECTION.
       INI-VAL-NUM-KEY-CRS.
           PERFORM VAL-NUM-IKY-CRS.
           PERFORM VAL-NUM-MKY-CRS.
           PERFORM VAL-NUM-AKY-CRS.
       FIN-VAL-NUM-KEY-CRS.
           EXIT.
       
       VAL-NUM-IKY-CRS SECTION.
       INI-VAL-NUM-IKY-CRS.
*% INCLUDE INI_VAL_NUM_IKY_CRS OF 'COLBMCRS.TXT'
       VAL-NUM-FRM-IIC-ICRS.
*% BUF = "CRS-FLD"
*% FLD = "FRM-IIC-ICRS"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-CRS.
*% INCLUDE FIN_VAL_NUM_IKY_CRS OF 'COLBMCRS.TXT'
       FIN-VAL-NUM-IKY-CRS.
           EXIT.
       
       VAL-NUM-MKY-CRS SECTION.
