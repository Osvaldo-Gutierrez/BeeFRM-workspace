*% INCLUDE INI_CLR_DBC_CCA OF 'COLBMCCA.TXT'
*% INCLUDE FIN_CLR_DBC_CCA OF 'COLBMCCA.TXT'
       FIN-CLR-DBC-CCA.
           EXIT.
       
       FST-KEY-CCA SECTION.
       INI-FST-KEY-CCA.
*% INCLUDE INI_FST_KEY_CCA OF 'COLBMCCA.TXT'
           MOVE -1 TO CYA-CAI-IOPC-LEN IN CCA-FLD.
*% INCLUDE FIN_FST_KEY_CCA OF 'COLBMCCA.TXT'
       FIN-FST-KEY-CCA.
           EXIT.
       
       FST-FLD-CCA SECTION.
       INI-FST-FLD-CCA.
*% INCLUDE INI_FST_FLD_CCA OF 'COLBMCCA.TXT'
*% INCLUDE FIN_FST_FLD_CCA OF 'COLBMCCA.TXT'
       FIN-FST-FLD-CCA.
           EXIT.
       
       PRO-KEY-CCA SECTION.
       INI-PRO-KEY-CCA.
*% INCLUDE INI_PRO_KEY_CCA OF 'COLBMCCA.TXT'
           PERFORM PRO-MKY-CCA.
*% INCLUDE FIN_PRO_KEY_CCA OF 'COLBMCCA.TXT'
       FIN-PRO-KEY-CCA.
           EXIT.
       
       PRO-MKY-CCA SECTION.
       INI-PRO-MKY-CCA.
*% INCLUDE INI_PRO_MKY_CCA OF 'COLBMCCA.TXT'
*% INCLUDE FIN_PRO_MKY_CCA OF 'COLBMCCA.TXT'
       FIN-PRO-MKY-CCA.
           EXIT.
       
