       INI-PRO-FLD-CCA.
*% INCLUDE INI_PRO_FLD_CCA OF 'COLBMCCA.TXT'
           PERFORM PRO-OPC-CCA.
           PERFORM PRO-CYA-CCA.
           PERFORM PRO-DBC-CCA.
*% INCLUDE FIN_PRO_FLD_CCA OF 'COLBMCCA.TXT'
       FIN-PRO-FLD-CCA.
           EXIT.
       
       PRO-OPC-CCA SECTION.
       INI-PRO-OPC-CCA.
*% INCLUDE INI_PRO_OPC_CCA OF 'COLBMCCA.TXT'
*% INCLUDE FIN_PRO_OPC_CCA OF 'COLBMCCA.TXT'
       FIN-PRO-OPC-CCA.
           EXIT.
       
       PRO-CYA-CCA SECTION.
       INI-PRO-CYA-CCA.
*% INCLUDE INI_PRO_CYA_CCA OF 'COLBMCCA.TXT'
*% INCLUDE FIN_PRO_CYA_CCA OF 'COLBMCCA.TXT'
       FIN-PRO-CYA-CCA.
           EXIT.
       
       PRO-DBC-CCA SECTION.
       INI-PRO-DBC-CCA.
*% INCLUDE INI_PRO_DBC_CCA OF 'COLBMCCA.TXT'
*% INCLUDE FIN_PRO_DBC_CCA OF 'COLBMCCA.TXT'
       FIN-PRO-DBC-CCA.
           EXIT.
       
       UNP-KEY-CCA SECTION.
       INI-UNP-KEY-CCA.
*% INCLUDE INI_UNP_KEY_CCA OF 'COLBMCCA.TXT'
           PERFORM UNP-IKY-CCA.
           PERFORM UNP-AKY-CCA.
           PERFORM UNP-MKY-CCA.
*% INCLUDE FIN_UNP_KEY_CCA OF 'COLBMCCA.TXT'
       FIN-UNP-KEY-CCA.
           EXIT.
       
       UNP-IKY-CCA SECTION.
       INI-UNP-IKY-CCA.
*% INCLUDE INI_UNP_IKY_CCA OF 'COLBMCCA.TXT'
           MOVE FRM-CPIM-UBY TO CYA-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO CYA-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO CYA-NUM-ICAN-ATR.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO DBC-NUM-ICLI-ATR IN CCA-FLD.
           MOVE FRM-CPIM-UBY TO DBC-VRF-ICLI-ATR IN CCA-FLD.
           MOVE FRM-CPIM-UBY TO DBC-IND-ICLI-ATR IN CCA-FLD.
           MOVE FRM-CPIM-UBY TO DBC-GLS-ICLI-ATR IN CCA-FLD.
      * IDC-FIN --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
*% INCLUDE FIN_UNP_IKY_CCA OF 'COLBMCCA.TXT'
       FIN-UNP-IKY-CCA.
           EXIT.
       
       UNP-MKY-CCA SECTION.
       INI-UNP-MKY-CCA.
*% INCLUDE INI_UNP_MKY_CCA OF 'COLBMCCA.TXT'
*% INCLUDE FIN_UNP_MKY_CCA OF 'COLBMCCA.TXT'
       FIN-UNP-MKY-CCA.
           EXIT.
       
       UNP-AKY-CCA SECTION.
       INI-UNP-AKY-CCA.
*% INCLUDE INI_UNP_AKY_CCA OF 'COLBMCCA.TXT'
           MOVE FRM-CPIM-UBY TO DBC-GLS-NOMC-ATR.
*% INCLUDE FIN_UNP_AKY_CCA OF 'COLBMCCA.TXT'
       FIN-UNP-AKY-CCA.
           EXIT.
       
       UNP-FLD-CCA SECTION.
       INI-UNP-FLD-CCA.
*% INCLUDE INI_UNP_FLD_CCA OF 'COLBMCCA.TXT'
           PERFORM UNP-OPC-CCA.
           PERFORM UNP-CYA-CCA.
           PERFORM UNP-DBC-CCA.
*% INCLUDE FIN_UNP_FLD_CCA OF 'COLBMCCA.TXT'
       FIN-UNP-FLD-CCA.
           EXIT.
       
       UNP-OPC-CCA SECTION.
       INI-UNP-OPC-CCA.
*% INCLUDE INI_UNP_OPC_CCA OF 'COLBMCCA.TXT'
*% INCLUDE FIN_UNP_OPC_CCA OF 'COLBMCCA.TXT'
       FIN-UNP-OPC-CCA.
           EXIT.
       
       UNP-CYA-CCA SECTION.
       INI-UNP-CYA-CCA.
*% INCLUDE INI_UNP_CYA_CCA OF 'COLBMCCA.TXT'
*% INCLUDE FIN_UNP_CYA_CCA OF 'COLBMCCA.TXT'
       FIN-UNP-CYA-CCA.
           EXIT.
       
       UNP-DBC-CCA SECTION.
       INI-UNP-DBC-CCA.
*% INCLUDE INI_UNP_DBC_CCA OF 'COLBMCCA.TXT'
*% INCLUDE FIN_UNP_DBC_CCA OF 'COLBMCCA.TXT'
       FIN-UNP-DBC-CCA.
           EXIT.
       
       VAL-MKY-CCA SECTION.
       INI-VAL-MKY-CCA.
*% INCLUDE INI_VAL_MKY_CCA OF 'COLBMCCA.TXT'
*% INCLUDE FIN_VAL_MKY_CCA OF 'COLBMCCA.TXT'
       FIN-VAL-MKY-CCA.
           EXIT.
       
       VAL-FLD-CCA SECTION.
       INI-VAL-FLD-CCA.
*% INCLUDE INI_VAL_FLD_CCA OF 'COLBMCCA.TXT'
           PERFORM VAL-OPC-MSC-TVOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-MSC-TVOC-LEN IN CCA-FLD
                GO TO FIN-VAL-FLD-CCA.
           PERFORM VAL-OPC-COD-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-TOOC-LEN IN CCA-FLD
                GO TO FIN-VAL-FLD-CCA.
           PERFORM VAL-OPC-COD-OFOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-OFOC-LEN IN CCA-FLD
                GO TO FIN-VAL-FLD-CCA.
           PERFORM VAL-FRM-GLS-TVOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TVOC-LEN IN CCA-FLD
                GO TO FIN-VAL-FLD-CCA.
           PERFORM VAL-FRM-GLS-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TOOC-LEN IN CCA-FLD
                GO TO FIN-VAL-FLD-CCA.
           PERFORM VAL-FRM-GLS-OFOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-OFOC-LEN IN CCA-FLD
                GO TO FIN-VAL-FLD-CCA.
           PERFORM VAL-FRM-GLS-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-VCOC-LEN IN CCA-FLD
                GO TO FIN-VAL-FLD-CCA.
           PERFORM VAL-OPC-COD-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-VCOC-LEN IN CCA-FLD
                GO TO FIN-VAL-FLD-CCA.
           PERFORM VAL-FRM-GLS-TM01.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TM01-LEN IN CCA-FLD
                GO TO FIN-VAL-FLD-CCA.
           PERFORM VAL-FRM-GLS-TM02.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TM02-LEN IN CCA-FLD
                GO TO FIN-VAL-FLD-CCA.
           PERFORM VAL-FRM-GLS-TM03.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TM03-LEN IN CCA-FLD
                GO TO FIN-VAL-FLD-CCA.
           PERFORM VAL-FRM-GLS-TM04.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TM04-LEN IN CCA-FLD
                GO TO FIN-VAL-FLD-CCA.
           PERFORM VAL-FRM-NUM-ICYA
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 10
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-NUM-ICYA-LEN IN CCA-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CCA.
           PERFORM VAL-FRM-GLS-TCYA
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 10
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-GLS-TCYA-LEN IN CCA-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CCA.
           PERFORM VAL-FRM-CAI-ICYA
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 10
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-CAI-ICYA-LEN IN CCA-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CCA.
           PERFORM VAL-FRM-IIC-ICYA
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 10
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-IIC-ICYA-LEN IN CCA-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CCA.
           PERFORM VAL-FRM-SGV-LIQU
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 10
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-LIQU-LEN IN CCA-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CCA.
           PERFORM VAL-FRM-VAL-VPES
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 10
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-VPES-LEN IN CCA-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CCA.
*% INCLUDE FIN_VAL_FLD_CCA OF 'COLBMCCA.TXT'
       FIN-VAL-FLD-CCA.
           EXIT.
       
