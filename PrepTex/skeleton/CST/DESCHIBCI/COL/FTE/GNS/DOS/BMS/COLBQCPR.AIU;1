*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAN"
*% IZR = "S"
       
       VAL-CON-KEY-CPR SECTION.
       INI-VAL-CON-KEY-CPR.
*% INCLUDE INI_VAL_CON_KEY_CPR OF 'COLBMCPR.TXT'
           IF     OPC-CAI-IOPC IN CPR-FLD NOT > SPACES 
              AND OPC-IIC-IOPC IN CPR-FLD NOT > ZEROES 
              AND FRM-COD-TREN IN CPR-FLD NOT > SPACES 
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FINI"
              AND FRM-FEC-FINI IN CPR-FLD NOT > ZEROES
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FTER"
              AND FRM-FEC-FTER IN CPR-FLD NOT > ZEROES
                PERFORM ERR-KEY
                PERFORM FST-KEY-CPR.
*% INCLUDE FIN_VAL_CON_KEY_CPR OF 'COLBMCPR.TXT'
       FIN-VAL-CON-KEY-CPR.
           EXIT.
       
       VAL-CON-IKY-CPR SECTION.
       INI-VAL-CON-IKY-CPR.
*% INCLUDE INI_VAL_CON_IKY_CPR OF 'COLBMCPR.TXT'
