      *
      * COLBQCFN
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:18 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CFN"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMCFN.TXT'
       SET-TAG-COLBQCFN SECTION.
       INI-SET-TAG-COLBQCFN.
           MOVE
           '<IDNMOD NOM=$COLBQCFN$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:18 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCFN.
           EXIT.
           
       PUT-ALL-CFN SECTION.
       INI-PUT-ALL-CFN.
*% INCLUDE INI_PUT_ALL_CFN OF 'COLBMCFN.TXT'
*% INCLUDE FIN_PUT_ALL_CFN OF 'COLBMCFN.TXT'
       FIN-PUT-ALL-CFN.
           EXIT.
       
       KEY-ALL-CFN SECTION.
       INI-KEY-ALL-CFN.
*% INCLUDE INI_KEY_ALL_CFN OF 'COLBMCFN.TXT'
*% INCLUDE FIN_KEY_ALL_CFN OF 'COLBMCFN.TXT'
       FIN-KEY-ALL-CFN.
           EXIT.
       
       SCR-CLRA-CFN SECTION.
       INI-SCR-CLRA-CFN.
*% INCLUDE INI_SCR_CLRA_CFN OF 'COLBMCFN.TXT'
           PERFORM CLR-KEY-CFN.
           PERFORM CLR-FLD-CFN.
*% INCLUDE FIN_SCR_CLRA_CFN OF 'COLBMCFN.TXT'
       FIN-SCR-CLRA-CFN.
           EXIT.
       
       CLR-KEY-CFN SECTION.
       INI-CLR-KEY-CFN.
*% INCLUDE INI_CLR_KEY_CFN OF 'COLBMCFN.TXT'
           MOVE ZEROES TO FRM-NUM-PLZO IN CFN-FLD.
           MOVE SPACES TO FRM-MSC-UTPZ IN CFN-FLD.
           MOVE SPACES TO FRM-COD-VCOC IN CFN-FLD.
           MOVE SPACES TO FRM-COD-TOOC IN CFN-FLD.
           MOVE SPACES TO FRM-COD-AOOC IN CFN-FLD.
           MOVE ZEROES TO FRM-NUM-VCOR IN CFN-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPRO"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FPRO IN CFN-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_KEY_CFN OF 'COLBMCFN.TXT'
       FIN-CLR-KEY-CFN.
           EXIT.
       
       CLR-FLD-CFN SECTION.
       INI-CLR-FLD-CFN.
*% INCLUDE INI_CLR_FLD_CFN OF 'COLBMCFN.TXT'
           MOVE SPACES TO FRM-GLS-UTPZ IN CFN-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN CFN-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN CFN-FLD.
           MOVE SPACES TO FRM-GLS-AOOC IN CFN-FLD.
           MOVE ZEROES TO FRM-SGV-CSTF IN CFN-FLD.
           MOVE SPACES TO FRM-GLS-MONE IN CFN-FLD.
*% INCLUDE FIN_CLR_FLD_CFN OF 'COLBMCFN.TXT'
       FIN-CLR-FLD-CFN.
           EXIT.
       
       FST-KEY-CFN SECTION.
       INI-FST-KEY-CFN.
*% INCLUDE INI_FST_KEY_CFN OF 'COLBMCFN.TXT'
           MOVE -1 TO FRM-NUM-PLZO-LEN IN CFN-FLD.
*% INCLUDE FIN_FST_KEY_CFN OF 'COLBMCFN.TXT'
       FIN-FST-KEY-CFN.
           EXIT.
       
       FST-FLD-CFN SECTION.
       INI-FST-FLD-CFN.
*% INCLUDE INI_FST_FLD_CFN OF 'COLBMCFN.TXT'
*% INCLUDE FIN_FST_FLD_CFN OF 'COLBMCFN.TXT'
       FIN-FST-FLD-CFN.
           EXIT.
       
       PRO-KEY-CFN SECTION.
       INI-PRO-KEY-CFN.
*% INCLUDE INI_PRO_KEY_CFN OF 'COLBMCFN.TXT'
           MOVE FRM-CPIM-AEY TO FRM-NUM-PLZO-ATR IN CFN-FLD.
           MOVE FRM-CPIM-AEY TO FRM-MSC-UTPZ-ATR IN CFN-FLD.
           MOVE FRM-CPIM-AEY TO FRM-COD-VCOC-ATR IN CFN-FLD.
           MOVE FRM-CPIM-AEY TO FRM-COD-TOOC-ATR IN CFN-FLD.
           MOVE FRM-CPIM-AEY TO FRM-COD-AOOC-ATR IN CFN-FLD.
           MOVE FRM-CPIM-AEY TO FRM-NUM-VCOR-ATR IN CFN-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPRO"
           MOVE FRM-CPIM-AEY TO FRM-FEC-FPRO-ATR IN CFN-FLD.
           PERFORM PRO-IKY-CFN.
           PERFORM PRO-AKY-CFN.
           PERFORM PRO-MKY-CFN.
*% INCLUDE FIN_PRO_KEY_CFN OF 'COLBMCFN.TXT'
       FIN-PRO-KEY-CFN.
           EXIT.
       
       PRO-IKY-CFN SECTION.
       INI-PRO-IKY-CFN.
*% INCLUDE INI_PRO_IKY_CFN OF 'COLBMCFN.TXT'
*% INCLUDE FIN_PRO_IKY_CFN OF 'COLBMCFN.TXT'
       FIN-PRO-IKY-CFN.
           EXIT.
       
       PRO-MKY-CFN SECTION.
       INI-PRO-MKY-CFN.
*% INCLUDE INI_PRO_MKY_CFN OF 'COLBMCFN.TXT'
*% INCLUDE FIN_PRO_MKY_CFN OF 'COLBMCFN.TXT'
       FIN-PRO-MKY-CFN.
           EXIT.
       
       PRO-AKY-CFN SECTION.
       INI-PRO-AKY-CFN.
*% INCLUDE INI_PRO_AKY_CFN OF 'COLBMCFN.TXT'
*% INCLUDE FIN_PRO_AKY_CFN OF 'COLBMCFN.TXT'
       FIN-PRO-AKY-CFN.
           EXIT.
       
       UNP-KEY-CFN SECTION.
       INI-UNP-KEY-CFN.
*% INCLUDE INI_UNP_KEY_CFN OF 'COLBMCFN.TXT'
           MOVE FRM-CPIM-UBY TO FRM-NUM-PLZO-ATR IN CFN-FLD.
           MOVE FRM-CPIM-UBY TO FRM-MSC-UTPZ-ATR IN CFN-FLD.
           MOVE FRM-CPIM-UBY TO FRM-COD-VCOC-ATR IN CFN-FLD.
           MOVE FRM-CPIM-UBY TO FRM-COD-TOOC-ATR IN CFN-FLD.
           MOVE FRM-CPIM-UBY TO FRM-COD-AOOC-ATR IN CFN-FLD.
           MOVE FRM-CPIM-UBY TO FRM-NUM-VCOR-ATR IN CFN-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPRO"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FPRO-ATR IN CFN-FLD.
           PERFORM UNP-IKY-CFN.
           PERFORM UNP-AKY-CFN.
           PERFORM UNP-MKY-CFN.
*% INCLUDE FIN_UNP_KEY_CFN OF 'COLBMCFN.TXT'
       FIN-UNP-KEY-CFN.
           EXIT.
       
       UNP-IKY-CFN SECTION.
       INI-UNP-IKY-CFN.
*% INCLUDE INI_UNP_IKY_CFN OF 'COLBMCFN.TXT'
           MOVE FRM-CPIM-UBY TO FRM-NUM-PLZO-ATR.
           MOVE FRM-CPIM-UBY TO FRM-MSC-UTPZ-ATR.
           MOVE FRM-CPIM-UBY TO FRM-COD-VCOC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-COD-TOOC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-COD-AOOC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-NUM-VCOR-ATR.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPRO"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FPRO-ATR IN CFN-FLD.
*% INCLUDE FIN_UNP_IKY_CFN OF 'COLBMCFN.TXT'
       FIN-UNP-IKY-CFN.
           EXIT.
       
       UNP-MKY-CFN SECTION.
       INI-UNP-MKY-CFN.
*% INCLUDE INI_UNP_MKY_CFN OF 'COLBMCFN.TXT'
*% INCLUDE FIN_UNP_MKY_CFN OF 'COLBMCFN.TXT'
       FIN-UNP-MKY-CFN.
           EXIT.
       
       UNP-AKY-CFN SECTION.
       INI-UNP-AKY-CFN.
*% INCLUDE INI_UNP_AKY_CFN OF 'COLBMCFN.TXT'
*% INCLUDE FIN_UNP_AKY_CFN OF 'COLBMCFN.TXT'
       FIN-UNP-AKY-CFN.
           EXIT.
       
       UNP-FLD-CFN SECTION.
       INI-UNP-FLD-CFN.
*% INCLUDE INI_UNP_FLD_CFN OF 'COLBMCFN.TXT'
*% INCLUDE FIN_UNP_FLD_CFN OF 'COLBMCFN.TXT'
       FIN-UNP-FLD-CFN.
           EXIT.
       
       VAL-KEY-CFN SECTION.
       INI-VAL-KEY-CFN.
           PERFORM VAL-IKY-CFN.
           PERFORM VAL-MKY-CFN.
           PERFORM VAL-AKY-CFN.
       FIN-VAL-KEY-CFN.
           EXIT.
       
       VAL-IKY-CFN SECTION.
       INI-VAL-IKY-CFN.
*% INCLUDE INI_VAL_IKY_CFN OF 'COLBMCFN.TXT'
           PERFORM VAL-FRM-NUM-PLZO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-NUM-PLZO-LEN IN CFN-FLD
                GO TO FIN-VAL-IKY-CFN.
           PERFORM VAL-FRM-MSC-UTPZ.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-MSC-UTPZ-LEN IN CFN-FLD
                GO TO FIN-VAL-IKY-CFN.
           PERFORM VAL-FRM-COD-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-VCOC-LEN IN CFN-FLD
                GO TO FIN-VAL-IKY-CFN.
           PERFORM VAL-FRM-COD-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-TOOC-LEN IN CFN-FLD
                GO TO FIN-VAL-IKY-CFN.
           PERFORM VAL-FRM-COD-AOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-AOOC-LEN IN CFN-FLD
                GO TO FIN-VAL-IKY-CFN.
           PERFORM VAL-FRM-NUM-VCOR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-NUM-VCOR-LEN IN CFN-FLD
                GO TO FIN-VAL-IKY-CFN.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPRO"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FPRO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FPRO-LEN IN CFN-FLD
                GO TO FIN-VAL-IKY-CFN.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% INCLUDE FIN_VAL_IKY_CFN OF 'COLBMCFN.TXT'
       FIN-VAL-IKY-CFN.
           EXIT.
       
       VAL-MKY-CFN SECTION.
       INI-VAL-MKY-CFN.
*% INCLUDE INI_VAL_MKY_CFN OF 'COLBMCFN.TXT'
*% INCLUDE FIN_VAL_MKY_CFN OF 'COLBMCFN.TXT'
       FIN-VAL-MKY-CFN.
           EXIT.
       
       VAL-AKY-CFN SECTION.
       INI-VAL-AKY-CFN.
*% INCLUDE INI_VAL_AKY_CFN OF 'COLBMCFN.TXT'
*% INCLUDE FIN_VAL_AKY_CFN OF 'COLBMCFN.TXT'
       FIN-VAL-AKY-CFN.
           EXIT.
       
       VAL-FRM-NUM-PLZO SECTION.
       INI-VAL-FRM-NUM-PLZO.
*% LBL = "FRM_NUM_PLZO OF 'COLBMCFN.TXT'"
*% INCLUDE INI_FRM_NUM_PLZO OF 'COLBMCFN.TXT'
           IF FRM-NUM-PLZO IN CFN-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-FRM-NUM-PLZO.
*% INCLUDE FRM_NUM_PLZO OF 'COLBMCFN.TXT'
*% INCLUDE FIN_FRM_NUM_PLZO OF 'COLBMCFN.TXT'
       FIN-VAL-FRM-NUM-PLZO.
           EXIT.
       
       VAL-FRM-MSC-UTPZ SECTION.
       INI-VAL-FRM-MSC-UTPZ.
*% LBL = "FRM_MSC_UTPZ OF 'COLBMCFN.TXT'"
*% INCLUDE INI_FRM_MSC_UTPZ OF 'COLBMCFN.TXT'
           IF FRM-MSC-UTPZ IN CFN-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-FRM-MSC-UTPZ.
*% INCLUDE FRM_MSC_UTPZ OF 'COLBMCFN.TXT'
*% INCLUDE FIN_FRM_MSC_UTPZ OF 'COLBMCFN.TXT'
       FIN-VAL-FRM-MSC-UTPZ.
           EXIT.
       
       VAL-FRM-COD-VCOC SECTION.
       INI-VAL-FRM-COD-VCOC.
*% LBL = "FRM_COD_VCOC OF 'COLBMCFN.TXT'"
*% INCLUDE INI_FRM_COD_VCOC OF 'COLBMCFN.TXT'
           IF FRM-COD-VCOC IN CFN-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-FRM-COD-VCOC.
*% INCLUDE FRM_COD_VCOC OF 'COLBMCFN.TXT'
*% INCLUDE FIN_FRM_COD_VCOC OF 'COLBMCFN.TXT'
       FIN-VAL-FRM-COD-VCOC.
           EXIT.
       
       VAL-FRM-COD-TOOC SECTION.
       INI-VAL-FRM-COD-TOOC.
*% LBL = "FRM_COD_TOOC OF 'COLBMCFN.TXT'"
*% INCLUDE INI_FRM_COD_TOOC OF 'COLBMCFN.TXT'
           IF FRM-COD-TOOC IN CFN-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-FRM-COD-TOOC.
*% INCLUDE FRM_COD_TOOC OF 'COLBMCFN.TXT'
*% INCLUDE FIN_FRM_COD_TOOC OF 'COLBMCFN.TXT'
       FIN-VAL-FRM-COD-TOOC.
           EXIT.
       
       VAL-FRM-COD-AOOC SECTION.
       INI-VAL-FRM-COD-AOOC.
*% LBL = "FRM_COD_AOOC OF 'COLBMCFN.TXT'"
*% INCLUDE INI_FRM_COD_AOOC OF 'COLBMCFN.TXT'
           IF FRM-COD-AOOC IN CFN-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-FRM-COD-AOOC.
*% INCLUDE FRM_COD_AOOC OF 'COLBMCFN.TXT'
*% INCLUDE FIN_FRM_COD_AOOC OF 'COLBMCFN.TXT'
       FIN-VAL-FRM-COD-AOOC.
           EXIT.
       
       VAL-FRM-NUM-VCOR SECTION.
       INI-VAL-FRM-NUM-VCOR.
*% LBL = "FRM_NUM_VCOR OF 'COLBMCFN.TXT'"
*% INCLUDE INI_FRM_NUM_VCOR OF 'COLBMCFN.TXT'
           IF FRM-NUM-VCOR IN CFN-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-FRM-NUM-VCOR.
*% INCLUDE FRM_NUM_VCOR OF 'COLBMCFN.TXT'
*% INCLUDE FIN_FRM_NUM_VCOR OF 'COLBMCFN.TXT'
       FIN-VAL-FRM-NUM-VCOR.
           EXIT.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPRO"
*% OBL = "N"
*% LBL = "FRM_FEC_FPRO OF 'COLBMCFN.TXT'"
*% IZR = "N"
*% ZRO = "S"
*% BUF = "CFN-FLD"
*% FLD = "FRM-FEC-FPRO"
*% SN  = "N"
*% HOY = "LE"
*% HBL = "S"
*% TIPO = "FEC"
*% FRM_IFLD = ""
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-FRM-FEC-FPRO SECTION.
       INI-VAL-FRM-FEC-FPRO.
           IF FRM-FEC-FPRO IN CFN-FLD  NOT > ZEROES
              MOVE ZEROES TO FRM-FEC-FPRO IN CFN-FLD.
*% INCLUDE 'SETFEC'
*% INCLUDE INI_FRM_FEC_FPRO OF 'COLBMCFN.TXT'
*% INCLUDE 'PROFEC'
*% INCLUDE FRM_FEC_FPRO OF 'COLBMCFN.TXT'
*% INCLUDE FIN_FRM_FEC_FPRO OF 'COLBMCFN.TXT'
       FIN-VAL-FRM-FEC-FPRO.
           EXIT.
      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-CON-KEY-CFN SECTION.
       INI-VAL-CON-KEY-CFN.
*% INCLUDE INI_VAL_CON_KEY_CFN OF 'COLBMCFN.TXT'
           IF     FRM-NUM-PLZO IN CFN-FLD NOT > ZEROES 
              AND FRM-MSC-UTPZ IN CFN-FLD NOT > SPACES 
              AND FRM-COD-VCOC IN CFN-FLD NOT > SPACES 
              AND FRM-COD-TOOC IN CFN-FLD NOT > SPACES 
              AND FRM-COD-AOOC IN CFN-FLD NOT > SPACES 
              AND FRM-NUM-VCOR IN CFN-FLD NOT > ZEROES 
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPRO"
              AND FRM-FEC-FPRO IN CFN-FLD NOT > ZEROES
                PERFORM ERR-KEY
                PERFORM FST-KEY-CFN.
*% INCLUDE FIN_VAL_CON_KEY_CFN OF 'COLBMCFN.TXT'
       FIN-VAL-CON-KEY-CFN.
           EXIT.
       
       VAL-CON-IKY-CFN SECTION.
       INI-VAL-CON-IKY-CFN.
*% INCLUDE INI_VAL_CON_IKY_CFN OF 'COLBMCFN.TXT'
           IF     FRM-NUM-PLZO IN CFN-FLD NOT > ZEROES 
              AND FRM-MSC-UTPZ IN CFN-FLD NOT > SPACES 
              AND FRM-COD-VCOC IN CFN-FLD NOT > SPACES 
              AND FRM-COD-TOOC IN CFN-FLD NOT > SPACES 
              AND FRM-COD-AOOC IN CFN-FLD NOT > SPACES 
              AND FRM-NUM-VCOR IN CFN-FLD NOT > ZEROES 
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPRO"
              AND FRM-FEC-FPRO IN CFN-FLD NOT > ZEROES
                PERFORM ERR-KEY
                PERFORM FST-KEY-CFN.
*% INCLUDE FIN_VAL_CON_IKY_CFN OF 'COLBMCFN.TXT'
       FIN-VAL-CON-IKY-CFN.
           EXIT.
       
       VAL-NUM-KEY-CFN SECTION.
       INI-VAL-NUM-KEY-CFN.
           PERFORM VAL-NUM-IKY-CFN.
           PERFORM VAL-NUM-MKY-CFN.
           PERFORM VAL-NUM-AKY-CFN.
       FIN-VAL-NUM-KEY-CFN.
           EXIT.
       
       VAL-NUM-IKY-CFN SECTION.
       INI-VAL-NUM-IKY-CFN.
*% INCLUDE INI_VAL_NUM_IKY_CFN OF 'COLBMCFN.TXT'
       VAL-NUM-FRM-NUM-PLZO.
*% BUF = "CFN-FLD"
*% FLD = "FRM-NUM-PLZO"
*% ENT = "5"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-CFN.
       VAL-NUM-FRM-NUM-VCOR.
*% BUF = "CFN-FLD"
*% FLD = "FRM-NUM-VCOR"
*% ENT = "3"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-CFN.
*% INCLUDE FIN_VAL_NUM_IKY_CFN OF 'COLBMCFN.TXT'
       FIN-VAL-NUM-IKY-CFN.
           EXIT.
       
       VAL-NUM-MKY-CFN SECTION.
       INI-VAL-NUM-MKY-CFN.
*% INCLUDE INI_VAL_NUM_MKY_CFN OF 'COLBMCFN.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_CFN OF 'COLBMCFN.TXT'
       FIN-VAL-NUM-MKY-CFN.
           EXIT.
       
       VAL-NUM-AKY-CFN SECTION.
       INI-VAL-NUM-AKY-CFN.
*% INCLUDE INI_VAL_NUM_AKY_CFN OF 'COLBMCFN.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_CFN OF 'COLBMCFN.TXT'
       FIN-VAL-NUM-AKY-CFN.
           EXIT.
       
       VAL-NUM-FLD-CFN SECTION.
       INI-VAL-NUM-FLD-CFN.
*% INCLUDE INI_VAL_NUM_FLD_CFN OF 'COLBMCFN.TXT'
       VAL-NUM-FRM-SGV-CSTF.
*% BUF = "CFN-FLD"
*% FLD = "FRM-SGV-CSTF"
*% ENT = "11"
*% DCM = "4"
*% NMR = "NN9V"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-CFN.
*% INCLUDE FIN_VAL_NUM_FLD_CFN OF 'COLBMCFN.TXT'
       FIN-VAL-NUM-FLD-CFN.
           EXIT.
       
       EDT-KEY-CFN SECTION.
       INI-EDT-KEY-CFN.
           PERFORM EDT-IKY-CFN.
           PERFORM EDT-MKY-CFN.
           PERFORM EDT-AKY-CFN.
*% INCLUDE EDT_KEY_CFN OF 'COLBMCFN.TXT'
       FIN-EDT-KEY-CFN.
           EXIT.
       
       EDT-IKY-CFN SECTION.
       INI-EDT-IKY-CFN.
*% INCLUDE EDT_IKY_CFN OF 'COLBMCFN.TXT'
           IF FRM-NUM-PLZO-LEN IN CFN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-PLZO-LEN IN CFN-FLD
                GO TO FIN-EDT-IKY-CFN.
           MOVE FRM-NUM-PLZO IN CFN-FLD TO FRM-NUM-PLZO-EDT IN CFN-EDT.
           MOVE FRM-NUM-PLZO-EDT IN CFN-EDT TO 
                FRM-NUM-PLZO-ALF IN CFN-FLD.
           IF FRM-NUM-VCOR-LEN IN CFN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-VCOR-LEN IN CFN-FLD
                GO TO FIN-EDT-IKY-CFN.
           MOVE FRM-NUM-VCOR IN CFN-FLD TO FRM-NUM-VCOR-EDT IN CFN-EDT.
           MOVE FRM-NUM-VCOR-EDT IN CFN-EDT TO 
                FRM-NUM-VCOR-ALF IN CFN-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPRO"
*% INCLUDE FIN_EDT_IKY_CFN OF 'COLBMCFN.TXT'
       FIN-EDT-IKY-CFN.
           EXIT.
       
       EDT-MKY-CFN SECTION.
       INI-EDT-MKY-CFN.
*% INCLUDE EDT_MKY_CFN OF 'COLBMCFN.TXT'
*% INCLUDE FIN_EDT_MKY_CFN OF 'COLBMCFN.TXT'
       FIN-EDT-MKY-CFN.
           EXIT.
       
       EDT-AKY-CFN SECTION.
       INI-EDT-AKY-CFN.
*% INCLUDE EDT_AKY_CFN OF 'COLBMCFN.TXT'
*% INCLUDE FIN_EDT_AKY_CFN OF 'COLBMCFN.TXT'
       FIN-EDT-AKY-CFN.
           EXIT.
       
       EDT-FLD-CFN SECTION.
       INI-EDT-FLD-CFN.
*% INCLUDE EDT_FLD_CFN OF 'COLBMCFN.TXT'
           IF FRM-SGV-CSTF-LEN IN CFN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-SGV-CSTF-LEN IN CFN-FLD
                GO TO FIN-EDT-FLD-CFN.
           MOVE FRM-SGV-CSTF IN CFN-FLD TO FRM-SGV-CSTF-EDT IN CFN-EDT.
           MOVE FRM-SGV-CSTF-EDT IN CFN-EDT TO 
                FRM-SGV-CSTF-ALF IN CFN-FLD.
*% INCLUDE FIN_EDT_FLD_CFN OF 'COLBMCFN.TXT'
       FIN-EDT-FLD-CFN.
           EXIT.
*% INCLUDE EOF_BQ OF 'COLBMCFN.TXT'
*% INCLUDE EOF OF 'COLBMCFN.TXT'
*% INCLUDE EOF_IBM OF 'COLBMCFN.TXT'
