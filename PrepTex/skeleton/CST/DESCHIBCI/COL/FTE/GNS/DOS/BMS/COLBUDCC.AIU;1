       FIN-UNP-AKY-DCC.
           EXIT.
       
       UNP-FLD-DCC SECTION.
       INI-UNP-FLD-DCC.
*% INCLUDE INI_UNP_FLD_DCC OF 'COLBMDCC.TXT'
           PERFORM UNP-DCC-DCC.
           PERFORM UNP-TAB-DCC.
*% INCLUDE FIN_UNP_FLD_DCC OF 'COLBMDCC.TXT'
       FIN-UNP-FLD-DCC.
           EXIT.
       
       UNP-DCC-DCC SECTION.
       INI-UNP-DCC-DCC.
*% INCLUDE INI_UNP_DCC_DCC OF 'COLBMDCC.TXT'
           MOVE FRM-CPIM-UBY TO DCC-CIC-CCTB-ATR IN DCC-FLD.
           MOVE FRM-CPIM-UBY TO DCC-EXT-CTAB-ATR IN DCC-FLD.
           MOVE FRM-CPIM-UBY TO DCC-GLS-CCSB-ATR IN DCC-FLD.
           MOVE FRM-CPIM-UBY TO DCC-IND-VIGE-ATR IN DCC-FLD.
           MOVE FRM-CPIM-UBY TO DCC-GLS-DISP-ATR IN DCC-FLD.
*% INCLUDE FIN_UNP_DCC_DCC OF 'COLBMDCC.TXT'
       FIN-UNP-DCC-DCC.
           EXIT.
       
       UNP-TAB-DCC SECTION.
       INI-UNP-TAB-DCC.
*% INCLUDE INI_UNP_TAB_DCC OF 'COLBMDCC.TXT'
*% INCLUDE FIN_UNP_TAB_DCC OF 'COLBMDCC.TXT'
       FIN-UNP-TAB-DCC.
           EXIT.
       
       VAL-MKY-DCC SECTION.
       INI-VAL-MKY-DCC.
*% INCLUDE INI_VAL_MKY_DCC OF 'COLBMDCC.TXT'
*% INCLUDE FIN_VAL_MKY_DCC OF 'COLBMDCC.TXT'
       FIN-VAL-MKY-DCC.
           EXIT.
       
       VAL-FLD-DCC SECTION.
       INI-VAL-FLD-DCC.
*% INCLUDE INI_VAL_FLD_DCC OF 'COLBMDCC.TXT'
           PERFORM VAL-FRM-GLS-TCCT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TCCT-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-FRM-GLS-VCAM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-VCAM-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-FRM-GLS-COCR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-COCR-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-FRM-GLS-PLZO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-PLZO-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-FRM-GLS-SCRT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-SCRT-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-FRM-GLS-SCTB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-SCTB-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-FRM-GLS-SCBZ.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-SCBZ-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-DCC-CIC-CCTB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCC-CIC-CCTB-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-DCC-EXT-CTAB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCC-EXT-CTAB-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-DCC-GLS-CCSB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCC-GLS-CCSB-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-TAB-CIC-CTAB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAB-CIC-CTAB-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-TAB-GLS-DESC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAB-GLS-DESC-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-TAB-GLS-DATA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAB-GLS-DATA-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-TAB-GLS-ABRV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAB-GLS-ABRV-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-TAB-GLS-DCOR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAB-GLS-DCOR-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-DCC-IND-VIGE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCC-IND-VIGE-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
*% RREG = "DCC"
*% RREG_FRM = "DCC"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DCC-FEC-FTRN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DCC-FEC-FTRN-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DCC-GLS-DISP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCC-GLS-DISP-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
*% INCLUDE FIN_VAL_FLD_DCC OF 'COLBMDCC.TXT'
       FIN-VAL-FLD-DCC.
           EXIT.
       
       VAL-FRM-GLS-TCCT SECTION.
       INI-VAL-FRM-GLS-TCCT.
*% LBL = "FRM_GLS_TCCT OF 'COLBMDCC.TXT'"
