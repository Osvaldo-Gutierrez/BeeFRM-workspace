*% FRM = "DCC"
      *
      * COLBUDCC
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:50 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMDCC.TXT'
       SET-TAG-COLBUDCC SECTION.
       INI-SET-TAG-COLBUDCC.
           MOVE
           '<IDNMOD NOM=$COLBUDCC$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:50 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUDCC.
           EXIT.
           
       RET-ALL-DCC SECTION.
       INI-RET-ALL-DCC.
*% INCLUDE INI_RET_ALL_DCC OF 'COLBMDCC.TXT'
           PERFORM KEY-ALL-DCC.
           PERFORM RET-DCC-DCC.
           PERFORM RET-TAB-DCC.
*% INCLUDE FIN_RET_ALL_DCC OF 'COLBMDCC.TXT'
       FIN-RET-ALL-DCC.
           EXIT.
       
       RET-DCC-DCC SECTION.
       INI-RET-DCC-DCC.
*% INCLUDE INI_RET_DCC_DCC OF 'COLBMDCC.TXT'
           MOVE DCC-CIC-CCTB IN DCC-FLD TO DCC-CIC-CCTB IN DCC.
           MOVE DCC-EXT-CTAB IN DCC-FLD TO DCC-EXT-CTAB IN DCC.
           MOVE DCC-GLS-CCSB IN DCC-FLD TO DCC-GLS-CCSB IN DCC.
           MOVE DCC-IND-VIGE IN DCC-FLD TO DCC-IND-VIGE IN DCC.
*% RREG = "DCC"
*% RREG_FRM = "DCC"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DCC-NUM-DTRN IN DCC-FLD TO DCC-NUM-DTRN IN DCC.
           MOVE DCC-NUM-MTRN IN DCC-FLD TO DCC-NUM-MTRN IN DCC.
           MOVE DCC-NUM-STRN IN DCC-FLD TO DCC-NUM-STRN IN DCC.
           MOVE DCC-NUM-ATRN IN DCC-FLD TO DCC-NUM-ATRN IN DCC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DCC-GLS-DISP IN DCC-FLD TO DCC-GLS-DISP IN DCC.
*% INCLUDE FIN_RET_DCC_DCC OF 'COLBMDCC.TXT'
       FIN-RET-DCC-DCC.
           EXIT.
       
       RET-TAB-DCC SECTION.
       INI-RET-TAB-DCC.
*% INCLUDE INI_RET_TAB_DCC OF 'COLBMDCC.TXT'
           MOVE TAB-CIC-CTAB IN DCC-FLD TO TAB-CIC-CTAB IN TAB.
           MOVE TAB-GLS-DESC IN DCC-FLD TO TAB-GLS-DESC IN TAB.
           MOVE TAB-GLS-DATA IN DCC-FLD TO TAB-GLS-DATA IN TAB.
           MOVE TAB-GLS-ABRV IN DCC-FLD TO TAB-GLS-ABRV IN TAB.
           MOVE TAB-GLS-DCOR IN DCC-FLD TO TAB-GLS-DCOR IN TAB.
*% INCLUDE FIN_RET_TAB_DCC OF 'COLBMDCC.TXT'
       FIN-RET-TAB-DCC.
           EXIT.
       
       KEY-ALL-DCC SECTION.
       INI-KEY-ALL-DCC.
*% INCLUDE INI_KEY_ALL_DCC OF 'COLBMDCC.TXT'
           PERFORM KEY-DCC-DCC.
           PERFORM KEY-TAB-DCC.
*% INCLUDE FIN_KEY_ALL_DCC OF 'COLBMDCC.TXT'
       FIN-KEY-ALL-DCC.
           EXIT.
       
       KEY-DCC-DCC SECTION.
       INI-KEY-DCC-DCC.
*% INCLUDE INI_KEY_DCC_DCC OF 'COLBMDCC.TXT'
           MOVE DCC-MSC-TCCT IN DCC-FLD TO DCC-MSC-TCCT IN DCC.
           MOVE DCC-COD-VCAM IN DCC-FLD TO DCC-COD-VCAM IN DCC.
           MOVE DCC-COD-COCR IN DCC-FLD TO DCC-COD-COCR IN DCC.
           MOVE DCC-COD-PLZO IN DCC-FLD TO DCC-COD-PLZO IN DCC.
           MOVE DCC-IND-SCRT IN DCC-FLD TO DCC-IND-SCRT IN DCC.
           MOVE DCC-IND-SCTB IN DCC-FLD TO DCC-IND-SCTB IN DCC.
           MOVE DCC-IND-SCBZ IN DCC-FLD TO DCC-IND-SCBZ IN DCC.
*% INCLUDE FIN_KEY_DCC_DCC OF 'COLBMDCC.TXT'
       FIN-KEY-DCC-DCC.
           EXIT.
       
       KEY-TAB-DCC SECTION.
       INI-KEY-TAB-DCC.
*% INCLUDE INI_KEY_TAB_DCC OF 'COLBMDCC.TXT'
*% INCLUDE FIN_KEY_TAB_DCC OF 'COLBMDCC.TXT'
       FIN-KEY-TAB-DCC.
           EXIT.
       
       SCR-CLRA-DCC SECTION.
       INI-SCR-CLRA-DCC.
*% INCLUDE INI_SCR_CLRA_DCC OF 'COLBMDCC.TXT'
           PERFORM CLR-KEY-DCC.
           PERFORM CLR-FLD-DCC.
*% INCLUDE FIN_SCR_CLRA_DCC OF 'COLBMDCC.TXT'
       FIN-SCR-CLRA-DCC.
           EXIT.
       
       CLR-KEY-DCC SECTION.
       INI-CLR-KEY-DCC.
*% INCLUDE INI_CLR_KEY_DCC OF 'COLBMDCC.TXT'
           MOVE SPACES TO DCC-MSC-TCCT IN DCC-FLD.
           MOVE SPACES TO DCC-COD-VCAM IN DCC-FLD.
           MOVE SPACES TO DCC-COD-COCR IN DCC-FLD.
           MOVE SPACES TO DCC-COD-PLZO IN DCC-FLD.
           MOVE SPACES TO DCC-IND-SCRT IN DCC-FLD.
           MOVE SPACES TO DCC-IND-SCTB IN DCC-FLD.
           MOVE SPACES TO DCC-IND-SCBZ IN DCC-FLD.
*% INCLUDE FIN_CLR_KEY_DCC OF 'COLBMDCC.TXT'
       FIN-CLR-KEY-DCC.
           EXIT.
       
       CLR-FLD-DCC SECTION.
       INI-CLR-FLD-DCC.
*% INCLUDE INI_CLR_FLD_DCC OF 'COLBMDCC.TXT'
           MOVE SPACES TO FRM-GLS-TCCT IN DCC-FLD.
           MOVE SPACES TO FRM-GLS-VCAM IN DCC-FLD.
           MOVE SPACES TO FRM-GLS-COCR IN DCC-FLD.
           MOVE SPACES TO FRM-GLS-PLZO IN DCC-FLD.
           MOVE SPACES TO FRM-GLS-SCRT IN DCC-FLD.
           MOVE SPACES TO FRM-GLS-SCTB IN DCC-FLD.
           MOVE SPACES TO FRM-GLS-SCBZ IN DCC-FLD.
           PERFORM CLR-DCC-DCC.
           PERFORM CLR-TAB-DCC.
*% INCLUDE FIN_CLR_FLD_DCC OF 'COLBMDCC.TXT'
       FIN-CLR-FLD-DCC.
           EXIT.
       
       CLR-DCC-DCC SECTION.
       INI-CLR-DCC-DCC.
*% INCLUDE INI_CLR_DCC_DCC OF 'COLBMDCC.TXT'
           MOVE SPACES TO DCC-CIC-CCTB IN DCC-FLD.
           MOVE SPACES TO DCC-EXT-CTAB IN DCC-FLD.
           MOVE SPACES TO DCC-GLS-CCSB IN DCC-FLD.
           MOVE SPACES TO DCC-IND-VIGE IN DCC-FLD.
*% RREG = "DCC"
*% RREG_FRM = "DCC"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DCC-FEC-FTRN IN DCC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO DCC-GLS-DISP IN DCC-FLD.
*% INCLUDE FIN_CLR_DCC_DCC OF 'COLBMDCC.TXT'
       FIN-CLR-DCC-DCC.
           EXIT.
       
       CLR-TAB-DCC SECTION.
       INI-CLR-TAB-DCC.
*% INCLUDE INI_CLR_TAB_DCC OF 'COLBMDCC.TXT'
           MOVE SPACES TO TAB-CIC-CTAB IN DCC-FLD.
           MOVE SPACES TO TAB-GLS-DESC IN DCC-FLD.
           MOVE SPACES TO TAB-GLS-DATA IN DCC-FLD.
           MOVE SPACES TO TAB-GLS-ABRV IN DCC-FLD.
           MOVE SPACES TO TAB-GLS-DCOR IN DCC-FLD.
*% INCLUDE FIN_CLR_TAB_DCC OF 'COLBMDCC.TXT'
       FIN-CLR-TAB-DCC.
           EXIT.
       
       FST-KEY-DCC SECTION.
       INI-FST-KEY-DCC.
*% INCLUDE INI_FST_KEY_DCC OF 'COLBMDCC.TXT'
           MOVE -1 TO DCC-MSC-TCCT-LEN IN DCC-FLD.
*% INCLUDE FIN_FST_KEY_DCC OF 'COLBMDCC.TXT'
       FIN-FST-KEY-DCC.
           EXIT.
       
       FST-FLD-DCC SECTION.
       INI-FST-FLD-DCC.
*% INCLUDE INI_FST_FLD_DCC OF 'COLBMDCC.TXT'
           MOVE -1 TO DCC-CIC-CCTB-LEN IN DCC-FLD.
*% INCLUDE FIN_FST_FLD_DCC OF 'COLBMDCC.TXT'
       FIN-FST-FLD-DCC.
           EXIT.
       
       PRO-KEY-DCC SECTION.
       INI-PRO-KEY-DCC.
*% INCLUDE INI_PRO_KEY_DCC OF 'COLBMDCC.TXT'
           PERFORM PRO-MKY-DCC.
*% INCLUDE FIN_PRO_KEY_DCC OF 'COLBMDCC.TXT'
       FIN-PRO-KEY-DCC.
           EXIT.
       
       PRO-MKY-DCC SECTION.
       INI-PRO-MKY-DCC.
*% INCLUDE INI_PRO_MKY_DCC OF 'COLBMDCC.TXT'
*% INCLUDE FIN_PRO_MKY_DCC OF 'COLBMDCC.TXT'
       FIN-PRO-MKY-DCC.
           EXIT.
       
       PRO-FLD-DCC SECTION.
       INI-PRO-FLD-DCC.
*% INCLUDE INI_PRO_FLD_DCC OF 'COLBMDCC.TXT'
           PERFORM PRO-DCC-DCC.
           PERFORM PRO-TAB-DCC.
*% INCLUDE FIN_PRO_FLD_DCC OF 'COLBMDCC.TXT'
       FIN-PRO-FLD-DCC.
           EXIT.
       
       PRO-DCC-DCC SECTION.
       INI-PRO-DCC-DCC.
*% INCLUDE INI_PRO_DCC_DCC OF 'COLBMDCC.TXT'
           MOVE FRM-CPIM-AEY TO DCC-CIC-CCTB-ATR IN DCC-FLD.
           MOVE FRM-CPIM-AEY TO DCC-EXT-CTAB-ATR IN DCC-FLD.
           MOVE FRM-CPIM-AEY TO DCC-GLS-CCSB-ATR IN DCC-FLD.
           MOVE FRM-CPIM-AEY TO DCC-IND-VIGE-ATR IN DCC-FLD.
           MOVE FRM-CPIM-AEY TO DCC-GLS-DISP-ATR IN DCC-FLD.
*% INCLUDE FIN_PRO_DCC_DCC OF 'COLBMDCC.TXT'
       FIN-PRO-DCC-DCC.
           EXIT.
       
       PRO-TAB-DCC SECTION.
       INI-PRO-TAB-DCC.
*% INCLUDE INI_PRO_TAB_DCC OF 'COLBMDCC.TXT'
*% INCLUDE FIN_PRO_TAB_DCC OF 'COLBMDCC.TXT'
       FIN-PRO-TAB-DCC.
           EXIT.
       
       UNP-KEY-DCC SECTION.
       INI-UNP-KEY-DCC.
*% INCLUDE INI_UNP_KEY_DCC OF 'COLBMDCC.TXT'
           PERFORM UNP-IKY-DCC.
           PERFORM UNP-AKY-DCC.
           PERFORM UNP-MKY-DCC.
*% INCLUDE FIN_UNP_KEY_DCC OF 'COLBMDCC.TXT'
       FIN-UNP-KEY-DCC.
           EXIT.
       
       UNP-IKY-DCC SECTION.
       INI-UNP-IKY-DCC.
*% INCLUDE INI_UNP_IKY_DCC OF 'COLBMDCC.TXT'
           MOVE FRM-CPIM-UBY TO DCC-MSC-TCCT-ATR.
           MOVE FRM-CPIM-UBY TO DCC-COD-VCAM-ATR.
           MOVE FRM-CPIM-UBY TO DCC-COD-COCR-ATR.
           MOVE FRM-CPIM-UBY TO DCC-COD-PLZO-ATR.
           MOVE FRM-CPIM-UBY TO DCC-IND-SCRT-ATR.
           MOVE FRM-CPIM-UBY TO DCC-IND-SCTB-ATR.
           MOVE FRM-CPIM-UBY TO DCC-IND-SCBZ-ATR.
*% INCLUDE FIN_UNP_IKY_DCC OF 'COLBMDCC.TXT'
       FIN-UNP-IKY-DCC.
           EXIT.
       
       UNP-MKY-DCC SECTION.
       INI-UNP-MKY-DCC.
*% INCLUDE INI_UNP_MKY_DCC OF 'COLBMDCC.TXT'
*% INCLUDE FIN_UNP_MKY_DCC OF 'COLBMDCC.TXT'
       FIN-UNP-MKY-DCC.
           EXIT.
       
