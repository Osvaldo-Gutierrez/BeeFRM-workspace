       INI-VAL-FRM-GLS-SCBZ.
*% LBL = "FRM_GLS_SCBZ OF 'COLBMDCC.TXT'"
*% INCLUDE INI_FRM_GLS_SCBZ OF 'COLBMDCC.TXT'
*% INCLUDE FRM_GLS_SCBZ OF 'COLBMDCC.TXT'
*% INCLUDE FIN_FRM_GLS_SCBZ OF 'COLBMDCC.TXT'
       FIN-VAL-FRM-GLS-SCBZ.
           EXIT.
       
       VAL-DCC-CIC-CCTB SECTION.
       INI-VAL-DCC-CIC-CCTB.
*% LBL = "DCC_CIC_CCTB OF 'COLBMDCC.TXT'"
*% INCLUDE INI_DCC_CIC_CCTB OF 'COLBMDCC.TXT'
           IF DCC-CIC-CCTB IN DCC-FLD NOT > SPACES
              MOVE SPACES TO DCC-CIC-CCTB IN DCC-FLD.
*% INCLUDE DCC_CIC_CCTB OF 'COLBMDCC.TXT'
*% INCLUDE FIN_DCC_CIC_CCTB OF 'COLBMDCC.TXT'
       FIN-VAL-DCC-CIC-CCTB.
           EXIT.
       
       VAL-DCC-EXT-CTAB SECTION.
       INI-VAL-DCC-EXT-CTAB.
*% LBL = "DCC_EXT_CTAB OF 'COLBMDCC.TXT'"
*% INCLUDE INI_DCC_EXT_CTAB OF 'COLBMDCC.TXT'
           IF DCC-EXT-CTAB IN DCC-FLD NOT > SPACES
