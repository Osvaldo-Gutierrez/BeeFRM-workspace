      *
      * COLBQBNF
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 03:11 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "BNF"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMBNF.TXT'
       SET-TAG-COLBQBNF SECTION.
       INI-SET-TAG-COLBQBNF.
           MOVE
           '<IDNMOD NOM=$COLBQBNF$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 03:11 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQBNF.
           EXIT.
           
       PUT-ALL-BNF SECTION.
       INI-PUT-ALL-BNF.
*% INCLUDE INI_PUT_ALL_BNF OF 'COLBMBNF.TXT'
           PERFORM PUT-RCO-BNF.
           PERFORM PUT-DBC-BNF.
           PERFORM PUT-OPC-BNF.
           PERFORM PUT-RCD-BNF.
*% INCLUDE FIN_PUT_ALL_BNF OF 'COLBMBNF.TXT'
       FIN-PUT-ALL-BNF.
           EXIT.
       
       PUT-RCO-BNF SECTION.
       INI-PUT-RCO-BNF.
*% INCLUDE INI_PUT_RCO_BNF OF 'COLBMBNF.TXT'
           IF PGM-STAT-RCO-NUL
               GO TO FIN-PUT-RCO-BNF.
           IF NOT PGM-STAT-RCO-OKS
               PERFORM CLR-RCO-BNF
               GO TO FIN-PUT-RCO-BNF.
           MOVE RCO-CAI-IOPE IN RCO TO RCO-CAI-IOPE IN BNF-FLD.
           MOVE RCO-IIC-IOPE IN RCO TO RCO-IIC-IOPE IN BNF-FLD.
           MOVE RCO-COD-TREL IN RCO TO RCO-COD-TREL IN BNF-FLD.
           MOVE RCO-TIP-BENF IN RCO TO RCO-TIP-BENF IN BNF-FLD.
*% INCLUDE FIN_PUT_RCO_BNF OF 'COLBMBNF.TXT'
       FIN-PUT-RCO-BNF.
           EXIT.
       
       PUT-DBC-BNF SECTION.
       INI-PUT-DBC-BNF.
*% INCLUDE INI_PUT_DBC_BNF OF 'COLBMBNF.TXT'
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-BNF.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-BNF
               GO TO FIN-PUT-DBC-BNF.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN BNF-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN BNF-FLD.
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN BNF-FLD.
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN BNF-FLD.
      * IDC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-IND-TICL IN DBC TO DBC-IND-TICL IN BNF-FLD.
           MOVE DBC-GLS-APAT IN DBC TO DBC-GLS-APAT IN BNF-FLD.
           MOVE DBC-GLS-AMAT IN DBC TO DBC-GLS-AMAT IN BNF-FLD.
           MOVE DBC-GLS-NOMB IN DBC TO DBC-GLS-NOMB IN BNF-FLD.
*% INCLUDE FIN_PUT_DBC_BNF OF 'COLBMBNF.TXT'
       FIN-PUT-DBC-BNF.
           EXIT.
       
       PUT-OPC-BNF SECTION.
       INI-PUT-OPC-BNF.
*% INCLUDE INI_PUT_OPC_BNF OF 'COLBMBNF.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-BNF.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-BNF
               GO TO FIN-PUT-OPC-BNF.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE OPC-NUM-DCOL IN OPC TO OPC-NUM-DCOL IN BNF-FLD.
           MOVE OPC-NUM-MCOL IN OPC TO OPC-NUM-MCOL IN BNF-FLD.
           MOVE OPC-NUM-SCOL IN OPC TO OPC-NUM-SCOL IN BNF-FLD.
           MOVE OPC-NUM-ACOL IN OPC TO OPC-NUM-ACOL IN BNF-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% INCLUDE FIN_PUT_OPC_BNF OF 'COLBMBNF.TXT'
       FIN-PUT-OPC-BNF.
           EXIT.
       
       PUT-RCD-BNF SECTION.
       INI-PUT-RCD-BNF.
*% INCLUDE INI_PUT_RCD_BNF OF 'COLBMBNF.TXT'
           IF PGM-STAT-RCD-NUL
               GO TO FIN-PUT-RCD-BNF.
           IF NOT PGM-STAT-RCD-OKS
               PERFORM CLR-RCD-BNF
               GO TO FIN-PUT-RCD-BNF.
           MOVE RCD-GLS-DIRE IN RCD TO RCD-GLS-DIRE IN BNF-FLD.
           MOVE RCD-COD-LUGA IN RCD TO RCD-COD-LUGA IN BNF-FLD.
           MOVE RCD-GLS-FONO IN RCD TO RCD-GLS-FONO IN BNF-FLD.
*% INCLUDE FIN_PUT_RCD_BNF OF 'COLBMBNF.TXT'
       FIN-PUT-RCD-BNF.
           EXIT.
       
       KEY-ALL-BNF SECTION.
       INI-KEY-ALL-BNF.
*% INCLUDE INI_KEY_ALL_BNF OF 'COLBMBNF.TXT'
           PERFORM KEY-RCO-BNF.
           PERFORM KEY-DBC-BNF.
           PERFORM KEY-OPC-BNF.
           PERFORM KEY-RCD-BNF.
*% INCLUDE FIN_KEY_ALL_BNF OF 'COLBMBNF.TXT'
       FIN-KEY-ALL-BNF.
           EXIT.
       
       KEY-RCO-BNF SECTION.
       INI-KEY-RCO-BNF.
*% INCLUDE INI_KEY_RCO_BNF OF 'COLBMBNF.TXT'
           MOVE RCO-CAI-IOPE IN BNF-FLD TO RCO-CAI-IOPE IN RCO.
           MOVE RCO-IIC-IOPE IN BNF-FLD TO RCO-IIC-IOPE IN RCO.
           MOVE RCO-COD-TREL IN BNF-FLD TO RCO-COD-TREL IN RCO.
*% INCLUDE FIN_KEY_RCO_BNF OF 'COLBMBNF.TXT'
       FIN-KEY-RCO-BNF.
           EXIT.
       
       KEY-DBC-BNF SECTION.
       INI-KEY-DBC-BNF.
*% INCLUDE INI_KEY_DBC_BNF OF 'COLBMBNF.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN BNF-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN BNF-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN BNF-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN BNF-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_KEY_DBC_BNF OF 'COLBMBNF.TXT'
       FIN-KEY-DBC-BNF.
           EXIT.
       
       KEY-OPC-BNF SECTION.
       INI-KEY-OPC-BNF.
*% INCLUDE INI_KEY_OPC_BNF OF 'COLBMBNF.TXT'
*% INCLUDE FIN_KEY_OPC_BNF OF 'COLBMBNF.TXT'
       FIN-KEY-OPC-BNF.
           EXIT.
       
       KEY-RCD-BNF SECTION.
       INI-KEY-RCD-BNF.
*% INCLUDE INI_KEY_RCD_BNF OF 'COLBMBNF.TXT'
*% INCLUDE FIN_KEY_RCD_BNF OF 'COLBMBNF.TXT'
       FIN-KEY-RCD-BNF.
           EXIT.
       
       SCR-CLRA-BNF SECTION.
       INI-SCR-CLRA-BNF.
*% INCLUDE INI_SCR_CLRA_BNF OF 'COLBMBNF.TXT'
           PERFORM CLR-KEY-BNF.
           PERFORM CLR-FLD-BNF.
*% INCLUDE FIN_SCR_CLRA_BNF OF 'COLBMBNF.TXT'
       FIN-SCR-CLRA-BNF.
           EXIT.
       
       CLR-KEY-BNF SECTION.
       INI-CLR-KEY-BNF.
*% INCLUDE INI_CLR_KEY_BNF OF 'COLBMBNF.TXT'
           MOVE SPACES TO RCO-CAI-IOPE IN BNF-FLD.
           MOVE ZEROES TO RCO-IIC-IOPE IN BNF-FLD.
           MOVE SPACES TO RCO-COD-TREL IN BNF-FLD.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN BNF-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN BNF-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN BNF-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN BNF-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_KEY_BNF OF 'COLBMBNF.TXT'
       FIN-CLR-KEY-BNF.
           EXIT.
       
       CLR-FLD-BNF SECTION.
       INI-CLR-FLD-BNF.
*% INCLUDE INI_CLR_FLD_BNF OF 'COLBMBNF.TXT'
           MOVE SPACES TO FRM-GLS-TREL IN BNF-FLD.
           MOVE SPACES TO FRM-GLS-BENF IN BNF-FLD.
           MOVE SPACES TO FRM-GLS-LUGA IN BNF-FLD.
           MOVE SPACES TO FRM-GLS-DIRC IN BNF-FLD.
           MOVE SPACES TO FRM-GLS-TDIR IN BNF-FLD.
           MOVE SPACES TO FRM-GLS-NDIR IN BNF-FLD.
           MOVE SPACES TO FRM-GLS-CONT IN BNF-FLD.
           MOVE SPACES TO FRM-GLS-GLS1 IN BNF-FLD.
           MOVE SPACES TO FRM-GLS-GLS2 IN BNF-FLD.
           MOVE ZEROES TO FRM-NUM-DOCU IN BNF-FLD.
           PERFORM CLR-RCO-BNF.
           PERFORM CLR-DBC-BNF.
           PERFORM CLR-OPC-BNF.
           PERFORM CLR-RCD-BNF.
*% INCLUDE FIN_CLR_FLD_BNF OF 'COLBMBNF.TXT'
       FIN-CLR-FLD-BNF.
           EXIT.
       
       CLR-RCO-BNF SECTION.
       INI-CLR-RCO-BNF.
*% INCLUDE INI_CLR_RCO_BNF OF 'COLBMBNF.TXT'
           MOVE SPACES TO RCO-TIP-BENF IN BNF-FLD.
*% INCLUDE FIN_CLR_RCO_BNF OF 'COLBMBNF.TXT'
       FIN-CLR-RCO-BNF.
           EXIT.
       
       CLR-DBC-BNF SECTION.
       INI-CLR-DBC-BNF.
*% INCLUDE INI_CLR_DBC_BNF OF 'COLBMBNF.TXT'
           MOVE SPACES TO DBC-IND-TICL IN BNF-FLD.
           MOVE SPACES TO DBC-GLS-APAT IN BNF-FLD.
           MOVE SPACES TO DBC-GLS-AMAT IN BNF-FLD.
           MOVE SPACES TO DBC-GLS-NOMB IN BNF-FLD.
*% INCLUDE FIN_CLR_DBC_BNF OF 'COLBMBNF.TXT'
       FIN-CLR-DBC-BNF.
           EXIT.
       
       CLR-OPC-BNF SECTION.
       INI-CLR-OPC-BNF.
*% INCLUDE INI_CLR_OPC_BNF OF 'COLBMBNF.TXT'
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO OPC-FEC-FCOL IN BNF-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_OPC_BNF OF 'COLBMBNF.TXT'
       FIN-CLR-OPC-BNF.
           EXIT.
       
       CLR-RCD-BNF SECTION.
       INI-CLR-RCD-BNF.
*% INCLUDE INI_CLR_RCD_BNF OF 'COLBMBNF.TXT'
           MOVE SPACES TO RCD-GLS-DIRE IN BNF-FLD.
           MOVE SPACES TO RCD-COD-LUGA IN BNF-FLD.
           MOVE SPACES TO RCD-GLS-FONO IN BNF-FLD.
*% INCLUDE FIN_CLR_RCD_BNF OF 'COLBMBNF.TXT'
       FIN-CLR-RCD-BNF.
           EXIT.
       
       FST-KEY-BNF SECTION.
       INI-FST-KEY-BNF.
*% INCLUDE INI_FST_KEY_BNF OF 'COLBMBNF.TXT'
           MOVE -1 TO RCO-CAI-IOPE-LEN IN BNF-FLD.
*% INCLUDE FIN_FST_KEY_BNF OF 'COLBMBNF.TXT'
       FIN-FST-KEY-BNF.
           EXIT.
       
       FST-FLD-BNF SECTION.
       INI-FST-FLD-BNF.
*% INCLUDE INI_FST_FLD_BNF OF 'COLBMBNF.TXT'
           MOVE -1 TO DBC-IND-TICL-LEN IN BNF-FLD.
*% INCLUDE FIN_FST_FLD_BNF OF 'COLBMBNF.TXT'
       FIN-FST-FLD-BNF.
           EXIT.
       
       PRO-KEY-BNF SECTION.
       INI-PRO-KEY-BNF.
*% INCLUDE INI_PRO_KEY_BNF OF 'COLBMBNF.TXT'
           PERFORM PRO-IKY-BNF.
           PERFORM PRO-AKY-BNF.
           PERFORM PRO-MKY-BNF.
*% INCLUDE FIN_PRO_KEY_BNF OF 'COLBMBNF.TXT'
       FIN-PRO-KEY-BNF.
           EXIT.
       
       PRO-IKY-BNF SECTION.
       INI-PRO-IKY-BNF.
*% INCLUDE INI_PRO_IKY_BNF OF 'COLBMBNF.TXT'
           MOVE FRM-CPIM-AEY TO RCO-CAI-IOPE-ATR IN BNF-FLD.
           MOVE FRM-CPIM-AEY TO RCO-IIC-IOPE-ATR IN BNF-FLD.
           MOVE FRM-CPIM-AEY TO RCO-COD-TREL-ATR IN BNF-FLD.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO DBC-NUM-ICLI-ATR IN BNF-FLD.
           MOVE FRM-CPIM-AEY TO DBC-VRF-ICLI-ATR IN BNF-FLD.
           MOVE FRM-CPIM-AEY TO DBC-IND-ICLI-ATR IN BNF-FLD.
           MOVE FRM-CPIM-AEY TO DBC-GLS-ICLI-ATR IN BNF-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
*% INCLUDE FIN_PRO_IKY_BNF OF 'COLBMBNF.TXT'
       FIN-PRO-IKY-BNF.
           EXIT.
       
       PRO-MKY-BNF SECTION.
       INI-PRO-MKY-BNF.
*% INCLUDE INI_PRO_MKY_BNF OF 'COLBMBNF.TXT'
*% INCLUDE FIN_PRO_MKY_BNF OF 'COLBMBNF.TXT'
       FIN-PRO-MKY-BNF.
           EXIT.
       
       PRO-AKY-BNF SECTION.
       INI-PRO-AKY-BNF.
*% INCLUDE INI_PRO_AKY_BNF OF 'COLBMBNF.TXT'
*% INCLUDE FIN_PRO_AKY_BNF OF 'COLBMBNF.TXT'
       FIN-PRO-AKY-BNF.
           EXIT.
       
       UNP-KEY-BNF SECTION.
       INI-UNP-KEY-BNF.
*% INCLUDE INI_UNP_KEY_BNF OF 'COLBMBNF.TXT'
           PERFORM UNP-IKY-BNF.
           PERFORM UNP-AKY-BNF.
           PERFORM UNP-MKY-BNF.
*% INCLUDE FIN_UNP_KEY_BNF OF 'COLBMBNF.TXT'
       FIN-UNP-KEY-BNF.
           EXIT.
       
       UNP-IKY-BNF SECTION.
       INI-UNP-IKY-BNF.
*% INCLUDE INI_UNP_IKY_BNF OF 'COLBMBNF.TXT'
           MOVE FRM-CPIM-UBY TO RCO-CAI-IOPE-ATR.
           MOVE FRM-CPIM-UBY TO RCO-IIC-IOPE-ATR.
           MOVE FRM-CPIM-UBY TO RCO-COD-TREL-ATR.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO DBC-NUM-ICLI-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO DBC-VRF-ICLI-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO DBC-IND-ICLI-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO DBC-GLS-ICLI-ATR IN BNF-FLD.
      * IDC-FIN --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
*% INCLUDE FIN_UNP_IKY_BNF OF 'COLBMBNF.TXT'
       FIN-UNP-IKY-BNF.
           EXIT.
       
       UNP-MKY-BNF SECTION.
       INI-UNP-MKY-BNF.
*% INCLUDE INI_UNP_MKY_BNF OF 'COLBMBNF.TXT'
*% INCLUDE FIN_UNP_MKY_BNF OF 'COLBMBNF.TXT'
       FIN-UNP-MKY-BNF.
           EXIT.
       
       UNP-AKY-BNF SECTION.
       INI-UNP-AKY-BNF.
*% INCLUDE INI_UNP_AKY_BNF OF 'COLBMBNF.TXT'
*% INCLUDE FIN_UNP_AKY_BNF OF 'COLBMBNF.TXT'
       FIN-UNP-AKY-BNF.
           EXIT.
       
       UNP-FLD-BNF SECTION.
       INI-UNP-FLD-BNF.
*% INCLUDE INI_UNP_FLD_BNF OF 'COLBMBNF.TXT'
           MOVE FRM-CPIM-UBY TO FRM-GLS-DIRC-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-NDIR-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-CONT-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-GLS1-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-GLS2-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO FRM-NUM-DOCU-ATR IN BNF-FLD.
           PERFORM UNP-RCO-BNF.
           PERFORM UNP-DBC-BNF.
           PERFORM UNP-OPC-BNF.
           PERFORM UNP-RCD-BNF.
*% INCLUDE FIN_UNP_FLD_BNF OF 'COLBMBNF.TXT'
       FIN-UNP-FLD-BNF.
           EXIT.
       
       UNP-RCO-BNF SECTION.
       INI-UNP-RCO-BNF.
*% INCLUDE INI_UNP_RCO_BNF OF 'COLBMBNF.TXT'
           MOVE FRM-CPIM-UBY TO RCO-TIP-BENF-ATR IN BNF-FLD.
*% INCLUDE FIN_UNP_RCO_BNF OF 'COLBMBNF.TXT'
       FIN-UNP-RCO-BNF.
           EXIT.
       
       UNP-DBC-BNF SECTION.
       INI-UNP-DBC-BNF.
*% INCLUDE INI_UNP_DBC_BNF OF 'COLBMBNF.TXT'
           MOVE FRM-CPIM-UBY TO DBC-IND-TICL-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO DBC-GLS-APAT-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO DBC-GLS-AMAT-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO DBC-GLS-NOMB-ATR IN BNF-FLD.
*% INCLUDE FIN_UNP_DBC_BNF OF 'COLBMBNF.TXT'
       FIN-UNP-DBC-BNF.
           EXIT.
       
       UNP-OPC-BNF SECTION.
       INI-UNP-OPC-BNF.
*% INCLUDE INI_UNP_OPC_BNF OF 'COLBMBNF.TXT'
*% INCLUDE FIN_UNP_OPC_BNF OF 'COLBMBNF.TXT'
       FIN-UNP-OPC-BNF.
           EXIT.
       
       UNP-RCD-BNF SECTION.
       INI-UNP-RCD-BNF.
*% INCLUDE INI_UNP_RCD_BNF OF 'COLBMBNF.TXT'
           MOVE FRM-CPIM-UBY TO RCD-GLS-DIRE-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO RCD-COD-LUGA-ATR IN BNF-FLD.
           MOVE FRM-CPIM-UBY TO RCD-GLS-FONO-ATR IN BNF-FLD.
*% INCLUDE FIN_UNP_RCD_BNF OF 'COLBMBNF.TXT'
       FIN-UNP-RCD-BNF.
           EXIT.
       
       VAL-KEY-BNF SECTION.
       INI-VAL-KEY-BNF.
           PERFORM VAL-IKY-BNF.
           PERFORM VAL-MKY-BNF.
           PERFORM VAL-AKY-BNF.
       FIN-VAL-KEY-BNF.
           EXIT.
       
       VAL-IKY-BNF SECTION.
       INI-VAL-IKY-BNF.
*% INCLUDE INI_VAL_IKY_BNF OF 'COLBMBNF.TXT'
           PERFORM VAL-RCO-CAI-IOPE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RCO-CAI-IOPE-LEN IN BNF-FLD
                GO TO FIN-VAL-IKY-BNF.
           PERFORM VAL-RCO-IIC-IOPE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RCO-IIC-IOPE-LEN IN BNF-FLD
                GO TO FIN-VAL-IKY-BNF.
           PERFORM VAL-RCO-COD-TREL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RCO-COD-TREL-LEN IN BNF-FLD
                GO TO FIN-VAL-IKY-BNF.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * VRF-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * VRF-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DBC-IND-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-IND-ICLI-LEN IN BNF-FLD
                GO TO FIN-VAL-IKY-BNF.
           PERFORM VAL-DBC-GLS-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-GLS-ICLI-LEN IN BNF-FLD
                GO TO FIN-VAL-IKY-BNF.
           PERFORM VAL-DBC-NUM-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-NUM-ICLI-LEN IN BNF-FLD
                GO TO FIN-VAL-IKY-BNF.
           PERFORM VAL-DBC-VRF-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-VRF-ICLI-LEN IN BNF-FLD
                GO TO FIN-VAL-IKY-BNF.
      * IDC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% INCLUDE FIN_VAL_IKY_BNF OF 'COLBMBNF.TXT'
       FIN-VAL-IKY-BNF.
           EXIT.
       
       VAL-MKY-BNF SECTION.
