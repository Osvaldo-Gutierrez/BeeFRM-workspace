      *
      * COLBQDLC
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:53 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "DLC"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMDLC.TXT'
       SET-TAG-COLBQDLC SECTION.
       INI-SET-TAG-COLBQDLC.
           MOVE
           '<IDNMOD NOM=$COLBQDLC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:53 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQDLC.
           EXIT.
           
       PUT-ALL-DLC SECTION.
       INI-PUT-ALL-DLC.
*% INCLUDE INI_PUT_ALL_DLC OF 'COLBMDLC.TXT'
           PERFORM PUT-DLC-DLC.
           PERFORM PUT-ICG-DLC.
           PERFORM PUT-RDC-DLC.
           PERFORM PUT-OPC-DLC.
           PERFORM PUT-DBC-DLC.
*% INCLUDE FIN_PUT_ALL_DLC OF 'COLBMDLC.TXT'
       FIN-PUT-ALL-DLC.
           EXIT.
       
       PUT-DLC-DLC SECTION.
       INI-PUT-DLC-DLC.
*% INCLUDE INI_PUT_DLC_DLC OF 'COLBMDLC.TXT'
           IF PGM-STAT-DLC-NUL
               GO TO FIN-PUT-DLC-DLC.
           IF NOT PGM-STAT-DLC-OKS
               PERFORM CLR-DLC-DLC
               GO TO FIN-PUT-DLC-DLC.
           MOVE DLC-CAI-IOCO IN DLC TO DLC-CAI-IOCO IN DLC-FLD.
           MOVE DLC-IIC-IOCO IN DLC TO DLC-IIC-IOCO IN DLC-FLD.
           MOVE DLC-NUM-IDLO IN DLC TO DLC-NUM-IDLO IN DLC-FLD.
           MOVE DLC-CAI-IOPC IN DLC TO DLC-CAI-IOPC IN DLC-FLD.
           MOVE DLC-IIC-IOPC IN DLC TO DLC-IIC-IOPC IN DLC-FLD.
           MOVE DLC-NUM-IDLC IN DLC TO DLC-NUM-IDLC IN DLC-FLD.
           MOVE DLC-VAL-NDLC IN DLC TO DLC-VAL-NDLC IN DLC-FLD.
           MOVE DLC-VAL-SDLC IN DLC TO DLC-VAL-SDLC IN DLC-FLD.
           MOVE DLC-IND-RIES IN DLC TO DLC-IND-RIES IN DLC-FLD.
           MOVE DLC-COD-TDOC IN DLC TO DLC-COD-TDOC IN DLC-FLD.
           MOVE DLC-COD-UBFD IN DLC TO DLC-COD-UBFD IN DLC-FLD.
           MOVE DLC-MSC-TIMP IN DLC TO DLC-MSC-TIMP IN DLC-FLD.
           MOVE DLC-VAL-IMPU IN DLC TO DLC-VAL-IMPU IN DLC-FLD.
           MOVE DLC-COD-PLZA IN DLC TO DLC-COD-PLZA IN DLC-FLD.
           MOVE DLC-COD-NOTC IN DLC TO DLC-COD-NOTC IN DLC-FLD.
           MOVE DLC-VAL-GNOT IN DLC TO DLC-VAL-GNOT IN DLC-FLD.
           MOVE DLC-COD-PLZO IN DLC TO DLC-COD-PLZO IN DLC-FLD.
           MOVE DLC-MSC-MIPL IN DLC TO DLC-MSC-MIPL IN DLC-FLD.
           MOVE DLC-VAL-IPEN IN DLC TO DLC-VAL-IPEN IN DLC-FLD.
           MOVE DLC-IND-SCRT IN DLC TO DLC-IND-SCRT IN DLC-FLD.
*% RREG = "DLC"
*% RREG_FRM = "DLC"
*% NOMB = "FDEV"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DLC-NUM-DDEV IN DLC TO DLC-NUM-DDEV IN DLC-FLD.
           MOVE DLC-NUM-MDEV IN DLC TO DLC-NUM-MDEV IN DLC-FLD.
           MOVE DLC-NUM-SDEV IN DLC TO DLC-NUM-SDEV IN DLC-FLD.
           MOVE DLC-NUM-ADEV IN DLC TO DLC-NUM-ADEV IN DLC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE DLC-IND-SCTB IN DLC TO DLC-IND-SCTB IN DLC-FLD.
           MOVE DLC-VAL-READ IN DLC TO DLC-VAL-READ IN DLC-FLD.
           MOVE DLC-IND-SCBZ IN DLC TO DLC-IND-SCBZ IN DLC-FLD.
           MOVE DLC-VAL-INTD IN DLC TO DLC-VAL-INTD IN DLC-FLD.
           MOVE DLC-COD-PAGO IN DLC TO DLC-COD-PAGO IN DLC-FLD.
           MOVE DLC-IND-ESTA IN DLC TO DLC-IND-ESTA IN DLC-FLD.
           MOVE DLC-IND-ETAP IN DLC TO DLC-IND-ETAP IN DLC-FLD.
           MOVE DLC-IND-HPER IN DLC TO DLC-IND-HPER IN DLC-FLD.
*% BUF = "DLC-FLD"
*% SIS = "TAB"
*% TAB = "DOC "
*% FLD = "DLC-COD-TDOC"
*% GLS = "FRM-GLS-TDOC"
*% TIENE_GLOSA = TRUE
*% LGL = 12
*% INCLUDE 'BUSTAB'
*% BUF = "DLC-FLD"
*% SIS = "COL"
*% TAB = "TIMP"
*% FLD = "DLC-MSC-TIMP"
*% GLS = "FRM-GLS-TIMP"
*% TIENE_GLOSA = TRUE
*% LGL = 12
*% INCLUDE 'BUSMSC'
*% BUF = "DLC-FLD"
*% SIS = "TAB"
*% TAB = "PZA "
*% FLD = "DLC-COD-PLZA"
*% GLS = "FRM-GLS-PLZA"
*% TIENE_GLOSA = TRUE
*% LGL = 20
*% INCLUDE 'BUSTAB'
*% BUF = "DLC-FLD"
*% SIS = "TAB"
*% TAB = "NOT "
*% FLD = "DLC-COD-NOTC"
*% GLS = "FRM-GLS-NOTC"
*% TIENE_GLOSA = TRUE
*% LGL = 18
*% INCLUDE 'BUSTAB'
*% BUF = "DLC-FLD"
*% SIS = "TAB"
*% TAB = "PZO "
*% FLD = "DLC-COD-PLZO"
*% GLS = "FRM-GLS-PLZO"
*% TIENE_GLOSA = TRUE
*% LGL = 28
*% INCLUDE 'BUSTAB'
*% BUF = "DLC-FLD"
*% SIS = "COL"
*% TAB = "MIPL"
*% FLD = "DLC-MSC-MIPL"
*% GLS = "FRM-GLS-MIPL"
*% TIENE_GLOSA = TRUE
*% LGL = 12
*% INCLUDE 'BUSMSC'
*% INCLUDE FIN_PUT_DLC_DLC OF 'COLBMDLC.TXT'
       FIN-PUT-DLC-DLC.
           EXIT.
       
       PUT-ICG-DLC SECTION.
       INI-PUT-ICG-DLC.
*% INCLUDE INI_PUT_ICG_DLC OF 'COLBMDLC.TXT'
           IF PGM-STAT-ICG-NUL
               GO TO FIN-PUT-ICG-DLC.
           IF NOT PGM-STAT-ICG-OKS
               PERFORM CLR-ICG-DLC
               GO TO FIN-PUT-ICG-DLC.
           MOVE ICG-COD-TINT IN ICG TO ICG-COD-TINT IN DLC-FLD.
           MOVE ICG-SGV-TINT IN ICG TO ICG-SGV-TINT IN DLC-FLD.
*% BUF = "DLC-FLD"
*% SIS = "COL"
*% TAB = "INT "
*% FLD = "ICG-COD-TINT"
*% GLS = "FRM-GLS-TINT"
*% TIENE_GLOSA = TRUE
*% LGL = 12
*% INCLUDE 'BUSTAB'
*% INCLUDE FIN_PUT_ICG_DLC OF 'COLBMDLC.TXT'
       FIN-PUT-ICG-DLC.
           EXIT.
       
       PUT-RDC-DLC SECTION.
       INI-PUT-RDC-DLC.
*% INCLUDE INI_PUT_RDC_DLC OF 'COLBMDLC.TXT'
           IF PGM-STAT-RDC-NUL
               GO TO FIN-PUT-RDC-DLC.
           IF NOT PGM-STAT-RDC-OKS
               PERFORM CLR-RDC-DLC
               GO TO FIN-PUT-RDC-DLC.
           MOVE RDC-CAI-ICLI IN RDC TO RDC-CAI-ICLI IN DLC-FLD.
           MOVE RDC-IIC-ICLI IN RDC TO RDC-IIC-ICLI IN DLC-FLD.
           MOVE RDC-NUM-DIRE IN RDC TO RDC-NUM-DIRE IN DLC-FLD.
*% INCLUDE FIN_PUT_RDC_DLC OF 'COLBMDLC.TXT'
       FIN-PUT-RDC-DLC.
           EXIT.
       
       PUT-OPC-DLC SECTION.
       INI-PUT-OPC-DLC.
*% INCLUDE INI_PUT_OPC_DLC OF 'COLBMDLC.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-DLC.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-DLC
               GO TO FIN-PUT-OPC-DLC.
           MOVE OPC-MSC-TVOC IN OPC TO OPC-MSC-TVOC IN DLC-FLD.
           MOVE OPC-COD-TOOC IN OPC TO OPC-COD-TOOC IN DLC-FLD.
           MOVE OPC-COD-OFOC IN OPC TO OPC-COD-OFOC IN DLC-FLD.
*% BUF = "DLC-FLD"
*% SIS = "TAB"
*% TAB = "TVAL"
*% FLD = "OPC-MSC-TVOC"
*% GLS = "FRM-GLS-TVOC"
*% TIENE_GLOSA = TRUE
*% LGL = 13
*% INCLUDE 'BUSMSC'
*% BUF = "DLC-FLD"
*% SIS = "TAB"
*% TAB = "TIO "
*% FLD = "OPC-COD-TOOC"
*% GLS = "FRM-GLS-TOOC"
*% TIENE_GLOSA = TRUE
*% LGL = 14
*% INCLUDE 'BUSTAB'
*% BUF = "DLC-FLD"
*% SIS = "TAB"
*% TAB = "OFI "
*% FLD = "OPC-COD-OFOC"
*% GLS = "FRM-GLS-OFOC"
*% TIENE_GLOSA = TRUE
*% LGL = 14
*% INCLUDE 'BUSOFI          '
*% INCLUDE FIN_PUT_OPC_DLC OF 'COLBMDLC.TXT'
       FIN-PUT-OPC-DLC.
           EXIT.
       
       PUT-DBC-DLC SECTION.
       INI-PUT-DBC-DLC.
*% INCLUDE INI_PUT_DBC_DLC OF 'COLBMDLC.TXT'
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-DLC.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-DLC
               GO TO FIN-PUT-DBC-DLC.
           MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN DLC-FLD.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN DLC-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN DLC-FLD.
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN DLC-FLD.
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN DLC-FLD.
      * IDC-FIN --> BMCHIBCI001210.SRCAIU 
*% INCLUDE FIN_PUT_DBC_DLC OF 'COLBMDLC.TXT'
       FIN-PUT-DBC-DLC.
           EXIT.
       
       KEY-ALL-DLC SECTION.
       INI-KEY-ALL-DLC.
*% INCLUDE INI_KEY_ALL_DLC OF 'COLBMDLC.TXT'
           PERFORM KEY-DLC-DLC.
           PERFORM KEY-ICG-DLC.
           PERFORM KEY-RDC-DLC.
           PERFORM KEY-OPC-DLC.
           PERFORM KEY-DBC-DLC.
*% INCLUDE FIN_KEY_ALL_DLC OF 'COLBMDLC.TXT'
       FIN-KEY-ALL-DLC.
           EXIT.
       
       KEY-DLC-DLC SECTION.
       INI-KEY-DLC-DLC.
*% INCLUDE INI_KEY_DLC_DLC OF 'COLBMDLC.TXT'
           MOVE DLC-CAI-IOPC IN DLC-FLD TO DLC-CAI-IOPC IN DLC.
           MOVE DLC-IIC-IOPC IN DLC-FLD TO DLC-IIC-IOPC IN DLC.
           MOVE DLC-NUM-IDLC IN DLC-FLD TO DLC-NUM-IDLC IN DLC.
*% INCLUDE FIN_KEY_DLC_DLC OF 'COLBMDLC.TXT'
       FIN-KEY-DLC-DLC.
           EXIT.
       
       KEY-ICG-DLC SECTION.
       INI-KEY-ICG-DLC.
*% INCLUDE INI_KEY_ICG_DLC OF 'COLBMDLC.TXT'
*% INCLUDE FIN_KEY_ICG_DLC OF 'COLBMDLC.TXT'
       FIN-KEY-ICG-DLC.
           EXIT.
       
       KEY-RDC-DLC SECTION.
       INI-KEY-RDC-DLC.
*% INCLUDE INI_KEY_RDC_DLC OF 'COLBMDLC.TXT'
           MOVE RDC-CAI-ICLI IN DLC-FLD TO RDC-CAI-ICLI IN RDC.
           MOVE RDC-IIC-ICLI IN DLC-FLD TO RDC-IIC-ICLI IN RDC.
*% INCLUDE FIN_KEY_RDC_DLC OF 'COLBMDLC.TXT'
       FIN-KEY-RDC-DLC.
           EXIT.
       
       KEY-OPC-DLC SECTION.
       INI-KEY-OPC-DLC.
*% INCLUDE INI_KEY_OPC_DLC OF 'COLBMDLC.TXT'
*% INCLUDE FIN_KEY_OPC_DLC OF 'COLBMDLC.TXT'
       FIN-KEY-OPC-DLC.
           EXIT.
       
       KEY-DBC-DLC SECTION.
       INI-KEY-DBC-DLC.
*% INCLUDE INI_KEY_DBC_DLC OF 'COLBMDLC.TXT'
           MOVE DBC-GLS-NOMC IN DLC-FLD TO DBC-GLS-NOMC IN DBC.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN DLC-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN DLC-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN DLC-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN DLC-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_KEY_DBC_DLC OF 'COLBMDLC.TXT'
       FIN-KEY-DBC-DLC.
           EXIT.
       
       SCR-CLRA-DLC SECTION.
       INI-SCR-CLRA-DLC.
*% INCLUDE INI_SCR_CLRA_DLC OF 'COLBMDLC.TXT'
           PERFORM CLR-KEY-DLC.
           PERFORM CLR-FLD-DLC.
*% INCLUDE FIN_SCR_CLRA_DLC OF 'COLBMDLC.TXT'
       FIN-SCR-CLRA-DLC.
           EXIT.
       
       CLR-KEY-DLC SECTION.
       INI-CLR-KEY-DLC.
*% INCLUDE INI_CLR_KEY_DLC OF 'COLBMDLC.TXT'
           MOVE SPACES TO RDC-CAI-ICLI IN DLC-FLD.
           MOVE SPACES TO RDC-IIC-ICLI IN DLC-FLD.
           MOVE SPACES TO DLC-CAI-IOPC IN DLC-FLD.
           MOVE ZEROES TO DLC-IIC-IOPC IN DLC-FLD.
           MOVE ZEROES TO DLC-NUM-IDLC IN DLC-FLD.
           MOVE SPACES TO DBC-GLS-NOMC IN DLC-FLD.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN DLC-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN DLC-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN DLC-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN DLC-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_KEY_DLC OF 'COLBMDLC.TXT'
       FIN-CLR-KEY-DLC.
           EXIT.
       
       CLR-FLD-DLC SECTION.
       INI-CLR-FLD-DLC.
*% INCLUDE INI_CLR_FLD_DLC OF 'COLBMDLC.TXT'
           MOVE SPACES TO FRM-IND-VSTR IN DLC-FLD.
           MOVE SPACES TO FRM-CIC-TDOC IN DLC-FLD.
           MOVE SPACES TO FRM-CIC-OPEC IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-TVOC IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-OFOC IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-TINT IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-TDOC IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-TIMP IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-UBFD IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-NOTC IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-PLZA IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-MIPL IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-PLZO IN DLC-FLD.
           MOVE ZEROES TO FRM-NUM-PIMP IN DLC-FLD.
           MOVE SPACES TO FRM-IND-PIMP IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-PIMP IN DLC-FLD.
           MOVE SPACES TO FRM-GLS-PAGO IN DLC-FLD.
           PERFORM CLR-DLC-DLC.
           PERFORM CLR-ICG-DLC.
           PERFORM CLR-RDC-DLC.
           PERFORM CLR-OPC-DLC.
           PERFORM CLR-DBC-DLC.
*% INCLUDE FIN_CLR_FLD_DLC OF 'COLBMDLC.TXT'
       FIN-CLR-FLD-DLC.
           EXIT.
       
       CLR-DLC-DLC SECTION.
       INI-CLR-DLC-DLC.
*% INCLUDE INI_CLR_DLC_DLC OF 'COLBMDLC.TXT'
           MOVE SPACES TO DLC-CAI-IOCO IN DLC-FLD.
           MOVE SPACES TO DLC-IIC-IOCO IN DLC-FLD.
           MOVE ZEROES TO DLC-NUM-IDLO IN DLC-FLD.
           MOVE ZEROES TO DLC-VAL-NDLC IN DLC-FLD.
           MOVE ZEROES TO DLC-VAL-SDLC IN DLC-FLD.
           MOVE SPACES TO DLC-IND-RIES IN DLC-FLD.
           MOVE SPACES TO DLC-COD-TDOC IN DLC-FLD.
           MOVE SPACES TO DLC-COD-UBFD IN DLC-FLD.
           MOVE SPACES TO DLC-MSC-TIMP IN DLC-FLD.
           MOVE ZEROES TO DLC-VAL-IMPU IN DLC-FLD.
           MOVE SPACES TO DLC-COD-PLZA IN DLC-FLD.
           MOVE SPACES TO DLC-COD-NOTC IN DLC-FLD.
           MOVE ZEROES TO DLC-VAL-GNOT IN DLC-FLD.
           MOVE SPACES TO DLC-COD-PLZO IN DLC-FLD.
           MOVE SPACES TO DLC-MSC-MIPL IN DLC-FLD.
           MOVE ZEROES TO DLC-VAL-IPEN IN DLC-FLD.
           MOVE SPACES TO DLC-IND-SCRT IN DLC-FLD.
*% RREG = "DLC"
*% RREG_FRM = "DLC"
*% NOMB = "FDEV"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DLC-FEC-FDEV IN DLC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO DLC-IND-SCTB IN DLC-FLD.
           MOVE ZEROES TO DLC-VAL-READ IN DLC-FLD.
           MOVE SPACES TO DLC-IND-SCBZ IN DLC-FLD.
           MOVE ZEROES TO DLC-VAL-INTD IN DLC-FLD.
           MOVE SPACES TO DLC-COD-PAGO IN DLC-FLD.
           MOVE SPACES TO DLC-IND-ESTA IN DLC-FLD.
           MOVE SPACES TO DLC-IND-ETAP IN DLC-FLD.
           MOVE SPACES TO DLC-IND-HPER IN DLC-FLD.
*% INCLUDE FIN_CLR_DLC_DLC OF 'COLBMDLC.TXT'
       FIN-CLR-DLC-DLC.
           EXIT.
       
       CLR-ICG-DLC SECTION.
       INI-CLR-ICG-DLC.
*% INCLUDE INI_CLR_ICG_DLC OF 'COLBMDLC.TXT'
           MOVE SPACES TO ICG-COD-TINT IN DLC-FLD.
           MOVE ZEROES TO ICG-SGV-TINT IN DLC-FLD.
*% INCLUDE FIN_CLR_ICG_DLC OF 'COLBMDLC.TXT'
       FIN-CLR-ICG-DLC.
           EXIT.
       
       CLR-RDC-DLC SECTION.
       INI-CLR-RDC-DLC.
*% INCLUDE INI_CLR_RDC_DLC OF 'COLBMDLC.TXT'
           MOVE ZEROES TO RDC-NUM-DIRE IN DLC-FLD.
*% INCLUDE FIN_CLR_RDC_DLC OF 'COLBMDLC.TXT'
       FIN-CLR-RDC-DLC.
           EXIT.
       
       CLR-OPC-DLC SECTION.
       INI-CLR-OPC-DLC.
*% INCLUDE INI_CLR_OPC_DLC OF 'COLBMDLC.TXT'
           MOVE SPACES TO OPC-MSC-TVOC IN DLC-FLD.
           MOVE SPACES TO OPC-COD-TOOC IN DLC-FLD.
           MOVE SPACES TO OPC-COD-OFOC IN DLC-FLD.
*% INCLUDE FIN_CLR_OPC_DLC OF 'COLBMDLC.TXT'
       FIN-CLR-OPC-DLC.
           EXIT.
       
       CLR-DBC-DLC SECTION.
       INI-CLR-DBC-DLC.
*% INCLUDE INI_CLR_DBC_DLC OF 'COLBMDLC.TXT'
*% INCLUDE FIN_CLR_DBC_DLC OF 'COLBMDLC.TXT'
       FIN-CLR-DBC-DLC.
           EXIT.
       
       FST-KEY-DLC SECTION.
       INI-FST-KEY-DLC.
*% INCLUDE INI_FST_KEY_DLC OF 'COLBMDLC.TXT'
           MOVE -1 TO DLC-CAI-IOPC-LEN IN DLC-FLD.
*% INCLUDE FIN_FST_KEY_DLC OF 'COLBMDLC.TXT'
       FIN-FST-KEY-DLC.
           EXIT.
       
       FST-FLD-DLC SECTION.
       INI-FST-FLD-DLC.
*% INCLUDE INI_FST_FLD_DLC OF 'COLBMDLC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
           MOVE FRM-POS-CURS TO DBC-NUM-ICLI-LEN IN DLC-FLD.
      * VRF-FIN --> BMCHIBCI004.SRCAIU ( FST-KEY Y FST-FLD )
*% INCLUDE FIN_FST_FLD_DLC OF 'COLBMDLC.TXT'
       FIN-FST-FLD-DLC.
           EXIT.
       
       PRO-KEY-DLC SECTION.
       INI-PRO-KEY-DLC.
*% INCLUDE INI_PRO_KEY_DLC OF 'COLBMDLC.TXT'
           PERFORM PRO-IKY-DLC.
           PERFORM PRO-AKY-DLC.
           PERFORM PRO-MKY-DLC.
*% INCLUDE FIN_PRO_KEY_DLC OF 'COLBMDLC.TXT'
       FIN-PRO-KEY-DLC.
           EXIT.
       
       PRO-IKY-DLC SECTION.
       INI-PRO-IKY-DLC.
*% INCLUDE INI_PRO_IKY_DLC OF 'COLBMDLC.TXT'
           MOVE FRM-CPIM-AEY TO DLC-CAI-IOPC-ATR IN DLC-FLD.
           MOVE FRM-CPIM-AEY TO DLC-IIC-IOPC-ATR IN DLC-FLD.
*% INCLUDE FIN_PRO_IKY_DLC OF 'COLBMDLC.TXT'
       FIN-PRO-IKY-DLC.
           EXIT.
       
       PRO-MKY-DLC SECTION.
       INI-PRO-MKY-DLC.
*% INCLUDE INI_PRO_MKY_DLC OF 'COLBMDLC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO DBC-NUM-ICLI-ATR IN DLC-FLD.
           MOVE FRM-CPIM-AEY TO DBC-VRF-ICLI-ATR IN DLC-FLD.
           MOVE FRM-CPIM-AEY TO DBC-IND-ICLI-ATR IN DLC-FLD.
           MOVE FRM-CPIM-AEY TO DBC-GLS-ICLI-ATR IN DLC-FLD.
