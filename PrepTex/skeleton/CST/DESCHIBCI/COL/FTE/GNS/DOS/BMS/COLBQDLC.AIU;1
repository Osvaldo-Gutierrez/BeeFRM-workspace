       EDT-KEY-DLC SECTION.
       INI-EDT-KEY-DLC.
           PERFORM EDT-IKY-DLC.
           PERFORM EDT-MKY-DLC.
           PERFORM EDT-AKY-DLC.
*% INCLUDE EDT_KEY_DLC OF 'COLBMDLC.TXT'
       FIN-EDT-KEY-DLC.
           EXIT.
       
       EDT-IKY-DLC SECTION.
       INI-EDT-IKY-DLC.
*% INCLUDE EDT_IKY_DLC OF 'COLBMDLC.TXT'
           IF DLC-IIC-IOPC-LEN IN DLC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DLC-IIC-IOPC-LEN IN DLC-FLD
                GO TO FIN-EDT-IKY-DLC.
           MOVE DLC-IIC-IOPC IN DLC-FLD TO DLC-IIC-IOPC-EDT IN DLC-EDT.
           MOVE DLC-IIC-IOPC-EDT IN DLC-EDT TO 
                DLC-IIC-IOPC-ALF IN DLC-FLD.
*% INCLUDE FIN_EDT_IKY_DLC OF 'COLBMDLC.TXT'
       FIN-EDT-IKY-DLC.
           EXIT.
       
       EDT-MKY-DLC SECTION.
       INI-EDT-MKY-DLC.
*% INCLUDE EDT_MKY_DLC OF 'COLBMDLC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF DBC-NUM-ICLI-LEN IN DLC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DBC-NUM-ICLI-LEN IN DLC-FLD
                GO TO FIN-EDT-MKY-DLC.
           MOVE DBC-NUM-ICLI IN DLC-FLD TO
                DBC-NUM-ICLI-EDT IN DLC-EDT.
           MOVE DBC-NUM-ICLI-EDT IN DLC-EDT TO
                DBC-NUM-ICLI-ALF IN DLC-FLD.
      * IDC-FIN --> BMCHIBCI011.SRCAIU EDICION NUMERICA
*% INCLUDE FIN_EDT_MKY_DLC OF 'COLBMDLC.TXT'
       FIN-EDT-MKY-DLC.
           EXIT.
       
       EDT-AKY-DLC SECTION.
       INI-EDT-AKY-DLC.
*% INCLUDE EDT_AKY_DLC OF 'COLBMDLC.TXT'
           IF DLC-NUM-IDLC-LEN IN DLC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DLC-NUM-IDLC-LEN IN DLC-FLD
                GO TO FIN-EDT-AKY-DLC.
           MOVE DLC-NUM-IDLC IN DLC-FLD TO DLC-NUM-IDLC-EDT IN DLC-EDT.
           MOVE DLC-NUM-IDLC-EDT IN DLC-EDT TO 
                DLC-NUM-IDLC-ALF IN DLC-FLD.
*% INCLUDE FIN_EDT_AKY_DLC OF 'COLBMDLC.TXT'
       FIN-EDT-AKY-DLC.
           EXIT.
       
       EDT-FLD-DLC SECTION.
       INI-EDT-FLD-DLC.
*% INCLUDE EDT_FLD_DLC OF 'COLBMDLC.TXT'
           IF DLC-NUM-IDLO-LEN IN DLC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DLC-NUM-IDLO-LEN IN DLC-FLD
                GO TO FIN-EDT-FLD-DLC.
           MOVE DLC-NUM-IDLO IN DLC-FLD TO DLC-NUM-IDLO-EDT IN DLC-EDT.
           MOVE DLC-NUM-IDLO-EDT IN DLC-EDT TO 
                DLC-NUM-IDLO-ALF IN DLC-FLD.
           IF RDC-NUM-DIRE-LEN IN DLC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO RDC-NUM-DIRE-LEN IN DLC-FLD
                GO TO FIN-EDT-FLD-DLC.
           MOVE RDC-NUM-DIRE IN DLC-FLD TO RDC-NUM-DIRE-EDT IN DLC-EDT.
           MOVE RDC-NUM-DIRE-EDT IN DLC-EDT TO 
                RDC-NUM-DIRE-ALF IN DLC-FLD.
           IF ICG-SGV-TINT-LEN IN DLC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO ICG-SGV-TINT-LEN IN DLC-FLD
                GO TO FIN-EDT-FLD-DLC.
           MOVE ICG-SGV-TINT IN DLC-FLD TO ICG-SGV-TINT-EDT IN DLC-EDT.
           MOVE ICG-SGV-TINT-EDT IN DLC-EDT TO 
                ICG-SGV-TINT-ALF IN DLC-FLD.
           IF DLC-VAL-NDLC-LEN IN DLC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DLC-VAL-NDLC-LEN IN DLC-FLD
                GO TO FIN-EDT-FLD-DLC.
           MOVE DLC-VAL-NDLC IN DLC-FLD TO DLC-VAL-NDLC-EDT IN DLC-EDT.
           MOVE DLC-VAL-NDLC-EDT IN DLC-EDT TO 
                DLC-VAL-NDLC-ALF IN DLC-FLD.
           IF DLC-VAL-SDLC-LEN IN DLC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DLC-VAL-SDLC-LEN IN DLC-FLD
                GO TO FIN-EDT-FLD-DLC.
           MOVE DLC-VAL-SDLC IN DLC-FLD TO DLC-VAL-SDLC-EDT IN DLC-EDT.
           MOVE DLC-VAL-SDLC-EDT IN DLC-EDT TO 
                DLC-VAL-SDLC-ALF IN DLC-FLD.
           IF DLC-VAL-IMPU-LEN IN DLC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DLC-VAL-IMPU-LEN IN DLC-FLD
                GO TO FIN-EDT-FLD-DLC.
           MOVE DLC-VAL-IMPU IN DLC-FLD TO DLC-VAL-IMPU-EDT IN DLC-EDT.
           MOVE DLC-VAL-IMPU-EDT IN DLC-EDT TO 
                DLC-VAL-IMPU-ALF IN DLC-FLD.
           IF DLC-VAL-GNOT-LEN IN DLC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DLC-VAL-GNOT-LEN IN DLC-FLD
                GO TO FIN-EDT-FLD-DLC.
           MOVE DLC-VAL-GNOT IN DLC-FLD TO DLC-VAL-GNOT-EDT IN DLC-EDT.
           MOVE DLC-VAL-GNOT-EDT IN DLC-EDT TO 
                DLC-VAL-GNOT-ALF IN DLC-FLD.
           IF DLC-VAL-IPEN-LEN IN DLC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DLC-VAL-IPEN-LEN IN DLC-FLD
                GO TO FIN-EDT-FLD-DLC.
           MOVE DLC-VAL-IPEN IN DLC-FLD TO DLC-VAL-IPEN-EDT IN DLC-EDT.
           MOVE DLC-VAL-IPEN-EDT IN DLC-EDT TO 
                DLC-VAL-IPEN-ALF IN DLC-FLD.
*% RREG = "DLC"
*% RREG_FRM = "DLC"
*% NOMB = "FDEV"
           IF DLC-VAL-READ-LEN IN DLC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DLC-VAL-READ-LEN IN DLC-FLD
                GO TO FIN-EDT-FLD-DLC.
           MOVE DLC-VAL-READ IN DLC-FLD TO DLC-VAL-READ-EDT IN DLC-EDT.
           MOVE DLC-VAL-READ-EDT IN DLC-EDT TO 
                DLC-VAL-READ-ALF IN DLC-FLD.
           IF DLC-VAL-INTD-LEN IN DLC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DLC-VAL-INTD-LEN IN DLC-FLD
                GO TO FIN-EDT-FLD-DLC.
           MOVE DLC-VAL-INTD IN DLC-FLD TO DLC-VAL-INTD-EDT IN DLC-EDT.
           MOVE DLC-VAL-INTD-EDT IN DLC-EDT TO 
                DLC-VAL-INTD-ALF IN DLC-FLD.
           IF FRM-NUM-PIMP-LEN IN DLC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-PIMP-LEN IN DLC-FLD
                GO TO FIN-EDT-FLD-DLC.
           MOVE FRM-NUM-PIMP IN DLC-FLD TO FRM-NUM-PIMP-EDT IN DLC-EDT.
           MOVE FRM-NUM-PIMP-EDT IN DLC-EDT TO 
                FRM-NUM-PIMP-ALF IN DLC-FLD.
*% INCLUDE FIN_EDT_FLD_DLC OF 'COLBMDLC.TXT'
       FIN-EDT-FLD-DLC.
           EXIT.
