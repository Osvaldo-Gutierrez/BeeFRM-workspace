*% FRM = "AUX"
      *
      * COLBUAUX
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 03:09 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMAUX.TXT'
       SET-TAG-COLBUAUX SECTION.
       INI-SET-TAG-COLBUAUX.
           MOVE
           '<IDNMOD NOM=$COLBUAUX$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 03:09 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUAUX.
           EXIT.
           
       RET-ALL-AUX SECTION.
       INI-RET-ALL-AUX.
*% INCLUDE INI_RET_ALL_AUX OF 'COLBMAUX.TXT'
           PERFORM KEY-ALL-AUX.
           PERFORM RET-AUX-AUX.
*% INCLUDE FIN_RET_ALL_AUX OF 'COLBMAUX.TXT'
       FIN-RET-ALL-AUX.
           EXIT.
       
       RET-AUX-AUX SECTION.
       INI-RET-AUX-AUX.
*% INCLUDE INI_RET_AUX_AUX OF 'COLBMAUX.TXT'
           MOVE AUX-CIC-COCR IN AUX-FLD TO AUX-CIC-COCR IN AUX.
           MOVE AUX-EXT-CTAB IN AUX-FLD TO AUX-EXT-CTAB IN AUX.
           MOVE AUX-GLS-DESC IN AUX-FLD TO AUX-GLS-DESC IN AUX.
           MOVE AUX-GLS-ABRV IN AUX-FLD TO AUX-GLS-ABRV IN AUX.
           MOVE AUX-GLS-DCOR IN AUX-FLD TO AUX-GLS-DCOR IN AUX.
           MOVE AUX-IND-VIGE IN AUX-FLD TO AUX-IND-VIGE IN AUX.
*% RREG = "AUX"
*% RREG_FRM = "AUX"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE AUX-NUM-DTRN IN AUX-FLD TO AUX-NUM-DTRN IN AUX.
           MOVE AUX-NUM-MTRN IN AUX-FLD TO AUX-NUM-MTRN IN AUX.
           MOVE AUX-NUM-STRN IN AUX-FLD TO AUX-NUM-STRN IN AUX.
           MOVE AUX-NUM-ATRN IN AUX-FLD TO AUX-NUM-ATRN IN AUX.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE AUX-GLS-CCTE IN AUX-FLD TO AUX-GLS-CCTE IN AUX.
           MOVE AUX-IND-PCTB IN AUX-FLD TO AUX-IND-PCTB IN AUX.
           MOVE AUX-IND-PCDR IN AUX-FLD TO AUX-IND-PCDR IN AUX.
*% INCLUDE FIN_RET_AUX_AUX OF 'COLBMAUX.TXT'
       FIN-RET-AUX-AUX.
           EXIT.
       
       KEY-ALL-AUX SECTION.
       INI-KEY-ALL-AUX.
*% INCLUDE INI_KEY_ALL_AUX OF 'COLBMAUX.TXT'
           PERFORM KEY-AUX-AUX.
*% INCLUDE FIN_KEY_ALL_AUX OF 'COLBMAUX.TXT'
       FIN-KEY-ALL-AUX.
           EXIT.
       
       KEY-AUX-AUX SECTION.
       INI-KEY-AUX-AUX.
*% INCLUDE INI_KEY_AUX_AUX OF 'COLBMAUX.TXT'
           MOVE AUX-COD-TOCR IN AUX-FLD TO AUX-COD-TOCR IN AUX.
           MOVE AUX-COD-AOCR IN AUX-FLD TO AUX-COD-AOCR IN AUX.
*% INCLUDE FIN_KEY_AUX_AUX OF 'COLBMAUX.TXT'
       FIN-KEY-AUX-AUX.
           EXIT.
       
       SCR-CLRA-AUX SECTION.
       INI-SCR-CLRA-AUX.
*% INCLUDE INI_SCR_CLRA_AUX OF 'COLBMAUX.TXT'
           PERFORM CLR-KEY-AUX.
           PERFORM CLR-FLD-AUX.
*% INCLUDE FIN_SCR_CLRA_AUX OF 'COLBMAUX.TXT'
       FIN-SCR-CLRA-AUX.
           EXIT.
       
       CLR-KEY-AUX SECTION.
       INI-CLR-KEY-AUX.
*% INCLUDE INI_CLR_KEY_AUX OF 'COLBMAUX.TXT'
           MOVE SPACES TO AUX-COD-TOCR IN AUX-FLD.
           MOVE SPACES TO AUX-COD-AOCR IN AUX-FLD.
*% INCLUDE FIN_CLR_KEY_AUX OF 'COLBMAUX.TXT'
       FIN-CLR-KEY-AUX.
           EXIT.
       
       CLR-FLD-AUX SECTION.
       INI-CLR-FLD-AUX.
*% INCLUDE INI_CLR_FLD_AUX OF 'COLBMAUX.TXT'
           MOVE SPACES TO FRM-GLS-TOCR IN AUX-FLD.
           PERFORM CLR-AUX-AUX.
*% INCLUDE FIN_CLR_FLD_AUX OF 'COLBMAUX.TXT'
       FIN-CLR-FLD-AUX.
           EXIT.
       
       CLR-AUX-AUX SECTION.
       INI-CLR-AUX-AUX.
*% INCLUDE INI_CLR_AUX_AUX OF 'COLBMAUX.TXT'
           MOVE SPACES TO AUX-CIC-COCR IN AUX-FLD.
           MOVE SPACES TO AUX-EXT-CTAB IN AUX-FLD.
           MOVE SPACES TO AUX-GLS-DESC IN AUX-FLD.
           MOVE SPACES TO AUX-GLS-ABRV IN AUX-FLD.
           MOVE SPACES TO AUX-GLS-DCOR IN AUX-FLD.
           MOVE SPACES TO AUX-IND-VIGE IN AUX-FLD.
*% RREG = "AUX"
*% RREG_FRM = "AUX"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO AUX-FEC-FTRN IN AUX-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO AUX-GLS-CCTE IN AUX-FLD.
           MOVE SPACES TO AUX-IND-PCTB IN AUX-FLD.
           MOVE SPACES TO AUX-IND-PCDR IN AUX-FLD.
*% INCLUDE FIN_CLR_AUX_AUX OF 'COLBMAUX.TXT'
       FIN-CLR-AUX-AUX.
           EXIT.
       
       FST-KEY-AUX SECTION.
       INI-FST-KEY-AUX.
*% INCLUDE INI_FST_KEY_AUX OF 'COLBMAUX.TXT'
           MOVE -1 TO AUX-COD-TOCR-LEN IN AUX-FLD.
*% INCLUDE FIN_FST_KEY_AUX OF 'COLBMAUX.TXT'
       FIN-FST-KEY-AUX.
           EXIT.
       
       FST-FLD-AUX SECTION.
       INI-FST-FLD-AUX.
*% INCLUDE INI_FST_FLD_AUX OF 'COLBMAUX.TXT'
           MOVE -1 TO AUX-CIC-COCR-LEN IN AUX-FLD.
*% INCLUDE FIN_FST_FLD_AUX OF 'COLBMAUX.TXT'
       FIN-FST-FLD-AUX.
           EXIT.
       
       PRO-KEY-AUX SECTION.
       INI-PRO-KEY-AUX.
*% INCLUDE INI_PRO_KEY_AUX OF 'COLBMAUX.TXT'
           PERFORM PRO-MKY-AUX.
*% INCLUDE FIN_PRO_KEY_AUX OF 'COLBMAUX.TXT'
       FIN-PRO-KEY-AUX.
           EXIT.
       
       PRO-MKY-AUX SECTION.
       INI-PRO-MKY-AUX.
*% INCLUDE INI_PRO_MKY_AUX OF 'COLBMAUX.TXT'
*% INCLUDE FIN_PRO_MKY_AUX OF 'COLBMAUX.TXT'
       FIN-PRO-MKY-AUX.
           EXIT.
       
       PRO-FLD-AUX SECTION.
       INI-PRO-FLD-AUX.
*% INCLUDE INI_PRO_FLD_AUX OF 'COLBMAUX.TXT'
           PERFORM PRO-AUX-AUX.
*% INCLUDE FIN_PRO_FLD_AUX OF 'COLBMAUX.TXT'
       FIN-PRO-FLD-AUX.
           EXIT.
       
       PRO-AUX-AUX SECTION.
       INI-PRO-AUX-AUX.
*% INCLUDE INI_PRO_AUX_AUX OF 'COLBMAUX.TXT'
           MOVE FRM-CPIM-AEY TO AUX-CIC-COCR-ATR IN AUX-FLD.
           MOVE FRM-CPIM-AEY TO AUX-EXT-CTAB-ATR IN AUX-FLD.
           MOVE FRM-CPIM-AEY TO AUX-GLS-DESC-ATR IN AUX-FLD.
           MOVE FRM-CPIM-AEY TO AUX-GLS-ABRV-ATR IN AUX-FLD.
           MOVE FRM-CPIM-AEY TO AUX-GLS-DCOR-ATR IN AUX-FLD.
           MOVE FRM-CPIM-AEY TO AUX-IND-VIGE-ATR IN AUX-FLD.
           MOVE FRM-CPIM-AEY TO AUX-GLS-CCTE-ATR IN AUX-FLD.
           MOVE FRM-CPIM-AEY TO AUX-IND-PCTB-ATR IN AUX-FLD.
           MOVE FRM-CPIM-AEY TO AUX-IND-PCDR-ATR IN AUX-FLD.
*% INCLUDE FIN_PRO_AUX_AUX OF 'COLBMAUX.TXT'
       FIN-PRO-AUX-AUX.
           EXIT.
       
       UNP-KEY-AUX SECTION.
       INI-UNP-KEY-AUX.
*% INCLUDE INI_UNP_KEY_AUX OF 'COLBMAUX.TXT'
           PERFORM UNP-IKY-AUX.
           PERFORM UNP-AKY-AUX.
           PERFORM UNP-MKY-AUX.
*% INCLUDE FIN_UNP_KEY_AUX OF 'COLBMAUX.TXT'
       FIN-UNP-KEY-AUX.
           EXIT.
       
       UNP-IKY-AUX SECTION.
       INI-UNP-IKY-AUX.
*% INCLUDE INI_UNP_IKY_AUX OF 'COLBMAUX.TXT'
           MOVE FRM-CPIM-UBY TO AUX-COD-TOCR-ATR.
           MOVE FRM-CPIM-UBY TO AUX-COD-AOCR-ATR.
*% INCLUDE FIN_UNP_IKY_AUX OF 'COLBMAUX.TXT'
       FIN-UNP-IKY-AUX.
           EXIT.
       
       UNP-MKY-AUX SECTION.
       INI-UNP-MKY-AUX.
*% INCLUDE INI_UNP_MKY_AUX OF 'COLBMAUX.TXT'
*% INCLUDE FIN_UNP_MKY_AUX OF 'COLBMAUX.TXT'
       FIN-UNP-MKY-AUX.
           EXIT.
       
       UNP-AKY-AUX SECTION.
       INI-UNP-AKY-AUX.
*% INCLUDE INI_UNP_AKY_AUX OF 'COLBMAUX.TXT'
*% INCLUDE FIN_UNP_AKY_AUX OF 'COLBMAUX.TXT'
       FIN-UNP-AKY-AUX.
           EXIT.
       
       UNP-FLD-AUX SECTION.
       INI-UNP-FLD-AUX.
*% INCLUDE INI_UNP_FLD_AUX OF 'COLBMAUX.TXT'
           PERFORM UNP-AUX-AUX.
*% INCLUDE FIN_UNP_FLD_AUX OF 'COLBMAUX.TXT'
       FIN-UNP-FLD-AUX.
           EXIT.
       
       UNP-AUX-AUX SECTION.
       INI-UNP-AUX-AUX.
*% INCLUDE INI_UNP_AUX_AUX OF 'COLBMAUX.TXT'
           MOVE FRM-CPIM-UBY TO AUX-CIC-COCR-ATR IN AUX-FLD.
           MOVE FRM-CPIM-UBY TO AUX-EXT-CTAB-ATR IN AUX-FLD.
           MOVE FRM-CPIM-UBY TO AUX-GLS-DESC-ATR IN AUX-FLD.
           MOVE FRM-CPIM-UBY TO AUX-GLS-ABRV-ATR IN AUX-FLD.
           MOVE FRM-CPIM-UBY TO AUX-GLS-DCOR-ATR IN AUX-FLD.
           MOVE FRM-CPIM-UBY TO AUX-IND-VIGE-ATR IN AUX-FLD.
           MOVE FRM-CPIM-UBY TO AUX-GLS-CCTE-ATR IN AUX-FLD.
           MOVE FRM-CPIM-UBY TO AUX-IND-PCTB-ATR IN AUX-FLD.
           MOVE FRM-CPIM-UBY TO AUX-IND-PCDR-ATR IN AUX-FLD.
*% INCLUDE FIN_UNP_AUX_AUX OF 'COLBMAUX.TXT'
       FIN-UNP-AUX-AUX.
           EXIT.
       
       VAL-MKY-AUX SECTION.
       INI-VAL-MKY-AUX.
*% INCLUDE INI_VAL_MKY_AUX OF 'COLBMAUX.TXT'
*% INCLUDE FIN_VAL_MKY_AUX OF 'COLBMAUX.TXT'
       FIN-VAL-MKY-AUX.
           EXIT.
       
       VAL-FLD-AUX SECTION.
       INI-VAL-FLD-AUX.
*% INCLUDE INI_VAL_FLD_AUX OF 'COLBMAUX.TXT'
           PERFORM VAL-FRM-GLS-TOCR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TOCR-LEN IN AUX-FLD
                GO TO FIN-VAL-FLD-AUX.
           PERFORM VAL-AUX-CIC-COCR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO AUX-CIC-COCR-LEN IN AUX-FLD
                GO TO FIN-VAL-FLD-AUX.
           PERFORM VAL-AUX-EXT-CTAB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO AUX-EXT-CTAB-LEN IN AUX-FLD
                GO TO FIN-VAL-FLD-AUX.
           PERFORM VAL-AUX-GLS-DESC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO AUX-GLS-DESC-LEN IN AUX-FLD
                GO TO FIN-VAL-FLD-AUX.
           PERFORM VAL-AUX-GLS-ABRV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO AUX-GLS-ABRV-LEN IN AUX-FLD
                GO TO FIN-VAL-FLD-AUX.
           PERFORM VAL-AUX-GLS-DCOR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO AUX-GLS-DCOR-LEN IN AUX-FLD
                GO TO FIN-VAL-FLD-AUX.
           PERFORM VAL-AUX-IND-VIGE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO AUX-IND-VIGE-LEN IN AUX-FLD
                GO TO FIN-VAL-FLD-AUX.
*% RREG = "AUX"
*% RREG_FRM = "AUX"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-AUX-FEC-FTRN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                AUX-FEC-FTRN-LEN IN AUX-FLD
                GO TO FIN-VAL-FLD-AUX.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-AUX-GLS-CCTE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO AUX-GLS-CCTE-LEN IN AUX-FLD
                GO TO FIN-VAL-FLD-AUX.
           PERFORM VAL-AUX-IND-PCTB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO AUX-IND-PCTB-LEN IN AUX-FLD
                GO TO FIN-VAL-FLD-AUX.
           PERFORM VAL-AUX-IND-PCDR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO AUX-IND-PCDR-LEN IN AUX-FLD
                GO TO FIN-VAL-FLD-AUX.
*% INCLUDE FIN_VAL_FLD_AUX OF 'COLBMAUX.TXT'
       FIN-VAL-FLD-AUX.
           EXIT.
       
       VAL-FRM-GLS-TOCR SECTION.
       INI-VAL-FRM-GLS-TOCR.
*% LBL = "FRM_GLS_TOCR OF 'COLBMAUX.TXT'"
*% INCLUDE INI_FRM_GLS_TOCR OF 'COLBMAUX.TXT'
*% INCLUDE FRM_GLS_TOCR OF 'COLBMAUX.TXT'
*% INCLUDE FIN_FRM_GLS_TOCR OF 'COLBMAUX.TXT'
       FIN-VAL-FRM-GLS-TOCR.
           EXIT.
       
       VAL-AUX-CIC-COCR SECTION.
       INI-VAL-AUX-CIC-COCR.
*% LBL = "AUX_CIC_COCR OF 'COLBMAUX.TXT'"
*% INCLUDE INI_AUX_CIC_COCR OF 'COLBMAUX.TXT'
           IF AUX-CIC-COCR IN AUX-FLD NOT > SPACES
              MOVE SPACES TO AUX-CIC-COCR IN AUX-FLD.
*% INCLUDE AUX_CIC_COCR OF 'COLBMAUX.TXT'
*% INCLUDE FIN_AUX_CIC_COCR OF 'COLBMAUX.TXT'
       FIN-VAL-AUX-CIC-COCR.
           EXIT.
       
       VAL-AUX-EXT-CTAB SECTION.
       INI-VAL-AUX-EXT-CTAB.
*% LBL = "AUX_EXT_CTAB OF 'COLBMAUX.TXT'"
*% INCLUDE INI_AUX_EXT_CTAB OF 'COLBMAUX.TXT'
           IF AUX-EXT-CTAB IN AUX-FLD NOT > SPACES
              MOVE SPACES TO AUX-EXT-CTAB IN AUX-FLD.
*% INCLUDE AUX_EXT_CTAB OF 'COLBMAUX.TXT'
*% INCLUDE FIN_AUX_EXT_CTAB OF 'COLBMAUX.TXT'
       FIN-VAL-AUX-EXT-CTAB.
           EXIT.
       
       VAL-AUX-GLS-DESC SECTION.
       INI-VAL-AUX-GLS-DESC.
*% LBL = "AUX_GLS_DESC OF 'COLBMAUX.TXT'"
*% INCLUDE INI_AUX_GLS_DESC OF 'COLBMAUX.TXT'
           IF AUX-GLS-DESC IN AUX-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-AUX-GLS-DESC.
*% INCLUDE AUX_GLS_DESC OF 'COLBMAUX.TXT'
*% INCLUDE FIN_AUX_GLS_DESC OF 'COLBMAUX.TXT'
       FIN-VAL-AUX-GLS-DESC.
           EXIT.
       
       VAL-AUX-GLS-ABRV SECTION.
       INI-VAL-AUX-GLS-ABRV.
*% LBL = "AUX_GLS_ABRV OF 'COLBMAUX.TXT'"
*% INCLUDE INI_AUX_GLS_ABRV OF 'COLBMAUX.TXT'
           IF AUX-GLS-ABRV IN AUX-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-AUX-GLS-ABRV.
*% INCLUDE AUX_GLS_ABRV OF 'COLBMAUX.TXT'
*% INCLUDE FIN_AUX_GLS_ABRV OF 'COLBMAUX.TXT'
       FIN-VAL-AUX-GLS-ABRV.
           EXIT.
       
       VAL-AUX-GLS-DCOR SECTION.
       INI-VAL-AUX-GLS-DCOR.
*% LBL = "AUX_GLS_DCOR OF 'COLBMAUX.TXT'"
*% INCLUDE INI_AUX_GLS_DCOR OF 'COLBMAUX.TXT'
           IF AUX-GLS-DCOR IN AUX-FLD NOT > SPACES
