*% INCLUDE AUX_IND_VIGE OF 'COLBMAUX.TXT'
*% BUF = "AUX-FLD"
*% SIS = "GNS"
*% TAB = "VIGE"
*% FLD = "AUX-IND-VIGE"
*% GLS = ""
*% TIENE_GLOSA = FALSE
*% LGL = 0
*% INCLUDE 'VALIND'
