      *
      * COLBQCVC
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:48 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CVC"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMCVC.TXT'
       SET-TAG-COLBQCVC SECTION.
       INI-SET-TAG-COLBQCVC.
           MOVE
           '<IDNMOD NOM=$COLBQCVC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:48 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCVC.
           EXIT.
           
       PUT-ALL-CVC SECTION.
       INI-PUT-ALL-CVC.
*% INCLUDE INI_PUT_ALL_CVC OF 'COLBMCVC.TXT'
           PERFORM PUT-CVC-CVC.
*% INCLUDE FIN_PUT_ALL_CVC OF 'COLBMCVC.TXT'
       FIN-PUT-ALL-CVC.
           EXIT.
       
       PUT-CVC-CVC SECTION.
       INI-PUT-CVC-CVC.
*% INCLUDE INI_PUT_CVC_CVC OF 'COLBMCVC.TXT'
           IF PGM-STAT-CVC-NUL
               GO TO FIN-PUT-CVC-CVC.
           IF NOT PGM-STAT-CVC-OKS
               PERFORM CLR-CVC-CVC
               GO TO FIN-PUT-CVC-CVC.
           MOVE CVC-MSC-BCCR IN CVC TO CVC-MSC-BCCR IN CVC-FLD.
           MOVE CVC-MSC-VICR IN CVC TO CVC-MSC-VICR IN CVC-FLD.
           MOVE CVC-CIC-TCVC IN CVC TO CVC-CIC-TCVC IN CVC-FLD.
           MOVE CVC-EXT-CTAB IN CVC TO CVC-EXT-CTAB IN CVC-FLD.
           MOVE CVC-GLS-DESC IN CVC TO CVC-GLS-DESC IN CVC-FLD.
           MOVE CVC-GLS-ABRV IN CVC TO CVC-GLS-ABRV IN CVC-FLD.
           MOVE CVC-GLS-DCOR IN CVC TO CVC-GLS-DCOR IN CVC-FLD.
           MOVE CVC-IND-VIGE IN CVC TO CVC-IND-VIGE IN CVC-FLD.
*% RREG = "CVC"
*% RREG_FRM = "CVC"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CVC-NUM-DTRN IN CVC TO CVC-NUM-DTRN IN CVC-FLD.
           MOVE CVC-NUM-MTRN IN CVC TO CVC-NUM-MTRN IN CVC-FLD.
           MOVE CVC-NUM-STRN IN CVC TO CVC-NUM-STRN IN CVC-FLD.
           MOVE CVC-NUM-ATRN IN CVC TO CVC-NUM-ATRN IN CVC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% BUF = "CVC-FLD"
*% SIS = "COL"
*% TAB = "BCCR"
*% FLD = "CVC-MSC-BCCR"
*% GLS = "FRM-GLS-BCCR"
*% TIENE_GLOSA = TRUE
*% LGL = 34
*% INCLUDE 'BUSMSC'
*% BUF = "CVC-FLD"
*% SIS = "COL"
*% TAB = "VICR"
*% FLD = "CVC-MSC-VICR"
*% GLS = "FRM-GLS-VICR"
*% TIENE_GLOSA = TRUE
*% LGL = 34
*% INCLUDE 'BUSMSC'
*% INCLUDE FIN_PUT_CVC_CVC OF 'COLBMCVC.TXT'
       FIN-PUT-CVC-CVC.
           EXIT.
       
       KEY-ALL-CVC SECTION.
       INI-KEY-ALL-CVC.
*% INCLUDE INI_KEY_ALL_CVC OF 'COLBMCVC.TXT'
           PERFORM KEY-CVC-CVC.
*% INCLUDE FIN_KEY_ALL_CVC OF 'COLBMCVC.TXT'
       FIN-KEY-ALL-CVC.
           EXIT.
       
       KEY-CVC-CVC SECTION.
       INI-KEY-CVC-CVC.
*% INCLUDE INI_KEY_CVC_CVC OF 'COLBMCVC.TXT'
           MOVE CVC-MSC-BCCR IN CVC-FLD TO CVC-MSC-BCCR IN CVC.
           MOVE CVC-MSC-VICR IN CVC-FLD TO CVC-MSC-VICR IN CVC.
*% INCLUDE FIN_KEY_CVC_CVC OF 'COLBMCVC.TXT'
       FIN-KEY-CVC-CVC.
           EXIT.
       
       SCR-CLRA-CVC SECTION.
       INI-SCR-CLRA-CVC.
*% INCLUDE INI_SCR_CLRA_CVC OF 'COLBMCVC.TXT'
           PERFORM CLR-KEY-CVC.
           PERFORM CLR-FLD-CVC.
*% INCLUDE FIN_SCR_CLRA_CVC OF 'COLBMCVC.TXT'
       FIN-SCR-CLRA-CVC.
           EXIT.
       
       CLR-KEY-CVC SECTION.
       INI-CLR-KEY-CVC.
*% INCLUDE INI_CLR_KEY_CVC OF 'COLBMCVC.TXT'
           MOVE SPACES TO CVC-MSC-BCCR IN CVC-FLD.
           MOVE SPACES TO CVC-MSC-VICR IN CVC-FLD.
*% INCLUDE FIN_CLR_KEY_CVC OF 'COLBMCVC.TXT'
       FIN-CLR-KEY-CVC.
           EXIT.
       
       CLR-FLD-CVC SECTION.
       INI-CLR-FLD-CVC.
*% INCLUDE INI_CLR_FLD_CVC OF 'COLBMCVC.TXT'
           MOVE SPACES TO FRM-GLS-BCCR IN CVC-FLD.
           MOVE SPACES TO FRM-GLS-VICR IN CVC-FLD.
           PERFORM CLR-CVC-CVC.
*% INCLUDE FIN_CLR_FLD_CVC OF 'COLBMCVC.TXT'
       FIN-CLR-FLD-CVC.
           EXIT.
       
       CLR-CVC-CVC SECTION.
       INI-CLR-CVC-CVC.
*% INCLUDE INI_CLR_CVC_CVC OF 'COLBMCVC.TXT'
           MOVE SPACES TO CVC-CIC-TCVC IN CVC-FLD.
           MOVE SPACES TO CVC-EXT-CTAB IN CVC-FLD.
           MOVE SPACES TO CVC-GLS-DESC IN CVC-FLD.
           MOVE SPACES TO CVC-GLS-ABRV IN CVC-FLD.
           MOVE SPACES TO CVC-GLS-DCOR IN CVC-FLD.
           MOVE SPACES TO CVC-IND-VIGE IN CVC-FLD.
*% RREG = "CVC"
*% RREG_FRM = "CVC"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CVC-FEC-FTRN IN CVC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_CVC_CVC OF 'COLBMCVC.TXT'
       FIN-CLR-CVC-CVC.
           EXIT.
       
       FST-KEY-CVC SECTION.
       INI-FST-KEY-CVC.
*% INCLUDE INI_FST_KEY_CVC OF 'COLBMCVC.TXT'
           MOVE -1 TO CVC-MSC-BCCR-LEN IN CVC-FLD.
*% INCLUDE FIN_FST_KEY_CVC OF 'COLBMCVC.TXT'
       FIN-FST-KEY-CVC.
           EXIT.
       
       FST-FLD-CVC SECTION.
       INI-FST-FLD-CVC.
*% INCLUDE INI_FST_FLD_CVC OF 'COLBMCVC.TXT'
           MOVE -1 TO CVC-CIC-TCVC-LEN IN CVC-FLD.
*% INCLUDE FIN_FST_FLD_CVC OF 'COLBMCVC.TXT'
       FIN-FST-FLD-CVC.
           EXIT.
       
       PRO-KEY-CVC SECTION.
       INI-PRO-KEY-CVC.
*% INCLUDE INI_PRO_KEY_CVC OF 'COLBMCVC.TXT'
           PERFORM PRO-IKY-CVC.
           PERFORM PRO-AKY-CVC.
           PERFORM PRO-MKY-CVC.
*% INCLUDE FIN_PRO_KEY_CVC OF 'COLBMCVC.TXT'
       FIN-PRO-KEY-CVC.
           EXIT.
       
       PRO-IKY-CVC SECTION.
       INI-PRO-IKY-CVC.
*% INCLUDE INI_PRO_IKY_CVC OF 'COLBMCVC.TXT'
           MOVE FRM-CPIM-AEY TO CVC-MSC-BCCR-ATR IN CVC-FLD.
           MOVE FRM-CPIM-AEY TO CVC-MSC-VICR-ATR IN CVC-FLD.
*% INCLUDE FIN_PRO_IKY_CVC OF 'COLBMCVC.TXT'
       FIN-PRO-IKY-CVC.
           EXIT.
       
       PRO-MKY-CVC SECTION.
       INI-PRO-MKY-CVC.
*% INCLUDE INI_PRO_MKY_CVC OF 'COLBMCVC.TXT'
*% INCLUDE FIN_PRO_MKY_CVC OF 'COLBMCVC.TXT'
       FIN-PRO-MKY-CVC.
           EXIT.
       
       PRO-AKY-CVC SECTION.
       INI-PRO-AKY-CVC.
*% INCLUDE INI_PRO_AKY_CVC OF 'COLBMCVC.TXT'
*% INCLUDE FIN_PRO_AKY_CVC OF 'COLBMCVC.TXT'
       FIN-PRO-AKY-CVC.
           EXIT.
       
       UNP-KEY-CVC SECTION.
       INI-UNP-KEY-CVC.
*% INCLUDE INI_UNP_KEY_CVC OF 'COLBMCVC.TXT'
           PERFORM UNP-IKY-CVC.
           PERFORM UNP-AKY-CVC.
           PERFORM UNP-MKY-CVC.
*% INCLUDE FIN_UNP_KEY_CVC OF 'COLBMCVC.TXT'
       FIN-UNP-KEY-CVC.
           EXIT.
       
       UNP-IKY-CVC SECTION.
       INI-UNP-IKY-CVC.
*% INCLUDE INI_UNP_IKY_CVC OF 'COLBMCVC.TXT'
           MOVE FRM-CPIM-UBY TO CVC-MSC-BCCR-ATR.
           MOVE FRM-CPIM-UBY TO CVC-MSC-VICR-ATR.
*% INCLUDE FIN_UNP_IKY_CVC OF 'COLBMCVC.TXT'
       FIN-UNP-IKY-CVC.
           EXIT.
       
       UNP-MKY-CVC SECTION.
       INI-UNP-MKY-CVC.
*% INCLUDE INI_UNP_MKY_CVC OF 'COLBMCVC.TXT'
*% INCLUDE FIN_UNP_MKY_CVC OF 'COLBMCVC.TXT'
       FIN-UNP-MKY-CVC.
           EXIT.
       
       UNP-AKY-CVC SECTION.
       INI-UNP-AKY-CVC.
*% INCLUDE INI_UNP_AKY_CVC OF 'COLBMCVC.TXT'
*% INCLUDE FIN_UNP_AKY_CVC OF 'COLBMCVC.TXT'
       FIN-UNP-AKY-CVC.
           EXIT.
       
       UNP-FLD-CVC SECTION.
       INI-UNP-FLD-CVC.
*% INCLUDE INI_UNP_FLD_CVC OF 'COLBMCVC.TXT'
           PERFORM UNP-CVC-CVC.
*% INCLUDE FIN_UNP_FLD_CVC OF 'COLBMCVC.TXT'
       FIN-UNP-FLD-CVC.
           EXIT.
       
       UNP-CVC-CVC SECTION.
       INI-UNP-CVC-CVC.
*% INCLUDE INI_UNP_CVC_CVC OF 'COLBMCVC.TXT'
           MOVE FRM-CPIM-UBY TO CVC-CIC-TCVC-ATR IN CVC-FLD.
           MOVE FRM-CPIM-UBY TO CVC-EXT-CTAB-ATR IN CVC-FLD.
           MOVE FRM-CPIM-UBY TO CVC-GLS-DESC-ATR IN CVC-FLD.
           MOVE FRM-CPIM-UBY TO CVC-GLS-ABRV-ATR IN CVC-FLD.
           MOVE FRM-CPIM-UBY TO CVC-GLS-DCOR-ATR IN CVC-FLD.
           MOVE FRM-CPIM-UBY TO CVC-IND-VIGE-ATR IN CVC-FLD.
*% INCLUDE FIN_UNP_CVC_CVC OF 'COLBMCVC.TXT'
       FIN-UNP-CVC-CVC.
           EXIT.
       
       VAL-KEY-CVC SECTION.
       INI-VAL-KEY-CVC.
           PERFORM VAL-IKY-CVC.
           PERFORM VAL-MKY-CVC.
           PERFORM VAL-AKY-CVC.
       FIN-VAL-KEY-CVC.
           EXIT.
       
       VAL-IKY-CVC SECTION.
       INI-VAL-IKY-CVC.
*% INCLUDE INI_VAL_IKY_CVC OF 'COLBMCVC.TXT'
           PERFORM VAL-CVC-MSC-BCCR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CVC-MSC-BCCR-LEN IN CVC-FLD
                GO TO FIN-VAL-IKY-CVC.
           PERFORM VAL-CVC-MSC-VICR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CVC-MSC-VICR-LEN IN CVC-FLD
                GO TO FIN-VAL-IKY-CVC.
*% INCLUDE FIN_VAL_IKY_CVC OF 'COLBMCVC.TXT'
       FIN-VAL-IKY-CVC.
           EXIT.
       
       VAL-MKY-CVC SECTION.
       INI-VAL-MKY-CVC.
*% INCLUDE INI_VAL_MKY_CVC OF 'COLBMCVC.TXT'
*% INCLUDE FIN_VAL_MKY_CVC OF 'COLBMCVC.TXT'
       FIN-VAL-MKY-CVC.
           EXIT.
       
       VAL-AKY-CVC SECTION.
       INI-VAL-AKY-CVC.
*% INCLUDE INI_VAL_AKY_CVC OF 'COLBMCVC.TXT'
*% INCLUDE FIN_VAL_AKY_CVC OF 'COLBMCVC.TXT'
       FIN-VAL-AKY-CVC.
           EXIT.
       
       VAL-CVC-MSC-BCCR SECTION.
       INI-VAL-CVC-MSC-BCCR.
*% LBL = "CVC_MSC_BCCR OF 'COLBMCVC.TXT'"
*% INCLUDE INI_CVC_MSC_BCCR OF 'COLBMCVC.TXT'
           IF CVC-MSC-BCCR IN CVC-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-CVC-MSC-BCCR.
*% INCLUDE CVC_MSC_BCCR OF 'COLBMCVC.TXT'
*% BUF = "CVC-FLD"
*% SIS = "COL"
*% TAB = "BCCR"
*% FLD = "CVC-MSC-BCCR"
*% GLS = "FRM-GLS-BCCR"
*% TIENE_GLOSA = TRUE
*% LGL = 34
*% INCLUDE 'VALIND'
*% INCLUDE FIN_CVC_MSC_BCCR OF 'COLBMCVC.TXT'
       FIN-VAL-CVC-MSC-BCCR.
           EXIT.
       
       VAL-CVC-MSC-VICR SECTION.
       INI-VAL-CVC-MSC-VICR.
*% LBL = "CVC_MSC_VICR OF 'COLBMCVC.TXT'"
*% INCLUDE INI_CVC_MSC_VICR OF 'COLBMCVC.TXT'
           IF CVC-MSC-VICR IN CVC-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-CVC-MSC-VICR.
*% INCLUDE CVC_MSC_VICR OF 'COLBMCVC.TXT'
*% BUF = "CVC-FLD"
*% SIS = "COL"
*% TAB = "VICR"
*% FLD = "CVC-MSC-VICR"
*% GLS = "FRM-GLS-VICR"
*% TIENE_GLOSA = TRUE
*% LGL = 34
*% INCLUDE 'VALIND'
*% INCLUDE FIN_CVC_MSC_VICR OF 'COLBMCVC.TXT'
       FIN-VAL-CVC-MSC-VICR.
           EXIT.
*% RREG = "CVC"
*% RREG_FRM = "CVC"
*% NOMB = "FTRN"
*% IZR = "S"
       
       VAL-CON-KEY-CVC SECTION.
       INI-VAL-CON-KEY-CVC.
*% INCLUDE INI_VAL_CON_KEY_CVC OF 'COLBMCVC.TXT'
           IF     CVC-MSC-BCCR IN CVC-FLD NOT > SPACES 
              AND CVC-MSC-VICR IN CVC-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CVC.
*% INCLUDE FIN_VAL_CON_KEY_CVC OF 'COLBMCVC.TXT'
       FIN-VAL-CON-KEY-CVC.
           EXIT.
       
       VAL-CON-IKY-CVC SECTION.
       INI-VAL-CON-IKY-CVC.
*% INCLUDE INI_VAL_CON_IKY_CVC OF 'COLBMCVC.TXT'
           IF     CVC-MSC-BCCR IN CVC-FLD NOT > SPACES 
              AND CVC-MSC-VICR IN CVC-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CVC.
*% INCLUDE FIN_VAL_CON_IKY_CVC OF 'COLBMCVC.TXT'
       FIN-VAL-CON-IKY-CVC.
           EXIT.
       
       VAL-NUM-KEY-CVC SECTION.
       INI-VAL-NUM-KEY-CVC.
           PERFORM VAL-NUM-IKY-CVC.
           PERFORM VAL-NUM-MKY-CVC.
           PERFORM VAL-NUM-AKY-CVC.
       FIN-VAL-NUM-KEY-CVC.
           EXIT.
       
       VAL-NUM-IKY-CVC SECTION.
       INI-VAL-NUM-IKY-CVC.
*% INCLUDE INI_VAL_NUM_IKY_CVC OF 'COLBMCVC.TXT'
*% INCLUDE FIN_VAL_NUM_IKY_CVC OF 'COLBMCVC.TXT'
       FIN-VAL-NUM-IKY-CVC.
           EXIT.
       
       VAL-NUM-MKY-CVC SECTION.
       INI-VAL-NUM-MKY-CVC.
*% INCLUDE INI_VAL_NUM_MKY_CVC OF 'COLBMCVC.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_CVC OF 'COLBMCVC.TXT'
       FIN-VAL-NUM-MKY-CVC.
           EXIT.
       
       VAL-NUM-AKY-CVC SECTION.
       INI-VAL-NUM-AKY-CVC.
*% INCLUDE INI_VAL_NUM_AKY_CVC OF 'COLBMCVC.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_CVC OF 'COLBMCVC.TXT'
       FIN-VAL-NUM-AKY-CVC.
           EXIT.
       
       VAL-NUM-FLD-CVC SECTION.
       INI-VAL-NUM-FLD-CVC.
*% INCLUDE INI_VAL_NUM_FLD_CVC OF 'COLBMCVC.TXT'
*% INCLUDE FIN_VAL_NUM_FLD_CVC OF 'COLBMCVC.TXT'
       FIN-VAL-NUM-FLD-CVC.
           EXIT.
       
       EDT-KEY-CVC SECTION.
       INI-EDT-KEY-CVC.
           PERFORM EDT-IKY-CVC.
           PERFORM EDT-MKY-CVC.
           PERFORM EDT-AKY-CVC.
*% INCLUDE EDT_KEY_CVC OF 'COLBMCVC.TXT'
       FIN-EDT-KEY-CVC.
           EXIT.
       
       EDT-IKY-CVC SECTION.
       INI-EDT-IKY-CVC.
*% INCLUDE EDT_IKY_CVC OF 'COLBMCVC.TXT'
*% INCLUDE FIN_EDT_IKY_CVC OF 'COLBMCVC.TXT'
       FIN-EDT-IKY-CVC.
           EXIT.
       
       EDT-MKY-CVC SECTION.
       INI-EDT-MKY-CVC.
*% INCLUDE EDT_MKY_CVC OF 'COLBMCVC.TXT'
*% INCLUDE FIN_EDT_MKY_CVC OF 'COLBMCVC.TXT'
       FIN-EDT-MKY-CVC.
           EXIT.
       
       EDT-AKY-CVC SECTION.
       INI-EDT-AKY-CVC.
*% INCLUDE EDT_AKY_CVC OF 'COLBMCVC.TXT'
*% INCLUDE FIN_EDT_AKY_CVC OF 'COLBMCVC.TXT'
       FIN-EDT-AKY-CVC.
           EXIT.
       
       EDT-FLD-CVC SECTION.
       INI-EDT-FLD-CVC.
*% INCLUDE EDT_FLD_CVC OF 'COLBMCVC.TXT'
*% RREG = "CVC"
*% RREG_FRM = "CVC"
*% NOMB = "FTRN"
*% INCLUDE FIN_EDT_FLD_CVC OF 'COLBMCVC.TXT'
       FIN-EDT-FLD-CVC.
           EXIT.
*% INCLUDE EOF_BQ OF 'COLBMCVC.TXT'
*% INCLUDE EOF OF 'COLBMCVC.TXT'
*% INCLUDE EOF_IBM OF 'COLBMCVC.TXT'
