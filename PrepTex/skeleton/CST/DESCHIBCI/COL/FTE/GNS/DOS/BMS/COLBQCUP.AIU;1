      *
      * COLBQCUP
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:44 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CUP"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMCUP.TXT'
       SET-TAG-COLBQCUP SECTION.
       INI-SET-TAG-COLBQCUP.
           MOVE
           '<IDNMOD NOM=$COLBQCUP$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:44 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCUP.
           EXIT.
           
       PUT-ALL-CUP SECTION.
       INI-PUT-ALL-CUP.
*% INCLUDE INI_PUT_ALL_CUP OF 'COLBMCUP.TXT'
           PERFORM PUT-PTT-CUP.
*% INCLUDE FIN_PUT_ALL_CUP OF 'COLBMCUP.TXT'
       FIN-PUT-ALL-CUP.
           EXIT.
       
       PUT-PTT-CUP SECTION.
       INI-PUT-PTT-CUP.
*% INCLUDE INI_PUT_PTT_CUP OF 'COLBMCUP.TXT'
           IF PGM-STAT-PTT-NUL
               GO TO FIN-PUT-PTT-CUP.
           IF NOT PGM-STAT-PTT-OKS
               PERFORM CLR-PTT-CUP
               GO TO FIN-PUT-PTT-CUP.
           MOVE PTT-CAI-IPTT IN PTT TO PTT-CAI-IPTT IN CUP-FLD.
           MOVE PTT-IIC-IPTT IN PTT TO PTT-IIC-IPTT IN CUP-FLD.
           MOVE PTT-CAI-ICVN IN PTT TO PTT-CAI-ICVN IN CUP-FLD.
           MOVE PTT-IIC-ICVN IN PTT TO PTT-IIC-ICVN IN CUP-FLD.
           MOVE PTT-COD-TIPO IN PTT TO PTT-COD-TIPO IN CUP-FLD.
           MOVE PTT-IND-SEPA IN PTT TO PTT-IND-SEPA IN CUP-FLD.
           MOVE PTT-GLS-NOMP IN PTT TO PTT-GLS-NOMP IN CUP-FLD.
*% INCLUDE FIN_PUT_PTT_CUP OF 'COLBMCUP.TXT'
       FIN-PUT-PTT-CUP.
           EXIT.
       
       KEY-ALL-CUP SECTION.
       INI-KEY-ALL-CUP.
*% INCLUDE INI_KEY_ALL_CUP OF 'COLBMCUP.TXT'
           PERFORM KEY-PTT-CUP.
*% INCLUDE FIN_KEY_ALL_CUP OF 'COLBMCUP.TXT'
       FIN-KEY-ALL-CUP.
           EXIT.
       
       KEY-PTT-CUP SECTION.
       INI-KEY-PTT-CUP.
*% INCLUDE INI_KEY_PTT_CUP OF 'COLBMCUP.TXT'
           MOVE PTT-CAI-IPTT IN CUP-FLD TO PTT-CAI-IPTT IN PTT.
           MOVE PTT-IIC-IPTT IN CUP-FLD TO PTT-IIC-IPTT IN PTT.
*% INCLUDE FIN_KEY_PTT_CUP OF 'COLBMCUP.TXT'
       FIN-KEY-PTT-CUP.
           EXIT.
       
       SCR-CLRA-CUP SECTION.
       INI-SCR-CLRA-CUP.
*% INCLUDE INI_SCR_CLRA_CUP OF 'COLBMCUP.TXT'
           PERFORM CLR-KEY-CUP.
           PERFORM CLR-FLD-CUP.
*% INCLUDE FIN_SCR_CLRA_CUP OF 'COLBMCUP.TXT'
       FIN-SCR-CLRA-CUP.
           EXIT.
       
       CLR-KEY-CUP SECTION.
       INI-CLR-KEY-CUP.
*% INCLUDE INI_CLR_KEY_CUP OF 'COLBMCUP.TXT'
           MOVE SPACES TO PTT-CAI-IPTT IN CUP-FLD.
           MOVE ZEROES TO PTT-IIC-IPTT IN CUP-FLD.
           MOVE ZEROES TO FRM-VAL-NDTT IN CUP-FLD.
*% INCLUDE FIN_CLR_KEY_CUP OF 'COLBMCUP.TXT'
       FIN-CLR-KEY-CUP.
           EXIT.
       
       CLR-FLD-CUP SECTION.
       INI-CLR-FLD-CUP.
*% INCLUDE INI_CLR_FLD_CUP OF 'COLBMCUP.TXT'
           MOVE SPACES TO FRM-GLS-TIPO IN CUP-FLD.
           MOVE ZEROES TO FRM-NUM-IDTT IN CUP-FLD(1).
           MOVE ZEROES TO FRM-NUM-IDTT IN CUP-FLD(2).
           MOVE ZEROES TO FRM-NUM-IDTT IN CUP-FLD(3).
           MOVE ZEROES TO FRM-NUM-IDTT IN CUP-FLD(4).
           MOVE ZEROES TO FRM-NUM-IDTT IN CUP-FLD(5).
           MOVE ZEROES TO FRM-NUM-IDTT IN CUP-FLD(6).
           MOVE ZEROES TO FRM-NUM-IDTT IN CUP-FLD(7).
           MOVE ZEROES TO FRM-NUM-IDTT IN CUP-FLD(8).
           MOVE ZEROES TO FRM-NUM-CODI IN CUP-FLD(1).
           MOVE ZEROES TO FRM-NUM-CODI IN CUP-FLD(2).
           MOVE ZEROES TO FRM-NUM-CODI IN CUP-FLD(3).
           MOVE ZEROES TO FRM-NUM-CODI IN CUP-FLD(4).
           MOVE ZEROES TO FRM-NUM-CODI IN CUP-FLD(5).
           MOVE ZEROES TO FRM-NUM-CODI IN CUP-FLD(6).
           MOVE ZEROES TO FRM-NUM-CODI IN CUP-FLD(7).
           MOVE ZEROES TO FRM-NUM-CODI IN CUP-FLD(8).
           MOVE SPACES TO FRM-GLS-CODI IN CUP-FLD(1).
           MOVE SPACES TO FRM-GLS-CODI IN CUP-FLD(2).
           MOVE SPACES TO FRM-GLS-CODI IN CUP-FLD(3).
           MOVE SPACES TO FRM-GLS-CODI IN CUP-FLD(4).
           MOVE SPACES TO FRM-GLS-CODI IN CUP-FLD(5).
           MOVE SPACES TO FRM-GLS-CODI IN CUP-FLD(6).
           MOVE SPACES TO FRM-GLS-CODI IN CUP-FLD(7).
           MOVE SPACES TO FRM-GLS-CODI IN CUP-FLD(8).
           MOVE ZEROES TO FRM-NUM-LARG IN CUP-FLD(1).
           MOVE ZEROES TO FRM-NUM-LARG IN CUP-FLD(2).
           MOVE ZEROES TO FRM-NUM-LARG IN CUP-FLD(3).
           MOVE ZEROES TO FRM-NUM-LARG IN CUP-FLD(4).
           MOVE ZEROES TO FRM-NUM-LARG IN CUP-FLD(5).
           MOVE ZEROES TO FRM-NUM-LARG IN CUP-FLD(6).
           MOVE ZEROES TO FRM-NUM-LARG IN CUP-FLD(7).
           MOVE ZEROES TO FRM-NUM-LARG IN CUP-FLD(8).
           PERFORM CLR-PTT-CUP.
*% INCLUDE FIN_CLR_FLD_CUP OF 'COLBMCUP.TXT'
       FIN-CLR-FLD-CUP.
           EXIT.
       
       CLR-PTT-CUP SECTION.
       INI-CLR-PTT-CUP.
*% INCLUDE INI_CLR_PTT_CUP OF 'COLBMCUP.TXT'
           MOVE SPACES TO PTT-CAI-ICVN IN CUP-FLD.
           MOVE ZEROES TO PTT-IIC-ICVN IN CUP-FLD.
           MOVE SPACES TO PTT-COD-TIPO IN CUP-FLD.
           MOVE SPACES TO PTT-IND-SEPA IN CUP-FLD.
           MOVE SPACES TO PTT-GLS-NOMP IN CUP-FLD.
*% INCLUDE FIN_CLR_PTT_CUP OF 'COLBMCUP.TXT'
       FIN-CLR-PTT-CUP.
           EXIT.
       
       FST-KEY-CUP SECTION.
       INI-FST-KEY-CUP.
*% INCLUDE INI_FST_KEY_CUP OF 'COLBMCUP.TXT'
           MOVE -1 TO PTT-CAI-IPTT-LEN IN CUP-FLD.
*% INCLUDE FIN_FST_KEY_CUP OF 'COLBMCUP.TXT'
       FIN-FST-KEY-CUP.
           EXIT.
       
       FST-FLD-CUP SECTION.
       INI-FST-FLD-CUP.
*% INCLUDE INI_FST_FLD_CUP OF 'COLBMCUP.TXT'
*% INCLUDE FIN_FST_FLD_CUP OF 'COLBMCUP.TXT'
       FIN-FST-FLD-CUP.
           EXIT.
       
       PRO-KEY-CUP SECTION.
       INI-PRO-KEY-CUP.
*% INCLUDE INI_PRO_KEY_CUP OF 'COLBMCUP.TXT'
           MOVE FRM-CPIM-AEY TO FRM-VAL-NDTT-ATR IN CUP-FLD.
           PERFORM PRO-IKY-CUP.
           PERFORM PRO-AKY-CUP.
           PERFORM PRO-MKY-CUP.
*% INCLUDE FIN_PRO_KEY_CUP OF 'COLBMCUP.TXT'
       FIN-PRO-KEY-CUP.
           EXIT.
       
       PRO-IKY-CUP SECTION.
       INI-PRO-IKY-CUP.
*% INCLUDE INI_PRO_IKY_CUP OF 'COLBMCUP.TXT'
           MOVE FRM-CPIM-AEY TO PTT-CAI-IPTT-ATR IN CUP-FLD.
           MOVE FRM-CPIM-AEY TO PTT-IIC-IPTT-ATR IN CUP-FLD.
*% INCLUDE FIN_PRO_IKY_CUP OF 'COLBMCUP.TXT'
       FIN-PRO-IKY-CUP.
           EXIT.
       
       PRO-MKY-CUP SECTION.
       INI-PRO-MKY-CUP.
*% INCLUDE INI_PRO_MKY_CUP OF 'COLBMCUP.TXT'
*% INCLUDE FIN_PRO_MKY_CUP OF 'COLBMCUP.TXT'
       FIN-PRO-MKY-CUP.
           EXIT.
       
       PRO-AKY-CUP SECTION.
       INI-PRO-AKY-CUP.
*% INCLUDE INI_PRO_AKY_CUP OF 'COLBMCUP.TXT'
*% INCLUDE FIN_PRO_AKY_CUP OF 'COLBMCUP.TXT'
       FIN-PRO-AKY-CUP.
           EXIT.
       
       UNP-KEY-CUP SECTION.
       INI-UNP-KEY-CUP.
*% INCLUDE INI_UNP_KEY_CUP OF 'COLBMCUP.TXT'
           MOVE FRM-CPIM-UBY TO FRM-VAL-NDTT-ATR IN CUP-FLD.
           PERFORM UNP-IKY-CUP.
           PERFORM UNP-AKY-CUP.
           PERFORM UNP-MKY-CUP.
*% INCLUDE FIN_UNP_KEY_CUP OF 'COLBMCUP.TXT'
       FIN-UNP-KEY-CUP.
           EXIT.
       
       UNP-IKY-CUP SECTION.
       INI-UNP-IKY-CUP.
*% INCLUDE INI_UNP_IKY_CUP OF 'COLBMCUP.TXT'
           MOVE FRM-CPIM-UBY TO PTT-CAI-IPTT-ATR.
           MOVE FRM-CPIM-UBY TO PTT-IIC-IPTT-ATR.
           MOVE FRM-CPIM-UBY TO FRM-VAL-NDTT-ATR.
*% INCLUDE FIN_UNP_IKY_CUP OF 'COLBMCUP.TXT'
       FIN-UNP-IKY-CUP.
           EXIT.
       
       UNP-MKY-CUP SECTION.
       INI-UNP-MKY-CUP.
*% INCLUDE INI_UNP_MKY_CUP OF 'COLBMCUP.TXT'
*% INCLUDE FIN_UNP_MKY_CUP OF 'COLBMCUP.TXT'
       FIN-UNP-MKY-CUP.
           EXIT.
       
       UNP-AKY-CUP SECTION.
       INI-UNP-AKY-CUP.
*% INCLUDE INI_UNP_AKY_CUP OF 'COLBMCUP.TXT'
*% INCLUDE FIN_UNP_AKY_CUP OF 'COLBMCUP.TXT'
       FIN-UNP-AKY-CUP.
           EXIT.
       
       UNP-FLD-CUP SECTION.
       INI-UNP-FLD-CUP.
*% INCLUDE INI_UNP_FLD_CUP OF 'COLBMCUP.TXT'
           PERFORM UNP-PTT-CUP.
*% INCLUDE FIN_UNP_FLD_CUP OF 'COLBMCUP.TXT'
       FIN-UNP-FLD-CUP.
           EXIT.
       
       UNP-PTT-CUP SECTION.
       INI-UNP-PTT-CUP.
*% INCLUDE INI_UNP_PTT_CUP OF 'COLBMCUP.TXT'
*% INCLUDE FIN_UNP_PTT_CUP OF 'COLBMCUP.TXT'
       FIN-UNP-PTT-CUP.
           EXIT.
       
       VAL-KEY-CUP SECTION.
       INI-VAL-KEY-CUP.
           PERFORM VAL-IKY-CUP.
           PERFORM VAL-MKY-CUP.
           PERFORM VAL-AKY-CUP.
       FIN-VAL-KEY-CUP.
           EXIT.
       
       VAL-IKY-CUP SECTION.
       INI-VAL-IKY-CUP.
*% INCLUDE INI_VAL_IKY_CUP OF 'COLBMCUP.TXT'
           PERFORM VAL-PTT-CAI-IPTT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PTT-CAI-IPTT-LEN IN CUP-FLD
                GO TO FIN-VAL-IKY-CUP.
           PERFORM VAL-PTT-IIC-IPTT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PTT-IIC-IPTT-LEN IN CUP-FLD
                GO TO FIN-VAL-IKY-CUP.
           PERFORM VAL-FRM-VAL-NDTT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-NDTT-LEN IN CUP-FLD
                GO TO FIN-VAL-IKY-CUP.
*% INCLUDE FIN_VAL_IKY_CUP OF 'COLBMCUP.TXT'
       FIN-VAL-IKY-CUP.
           EXIT.
       
       VAL-MKY-CUP SECTION.
       INI-VAL-MKY-CUP.
*% INCLUDE INI_VAL_MKY_CUP OF 'COLBMCUP.TXT'
*% INCLUDE FIN_VAL_MKY_CUP OF 'COLBMCUP.TXT'
       FIN-VAL-MKY-CUP.
           EXIT.
       
       VAL-AKY-CUP SECTION.
       INI-VAL-AKY-CUP.
*% INCLUDE INI_VAL_AKY_CUP OF 'COLBMCUP.TXT'
*% INCLUDE FIN_VAL_AKY_CUP OF 'COLBMCUP.TXT'
       FIN-VAL-AKY-CUP.
           EXIT.
       
       VAL-PTT-CAI-IPTT SECTION.
       INI-VAL-PTT-CAI-IPTT.
*% LBL = "PTT_CAI_IPTT OF 'COLBMCUP.TXT'"
*% INCLUDE INI_PTT_CAI_IPTT OF 'COLBMCUP.TXT'
           IF PTT-CAI-IPTT IN CUP-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-PTT-CAI-IPTT.
*% INCLUDE PTT_CAI_IPTT OF 'COLBMCUP.TXT'
*% INCLUDE FIN_PTT_CAI_IPTT OF 'COLBMCUP.TXT'
       FIN-VAL-PTT-CAI-IPTT.
           EXIT.
       
       VAL-PTT-IIC-IPTT SECTION.
       INI-VAL-PTT-IIC-IPTT.
*% LBL = "PTT_IIC_IPTT OF 'COLBMCUP.TXT'"
*% INCLUDE INI_PTT_IIC_IPTT OF 'COLBMCUP.TXT'
           IF PTT-IIC-IPTT IN CUP-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-PTT-IIC-IPTT.
*% INCLUDE PTT_IIC_IPTT OF 'COLBMCUP.TXT'
*% INCLUDE FIN_PTT_IIC_IPTT OF 'COLBMCUP.TXT'
       FIN-VAL-PTT-IIC-IPTT.
           EXIT.
       
       VAL-FRM-VAL-NDTT SECTION.
       INI-VAL-FRM-VAL-NDTT.
*% LBL = "FRM_VAL_NDTT OF 'COLBMCUP.TXT'"
*% INCLUDE INI_FRM_VAL_NDTT OF 'COLBMCUP.TXT'
*% INCLUDE FRM_VAL_NDTT OF 'COLBMCUP.TXT'
*% INCLUDE FIN_FRM_VAL_NDTT OF 'COLBMCUP.TXT'
       FIN-VAL-FRM-VAL-NDTT.
           EXIT.
       
       VAL-CON-KEY-CUP SECTION.
       INI-VAL-CON-KEY-CUP.
*% INCLUDE INI_VAL_CON_KEY_CUP OF 'COLBMCUP.TXT'
           IF     PTT-CAI-IPTT IN CUP-FLD NOT > SPACES 
              AND PTT-IIC-IPTT IN CUP-FLD NOT > ZEROES 
              AND FRM-VAL-NDTT IN CUP-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CUP.
*% INCLUDE FIN_VAL_CON_KEY_CUP OF 'COLBMCUP.TXT'
       FIN-VAL-CON-KEY-CUP.
           EXIT.
       
       VAL-CON-IKY-CUP SECTION.
       INI-VAL-CON-IKY-CUP.
*% INCLUDE INI_VAL_CON_IKY_CUP OF 'COLBMCUP.TXT'
           IF     PTT-CAI-IPTT IN CUP-FLD NOT > SPACES 
              AND PTT-IIC-IPTT IN CUP-FLD NOT > ZEROES 
              AND FRM-VAL-NDTT IN CUP-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CUP.
*% INCLUDE FIN_VAL_CON_IKY_CUP OF 'COLBMCUP.TXT'
       FIN-VAL-CON-IKY-CUP.
           EXIT.
       
       VAL-NUM-KEY-CUP SECTION.
       INI-VAL-NUM-KEY-CUP.
           PERFORM VAL-NUM-IKY-CUP.
           PERFORM VAL-NUM-MKY-CUP.
           PERFORM VAL-NUM-AKY-CUP.
       FIN-VAL-NUM-KEY-CUP.
           EXIT.
       
       VAL-NUM-IKY-CUP SECTION.
       INI-VAL-NUM-IKY-CUP.
*% INCLUDE INI_VAL_NUM_IKY_CUP OF 'COLBMCUP.TXT'
       VAL-NUM-PTT-IIC-IPTT.
*% BUF = "CUP-FLD"
*% FLD = "PTT-IIC-IPTT"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-CUP.
       VAL-NUM-FRM-VAL-NDTT.
*% BUF = "CUP-FLD"
*% FLD = "FRM-VAL-NDTT"
*% ENT = "3"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-CUP.
*% INCLUDE FIN_VAL_NUM_IKY_CUP OF 'COLBMCUP.TXT'
       FIN-VAL-NUM-IKY-CUP.
           EXIT.
       
       VAL-NUM-MKY-CUP SECTION.
       INI-VAL-NUM-MKY-CUP.
*% INCLUDE INI_VAL_NUM_MKY_CUP OF 'COLBMCUP.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_CUP OF 'COLBMCUP.TXT'
       FIN-VAL-NUM-MKY-CUP.
           EXIT.
       
       VAL-NUM-AKY-CUP SECTION.
       INI-VAL-NUM-AKY-CUP.
*% INCLUDE INI_VAL_NUM_AKY_CUP OF 'COLBMCUP.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_CUP OF 'COLBMCUP.TXT'
       FIN-VAL-NUM-AKY-CUP.
           EXIT.
       
       VAL-NUM-FLD-CUP SECTION.
       INI-VAL-NUM-FLD-CUP.
*% INCLUDE INI_VAL_NUM_FLD_CUP OF 'COLBMCUP.TXT'
       VAL-NUM-PTT-IIC-ICVN.
*% BUF = "CUP-FLD"
*% FLD = "PTT-IIC-ICVN"
*% ENT = "8"
*% DCM = "0"
