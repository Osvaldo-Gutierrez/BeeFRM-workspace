*% FRM = "DCP"
      *
      * COLBUDCP
      * Version 3.0.1 Nov-2005
      * 19-Dec-12 01:09 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMDCP.TXT'
       SET-TAG-COLBUDCP SECTION.
       INI-SET-TAG-COLBUDCP.
           MOVE
           '<IDNMOD NOM=$COLBUDCP$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$19-Dec-12 01:09 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUDCP.
           EXIT.
           
       RET-ALL-DCP SECTION.
       INI-RET-ALL-DCP.
*% INCLUDE INI_RET_ALL_DCP OF 'COLBMDCP.TXT'
           PERFORM KEY-ALL-DCP.
           PERFORM RET-OPC-DCP.
*% INCLUDE FIN_RET_ALL_DCP OF 'COLBMDCP.TXT'
       FIN-RET-ALL-DCP.
           EXIT.
       
       RET-OPC-DCP SECTION.
       INI-RET-OPC-DCP.
*% INCLUDE INI_RET_OPC_DCP OF 'COLBMDCP.TXT'
*% INCLUDE FIN_RET_OPC_DCP OF 'COLBMDCP.TXT'
       FIN-RET-OPC-DCP.
           EXIT.
       
       KEY-ALL-DCP SECTION.
       INI-KEY-ALL-DCP.
*% INCLUDE INI_KEY_ALL_DCP OF 'COLBMDCP.TXT'
           PERFORM KEY-OPC-DCP.
*% INCLUDE FIN_KEY_ALL_DCP OF 'COLBMDCP.TXT'
       FIN-KEY-ALL-DCP.
           EXIT.
       
       KEY-OPC-DCP SECTION.
       INI-KEY-OPC-DCP.
*% INCLUDE INI_KEY_OPC_DCP OF 'COLBMDCP.TXT'
           MOVE OPC-CAI-IOPC IN DCP-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE OPC-IIC-IOPC IN DCP-FLD TO OPC-IIC-IOPC IN OPC.
*% INCLUDE FIN_KEY_OPC_DCP OF 'COLBMDCP.TXT'
       FIN-KEY-OPC-DCP.
           EXIT.
       
       SCR-CLRA-DCP SECTION.
       INI-SCR-CLRA-DCP.
*% INCLUDE INI_SCR_CLRA_DCP OF 'COLBMDCP.TXT'
           PERFORM CLR-KEY-DCP.
           PERFORM CLR-FLD-DCP.
*% INCLUDE FIN_SCR_CLRA_DCP OF 'COLBMDCP.TXT'
       FIN-SCR-CLRA-DCP.
           EXIT.
       
       CLR-KEY-DCP SECTION.
       INI-CLR-KEY-DCP.
*% INCLUDE INI_CLR_KEY_DCP OF 'COLBMDCP.TXT'
           MOVE SPACES TO OPC-CAI-IOPC IN DCP-FLD.
           MOVE ZEROES TO OPC-IIC-IOPC IN DCP-FLD.
           MOVE ZEROES TO FRM-NUM-IVEN IN DCP-FLD.
           MOVE SPACES TO FRM-IND-HMAS IN DCP-FLD.
*% INCLUDE FIN_CLR_KEY_DCP OF 'COLBMDCP.TXT'
       FIN-CLR-KEY-DCP.
           EXIT.
       
       CLR-FLD-DCP SECTION.
       INI-CLR-FLD-DCP.
*% INCLUDE INI_CLR_FLD_DCP OF 'COLBMDCP.TXT'
           MOVE SPACES TO FRM-CAI-IOP2 IN DCP-FLD.
           MOVE ZEROES TO FRM-IIC-IOP2 IN DCP-FLD.
           MOVE ZEROES TO FRM-VAL-CRED IN DCP-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FCOL IN DCP-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-COD-COOC IN DCP-FLD.
           MOVE ZEROES TO FRM-SGV-INTR IN DCP-FLD.
           MOVE ZEROES TO FRM-SGV-SPAG IN DCP-FLD.
           MOVE ZEROES TO FRM-NUM-NCRE IN DCP-FLD(1).
           MOVE ZEROES TO FRM-NUM-NCRE IN DCP-FLD(2).
           MOVE SPACES TO FRM-IND-PAGO IN DCP-FLD(1).
           MOVE SPACES TO FRM-IND-PAGO IN DCP-FLD(2).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FVEN IN DCP-FLD(1).
           MOVE ZEROES TO FRM-FEC-FVEN IN DCP-FLD(2).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-SGV-IPTO IN DCP-FLD(1).
           MOVE ZEROES TO FRM-SGV-IPTO IN DCP-FLD(2).
           MOVE ZEROES TO FRM-SGV-COMI IN DCP-FLD(1).
           MOVE ZEROES TO FRM-SGV-COMI IN DCP-FLD(2).
           MOVE ZEROES TO FRM-SGV-INTE IN DCP-FLD(1).
           MOVE ZEROES TO FRM-SGV-INTE IN DCP-FLD(2).
           MOVE ZEROES TO FRM-SGV-CAPI IN DCP-FLD(1).
           MOVE ZEROES TO FRM-SGV-CAPI IN DCP-FLD(2).
           MOVE ZEROES TO FRM-SGV-TCUO IN DCP-FLD(1).
           MOVE ZEROES TO FRM-SGV-TCUO IN DCP-FLD(2).
           MOVE ZEROES TO FRM-SGV-SINS IN DCP-FLD(1).
           MOVE ZEROES TO FRM-SGV-SINS IN DCP-FLD(2).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FCAN IN DCP-FLD(1).
           MOVE ZEROES TO FRM-FEC-FCAN IN DCP-FLD(2).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-SGV-TINT IN DCP-FLD(1).
           MOVE ZEROES TO FRM-SGV-TINT IN DCP-FLD(2).
           MOVE SPACES TO FRM-IND-TCYA IN DCP-FLD(1).
           MOVE SPACES TO FRM-IND-TCYA IN DCP-FLD(2).
           MOVE ZEROES TO FRM-SGV-MORA IN DCP-FLD(1).
           MOVE ZEROES TO FRM-SGV-MORA IN DCP-FLD(2).
           MOVE ZEROES TO FRM-SGV-GCOB IN DCP-FLD(1).
           MOVE ZEROES TO FRM-SGV-GCOB IN DCP-FLD(2).
           MOVE ZEROES TO FRM-SGV-GJUD IN DCP-FLD(1).
           MOVE ZEROES TO FRM-SGV-GJUD IN DCP-FLD(2).
           MOVE ZEROES TO FRM-SGV-PREP IN DCP-FLD(1).
           MOVE ZEROES TO FRM-SGV-PREP IN DCP-FLD(2).
           MOVE SPACES TO FRM-IND-SITU IN DCP-FLD(1).
           MOVE SPACES TO FRM-IND-SITU IN DCP-FLD(2).
           MOVE SPACES TO FRM-CAI-IOPC IN DCP-FLD(1).
           MOVE SPACES TO FRM-CAI-IOPC IN DCP-FLD(2).
           MOVE ZEROES TO FRM-IIC-IOPC IN DCP-FLD(1).
           MOVE ZEROES TO FRM-IIC-IOPC IN DCP-FLD(2).
           MOVE ZEROES TO FRM-SGV-PCTA IN DCP-FLD(1).
           MOVE ZEROES TO FRM-SGV-PCTA IN DCP-FLD(2).
           PERFORM CLR-OPC-DCP.
*% INCLUDE FIN_CLR_FLD_DCP OF 'COLBMDCP.TXT'
       FIN-CLR-FLD-DCP.
           EXIT.
       
       CLR-OPC-DCP SECTION.
       INI-CLR-OPC-DCP.
*% INCLUDE INI_CLR_OPC_DCP OF 'COLBMDCP.TXT'
*% INCLUDE FIN_CLR_OPC_DCP OF 'COLBMDCP.TXT'
       FIN-CLR-OPC-DCP.
           EXIT.
       
       FST-KEY-DCP SECTION.
       INI-FST-KEY-DCP.
*% INCLUDE INI_FST_KEY_DCP OF 'COLBMDCP.TXT'
           MOVE -1 TO OPC-CAI-IOPC-LEN IN DCP-FLD.
*% INCLUDE FIN_FST_KEY_DCP OF 'COLBMDCP.TXT'
       FIN-FST-KEY-DCP.
           EXIT.
       
       FST-FLD-DCP SECTION.
       INI-FST-FLD-DCP.
*% INCLUDE INI_FST_FLD_DCP OF 'COLBMDCP.TXT'
*% INCLUDE FIN_FST_FLD_DCP OF 'COLBMDCP.TXT'
       FIN-FST-FLD-DCP.
           EXIT.
       
       PRO-KEY-DCP SECTION.
       INI-PRO-KEY-DCP.
*% INCLUDE INI_PRO_KEY_DCP OF 'COLBMDCP.TXT'
           MOVE FRM-CPIM-AEY TO FRM-NUM-IVEN-ATR IN DCP-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-HMAS-ATR IN DCP-FLD.
           PERFORM PRO-MKY-DCP.
*% INCLUDE FIN_PRO_KEY_DCP OF 'COLBMDCP.TXT'
       FIN-PRO-KEY-DCP.
           EXIT.
       
       PRO-MKY-DCP SECTION.
       INI-PRO-MKY-DCP.
*% INCLUDE INI_PRO_MKY_DCP OF 'COLBMDCP.TXT'
*% INCLUDE FIN_PRO_MKY_DCP OF 'COLBMDCP.TXT'
       FIN-PRO-MKY-DCP.
           EXIT.
       
       PRO-FLD-DCP SECTION.
       INI-PRO-FLD-DCP.
*% INCLUDE INI_PRO_FLD_DCP OF 'COLBMDCP.TXT'
           PERFORM PRO-OPC-DCP.
*% INCLUDE FIN_PRO_FLD_DCP OF 'COLBMDCP.TXT'
       FIN-PRO-FLD-DCP.
           EXIT.
       
       PRO-OPC-DCP SECTION.
       INI-PRO-OPC-DCP.
*% INCLUDE INI_PRO_OPC_DCP OF 'COLBMDCP.TXT'
*% INCLUDE FIN_PRO_OPC_DCP OF 'COLBMDCP.TXT'
       FIN-PRO-OPC-DCP.
           EXIT.
       
       UNP-KEY-DCP SECTION.
       INI-UNP-KEY-DCP.
*% INCLUDE INI_UNP_KEY_DCP OF 'COLBMDCP.TXT'
           MOVE FRM-CPIM-UBY TO FRM-NUM-IVEN-ATR IN DCP-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IND-HMAS-ATR IN DCP-FLD.
           PERFORM UNP-IKY-DCP.
           PERFORM UNP-AKY-DCP.
           PERFORM UNP-MKY-DCP.
*% INCLUDE FIN_UNP_KEY_DCP OF 'COLBMDCP.TXT'
       FIN-UNP-KEY-DCP.
           EXIT.
       
       UNP-IKY-DCP SECTION.
       INI-UNP-IKY-DCP.
*% INCLUDE INI_UNP_IKY_DCP OF 'COLBMDCP.TXT'
           MOVE FRM-CPIM-UBY TO OPC-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO OPC-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-NUM-IVEN-ATR.
           MOVE FRM-CPIM-UBY TO FRM-IND-HMAS-ATR.
*% INCLUDE FIN_UNP_IKY_DCP OF 'COLBMDCP.TXT'
       FIN-UNP-IKY-DCP.
           EXIT.
       
       UNP-MKY-DCP SECTION.
       INI-UNP-MKY-DCP.
*% INCLUDE INI_UNP_MKY_DCP OF 'COLBMDCP.TXT'
*% INCLUDE FIN_UNP_MKY_DCP OF 'COLBMDCP.TXT'
       FIN-UNP-MKY-DCP.
           EXIT.
       
       UNP-AKY-DCP SECTION.
       INI-UNP-AKY-DCP.
*% INCLUDE INI_UNP_AKY_DCP OF 'COLBMDCP.TXT'
*% INCLUDE FIN_UNP_AKY_DCP OF 'COLBMDCP.TXT'
       FIN-UNP-AKY-DCP.
           EXIT.
       
       UNP-FLD-DCP SECTION.
       INI-UNP-FLD-DCP.
*% INCLUDE INI_UNP_FLD_DCP OF 'COLBMDCP.TXT'
           PERFORM UNP-OPC-DCP.
*% INCLUDE FIN_UNP_FLD_DCP OF 'COLBMDCP.TXT'
       FIN-UNP-FLD-DCP.
           EXIT.
       
       UNP-OPC-DCP SECTION.
       INI-UNP-OPC-DCP.
*% INCLUDE INI_UNP_OPC_DCP OF 'COLBMDCP.TXT'
*% INCLUDE FIN_UNP_OPC_DCP OF 'COLBMDCP.TXT'
       FIN-UNP-OPC-DCP.
           EXIT.
       
       VAL-MKY-DCP SECTION.
       INI-VAL-MKY-DCP.
*% INCLUDE INI_VAL_MKY_DCP OF 'COLBMDCP.TXT'
*% INCLUDE FIN_VAL_MKY_DCP OF 'COLBMDCP.TXT'
       FIN-VAL-MKY-DCP.
           EXIT.
       
       VAL-FLD-DCP SECTION.
       INI-VAL-FLD-DCP.
*% INCLUDE INI_VAL_FLD_DCP OF 'COLBMDCP.TXT'
           PERFORM VAL-FRM-CAI-IOP2.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-IOP2-LEN IN DCP-FLD
                GO TO FIN-VAL-FLD-DCP.
           PERFORM VAL-FRM-IIC-IOP2.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-IOP2-LEN IN DCP-FLD
                GO TO FIN-VAL-FLD-DCP.
           PERFORM VAL-FRM-VAL-CRED.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-CRED-LEN IN DCP-FLD
                GO TO FIN-VAL-FLD-DCP.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FCOL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FCOL-LEN IN DCP-FLD
                GO TO FIN-VAL-FLD-DCP.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-COD-COOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-COOC-LEN IN DCP-FLD
                GO TO FIN-VAL-FLD-DCP.
           PERFORM VAL-FRM-SGV-INTR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-SGV-INTR-LEN IN DCP-FLD
                GO TO FIN-VAL-FLD-DCP.
           PERFORM VAL-FRM-SGV-SPAG.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-SGV-SPAG-LEN IN DCP-FLD
                GO TO FIN-VAL-FLD-DCP.
           PERFORM VAL-FRM-NUM-NCRE
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 2
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-NUM-NCRE-LEN IN DCP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DCP.
           PERFORM VAL-FRM-IND-PAGO
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 2
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-IND-PAGO-LEN IN DCP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DCP.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FVEN VARYING FRM-IFLD
                FROM 1 BY 1 UNTIL FRM-IFLD > 2 OR
                FRM-SUAR = FRM-SUAR-MAL OR
                FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FVEN-LEN IN DCP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DCP.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-SGV-IPTO
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 2
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-IPTO-LEN IN DCP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DCP.
           PERFORM VAL-FRM-SGV-COMI
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 2
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-COMI-LEN IN DCP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DCP.
           PERFORM VAL-FRM-SGV-INTE
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 2
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-INTE-LEN IN DCP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DCP.
           PERFORM VAL-FRM-SGV-CAPI
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 2
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-CAPI-LEN IN DCP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DCP.
           PERFORM VAL-FRM-SGV-TCUO
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 2
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-TCUO-LEN IN DCP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DCP.
           PERFORM VAL-FRM-SGV-SINS
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 2
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-SINS-LEN IN DCP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DCP.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAN"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FCAN VARYING FRM-IFLD
                FROM 1 BY 1 UNTIL FRM-IFLD > 2 OR
                FRM-SUAR = FRM-SUAR-MAL OR
                FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FCAN-LEN IN DCP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DCP.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-SGV-TINT
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 2
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-TINT-LEN IN DCP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DCP.
           PERFORM VAL-FRM-IND-TCYA
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 2
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-IND-TCYA-LEN IN DCP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DCP.
           PERFORM VAL-FRM-SGV-MORA
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 2
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-MORA-LEN IN DCP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DCP.
           PERFORM VAL-FRM-SGV-GCOB
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 2
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-GCOB-LEN IN DCP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DCP.
           PERFORM VAL-FRM-SGV-GJUD
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 2
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-GJUD-LEN IN DCP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DCP.
           PERFORM VAL-FRM-SGV-PREP
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 2
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-PREP-LEN IN DCP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DCP.
           PERFORM VAL-FRM-IND-SITU
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 2
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-IND-SITU-LEN IN DCP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DCP.
           PERFORM VAL-FRM-CAI-IOPC
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 2
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-CAI-IOPC-LEN IN DCP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DCP.
           PERFORM VAL-FRM-IIC-IOPC
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 2
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-IIC-IOPC-LEN IN DCP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DCP.
           PERFORM VAL-FRM-SGV-PCTA
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 2
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-PCTA-LEN IN DCP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DCP.
*% INCLUDE FIN_VAL_FLD_DCP OF 'COLBMDCP.TXT'
       FIN-VAL-FLD-DCP.
           EXIT.
       
       VAL-FRM-CAI-IOP2 SECTION.
       INI-VAL-FRM-CAI-IOP2.
*% LBL = "FRM_CAI_IOP2 OF 'COLBMDCP.TXT'"
