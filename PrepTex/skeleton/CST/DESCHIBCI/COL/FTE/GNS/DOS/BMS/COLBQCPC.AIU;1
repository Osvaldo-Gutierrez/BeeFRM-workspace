*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% OBL = "N"
*% LBL = "DBC_VRF_ICLI OF 'COLBMCPC.TXT'"
*% IZR = "N"
*% ZRO = "S"
*% KO0 = "K"
*% ZRO = "S"
*% LMX = "7"
*% FRM_IFLD = ""
      * VRF-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
      * VRF-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% OBL = "S"
*% LBL = "DBC_IDC_ICLI OF 'COLBMCPC.TXT'"
*% IZR = "S"
*% ZRO = "S"
*% KO0 = "K"
*% ZRO = "S"
*% LMX = "7"
*% FRM_IFLD = ""
      * IDC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION

       VAL-DBC-NUM-ICLI SECTION.
       INI-VAL-DBC-NUM-ICLI.
*% INCLUDE INI_DBC_NUM_ICLI  OF 'COLBMCPC.TXT'
*% INCLUDE DBC_NUM_ICLI  OF 'COLBMCPC.TXT'
*% INCLUDE FIN_DBC_NUM_ICLI  OF 'COLBMCPC.TXT'
       FIN-VAL-DBC-NUM-ICLI.
           EXIT.

       VAL-DBC-VRF-ICLI SECTION.
       INI-VAL-DBC-VRF-ICLI.
*% INCLUDE INI_DBC_VRF_ICLI  OF 'COLBMCPC.TXT'
           IF DBC-VRF-ICLI IN CPC-FLD NOT > SPACES
              MOVE SPACES TO DBC-VRF-ICLI IN CPC-FLD.
*% INCLUDE 'GNSSRC:SETVRF202.SRC'
*%! INCLUDE 'PROVRF'
*% INCLUDE DBC_VRF_ICLI  OF 'COLBMCPC.TXT'
*% INCLUDE FIN_DBC_VRF_ICLI  OF 'COLBMCPC.TXT'
*% LBL = ""
       FIN-VAL-DBC-VRF-ICLI.
           EXIT.
       VAL-DBC-IND-ICLI SECTION.
       INI-VAL-DBC-IND-ICLI.
*% INCLUDE INI_DBC_IND_ICLI  OF 'COLBMCPC.TXT'
           IF DBC-IND-ICLI IN CPC-FLD NOT > SPACES
              MOVE SPACES TO DBC-IND-ICLI IN CPC-FLD.
*% INCLUDE 'GNSSRC:VALIDC202.SRC'
*% INCLUDE DBC_IND_ICLI  OF 'COLBMCPC.TXT'
*% INCLUDE FIN_DBC_IND_ICLI  OF 'COLBMCPC.TXT'
       FIN-VAL-DBC-IND-ICLI.
           EXIT.

       VAL-DBC-GLS-ICLI SECTION.
       INI-VAL-DBC-GLS-ICLI.
*% LBL = "DBC_GLS_ICLI  OF 'COLBMCPC.TXT'"
*% INCLUDE INI_DBC_GLS_ICLI  OF 'COLBMCPC.TXT'
           IF DBC-GLS-ICLI IN CPC-FLD NOT > IDC-INIC-GLS 
              MOVE IDC-INIC-GLS TO DBC-GLS-ICLI IN CPC-FLD.
      *VAL-GLS para IDC valida glosa ( extension ) de identificador
           MOVE DBC-GLS-ICLI IN CPC-FLD TO IDC-GLS-RUTV.
           PERFORM VAL-GLS.
           MOVE IDC-GLS-RUTV TO DBC-GLS-ICLI IN CPC-FLD.
*% INCLUDE DBC_GLS_ICLI  OF 'COLBMCPC.TXT'
*% INCLUDE FIN_DBC_GLS_ICLI  OF 'COLBMCPC.TXT'
       FIN-VAL-DBC-GLS-ICLI.
           EXIT.

