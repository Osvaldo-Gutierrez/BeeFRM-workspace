       FST-KEY-POC SECTION.
       INI-FST-KEY-POC.
*% INCLUDE INI_FST_KEY_POC OF 'COLBMPOC.TXT'
           MOVE -1 TO POC-COD-VCOC-LEN IN POC-FLD.
*% INCLUDE FIN_FST_KEY_POC OF 'COLBMPOC.TXT'
       FIN-FST-KEY-POC.
           EXIT.
       
       FST-FLD-POC SECTION.
       INI-FST-FLD-POC.
*% INCLUDE INI_FST_FLD_POC OF 'COLBMPOC.TXT'
*% INCLUDE FIN_FST_FLD_POC OF 'COLBMPOC.TXT'
       FIN-FST-FLD-POC.
           EXIT.
       
       PRO-KEY-POC SECTION.
       INI-PRO-KEY-POC.
*% INCLUDE INI_PRO_KEY_POC OF 'COLBMPOC.TXT'
           PERFORM PRO-IKY-POC.
           PERFORM PRO-AKY-POC.
           PERFORM PRO-MKY-POC.
*% INCLUDE FIN_PRO_KEY_POC OF 'COLBMPOC.TXT'
       FIN-PRO-KEY-POC.
           EXIT.
       
       PRO-IKY-POC SECTION.
       INI-PRO-IKY-POC.
*% INCLUDE INI_PRO_IKY_POC OF 'COLBMPOC.TXT'
           MOVE FRM-CPIM-AEY TO POC-COD-VCOC-ATR IN POC-FLD.
           MOVE FRM-CPIM-AEY TO POC-COD-TOOC-ATR IN POC-FLD.
           MOVE FRM-CPIM-AEY TO POC-COD-AOOC-ATR IN POC-FLD.
           MOVE FRM-CPIM-AEY TO POC-NUM-IPOC-ATR IN POC-FLD.
*% INCLUDE FIN_PRO_IKY_POC OF 'COLBMPOC.TXT'
       FIN-PRO-IKY-POC.
           EXIT.
       
       PRO-MKY-POC SECTION.
       INI-PRO-MKY-POC.
*% INCLUDE INI_PRO_MKY_POC OF 'COLBMPOC.TXT'
*% INCLUDE FIN_PRO_MKY_POC OF 'COLBMPOC.TXT'
       FIN-PRO-MKY-POC.
           EXIT.
       
