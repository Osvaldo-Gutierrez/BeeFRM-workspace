*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "POC"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMPOC.TXT'
       SET-TAG-COLBQPOC SECTION.
       INI-SET-TAG-COLBQPOC.
           MOVE
           '<IDNMOD NOM=$COLBQPOC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:42 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQPOC.
           EXIT.
           
       PUT-ALL-POC SECTION.
       INI-PUT-ALL-POC.
*% INCLUDE INI_PUT_ALL_POC OF 'COLBMPOC.TXT'
           PERFORM PUT-POC-POC.
*% INCLUDE FIN_PUT_ALL_POC OF 'COLBMPOC.TXT'
       FIN-PUT-ALL-POC.
           EXIT.
       
       PUT-POC-POC SECTION.
       INI-PUT-POC-POC.
*% INCLUDE INI_PUT_POC_POC OF 'COLBMPOC.TXT'
           IF PGM-STAT-POC-NUL
               GO TO FIN-PUT-POC-POC.
           IF NOT PGM-STAT-POC-OKS
               PERFORM CLR-POC-POC
               GO TO FIN-PUT-POC-POC.
           MOVE POC-COD-VCOC IN POC TO POC-COD-VCOC IN POC-FLD.
           MOVE POC-COD-TOOC IN POC TO POC-COD-TOOC IN POC-FLD.
           MOVE POC-COD-AOOC IN POC TO POC-COD-AOOC IN POC-FLD.
           MOVE POC-NUM-IPOC IN POC TO POC-NUM-IPOC IN POC-FLD.
           MOVE POC-VAL-CMIN IN POC TO POC-VAL-CMIN IN POC-FLD.
           MOVE POC-NUM-PMIN IN POC TO POC-NUM-PMIN IN POC-FLD.
           MOVE POC-VAL-CMAX IN POC TO POC-VAL-CMAX IN POC-FLD.
           MOVE POC-IND-TICE IN POC TO POC-IND-TICE IN POC-FLD.
           MOVE POC-NUM-PMAX IN POC TO POC-NUM-PMAX IN POC-FLD.
           MOVE POC-IND-PMAX IN POC TO POC-IND-PMAX IN POC-FLD.
           MOVE POC-IND-TINE IN POC TO POC-IND-TINE IN POC-FLD.
           MOVE POC-IND-MTIC IN POC TO POC-IND-MTIC IN POC-FLD.
           MOVE POC-NUM-VMIN IN POC TO POC-NUM-VMIN IN POC-FLD.
           MOVE POC-MSC-BCTI IN POC TO POC-MSC-BCTI IN POC-FLD.
           MOVE POC-NUM-VMAX IN POC TO POC-NUM-VMAX IN POC-FLD.
           MOVE POC-MSC-PBTI IN POC TO POC-MSC-PBTI IN POC-FLD.
           MOVE POC-IND-RENO IN POC TO POC-IND-RENO IN POC-FLD.
           MOVE POC-MSC-BPBT IN POC TO POC-MSC-BPBT IN POC-FLD.
           MOVE POC-GLS-PCMI IN POC TO POC-GLS-PCMI IN POC-FLD.
           MOVE POC-GLS-PCMA IN POC TO POC-GLS-PCMA IN POC-FLD.
           MOVE POC-COD-CVTI IN POC TO POC-COD-CVTI IN POC-FLD.
           MOVE POC-IND-TCCE IN POC TO POC-IND-TCCE IN POC-FLD.
           MOVE POC-IND-TCNE IN POC TO POC-IND-TCNE IN POC-FLD.
           MOVE POC-MSC-SPBT IN POC TO POC-MSC-SPBT IN POC-FLD.
           MOVE POC-COD-SEGD IN POC TO POC-COD-SEGD IN POC-FLD.
           MOVE POC-NUM-PREP IN POC TO POC-NUM-PREP IN POC-FLD.
           MOVE POC-MSC-UTPR IN POC TO POC-MSC-UTPR IN POC-FLD.
           MOVE POC-IND-ENBO IN POC TO POC-IND-ENBO IN POC-FLD.
           MOVE POC-IND-AVAL IN POC TO POC-IND-AVAL IN POC-FLD.
           MOVE POC-COD-CDOC IN POC TO POC-COD-CDOC IN POC-FLD.
           MOVE POC-COD-NAPR IN POC TO POC-COD-NAPR IN POC-FLD.
           MOVE POC-MSC-TFPR IN POC TO POC-MSC-TFPR IN POC-FLD.
           MOVE POC-COD-TDOC IN POC TO POC-COD-TDOC IN POC-FLD.
           MOVE POC-MSC-STAT IN POC TO POC-MSC-STAT IN POC-FLD.
           MOVE POC-MSC-MIPL IN POC TO POC-MSC-MIPL IN POC-FLD.
           MOVE POC-IND-GARA IN POC TO POC-IND-GARA IN POC-FLD.
           MOVE POC-COD-NCUR IN POC TO POC-COD-NCUR IN POC-FLD.
           MOVE POC-MSC-TCVE IN POC TO POC-MSC-TCVE IN POC-FLD.
           MOVE POC-IND-AFER IN POC TO POC-IND-AFER IN POC-FLD.
           MOVE POC-IND-PMIN IN POC TO POC-IND-PMIN IN POC-FLD.
           MOVE POC-COD-TCOM IN POC TO POC-COD-TCOM IN POC-FLD.
           MOVE POC-COD-ACTI IN POC TO POC-COD-ACTI IN POC-FLD.
           MOVE POC-IND-CAUT IN POC TO POC-IND-CAUT IN POC-FLD.
           MOVE POC-COD-COTO IN POC TO POC-COD-COTO IN POC-FLD.
           MOVE POC-IND-TABO IN POC TO POC-IND-TABO IN POC-FLD.
           MOVE POC-NUM-MINC IN POC TO POC-NUM-MINC IN POC-FLD.
           MOVE POC-IND-MINC IN POC TO POC-IND-MINC IN POC-FLD.
           MOVE POC-NUM-MINI IN POC TO POC-NUM-MINI IN POC-FLD.
           MOVE POC-IND-MINI IN POC TO POC-IND-MINI IN POC-FLD.
           MOVE POC-NUM-MAXC IN POC TO POC-NUM-MAXC IN POC-FLD.
           MOVE POC-IND-MAXC IN POC TO POC-IND-MAXC IN POC-FLD.
           MOVE POC-NUM-MAXI IN POC TO POC-NUM-MAXI IN POC-FLD.
           MOVE POC-IND-MAXI IN POC TO POC-IND-MAXI IN POC-FLD.
           MOVE POC-IND-LIQI IN POC TO POC-IND-LIQI IN POC-FLD.
           MOVE POC-IND-ECDC IN POC TO POC-IND-ECDC IN POC-FLD.
           MOVE POC-COD-IPVC IN POC TO POC-COD-IPVC IN POC-FLD.
           MOVE POC-COD-TPAG IN POC TO POC-COD-TPAG IN POC-FLD.
           MOVE POC-MSC-MCVF IN POC TO POC-MSC-MCVF IN POC-FLD.
