*% INCLUDE INI_PRO_CUF_CUF OF 'COLBMCUF.TXT'
*% RREG = "CUF"
*% RREG_FRM = "CUF"
*% NOMB = "FCUF"
           MOVE FRM-CPIM-AEY TO CUF-FEC-FCUF-ATR IN CUF-FLD.
           MOVE FRM-CPIM-AEY TO CUF-COD-UBFD-ATR IN CUF-FLD.
*% INCLUDE FIN_PRO_CUF_CUF OF 'COLBMCUF.TXT'
       FIN-PRO-CUF-CUF.
           EXIT.
       
