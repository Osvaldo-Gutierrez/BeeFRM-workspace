*% FRM = "CUF"
      *
      * COLBUCUF
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:43 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMCUF.TXT'
       SET-TAG-COLBUCUF SECTION.
       INI-SET-TAG-COLBUCUF.
           MOVE
           '<IDNMOD NOM=$COLBUCUF$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:43 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCUF.
           EXIT.
           
       RET-ALL-CUF SECTION.
       INI-RET-ALL-CUF.
*% INCLUDE INI_RET_ALL_CUF OF 'COLBMCUF.TXT'
           PERFORM KEY-ALL-CUF.
           PERFORM RET-OPC-CUF.
           PERFORM RET-CUF-CUF.
           PERFORM RET-DLC-CUF.
*% INCLUDE FIN_RET_ALL_CUF OF 'COLBMCUF.TXT'
       FIN-RET-ALL-CUF.
           EXIT.
       
       RET-OPC-CUF SECTION.
       INI-RET-OPC-CUF.
*% INCLUDE INI_RET_OPC_CUF OF 'COLBMCUF.TXT'
           MOVE OPC-MSC-TVOC IN CUF-FLD TO OPC-MSC-TVOC IN OPC.
           MOVE OPC-COD-TOOC IN CUF-FLD TO OPC-COD-TOOC IN OPC.
           MOVE OPC-COD-OFOC IN CUF-FLD TO OPC-COD-OFOC IN OPC.
*% INCLUDE FIN_RET_OPC_CUF OF 'COLBMCUF.TXT'
       FIN-RET-OPC-CUF.
           EXIT.
       
       RET-CUF-CUF SECTION.
       INI-RET-CUF-CUF.
*% INCLUDE INI_RET_CUF_CUF OF 'COLBMCUF.TXT'
           MOVE CUF-COD-UFAD IN CUF-FLD TO CUF-COD-UFAD IN CUF.
*% RREG = "CUF"
*% RREG_FRM = "CUF"
*% NOMB = "FCUF"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CUF-NUM-DCUF IN CUF-FLD TO CUF-NUM-DCUF IN CUF.
           MOVE CUF-NUM-MCUF IN CUF-FLD TO CUF-NUM-MCUF IN CUF.
           MOVE CUF-NUM-SCUF IN CUF-FLD TO CUF-NUM-SCUF IN CUF.
           MOVE CUF-NUM-ACUF IN CUF-FLD TO CUF-NUM-ACUF IN CUF.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CUF-COD-UBFD IN CUF-FLD TO CUF-COD-UBFD IN CUF.
           MOVE CUF-COD-RCUF IN CUF-FLD TO CUF-COD-RCUF IN CUF.
*% INCLUDE FIN_RET_CUF_CUF OF 'COLBMCUF.TXT'
       FIN-RET-CUF-CUF.
           EXIT.
       
       RET-DLC-CUF SECTION.
       INI-RET-DLC-CUF.
*% INCLUDE INI_RET_DLC_CUF OF 'COLBMCUF.TXT'
           MOVE DLC-COD-UBFD IN CUF-FLD TO DLC-COD-UBFD IN DLC.
*% INCLUDE FIN_RET_DLC_CUF OF 'COLBMCUF.TXT'
       FIN-RET-DLC-CUF.
           EXIT.
       
       KEY-ALL-CUF SECTION.
       INI-KEY-ALL-CUF.
*% INCLUDE INI_KEY_ALL_CUF OF 'COLBMCUF.TXT'
           PERFORM KEY-OPC-CUF.
           PERFORM KEY-CUF-CUF.
           PERFORM KEY-DLC-CUF.
*% INCLUDE FIN_KEY_ALL_CUF OF 'COLBMCUF.TXT'
       FIN-KEY-ALL-CUF.
           EXIT.
       
       KEY-OPC-CUF SECTION.
       INI-KEY-OPC-CUF.
*% INCLUDE INI_KEY_OPC_CUF OF 'COLBMCUF.TXT'
*% INCLUDE FIN_KEY_OPC_CUF OF 'COLBMCUF.TXT'
       FIN-KEY-OPC-CUF.
           EXIT.
       
       KEY-CUF-CUF SECTION.
       INI-KEY-CUF-CUF.
*% INCLUDE INI_KEY_CUF_CUF OF 'COLBMCUF.TXT'
           MOVE CUF-CAI-IOPC IN CUF-FLD TO CUF-CAI-IOPC IN CUF.
           MOVE CUF-IIC-IOPC IN CUF-FLD TO CUF-IIC-IOPC IN CUF.
           MOVE CUF-NUM-IDLC IN CUF-FLD TO CUF-NUM-IDLC IN CUF.
           MOVE CUF-NUM-ICUF IN CUF-FLD TO CUF-NUM-ICUF IN CUF.
*% INCLUDE FIN_KEY_CUF_CUF OF 'COLBMCUF.TXT'
       FIN-KEY-CUF-CUF.
           EXIT.
       
       KEY-DLC-CUF SECTION.
       INI-KEY-DLC-CUF.
*% INCLUDE INI_KEY_DLC_CUF OF 'COLBMCUF.TXT'
*% INCLUDE FIN_KEY_DLC_CUF OF 'COLBMCUF.TXT'
       FIN-KEY-DLC-CUF.
           EXIT.
       
       SCR-CLRA-CUF SECTION.
       INI-SCR-CLRA-CUF.
*% INCLUDE INI_SCR_CLRA_CUF OF 'COLBMCUF.TXT'
           PERFORM CLR-KEY-CUF.
           PERFORM CLR-FLD-CUF.
*% INCLUDE FIN_SCR_CLRA_CUF OF 'COLBMCUF.TXT'
       FIN-SCR-CLRA-CUF.
           EXIT.
       
       CLR-KEY-CUF SECTION.
       INI-CLR-KEY-CUF.
*% INCLUDE INI_CLR_KEY_CUF OF 'COLBMCUF.TXT'
           MOVE SPACES TO CUF-CAI-IOPC IN CUF-FLD.
           MOVE ZEROES TO CUF-IIC-IOPC IN CUF-FLD.
           MOVE ZEROES TO CUF-NUM-IDLC IN CUF-FLD.
           MOVE ZEROES TO CUF-NUM-ICUF IN CUF-FLD.
           MOVE SPACES TO FRM-GLS-NOMC IN CUF-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-ICLI IN CUF-FLD.
           MOVE SPACES TO FRM-VRF-ICLI IN CUF-FLD.
           MOVE SPACES TO FRM-IND-ICLI IN CUF-FLD.
           MOVE SPACES TO FRM-GLS-ICLI IN CUF-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-CAI-ICLI IN CUF-FLD.
           MOVE SPACES TO FRM-IIC-ICLI IN CUF-FLD.
           MOVE SPACES TO FRM-GLS-NOMA IN CUF-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-IAVA IN CUF-FLD.
           MOVE SPACES TO FRM-VRF-IAVA IN CUF-FLD.
           MOVE SPACES TO FRM-IND-IAVA IN CUF-FLD.
           MOVE SPACES TO FRM-GLS-IAVA IN CUF-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-CAI-IAVA IN CUF-FLD.
           MOVE SPACES TO FRM-IIC-IAVA IN CUF-FLD.
*% INCLUDE FIN_CLR_KEY_CUF OF 'COLBMCUF.TXT'
       FIN-CLR-KEY-CUF.
           EXIT.
       
       CLR-FLD-CUF SECTION.
       INI-CLR-FLD-CUF.
*% INCLUDE INI_CLR_FLD_CUF OF 'COLBMCUF.TXT'
           MOVE SPACES TO FRM-GLS-TVOC IN CUF-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN CUF-FLD.
           MOVE SPACES TO FRM-GLS-OFOC IN CUF-FLD.
           MOVE SPACES TO FRM-GLS-UFAD IN CUF-FLD.
           MOVE SPACES TO FRM-GLS-UBFD IN CUF-FLD.
           MOVE SPACES TO FRM-GLS-RCUF IN CUF-FLD.
           PERFORM CLR-OPC-CUF.
           PERFORM CLR-CUF-CUF.
           PERFORM CLR-DLC-CUF.
*% INCLUDE FIN_CLR_FLD_CUF OF 'COLBMCUF.TXT'
       FIN-CLR-FLD-CUF.
           EXIT.
       
       CLR-OPC-CUF SECTION.
       INI-CLR-OPC-CUF.
*% INCLUDE INI_CLR_OPC_CUF OF 'COLBMCUF.TXT'
           MOVE SPACES TO OPC-MSC-TVOC IN CUF-FLD.
           MOVE SPACES TO OPC-COD-TOOC IN CUF-FLD.
           MOVE SPACES TO OPC-COD-OFOC IN CUF-FLD.
*% INCLUDE FIN_CLR_OPC_CUF OF 'COLBMCUF.TXT'
       FIN-CLR-OPC-CUF.
           EXIT.
       
       CLR-CUF-CUF SECTION.
       INI-CLR-CUF-CUF.
*% INCLUDE INI_CLR_CUF_CUF OF 'COLBMCUF.TXT'
           MOVE SPACES TO CUF-COD-UFAD IN CUF-FLD.
*% RREG = "CUF"
*% RREG_FRM = "CUF"
*% NOMB = "FCUF"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CUF-FEC-FCUF IN CUF-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO CUF-COD-UBFD IN CUF-FLD.
           MOVE SPACES TO CUF-COD-RCUF IN CUF-FLD.
*% INCLUDE FIN_CLR_CUF_CUF OF 'COLBMCUF.TXT'
       FIN-CLR-CUF-CUF.
           EXIT.
       
       CLR-DLC-CUF SECTION.
       INI-CLR-DLC-CUF.
*% INCLUDE INI_CLR_DLC_CUF OF 'COLBMCUF.TXT'
           MOVE SPACES TO DLC-COD-UBFD IN CUF-FLD.
*% INCLUDE FIN_CLR_DLC_CUF OF 'COLBMCUF.TXT'
       FIN-CLR-DLC-CUF.
           EXIT.
       
       FST-KEY-CUF SECTION.
       INI-FST-KEY-CUF.
*% INCLUDE INI_FST_KEY_CUF OF 'COLBMCUF.TXT'
           MOVE -1 TO CUF-CAI-IOPC-LEN IN CUF-FLD.
*% INCLUDE FIN_FST_KEY_CUF OF 'COLBMCUF.TXT'
       FIN-FST-KEY-CUF.
           EXIT.
       
       FST-FLD-CUF SECTION.
       INI-FST-FLD-CUF.
*% INCLUDE INI_FST_FLD_CUF OF 'COLBMCUF.TXT'
*% RREG = "CUF"
*% RREG_FRM = "CUF"
*% NOMB = "FCUF"
           MOVE FRM-POS-CURS TO CUF-FEC-FCUF-LEN IN CUF-FLD.
*% INCLUDE FIN_FST_FLD_CUF OF 'COLBMCUF.TXT'
       FIN-FST-FLD-CUF.
           EXIT.
       
       PRO-KEY-CUF SECTION.
       INI-PRO-KEY-CUF.
*% INCLUDE INI_PRO_KEY_CUF OF 'COLBMCUF.TXT'
           MOVE FRM-CPIM-AEY TO FRM-GLS-NOMC-ATR IN CUF-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-NUM-ICLI-ATR IN CUF-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VRF-ICLI-ATR IN CUF-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-ICLI-ATR IN CUF-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-ICLI-ATR IN CUF-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-GLS-NOMA-ATR IN CUF-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-NUM-IAVA-ATR IN CUF-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VRF-IAVA-ATR IN CUF-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-IAVA-ATR IN CUF-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-IAVA-ATR IN CUF-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           PERFORM PRO-MKY-CUF.
*% INCLUDE FIN_PRO_KEY_CUF OF 'COLBMCUF.TXT'
       FIN-PRO-KEY-CUF.
           EXIT.
       
       PRO-MKY-CUF SECTION.
       INI-PRO-MKY-CUF.
*% INCLUDE INI_PRO_MKY_CUF OF 'COLBMCUF.TXT'
