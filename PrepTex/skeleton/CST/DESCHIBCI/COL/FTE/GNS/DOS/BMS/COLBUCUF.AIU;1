       INI-UNP-CUF-CUF.
*% INCLUDE INI_UNP_CUF_CUF OF 'COLBMCUF.TXT'
*% RREG = "CUF"
*% RREG_FRM = "CUF"
*% NOMB = "FCUF"
           MOVE FRM-CPIM-UBY TO CUF-FEC-FCUF-ATR IN CUF-FLD.
           MOVE FRM-CPIM-UBY TO CUF-COD-UBFD-ATR IN CUF-FLD.
*% INCLUDE FIN_UNP_CUF_CUF OF 'COLBMCUF.TXT'
       FIN-UNP-CUF-CUF.
           EXIT.
       
       UNP-DLC-CUF SECTION.
       INI-UNP-DLC-CUF.
*% INCLUDE INI_UNP_DLC_CUF OF 'COLBMCUF.TXT'
*% INCLUDE FIN_UNP_DLC_CUF OF 'COLBMCUF.TXT'
       FIN-UNP-DLC-CUF.
           EXIT.
       
       VAL-MKY-CUF SECTION.
       INI-VAL-MKY-CUF.
*% INCLUDE INI_VAL_MKY_CUF OF 'COLBMCUF.TXT'
*% INCLUDE FIN_VAL_MKY_CUF OF 'COLBMCUF.TXT'
       FIN-VAL-MKY-CUF.
           EXIT.
       
       VAL-FLD-CUF SECTION.
       INI-VAL-FLD-CUF.
*% INCLUDE INI_VAL_FLD_CUF OF 'COLBMCUF.TXT'
           PERFORM VAL-OPC-MSC-TVOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-MSC-TVOC-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-OPC-COD-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-TOOC-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-OPC-COD-OFOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-OFOC-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-FRM-GLS-TVOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TVOC-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-FRM-GLS-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TOOC-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-FRM-GLS-OFOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-OFOC-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-FRM-CAI-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-ICLI-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-FRM-IIC-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-ICLI-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-FRM-CAI-IAVA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-IAVA-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-FRM-IIC-IAVA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-IAVA-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-DLC-COD-UBFD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DLC-COD-UBFD-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-CUF-COD-UFAD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CUF-COD-UFAD-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
*% RREG = "CUF"
*% RREG_FRM = "CUF"
*% NOMB = "FCUF"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-CUF-FEC-FCUF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                CUF-FEC-FCUF-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-GLS-UFAD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-UFAD-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-CUF-COD-UBFD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CUF-COD-UBFD-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-CUF-COD-RCUF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CUF-COD-RCUF-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-FRM-GLS-UBFD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-UBFD-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-FRM-GLS-RCUF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-RCUF-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
