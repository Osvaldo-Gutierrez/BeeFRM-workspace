      *
      * COLBQAUX
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 03:09 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "AUX"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMAUX.TXT'
       SET-TAG-COLBQAUX SECTION.
       INI-SET-TAG-COLBQAUX.
           MOVE
           '<IDNMOD NOM=$COLBQAUX$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 03:09 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQAUX.
           EXIT.
           
       PUT-ALL-AUX SECTION.
       INI-PUT-ALL-AUX.
*% INCLUDE INI_PUT_ALL_AUX OF 'COLBMAUX.TXT'
           PERFORM PUT-AUX-AUX.
*% INCLUDE FIN_PUT_ALL_AUX OF 'COLBMAUX.TXT'
       FIN-PUT-ALL-AUX.
           EXIT.
       
       PUT-AUX-AUX SECTION.
       INI-PUT-AUX-AUX.
*% INCLUDE INI_PUT_AUX_AUX OF 'COLBMAUX.TXT'
           IF PGM-STAT-AUX-NUL
               GO TO FIN-PUT-AUX-AUX.
           IF NOT PGM-STAT-AUX-OKS
               PERFORM CLR-AUX-AUX
               GO TO FIN-PUT-AUX-AUX.
           MOVE AUX-COD-TOCR IN AUX TO AUX-COD-TOCR IN AUX-FLD.
           MOVE AUX-COD-AOCR IN AUX TO AUX-COD-AOCR IN AUX-FLD.
           MOVE AUX-CIC-COCR IN AUX TO AUX-CIC-COCR IN AUX-FLD.
           MOVE AUX-EXT-CTAB IN AUX TO AUX-EXT-CTAB IN AUX-FLD.
           MOVE AUX-GLS-DESC IN AUX TO AUX-GLS-DESC IN AUX-FLD.
           MOVE AUX-GLS-ABRV IN AUX TO AUX-GLS-ABRV IN AUX-FLD.
           MOVE AUX-GLS-DCOR IN AUX TO AUX-GLS-DCOR IN AUX-FLD.
           MOVE AUX-IND-VIGE IN AUX TO AUX-IND-VIGE IN AUX-FLD.
*% RREG = "AUX"
*% RREG_FRM = "AUX"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE AUX-NUM-DTRN IN AUX TO AUX-NUM-DTRN IN AUX-FLD.
           MOVE AUX-NUM-MTRN IN AUX TO AUX-NUM-MTRN IN AUX-FLD.
           MOVE AUX-NUM-STRN IN AUX TO AUX-NUM-STRN IN AUX-FLD.
           MOVE AUX-NUM-ATRN IN AUX TO AUX-NUM-ATRN IN AUX-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE AUX-GLS-CCTE IN AUX TO AUX-GLS-CCTE IN AUX-FLD.
           MOVE AUX-IND-PCTB IN AUX TO AUX-IND-PCTB IN AUX-FLD.
           MOVE AUX-IND-PCDR IN AUX TO AUX-IND-PCDR IN AUX-FLD.
*% INCLUDE FIN_PUT_AUX_AUX OF 'COLBMAUX.TXT'
       FIN-PUT-AUX-AUX.
           EXIT.
       
       KEY-ALL-AUX SECTION.
       INI-KEY-ALL-AUX.
*% INCLUDE INI_KEY_ALL_AUX OF 'COLBMAUX.TXT'
           PERFORM KEY-AUX-AUX.
*% INCLUDE FIN_KEY_ALL_AUX OF 'COLBMAUX.TXT'
       FIN-KEY-ALL-AUX.
           EXIT.
       
       KEY-AUX-AUX SECTION.
       INI-KEY-AUX-AUX.
*% INCLUDE INI_KEY_AUX_AUX OF 'COLBMAUX.TXT'
           MOVE AUX-COD-TOCR IN AUX-FLD TO AUX-COD-TOCR IN AUX.
           MOVE AUX-COD-AOCR IN AUX-FLD TO AUX-COD-AOCR IN AUX.
*% INCLUDE FIN_KEY_AUX_AUX OF 'COLBMAUX.TXT'
       FIN-KEY-AUX-AUX.
           EXIT.
       
       SCR-CLRA-AUX SECTION.
       INI-SCR-CLRA-AUX.
*% INCLUDE INI_SCR_CLRA_AUX OF 'COLBMAUX.TXT'
           PERFORM CLR-KEY-AUX.
           PERFORM CLR-FLD-AUX.
*% INCLUDE FIN_SCR_CLRA_AUX OF 'COLBMAUX.TXT'
       FIN-SCR-CLRA-AUX.
           EXIT.
       
       CLR-KEY-AUX SECTION.
       INI-CLR-KEY-AUX.
*% INCLUDE INI_CLR_KEY_AUX OF 'COLBMAUX.TXT'
           MOVE SPACES TO AUX-COD-TOCR IN AUX-FLD.
           MOVE SPACES TO AUX-COD-AOCR IN AUX-FLD.
*% INCLUDE FIN_CLR_KEY_AUX OF 'COLBMAUX.TXT'
       FIN-CLR-KEY-AUX.
           EXIT.
       
       CLR-FLD-AUX SECTION.
       INI-CLR-FLD-AUX.
*% INCLUDE INI_CLR_FLD_AUX OF 'COLBMAUX.TXT'
           MOVE SPACES TO FRM-GLS-TOCR IN AUX-FLD.
           PERFORM CLR-AUX-AUX.
*% INCLUDE FIN_CLR_FLD_AUX OF 'COLBMAUX.TXT'
       FIN-CLR-FLD-AUX.
           EXIT.
       
       CLR-AUX-AUX SECTION.
       INI-CLR-AUX-AUX.
*% INCLUDE INI_CLR_AUX_AUX OF 'COLBMAUX.TXT'
           MOVE SPACES TO AUX-CIC-COCR IN AUX-FLD.
           MOVE SPACES TO AUX-EXT-CTAB IN AUX-FLD.
           MOVE SPACES TO AUX-GLS-DESC IN AUX-FLD.
           MOVE SPACES TO AUX-GLS-ABRV IN AUX-FLD.
           MOVE SPACES TO AUX-GLS-DCOR IN AUX-FLD.
           MOVE SPACES TO AUX-IND-VIGE IN AUX-FLD.
*% RREG = "AUX"
*% RREG_FRM = "AUX"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO AUX-FEC-FTRN IN AUX-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO AUX-GLS-CCTE IN AUX-FLD.
           MOVE SPACES TO AUX-IND-PCTB IN AUX-FLD.
           MOVE SPACES TO AUX-IND-PCDR IN AUX-FLD.
*% INCLUDE FIN_CLR_AUX_AUX OF 'COLBMAUX.TXT'
       FIN-CLR-AUX-AUX.
           EXIT.
       
       FST-KEY-AUX SECTION.
       INI-FST-KEY-AUX.
*% INCLUDE INI_FST_KEY_AUX OF 'COLBMAUX.TXT'
           MOVE -1 TO AUX-COD-TOCR-LEN IN AUX-FLD.
*% INCLUDE FIN_FST_KEY_AUX OF 'COLBMAUX.TXT'
       FIN-FST-KEY-AUX.
           EXIT.
       
       FST-FLD-AUX SECTION.
       INI-FST-FLD-AUX.
*% INCLUDE INI_FST_FLD_AUX OF 'COLBMAUX.TXT'
           MOVE -1 TO AUX-CIC-COCR-LEN IN AUX-FLD.
*% INCLUDE FIN_FST_FLD_AUX OF 'COLBMAUX.TXT'
       FIN-FST-FLD-AUX.
           EXIT.
       
       PRO-KEY-AUX SECTION.
       INI-PRO-KEY-AUX.
*% INCLUDE INI_PRO_KEY_AUX OF 'COLBMAUX.TXT'
           PERFORM PRO-IKY-AUX.
           PERFORM PRO-AKY-AUX.
           PERFORM PRO-MKY-AUX.
*% INCLUDE FIN_PRO_KEY_AUX OF 'COLBMAUX.TXT'
       FIN-PRO-KEY-AUX.
           EXIT.
       
       PRO-IKY-AUX SECTION.
       INI-PRO-IKY-AUX.
*% INCLUDE INI_PRO_IKY_AUX OF 'COLBMAUX.TXT'
           MOVE FRM-CPIM-AEY TO AUX-COD-TOCR-ATR IN AUX-FLD.
           MOVE FRM-CPIM-AEY TO AUX-COD-AOCR-ATR IN AUX-FLD.
*% INCLUDE FIN_PRO_IKY_AUX OF 'COLBMAUX.TXT'
       FIN-PRO-IKY-AUX.
           EXIT.
       
       PRO-MKY-AUX SECTION.
       INI-PRO-MKY-AUX.
*% INCLUDE INI_PRO_MKY_AUX OF 'COLBMAUX.TXT'
*% INCLUDE FIN_PRO_MKY_AUX OF 'COLBMAUX.TXT'
       FIN-PRO-MKY-AUX.
           EXIT.
       
       PRO-AKY-AUX SECTION.
       INI-PRO-AKY-AUX.
*% INCLUDE INI_PRO_AKY_AUX OF 'COLBMAUX.TXT'
*% INCLUDE FIN_PRO_AKY_AUX OF 'COLBMAUX.TXT'
       FIN-PRO-AKY-AUX.
           EXIT.
       
       UNP-KEY-AUX SECTION.
       INI-UNP-KEY-AUX.
*% INCLUDE INI_UNP_KEY_AUX OF 'COLBMAUX.TXT'
           PERFORM UNP-IKY-AUX.
           PERFORM UNP-AKY-AUX.
           PERFORM UNP-MKY-AUX.
*% INCLUDE FIN_UNP_KEY_AUX OF 'COLBMAUX.TXT'
       FIN-UNP-KEY-AUX.
           EXIT.
       
       UNP-IKY-AUX SECTION.
       INI-UNP-IKY-AUX.
*% INCLUDE INI_UNP_IKY_AUX OF 'COLBMAUX.TXT'
           MOVE FRM-CPIM-UBY TO AUX-COD-TOCR-ATR.
           MOVE FRM-CPIM-UBY TO AUX-COD-AOCR-ATR.
*% INCLUDE FIN_UNP_IKY_AUX OF 'COLBMAUX.TXT'
       FIN-UNP-IKY-AUX.
           EXIT.
       
       UNP-MKY-AUX SECTION.
       INI-UNP-MKY-AUX.
*% INCLUDE INI_UNP_MKY_AUX OF 'COLBMAUX.TXT'
*% INCLUDE FIN_UNP_MKY_AUX OF 'COLBMAUX.TXT'
       FIN-UNP-MKY-AUX.
           EXIT.
       
       UNP-AKY-AUX SECTION.
       INI-UNP-AKY-AUX.
*% INCLUDE INI_UNP_AKY_AUX OF 'COLBMAUX.TXT'
*% INCLUDE FIN_UNP_AKY_AUX OF 'COLBMAUX.TXT'
       FIN-UNP-AKY-AUX.
           EXIT.
       
       UNP-FLD-AUX SECTION.
       INI-UNP-FLD-AUX.
*% INCLUDE INI_UNP_FLD_AUX OF 'COLBMAUX.TXT'
           PERFORM UNP-AUX-AUX.
*% INCLUDE FIN_UNP_FLD_AUX OF 'COLBMAUX.TXT'
       FIN-UNP-FLD-AUX.
           EXIT.
       
       UNP-AUX-AUX SECTION.
       INI-UNP-AUX-AUX.
*% INCLUDE INI_UNP_AUX_AUX OF 'COLBMAUX.TXT'
           MOVE FRM-CPIM-UBY TO AUX-CIC-COCR-ATR IN AUX-FLD.
           MOVE FRM-CPIM-UBY TO AUX-EXT-CTAB-ATR IN AUX-FLD.
           MOVE FRM-CPIM-UBY TO AUX-GLS-DESC-ATR IN AUX-FLD.
           MOVE FRM-CPIM-UBY TO AUX-GLS-ABRV-ATR IN AUX-FLD.
           MOVE FRM-CPIM-UBY TO AUX-GLS-DCOR-ATR IN AUX-FLD.
           MOVE FRM-CPIM-UBY TO AUX-IND-VIGE-ATR IN AUX-FLD.
           MOVE FRM-CPIM-UBY TO AUX-GLS-CCTE-ATR IN AUX-FLD.
           MOVE FRM-CPIM-UBY TO AUX-IND-PCTB-ATR IN AUX-FLD.
           MOVE FRM-CPIM-UBY TO AUX-IND-PCDR-ATR IN AUX-FLD.
*% INCLUDE FIN_UNP_AUX_AUX OF 'COLBMAUX.TXT'
       FIN-UNP-AUX-AUX.
           EXIT.
       
       VAL-KEY-AUX SECTION.
       INI-VAL-KEY-AUX.
           PERFORM VAL-IKY-AUX.
           PERFORM VAL-MKY-AUX.
           PERFORM VAL-AKY-AUX.
       FIN-VAL-KEY-AUX.
           EXIT.
       
       VAL-IKY-AUX SECTION.
       INI-VAL-IKY-AUX.
*% INCLUDE INI_VAL_IKY_AUX OF 'COLBMAUX.TXT'
           PERFORM VAL-AUX-COD-TOCR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO AUX-COD-TOCR-LEN IN AUX-FLD
                GO TO FIN-VAL-IKY-AUX.
           PERFORM VAL-AUX-COD-AOCR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO AUX-COD-AOCR-LEN IN AUX-FLD
                GO TO FIN-VAL-IKY-AUX.
*% INCLUDE FIN_VAL_IKY_AUX OF 'COLBMAUX.TXT'
       FIN-VAL-IKY-AUX.
           EXIT.
       
       VAL-MKY-AUX SECTION.
       INI-VAL-MKY-AUX.
*% INCLUDE INI_VAL_MKY_AUX OF 'COLBMAUX.TXT'
*% INCLUDE FIN_VAL_MKY_AUX OF 'COLBMAUX.TXT'
       FIN-VAL-MKY-AUX.
           EXIT.
       
       VAL-AKY-AUX SECTION.
       INI-VAL-AKY-AUX.
*% INCLUDE INI_VAL_AKY_AUX OF 'COLBMAUX.TXT'
*% INCLUDE FIN_VAL_AKY_AUX OF 'COLBMAUX.TXT'
       FIN-VAL-AKY-AUX.
           EXIT.
       
       VAL-AUX-COD-TOCR SECTION.
       INI-VAL-AUX-COD-TOCR.
*% LBL = "AUX_COD_TOCR OF 'COLBMAUX.TXT'"
*% INCLUDE INI_AUX_COD_TOCR OF 'COLBMAUX.TXT'
           IF AUX-COD-TOCR IN AUX-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-AUX-COD-TOCR.
*% INCLUDE AUX_COD_TOCR OF 'COLBMAUX.TXT'
*% INCLUDE FIN_AUX_COD_TOCR OF 'COLBMAUX.TXT'
       FIN-VAL-AUX-COD-TOCR.
           EXIT.
       
       VAL-AUX-COD-AOCR SECTION.
       INI-VAL-AUX-COD-AOCR.
*% LBL = "AUX_COD_AOCR OF 'COLBMAUX.TXT'"
*% INCLUDE INI_AUX_COD_AOCR OF 'COLBMAUX.TXT'
           IF AUX-COD-AOCR IN AUX-FLD NOT > SPACES
              MOVE SPACES TO AUX-COD-AOCR IN AUX-FLD.
*% INCLUDE AUX_COD_AOCR OF 'COLBMAUX.TXT'
*% INCLUDE FIN_AUX_COD_AOCR OF 'COLBMAUX.TXT'
       FIN-VAL-AUX-COD-AOCR.
           EXIT.
*% RREG = "AUX"
*% RREG_FRM = "AUX"
*% NOMB = "FTRN"
*% IZR = "S"
       
       VAL-CON-KEY-AUX SECTION.
       INI-VAL-CON-KEY-AUX.
*% INCLUDE INI_VAL_CON_KEY_AUX OF 'COLBMAUX.TXT'
           IF     AUX-COD-TOCR IN AUX-FLD NOT > SPACES 
              AND AUX-COD-AOCR IN AUX-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-AUX.
*% INCLUDE FIN_VAL_CON_KEY_AUX OF 'COLBMAUX.TXT'
       FIN-VAL-CON-KEY-AUX.
           EXIT.
       
       VAL-CON-IKY-AUX SECTION.
       INI-VAL-CON-IKY-AUX.
*% INCLUDE INI_VAL_CON_IKY_AUX OF 'COLBMAUX.TXT'
           IF     AUX-COD-TOCR IN AUX-FLD NOT > SPACES 
              AND AUX-COD-AOCR IN AUX-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-AUX.
*% INCLUDE FIN_VAL_CON_IKY_AUX OF 'COLBMAUX.TXT'
       FIN-VAL-CON-IKY-AUX.
           EXIT.
       
       VAL-NUM-KEY-AUX SECTION.
       INI-VAL-NUM-KEY-AUX.
           PERFORM VAL-NUM-IKY-AUX.
           PERFORM VAL-NUM-MKY-AUX.
           PERFORM VAL-NUM-AKY-AUX.
       FIN-VAL-NUM-KEY-AUX.
           EXIT.
       
       VAL-NUM-IKY-AUX SECTION.
       INI-VAL-NUM-IKY-AUX.
*% INCLUDE INI_VAL_NUM_IKY_AUX OF 'COLBMAUX.TXT'
*% INCLUDE FIN_VAL_NUM_IKY_AUX OF 'COLBMAUX.TXT'
       FIN-VAL-NUM-IKY-AUX.
           EXIT.
       
       VAL-NUM-MKY-AUX SECTION.
       INI-VAL-NUM-MKY-AUX.
*% INCLUDE INI_VAL_NUM_MKY_AUX OF 'COLBMAUX.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_AUX OF 'COLBMAUX.TXT'
       FIN-VAL-NUM-MKY-AUX.
           EXIT.
       
       VAL-NUM-AKY-AUX SECTION.
       INI-VAL-NUM-AKY-AUX.
*% INCLUDE INI_VAL_NUM_AKY_AUX OF 'COLBMAUX.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_AUX OF 'COLBMAUX.TXT'
       FIN-VAL-NUM-AKY-AUX.
           EXIT.
       
       VAL-NUM-FLD-AUX SECTION.
       INI-VAL-NUM-FLD-AUX.
*% INCLUDE INI_VAL_NUM_FLD_AUX OF 'COLBMAUX.TXT'
*% INCLUDE FIN_VAL_NUM_FLD_AUX OF 'COLBMAUX.TXT'
       FIN-VAL-NUM-FLD-AUX.
           EXIT.
       
       EDT-KEY-AUX SECTION.
       INI-EDT-KEY-AUX.
           PERFORM EDT-IKY-AUX.
           PERFORM EDT-MKY-AUX.
           PERFORM EDT-AKY-AUX.
*% INCLUDE EDT_KEY_AUX OF 'COLBMAUX.TXT'
       FIN-EDT-KEY-AUX.
           EXIT.
       
       EDT-IKY-AUX SECTION.
       INI-EDT-IKY-AUX.
*% INCLUDE EDT_IKY_AUX OF 'COLBMAUX.TXT'
*% INCLUDE FIN_EDT_IKY_AUX OF 'COLBMAUX.TXT'
       FIN-EDT-IKY-AUX.
           EXIT.
       
       EDT-MKY-AUX SECTION.
       INI-EDT-MKY-AUX.
*% INCLUDE EDT_MKY_AUX OF 'COLBMAUX.TXT'
*% INCLUDE FIN_EDT_MKY_AUX OF 'COLBMAUX.TXT'
       FIN-EDT-MKY-AUX.
           EXIT.
       
       EDT-AKY-AUX SECTION.
       INI-EDT-AKY-AUX.
*% INCLUDE EDT_AKY_AUX OF 'COLBMAUX.TXT'
*% INCLUDE FIN_EDT_AKY_AUX OF 'COLBMAUX.TXT'
       FIN-EDT-AKY-AUX.
           EXIT.
       
       EDT-FLD-AUX SECTION.
       INI-EDT-FLD-AUX.
*% INCLUDE EDT_FLD_AUX OF 'COLBMAUX.TXT'
*% RREG = "AUX"
*% RREG_FRM = "AUX"
*% NOMB = "FTRN"
*% INCLUDE FIN_EDT_FLD_AUX OF 'COLBMAUX.TXT'
       FIN-EDT-FLD-AUX.
           EXIT.
