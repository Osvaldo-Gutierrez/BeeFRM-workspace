      *
      * COLBQCHI
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:21 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CHI"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMCHI.TXT'
       SET-TAG-COLBQCHI SECTION.
       INI-SET-TAG-COLBQCHI.
           MOVE
           '<IDNMOD NOM=$COLBQCHI$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:21 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCHI.
           EXIT.
           
       PUT-ALL-CHI SECTION.
       INI-PUT-ALL-CHI.
*% INCLUDE INI_PUT_ALL_CHI OF 'COLBMCHI.TXT'
           PERFORM PUT-OPC-CHI.
           PERFORM PUT-DBC-CHI.
*% INCLUDE FIN_PUT_ALL_CHI OF 'COLBMCHI.TXT'
       FIN-PUT-ALL-CHI.
           EXIT.
       
       PUT-OPC-CHI SECTION.
       INI-PUT-OPC-CHI.
*% INCLUDE INI_PUT_OPC_CHI OF 'COLBMCHI.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-CHI.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-CHI
               GO TO FIN-PUT-OPC-CHI.
           MOVE OPC-CAI-IOPC IN OPC TO OPC-CAI-IOPC IN CHI-FLD.
           MOVE OPC-IIC-IOPC IN OPC TO OPC-IIC-IOPC IN CHI-FLD.
*% INCLUDE FIN_PUT_OPC_CHI OF 'COLBMCHI.TXT'
       FIN-PUT-OPC-CHI.
           EXIT.
       
       PUT-DBC-CHI SECTION.
       INI-PUT-DBC-CHI.
*% INCLUDE INI_PUT_DBC_CHI OF 'COLBMCHI.TXT'
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-CHI.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-CHI
               GO TO FIN-PUT-DBC-CHI.
           MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN CHI-FLD.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN CHI-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN CHI-FLD.
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN CHI-FLD.
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN CHI-FLD.
      * IDC-FIN --> BMCHIBCI001210.SRCAIU 
*% INCLUDE FIN_PUT_DBC_CHI OF 'COLBMCHI.TXT'
       FIN-PUT-DBC-CHI.
           EXIT.
       
       KEY-ALL-CHI SECTION.
       INI-KEY-ALL-CHI.
*% INCLUDE INI_KEY_ALL_CHI OF 'COLBMCHI.TXT'
           PERFORM KEY-OPC-CHI.
           PERFORM KEY-DBC-CHI.
*% INCLUDE FIN_KEY_ALL_CHI OF 'COLBMCHI.TXT'
       FIN-KEY-ALL-CHI.
           EXIT.
       
       KEY-OPC-CHI SECTION.
       INI-KEY-OPC-CHI.
*% INCLUDE INI_KEY_OPC_CHI OF 'COLBMCHI.TXT'
           MOVE OPC-CAI-IOPC IN CHI-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE OPC-IIC-IOPC IN CHI-FLD TO OPC-IIC-IOPC IN OPC.
*% INCLUDE FIN_KEY_OPC_CHI OF 'COLBMCHI.TXT'
       FIN-KEY-OPC-CHI.
           EXIT.
       
       KEY-DBC-CHI SECTION.
       INI-KEY-DBC-CHI.
*% INCLUDE INI_KEY_DBC_CHI OF 'COLBMCHI.TXT'
*% INCLUDE FIN_KEY_DBC_CHI OF 'COLBMCHI.TXT'
       FIN-KEY-DBC-CHI.
           EXIT.
       
       SCR-CLRA-CHI SECTION.
       INI-SCR-CLRA-CHI.
*% INCLUDE INI_SCR_CLRA_CHI OF 'COLBMCHI.TXT'
           PERFORM CLR-KEY-CHI.
           PERFORM CLR-FLD-CHI.
*% INCLUDE FIN_SCR_CLRA_CHI OF 'COLBMCHI.TXT'
       FIN-SCR-CLRA-CHI.
           EXIT.
       
       CLR-KEY-CHI SECTION.
       INI-CLR-KEY-CHI.
*% INCLUDE INI_CLR_KEY_CHI OF 'COLBMCHI.TXT'
           MOVE SPACES TO OPC-CAI-IOPC IN CHI-FLD.
           MOVE ZEROES TO OPC-IIC-IOPC IN CHI-FLD.
           MOVE SPACES TO FRM-COD-TREN IN CHI-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FINI"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FINI IN CHI-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FTER"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FTER IN CHI-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_KEY_CHI OF 'COLBMCHI.TXT'
       FIN-CLR-KEY-CHI.
           EXIT.
       
       CLR-FLD-CHI SECTION.
       INI-CLR-FLD-CHI.
*% INCLUDE INI_CLR_FLD_CHI OF 'COLBMCHI.TXT'
           MOVE SPACES TO FRM-CAI-IOPC IN CHI-FLD(1).
           MOVE SPACES TO FRM-CAI-IOPC IN CHI-FLD(2).
           MOVE SPACES TO FRM-CAI-IOPC IN CHI-FLD(3).
           MOVE SPACES TO FRM-CAI-IOPC IN CHI-FLD(4).
           MOVE SPACES TO FRM-CAI-IOPC IN CHI-FLD(5).
           MOVE ZEROES TO FRM-IIC-IOPC IN CHI-FLD(1).
           MOVE ZEROES TO FRM-IIC-IOPC IN CHI-FLD(2).
           MOVE ZEROES TO FRM-IIC-IOPC IN CHI-FLD(3).
           MOVE ZEROES TO FRM-IIC-IOPC IN CHI-FLD(4).
           MOVE ZEROES TO FRM-IIC-IOPC IN CHI-FLD(5).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FCAN IN CHI-FLD(1).
           MOVE ZEROES TO FRM-FEC-FCAN IN CHI-FLD(2).
           MOVE ZEROES TO FRM-FEC-FCAN IN CHI-FLD(3).
           MOVE ZEROES TO FRM-FEC-FCAN IN CHI-FLD(4).
           MOVE ZEROES TO FRM-FEC-FCAN IN CHI-FLD(5).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-ICAN IN CHI-FLD(1).
           MOVE ZEROES TO FRM-NUM-ICAN IN CHI-FLD(2).
           MOVE ZEROES TO FRM-NUM-ICAN IN CHI-FLD(3).
           MOVE ZEROES TO FRM-NUM-ICAN IN CHI-FLD(4).
           MOVE ZEROES TO FRM-NUM-ICAN IN CHI-FLD(5).
           MOVE ZEROES TO FRM-NUM-IVEN IN CHI-FLD(1).
           MOVE ZEROES TO FRM-NUM-IVEN IN CHI-FLD(2).
           MOVE ZEROES TO FRM-NUM-IVEN IN CHI-FLD(3).
           MOVE ZEROES TO FRM-NUM-IVEN IN CHI-FLD(4).
           MOVE ZEROES TO FRM-NUM-IVEN IN CHI-FLD(5).
           MOVE ZEROES TO FRM-SGV-RENO IN CHI-FLD(1).
           MOVE ZEROES TO FRM-SGV-RENO IN CHI-FLD(2).
           MOVE ZEROES TO FRM-SGV-RENO IN CHI-FLD(3).
           MOVE ZEROES TO FRM-SGV-RENO IN CHI-FLD(4).
           MOVE ZEROES TO FRM-SGV-RENO IN CHI-FLD(5).
           MOVE ZEROES TO FRM-SGV-CAPI IN CHI-FLD(1).
           MOVE ZEROES TO FRM-SGV-CAPI IN CHI-FLD(2).
           MOVE ZEROES TO FRM-SGV-CAPI IN CHI-FLD(3).
           MOVE ZEROES TO FRM-SGV-CAPI IN CHI-FLD(4).
           MOVE ZEROES TO FRM-SGV-CAPI IN CHI-FLD(5).
           MOVE SPACES TO FRM-GLS-VCOC IN CHI-FLD(1).
           MOVE SPACES TO FRM-GLS-VCOC IN CHI-FLD(2).
           MOVE SPACES TO FRM-GLS-VCOC IN CHI-FLD(3).
           MOVE SPACES TO FRM-GLS-VCOC IN CHI-FLD(4).
           MOVE SPACES TO FRM-GLS-VCOC IN CHI-FLD(5).
           MOVE SPACES TO FRM-COD-COOC IN CHI-FLD(1).
           MOVE SPACES TO FRM-COD-COOC IN CHI-FLD(2).
           MOVE SPACES TO FRM-COD-COOC IN CHI-FLD(3).
           MOVE SPACES TO FRM-COD-COOC IN CHI-FLD(4).
           MOVE SPACES TO FRM-COD-COOC IN CHI-FLD(5).
           MOVE SPACES TO FRM-COD-OFIC IN CHI-FLD(1).
           MOVE SPACES TO FRM-COD-OFIC IN CHI-FLD(2).
           MOVE SPACES TO FRM-COD-OFIC IN CHI-FLD(3).
           MOVE SPACES TO FRM-COD-OFIC IN CHI-FLD(4).
           MOVE SPACES TO FRM-COD-OFIC IN CHI-FLD(5).
           MOVE ZEROES TO FRM-SGV-TINT IN CHI-FLD(1).
           MOVE ZEROES TO FRM-SGV-TINT IN CHI-FLD(2).
           MOVE ZEROES TO FRM-SGV-TINT IN CHI-FLD(3).
           MOVE ZEROES TO FRM-SGV-TINT IN CHI-FLD(4).
           MOVE ZEROES TO FRM-SGV-TINT IN CHI-FLD(5).
           MOVE ZEROES TO FRM-NUM-VCOR IN CHI-FLD(1).
           MOVE ZEROES TO FRM-NUM-VCOR IN CHI-FLD(2).
           MOVE ZEROES TO FRM-NUM-VCOR IN CHI-FLD(3).
           MOVE ZEROES TO FRM-NUM-VCOR IN CHI-FLD(4).
           MOVE ZEROES TO FRM-NUM-VCOR IN CHI-FLD(5).
           MOVE SPACES TO FRM-IND-RENO IN CHI-FLD(1).
           MOVE SPACES TO FRM-IND-RENO IN CHI-FLD(2).
           MOVE SPACES TO FRM-IND-RENO IN CHI-FLD(3).
           MOVE SPACES TO FRM-IND-RENO IN CHI-FLD(4).
           MOVE SPACES TO FRM-IND-RENO IN CHI-FLD(5).
           MOVE SPACES TO FRM-COD-RCAN IN CHI-FLD(1).
           MOVE SPACES TO FRM-COD-RCAN IN CHI-FLD(2).
           MOVE SPACES TO FRM-COD-RCAN IN CHI-FLD(3).
           MOVE SPACES TO FRM-COD-RCAN IN CHI-FLD(4).
           MOVE SPACES TO FRM-COD-RCAN IN CHI-FLD(5).
           MOVE ZEROES TO FRM-SGV-TPAG IN CHI-FLD(1).
           MOVE ZEROES TO FRM-SGV-TPAG IN CHI-FLD(2).
           MOVE ZEROES TO FRM-SGV-TPAG IN CHI-FLD(3).
           MOVE ZEROES TO FRM-SGV-TPAG IN CHI-FLD(4).
           MOVE ZEROES TO FRM-SGV-TPAG IN CHI-FLD(5).
           PERFORM CLR-OPC-CHI.
           PERFORM CLR-DBC-CHI.
*% INCLUDE FIN_CLR_FLD_CHI OF 'COLBMCHI.TXT'
       FIN-CLR-FLD-CHI.
           EXIT.
       
       CLR-OPC-CHI SECTION.
       INI-CLR-OPC-CHI.
*% INCLUDE INI_CLR_OPC_CHI OF 'COLBMCHI.TXT'
*% INCLUDE FIN_CLR_OPC_CHI OF 'COLBMCHI.TXT'
       FIN-CLR-OPC-CHI.
           EXIT.
       
       CLR-DBC-CHI SECTION.
       INI-CLR-DBC-CHI.
*% INCLUDE INI_CLR_DBC_CHI OF 'COLBMCHI.TXT'
           MOVE SPACES TO DBC-GLS-NOMC IN CHI-FLD.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN CHI-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN CHI-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN CHI-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN CHI-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_DBC_CHI OF 'COLBMCHI.TXT'
       FIN-CLR-DBC-CHI.
           EXIT.
       
       FST-KEY-CHI SECTION.
       INI-FST-KEY-CHI.
*% INCLUDE INI_FST_KEY_CHI OF 'COLBMCHI.TXT'
           MOVE -1 TO OPC-CAI-IOPC-LEN IN CHI-FLD.
*% INCLUDE FIN_FST_KEY_CHI OF 'COLBMCHI.TXT'
       FIN-FST-KEY-CHI.
           EXIT.
       
       FST-FLD-CHI SECTION.
       INI-FST-FLD-CHI.
*% INCLUDE INI_FST_FLD_CHI OF 'COLBMCHI.TXT'
*% INCLUDE FIN_FST_FLD_CHI OF 'COLBMCHI.TXT'
       FIN-FST-FLD-CHI.
           EXIT.
       
       PRO-KEY-CHI SECTION.
       INI-PRO-KEY-CHI.
*% INCLUDE INI_PRO_KEY_CHI OF 'COLBMCHI.TXT'
           MOVE FRM-CPIM-AEY TO FRM-COD-TREN-ATR IN CHI-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FINI"
           MOVE FRM-CPIM-AEY TO FRM-FEC-FINI-ATR IN CHI-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FTER"
           MOVE FRM-CPIM-AEY TO FRM-FEC-FTER-ATR IN CHI-FLD.
           PERFORM PRO-IKY-CHI.
           PERFORM PRO-AKY-CHI.
           PERFORM PRO-MKY-CHI.
*% INCLUDE FIN_PRO_KEY_CHI OF 'COLBMCHI.TXT'
       FIN-PRO-KEY-CHI.
           EXIT.
       
       PRO-IKY-CHI SECTION.
       INI-PRO-IKY-CHI.
*% INCLUDE INI_PRO_IKY_CHI OF 'COLBMCHI.TXT'
           MOVE FRM-CPIM-AEY TO OPC-CAI-IOPC-ATR IN CHI-FLD.
           MOVE FRM-CPIM-AEY TO OPC-IIC-IOPC-ATR IN CHI-FLD.
*% INCLUDE FIN_PRO_IKY_CHI OF 'COLBMCHI.TXT'
       FIN-PRO-IKY-CHI.
           EXIT.
       
       PRO-MKY-CHI SECTION.
       INI-PRO-MKY-CHI.
*% INCLUDE INI_PRO_MKY_CHI OF 'COLBMCHI.TXT'
*% INCLUDE FIN_PRO_MKY_CHI OF 'COLBMCHI.TXT'
       FIN-PRO-MKY-CHI.
           EXIT.
       
       PRO-AKY-CHI SECTION.
       INI-PRO-AKY-CHI.
*% INCLUDE INI_PRO_AKY_CHI OF 'COLBMCHI.TXT'
           MOVE FRM-CPIM-AEY TO FRM-COD-TREN-ATR IN CHI-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FINI"
           MOVE FRM-CPIM-AEY TO FRM-FEC-FINI-ATR IN CHI-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FTER"
           MOVE FRM-CPIM-AEY TO FRM-FEC-FTER-ATR IN CHI-FLD.
*% INCLUDE FIN_PRO_AKY_CHI OF 'COLBMCHI.TXT'
       FIN-PRO-AKY-CHI.
           EXIT.
       
       UNP-KEY-CHI SECTION.
       INI-UNP-KEY-CHI.
*% INCLUDE INI_UNP_KEY_CHI OF 'COLBMCHI.TXT'
           MOVE FRM-CPIM-UBY TO FRM-COD-TREN-ATR IN CHI-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FINI"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FINI-ATR IN CHI-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FTER"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FTER-ATR IN CHI-FLD.
           PERFORM UNP-IKY-CHI.
           PERFORM UNP-AKY-CHI.
           PERFORM UNP-MKY-CHI.
*% INCLUDE FIN_UNP_KEY_CHI OF 'COLBMCHI.TXT'
       FIN-UNP-KEY-CHI.
           EXIT.
       
       UNP-IKY-CHI SECTION.
       INI-UNP-IKY-CHI.
*% INCLUDE INI_UNP_IKY_CHI OF 'COLBMCHI.TXT'
           MOVE FRM-CPIM-UBY TO OPC-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO OPC-IIC-IOPC-ATR.
*% INCLUDE FIN_UNP_IKY_CHI OF 'COLBMCHI.TXT'
       FIN-UNP-IKY-CHI.
           EXIT.
       
       UNP-MKY-CHI SECTION.
       INI-UNP-MKY-CHI.
*% INCLUDE INI_UNP_MKY_CHI OF 'COLBMCHI.TXT'
*% INCLUDE FIN_UNP_MKY_CHI OF 'COLBMCHI.TXT'
       FIN-UNP-MKY-CHI.
           EXIT.
       
       UNP-AKY-CHI SECTION.
       INI-UNP-AKY-CHI.
*% INCLUDE INI_UNP_AKY_CHI OF 'COLBMCHI.TXT'
           MOVE FRM-CPIM-UBY TO FRM-COD-TREN-ATR.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FINI"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FINI-ATR IN CHI-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FTER"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FTER-ATR IN CHI-FLD.
*% INCLUDE FIN_UNP_AKY_CHI OF 'COLBMCHI.TXT'
       FIN-UNP-AKY-CHI.
           EXIT.
       
       UNP-FLD-CHI SECTION.
       INI-UNP-FLD-CHI.
*% INCLUDE INI_UNP_FLD_CHI OF 'COLBMCHI.TXT'
           PERFORM UNP-OPC-CHI.
           PERFORM UNP-DBC-CHI.
*% INCLUDE FIN_UNP_FLD_CHI OF 'COLBMCHI.TXT'
       FIN-UNP-FLD-CHI.
           EXIT.
       
       UNP-OPC-CHI SECTION.
       INI-UNP-OPC-CHI.
*% INCLUDE INI_UNP_OPC_CHI OF 'COLBMCHI.TXT'
*% INCLUDE FIN_UNP_OPC_CHI OF 'COLBMCHI.TXT'
       FIN-UNP-OPC-CHI.
           EXIT.
       
       UNP-DBC-CHI SECTION.
       INI-UNP-DBC-CHI.
*% INCLUDE INI_UNP_DBC_CHI OF 'COLBMCHI.TXT'
*% INCLUDE FIN_UNP_DBC_CHI OF 'COLBMCHI.TXT'
       FIN-UNP-DBC-CHI.
           EXIT.
       
       VAL-KEY-CHI SECTION.
       INI-VAL-KEY-CHI.
           PERFORM VAL-IKY-CHI.
           PERFORM VAL-MKY-CHI.
           PERFORM VAL-AKY-CHI.
       FIN-VAL-KEY-CHI.
           EXIT.
       
       VAL-IKY-CHI SECTION.
       INI-VAL-IKY-CHI.
*% INCLUDE INI_VAL_IKY_CHI OF 'COLBMCHI.TXT'
           PERFORM VAL-OPC-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-CAI-IOPC-LEN IN CHI-FLD
                GO TO FIN-VAL-IKY-CHI.
           PERFORM VAL-OPC-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-IIC-IOPC-LEN IN CHI-FLD
                GO TO FIN-VAL-IKY-CHI.
*% INCLUDE FIN_VAL_IKY_CHI OF 'COLBMCHI.TXT'
       FIN-VAL-IKY-CHI.
           EXIT.
       
       VAL-MKY-CHI SECTION.
       INI-VAL-MKY-CHI.
*% INCLUDE INI_VAL_MKY_CHI OF 'COLBMCHI.TXT'
*% INCLUDE FIN_VAL_MKY_CHI OF 'COLBMCHI.TXT'
       FIN-VAL-MKY-CHI.
           EXIT.
       
       VAL-AKY-CHI SECTION.
       INI-VAL-AKY-CHI.
*% INCLUDE INI_VAL_AKY_CHI OF 'COLBMCHI.TXT'
           PERFORM VAL-FRM-COD-TREN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-TREN-LEN IN CHI-FLD
                GO TO FIN-VAL-AKY-CHI.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FINI"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FINI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FINI-LEN IN CHI-FLD
                GO TO FIN-VAL-AKY-CHI.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FTER"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FTER.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FTER-LEN IN CHI-FLD
                GO TO FIN-VAL-AKY-CHI.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% INCLUDE FIN_VAL_AKY_CHI OF 'COLBMCHI.TXT'
       FIN-VAL-AKY-CHI.
           EXIT.
       
       VAL-OPC-CAI-IOPC SECTION.
       INI-VAL-OPC-CAI-IOPC.
*% LBL = "OPC_CAI_IOPC OF 'COLBMCHI.TXT'"
*% INCLUDE INI_OPC_CAI_IOPC OF 'COLBMCHI.TXT'
           IF OPC-CAI-IOPC IN CHI-FLD NOT > SPACES
              MOVE SPACES TO OPC-CAI-IOPC IN CHI-FLD.
*% INCLUDE OPC_CAI_IOPC OF 'COLBMCHI.TXT'
*% INCLUDE FIN_OPC_CAI_IOPC OF 'COLBMCHI.TXT'
       FIN-VAL-OPC-CAI-IOPC.
           EXIT.
       
       VAL-OPC-IIC-IOPC SECTION.
       INI-VAL-OPC-IIC-IOPC.
*% LBL = "OPC_IIC_IOPC OF 'COLBMCHI.TXT'"
*% INCLUDE INI_OPC_IIC_IOPC OF 'COLBMCHI.TXT'
*% INCLUDE OPC_IIC_IOPC OF 'COLBMCHI.TXT'
*% INCLUDE FIN_OPC_IIC_IOPC OF 'COLBMCHI.TXT'
       FIN-VAL-OPC-IIC-IOPC.
           EXIT.
       
       VAL-FRM-COD-TREN SECTION.
       INI-VAL-FRM-COD-TREN.
*% LBL = "FRM_COD_TREN OF 'COLBMCHI.TXT'"
*% INCLUDE INI_FRM_COD_TREN OF 'COLBMCHI.TXT'
           IF FRM-COD-TREN IN CHI-FLD NOT > SPACES
              MOVE SPACES TO FRM-COD-TREN IN CHI-FLD.
*% INCLUDE FRM_COD_TREN OF 'COLBMCHI.TXT'
*% INCLUDE FIN_FRM_COD_TREN OF 'COLBMCHI.TXT'
       FIN-VAL-FRM-COD-TREN.
           EXIT.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% IZR = "S"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% IZR = "S"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FINI"
*% OBL = "S"
*% LBL = "FRM_FEC_FINI OF 'COLBMCHI.TXT'"
*% IZR = "S"
*% ZRO = "S"
*% BUF = "CHI-FLD"
*% FLD = "FRM-FEC-FINI"
*% SN  = "S"
*% HOY = "LE"
*% HBL = "S"
*% TIPO = "FEC"
*% FRM_IFLD = ""
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-FRM-FEC-FINI SECTION.
       INI-VAL-FRM-FEC-FINI.
           IF FRM-FEC-FINI IN CHI-FLD  NOT > ZEROES
              MOVE ZEROES TO FRM-FEC-FINI IN CHI-FLD.
*% INCLUDE 'SETFEC'
*% INCLUDE INI_FRM_FEC_FINI OF 'COLBMCHI.TXT'
*% INCLUDE 'PROFEC'
*% INCLUDE FRM_FEC_FINI OF 'COLBMCHI.TXT'
*% INCLUDE FIN_FRM_FEC_FINI OF 'COLBMCHI.TXT'
       FIN-VAL-FRM-FEC-FINI.
           EXIT.
      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
*% RREG = "FRM"
*% RREG_FRM = "FRM"
