*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "QGP"
*% PGM_BQ = TRUE
       PUT-ALL-QGP SECTION.
       INI-PUT-ALL-QGP.
*% INCLUDE INI_PUT_ALL_QGP OF 'COLBMQGP.TXT'
           PERFORM PUT-POC-QGP.
*% INCLUDE FIN_PUT_ALL_QGP OF 'COLBMQGP.TXT'
       FIN-PUT-ALL-QGP.
           EXIT.
       
       PUT-POC-QGP SECTION.
       INI-PUT-POC-QGP.
*% INCLUDE INI_PUT_POC_QGP OF 'COLBMQGP.TXT'
           IF PGM-STAT-POC-NUL
               GO TO FIN-PUT-POC-QGP.
           IF NOT PGM-STAT-POC-OKS
               PERFORM CLR-POC-QGP
               GO TO FIN-PUT-POC-QGP.
           MOVE POC-COD-VCOC IN POC TO POC-COD-VCOC IN QGP-FLD.
           MOVE POC-COD-TOOC IN POC TO POC-COD-TOOC IN QGP-FLD.
           MOVE POC-COD-AOOC IN POC TO POC-COD-AOOC IN QGP-FLD.
           MOVE POC-NUM-IPOC IN POC TO POC-NUM-IPOC IN QGP-FLD.
*% BUF = "QGP-FLD"
*% SIS = "TAB"
*% TAB = "VLR "
*% FLD = "POC-COD-VCOC"
*% GLS = "FRM-GLS-VCOC"
*% TIENE_GLOSA = TRUE
*% LGL = 15
*% INCLUDE 'BUSTAB'
*% BUF = "QGP-FLD"
*% SIS = "TAB"
*% TAB = "TIO "
*% FLD = "POC-COD-TOOC"
*% GLS = "FRM-GLS-TOOC"
*% TIENE_GLOSA = TRUE
*% LGL = 18
*% INCLUDE 'BUSTAB'
*% INCLUDE FIN_PUT_POC_QGP OF 'COLBMQGP.TXT'
       FIN-PUT-POC-QGP.
           EXIT.
       
       KEY-ALL-QGP SECTION.
       INI-KEY-ALL-QGP.
*% INCLUDE INI_KEY_ALL_QGP OF 'COLBMQGP.TXT'
           PERFORM KEY-POC-QGP.
*% INCLUDE FIN_KEY_ALL_QGP OF 'COLBMQGP.TXT'
       FIN-KEY-ALL-QGP.
           EXIT.
       
       KEY-POC-QGP SECTION.
       INI-KEY-POC-QGP.
*% INCLUDE INI_KEY_POC_QGP OF 'COLBMQGP.TXT'
           MOVE POC-COD-VCOC IN QGP-FLD TO POC-COD-VCOC IN POC.
           MOVE POC-COD-TOOC IN QGP-FLD TO POC-COD-TOOC IN POC.
           MOVE POC-COD-AOOC IN QGP-FLD TO POC-COD-AOOC IN POC.
*% INCLUDE FIN_KEY_POC_QGP OF 'COLBMQGP.TXT'
       FIN-KEY-POC-QGP.
           EXIT.
       
       SCR-CLRA-QGP SECTION.
       INI-SCR-CLRA-QGP.
*% INCLUDE INI_SCR_CLRA_QGP OF 'COLBMQGP.TXT'
           PERFORM CLR-KEY-QGP.
           PERFORM CLR-FLD-QGP.
*% INCLUDE FIN_SCR_CLRA_QGP OF 'COLBMQGP.TXT'
       FIN-SCR-CLRA-QGP.
           EXIT.
       
       CLR-KEY-QGP SECTION.
       INI-CLR-KEY-QGP.
*% INCLUDE INI_CLR_KEY_QGP OF 'COLBMQGP.TXT'
           MOVE SPACES TO POC-COD-VCOC IN QGP-FLD.
           MOVE SPACES TO POC-COD-TOOC IN QGP-FLD.
           MOVE SPACES TO POC-COD-AOOC IN QGP-FLD.
*% INCLUDE FIN_CLR_KEY_QGP OF 'COLBMQGP.TXT'
       FIN-CLR-KEY-QGP.
           EXIT.
       
       CLR-FLD-QGP SECTION.
       INI-CLR-FLD-QGP.
*% INCLUDE INI_CLR_FLD_QGP OF 'COLBMQGP.TXT'
           MOVE SPACES TO FRM-GLS-VCOC IN QGP-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN QGP-FLD.
           MOVE SPACES TO FRM-GLS-AOOC IN QGP-FLD.
           MOVE SPACES TO FRM-IND-TACC IN QGP-FLD(1).
           MOVE SPACES TO FRM-IND-TACC IN QGP-FLD(2).
           MOVE SPACES TO FRM-IND-TACC IN QGP-FLD(3).
           MOVE SPACES TO FRM-IND-TACC IN QGP-FLD(4).
           MOVE SPACES TO FRM-IND-TACC IN QGP-FLD(5).
           MOVE SPACES TO FRM-IND-TACC IN QGP-FLD(6).
           MOVE SPACES TO FRM-IND-TACC IN QGP-FLD(7).
           MOVE SPACES TO FRM-IND-TACC IN QGP-FLD(8).
           MOVE SPACES TO FRM-IND-TACC IN QGP-FLD(9).
           MOVE SPACES TO FRM-IND-TACC IN QGP-FLD(10).
           MOVE SPACES TO FRM-IND-TACC IN QGP-FLD(11).
           MOVE SPACES TO FRM-IND-TACC IN QGP-FLD(12).
           MOVE SPACES TO FRM-IND-TACC IN QGP-FLD(13).
           MOVE SPACES TO FRM-IND-TACC IN QGP-FLD(14).
           MOVE SPACES TO FRM-IND-TACC IN QGP-FLD(15).
           MOVE SPACES TO FRM-COD-VCAM IN QGP-FLD(1).
           MOVE SPACES TO FRM-COD-VCAM IN QGP-FLD(2).
           MOVE SPACES TO FRM-COD-VCAM IN QGP-FLD(3).
           MOVE SPACES TO FRM-COD-VCAM IN QGP-FLD(4).
           MOVE SPACES TO FRM-COD-VCAM IN QGP-FLD(5).
           MOVE SPACES TO FRM-COD-VCAM IN QGP-FLD(6).
           MOVE SPACES TO FRM-COD-VCAM IN QGP-FLD(7).
           MOVE SPACES TO FRM-COD-VCAM IN QGP-FLD(8).
           MOVE SPACES TO FRM-COD-VCAM IN QGP-FLD(9).
           MOVE SPACES TO FRM-COD-VCAM IN QGP-FLD(10).
           MOVE SPACES TO FRM-COD-VCAM IN QGP-FLD(11).
           MOVE SPACES TO FRM-COD-VCAM IN QGP-FLD(12).
           MOVE SPACES TO FRM-COD-VCAM IN QGP-FLD(13).
           MOVE SPACES TO FRM-COD-VCAM IN QGP-FLD(14).
           MOVE SPACES TO FRM-COD-VCAM IN QGP-FLD(15).
           MOVE SPACES TO FRM-GLS-VCAM IN QGP-FLD(1).
           MOVE SPACES TO FRM-GLS-VCAM IN QGP-FLD(2).
           MOVE SPACES TO FRM-GLS-VCAM IN QGP-FLD(3).
           MOVE SPACES TO FRM-GLS-VCAM IN QGP-FLD(4).
           MOVE SPACES TO FRM-GLS-VCAM IN QGP-FLD(5).
           MOVE SPACES TO FRM-GLS-VCAM IN QGP-FLD(6).
           MOVE SPACES TO FRM-GLS-VCAM IN QGP-FLD(7).
           MOVE SPACES TO FRM-GLS-VCAM IN QGP-FLD(8).
           MOVE SPACES TO FRM-GLS-VCAM IN QGP-FLD(9).
           MOVE SPACES TO FRM-GLS-VCAM IN QGP-FLD(10).
           MOVE SPACES TO FRM-GLS-VCAM IN QGP-FLD(11).
           MOVE SPACES TO FRM-GLS-VCAM IN QGP-FLD(12).
           MOVE SPACES TO FRM-GLS-VCAM IN QGP-FLD(13).
