           EXIT.
       
       VAL-POC-COD-AOOC SECTION.
       INI-VAL-POC-COD-AOOC.
*% LBL = "POC_COD_AOOC OF 'COLBMQGP.TXT'"
*% INCLUDE INI_POC_COD_AOOC OF 'COLBMQGP.TXT'
           IF POC-COD-AOOC IN QGP-FLD NOT > SPACES
              MOVE SPACES TO POC-COD-AOOC IN QGP-FLD.
*% INCLUDE POC_COD_AOOC OF 'COLBMQGP.TXT'
*% INCLUDE FIN_POC_COD_AOOC OF 'COLBMQGP.TXT'
       FIN-VAL-POC-COD-AOOC.
           EXIT.
       
       VAL-CON-KEY-QGP SECTION.
       INI-VAL-CON-KEY-QGP.
*% INCLUDE INI_VAL_CON_KEY_QGP OF 'COLBMQGP.TXT'
           IF     POC-COD-VCOC IN QGP-FLD NOT > SPACES 
              AND POC-COD-TOOC IN QGP-FLD NOT > SPACES 
              AND POC-COD-AOOC IN QGP-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-QGP.
*% INCLUDE FIN_VAL_CON_KEY_QGP OF 'COLBMQGP.TXT'
       FIN-VAL-CON-KEY-QGP.
           EXIT.
       
       VAL-CON-IKY-QGP SECTION.
       INI-VAL-CON-IKY-QGP.
*% INCLUDE INI_VAL_CON_IKY_QGP OF 'COLBMQGP.TXT'
           IF     POC-COD-VCOC IN QGP-FLD NOT > SPACES 
              AND POC-COD-TOOC IN QGP-FLD NOT > SPACES 
              AND POC-COD-AOOC IN QGP-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-QGP.
*% INCLUDE FIN_VAL_CON_IKY_QGP OF 'COLBMQGP.TXT'
       FIN-VAL-CON-IKY-QGP.
           EXIT.
       
       VAL-NUM-KEY-QGP SECTION.
       INI-VAL-NUM-KEY-QGP.
           PERFORM VAL-NUM-IKY-QGP.
           PERFORM VAL-NUM-MKY-QGP.
           PERFORM VAL-NUM-AKY-QGP.
       FIN-VAL-NUM-KEY-QGP.
           EXIT.
       
       VAL-NUM-IKY-QGP SECTION.
       INI-VAL-NUM-IKY-QGP.
*% INCLUDE INI_VAL_NUM_IKY_QGP OF 'COLBMQGP.TXT'
*% INCLUDE FIN_VAL_NUM_IKY_QGP OF 'COLBMQGP.TXT'
       FIN-VAL-NUM-IKY-QGP.
           EXIT.
       
       VAL-NUM-MKY-QGP SECTION.
       INI-VAL-NUM-MKY-QGP.
*% INCLUDE INI_VAL_NUM_MKY_QGP OF 'COLBMQGP.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_QGP OF 'COLBMQGP.TXT'
       FIN-VAL-NUM-MKY-QGP.
           EXIT.
       
       VAL-NUM-AKY-QGP SECTION.
       INI-VAL-NUM-AKY-QGP.
*% INCLUDE INI_VAL_NUM_AKY_QGP OF 'COLBMQGP.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_QGP OF 'COLBMQGP.TXT'
       FIN-VAL-NUM-AKY-QGP.
           EXIT.
       
       VAL-NUM-FLD-QGP SECTION.
       INI-VAL-NUM-FLD-QGP.
*% INCLUDE INI_VAL_NUM_FLD_QGP OF 'COLBMQGP.TXT'
       VAL-NUM-POC-NUM-IPOC.
*% BUF = "QGP-FLD"
*% FLD = "POC-NUM-IPOC"
*% ENT = "1"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-QGP.
*% INCLUDE FIN_VAL_NUM_FLD_QGP OF 'COLBMQGP.TXT'
       FIN-VAL-NUM-FLD-QGP.
           EXIT.
       
       EDT-KEY-QGP SECTION.
       INI-EDT-KEY-QGP.
           PERFORM EDT-IKY-QGP.
           PERFORM EDT-MKY-QGP.
           PERFORM EDT-AKY-QGP.
*% INCLUDE EDT_KEY_QGP OF 'COLBMQGP.TXT'
       FIN-EDT-KEY-QGP.
           EXIT.
       
       EDT-IKY-QGP SECTION.
       INI-EDT-IKY-QGP.
*% INCLUDE EDT_IKY_QGP OF 'COLBMQGP.TXT'
*% INCLUDE FIN_EDT_IKY_QGP OF 'COLBMQGP.TXT'
       FIN-EDT-IKY-QGP.
           EXIT.
       
       EDT-MKY-QGP SECTION.
       INI-EDT-MKY-QGP.
*% INCLUDE EDT_MKY_QGP OF 'COLBMQGP.TXT'
*% INCLUDE FIN_EDT_MKY_QGP OF 'COLBMQGP.TXT'
       FIN-EDT-MKY-QGP.
           EXIT.
       
       EDT-AKY-QGP SECTION.
       INI-EDT-AKY-QGP.
*% INCLUDE EDT_AKY_QGP OF 'COLBMQGP.TXT'
*% INCLUDE FIN_EDT_AKY_QGP OF 'COLBMQGP.TXT'
       FIN-EDT-AKY-QGP.
           EXIT.
       
       EDT-FLD-QGP SECTION.
       INI-EDT-FLD-QGP.
*% INCLUDE EDT_FLD_QGP OF 'COLBMQGP.TXT'
           IF POC-NUM-IPOC-LEN IN QGP-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO POC-NUM-IPOC-LEN IN QGP-FLD
                GO TO FIN-EDT-FLD-QGP.
           MOVE POC-NUM-IPOC IN QGP-FLD TO POC-NUM-IPOC-EDT IN QGP-EDT.
           MOVE POC-NUM-IPOC-EDT IN QGP-EDT TO 
                POC-NUM-IPOC-ALF IN QGP-FLD.
*% INCLUDE FIN_EDT_FLD_QGP OF 'COLBMQGP.TXT'
       FIN-EDT-FLD-QGP.
           EXIT.
*% INCLUDE EOF_BQ OF 'COLBMQGP.TXT'
*% INCLUDE EOF OF 'COLBMQGP.TXT'
*% INCLUDE EOF_IBM OF 'COLBMQGP.TXT'
