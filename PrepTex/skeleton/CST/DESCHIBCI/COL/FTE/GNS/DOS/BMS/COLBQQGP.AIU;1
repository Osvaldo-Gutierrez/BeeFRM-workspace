           MOVE SPACES TO FRM-GLS-CAUX IN QGP-FLD(9).
           MOVE SPACES TO FRM-GLS-CAUX IN QGP-FLD(10).
           MOVE SPACES TO FRM-GLS-CAUX IN QGP-FLD(11).
           MOVE SPACES TO FRM-GLS-CAUX IN QGP-FLD(12).
           MOVE SPACES TO FRM-GLS-CAUX IN QGP-FLD(13).
           MOVE SPACES TO FRM-GLS-CAUX IN QGP-FLD(14).
           MOVE SPACES TO FRM-GLS-CAUX IN QGP-FLD(15).
           PERFORM CLR-POC-QGP.
*% INCLUDE FIN_CLR_FLD_QGP OF 'COLBMQGP.TXT'
       FIN-CLR-FLD-QGP.
           EXIT.
       
       CLR-POC-QGP SECTION.
       INI-CLR-POC-QGP.
*% INCLUDE INI_CLR_POC_QGP OF 'COLBMQGP.TXT'
           MOVE ZEROES TO POC-NUM-IPOC IN QGP-FLD.
*% INCLUDE FIN_CLR_POC_QGP OF 'COLBMQGP.TXT'
       FIN-CLR-POC-QGP.
           EXIT.
       
       FST-KEY-QGP SECTION.
       INI-FST-KEY-QGP.
*% INCLUDE INI_FST_KEY_QGP OF 'COLBMQGP.TXT'
           MOVE -1 TO POC-COD-VCOC-LEN IN QGP-FLD.
*% INCLUDE FIN_FST_KEY_QGP OF 'COLBMQGP.TXT'
       FIN-FST-KEY-QGP.
           EXIT.
       
       FST-FLD-QGP SECTION.
       INI-FST-FLD-QGP.
*% INCLUDE INI_FST_FLD_QGP OF 'COLBMQGP.TXT'
           MOVE -1 TO FRM-IND-TACC-LEN IN QGP-FLD(1).
*% INCLUDE FIN_FST_FLD_QGP OF 'COLBMQGP.TXT'
       FIN-FST-FLD-QGP.
           EXIT.
       
       PRO-KEY-QGP SECTION.
       INI-PRO-KEY-QGP.
*% INCLUDE INI_PRO_KEY_QGP OF 'COLBMQGP.TXT'
           PERFORM PRO-IKY-QGP.
           PERFORM PRO-AKY-QGP.
           PERFORM PRO-MKY-QGP.
*% INCLUDE FIN_PRO_KEY_QGP OF 'COLBMQGP.TXT'
       FIN-PRO-KEY-QGP.
           EXIT.
       
       PRO-IKY-QGP SECTION.
       INI-PRO-IKY-QGP.
*% INCLUDE INI_PRO_IKY_QGP OF 'COLBMQGP.TXT'
           MOVE FRM-CPIM-AEY TO POC-COD-VCOC-ATR IN QGP-FLD.
           MOVE FRM-CPIM-AEY TO POC-COD-TOOC-ATR IN QGP-FLD.
           MOVE FRM-CPIM-AEY TO POC-COD-AOOC-ATR IN QGP-FLD.
*% INCLUDE FIN_PRO_IKY_QGP OF 'COLBMQGP.TXT'
       FIN-PRO-IKY-QGP.
           EXIT.
       
       PRO-MKY-QGP SECTION.
       INI-PRO-MKY-QGP.
*% INCLUDE INI_PRO_MKY_QGP OF 'COLBMQGP.TXT'
*% INCLUDE FIN_PRO_MKY_QGP OF 'COLBMQGP.TXT'
       FIN-PRO-MKY-QGP.
           EXIT.
       
       PRO-AKY-QGP SECTION.
       INI-PRO-AKY-QGP.
*% INCLUDE INI_PRO_AKY_QGP OF 'COLBMQGP.TXT'
*% INCLUDE FIN_PRO_AKY_QGP OF 'COLBMQGP.TXT'
       FIN-PRO-AKY-QGP.
           EXIT.
       
       UNP-KEY-QGP SECTION.
       INI-UNP-KEY-QGP.
*% INCLUDE INI_UNP_KEY_QGP OF 'COLBMQGP.TXT'
           PERFORM UNP-IKY-QGP.
           PERFORM UNP-AKY-QGP.
           PERFORM UNP-MKY-QGP.
*% INCLUDE FIN_UNP_KEY_QGP OF 'COLBMQGP.TXT'
       FIN-UNP-KEY-QGP.
           EXIT.
       
       UNP-IKY-QGP SECTION.
       INI-UNP-IKY-QGP.
*% INCLUDE INI_UNP_IKY_QGP OF 'COLBMQGP.TXT'
           MOVE FRM-CPIM-UBY TO POC-COD-VCOC-ATR.
           MOVE FRM-CPIM-UBY TO POC-COD-TOOC-ATR.
           MOVE FRM-CPIM-UBY TO POC-COD-AOOC-ATR.
*% INCLUDE FIN_UNP_IKY_QGP OF 'COLBMQGP.TXT'
       FIN-UNP-IKY-QGP.
           EXIT.
       
       UNP-MKY-QGP SECTION.
       INI-UNP-MKY-QGP.
*% INCLUDE INI_UNP_MKY_QGP OF 'COLBMQGP.TXT'
*% INCLUDE FIN_UNP_MKY_QGP OF 'COLBMQGP.TXT'
       FIN-UNP-MKY-QGP.
           EXIT.
       
       UNP-AKY-QGP SECTION.
       INI-UNP-AKY-QGP.
*% INCLUDE INI_UNP_AKY_QGP OF 'COLBMQGP.TXT'
*% INCLUDE FIN_UNP_AKY_QGP OF 'COLBMQGP.TXT'
       FIN-UNP-AKY-QGP.
           EXIT.
       
       UNP-FLD-QGP SECTION.
       INI-UNP-FLD-QGP.
*% INCLUDE INI_UNP_FLD_QGP OF 'COLBMQGP.TXT'
           MOVE FRM-CPIM-UBY TO FRM-IND-TACC-ATR IN QGP-FLD(1).
           MOVE FRM-CPIM-UBY TO FRM-IND-TACC-ATR IN QGP-FLD(2).
           MOVE FRM-CPIM-UBY TO FRM-IND-TACC-ATR IN QGP-FLD(3).
           MOVE FRM-CPIM-UBY TO FRM-IND-TACC-ATR IN QGP-FLD(4).
           MOVE FRM-CPIM-UBY TO FRM-IND-TACC-ATR IN QGP-FLD(5).
           MOVE FRM-CPIM-UBY TO FRM-IND-TACC-ATR IN QGP-FLD(6).
           MOVE FRM-CPIM-UBY TO FRM-IND-TACC-ATR IN QGP-FLD(7).
           MOVE FRM-CPIM-UBY TO FRM-IND-TACC-ATR IN QGP-FLD(8).
           MOVE FRM-CPIM-UBY TO FRM-IND-TACC-ATR IN QGP-FLD(9).
           MOVE FRM-CPIM-UBY TO FRM-IND-TACC-ATR IN QGP-FLD(10).
           MOVE FRM-CPIM-UBY TO FRM-IND-TACC-ATR IN QGP-FLD(11).
           MOVE FRM-CPIM-UBY TO FRM-IND-TACC-ATR IN QGP-FLD(12).
           MOVE FRM-CPIM-UBY TO FRM-IND-TACC-ATR IN QGP-FLD(13).
           MOVE FRM-CPIM-UBY TO FRM-IND-TACC-ATR IN QGP-FLD(14).
           MOVE FRM-CPIM-UBY TO FRM-IND-TACC-ATR IN QGP-FLD(15).
           PERFORM UNP-POC-QGP.
*% INCLUDE FIN_UNP_FLD_QGP OF 'COLBMQGP.TXT'
       FIN-UNP-FLD-QGP.
           EXIT.
       
       UNP-POC-QGP SECTION.
       INI-UNP-POC-QGP.
*% INCLUDE INI_UNP_POC_QGP OF 'COLBMQGP.TXT'
*% INCLUDE FIN_UNP_POC_QGP OF 'COLBMQGP.TXT'
       FIN-UNP-POC-QGP.
           EXIT.
       
       VAL-KEY-QGP SECTION.
       INI-VAL-KEY-QGP.
           PERFORM VAL-IKY-QGP.
           PERFORM VAL-MKY-QGP.
           PERFORM VAL-AKY-QGP.
       FIN-VAL-KEY-QGP.
           EXIT.
       
       VAL-IKY-QGP SECTION.
       INI-VAL-IKY-QGP.
*% INCLUDE INI_VAL_IKY_QGP OF 'COLBMQGP.TXT'
           PERFORM VAL-POC-COD-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO POC-COD-VCOC-LEN IN QGP-FLD
                GO TO FIN-VAL-IKY-QGP.
           PERFORM VAL-POC-COD-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO POC-COD-TOOC-LEN IN QGP-FLD
                GO TO FIN-VAL-IKY-QGP.
           PERFORM VAL-POC-COD-AOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO POC-COD-AOOC-LEN IN QGP-FLD
                GO TO FIN-VAL-IKY-QGP.
*% INCLUDE FIN_VAL_IKY_QGP OF 'COLBMQGP.TXT'
       FIN-VAL-IKY-QGP.
           EXIT.
       
       VAL-MKY-QGP SECTION.
       INI-VAL-MKY-QGP.
*% INCLUDE INI_VAL_MKY_QGP OF 'COLBMQGP.TXT'
*% INCLUDE FIN_VAL_MKY_QGP OF 'COLBMQGP.TXT'
       FIN-VAL-MKY-QGP.
           EXIT.
       
       VAL-AKY-QGP SECTION.
       INI-VAL-AKY-QGP.
*% INCLUDE INI_VAL_AKY_QGP OF 'COLBMQGP.TXT'
*% INCLUDE FIN_VAL_AKY_QGP OF 'COLBMQGP.TXT'
       FIN-VAL-AKY-QGP.
           EXIT.
       
       VAL-POC-COD-VCOC SECTION.
       INI-VAL-POC-COD-VCOC.
*% LBL = "POC_COD_VCOC OF 'COLBMQGP.TXT'"
*% INCLUDE INI_POC_COD_VCOC OF 'COLBMQGP.TXT'
           IF POC-COD-VCOC IN QGP-FLD NOT > SPACES
              MOVE SPACES TO POC-COD-VCOC IN QGP-FLD.
*% INCLUDE POC_COD_VCOC OF 'COLBMQGP.TXT'
*% BUF = "QGP-FLD"
*% SIS = "TAB"
*% TAB = "VLR "
*% FLD = "POC-COD-VCOC"
*% GLS = "FRM-GLS-VCOC"
*% TIENE_GLOSA = TRUE
*% LGL = 15
*% INCLUDE 'VALCOD'
*% INCLUDE FIN_POC_COD_VCOC OF 'COLBMQGP.TXT'
       FIN-VAL-POC-COD-VCOC.
           EXIT.
       
       VAL-POC-COD-TOOC SECTION.
       INI-VAL-POC-COD-TOOC.
*% LBL = "POC_COD_TOOC OF 'COLBMQGP.TXT'"
*% INCLUDE INI_POC_COD_TOOC OF 'COLBMQGP.TXT'
           IF POC-COD-TOOC IN QGP-FLD NOT > SPACES
              MOVE SPACES TO POC-COD-TOOC IN QGP-FLD.
*% INCLUDE POC_COD_TOOC OF 'COLBMQGP.TXT'
*% BUF = "QGP-FLD"
*% SIS = "TAB"
*% TAB = "TIO "
*% FLD = "POC-COD-TOOC"
*% GLS = "FRM-GLS-TOOC"
*% TIENE_GLOSA = TRUE
*% LGL = 18
