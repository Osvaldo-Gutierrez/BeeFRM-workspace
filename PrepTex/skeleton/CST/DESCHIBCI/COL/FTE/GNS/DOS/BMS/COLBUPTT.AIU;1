*% FRM = "PTT"
      *
      * COLBUPTT
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:46 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMPTT.TXT'
       SET-TAG-COLBUPTT SECTION.
       INI-SET-TAG-COLBUPTT.
           MOVE
           '<IDNMOD NOM=$COLBUPTT$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:46 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUPTT.
           EXIT.
           
       RET-ALL-PTT SECTION.
       INI-RET-ALL-PTT.
*% INCLUDE INI_RET_ALL_PTT OF 'COLBMPTT.TXT'
           PERFORM KEY-ALL-PTT.
           PERFORM RET-PTT-PTT.
*% INCLUDE FIN_RET_ALL_PTT OF 'COLBMPTT.TXT'
       FIN-RET-ALL-PTT.
           EXIT.
       
       RET-PTT-PTT SECTION.
       INI-RET-PTT-PTT.
*% INCLUDE INI_RET_PTT_PTT OF 'COLBMPTT.TXT'
           MOVE PTT-COD-TIPO IN PTT-FLD TO PTT-COD-TIPO IN PTT.
           MOVE PTT-IND-SEPA IN PTT-FLD TO PTT-IND-SEPA IN PTT.
           MOVE PTT-GLS-NOMP IN PTT-FLD TO PTT-GLS-NOMP IN PTT.
*% INCLUDE FIN_RET_PTT_PTT OF 'COLBMPTT.TXT'
       FIN-RET-PTT-PTT.
           EXIT.
       
       KEY-ALL-PTT SECTION.
       INI-KEY-ALL-PTT.
*% INCLUDE INI_KEY_ALL_PTT OF 'COLBMPTT.TXT'
           PERFORM KEY-PTT-PTT.
*% INCLUDE FIN_KEY_ALL_PTT OF 'COLBMPTT.TXT'
       FIN-KEY-ALL-PTT.
           EXIT.
       
       KEY-PTT-PTT SECTION.
       INI-KEY-PTT-PTT.
*% INCLUDE INI_KEY_PTT_PTT OF 'COLBMPTT.TXT'
           MOVE PTT-CAI-IPTT IN PTT-FLD TO PTT-CAI-IPTT IN PTT.
           MOVE PTT-IIC-IPTT IN PTT-FLD TO PTT-IIC-IPTT IN PTT.
           MOVE PTT-CAI-ICVN IN PTT-FLD TO PTT-CAI-ICVN IN PTT.
           MOVE PTT-IIC-ICVN IN PTT-FLD TO PTT-IIC-ICVN IN PTT.
*% INCLUDE FIN_KEY_PTT_PTT OF 'COLBMPTT.TXT'
       FIN-KEY-PTT-PTT.
           EXIT.
       
       SCR-CLRA-PTT SECTION.
       INI-SCR-CLRA-PTT.
*% INCLUDE INI_SCR_CLRA_PTT OF 'COLBMPTT.TXT'
           PERFORM CLR-KEY-PTT.
           PERFORM CLR-FLD-PTT.
*% INCLUDE FIN_SCR_CLRA_PTT OF 'COLBMPTT.TXT'
       FIN-SCR-CLRA-PTT.
           EXIT.
       
       CLR-KEY-PTT SECTION.
       INI-CLR-KEY-PTT.
*% INCLUDE INI_CLR_KEY_PTT OF 'COLBMPTT.TXT'
           MOVE SPACES TO PTT-CAI-IPTT IN PTT-FLD.
           MOVE ZEROES TO PTT-IIC-IPTT IN PTT-FLD.
           MOVE SPACES TO PTT-CAI-ICVN IN PTT-FLD.
           MOVE ZEROES TO PTT-IIC-ICVN IN PTT-FLD.
*% INCLUDE FIN_CLR_KEY_PTT OF 'COLBMPTT.TXT'
       FIN-CLR-KEY-PTT.
           EXIT.
       
       CLR-FLD-PTT SECTION.
       INI-CLR-FLD-PTT.
*% INCLUDE INI_CLR_FLD_PTT OF 'COLBMPTT.TXT'
           MOVE SPACES TO FRM-GLS-TIPO IN PTT-FLD.
           MOVE SPACES TO FRM-GLS-SEPA IN PTT-FLD.
           PERFORM CLR-PTT-PTT.
*% INCLUDE FIN_CLR_FLD_PTT OF 'COLBMPTT.TXT'
       FIN-CLR-FLD-PTT.
           EXIT.
       
       CLR-PTT-PTT SECTION.
       INI-CLR-PTT-PTT.
*% INCLUDE INI_CLR_PTT_PTT OF 'COLBMPTT.TXT'
           MOVE SPACES TO PTT-COD-TIPO IN PTT-FLD.
           MOVE SPACES TO PTT-IND-SEPA IN PTT-FLD.
           MOVE SPACES TO PTT-GLS-NOMP IN PTT-FLD.
*% INCLUDE FIN_CLR_PTT_PTT OF 'COLBMPTT.TXT'
       FIN-CLR-PTT-PTT.
           EXIT.
       
       FST-KEY-PTT SECTION.
       INI-FST-KEY-PTT.
*% INCLUDE INI_FST_KEY_PTT OF 'COLBMPTT.TXT'
           MOVE -1 TO PTT-CAI-IPTT-LEN IN PTT-FLD.
*% INCLUDE FIN_FST_KEY_PTT OF 'COLBMPTT.TXT'
       FIN-FST-KEY-PTT.
           EXIT.
       
       FST-FLD-PTT SECTION.
       INI-FST-FLD-PTT.
*% INCLUDE INI_FST_FLD_PTT OF 'COLBMPTT.TXT'
           MOVE -1 TO PTT-COD-TIPO-LEN IN PTT-FLD.
*% INCLUDE FIN_FST_FLD_PTT OF 'COLBMPTT.TXT'
       FIN-FST-FLD-PTT.
           EXIT.
       
       PRO-KEY-PTT SECTION.
       INI-PRO-KEY-PTT.
*% INCLUDE INI_PRO_KEY_PTT OF 'COLBMPTT.TXT'
           PERFORM PRO-MKY-PTT.
*% INCLUDE FIN_PRO_KEY_PTT OF 'COLBMPTT.TXT'
       FIN-PRO-KEY-PTT.
           EXIT.
       
       PRO-MKY-PTT SECTION.
       INI-PRO-MKY-PTT.
*% INCLUDE INI_PRO_MKY_PTT OF 'COLBMPTT.TXT'
*% INCLUDE FIN_PRO_MKY_PTT OF 'COLBMPTT.TXT'
       FIN-PRO-MKY-PTT.
           EXIT.
       
       PRO-FLD-PTT SECTION.
       INI-PRO-FLD-PTT.
*% INCLUDE INI_PRO_FLD_PTT OF 'COLBMPTT.TXT'
           PERFORM PRO-PTT-PTT.
*% INCLUDE FIN_PRO_FLD_PTT OF 'COLBMPTT.TXT'
       FIN-PRO-FLD-PTT.
           EXIT.
       
       PRO-PTT-PTT SECTION.
       INI-PRO-PTT-PTT.
*% INCLUDE INI_PRO_PTT_PTT OF 'COLBMPTT.TXT'
           MOVE FRM-CPIM-AEY TO PTT-COD-TIPO-ATR IN PTT-FLD.
           MOVE FRM-CPIM-AEY TO PTT-IND-SEPA-ATR IN PTT-FLD.
           MOVE FRM-CPIM-AEY TO PTT-GLS-NOMP-ATR IN PTT-FLD.
*% INCLUDE FIN_PRO_PTT_PTT OF 'COLBMPTT.TXT'
       FIN-PRO-PTT-PTT.
           EXIT.
       
       UNP-KEY-PTT SECTION.
       INI-UNP-KEY-PTT.
*% INCLUDE INI_UNP_KEY_PTT OF 'COLBMPTT.TXT'
           PERFORM UNP-IKY-PTT.
           PERFORM UNP-AKY-PTT.
           PERFORM UNP-MKY-PTT.
*% INCLUDE FIN_UNP_KEY_PTT OF 'COLBMPTT.TXT'
       FIN-UNP-KEY-PTT.
           EXIT.
       
       UNP-IKY-PTT SECTION.
       INI-UNP-IKY-PTT.
*% INCLUDE INI_UNP_IKY_PTT OF 'COLBMPTT.TXT'
           MOVE FRM-CPIM-UBY TO PTT-CAI-IPTT-ATR.
           MOVE FRM-CPIM-UBY TO PTT-IIC-IPTT-ATR.
           MOVE FRM-CPIM-UBY TO PTT-CAI-ICVN-ATR.
           MOVE FRM-CPIM-UBY TO PTT-IIC-ICVN-ATR.
*% INCLUDE FIN_UNP_IKY_PTT OF 'COLBMPTT.TXT'
       FIN-UNP-IKY-PTT.
           EXIT.
       
       UNP-MKY-PTT SECTION.
       INI-UNP-MKY-PTT.
*% INCLUDE INI_UNP_MKY_PTT OF 'COLBMPTT.TXT'
*% INCLUDE FIN_UNP_MKY_PTT OF 'COLBMPTT.TXT'
       FIN-UNP-MKY-PTT.
           EXIT.
       
       UNP-AKY-PTT SECTION.
       INI-UNP-AKY-PTT.
*% INCLUDE INI_UNP_AKY_PTT OF 'COLBMPTT.TXT'
*% INCLUDE FIN_UNP_AKY_PTT OF 'COLBMPTT.TXT'
       FIN-UNP-AKY-PTT.
           EXIT.
       
       UNP-FLD-PTT SECTION.
       INI-UNP-FLD-PTT.
*% INCLUDE INI_UNP_FLD_PTT OF 'COLBMPTT.TXT'
           PERFORM UNP-PTT-PTT.
*% INCLUDE FIN_UNP_FLD_PTT OF 'COLBMPTT.TXT'
       FIN-UNP-FLD-PTT.
           EXIT.
       
       UNP-PTT-PTT SECTION.
       INI-UNP-PTT-PTT.
*% INCLUDE INI_UNP_PTT_PTT OF 'COLBMPTT.TXT'
           MOVE FRM-CPIM-UBY TO PTT-COD-TIPO-ATR IN PTT-FLD.
           MOVE FRM-CPIM-UBY TO PTT-IND-SEPA-ATR IN PTT-FLD.
           MOVE FRM-CPIM-UBY TO PTT-GLS-NOMP-ATR IN PTT-FLD.
*% INCLUDE FIN_UNP_PTT_PTT OF 'COLBMPTT.TXT'
       FIN-UNP-PTT-PTT.
           EXIT.
       
       VAL-MKY-PTT SECTION.
       INI-VAL-MKY-PTT.
*% INCLUDE INI_VAL_MKY_PTT OF 'COLBMPTT.TXT'
*% INCLUDE FIN_VAL_MKY_PTT OF 'COLBMPTT.TXT'
       FIN-VAL-MKY-PTT.
           EXIT.
       
       VAL-FLD-PTT SECTION.
       INI-VAL-FLD-PTT.
*% INCLUDE INI_VAL_FLD_PTT OF 'COLBMPTT.TXT'
           PERFORM VAL-PTT-COD-TIPO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PTT-COD-TIPO-LEN IN PTT-FLD
                GO TO FIN-VAL-FLD-PTT.
           PERFORM VAL-FRM-GLS-TIPO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TIPO-LEN IN PTT-FLD
                GO TO FIN-VAL-FLD-PTT.
           PERFORM VAL-PTT-IND-SEPA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PTT-IND-SEPA-LEN IN PTT-FLD
                GO TO FIN-VAL-FLD-PTT.
           PERFORM VAL-FRM-GLS-SEPA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-SEPA-LEN IN PTT-FLD
                GO TO FIN-VAL-FLD-PTT.
           PERFORM VAL-PTT-GLS-NOMP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PTT-GLS-NOMP-LEN IN PTT-FLD
                GO TO FIN-VAL-FLD-PTT.
*% INCLUDE FIN_VAL_FLD_PTT OF 'COLBMPTT.TXT'
       FIN-VAL-FLD-PTT.
           EXIT.
       
       VAL-PTT-COD-TIPO SECTION.
       INI-VAL-PTT-COD-TIPO.
*% LBL = "PTT_COD_TIPO OF 'COLBMPTT.TXT'"
*% INCLUDE INI_PTT_COD_TIPO OF 'COLBMPTT.TXT'
           IF PTT-COD-TIPO IN PTT-FLD NOT > SPACES
              MOVE SPACES TO PTT-COD-TIPO IN PTT-FLD.
*% INCLUDE PTT_COD_TIPO OF 'COLBMPTT.TXT'
*% INCLUDE FIN_PTT_COD_TIPO OF 'COLBMPTT.TXT'
       FIN-VAL-PTT-COD-TIPO.
           EXIT.
       
       VAL-FRM-GLS-TIPO SECTION.
       INI-VAL-FRM-GLS-TIPO.
*% LBL = "FRM_GLS_TIPO OF 'COLBMPTT.TXT'"
*% INCLUDE INI_FRM_GLS_TIPO OF 'COLBMPTT.TXT'
*% INCLUDE FRM_GLS_TIPO OF 'COLBMPTT.TXT'
*% INCLUDE FIN_FRM_GLS_TIPO OF 'COLBMPTT.TXT'
       FIN-VAL-FRM-GLS-TIPO.
           EXIT.
       
       VAL-PTT-IND-SEPA SECTION.
       INI-VAL-PTT-IND-SEPA.
*% LBL = "PTT_IND_SEPA OF 'COLBMPTT.TXT'"
*% INCLUDE INI_PTT_IND_SEPA OF 'COLBMPTT.TXT'
           IF PTT-IND-SEPA IN PTT-FLD NOT > SPACES
              MOVE SPACES TO PTT-IND-SEPA IN PTT-FLD.
*% INCLUDE PTT_IND_SEPA OF 'COLBMPTT.TXT'
*% INCLUDE FIN_PTT_IND_SEPA OF 'COLBMPTT.TXT'
       FIN-VAL-PTT-IND-SEPA.
           EXIT.
       
       VAL-FRM-GLS-SEPA SECTION.
       INI-VAL-FRM-GLS-SEPA.
*% LBL = "FRM_GLS_SEPA OF 'COLBMPTT.TXT'"
*% INCLUDE INI_FRM_GLS_SEPA OF 'COLBMPTT.TXT'
*% INCLUDE FRM_GLS_SEPA OF 'COLBMPTT.TXT'
*% INCLUDE FIN_FRM_GLS_SEPA OF 'COLBMPTT.TXT'
       FIN-VAL-FRM-GLS-SEPA.
           EXIT.
       
       VAL-PTT-GLS-NOMP SECTION.
       INI-VAL-PTT-GLS-NOMP.
*% LBL = "PTT_GLS_NOMP OF 'COLBMPTT.TXT'"
*% INCLUDE INI_PTT_GLS_NOMP OF 'COLBMPTT.TXT'
           IF PTT-GLS-NOMP IN PTT-FLD NOT > SPACES
              MOVE SPACES TO PTT-GLS-NOMP IN PTT-FLD.
*% INCLUDE PTT_GLS_NOMP OF 'COLBMPTT.TXT'
*% INCLUDE FIN_PTT_GLS_NOMP OF 'COLBMPTT.TXT'
       FIN-VAL-PTT-GLS-NOMP.
           EXIT.
       
       VAL-CON-FLD-PTT SECTION.
       INI-VAL-CON-FLD-PTT.
*% INCLUDE VAL_CON_FLD_PTT OF 'COLBMPTT.TXT'
       FIN-VAL-CON-FLD-PTT.
           EXIT.
       
       VAL-CON-PTT SECTION.
       INI-VAL-CON-PTT.
*% INCLUDE VAL_CON_PTT OF 'COLBMPTT.TXT'
       FIN-VAL-CON-PTT.
           EXIT.
       
       VAL-NUM-KEY-PTT SECTION.
       INI-VAL-NUM-KEY-PTT.
           PERFORM VAL-NUM-IKY-PTT.
           PERFORM VAL-NUM-MKY-PTT.
           PERFORM VAL-NUM-AKY-PTT.
       FIN-VAL-NUM-KEY-PTT.
           EXIT.
       
       VAL-NUM-IKY-PTT SECTION.
       INI-VAL-NUM-IKY-PTT.
*% INCLUDE INI_VAL_NUM_IKY_PTT OF 'COLBMPTT.TXT'
       VAL-NUM-PTT-IIC-IPTT.
*% BUF = "PTT-FLD"
*% FLD = "PTT-IIC-IPTT"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-PTT.
       VAL-NUM-PTT-IIC-ICVN.
*% BUF = "PTT-FLD"
*% FLD = "PTT-IIC-ICVN"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-PTT.
*% INCLUDE FIN_VAL_NUM_IKY_PTT OF 'COLBMPTT.TXT'
       FIN-VAL-NUM-IKY-PTT.
           EXIT.
       
       VAL-NUM-MKY-PTT SECTION.
       INI-VAL-NUM-MKY-PTT.
*% INCLUDE INI_VAL_NUM_MKY_PTT OF 'COLBMPTT.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_PTT OF 'COLBMPTT.TXT'
       FIN-VAL-NUM-MKY-PTT.
           EXIT.
       
       VAL-NUM-AKY-PTT SECTION.
       INI-VAL-NUM-AKY-PTT.
*% INCLUDE INI_VAL_NUM_AKY_PTT OF 'COLBMPTT.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_PTT OF 'COLBMPTT.TXT'
       FIN-VAL-NUM-AKY-PTT.
           EXIT.
       
       VAL-NUM-FLD-PTT SECTION.
       INI-VAL-NUM-FLD-PTT.
*% INCLUDE INI_VAL_NUM_FLD_PTT OF 'COLBMPTT.TXT'
*% INCLUDE FIN_VAL_NUM_FLD_PTT OF 'COLBMPTT.TXT'
       FIN-VAL-NUM-FLD-PTT.
           EXIT.
       
       EDT-KEY-PTT SECTION.
       INI-EDT-KEY-PTT.
           PERFORM EDT-IKY-PTT.
           PERFORM EDT-MKY-PTT.
           PERFORM EDT-AKY-PTT.
*% INCLUDE EDT_KEY_PTT OF 'COLBMPTT.TXT'
       FIN-EDT-KEY-PTT.
           EXIT.
       
       EDT-IKY-PTT SECTION.
       INI-EDT-IKY-PTT.
*% INCLUDE EDT_IKY_PTT OF 'COLBMPTT.TXT'
           IF PTT-IIC-IPTT-LEN IN PTT-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PTT-IIC-IPTT-LEN IN PTT-FLD
                GO TO FIN-EDT-IKY-PTT.
           MOVE PTT-IIC-IPTT IN PTT-FLD TO PTT-IIC-IPTT-EDT IN PTT-EDT.
           MOVE PTT-IIC-IPTT-EDT IN PTT-EDT TO 
                PTT-IIC-IPTT-ALF IN PTT-FLD.
           IF PTT-IIC-ICVN-LEN IN PTT-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PTT-IIC-ICVN-LEN IN PTT-FLD
                GO TO FIN-EDT-IKY-PTT.
           MOVE PTT-IIC-ICVN IN PTT-FLD TO PTT-IIC-ICVN-EDT IN PTT-EDT.
           MOVE PTT-IIC-ICVN-EDT IN PTT-EDT TO 
                PTT-IIC-ICVN-ALF IN PTT-FLD.
*% INCLUDE FIN_EDT_IKY_PTT OF 'COLBMPTT.TXT'
       FIN-EDT-IKY-PTT.
           EXIT.
       
       EDT-MKY-PTT SECTION.
       INI-EDT-MKY-PTT.
*% INCLUDE EDT_MKY_PTT OF 'COLBMPTT.TXT'
*% INCLUDE FIN_EDT_MKY_PTT OF 'COLBMPTT.TXT'
       FIN-EDT-MKY-PTT.
           EXIT.
       
       EDT-AKY-PTT SECTION.
       INI-EDT-AKY-PTT.
*% INCLUDE EDT_AKY_PTT OF 'COLBMPTT.TXT'
*% INCLUDE FIN_EDT_AKY_PTT OF 'COLBMPTT.TXT'
       FIN-EDT-AKY-PTT.
           EXIT.
       
       EDT-FLD-PTT SECTION.
       INI-EDT-FLD-PTT.
*% INCLUDE EDT_FLD_PTT OF 'COLBMPTT.TXT'
*% INCLUDE FIN_EDT_FLD_PTT OF 'COLBMPTT.TXT'
       FIN-EDT-FLD-PTT.
           EXIT.
*% INCLUDE EOF_BU OF 'COLBMPTT.TXT'
*% INCLUDE EOF OF 'COLBMPTT.TXT'
*% INCLUDE EOF_IBM OF 'COLBMPTT.TXT'
