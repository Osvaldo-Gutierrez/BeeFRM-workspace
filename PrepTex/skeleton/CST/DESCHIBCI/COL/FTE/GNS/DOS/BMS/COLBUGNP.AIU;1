*% FRM = "GNP"
      *
      * COLBUGNP
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:02 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMGNP.TXT'
       SET-TAG-COLBUGNP SECTION.
       INI-SET-TAG-COLBUGNP.
           MOVE
           '<IDNMOD NOM=$COLBUGNP$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:02 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUGNP.
           EXIT.
           
       RET-ALL-GNP SECTION.
       INI-RET-ALL-GNP.
*% INCLUDE INI_RET_ALL_GNP OF 'COLBMGNP.TXT'
           PERFORM KEY-ALL-GNP.
           PERFORM RET-OPC-GNP.
*% INCLUDE FIN_RET_ALL_GNP OF 'COLBMGNP.TXT'
       FIN-RET-ALL-GNP.
           EXIT.
       
       RET-OPC-GNP SECTION.
       INI-RET-OPC-GNP.
*% INCLUDE INI_RET_OPC_GNP OF 'COLBMGNP.TXT'
*% INCLUDE FIN_RET_OPC_GNP OF 'COLBMGNP.TXT'
       FIN-RET-OPC-GNP.
           EXIT.
       
       KEY-ALL-GNP SECTION.
       INI-KEY-ALL-GNP.
*% INCLUDE INI_KEY_ALL_GNP OF 'COLBMGNP.TXT'
           PERFORM KEY-OPC-GNP.
*% INCLUDE FIN_KEY_ALL_GNP OF 'COLBMGNP.TXT'
       FIN-KEY-ALL-GNP.
           EXIT.
       
       KEY-OPC-GNP SECTION.
       INI-KEY-OPC-GNP.
*% INCLUDE INI_KEY_OPC_GNP OF 'COLBMGNP.TXT'
           MOVE OPC-CAI-IOPC IN GNP-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE OPC-IIC-IOPC IN GNP-FLD TO OPC-IIC-IOPC IN OPC.
*% INCLUDE FIN_KEY_OPC_GNP OF 'COLBMGNP.TXT'
       FIN-KEY-OPC-GNP.
           EXIT.
       
       SCR-CLRA-GNP SECTION.
       INI-SCR-CLRA-GNP.
*% INCLUDE INI_SCR_CLRA_GNP OF 'COLBMGNP.TXT'
           PERFORM CLR-KEY-GNP.
           PERFORM CLR-FLD-GNP.
*% INCLUDE FIN_SCR_CLRA_GNP OF 'COLBMGNP.TXT'
       FIN-SCR-CLRA-GNP.
           EXIT.
       
       CLR-KEY-GNP SECTION.
       INI-CLR-KEY-GNP.
*% INCLUDE INI_CLR_KEY_GNP OF 'COLBMGNP.TXT'
           MOVE SPACES TO OPC-CAI-IOPC IN GNP-FLD.
           MOVE ZEROES TO OPC-IIC-IOPC IN GNP-FLD.
           MOVE ZEROES TO FRM-EXT-IOPC IN GNP-FLD.
           MOVE SPACES TO FRM-IND-MODO IN GNP-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCUR"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FCUR IN GNP-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-VIGU IN GNP-FLD.
           MOVE ZEROES TO FRM-NUM-SPVE IN GNP-FLD.
           MOVE SPACES TO FRM-IND-UTPV IN GNP-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPVC"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FPVC IN GNP-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-SEVE IN GNP-FLD.
           MOVE SPACES TO FRM-IND-UTSV IN GNP-FLD.
           MOVE SPACES TO FRM-MSC-TCVE IN GNP-FLD.
           MOVE ZEROES TO FRM-NUM-NP01 IN GNP-FLD.
           MOVE ZEROES TO FRM-NUM-NP02 IN GNP-FLD.
           MOVE ZEROES TO FRM-NUM-NP03 IN GNP-FLD.
           MOVE ZEROES TO FRM-NUM-NP04 IN GNP-FLD.
           MOVE ZEROES TO FRM-NUM-NP05 IN GNP-FLD.
           MOVE ZEROES TO FRM-NUM-NP06 IN GNP-FLD.
           MOVE ZEROES TO FRM-NUM-NP07 IN GNP-FLD.
           MOVE ZEROES TO FRM-NUM-NP08 IN GNP-FLD.
           MOVE ZEROES TO FRM-NUM-NP09 IN GNP-FLD.
           MOVE ZEROES TO FRM-NUM-NP10 IN GNP-FLD.
           MOVE ZEROES TO FRM-NUM-NP11 IN GNP-FLD.
*% INCLUDE FIN_CLR_KEY_GNP OF 'COLBMGNP.TXT'
       FIN-CLR-KEY-GNP.
           EXIT.
       
       CLR-FLD-GNP SECTION.
       INI-CLR-FLD-GNP.
*% INCLUDE INI_CLR_FLD_GNP OF 'COLBMGNP.TXT'
           MOVE ZEROES TO FRM-NUM-PLZT IN GNP-FLD.
