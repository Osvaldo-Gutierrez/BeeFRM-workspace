                MOVE FRM-IND-ERRO TO FRM-VAL-VENC-R-LEN IN GNP-FLD(7)
                GO TO FIN-EDT-FLD-GNP.
           MOVE FRM-VAL-VENC-R IN GNP-FLD(7) TO 
                FRM-VAL-VENC-R-EDT IN GNP-EDT.
           MOVE FRM-VAL-VENC-R-EDT IN GNP-EDT TO 
                FRM-VAL-VENC-R-ALF IN GNP-FLD(7).
           IF FRM-VAL-VENC-R-LEN IN GNP-FLD(8) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-VENC-R-LEN IN GNP-FLD(8)
                GO TO FIN-EDT-FLD-GNP.
           MOVE FRM-VAL-VENC-R IN GNP-FLD(8) TO 
                FRM-VAL-VENC-R-EDT IN GNP-EDT.
           MOVE FRM-VAL-VENC-R-EDT IN GNP-EDT TO 
                FRM-VAL-VENC-R-ALF IN GNP-FLD(8).
           IF FRM-VAL-VENC-R-LEN IN GNP-FLD(9) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-VENC-R-LEN IN GNP-FLD(9)
                GO TO FIN-EDT-FLD-GNP.
           MOVE FRM-VAL-VENC-R IN GNP-FLD(9) TO 
                FRM-VAL-VENC-R-EDT IN GNP-EDT.
           MOVE FRM-VAL-VENC-R-EDT IN GNP-EDT TO 
                FRM-VAL-VENC-R-ALF IN GNP-FLD(9).
           IF FRM-VAL-VENC-R-LEN IN GNP-FLD(10) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-VENC-R-LEN IN GNP-FLD(10)
                GO TO FIN-EDT-FLD-GNP.
           MOVE FRM-VAL-VENC-R IN GNP-FLD(10) TO 
                FRM-VAL-VENC-R-EDT IN GNP-EDT.
           MOVE FRM-VAL-VENC-R-EDT IN GNP-EDT TO 
                FRM-VAL-VENC-R-ALF IN GNP-FLD(10).
*% INCLUDE FIN_EDT_FLD_GNP OF 'COLBMGNP.TXT'
       FIN-EDT-FLD-GNP.
           EXIT.
*% INCLUDE EOF_BU OF 'COLBMGNP.TXT'
*% INCLUDE EOF OF 'COLBMGNP.TXT'
*% INCLUDE EOF_IBM OF 'COLBMGNP.TXT'
