       VAL-MKY-DTC SECTION.
       INI-VAL-MKY-DTC.
*% INCLUDE INI_VAL_MKY_DTC OF 'COLBMDTC.TXT'
*% INCLUDE FIN_VAL_MKY_DTC OF 'COLBMDTC.TXT'
       FIN-VAL-MKY-DTC.
           EXIT.
       
       VAL-FLD-DTC SECTION.
       INI-VAL-FLD-DTC.
*% INCLUDE INI_VAL_FLD_DTC OF 'COLBMDTC.TXT'
           PERFORM VAL-OPC-COD-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-TOOC-LEN IN DTC-FLD
                GO TO FIN-VAL-FLD-DTC.
           PERFORM VAL-OPC-COD-AOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-AOOC-LEN IN DTC-FLD
                GO TO FIN-VAL-FLD-DTC.
           PERFORM VAL-FRM-GLS-COOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-COOC-LEN IN DTC-FLD
                GO TO FIN-VAL-FLD-DTC.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * VRF-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * VRF-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DBC-IND-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-IND-ICLI-LEN IN DTC-FLD
                GO TO FIN-VAL-FLD-DTC.
           PERFORM VAL-DBC-GLS-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-GLS-ICLI-LEN IN DTC-FLD
                GO TO FIN-VAL-FLD-DTC.
           PERFORM VAL-DBC-NUM-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-NUM-ICLI-LEN IN DTC-FLD
                GO TO FIN-VAL-FLD-DTC.
           PERFORM VAL-DBC-VRF-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-VRF-ICLI-LEN IN DTC-FLD
                GO TO FIN-VAL-FLD-DTC.
      * IDC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DBC-GLS-NOMC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DBC-GLS-NOMC-LEN IN DTC-FLD
                GO TO FIN-VAL-FLD-DTC.
           PERFORM VAL-FRM-COD-TCAN
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 10
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-COD-TCAN-LEN IN DTC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DTC.
           PERFORM VAL-FRM-GLS-TCAN
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 10
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-GLS-TCAN-LEN IN DTC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DTC.
           PERFORM VAL-FRM-COD-GRPO
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 10
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-COD-GRPO-LEN IN DTC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DTC.
           PERFORM VAL-FRM-GLS-GRPO
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 10
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-GLS-GRPO-LEN IN DTC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DTC.
*% INCLUDE FIN_VAL_FLD_DTC OF 'COLBMDTC.TXT'
       FIN-VAL-FLD-DTC.
           EXIT.
       
