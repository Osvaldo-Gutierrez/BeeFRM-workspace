*% INI_LBL = TRUE
*% PGM_BU = TRUE
       RET-ALL-DTC SECTION.
       INI-RET-ALL-DTC.
*% INCLUDE INI_RET_ALL_DTC OF 'COLBMDTC.TXT'
           PERFORM KEY-ALL-DTC.
           PERFORM RET-OPC-DTC.
           PERFORM RET-DBC-DTC.
*% INCLUDE FIN_RET_ALL_DTC OF 'COLBMDTC.TXT'
       FIN-RET-ALL-DTC.
           EXIT.
       
       RET-OPC-DTC SECTION.
       INI-RET-OPC-DTC.
*% INCLUDE INI_RET_OPC_DTC OF 'COLBMDTC.TXT'
           MOVE OPC-COD-TOOC IN DTC-FLD TO OPC-COD-TOOC IN OPC.
           MOVE OPC-COD-AOOC IN DTC-FLD TO OPC-COD-AOOC IN OPC.
*% INCLUDE FIN_RET_OPC_DTC OF 'COLBMDTC.TXT'
       FIN-RET-OPC-DTC.
           EXIT.
       
       RET-DBC-DTC SECTION.
       INI-RET-DBC-DTC.
*% INCLUDE INI_RET_DBC_DTC OF 'COLBMDTC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN DTC-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN DTC-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN DTC-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN DTC-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-GLS-NOMC IN DTC-FLD TO DBC-GLS-NOMC IN DBC.
*% INCLUDE FIN_RET_DBC_DTC OF 'COLBMDTC.TXT'
       FIN-RET-DBC-DTC.
           EXIT.
       
       KEY-ALL-DTC SECTION.
       INI-KEY-ALL-DTC.
*% INCLUDE INI_KEY_ALL_DTC OF 'COLBMDTC.TXT'
           PERFORM KEY-OPC-DTC.
           PERFORM KEY-DBC-DTC.
*% INCLUDE FIN_KEY_ALL_DTC OF 'COLBMDTC.TXT'
       FIN-KEY-ALL-DTC.
           EXIT.
       
       KEY-OPC-DTC SECTION.
       INI-KEY-OPC-DTC.
*% INCLUDE INI_KEY_OPC_DTC OF 'COLBMDTC.TXT'
           MOVE OPC-CAI-IOPC IN DTC-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE OPC-IIC-IOPC IN DTC-FLD TO OPC-IIC-IOPC IN OPC.
*% INCLUDE FIN_KEY_OPC_DTC OF 'COLBMDTC.TXT'
       FIN-KEY-OPC-DTC.
           EXIT.
       
       KEY-DBC-DTC SECTION.
       INI-KEY-DBC-DTC.
*% INCLUDE INI_KEY_DBC_DTC OF 'COLBMDTC.TXT'
*% INCLUDE FIN_KEY_DBC_DTC OF 'COLBMDTC.TXT'
       FIN-KEY-DBC-DTC.
           EXIT.
       
       SCR-CLRA-DTC SECTION.
       INI-SCR-CLRA-DTC.
*% INCLUDE INI_SCR_CLRA_DTC OF 'COLBMDTC.TXT'
           PERFORM CLR-KEY-DTC.
           PERFORM CLR-FLD-DTC.
*% INCLUDE FIN_SCR_CLRA_DTC OF 'COLBMDTC.TXT'
       FIN-SCR-CLRA-DTC.
           EXIT.
       
       CLR-KEY-DTC SECTION.
       INI-CLR-KEY-DTC.
*% INCLUDE INI_CLR_KEY_DTC OF 'COLBMDTC.TXT'
           MOVE SPACES TO OPC-CAI-IOPC IN DTC-FLD.
           MOVE ZEROES TO OPC-IIC-IOPC IN DTC-FLD.
*% INCLUDE FIN_CLR_KEY_DTC OF 'COLBMDTC.TXT'
       FIN-CLR-KEY-DTC.
           EXIT.
       
       CLR-FLD-DTC SECTION.
       INI-CLR-FLD-DTC.
*% INCLUDE INI_CLR_FLD_DTC OF 'COLBMDTC.TXT'
           MOVE SPACES TO FRM-GLS-COOC IN DTC-FLD.
           MOVE SPACES TO FRM-COD-TCAN IN DTC-FLD(1).
           MOVE SPACES TO FRM-COD-TCAN IN DTC-FLD(2).
           MOVE SPACES TO FRM-COD-TCAN IN DTC-FLD(3).
           MOVE SPACES TO FRM-COD-TCAN IN DTC-FLD(4).
           MOVE SPACES TO FRM-COD-TCAN IN DTC-FLD(5).
           MOVE SPACES TO FRM-COD-TCAN IN DTC-FLD(6).
           MOVE SPACES TO FRM-COD-TCAN IN DTC-FLD(7).
           MOVE SPACES TO FRM-COD-TCAN IN DTC-FLD(8).
           MOVE SPACES TO FRM-COD-TCAN IN DTC-FLD(9).
           MOVE SPACES TO FRM-COD-TCAN IN DTC-FLD(10).
           MOVE SPACES TO FRM-GLS-TCAN IN DTC-FLD(1).
           MOVE SPACES TO FRM-GLS-TCAN IN DTC-FLD(2).
           MOVE SPACES TO FRM-GLS-TCAN IN DTC-FLD(3).
           MOVE SPACES TO FRM-GLS-TCAN IN DTC-FLD(4).
           MOVE SPACES TO FRM-GLS-TCAN IN DTC-FLD(5).
           MOVE SPACES TO FRM-GLS-TCAN IN DTC-FLD(6).
           MOVE SPACES TO FRM-GLS-TCAN IN DTC-FLD(7).
           MOVE SPACES TO FRM-GLS-TCAN IN DTC-FLD(8).
           MOVE SPACES TO FRM-GLS-TCAN IN DTC-FLD(9).
           MOVE SPACES TO FRM-GLS-TCAN IN DTC-FLD(10).
           MOVE SPACES TO FRM-COD-GRPO IN DTC-FLD(1).
           MOVE SPACES TO FRM-COD-GRPO IN DTC-FLD(2).
           MOVE SPACES TO FRM-COD-GRPO IN DTC-FLD(3).
           MOVE SPACES TO FRM-COD-GRPO IN DTC-FLD(4).
           MOVE SPACES TO FRM-COD-GRPO IN DTC-FLD(5).
           MOVE SPACES TO FRM-COD-GRPO IN DTC-FLD(6).
           MOVE SPACES TO FRM-COD-GRPO IN DTC-FLD(7).
           MOVE SPACES TO FRM-COD-GRPO IN DTC-FLD(8).
           MOVE SPACES TO FRM-COD-GRPO IN DTC-FLD(9).
           MOVE SPACES TO FRM-COD-GRPO IN DTC-FLD(10).
           MOVE SPACES TO FRM-GLS-GRPO IN DTC-FLD(1).
           MOVE SPACES TO FRM-GLS-GRPO IN DTC-FLD(2).
           MOVE SPACES TO FRM-GLS-GRPO IN DTC-FLD(3).
           MOVE SPACES TO FRM-GLS-GRPO IN DTC-FLD(4).
           MOVE SPACES TO FRM-GLS-GRPO IN DTC-FLD(5).
           MOVE SPACES TO FRM-GLS-GRPO IN DTC-FLD(6).
           MOVE SPACES TO FRM-GLS-GRPO IN DTC-FLD(7).
           MOVE SPACES TO FRM-GLS-GRPO IN DTC-FLD(8).
           MOVE SPACES TO FRM-GLS-GRPO IN DTC-FLD(9).
           MOVE SPACES TO FRM-GLS-GRPO IN DTC-FLD(10).
           PERFORM CLR-OPC-DTC.
           PERFORM CLR-DBC-DTC.
*% INCLUDE FIN_CLR_FLD_DTC OF 'COLBMDTC.TXT'
       FIN-CLR-FLD-DTC.
