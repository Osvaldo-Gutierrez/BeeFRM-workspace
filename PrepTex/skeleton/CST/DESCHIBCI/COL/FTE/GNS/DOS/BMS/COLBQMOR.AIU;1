      *
      * COLBQMOR
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:08 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "MOR"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMMOR.TXT'
       SET-TAG-COLBQMOR SECTION.
       INI-SET-TAG-COLBQMOR.
           MOVE
           '<IDNMOD NOM=$COLBQMOR$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:08 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQMOR.
           EXIT.
           
       PUT-ALL-MOR SECTION.
       INI-PUT-ALL-MOR.
*% INCLUDE INI_PUT_ALL_MOR OF 'COLBMMOR.TXT'
           PERFORM PUT-MOR-MOR.
*% INCLUDE FIN_PUT_ALL_MOR OF 'COLBMMOR.TXT'
       FIN-PUT-ALL-MOR.
           EXIT.
       
       PUT-MOR-MOR SECTION.
       INI-PUT-MOR-MOR.
*% INCLUDE INI_PUT_MOR_MOR OF 'COLBMMOR.TXT'
           IF PGM-STAT-MOR-NUL
               GO TO FIN-PUT-MOR-MOR.
           IF NOT PGM-STAT-MOR-OKS
               PERFORM CLR-MOR-MOR
               GO TO FIN-PUT-MOR-MOR.
           MOVE MOR-COD-OFI IN MOR TO MOR-COD-OFI IN MOR-FLD.
*% BUF = "MOR-FLD"
*% SIS = "TAB"
*% TAB = "OFI "
*% FLD = "MOR-COD-OFI"
*% GLS = "FRM-GLS-OFI"
*% TIENE_GLOSA = TRUE
*% LGL = 15
*% INCLUDE 'BUSOFI          '
*% INCLUDE FIN_PUT_MOR_MOR OF 'COLBMMOR.TXT'
       FIN-PUT-MOR-MOR.
           EXIT.
       
       KEY-ALL-MOR SECTION.
       INI-KEY-ALL-MOR.
*% INCLUDE INI_KEY_ALL_MOR OF 'COLBMMOR.TXT'
           PERFORM KEY-MOR-MOR.
*% INCLUDE FIN_KEY_ALL_MOR OF 'COLBMMOR.TXT'
       FIN-KEY-ALL-MOR.
           EXIT.
       
       KEY-MOR-MOR SECTION.
       INI-KEY-MOR-MOR.
*% INCLUDE INI_KEY_MOR_MOR OF 'COLBMMOR.TXT'
           MOVE MOR-COD-OFI IN MOR-FLD TO MOR-COD-OFI IN MOR.
*% INCLUDE FIN_KEY_MOR_MOR OF 'COLBMMOR.TXT'
       FIN-KEY-MOR-MOR.
           EXIT.
       
       SCR-CLRA-MOR SECTION.
       INI-SCR-CLRA-MOR.
*% INCLUDE INI_SCR_CLRA_MOR OF 'COLBMMOR.TXT'
           PERFORM CLR-KEY-MOR.
           PERFORM CLR-FLD-MOR.
*% INCLUDE FIN_SCR_CLRA_MOR OF 'COLBMMOR.TXT'
       FIN-SCR-CLRA-MOR.
           EXIT.
       
       CLR-KEY-MOR SECTION.
       INI-CLR-KEY-MOR.
*% INCLUDE INI_CLR_KEY_MOR OF 'COLBMMOR.TXT'
           MOVE ZEROES TO MOR-COD-OFI IN MOR-FLD.
           MOVE SPACES TO FRM-COD-EJE IN MOR-FLD.
*% INCLUDE FIN_CLR_KEY_MOR OF 'COLBMMOR.TXT'
       FIN-CLR-KEY-MOR.
           EXIT.
       
       CLR-FLD-MOR SECTION.
       INI-CLR-FLD-MOR.
*% INCLUDE INI_CLR_FLD_MOR OF 'COLBMMOR.TXT'
           MOVE SPACES TO FRM-INF-DES IN MOR-FLD.
           MOVE SPACES TO FRM-INF-HAS IN MOR-FLD.
           MOVE SPACES TO FRM-GLS-OFI IN MOR-FLD.
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(1).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(2).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(3).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(4).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(5).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(6).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(7).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(8).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(9).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(10).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(11).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(12).
           PERFORM CLR-MOR-MOR.
*% INCLUDE FIN_CLR_FLD_MOR OF 'COLBMMOR.TXT'
       FIN-CLR-FLD-MOR.
           EXIT.
       
       CLR-MOR-MOR SECTION.
       INI-CLR-MOR-MOR.
*% INCLUDE INI_CLR_MOR_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_CLR_MOR_MOR OF 'COLBMMOR.TXT'
       FIN-CLR-MOR-MOR.
           EXIT.
       
       FST-KEY-MOR SECTION.
       INI-FST-KEY-MOR.
*% INCLUDE INI_FST_KEY_MOR OF 'COLBMMOR.TXT'
           MOVE -1 TO MOR-COD-OFI-LEN IN MOR-FLD.
*% INCLUDE FIN_FST_KEY_MOR OF 'COLBMMOR.TXT'
       FIN-FST-KEY-MOR.
           EXIT.
       
       FST-FLD-MOR SECTION.
       INI-FST-FLD-MOR.
*% INCLUDE INI_FST_FLD_MOR OF 'COLBMMOR.TXT'
           MOVE -1 TO FRM-INF-DES-LEN IN MOR-FLD.
*% INCLUDE FIN_FST_FLD_MOR OF 'COLBMMOR.TXT'
       FIN-FST-FLD-MOR.
           EXIT.
       
       PRO-KEY-MOR SECTION.
       INI-PRO-KEY-MOR.
*% INCLUDE INI_PRO_KEY_MOR OF 'COLBMMOR.TXT'
           MOVE FRM-CPIM-AEY TO FRM-COD-EJE-ATR IN MOR-FLD.
           PERFORM PRO-IKY-MOR.
           PERFORM PRO-AKY-MOR.
           PERFORM PRO-MKY-MOR.
*% INCLUDE FIN_PRO_KEY_MOR OF 'COLBMMOR.TXT'
       FIN-PRO-KEY-MOR.
           EXIT.
       
       PRO-IKY-MOR SECTION.
       INI-PRO-IKY-MOR.
*% INCLUDE INI_PRO_IKY_MOR OF 'COLBMMOR.TXT'
           MOVE FRM-CPIM-AEY TO MOR-COD-OFI-ATR IN MOR-FLD.
*% INCLUDE FIN_PRO_IKY_MOR OF 'COLBMMOR.TXT'
       FIN-PRO-IKY-MOR.
           EXIT.
       
       PRO-MKY-MOR SECTION.
       INI-PRO-MKY-MOR.
*% INCLUDE INI_PRO_MKY_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_PRO_MKY_MOR OF 'COLBMMOR.TXT'
       FIN-PRO-MKY-MOR.
           EXIT.
       
       PRO-AKY-MOR SECTION.
       INI-PRO-AKY-MOR.
*% INCLUDE INI_PRO_AKY_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_PRO_AKY_MOR OF 'COLBMMOR.TXT'
       FIN-PRO-AKY-MOR.
           EXIT.
       
       UNP-KEY-MOR SECTION.
       INI-UNP-KEY-MOR.
*% INCLUDE INI_UNP_KEY_MOR OF 'COLBMMOR.TXT'
           MOVE FRM-CPIM-UBY TO FRM-COD-EJE-ATR IN MOR-FLD.
           PERFORM UNP-IKY-MOR.
           PERFORM UNP-AKY-MOR.
           PERFORM UNP-MKY-MOR.
*% INCLUDE FIN_UNP_KEY_MOR OF 'COLBMMOR.TXT'
       FIN-UNP-KEY-MOR.
           EXIT.
       
       UNP-IKY-MOR SECTION.
       INI-UNP-IKY-MOR.
*% INCLUDE INI_UNP_IKY_MOR OF 'COLBMMOR.TXT'
           MOVE FRM-CPIM-UBY TO MOR-COD-OFI-ATR.
           MOVE FRM-CPIM-UBY TO FRM-COD-EJE-ATR.
*% INCLUDE FIN_UNP_IKY_MOR OF 'COLBMMOR.TXT'
       FIN-UNP-IKY-MOR.
           EXIT.
       
       UNP-MKY-MOR SECTION.
       INI-UNP-MKY-MOR.
*% INCLUDE INI_UNP_MKY_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_UNP_MKY_MOR OF 'COLBMMOR.TXT'
       FIN-UNP-MKY-MOR.
           EXIT.
       
       UNP-AKY-MOR SECTION.
       INI-UNP-AKY-MOR.
*% INCLUDE INI_UNP_AKY_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_UNP_AKY_MOR OF 'COLBMMOR.TXT'
       FIN-UNP-AKY-MOR.
           EXIT.
       
       UNP-FLD-MOR SECTION.
       INI-UNP-FLD-MOR.
*% INCLUDE INI_UNP_FLD_MOR OF 'COLBMMOR.TXT'
           MOVE FRM-CPIM-UBY TO FRM-INF-DES-ATR IN MOR-FLD.
           MOVE FRM-CPIM-UBY TO FRM-INF-HAS-ATR IN MOR-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-OFI-ATR IN MOR-FLD.
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(1).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(2).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(3).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(4).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(5).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(6).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(7).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(8).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(9).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(10).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(11).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(12).
           PERFORM UNP-MOR-MOR.
*% INCLUDE FIN_UNP_FLD_MOR OF 'COLBMMOR.TXT'
       FIN-UNP-FLD-MOR.
           EXIT.
       
       UNP-MOR-MOR SECTION.
       INI-UNP-MOR-MOR.
*% INCLUDE INI_UNP_MOR_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_UNP_MOR_MOR OF 'COLBMMOR.TXT'
       FIN-UNP-MOR-MOR.
           EXIT.
       
       VAL-KEY-MOR SECTION.
       INI-VAL-KEY-MOR.
           PERFORM VAL-IKY-MOR.
           PERFORM VAL-MKY-MOR.
           PERFORM VAL-AKY-MOR.
       FIN-VAL-KEY-MOR.
           EXIT.
       
       VAL-IKY-MOR SECTION.
       INI-VAL-IKY-MOR.
*% INCLUDE INI_VAL_IKY_MOR OF 'COLBMMOR.TXT'
           PERFORM VAL-MOR-COD-OFI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO MOR-COD-OFI-LEN IN MOR-FLD
                GO TO FIN-VAL-IKY-MOR.
           PERFORM VAL-FRM-COD-EJE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-EJE-LEN IN MOR-FLD
                GO TO FIN-VAL-IKY-MOR.
*% INCLUDE FIN_VAL_IKY_MOR OF 'COLBMMOR.TXT'
       FIN-VAL-IKY-MOR.
           EXIT.
       
       VAL-MKY-MOR SECTION.
       INI-VAL-MKY-MOR.
*% INCLUDE INI_VAL_MKY_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_VAL_MKY_MOR OF 'COLBMMOR.TXT'
       FIN-VAL-MKY-MOR.
           EXIT.
       
       VAL-AKY-MOR SECTION.
       INI-VAL-AKY-MOR.
*% INCLUDE INI_VAL_AKY_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_VAL_AKY_MOR OF 'COLBMMOR.TXT'
       FIN-VAL-AKY-MOR.
           EXIT.
       
       VAL-MOR-COD-OFI SECTION.
       INI-VAL-MOR-COD-OFI.
*% LBL = "MOR_COD_OFI OF 'COLBMMOR.TXT'"
*% INCLUDE INI_MOR_COD_OFI OF 'COLBMMOR.TXT'
*% INCLUDE MOR_COD_OFI OF 'COLBMMOR.TXT'
*% BUF = "MOR-FLD"
*% SIS = "TAB"
*% TAB = "OFI "
*% FLD = "MOR-COD-OFI"
*% GLS = "FRM-GLS-OFI"
*% TIENE_GLOSA = TRUE
*% LGL = 15
*% INCLUDE 'VALOFI          '
*% INCLUDE FIN_MOR_COD_OFI OF 'COLBMMOR.TXT'
       FIN-VAL-MOR-COD-OFI.
           EXIT.
       
       VAL-FRM-COD-EJE SECTION.
       INI-VAL-FRM-COD-EJE.
*% LBL = "FRM_COD_EJE OF 'COLBMMOR.TXT'"
*% INCLUDE INI_FRM_COD_EJE OF 'COLBMMOR.TXT'
           IF FRM-COD-EJE IN MOR-FLD NOT > SPACES
              MOVE SPACES TO FRM-COD-EJE IN MOR-FLD.
*% INCLUDE FRM_COD_EJE OF 'COLBMMOR.TXT'
*% INCLUDE FIN_FRM_COD_EJE OF 'COLBMMOR.TXT'
       FIN-VAL-FRM-COD-EJE.
           EXIT.
       
       VAL-CON-KEY-MOR SECTION.
       INI-VAL-CON-KEY-MOR.
*% INCLUDE INI_VAL_CON_KEY_MOR OF 'COLBMMOR.TXT'
           IF     MOR-COD-OFI IN MOR-FLD NOT > ZEROES 
              AND FRM-COD-EJE IN MOR-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-MOR.
*% INCLUDE FIN_VAL_CON_KEY_MOR OF 'COLBMMOR.TXT'
       FIN-VAL-CON-KEY-MOR.
           EXIT.
       
       VAL-CON-IKY-MOR SECTION.
       INI-VAL-CON-IKY-MOR.
*% INCLUDE INI_VAL_CON_IKY_MOR OF 'COLBMMOR.TXT'
           IF     MOR-COD-OFI IN MOR-FLD NOT > ZEROES 
              AND FRM-COD-EJE IN MOR-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-MOR.
*% INCLUDE FIN_VAL_CON_IKY_MOR OF 'COLBMMOR.TXT'
       FIN-VAL-CON-IKY-MOR.
           EXIT.
       
       VAL-NUM-KEY-MOR SECTION.
       INI-VAL-NUM-KEY-MOR.
           PERFORM VAL-NUM-IKY-MOR.
           PERFORM VAL-NUM-MKY-MOR.
           PERFORM VAL-NUM-AKY-MOR.
       FIN-VAL-NUM-KEY-MOR.
           EXIT.
       
       VAL-NUM-IKY-MOR SECTION.
       INI-VAL-NUM-IKY-MOR.
*% INCLUDE INI_VAL_NUM_IKY_MOR OF 'COLBMMOR.TXT'
       VAL-NUM-MOR-COD-OFI.
*% BUF = "MOR-FLD"
*% FLD = "MOR-COD-OFI"
*% ENT = "3"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-MOR.
*% INCLUDE FIN_VAL_NUM_IKY_MOR OF 'COLBMMOR.TXT'
       FIN-VAL-NUM-IKY-MOR.
           EXIT.
       
       VAL-NUM-MKY-MOR SECTION.
       INI-VAL-NUM-MKY-MOR.
*% INCLUDE INI_VAL_NUM_MKY_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_MOR OF 'COLBMMOR.TXT'
       FIN-VAL-NUM-MKY-MOR.
           EXIT.
       
       VAL-NUM-AKY-MOR SECTION.
       INI-VAL-NUM-AKY-MOR.
*% INCLUDE INI_VAL_NUM_AKY_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_MOR OF 'COLBMMOR.TXT'
       FIN-VAL-NUM-AKY-MOR.
           EXIT.
       
       VAL-NUM-FLD-MOR SECTION.
       INI-VAL-NUM-FLD-MOR.
*% INCLUDE INI_VAL_NUM_FLD_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_VAL_NUM_FLD_MOR OF 'COLBMMOR.TXT'
       FIN-VAL-NUM-FLD-MOR.
           EXIT.
       
       EDT-KEY-MOR SECTION.
       INI-EDT-KEY-MOR.
           PERFORM EDT-IKY-MOR.
           PERFORM EDT-MKY-MOR.
           PERFORM EDT-AKY-MOR.
*% INCLUDE EDT_KEY_MOR OF 'COLBMMOR.TXT'
       FIN-EDT-KEY-MOR.
           EXIT.
       
       EDT-IKY-MOR SECTION.
       INI-EDT-IKY-MOR.
*% INCLUDE EDT_IKY_MOR OF 'COLBMMOR.TXT'
           IF MOR-COD-OFI-LEN IN MOR-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO MOR-COD-OFI-LEN IN MOR-FLD
                GO TO FIN-EDT-IKY-MOR.
           MOVE MOR-COD-OFI IN MOR-FLD TO MOR-COD-OFI-EDT IN MOR-EDT.
           MOVE MOR-COD-OFI-EDT IN MOR-EDT TO 
                MOR-COD-OFI-ALF IN MOR-FLD.
*% INCLUDE FIN_EDT_IKY_MOR OF 'COLBMMOR.TXT'
       FIN-EDT-IKY-MOR.
           EXIT.
       
       EDT-MKY-MOR SECTION.
       INI-EDT-MKY-MOR.
*% INCLUDE EDT_MKY_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_EDT_MKY_MOR OF 'COLBMMOR.TXT'
       FIN-EDT-MKY-MOR.
           EXIT.
       
       EDT-AKY-MOR SECTION.
       INI-EDT-AKY-MOR.
*% INCLUDE EDT_AKY_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_EDT_AKY_MOR OF 'COLBMMOR.TXT'
       FIN-EDT-AKY-MOR.
           EXIT.
       
       EDT-FLD-MOR SECTION.
       INI-EDT-FLD-MOR.
*% INCLUDE EDT_FLD_MOR OF 'COLBMMOR.TXT'
*% INCLUDE FIN_EDT_FLD_MOR OF 'COLBMMOR.TXT'
       FIN-EDT-FLD-MOR.
           EXIT.
*% INCLUDE EOF_BQ OF 'COLBMMOR.TXT'
*% INCLUDE EOF OF 'COLBMMOR.TXT'
*% INCLUDE EOF_IBM OF 'COLBMMOR.TXT'
