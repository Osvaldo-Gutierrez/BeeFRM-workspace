*% INI_LBL = TRUE
*% PGM_BU = TRUE
       RET-ALL-DVG SECTION.
       INI-RET-ALL-DVG.
*% INCLUDE INI_RET_ALL_DVG OF 'COLBMDVG.TXT'
           PERFORM KEY-ALL-DVG.
           PERFORM RET-DVG-DVG.
*% INCLUDE FIN_RET_ALL_DVG OF 'COLBMDVG.TXT'
       FIN-RET-ALL-DVG.
           EXIT.
       
       RET-DVG-DVG SECTION.
       INI-RET-DVG-DVG.
*% INCLUDE INI_RET_DVG_DVG OF 'COLBMDVG.TXT'
           MOVE DVG-COD-MVTO IN DVG-FLD TO DVG-COD-MVTO IN DVG.
*% RREG = "DVG"
*% RREG_FRM = "DVG"
*% NOMB = "FPRO"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DVG-NUM-DPRO IN DVG-FLD TO DVG-NUM-DPRO IN DVG.
           MOVE DVG-NUM-MPRO IN DVG-FLD TO DVG-NUM-MPRO IN DVG.
           MOVE DVG-NUM-SPRO IN DVG-FLD TO DVG-NUM-SPRO IN DVG.
           MOVE DVG-NUM-APRO IN DVG-FLD TO DVG-NUM-APRO IN DVG.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DVG-COD-VCOC IN DVG-FLD TO DVG-COD-VCOC IN DVG.
*% RREG = "DVG"
*% RREG_FRM = "DVG"
*% NOMB = "FCTB"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DVG-NUM-DCTB IN DVG-FLD TO DVG-NUM-DCTB IN DVG.
           MOVE DVG-NUM-MCTB IN DVG-FLD TO DVG-NUM-MCTB IN DVG.
           MOVE DVG-NUM-SCTB IN DVG-FLD TO DVG-NUM-SCTB IN DVG.
           MOVE DVG-NUM-ACTB IN DVG-FLD TO DVG-NUM-ACTB IN DVG.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DVG-COD-OFOC IN DVG-FLD TO DVG-COD-OFOC IN DVG.
*% RREG = "DVG"
*% RREG_FRM = "DVG"
*% NOMB = "FREA"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DVG-NUM-DREA IN DVG-FLD TO DVG-NUM-DREA IN DVG.
           MOVE DVG-NUM-MREA IN DVG-FLD TO DVG-NUM-MREA IN DVG.
           MOVE DVG-NUM-SREA IN DVG-FLD TO DVG-NUM-SREA IN DVG.
           MOVE DVG-NUM-AREA IN DVG-FLD TO DVG-NUM-AREA IN DVG.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DVG-COD-TOOC IN DVG-FLD TO DVG-COD-TOOC IN DVG.
*% RREG = "DVG"
*% RREG_FRM = "DVG"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
