       INI-UNP-DBC-CCA.
*% INCLUDE INI_UNP_DBC_CCA OF 'COLBMCCA.TXT'
*% INCLUDE FIN_UNP_DBC_CCA OF 'COLBMCCA.TXT'
       FIN-UNP-DBC-CCA.
           EXIT.
       
       VAL-KEY-CCA SECTION.
       INI-VAL-KEY-CCA.
           PERFORM VAL-IKY-CCA.
           PERFORM VAL-MKY-CCA.
           PERFORM VAL-AKY-CCA.
       FIN-VAL-KEY-CCA.
           EXIT.
       
       VAL-IKY-CCA SECTION.
       INI-VAL-IKY-CCA.
*% INCLUDE INI_VAL_IKY_CCA OF 'COLBMCCA.TXT'
           PERFORM VAL-CYA-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CYA-CAI-IOPC-LEN IN CCA-FLD
                GO TO FIN-VAL-IKY-CCA.
           PERFORM VAL-CYA-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CYA-IIC-IOPC-LEN IN CCA-FLD
                GO TO FIN-VAL-IKY-CCA.
           PERFORM VAL-CYA-NUM-ICAN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CYA-NUM-ICAN-LEN IN CCA-FLD
                GO TO FIN-VAL-IKY-CCA.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * VRF-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * VRF-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DBC-IND-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-IND-ICLI-LEN IN CCA-FLD
                GO TO FIN-VAL-IKY-CCA.
           PERFORM VAL-DBC-GLS-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-GLS-ICLI-LEN IN CCA-FLD
                GO TO FIN-VAL-IKY-CCA.
           PERFORM VAL-DBC-NUM-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-NUM-ICLI-LEN IN CCA-FLD
                GO TO FIN-VAL-IKY-CCA.
           PERFORM VAL-DBC-VRF-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-VRF-ICLI-LEN IN CCA-FLD
                GO TO FIN-VAL-IKY-CCA.
      * IDC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% INCLUDE FIN_VAL_IKY_CCA OF 'COLBMCCA.TXT'
       FIN-VAL-IKY-CCA.
           EXIT.
       
