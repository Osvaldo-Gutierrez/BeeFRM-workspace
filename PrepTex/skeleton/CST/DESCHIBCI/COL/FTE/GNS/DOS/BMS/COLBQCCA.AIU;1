*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CCA"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMCCA.TXT'
       SET-TAG-COLBQCCA SECTION.
       INI-SET-TAG-COLBQCCA.
           MOVE
           '<IDNMOD NOM=$COLBQCCA$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 03:19 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCCA.
           EXIT.
           
       PUT-ALL-CCA SECTION.
       INI-PUT-ALL-CCA.
*% INCLUDE INI_PUT_ALL_CCA OF 'COLBMCCA.TXT'
           PERFORM PUT-OPC-CCA.
           PERFORM PUT-CYA-CCA.
           PERFORM PUT-DBC-CCA.
*% INCLUDE FIN_PUT_ALL_CCA OF 'COLBMCCA.TXT'
       FIN-PUT-ALL-CCA.
           EXIT.
       
       PUT-OPC-CCA SECTION.
       INI-PUT-OPC-CCA.
*% INCLUDE INI_PUT_OPC_CCA OF 'COLBMCCA.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-CCA.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-CCA
               GO TO FIN-PUT-OPC-CCA.
           MOVE OPC-MSC-TVOC IN OPC TO OPC-MSC-TVOC IN CCA-FLD.
           MOVE OPC-COD-TOOC IN OPC TO OPC-COD-TOOC IN CCA-FLD.
           MOVE OPC-COD-OFOC IN OPC TO OPC-COD-OFOC IN CCA-FLD.
           MOVE OPC-COD-VCOC IN OPC TO OPC-COD-VCOC IN CCA-FLD.
*% BUF = "CCA-FLD"
*% SIS = "TAB"
*% TAB = "TVAL"
*% FLD = "OPC-MSC-TVOC"
*% GLS = "FRM-GLS-TVOC"
*% TIENE_GLOSA = TRUE
*% LGL = 13
*% INCLUDE 'BUSMSC'
*% BUF = "CCA-FLD"
*% SIS = "TAB"
*% TAB = "TIO "
*% FLD = "OPC-COD-TOOC"
*% GLS = "FRM-GLS-TOOC"
*% TIENE_GLOSA = TRUE
*% LGL = 14
*% INCLUDE 'BUSTAB'
*% BUF = "CCA-FLD"
*% SIS = "TAB"
*% TAB = "OFI "
*% FLD = "OPC-COD-OFOC"
*% GLS = "FRM-GLS-OFOC"
*% TIENE_GLOSA = TRUE
*% LGL = 14
*% INCLUDE 'BUSOFI          '
*% BUF = "CCA-FLD"
*% SIS = "TAB"
*% TAB = "VLR "
*% FLD = "OPC-COD-VCOC"
*% GLS = "FRM-GLS-VCOC"
*% TIENE_GLOSA = TRUE
*% LGL = 15
*% INCLUDE 'BUSTAB'
*% INCLUDE FIN_PUT_OPC_CCA OF 'COLBMCCA.TXT'
       FIN-PUT-OPC-CCA.
           EXIT.
       
       PUT-CYA-CCA SECTION.
       INI-PUT-CYA-CCA.
*% INCLUDE INI_PUT_CYA_CCA OF 'COLBMCCA.TXT'
           IF PGM-STAT-CYA-NUL
               GO TO FIN-PUT-CYA-CCA.
           IF NOT PGM-STAT-CYA-OKS
               PERFORM CLR-CYA-CCA
               GO TO FIN-PUT-CYA-CCA.
           MOVE CYA-CAI-IOPC IN CYA TO CYA-CAI-IOPC IN CCA-FLD.
           MOVE CYA-IIC-IOPC IN CYA TO CYA-IIC-IOPC IN CCA-FLD.
           MOVE CYA-NUM-ICAN IN CYA TO CYA-NUM-ICAN IN CCA-FLD.
*% INCLUDE FIN_PUT_CYA_CCA OF 'COLBMCCA.TXT'
       FIN-PUT-CYA-CCA.
           EXIT.
       
       PUT-DBC-CCA SECTION.
       INI-PUT-DBC-CCA.
*% INCLUDE INI_PUT_DBC_CCA OF 'COLBMCCA.TXT'
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-CCA.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-CCA
               GO TO FIN-PUT-DBC-CCA.
           MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN CCA-FLD.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN CCA-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN CCA-FLD.
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN CCA-FLD.
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN CCA-FLD.
      * IDC-FIN --> BMCHIBCI001210.SRCAIU 
*% INCLUDE FIN_PUT_DBC_CCA OF 'COLBMCCA.TXT'
       FIN-PUT-DBC-CCA.
           EXIT.
       
       KEY-ALL-CCA SECTION.
       INI-KEY-ALL-CCA.
*% INCLUDE INI_KEY_ALL_CCA OF 'COLBMCCA.TXT'
           PERFORM KEY-OPC-CCA.
           PERFORM KEY-CYA-CCA.
           PERFORM KEY-DBC-CCA.
*% INCLUDE FIN_KEY_ALL_CCA OF 'COLBMCCA.TXT'
       FIN-KEY-ALL-CCA.
           EXIT.
       
       KEY-OPC-CCA SECTION.
       INI-KEY-OPC-CCA.
*% INCLUDE INI_KEY_OPC_CCA OF 'COLBMCCA.TXT'
*% INCLUDE FIN_KEY_OPC_CCA OF 'COLBMCCA.TXT'
       FIN-KEY-OPC-CCA.
           EXIT.
       
       KEY-CYA-CCA SECTION.
       INI-KEY-CYA-CCA.
*% INCLUDE INI_KEY_CYA_CCA OF 'COLBMCCA.TXT'
           MOVE CYA-CAI-IOPC IN CCA-FLD TO CYA-CAI-IOPC IN CYA.
           MOVE CYA-IIC-IOPC IN CCA-FLD TO CYA-IIC-IOPC IN CYA.
           MOVE CYA-NUM-ICAN IN CCA-FLD TO CYA-NUM-ICAN IN CYA.
*% INCLUDE FIN_KEY_CYA_CCA OF 'COLBMCCA.TXT'
       FIN-KEY-CYA-CCA.
           EXIT.
       
       KEY-DBC-CCA SECTION.
       INI-KEY-DBC-CCA.
*% INCLUDE INI_KEY_DBC_CCA OF 'COLBMCCA.TXT'
           MOVE DBC-GLS-NOMC IN CCA-FLD TO DBC-GLS-NOMC IN DBC.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN CCA-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN CCA-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN CCA-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN CCA-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_KEY_DBC_CCA OF 'COLBMCCA.TXT'
       FIN-KEY-DBC-CCA.
           EXIT.
       
       SCR-CLRA-CCA SECTION.
       INI-SCR-CLRA-CCA.
*% INCLUDE INI_SCR_CLRA_CCA OF 'COLBMCCA.TXT'
           PERFORM CLR-KEY-CCA.
           PERFORM CLR-FLD-CCA.
*% INCLUDE FIN_SCR_CLRA_CCA OF 'COLBMCCA.TXT'
       FIN-SCR-CLRA-CCA.
           EXIT.
       
       CLR-KEY-CCA SECTION.
       INI-CLR-KEY-CCA.
*% INCLUDE INI_CLR_KEY_CCA OF 'COLBMCCA.TXT'
           MOVE SPACES TO CYA-CAI-IOPC IN CCA-FLD.
           MOVE ZEROES TO CYA-IIC-IOPC IN CCA-FLD.
           MOVE SPACES TO CYA-NUM-ICAN IN CCA-FLD.
           MOVE SPACES TO DBC-GLS-NOMC IN CCA-FLD.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN CCA-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN CCA-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN CCA-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN CCA-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_KEY_CCA OF 'COLBMCCA.TXT'
       FIN-CLR-KEY-CCA.
           EXIT.
       
       CLR-FLD-CCA SECTION.
       INI-CLR-FLD-CCA.
*% INCLUDE INI_CLR_FLD_CCA OF 'COLBMCCA.TXT'
           MOVE SPACES TO FRM-GLS-TVOC IN CCA-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN CCA-FLD.
           MOVE SPACES TO FRM-GLS-OFOC IN CCA-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN CCA-FLD.
           MOVE SPACES TO FRM-GLS-TM01 IN CCA-FLD.
           MOVE SPACES TO FRM-GLS-TM02 IN CCA-FLD.
           MOVE SPACES TO FRM-GLS-TM03 IN CCA-FLD.
           MOVE SPACES TO FRM-GLS-TM04 IN CCA-FLD.
           MOVE ZEROES TO FRM-NUM-ICYA IN CCA-FLD(1).
           MOVE ZEROES TO FRM-NUM-ICYA IN CCA-FLD(2).
           MOVE ZEROES TO FRM-NUM-ICYA IN CCA-FLD(3).
           MOVE ZEROES TO FRM-NUM-ICYA IN CCA-FLD(4).
           MOVE ZEROES TO FRM-NUM-ICYA IN CCA-FLD(5).
           MOVE ZEROES TO FRM-NUM-ICYA IN CCA-FLD(6).
           MOVE ZEROES TO FRM-NUM-ICYA IN CCA-FLD(7).
           MOVE ZEROES TO FRM-NUM-ICYA IN CCA-FLD(8).
           MOVE ZEROES TO FRM-NUM-ICYA IN CCA-FLD(9).
           MOVE ZEROES TO FRM-NUM-ICYA IN CCA-FLD(10).
           MOVE SPACES TO FRM-GLS-TCYA IN CCA-FLD(1).
           MOVE SPACES TO FRM-GLS-TCYA IN CCA-FLD(2).
           MOVE SPACES TO FRM-GLS-TCYA IN CCA-FLD(3).
           MOVE SPACES TO FRM-GLS-TCYA IN CCA-FLD(4).
           MOVE SPACES TO FRM-GLS-TCYA IN CCA-FLD(5).
           MOVE SPACES TO FRM-GLS-TCYA IN CCA-FLD(6).
           MOVE SPACES TO FRM-GLS-TCYA IN CCA-FLD(7).
           MOVE SPACES TO FRM-GLS-TCYA IN CCA-FLD(8).
           MOVE SPACES TO FRM-GLS-TCYA IN CCA-FLD(9).
           MOVE SPACES TO FRM-GLS-TCYA IN CCA-FLD(10).
           MOVE SPACES TO FRM-CAI-ICYA IN CCA-FLD(1).
           MOVE SPACES TO FRM-CAI-ICYA IN CCA-FLD(2).
           MOVE SPACES TO FRM-CAI-ICYA IN CCA-FLD(3).
           MOVE SPACES TO FRM-CAI-ICYA IN CCA-FLD(4).
           MOVE SPACES TO FRM-CAI-ICYA IN CCA-FLD(5).
           MOVE SPACES TO FRM-CAI-ICYA IN CCA-FLD(6).
           MOVE SPACES TO FRM-CAI-ICYA IN CCA-FLD(7).
           MOVE SPACES TO FRM-CAI-ICYA IN CCA-FLD(8).
           MOVE SPACES TO FRM-CAI-ICYA IN CCA-FLD(9).
           MOVE SPACES TO FRM-CAI-ICYA IN CCA-FLD(10).
           MOVE ZEROES TO FRM-IIC-ICYA IN CCA-FLD(1).
           MOVE ZEROES TO FRM-IIC-ICYA IN CCA-FLD(2).
           MOVE ZEROES TO FRM-IIC-ICYA IN CCA-FLD(3).
           MOVE ZEROES TO FRM-IIC-ICYA IN CCA-FLD(4).
           MOVE ZEROES TO FRM-IIC-ICYA IN CCA-FLD(5).
           MOVE ZEROES TO FRM-IIC-ICYA IN CCA-FLD(6).
           MOVE ZEROES TO FRM-IIC-ICYA IN CCA-FLD(7).
           MOVE ZEROES TO FRM-IIC-ICYA IN CCA-FLD(8).
           MOVE ZEROES TO FRM-IIC-ICYA IN CCA-FLD(9).
           MOVE ZEROES TO FRM-IIC-ICYA IN CCA-FLD(10).
           MOVE ZEROES TO FRM-SGV-LIQU IN CCA-FLD(1).
           MOVE ZEROES TO FRM-SGV-LIQU IN CCA-FLD(2).
           MOVE ZEROES TO FRM-SGV-LIQU IN CCA-FLD(3).
           MOVE ZEROES TO FRM-SGV-LIQU IN CCA-FLD(4).
           MOVE ZEROES TO FRM-SGV-LIQU IN CCA-FLD(5).
           MOVE ZEROES TO FRM-SGV-LIQU IN CCA-FLD(6).
           MOVE ZEROES TO FRM-SGV-LIQU IN CCA-FLD(7).
           MOVE ZEROES TO FRM-SGV-LIQU IN CCA-FLD(8).
           MOVE ZEROES TO FRM-SGV-LIQU IN CCA-FLD(9).
           MOVE ZEROES TO FRM-SGV-LIQU IN CCA-FLD(10).
           MOVE ZEROES TO FRM-VAL-VPES IN CCA-FLD(1).
           MOVE ZEROES TO FRM-VAL-VPES IN CCA-FLD(2).
           MOVE ZEROES TO FRM-VAL-VPES IN CCA-FLD(3).
           MOVE ZEROES TO FRM-VAL-VPES IN CCA-FLD(4).
           MOVE ZEROES TO FRM-VAL-VPES IN CCA-FLD(5).
           MOVE ZEROES TO FRM-VAL-VPES IN CCA-FLD(6).
           MOVE ZEROES TO FRM-VAL-VPES IN CCA-FLD(7).
           MOVE ZEROES TO FRM-VAL-VPES IN CCA-FLD(8).
           MOVE ZEROES TO FRM-VAL-VPES IN CCA-FLD(9).
           MOVE ZEROES TO FRM-VAL-VPES IN CCA-FLD(10).
           PERFORM CLR-OPC-CCA.
           PERFORM CLR-CYA-CCA.
           PERFORM CLR-DBC-CCA.
*% INCLUDE FIN_CLR_FLD_CCA OF 'COLBMCCA.TXT'
       FIN-CLR-FLD-CCA.
           EXIT.
       
