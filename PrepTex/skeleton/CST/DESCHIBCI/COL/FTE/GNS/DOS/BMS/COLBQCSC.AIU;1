       EDT-KEY-CSC SECTION.
       INI-EDT-KEY-CSC.
           PERFORM EDT-IKY-CSC.
           PERFORM EDT-MKY-CSC.
           PERFORM EDT-AKY-CSC.
*% INCLUDE EDT_KEY_CSC OF 'COLBMCSC.TXT'
       FIN-EDT-KEY-CSC.
           EXIT.
       
       EDT-IKY-CSC SECTION.
       INI-EDT-IKY-CSC.
*% INCLUDE EDT_IKY_CSC OF 'COLBMCSC.TXT'
           IF OPC-IIC-IOPC-LEN IN CSC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO OPC-IIC-IOPC-LEN IN CSC-FLD
                GO TO FIN-EDT-IKY-CSC.
           MOVE OPC-IIC-IOPC IN CSC-FLD TO OPC-IIC-IOPC-EDT IN CSC-EDT.
           MOVE OPC-IIC-IOPC-EDT IN CSC-EDT TO 
                OPC-IIC-IOPC-ALF IN CSC-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAL"
           IF FRM-VAL-TDES-LEN IN CSC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-TDES-LEN IN CSC-FLD
                GO TO FIN-EDT-IKY-CSC.
           MOVE FRM-VAL-TDES IN CSC-FLD TO FRM-VAL-TDES-EDT IN CSC-EDT.
           MOVE FRM-VAL-TDES-EDT IN CSC-EDT TO 
                FRM-VAL-TDES-ALF IN CSC-FLD.
           IF FRM-VAL-TMOR-LEN IN CSC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-TMOR-LEN IN CSC-FLD
                GO TO FIN-EDT-IKY-CSC.
           MOVE FRM-VAL-TMOR IN CSC-FLD TO FRM-VAL-TMOR-EDT IN CSC-EDT.
           MOVE FRM-VAL-TMOR-EDT IN CSC-EDT TO 
                FRM-VAL-TMOR-ALF IN CSC-FLD.
           IF FRM-VAL-VPAG-LEN IN CSC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-VPAG-LEN IN CSC-FLD
                GO TO FIN-EDT-IKY-CSC.
           MOVE FRM-VAL-VPAG IN CSC-FLD TO FRM-VAL-VPAG-EDT IN CSC-EDT.
           MOVE FRM-VAL-VPAG-EDT IN CSC-EDT TO 
                FRM-VAL-VPAG-ALF IN CSC-FLD.
*% INCLUDE FIN_EDT_IKY_CSC OF 'COLBMCSC.TXT'
       FIN-EDT-IKY-CSC.
           EXIT.
       
       EDT-MKY-CSC SECTION.
       INI-EDT-MKY-CSC.
*% INCLUDE EDT_MKY_CSC OF 'COLBMCSC.TXT'
*% INCLUDE FIN_EDT_MKY_CSC OF 'COLBMCSC.TXT'
       FIN-EDT-MKY-CSC.
           EXIT.
       
       EDT-AKY-CSC SECTION.
       INI-EDT-AKY-CSC.
*% INCLUDE EDT_AKY_CSC OF 'COLBMCSC.TXT'
*% INCLUDE FIN_EDT_AKY_CSC OF 'COLBMCSC.TXT'
       FIN-EDT-AKY-CSC.
           EXIT.
       
       EDT-FLD-CSC SECTION.
       INI-EDT-FLD-CSC.
*% INCLUDE EDT_FLD_CSC OF 'COLBMCSC.TXT'
           IF FRM-VAL-SREN-LEN IN CSC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-SREN-LEN IN CSC-FLD
                GO TO FIN-EDT-FLD-CSC.
           MOVE FRM-VAL-SREN IN CSC-FLD TO FRM-VAL-SREN-EDT IN CSC-EDT.
           MOVE FRM-VAL-SREN-EDT IN CSC-EDT TO 
                FRM-VAL-SREN-ALF IN CSC-FLD.
           IF FRM-VAL-SDIF-LEN IN CSC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-SDIF-LEN IN CSC-FLD
                GO TO FIN-EDT-FLD-CSC.
           MOVE FRM-VAL-SDIF IN CSC-FLD TO FRM-VAL-SDIF-EDT IN CSC-EDT.
           MOVE FRM-VAL-SDIF-EDT IN CSC-EDT TO 
                FRM-VAL-SDIF-ALF IN CSC-FLD.
           IF FRM-VAL-STOT-LEN IN CSC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-STOT-LEN IN CSC-FLD
                GO TO FIN-EDT-FLD-CSC.
           MOVE FRM-VAL-STOT IN CSC-FLD TO FRM-VAL-STOT-EDT IN CSC-EDT.
           MOVE FRM-VAL-STOT-EDT IN CSC-EDT TO 
                FRM-VAL-STOT-ALF IN CSC-FLD.
           IF FRM-NUM-NCAN-LEN IN CSC-FLD(1) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-NCAN-LEN IN CSC-FLD(1)
                GO TO FIN-EDT-FLD-CSC.
           MOVE FRM-NUM-NCAN IN CSC-FLD(1) TO 
                FRM-NUM-NCAN-EDT IN CSC-EDT.
           MOVE FRM-NUM-NCAN-EDT IN CSC-EDT TO 
                FRM-NUM-NCAN-ALF IN CSC-FLD(1).
           IF FRM-NUM-NCAN-LEN IN CSC-FLD(2) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-NCAN-LEN IN CSC-FLD(2)
                GO TO FIN-EDT-FLD-CSC.
           MOVE FRM-NUM-NCAN IN CSC-FLD(2) TO 
                FRM-NUM-NCAN-EDT IN CSC-EDT.
           MOVE FRM-NUM-NCAN-EDT IN CSC-EDT TO 
                FRM-NUM-NCAN-ALF IN CSC-FLD(2).
           IF FRM-NUM-NCAN-LEN IN CSC-FLD(3) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-NCAN-LEN IN CSC-FLD(3)
                GO TO FIN-EDT-FLD-CSC.
           MOVE FRM-NUM-NCAN IN CSC-FLD(3) TO 
                FRM-NUM-NCAN-EDT IN CSC-EDT.
           MOVE FRM-NUM-NCAN-EDT IN CSC-EDT TO 
                FRM-NUM-NCAN-ALF IN CSC-FLD(3).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVEN"
           IF FRM-VAL-CAPI-LEN IN CSC-FLD(1) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-CAPI-LEN IN CSC-FLD(1)
                GO TO FIN-EDT-FLD-CSC.
           MOVE FRM-VAL-CAPI IN CSC-FLD(1) TO 
                FRM-VAL-CAPI-EDT IN CSC-EDT.
           MOVE FRM-VAL-CAPI-EDT IN CSC-EDT TO 
                FRM-VAL-CAPI-ALF IN CSC-FLD(1).
           IF FRM-VAL-CAPI-LEN IN CSC-FLD(2) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-CAPI-LEN IN CSC-FLD(2)
                GO TO FIN-EDT-FLD-CSC.
           MOVE FRM-VAL-CAPI IN CSC-FLD(2) TO 
                FRM-VAL-CAPI-EDT IN CSC-EDT.
           MOVE FRM-VAL-CAPI-EDT IN CSC-EDT TO 
                FRM-VAL-CAPI-ALF IN CSC-FLD(2).
           IF FRM-VAL-CAPI-LEN IN CSC-FLD(3) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-CAPI-LEN IN CSC-FLD(3)
                GO TO FIN-EDT-FLD-CSC.
           MOVE FRM-VAL-CAPI IN CSC-FLD(3) TO 
                FRM-VAL-CAPI-EDT IN CSC-EDT.
           MOVE FRM-VAL-CAPI-EDT IN CSC-EDT TO 
                FRM-VAL-CAPI-ALF IN CSC-FLD(3).
           IF FRM-VAL-INTE-LEN IN CSC-FLD(1) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-INTE-LEN IN CSC-FLD(1)
                GO TO FIN-EDT-FLD-CSC.
           MOVE FRM-VAL-INTE IN CSC-FLD(1) TO 
