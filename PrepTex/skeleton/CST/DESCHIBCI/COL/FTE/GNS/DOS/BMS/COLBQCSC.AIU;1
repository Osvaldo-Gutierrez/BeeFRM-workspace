      *
      * COLBQCSC
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:42 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CSC"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMCSC.TXT'
       SET-TAG-COLBQCSC SECTION.
       INI-SET-TAG-COLBQCSC.
           MOVE
           '<IDNMOD NOM=$COLBQCSC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:42 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCSC.
           EXIT.
           
       PUT-ALL-CSC SECTION.
       INI-PUT-ALL-CSC.
*% INCLUDE INI_PUT_ALL_CSC OF 'COLBMCSC.TXT'
           PERFORM PUT-OPC-CSC.
*% INCLUDE FIN_PUT_ALL_CSC OF 'COLBMCSC.TXT'
       FIN-PUT-ALL-CSC.
           EXIT.
       
       PUT-OPC-CSC SECTION.
       INI-PUT-OPC-CSC.
*% INCLUDE INI_PUT_OPC_CSC OF 'COLBMCSC.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-CSC.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-CSC
               GO TO FIN-PUT-OPC-CSC.
           MOVE OPC-CAI-IOPC IN OPC TO OPC-CAI-IOPC IN CSC-FLD.
           MOVE OPC-IIC-IOPC IN OPC TO OPC-IIC-IOPC IN CSC-FLD.
*% INCLUDE FIN_PUT_OPC_CSC OF 'COLBMCSC.TXT'
       FIN-PUT-OPC-CSC.
           EXIT.
       
       KEY-ALL-CSC SECTION.
       INI-KEY-ALL-CSC.
*% INCLUDE INI_KEY_ALL_CSC OF 'COLBMCSC.TXT'
           PERFORM KEY-OPC-CSC.
*% INCLUDE FIN_KEY_ALL_CSC OF 'COLBMCSC.TXT'
       FIN-KEY-ALL-CSC.
           EXIT.
       
       KEY-OPC-CSC SECTION.
       INI-KEY-OPC-CSC.
*% INCLUDE INI_KEY_OPC_CSC OF 'COLBMCSC.TXT'
           MOVE OPC-CAI-IOPC IN CSC-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE OPC-IIC-IOPC IN CSC-FLD TO OPC-IIC-IOPC IN OPC.
*% INCLUDE FIN_KEY_OPC_CSC OF 'COLBMCSC.TXT'
       FIN-KEY-OPC-CSC.
           EXIT.
       
       SCR-CLRA-CSC SECTION.
       INI-SCR-CLRA-CSC.
*% INCLUDE INI_SCR_CLRA_CSC OF 'COLBMCSC.TXT'
           PERFORM CLR-KEY-CSC.
           PERFORM CLR-FLD-CSC.
*% INCLUDE FIN_SCR_CLRA_CSC OF 'COLBMCSC.TXT'
       FIN-SCR-CLRA-CSC.
           EXIT.
       
       CLR-KEY-CSC SECTION.
       INI-CLR-KEY-CSC.
*% INCLUDE INI_CLR_KEY_CSC OF 'COLBMCSC.TXT'
           MOVE SPACES TO OPC-CAI-IOPC IN CSC-FLD.
           MOVE ZEROES TO OPC-IIC-IOPC IN CSC-FLD.
           MOVE SPACES TO FRM-MSC-CANC IN CSC-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAL"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FCAL IN CSC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-VAL-TDES IN CSC-FLD.
           MOVE ZEROES TO FRM-VAL-TMOR IN CSC-FLD.
           MOVE ZEROES TO FRM-VAL-VPAG IN CSC-FLD.
*% INCLUDE FIN_CLR_KEY_CSC OF 'COLBMCSC.TXT'
       FIN-CLR-KEY-CSC.
           EXIT.
       
       CLR-FLD-CSC SECTION.
       INI-CLR-FLD-CSC.
*% INCLUDE INI_CLR_FLD_CSC OF 'COLBMCSC.TXT'
           MOVE ZEROES TO FRM-VAL-SREN IN CSC-FLD.
           MOVE ZEROES TO FRM-VAL-SDIF IN CSC-FLD.
           MOVE ZEROES TO FRM-VAL-STOT IN CSC-FLD.
           MOVE SPACES TO FRM-GLS-TCAN IN CSC-FLD(1).
           MOVE SPACES TO FRM-GLS-TCAN IN CSC-FLD(2).
           MOVE SPACES TO FRM-GLS-TCAN IN CSC-FLD(3).
           MOVE ZEROES TO FRM-NUM-NCAN IN CSC-FLD(1).
           MOVE ZEROES TO FRM-NUM-NCAN IN CSC-FLD(2).
           MOVE ZEROES TO FRM-NUM-NCAN IN CSC-FLD(3).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FVEN IN CSC-FLD(1).
           MOVE ZEROES TO FRM-FEC-FVEN IN CSC-FLD(2).
           MOVE ZEROES TO FRM-FEC-FVEN IN CSC-FLD(3).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-VAL-CAPI IN CSC-FLD(1).
           MOVE ZEROES TO FRM-VAL-CAPI IN CSC-FLD(2).
           MOVE ZEROES TO FRM-VAL-CAPI IN CSC-FLD(3).
           MOVE ZEROES TO FRM-VAL-INTE IN CSC-FLD(1).
           MOVE ZEROES TO FRM-VAL-INTE IN CSC-FLD(2).
           MOVE ZEROES TO FRM-VAL-INTE IN CSC-FLD(3).
           MOVE ZEROES TO FRM-VAL-MORA IN CSC-FLD(1).
           MOVE ZEROES TO FRM-VAL-MORA IN CSC-FLD(2).
           MOVE ZEROES TO FRM-VAL-MORA IN CSC-FLD(3).
           MOVE ZEROES TO FRM-VAL-HONO IN CSC-FLD(1).
           MOVE ZEROES TO FRM-VAL-HONO IN CSC-FLD(2).
           MOVE ZEROES TO FRM-VAL-HONO IN CSC-FLD(3).
           MOVE ZEROES TO FRM-VAL-IMPT IN CSC-FLD(1).
           MOVE ZEROES TO FRM-VAL-IMPT IN CSC-FLD(2).
           MOVE ZEROES TO FRM-VAL-IMPT IN CSC-FLD(3).
           MOVE ZEROES TO FRM-VAL-GAST IN CSC-FLD(1).
           MOVE ZEROES TO FRM-VAL-GAST IN CSC-FLD(2).
           MOVE ZEROES TO FRM-VAL-GAST IN CSC-FLD(3).
           MOVE ZEROES TO FRM-VAL-COMI IN CSC-FLD(1).
           MOVE ZEROES TO FRM-VAL-COMI IN CSC-FLD(2).
           MOVE ZEROES TO FRM-VAL-COMI IN CSC-FLD(3).
           MOVE ZEROES TO FRM-VAL-RENO IN CSC-FLD(1).
           MOVE ZEROES TO FRM-VAL-RENO IN CSC-FLD(2).
           MOVE ZEROES TO FRM-VAL-RENO IN CSC-FLD(3).
           MOVE ZEROES TO FRM-VAL-DIFE IN CSC-FLD(1).
           MOVE ZEROES TO FRM-VAL-DIFE IN CSC-FLD(2).
           MOVE ZEROES TO FRM-VAL-DIFE IN CSC-FLD(3).
           MOVE ZEROES TO FRM-VAL-SEGU IN CSC-FLD(1).
           MOVE ZEROES TO FRM-VAL-SEGU IN CSC-FLD(2).
           MOVE ZEROES TO FRM-VAL-SEGU IN CSC-FLD(3).
           MOVE ZEROES TO FRM-VAL-TOTA IN CSC-FLD(1).
           MOVE ZEROES TO FRM-VAL-TOTA IN CSC-FLD(2).
           MOVE ZEROES TO FRM-VAL-TOTA IN CSC-FLD(3).
           PERFORM CLR-OPC-CSC.
*% INCLUDE FIN_CLR_FLD_CSC OF 'COLBMCSC.TXT'
       FIN-CLR-FLD-CSC.
           EXIT.
       
       CLR-OPC-CSC SECTION.
       INI-CLR-OPC-CSC.
*% INCLUDE INI_CLR_OPC_CSC OF 'COLBMCSC.TXT'
*% INCLUDE FIN_CLR_OPC_CSC OF 'COLBMCSC.TXT'
       FIN-CLR-OPC-CSC.
           EXIT.
       
       FST-KEY-CSC SECTION.
       INI-FST-KEY-CSC.
*% INCLUDE INI_FST_KEY_CSC OF 'COLBMCSC.TXT'
           MOVE -1 TO OPC-CAI-IOPC-LEN IN CSC-FLD.
*% INCLUDE FIN_FST_KEY_CSC OF 'COLBMCSC.TXT'
       FIN-FST-KEY-CSC.
           EXIT.
       
       FST-FLD-CSC SECTION.
       INI-FST-FLD-CSC.
*% INCLUDE INI_FST_FLD_CSC OF 'COLBMCSC.TXT'
*% INCLUDE FIN_FST_FLD_CSC OF 'COLBMCSC.TXT'
       FIN-FST-FLD-CSC.
           EXIT.
       
       PRO-KEY-CSC SECTION.
       INI-PRO-KEY-CSC.
*% INCLUDE INI_PRO_KEY_CSC OF 'COLBMCSC.TXT'
           MOVE FRM-CPIM-AEY TO FRM-MSC-CANC-ATR IN CSC-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAL"
           MOVE FRM-CPIM-AEY TO FRM-FEC-FCAL-ATR IN CSC-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VAL-TMOR-ATR IN CSC-FLD.
           PERFORM PRO-IKY-CSC.
           PERFORM PRO-AKY-CSC.
           PERFORM PRO-MKY-CSC.
*% INCLUDE FIN_PRO_KEY_CSC OF 'COLBMCSC.TXT'
       FIN-PRO-KEY-CSC.
           EXIT.
       
       PRO-IKY-CSC SECTION.
       INI-PRO-IKY-CSC.
*% INCLUDE INI_PRO_IKY_CSC OF 'COLBMCSC.TXT'
           MOVE FRM-CPIM-AEY TO OPC-CAI-IOPC-ATR IN CSC-FLD.
           MOVE FRM-CPIM-AEY TO OPC-IIC-IOPC-ATR IN CSC-FLD.
*% INCLUDE FIN_PRO_IKY_CSC OF 'COLBMCSC.TXT'
       FIN-PRO-IKY-CSC.
           EXIT.
       
       PRO-MKY-CSC SECTION.
       INI-PRO-MKY-CSC.
*% INCLUDE INI_PRO_MKY_CSC OF 'COLBMCSC.TXT'
*% INCLUDE FIN_PRO_MKY_CSC OF 'COLBMCSC.TXT'
       FIN-PRO-MKY-CSC.
           EXIT.
       
       PRO-AKY-CSC SECTION.
       INI-PRO-AKY-CSC.
*% INCLUDE INI_PRO_AKY_CSC OF 'COLBMCSC.TXT'
*% INCLUDE FIN_PRO_AKY_CSC OF 'COLBMCSC.TXT'
       FIN-PRO-AKY-CSC.
           EXIT.
       
       UNP-KEY-CSC SECTION.
       INI-UNP-KEY-CSC.
*% INCLUDE INI_UNP_KEY_CSC OF 'COLBMCSC.TXT'
           MOVE FRM-CPIM-UBY TO FRM-MSC-CANC-ATR IN CSC-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAL"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FCAL-ATR IN CSC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-VAL-TMOR-ATR IN CSC-FLD.
           PERFORM UNP-IKY-CSC.
           PERFORM UNP-AKY-CSC.
           PERFORM UNP-MKY-CSC.
*% INCLUDE FIN_UNP_KEY_CSC OF 'COLBMCSC.TXT'
       FIN-UNP-KEY-CSC.
           EXIT.
       
       UNP-IKY-CSC SECTION.
       INI-UNP-IKY-CSC.
*% INCLUDE INI_UNP_IKY_CSC OF 'COLBMCSC.TXT'
           MOVE FRM-CPIM-UBY TO OPC-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO OPC-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-MSC-CANC-ATR.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAL"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FCAL-ATR IN CSC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-VAL-TMOR-ATR.
*% INCLUDE FIN_UNP_IKY_CSC OF 'COLBMCSC.TXT'
       FIN-UNP-IKY-CSC.
           EXIT.
       
       UNP-MKY-CSC SECTION.
       INI-UNP-MKY-CSC.
*% INCLUDE INI_UNP_MKY_CSC OF 'COLBMCSC.TXT'
*% INCLUDE FIN_UNP_MKY_CSC OF 'COLBMCSC.TXT'
       FIN-UNP-MKY-CSC.
           EXIT.
       
       UNP-AKY-CSC SECTION.
       INI-UNP-AKY-CSC.
*% INCLUDE INI_UNP_AKY_CSC OF 'COLBMCSC.TXT'
*% INCLUDE FIN_UNP_AKY_CSC OF 'COLBMCSC.TXT'
       FIN-UNP-AKY-CSC.
           EXIT.
       
       UNP-FLD-CSC SECTION.
       INI-UNP-FLD-CSC.
*% INCLUDE INI_UNP_FLD_CSC OF 'COLBMCSC.TXT'
           PERFORM UNP-OPC-CSC.
*% INCLUDE FIN_UNP_FLD_CSC OF 'COLBMCSC.TXT'
       FIN-UNP-FLD-CSC.
           EXIT.
       
       UNP-OPC-CSC SECTION.
       INI-UNP-OPC-CSC.
*% INCLUDE INI_UNP_OPC_CSC OF 'COLBMCSC.TXT'
*% INCLUDE FIN_UNP_OPC_CSC OF 'COLBMCSC.TXT'
       FIN-UNP-OPC-CSC.
           EXIT.
       
       VAL-KEY-CSC SECTION.
       INI-VAL-KEY-CSC.
           PERFORM VAL-IKY-CSC.
           PERFORM VAL-MKY-CSC.
           PERFORM VAL-AKY-CSC.
       FIN-VAL-KEY-CSC.
           EXIT.
       
       VAL-IKY-CSC SECTION.
       INI-VAL-IKY-CSC.
*% INCLUDE INI_VAL_IKY_CSC OF 'COLBMCSC.TXT'
           PERFORM VAL-OPC-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-CAI-IOPC-LEN IN CSC-FLD
                GO TO FIN-VAL-IKY-CSC.
           PERFORM VAL-OPC-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-IIC-IOPC-LEN IN CSC-FLD
                GO TO FIN-VAL-IKY-CSC.
           PERFORM VAL-FRM-MSC-CANC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-MSC-CANC-LEN IN CSC-FLD
                GO TO FIN-VAL-IKY-CSC.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAL"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FCAL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FCAL-LEN IN CSC-FLD
                GO TO FIN-VAL-IKY-CSC.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-VAL-TDES.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-TDES-LEN IN CSC-FLD
                GO TO FIN-VAL-IKY-CSC.
           PERFORM VAL-FRM-VAL-TMOR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-TMOR-LEN IN CSC-FLD
                GO TO FIN-VAL-IKY-CSC.
           PERFORM VAL-FRM-VAL-VPAG.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-VPAG-LEN IN CSC-FLD
                GO TO FIN-VAL-IKY-CSC.
*% INCLUDE FIN_VAL_IKY_CSC OF 'COLBMCSC.TXT'
       FIN-VAL-IKY-CSC.
           EXIT.
       
       VAL-MKY-CSC SECTION.
       INI-VAL-MKY-CSC.
*% INCLUDE INI_VAL_MKY_CSC OF 'COLBMCSC.TXT'
*% INCLUDE FIN_VAL_MKY_CSC OF 'COLBMCSC.TXT'
       FIN-VAL-MKY-CSC.
           EXIT.
       
       VAL-AKY-CSC SECTION.
       INI-VAL-AKY-CSC.
*% INCLUDE INI_VAL_AKY_CSC OF 'COLBMCSC.TXT'
*% INCLUDE FIN_VAL_AKY_CSC OF 'COLBMCSC.TXT'
       FIN-VAL-AKY-CSC.
           EXIT.
       
       VAL-OPC-CAI-IOPC SECTION.
       INI-VAL-OPC-CAI-IOPC.
*% LBL = "OPC_CAI_IOPC OF 'COLBMCSC.TXT'"
*% INCLUDE INI_OPC_CAI_IOPC OF 'COLBMCSC.TXT'
           IF OPC-CAI-IOPC IN CSC-FLD NOT > SPACES
              MOVE SPACES TO OPC-CAI-IOPC IN CSC-FLD.
*% INCLUDE OPC_CAI_IOPC OF 'COLBMCSC.TXT'
*% INCLUDE FIN_OPC_CAI_IOPC OF 'COLBMCSC.TXT'
       FIN-VAL-OPC-CAI-IOPC.
           EXIT.
       
       VAL-OPC-IIC-IOPC SECTION.
       INI-VAL-OPC-IIC-IOPC.
*% LBL = "OPC_IIC_IOPC OF 'COLBMCSC.TXT'"
*% INCLUDE INI_OPC_IIC_IOPC OF 'COLBMCSC.TXT'
*% INCLUDE OPC_IIC_IOPC OF 'COLBMCSC.TXT'
*% INCLUDE FIN_OPC_IIC_IOPC OF 'COLBMCSC.TXT'
       FIN-VAL-OPC-IIC-IOPC.
           EXIT.
       
       VAL-FRM-MSC-CANC SECTION.
       INI-VAL-FRM-MSC-CANC.
*% LBL = "FRM_MSC_CANC OF 'COLBMCSC.TXT'"
*% INCLUDE INI_FRM_MSC_CANC OF 'COLBMCSC.TXT'
           IF FRM-MSC-CANC IN CSC-FLD NOT > SPACES
              MOVE SPACES TO FRM-MSC-CANC IN CSC-FLD.
*% INCLUDE FRM_MSC_CANC OF 'COLBMCSC.TXT'
*% INCLUDE FIN_FRM_MSC_CANC OF 'COLBMCSC.TXT'
       FIN-VAL-FRM-MSC-CANC.
           EXIT.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAL"
*% OBL = "S"
*% LBL = "FRM_FEC_FCAL OF 'COLBMCSC.TXT'"
*% IZR = "S"
*% ZRO = "S"
*% BUF = "CSC-FLD"
*% FLD = "FRM-FEC-FCAL"
*% SN  = "S"
*% HOY = "LE"
*% HBL = "S"
*% TIPO = "FEC"
*% FRM_IFLD = ""
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-FRM-FEC-FCAL SECTION.
       INI-VAL-FRM-FEC-FCAL.
           IF FRM-FEC-FCAL IN CSC-FLD  NOT > ZEROES
              MOVE ZEROES TO FRM-FEC-FCAL IN CSC-FLD.
*% INCLUDE 'SETFEC'
*% INCLUDE INI_FRM_FEC_FCAL OF 'COLBMCSC.TXT'
*% INCLUDE 'PROFEC'
*% INCLUDE FRM_FEC_FCAL OF 'COLBMCSC.TXT'
*% INCLUDE FIN_FRM_FEC_FCAL OF 'COLBMCSC.TXT'
       FIN-VAL-FRM-FEC-FCAL.
           EXIT.
      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-FRM-VAL-TDES SECTION.
       INI-VAL-FRM-VAL-TDES.
*% LBL = "FRM_VAL_TDES OF 'COLBMCSC.TXT'"
*% INCLUDE INI_FRM_VAL_TDES OF 'COLBMCSC.TXT'
*% INCLUDE FRM_VAL_TDES OF 'COLBMCSC.TXT'
*% INCLUDE FIN_FRM_VAL_TDES OF 'COLBMCSC.TXT'
       FIN-VAL-FRM-VAL-TDES.
           EXIT.
       
       VAL-FRM-VAL-TMOR SECTION.
       INI-VAL-FRM-VAL-TMOR.
*% LBL = "FRM_VAL_TMOR OF 'COLBMCSC.TXT'"
*% INCLUDE INI_FRM_VAL_TMOR OF 'COLBMCSC.TXT'
*% INCLUDE FRM_VAL_TMOR OF 'COLBMCSC.TXT'
*% INCLUDE FIN_FRM_VAL_TMOR OF 'COLBMCSC.TXT'
       FIN-VAL-FRM-VAL-TMOR.
           EXIT.
       
       VAL-FRM-VAL-VPAG SECTION.
       INI-VAL-FRM-VAL-VPAG.
*% LBL = "FRM_VAL_VPAG OF 'COLBMCSC.TXT'"
*% INCLUDE INI_FRM_VAL_VPAG OF 'COLBMCSC.TXT'
*% INCLUDE FRM_VAL_VPAG OF 'COLBMCSC.TXT'
*% INCLUDE FIN_FRM_VAL_VPAG OF 'COLBMCSC.TXT'
       FIN-VAL-FRM-VAL-VPAG.
           EXIT.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVEN"
*% IZR = "S"
       
       VAL-CON-KEY-CSC SECTION.
       INI-VAL-CON-KEY-CSC.
*% INCLUDE INI_VAL_CON_KEY_CSC OF 'COLBMCSC.TXT'
           IF     OPC-CAI-IOPC IN CSC-FLD NOT > SPACES 
              AND OPC-IIC-IOPC IN CSC-FLD NOT > ZEROES 
              AND FRM-MSC-CANC IN CSC-FLD NOT > SPACES 
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAL"
              AND FRM-FEC-FCAL IN CSC-FLD NOT > ZEROES
              AND FRM-VAL-TDES IN CSC-FLD NOT > ZEROES 
              AND FRM-VAL-TMOR IN CSC-FLD NOT > ZEROES 
              AND FRM-VAL-VPAG IN CSC-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CSC.
*% INCLUDE FIN_VAL_CON_KEY_CSC OF 'COLBMCSC.TXT'
       FIN-VAL-CON-KEY-CSC.
           EXIT.
       
       VAL-CON-IKY-CSC SECTION.
       INI-VAL-CON-IKY-CSC.
*% INCLUDE INI_VAL_CON_IKY_CSC OF 'COLBMCSC.TXT'
           IF     OPC-CAI-IOPC IN CSC-FLD NOT > SPACES 
              AND OPC-IIC-IOPC IN CSC-FLD NOT > ZEROES 
              AND FRM-MSC-CANC IN CSC-FLD NOT > SPACES 
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAL"
              AND FRM-FEC-FCAL IN CSC-FLD NOT > ZEROES
              AND FRM-VAL-TDES IN CSC-FLD NOT > ZEROES 
              AND FRM-VAL-TMOR IN CSC-FLD NOT > ZEROES 
              AND FRM-VAL-VPAG IN CSC-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CSC.
*% INCLUDE FIN_VAL_CON_IKY_CSC OF 'COLBMCSC.TXT'
       FIN-VAL-CON-IKY-CSC.
           EXIT.
       
       VAL-NUM-KEY-CSC SECTION.
       INI-VAL-NUM-KEY-CSC.
           PERFORM VAL-NUM-IKY-CSC.
           PERFORM VAL-NUM-MKY-CSC.
           PERFORM VAL-NUM-AKY-CSC.
       FIN-VAL-NUM-KEY-CSC.
           EXIT.
       
       VAL-NUM-IKY-CSC SECTION.
       INI-VAL-NUM-IKY-CSC.
*% INCLUDE INI_VAL_NUM_IKY_CSC OF 'COLBMCSC.TXT'
       VAL-NUM-OPC-IIC-IOPC.
*% BUF = "CSC-FLD"
*% FLD = "OPC-IIC-IOPC"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-CSC.
       VAL-NUM-FRM-VAL-TDES.
*% BUF = "CSC-FLD"
*% FLD = "FRM-VAL-TDES"
*% ENT = "3"
*% DCM = "4"
*% NMR = "NS9V"
*% ISG = "S"
*% IZR = "S"
*% INCLUDE 'VALNUM'
