*% FRM = "PLL"
      *
      * COLBUPLL
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:26 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMPLL.TXT'
       SET-TAG-COLBUPLL SECTION.
       INI-SET-TAG-COLBUPLL.
           MOVE
           '<IDNMOD NOM=$COLBUPLL$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:26 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUPLL.
           EXIT.
           
       RET-ALL-PLL SECTION.
       INI-RET-ALL-PLL.
*% INCLUDE INI_RET_ALL_PLL OF 'COLBMPLL.TXT'
           PERFORM KEY-ALL-PLL.
           PERFORM RET-PLL-PLL.
*% INCLUDE FIN_RET_ALL_PLL OF 'COLBMPLL.TXT'
       FIN-RET-ALL-PLL.
           EXIT.
       
       RET-PLL-PLL SECTION.
       INI-RET-PLL-PLL.
*% INCLUDE INI_RET_PLL_PLL OF 'COLBMPLL.TXT'
           MOVE PLL-NUM-RDPL IN PLL-FLD TO PLL-NUM-RDPL IN PLL.
*% RREG = "PLL"
*% RREG_FRM = "PLL"
*% NOMB = "FEMI"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE PLL-NUM-DEMI IN PLL-FLD TO PLL-NUM-DEMI IN PLL.
           MOVE PLL-NUM-MEMI IN PLL-FLD TO PLL-NUM-MEMI IN PLL.
           MOVE PLL-NUM-SEMI IN PLL-FLD TO PLL-NUM-SEMI IN PLL.
           MOVE PLL-NUM-AEMI IN PLL-FLD TO PLL-NUM-AEMI IN PLL.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE PLL-MSC-STAT IN PLL-FLD TO PLL-MSC-STAT IN PLL.
*% RREG = "PLL"
*% RREG_FRM = "PLL"
*% NOMB = "FENV"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE PLL-NUM-DENV IN PLL-FLD TO PLL-NUM-DENV IN PLL.
           MOVE PLL-NUM-MENV IN PLL-FLD TO PLL-NUM-MENV IN PLL.
           MOVE PLL-NUM-SENV IN PLL-FLD TO PLL-NUM-SENV IN PLL.
           MOVE PLL-NUM-AENV IN PLL-FLD TO PLL-NUM-AENV IN PLL.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% RREG = "PLL"
*% RREG_FRM = "PLL"
*% NOMB = "FRCP"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE PLL-NUM-DRCP IN PLL-FLD TO PLL-NUM-DRCP IN PLL.
           MOVE PLL-NUM-MRCP IN PLL-FLD TO PLL-NUM-MRCP IN PLL.
           MOVE PLL-NUM-SRCP IN PLL-FLD TO PLL-NUM-SRCP IN PLL.
           MOVE PLL-NUM-ARCP IN PLL-FLD TO PLL-NUM-ARCP IN PLL.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% RREG = "PLL"
*% RREG_FRM = "PLL"
*% NOMB = "FEST"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE PLL-NUM-DEST IN PLL-FLD TO PLL-NUM-DEST IN PLL.
           MOVE PLL-NUM-MEST IN PLL-FLD TO PLL-NUM-MEST IN PLL.
           MOVE PLL-NUM-SEST IN PLL-FLD TO PLL-NUM-SEST IN PLL.
           MOVE PLL-NUM-AEST IN PLL-FLD TO PLL-NUM-AEST IN PLL.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
