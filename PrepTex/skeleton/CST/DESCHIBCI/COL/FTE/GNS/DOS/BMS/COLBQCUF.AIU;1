*% RREG_FRM = "CUF"
*% NOMB = "FCUF"
           MOVE FRM-CPIM-UBY TO CUF-FEC-FCUF-ATR IN CUF-FLD.
           MOVE FRM-CPIM-UBY TO CUF-COD-UBFD-ATR IN CUF-FLD.
*% INCLUDE FIN_UNP_CUF_CUF OF 'COLBMCUF.TXT'
       FIN-UNP-CUF-CUF.
           EXIT.
       
       UNP-DLC-CUF SECTION.
       INI-UNP-DLC-CUF.
*% INCLUDE INI_UNP_DLC_CUF OF 'COLBMCUF.TXT'
*% INCLUDE FIN_UNP_DLC_CUF OF 'COLBMCUF.TXT'
       FIN-UNP-DLC-CUF.
           EXIT.
       
       VAL-KEY-CUF SECTION.
       INI-VAL-KEY-CUF.
           PERFORM VAL-IKY-CUF.
           PERFORM VAL-MKY-CUF.
           PERFORM VAL-AKY-CUF.
       FIN-VAL-KEY-CUF.
           EXIT.
       
       VAL-IKY-CUF SECTION.
       INI-VAL-IKY-CUF.
*% INCLUDE INI_VAL_IKY_CUF OF 'COLBMCUF.TXT'
           PERFORM VAL-CUF-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CUF-CAI-IOPC-LEN IN CUF-FLD
                GO TO FIN-VAL-IKY-CUF.
           PERFORM VAL-CUF-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CUF-IIC-IOPC-LEN IN CUF-FLD
                GO TO FIN-VAL-IKY-CUF.
           PERFORM VAL-CUF-NUM-IDLC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CUF-NUM-IDLC-LEN IN CUF-FLD
                GO TO FIN-VAL-IKY-CUF.
*% INCLUDE FIN_VAL_IKY_CUF OF 'COLBMCUF.TXT'
       FIN-VAL-IKY-CUF.
           EXIT.
       
       VAL-MKY-CUF SECTION.
       INI-VAL-MKY-CUF.
*% INCLUDE INI_VAL_MKY_CUF OF 'COLBMCUF.TXT'
           PERFORM VAL-FRM-CAI-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-ICLI-LEN IN CUF-FLD
                GO TO FIN-VAL-MKY-CUF.
           PERFORM VAL-FRM-IIC-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-ICLI-LEN IN CUF-FLD
                GO TO FIN-VAL-MKY-CUF.
           PERFORM VAL-FRM-CAI-IAVA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-IAVA-LEN IN CUF-FLD
                GO TO FIN-VAL-MKY-CUF.
           PERFORM VAL-FRM-IIC-IAVA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-IAVA-LEN IN CUF-FLD
                GO TO FIN-VAL-MKY-CUF.
*% INCLUDE FIN_VAL_MKY_CUF OF 'COLBMCUF.TXT'
       FIN-VAL-MKY-CUF.
           EXIT.
       
       VAL-AKY-CUF SECTION.
       INI-VAL-AKY-CUF.
*% INCLUDE INI_VAL_AKY_CUF OF 'COLBMCUF.TXT'
           PERFORM VAL-CUF-NUM-ICUF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CUF-NUM-ICUF-LEN IN CUF-FLD
                GO TO FIN-VAL-AKY-CUF.
           PERFORM VAL-FRM-GLS-NOMC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-NOMC-LEN IN CUF-FLD
                GO TO FIN-VAL-AKY-CUF.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * VRF-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * VRF-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-IND-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-IND-ICLI-LEN IN CUF-FLD
                GO TO FIN-VAL-AKY-CUF.
           PERFORM VAL-FRM-GLS-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-GLS-ICLI-LEN IN CUF-FLD
                GO TO FIN-VAL-AKY-CUF.
           PERFORM VAL-FRM-NUM-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-NUM-ICLI-LEN IN CUF-FLD
                GO TO FIN-VAL-AKY-CUF.
           PERFORM VAL-FRM-VRF-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-VRF-ICLI-LEN IN CUF-FLD
                GO TO FIN-VAL-AKY-CUF.
      * IDC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-GLS-NOMA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-NOMA-LEN IN CUF-FLD
                GO TO FIN-VAL-AKY-CUF.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
      * VRF-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * VRF-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
      * IDC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-IND-IAVA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-IND-IAVA-LEN IN CUF-FLD
                GO TO FIN-VAL-AKY-CUF.
           PERFORM VAL-FRM-GLS-IAVA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-GLS-IAVA-LEN IN CUF-FLD
                GO TO FIN-VAL-AKY-CUF.
           PERFORM VAL-FRM-NUM-IAVA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-NUM-IAVA-LEN IN CUF-FLD
                GO TO FIN-VAL-AKY-CUF.
           PERFORM VAL-FRM-VRF-IAVA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-VRF-IAVA-LEN IN CUF-FLD
                GO TO FIN-VAL-AKY-CUF.
      * IDC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% INCLUDE FIN_VAL_AKY_CUF OF 'COLBMCUF.TXT'
       FIN-VAL-AKY-CUF.
           EXIT.
       
