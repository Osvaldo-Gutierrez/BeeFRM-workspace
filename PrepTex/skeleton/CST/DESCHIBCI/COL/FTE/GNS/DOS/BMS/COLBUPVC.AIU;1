*% FRM = "PVC"
      *
      * COLBUPVC
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:46 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMPVC.TXT'
       SET-TAG-COLBUPVC SECTION.
       INI-SET-TAG-COLBUPVC.
           MOVE
           '<IDNMOD NOM=$COLBUPVC$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:46 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUPVC.
           EXIT.
           
       RET-ALL-PVC SECTION.
       INI-RET-ALL-PVC.
*% INCLUDE INI_RET_ALL_PVC OF 'COLBMPVC.TXT'
           PERFORM KEY-ALL-PVC.
           PERFORM RET-LAP-PVC.
           PERFORM RET-PVC-PVC.
*% INCLUDE FIN_RET_ALL_PVC OF 'COLBMPVC.TXT'
       FIN-RET-ALL-PVC.
           EXIT.
       
       RET-LAP-PVC SECTION.
       INI-RET-LAP-PVC.
*% INCLUDE INI_RET_LAP_PVC OF 'COLBMPVC.TXT'
           MOVE LAP-COD-COME IN PVC-FLD TO LAP-COD-COME IN LAP.
*% INCLUDE FIN_RET_LAP_PVC OF 'COLBMPVC.TXT'
       FIN-RET-LAP-PVC.
           EXIT.
       
       RET-PVC-PVC SECTION.
       INI-RET-PVC-PVC.
*% INCLUDE INI_RET_PVC_PVC OF 'COLBMPVC.TXT'
           MOVE PVC-NUM-SVMI IN PVC-FLD TO PVC-NUM-SVMI IN PVC.
           MOVE PVC-IND-SVMI IN PVC-FLD TO PVC-IND-SVMI IN PVC.
           MOVE PVC-NUM-VIMI IN PVC-FLD TO PVC-NUM-VIMI IN PVC.
           MOVE PVC-NUM-SVMA IN PVC-FLD TO PVC-NUM-SVMA IN PVC.
           MOVE PVC-NUM-VIMA IN PVC-FLD TO PVC-NUM-VIMA IN PVC.
           MOVE PVC-IND-SVMA IN PVC-FLD TO PVC-IND-SVMA IN PVC.
           MOVE PVC-NUM-DVMI IN PVC-FLD TO PVC-NUM-DVMI IN PVC.
           MOVE PVC-MSC-TCVE IN PVC-FLD TO PVC-MSC-TCVE IN PVC.
           MOVE PVC-IND-DVMI IN PVC-FLD TO PVC-IND-DVMI IN PVC.
           MOVE PVC-NUM-DVMA IN PVC-FLD TO PVC-NUM-DVMA IN PVC.
           MOVE PVC-COD-VCOC IN PVC-FLD TO PVC-COD-VCOC IN PVC.
           MOVE PVC-IND-DVMA IN PVC-FLD TO PVC-IND-DVMA IN PVC.
           MOVE PVC-VAL-VVMI IN PVC-FLD TO PVC-VAL-VVMI IN PVC.
           MOVE PVC-NUM-SPMI IN PVC-FLD TO PVC-NUM-SPMI IN PVC.
           MOVE PVC-VAL-VVMA IN PVC-FLD TO PVC-VAL-VVMA IN PVC.
           MOVE PVC-IND-SPMI IN PVC-FLD TO PVC-IND-SPMI IN PVC.
           MOVE PVC-NUM-SPMA IN PVC-FLD TO PVC-NUM-SPMA IN PVC.
           MOVE PVC-SGV-FMMI IN PVC-FLD TO PVC-SGV-FMMI IN PVC.
           MOVE PVC-IND-SPMA IN PVC-FLD TO PVC-IND-SPMA IN PVC.
           MOVE PVC-SGV-FMMA IN PVC-FLD TO PVC-SGV-FMMA IN PVC.
           MOVE PVC-SGV-FPMI IN PVC-FLD TO PVC-SGV-FPMI IN PVC.
           MOVE PVC-NUM-DESD IN PVC-FLD TO PVC-NUM-DESD IN PVC.
           MOVE PVC-SGV-FPMA IN PVC-FLD TO PVC-SGV-FPMA IN PVC.
*% INCLUDE FIN_RET_PVC_PVC OF 'COLBMPVC.TXT'
       FIN-RET-PVC-PVC.
           EXIT.
       
       KEY-ALL-PVC SECTION.
       INI-KEY-ALL-PVC.
*% INCLUDE INI_KEY_ALL_PVC OF 'COLBMPVC.TXT'
           PERFORM KEY-LAP-PVC.
           PERFORM KEY-PVC-PVC.
*% INCLUDE FIN_KEY_ALL_PVC OF 'COLBMPVC.TXT'
       FIN-KEY-ALL-PVC.
           EXIT.
       
       KEY-LAP-PVC SECTION.
       INI-KEY-LAP-PVC.
*% INCLUDE INI_KEY_LAP_PVC OF 'COLBMPVC.TXT'
*% INCLUDE FIN_KEY_LAP_PVC OF 'COLBMPVC.TXT'
       FIN-KEY-LAP-PVC.
           EXIT.
       
       KEY-PVC-PVC SECTION.
       INI-KEY-PVC-PVC.
*% INCLUDE INI_KEY_PVC_PVC OF 'COLBMPVC.TXT'
           MOVE PVC-KEY-IPVC IN PVC-FLD TO PVC-KEY-IPVC IN PVC.
*% INCLUDE FIN_KEY_PVC_PVC OF 'COLBMPVC.TXT'
       FIN-KEY-PVC-PVC.
           EXIT.
       
       SCR-CLRA-PVC SECTION.
       INI-SCR-CLRA-PVC.
*% INCLUDE INI_SCR_CLRA_PVC OF 'COLBMPVC.TXT'
           PERFORM CLR-KEY-PVC.
           PERFORM CLR-FLD-PVC.
*% INCLUDE FIN_SCR_CLRA_PVC OF 'COLBMPVC.TXT'
       FIN-SCR-CLRA-PVC.
           EXIT.
       
       CLR-KEY-PVC SECTION.
       INI-CLR-KEY-PVC.
*% INCLUDE INI_CLR_KEY_PVC OF 'COLBMPVC.TXT'
           MOVE SPACES TO PVC-KEY-IPVC IN PVC-FLD.
*% INCLUDE FIN_CLR_KEY_PVC OF 'COLBMPVC.TXT'
       FIN-CLR-KEY-PVC.
           EXIT.
       
       CLR-FLD-PVC SECTION.
       INI-CLR-FLD-PVC.
*% INCLUDE INI_CLR_FLD_PVC OF 'COLBMPVC.TXT'
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FHOY"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FHOY IN PVC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-GLS-SVMI IN PVC-FLD.
           MOVE SPACES TO FRM-GLS-SVMA IN PVC-FLD.
           MOVE SPACES TO FRM-GLS-DVMI IN PVC-FLD.
           MOVE SPACES TO FRM-GLS-TCVE IN PVC-FLD.
           MOVE SPACES TO FRM-GLS-DVMA IN PVC-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN PVC-FLD.
           MOVE SPACES TO FRM-GLS-SPMI IN PVC-FLD.
           MOVE SPACES TO FRM-GLS-SPMA IN PVC-FLD.
           PERFORM CLR-LAP-PVC.
           PERFORM CLR-PVC-PVC.
*% INCLUDE FIN_CLR_FLD_PVC OF 'COLBMPVC.TXT'
       FIN-CLR-FLD-PVC.
           EXIT.
       
       CLR-LAP-PVC SECTION.
       INI-CLR-LAP-PVC.
*% INCLUDE INI_CLR_LAP_PVC OF 'COLBMPVC.TXT'
           MOVE SPACES TO LAP-COD-COME IN PVC-FLD.
*% INCLUDE FIN_CLR_LAP_PVC OF 'COLBMPVC.TXT'
       FIN-CLR-LAP-PVC.
           EXIT.
       
       CLR-PVC-PVC SECTION.
       INI-CLR-PVC-PVC.
*% INCLUDE INI_CLR_PVC_PVC OF 'COLBMPVC.TXT'
           MOVE ZEROES TO PVC-NUM-SVMI IN PVC-FLD.
           MOVE SPACES TO PVC-IND-SVMI IN PVC-FLD.
           MOVE ZEROES TO PVC-NUM-VIMI IN PVC-FLD.
           MOVE ZEROES TO PVC-NUM-SVMA IN PVC-FLD.
           MOVE ZEROES TO PVC-NUM-VIMA IN PVC-FLD.
           MOVE SPACES TO PVC-IND-SVMA IN PVC-FLD.
           MOVE ZEROES TO PVC-NUM-DVMI IN PVC-FLD.
           MOVE SPACES TO PVC-MSC-TCVE IN PVC-FLD.
           MOVE SPACES TO PVC-IND-DVMI IN PVC-FLD.
           MOVE ZEROES TO PVC-NUM-DVMA IN PVC-FLD.
           MOVE SPACES TO PVC-COD-VCOC IN PVC-FLD.
           MOVE SPACES TO PVC-IND-DVMA IN PVC-FLD.
           MOVE ZEROES TO PVC-VAL-VVMI IN PVC-FLD.
           MOVE ZEROES TO PVC-NUM-SPMI IN PVC-FLD.
           MOVE ZEROES TO PVC-VAL-VVMA IN PVC-FLD.
           MOVE SPACES TO PVC-IND-SPMI IN PVC-FLD.
           MOVE ZEROES TO PVC-NUM-SPMA IN PVC-FLD.
           MOVE ZEROES TO PVC-SGV-FMMI IN PVC-FLD.
           MOVE SPACES TO PVC-IND-SPMA IN PVC-FLD.
           MOVE ZEROES TO PVC-SGV-FMMA IN PVC-FLD.
           MOVE ZEROES TO PVC-SGV-FPMI IN PVC-FLD.
           MOVE ZEROES TO PVC-NUM-DESD IN PVC-FLD.
           MOVE ZEROES TO PVC-SGV-FPMA IN PVC-FLD.
*% INCLUDE FIN_CLR_PVC_PVC OF 'COLBMPVC.TXT'
       FIN-CLR-PVC-PVC.
           EXIT.
       
       FST-KEY-PVC SECTION.
       INI-FST-KEY-PVC.
*% INCLUDE INI_FST_KEY_PVC OF 'COLBMPVC.TXT'
           MOVE -1 TO PVC-KEY-IPVC-LEN IN PVC-FLD.
*% INCLUDE FIN_FST_KEY_PVC OF 'COLBMPVC.TXT'
       FIN-FST-KEY-PVC.
           EXIT.
       
       FST-FLD-PVC SECTION.
       INI-FST-FLD-PVC.
*% INCLUDE INI_FST_FLD_PVC OF 'COLBMPVC.TXT'
           MOVE -1 TO LAP-COD-COME-LEN IN PVC-FLD.
*% INCLUDE FIN_FST_FLD_PVC OF 'COLBMPVC.TXT'
       FIN-FST-FLD-PVC.
           EXIT.
       
       PRO-KEY-PVC SECTION.
       INI-PRO-KEY-PVC.
*% INCLUDE INI_PRO_KEY_PVC OF 'COLBMPVC.TXT'
           PERFORM PRO-MKY-PVC.
*% INCLUDE FIN_PRO_KEY_PVC OF 'COLBMPVC.TXT'
       FIN-PRO-KEY-PVC.
           EXIT.
       
       PRO-MKY-PVC SECTION.
       INI-PRO-MKY-PVC.
*% INCLUDE INI_PRO_MKY_PVC OF 'COLBMPVC.TXT'
*% INCLUDE FIN_PRO_MKY_PVC OF 'COLBMPVC.TXT'
       FIN-PRO-MKY-PVC.
           EXIT.
       
       PRO-FLD-PVC SECTION.
       INI-PRO-FLD-PVC.
*% INCLUDE INI_PRO_FLD_PVC OF 'COLBMPVC.TXT'
           PERFORM PRO-LAP-PVC.
           PERFORM PRO-PVC-PVC.
*% INCLUDE FIN_PRO_FLD_PVC OF 'COLBMPVC.TXT'
       FIN-PRO-FLD-PVC.
           EXIT.
       
       PRO-LAP-PVC SECTION.
       INI-PRO-LAP-PVC.
*% INCLUDE INI_PRO_LAP_PVC OF 'COLBMPVC.TXT'
           MOVE FRM-CPIM-AEY TO LAP-COD-COME-ATR IN PVC-FLD.
*% INCLUDE FIN_PRO_LAP_PVC OF 'COLBMPVC.TXT'
       FIN-PRO-LAP-PVC.
           EXIT.
       
       PRO-PVC-PVC SECTION.
       INI-PRO-PVC-PVC.
*% INCLUDE INI_PRO_PVC_PVC OF 'COLBMPVC.TXT'
           MOVE FRM-CPIM-AEY TO PVC-NUM-SVMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-AEY TO PVC-IND-SVMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-AEY TO PVC-NUM-VIMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-AEY TO PVC-NUM-SVMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-AEY TO PVC-NUM-VIMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-AEY TO PVC-IND-SVMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-AEY TO PVC-NUM-DVMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-AEY TO PVC-MSC-TCVE-ATR IN PVC-FLD.
           MOVE FRM-CPIM-AEY TO PVC-IND-DVMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-AEY TO PVC-NUM-DVMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-AEY TO PVC-COD-VCOC-ATR IN PVC-FLD.
           MOVE FRM-CPIM-AEY TO PVC-IND-DVMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-AEY TO PVC-VAL-VVMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-AEY TO PVC-NUM-SPMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-AEY TO PVC-VAL-VVMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-AEY TO PVC-IND-SPMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-AEY TO PVC-NUM-SPMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-AEY TO PVC-SGV-FMMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-AEY TO PVC-IND-SPMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-AEY TO PVC-SGV-FMMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-AEY TO PVC-SGV-FPMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-AEY TO PVC-NUM-DESD-ATR IN PVC-FLD.
           MOVE FRM-CPIM-AEY TO PVC-SGV-FPMA-ATR IN PVC-FLD.
*% INCLUDE FIN_PRO_PVC_PVC OF 'COLBMPVC.TXT'
       FIN-PRO-PVC-PVC.
           EXIT.
       
       UNP-KEY-PVC SECTION.
       INI-UNP-KEY-PVC.
*% INCLUDE INI_UNP_KEY_PVC OF 'COLBMPVC.TXT'
           PERFORM UNP-IKY-PVC.
           PERFORM UNP-AKY-PVC.
           PERFORM UNP-MKY-PVC.
*% INCLUDE FIN_UNP_KEY_PVC OF 'COLBMPVC.TXT'
       FIN-UNP-KEY-PVC.
           EXIT.
       
       UNP-IKY-PVC SECTION.
       INI-UNP-IKY-PVC.
*% INCLUDE INI_UNP_IKY_PVC OF 'COLBMPVC.TXT'
           MOVE FRM-CPIM-UBY TO PVC-KEY-IPVC-ATR.
*% INCLUDE FIN_UNP_IKY_PVC OF 'COLBMPVC.TXT'
       FIN-UNP-IKY-PVC.
           EXIT.
       
       UNP-MKY-PVC SECTION.
       INI-UNP-MKY-PVC.
*% INCLUDE INI_UNP_MKY_PVC OF 'COLBMPVC.TXT'
*% INCLUDE FIN_UNP_MKY_PVC OF 'COLBMPVC.TXT'
       FIN-UNP-MKY-PVC.
           EXIT.
       
       UNP-AKY-PVC SECTION.
       INI-UNP-AKY-PVC.
*% INCLUDE INI_UNP_AKY_PVC OF 'COLBMPVC.TXT'
*% INCLUDE FIN_UNP_AKY_PVC OF 'COLBMPVC.TXT'
       FIN-UNP-AKY-PVC.
           EXIT.
       
       UNP-FLD-PVC SECTION.
       INI-UNP-FLD-PVC.
*% INCLUDE INI_UNP_FLD_PVC OF 'COLBMPVC.TXT'
           PERFORM UNP-LAP-PVC.
           PERFORM UNP-PVC-PVC.
*% INCLUDE FIN_UNP_FLD_PVC OF 'COLBMPVC.TXT'
       FIN-UNP-FLD-PVC.
           EXIT.
       
       UNP-LAP-PVC SECTION.
       INI-UNP-LAP-PVC.
*% INCLUDE INI_UNP_LAP_PVC OF 'COLBMPVC.TXT'
           MOVE FRM-CPIM-UBY TO LAP-COD-COME-ATR IN PVC-FLD.
*% INCLUDE FIN_UNP_LAP_PVC OF 'COLBMPVC.TXT'
       FIN-UNP-LAP-PVC.
           EXIT.
       
       UNP-PVC-PVC SECTION.
       INI-UNP-PVC-PVC.
*% INCLUDE INI_UNP_PVC_PVC OF 'COLBMPVC.TXT'
           MOVE FRM-CPIM-UBY TO PVC-NUM-SVMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-IND-SVMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-NUM-VIMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-NUM-SVMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-NUM-VIMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-IND-SVMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-NUM-DVMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-MSC-TCVE-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-IND-DVMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-NUM-DVMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-COD-VCOC-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-IND-DVMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-VAL-VVMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-NUM-SPMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-VAL-VVMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-IND-SPMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-NUM-SPMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-SGV-FMMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-IND-SPMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-SGV-FMMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-SGV-FPMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-NUM-DESD-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-SGV-FPMA-ATR IN PVC-FLD.
*% INCLUDE FIN_UNP_PVC_PVC OF 'COLBMPVC.TXT'
       FIN-UNP-PVC-PVC.
           EXIT.
       
       VAL-MKY-PVC SECTION.
       INI-VAL-MKY-PVC.
*% INCLUDE INI_VAL_MKY_PVC OF 'COLBMPVC.TXT'
*% INCLUDE FIN_VAL_MKY_PVC OF 'COLBMPVC.TXT'
       FIN-VAL-MKY-PVC.
           EXIT.
       
       VAL-FLD-PVC SECTION.
       INI-VAL-FLD-PVC.
*% INCLUDE INI_VAL_FLD_PVC OF 'COLBMPVC.TXT'
           PERFORM VAL-LAP-COD-COME.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO LAP-COD-COME-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FHOY"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FHOY.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FHOY-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-PVC-NUM-SVMI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-NUM-SVMI-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-PVC-IND-SVMI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-IND-SVMI-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-FRM-GLS-SVMI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-SVMI-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-PVC-NUM-VIMI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-NUM-VIMI-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-PVC-NUM-SVMA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-NUM-SVMA-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-PVC-NUM-VIMA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-NUM-VIMA-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-PVC-IND-SVMA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-IND-SVMA-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-FRM-GLS-SVMA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-SVMA-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-PVC-NUM-DVMI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-NUM-DVMI-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-PVC-MSC-TCVE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-MSC-TCVE-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-PVC-IND-DVMI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-IND-DVMI-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-FRM-GLS-DVMI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-DVMI-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-FRM-GLS-TCVE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TCVE-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-PVC-NUM-DVMA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-NUM-DVMA-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-PVC-COD-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-COD-VCOC-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-PVC-IND-DVMA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-IND-DVMA-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-FRM-GLS-DVMA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-DVMA-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-FRM-GLS-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-VCOC-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-PVC-VAL-VVMI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-VAL-VVMI-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-PVC-NUM-SPMI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-NUM-SPMI-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-PVC-VAL-VVMA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-VAL-VVMA-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-PVC-IND-SPMI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-IND-SPMI-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-FRM-GLS-SPMI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-SPMI-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-PVC-NUM-SPMA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-NUM-SPMA-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-PVC-SGV-FMMI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-SGV-FMMI-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-PVC-IND-SPMA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-IND-SPMA-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-FRM-GLS-SPMA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-SPMA-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-PVC-SGV-FMMA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-SGV-FMMA-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-PVC-SGV-FPMI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-SGV-FPMI-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-PVC-NUM-DESD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-NUM-DESD-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
           PERFORM VAL-PVC-SGV-FPMA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-SGV-FPMA-LEN IN PVC-FLD
                GO TO FIN-VAL-FLD-PVC.
*% INCLUDE FIN_VAL_FLD_PVC OF 'COLBMPVC.TXT'
       FIN-VAL-FLD-PVC.
           EXIT.
       
       VAL-LAP-COD-COME SECTION.
       INI-VAL-LAP-COD-COME.
*% LBL = "LAP_COD_COME OF 'COLBMPVC.TXT'"
*% INCLUDE INI_LAP_COD_COME OF 'COLBMPVC.TXT'
