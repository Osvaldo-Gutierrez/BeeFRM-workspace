*% INCLUDE INI_FST_KEY_QCE OF 'COLBMQCE.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
           MOVE FRM-POS-CURS TO DBC-NUM-ICLI-LEN IN QCE-FLD.
      * VRF-FIN --> BMCHIBCI004.SRCAIU ( FST-KEY Y FST-FLD )
*% INCLUDE FIN_FST_KEY_QCE OF 'COLBMQCE.TXT'
       FIN-FST-KEY-QCE.
           EXIT.
       
       FST-FLD-QCE SECTION.
       INI-FST-FLD-QCE.
*% INCLUDE INI_FST_FLD_QCE OF 'COLBMQCE.TXT'
           MOVE -1 TO CEC-MSC-ESTA-LEN IN QCE-FLD.
*% INCLUDE FIN_FST_FLD_QCE OF 'COLBMQCE.TXT'
       FIN-FST-FLD-QCE.
           EXIT.
       
