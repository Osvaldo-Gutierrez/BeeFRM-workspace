      *
      * COLBQQCE
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:50 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "QCE"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMQCE.TXT'
       SET-TAG-COLBQQCE SECTION.
       INI-SET-TAG-COLBQQCE.
           MOVE
           '<IDNMOD NOM=$COLBQQCE$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:50 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQQCE.
           EXIT.
           
       PUT-ALL-QCE SECTION.
       INI-PUT-ALL-QCE.
*% INCLUDE INI_PUT_ALL_QCE OF 'COLBMQCE.TXT'
           PERFORM PUT-DBC-QCE.
           PERFORM PUT-CEC-QCE.
*% INCLUDE FIN_PUT_ALL_QCE OF 'COLBMQCE.TXT'
       FIN-PUT-ALL-QCE.
           EXIT.
       
       PUT-DBC-QCE SECTION.
       INI-PUT-DBC-QCE.
*% INCLUDE INI_PUT_DBC_QCE OF 'COLBMQCE.TXT'
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-QCE.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-QCE
               GO TO FIN-PUT-DBC-QCE.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN QCE-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN QCE-FLD.
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN QCE-FLD.
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN QCE-FLD.
      * IDC-FIN --> BMCHIBCI001210.SRCAIU 
*% INCLUDE FIN_PUT_DBC_QCE OF 'COLBMQCE.TXT'
       FIN-PUT-DBC-QCE.
           EXIT.
       
       PUT-CEC-QCE SECTION.
       INI-PUT-CEC-QCE.
*% INCLUDE INI_PUT_CEC_QCE OF 'COLBMQCE.TXT'
           IF PGM-STAT-CEC-NUL
               GO TO FIN-PUT-CEC-QCE.
           IF NOT PGM-STAT-CEC-OKS
               PERFORM CLR-CEC-QCE
               GO TO FIN-PUT-CEC-QCE.
           MOVE CEC-MSC-ESTA IN CEC TO CEC-MSC-ESTA IN QCE-FLD.
           MOVE CEC-NUM-IENT IN CEC TO CEC-NUM-IENT IN QCE-FLD.
           MOVE CEC-CIC-IOPC IN CEC TO CEC-CIC-IOPC IN QCE-FLD.
           MOVE CEC-CIC-IOPV IN CEC TO CEC-CIC-IOPV IN QCE-FLD.
           MOVE CEC-COD-COPR IN CEC TO CEC-COD-COPR IN QCE-FLD.
           MOVE CEC-COD-VCAM IN CEC TO CEC-COD-VCAM IN QCE-FLD.
           MOVE CEC-COD-COD1 IN CEC TO CEC-COD-COD1 IN QCE-FLD.
           MOVE CEC-SGV-ARNS IN CEC TO CEC-SGV-ARNS IN QCE-FLD.
           MOVE CEC-SGV-ARNE IN CEC TO CEC-SGV-ARNE IN QCE-FLD.
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FCEC"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CEC-NUM-DCEC IN CEC TO CEC-NUM-DCEC IN QCE-FLD.
           MOVE CEC-NUM-MCEC IN CEC TO CEC-NUM-MCEC IN QCE-FLD.
           MOVE CEC-NUM-SCEC IN CEC TO CEC-NUM-SCEC IN QCE-FLD.
           MOVE CEC-NUM-ACEC IN CEC TO CEC-NUM-ACEC IN QCE-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FCUA"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CEC-NUM-DCUA IN CEC TO CEC-NUM-DCUA IN QCE-FLD.
           MOVE CEC-NUM-MCUA IN CEC TO CEC-NUM-MCUA IN QCE-FLD.
           MOVE CEC-NUM-SCUA IN CEC TO CEC-NUM-SCUA IN QCE-FLD.
           MOVE CEC-NUM-ACUA IN CEC TO CEC-NUM-ACUA IN QCE-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FCUC"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CEC-NUM-DCUC IN CEC TO CEC-NUM-DCUC IN QCE-FLD.
           MOVE CEC-NUM-MCUC IN CEC TO CEC-NUM-MCUC IN QCE-FLD.
           MOVE CEC-NUM-SCUC IN CEC TO CEC-NUM-SCUC IN QCE-FLD.
           MOVE CEC-NUM-ACUC IN CEC TO CEC-NUM-ACUC IN QCE-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FFIA"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CEC-NUM-DFIA IN CEC TO CEC-NUM-DFIA IN QCE-FLD.
           MOVE CEC-NUM-MFIA IN CEC TO CEC-NUM-MFIA IN QCE-FLD.
           MOVE CEC-NUM-SFIA IN CEC TO CEC-NUM-SFIA IN QCE-FLD.
           MOVE CEC-NUM-AFIA IN CEC TO CEC-NUM-AFIA IN QCE-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FVES"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CEC-NUM-DVES IN CEC TO CEC-NUM-DVES IN QCE-FLD.
           MOVE CEC-NUM-MVES IN CEC TO CEC-NUM-MVES IN QCE-FLD.
           MOVE CEC-NUM-SVES IN CEC TO CEC-NUM-SVES IN QCE-FLD.
           MOVE CEC-NUM-AVES IN CEC TO CEC-NUM-AVES IN QCE-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FCOI"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CEC-NUM-DCOI IN CEC TO CEC-NUM-DCOI IN QCE-FLD.
           MOVE CEC-NUM-MCOI IN CEC TO CEC-NUM-MCOI IN QCE-FLD.
           MOVE CEC-NUM-SCOI IN CEC TO CEC-NUM-SCOI IN QCE-FLD.
           MOVE CEC-NUM-ACOI IN CEC TO CEC-NUM-ACOI IN QCE-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE CEC-NUM-PZOC IN CEC TO CEC-NUM-PZOC IN QCE-FLD.
           MOVE CEC-SGV-TINT IN CEC TO CEC-SGV-TINT IN QCE-FLD.
           MOVE CEC-SGV-TIMP IN CEC TO CEC-SGV-TIMP IN QCE-FLD.
           MOVE CEC-NUM-TADE IN CEC TO CEC-NUM-TADE IN QCE-FLD.
           MOVE CEC-COD-TIOP IN CEC TO CEC-COD-TIOP IN QCE-FLD.
           MOVE CEC-NUM-ANOL IN CEC TO CEC-NUM-ANOL IN QCE-FLD.
           MOVE CEC-NUM-ANOO IN CEC TO CEC-NUM-ANOO IN QCE-FLD.
           MOVE CEC-NUM-ANOI IN CEC TO CEC-NUM-ANOI IN QCE-FLD.
           MOVE CEC-NUM-ANOD IN CEC TO CEC-NUM-ANOD IN QCE-FLD.
           MOVE CEC-NUM-ANOA IN CEC TO CEC-NUM-ANOA IN QCE-FLD.
           MOVE CEC-NUM-CNCA IN CEC TO CEC-NUM-CNCA IN QCE-FLD.
           MOVE CEC-IND-QNTL IN CEC TO CEC-IND-QNTL IN QCE-FLD.
           MOVE CEC-NUM-NIES IN CEC TO CEC-NUM-NIES IN QCE-FLD.
           MOVE CEC-NUM-PGAR IN CEC TO CEC-NUM-PGAR IN QCE-FLD.
*% INCLUDE FIN_PUT_CEC_QCE OF 'COLBMQCE.TXT'
       FIN-PUT-CEC-QCE.
           EXIT.
       
       KEY-ALL-QCE SECTION.
       INI-KEY-ALL-QCE.
*% INCLUDE INI_KEY_ALL_QCE OF 'COLBMQCE.TXT'
           PERFORM KEY-DBC-QCE.
           PERFORM KEY-CEC-QCE.
*% INCLUDE FIN_KEY_ALL_QCE OF 'COLBMQCE.TXT'
       FIN-KEY-ALL-QCE.
           EXIT.
       
       KEY-DBC-QCE SECTION.
       INI-KEY-DBC-QCE.
*% INCLUDE INI_KEY_DBC_QCE OF 'COLBMQCE.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN QCE-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN QCE-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN QCE-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN QCE-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_KEY_DBC_QCE OF 'COLBMQCE.TXT'
       FIN-KEY-DBC-QCE.
           EXIT.
       
       KEY-CEC-QCE SECTION.
       INI-KEY-CEC-QCE.
*% INCLUDE INI_KEY_CEC_QCE OF 'COLBMQCE.TXT'
           MOVE CEC-NUM-IENT IN QCE-FLD TO CEC-NUM-IENT IN CEC.
*% INCLUDE FIN_KEY_CEC_QCE OF 'COLBMQCE.TXT'
       FIN-KEY-CEC-QCE.
           EXIT.
       
       SCR-CLRA-QCE SECTION.
       INI-SCR-CLRA-QCE.
*% INCLUDE INI_SCR_CLRA_QCE OF 'COLBMQCE.TXT'
           PERFORM CLR-KEY-QCE.
           PERFORM CLR-FLD-QCE.
*% INCLUDE FIN_SCR_CLRA_QCE OF 'COLBMQCE.TXT'
       FIN-SCR-CLRA-QCE.
           EXIT.
       
       CLR-KEY-QCE SECTION.
       INI-CLR-KEY-QCE.
*% INCLUDE INI_CLR_KEY_QCE OF 'COLBMQCE.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN QCE-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN QCE-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN QCE-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN QCE-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CEC-NUM-IENT IN QCE-FLD.
*% INCLUDE FIN_CLR_KEY_QCE OF 'COLBMQCE.TXT'
       FIN-CLR-KEY-QCE.
           EXIT.
       
       CLR-FLD-QCE SECTION.
       INI-CLR-FLD-QCE.
*% INCLUDE INI_CLR_FLD_QCE OF 'COLBMQCE.TXT'
           MOVE SPACES TO FRM-GLS-CCSE IN QCE-FLD.
           MOVE SPACES TO FRM-GLS-NOMC IN QCE-FLD.
           MOVE SPACES TO FRM-GLS-CON1 IN QCE-FLD.
           MOVE SPACES TO FRM-JOR-ESTU IN QCE-FLD.
           MOVE SPACES TO FRM-RUT-BANC IN QCE-FLD.
           MOVE SPACES TO FRM-RUT-ACRE IN QCE-FLD.
           MOVE SPACES TO FRM-RUT-CIAS IN QCE-FLD.
           MOVE ZEROES TO FRM-ARN-REAL IN QCE-FLD.
           MOVE ZEROES TO FRM-ARN-REFE IN QCE-FLD.
           PERFORM CLR-DBC-QCE.
           PERFORM CLR-CEC-QCE.
*% INCLUDE FIN_CLR_FLD_QCE OF 'COLBMQCE.TXT'
       FIN-CLR-FLD-QCE.
           EXIT.
       
       CLR-DBC-QCE SECTION.
       INI-CLR-DBC-QCE.
*% INCLUDE INI_CLR_DBC_QCE OF 'COLBMQCE.TXT'
*% INCLUDE FIN_CLR_DBC_QCE OF 'COLBMQCE.TXT'
       FIN-CLR-DBC-QCE.
           EXIT.
       
       CLR-CEC-QCE SECTION.
       INI-CLR-CEC-QCE.
*% INCLUDE INI_CLR_CEC_QCE OF 'COLBMQCE.TXT'
           MOVE SPACES TO CEC-MSC-ESTA IN QCE-FLD.
           MOVE SPACES TO CEC-CIC-IOPC IN QCE-FLD.
           MOVE SPACES TO CEC-CIC-IOPV IN QCE-FLD.
           MOVE SPACES TO CEC-COD-COPR IN QCE-FLD.
           MOVE SPACES TO CEC-COD-VCAM IN QCE-FLD.
           MOVE SPACES TO CEC-COD-COD1 IN QCE-FLD.
           MOVE ZEROES TO CEC-SGV-ARNS IN QCE-FLD.
           MOVE ZEROES TO CEC-SGV-ARNE IN QCE-FLD.
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FCEC"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CEC-FEC-FCEC IN QCE-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FCUA"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CEC-FEC-FCUA IN QCE-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FCUC"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CEC-FEC-FCUC IN QCE-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FFIA"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CEC-FEC-FFIA IN QCE-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FVES"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CEC-FEC-FVES IN QCE-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FCOI"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CEC-FEC-FCOI IN QCE-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CEC-NUM-PZOC IN QCE-FLD.
           MOVE ZEROES TO CEC-SGV-TINT IN QCE-FLD.
           MOVE ZEROES TO CEC-SGV-TIMP IN QCE-FLD.
           MOVE ZEROES TO CEC-NUM-TADE IN QCE-FLD.
           MOVE SPACES TO CEC-COD-TIOP IN QCE-FLD.
           MOVE ZEROES TO CEC-NUM-ANOL IN QCE-FLD.
           MOVE ZEROES TO CEC-NUM-ANOO IN QCE-FLD.
           MOVE ZEROES TO CEC-NUM-ANOI IN QCE-FLD.
           MOVE ZEROES TO CEC-NUM-ANOD IN QCE-FLD.
           MOVE ZEROES TO CEC-NUM-ANOA IN QCE-FLD.
           MOVE ZEROES TO CEC-NUM-CNCA IN QCE-FLD.
           MOVE ZEROES TO CEC-IND-QNTL IN QCE-FLD.
