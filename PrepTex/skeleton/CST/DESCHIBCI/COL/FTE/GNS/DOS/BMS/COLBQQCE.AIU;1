*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FCUA"
           MOVE FRM-CPIM-UBY TO CEC-FEC-FCUA-ATR IN QCE-FLD.
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FCUC"
           MOVE FRM-CPIM-UBY TO CEC-FEC-FCUC-ATR IN QCE-FLD.
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FFIA"
           MOVE FRM-CPIM-UBY TO CEC-FEC-FFIA-ATR IN QCE-FLD.
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FVES"
           MOVE FRM-CPIM-UBY TO CEC-FEC-FVES-ATR IN QCE-FLD.
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FCOI"
           MOVE FRM-CPIM-UBY TO CEC-FEC-FCOI-ATR IN QCE-FLD.
           MOVE FRM-CPIM-UBY TO CEC-NUM-PZOC-ATR IN QCE-FLD.
           MOVE FRM-CPIM-UBY TO CEC-SGV-TINT-ATR IN QCE-FLD.
           MOVE FRM-CPIM-UBY TO CEC-SGV-TIMP-ATR IN QCE-FLD.
           MOVE FRM-CPIM-UBY TO CEC-NUM-TADE-ATR IN QCE-FLD.
           MOVE FRM-CPIM-UBY TO CEC-COD-TIOP-ATR IN QCE-FLD.
           MOVE FRM-CPIM-UBY TO CEC-NUM-ANOL-ATR IN QCE-FLD.
           MOVE FRM-CPIM-UBY TO CEC-NUM-ANOO-ATR IN QCE-FLD.
           MOVE FRM-CPIM-UBY TO CEC-NUM-ANOI-ATR IN QCE-FLD.
           MOVE FRM-CPIM-UBY TO CEC-NUM-ANOD-ATR IN QCE-FLD.
           MOVE FRM-CPIM-UBY TO CEC-NUM-ANOA-ATR IN QCE-FLD.
           MOVE FRM-CPIM-UBY TO CEC-NUM-CNCA-ATR IN QCE-FLD.
           MOVE FRM-CPIM-UBY TO CEC-IND-QNTL-ATR IN QCE-FLD.
           MOVE FRM-CPIM-UBY TO CEC-NUM-NIES-ATR IN QCE-FLD.
           MOVE FRM-CPIM-UBY TO CEC-NUM-PGAR-ATR IN QCE-FLD.
*% INCLUDE FIN_UNP_CEC_QCE OF 'COLBMQCE.TXT'
       FIN-UNP-CEC-QCE.
           EXIT.
       
       VAL-KEY-QCE SECTION.
       INI-VAL-KEY-QCE.
           PERFORM VAL-IKY-QCE.
           PERFORM VAL-MKY-QCE.
           PERFORM VAL-AKY-QCE.
       FIN-VAL-KEY-QCE.
           EXIT.
       
       VAL-IKY-QCE SECTION.
       INI-VAL-IKY-QCE.
*% INCLUDE INI_VAL_IKY_QCE OF 'COLBMQCE.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * VRF-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * VRF-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DBC-IND-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-IND-ICLI-LEN IN QCE-FLD
                GO TO FIN-VAL-IKY-QCE.
           PERFORM VAL-DBC-GLS-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-GLS-ICLI-LEN IN QCE-FLD
                GO TO FIN-VAL-IKY-QCE.
           PERFORM VAL-DBC-NUM-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-NUM-ICLI-LEN IN QCE-FLD
                GO TO FIN-VAL-IKY-QCE.
           PERFORM VAL-DBC-VRF-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-VRF-ICLI-LEN IN QCE-FLD
                GO TO FIN-VAL-IKY-QCE.
      * IDC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-CEC-NUM-IENT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CEC-NUM-IENT-LEN IN QCE-FLD
                GO TO FIN-VAL-IKY-QCE.
*% INCLUDE FIN_VAL_IKY_QCE OF 'COLBMQCE.TXT'
       FIN-VAL-IKY-QCE.
           EXIT.
       
