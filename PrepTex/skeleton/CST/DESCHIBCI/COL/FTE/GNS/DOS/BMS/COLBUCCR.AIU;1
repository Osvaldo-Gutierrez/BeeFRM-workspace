*% FRM = "CCR"
      *
      * COLBUCCR
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 03:23 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMCCR.TXT'
       SET-TAG-COLBUCCR SECTION.
       INI-SET-TAG-COLBUCCR.
           MOVE
           '<IDNMOD NOM=$COLBUCCR$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 03:23 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCCR.
           EXIT.
           
       RET-ALL-CCR SECTION.
       INI-RET-ALL-CCR.
*% INCLUDE INI_RET_ALL_CCR OF 'COLBMCCR.TXT'
           PERFORM KEY-ALL-CCR.
           PERFORM RET-OPC-CCR.
           PERFORM RET-CCR-CCR.
           PERFORM RET-DLC-CCR.
*% INCLUDE FIN_RET_ALL_CCR OF 'COLBMCCR.TXT'
       FIN-RET-ALL-CCR.
           EXIT.
       
       RET-OPC-CCR SECTION.
       INI-RET-OPC-CCR.
*% INCLUDE INI_RET_OPC_CCR OF 'COLBMCCR.TXT'
           MOVE OPC-MSC-TVOC IN CCR-FLD TO OPC-MSC-TVOC IN OPC.
           MOVE OPC-COD-TOOC IN CCR-FLD TO OPC-COD-TOOC IN OPC.
           MOVE OPC-COD-OFOC IN CCR-FLD TO OPC-COD-OFOC IN OPC.
*% INCLUDE FIN_RET_OPC_CCR OF 'COLBMCCR.TXT'
       FIN-RET-OPC-CCR.
           EXIT.
       
       RET-CCR-CCR SECTION.
       INI-RET-CCR-CCR.
*% INCLUDE INI_RET_CCR_CCR OF 'COLBMCCR.TXT'
           MOVE CCR-IND-SCRA IN CCR-FLD TO CCR-IND-SCRA IN CCR.
*% RREG = "CCR"
*% RREG_FRM = "CCR"
*% NOMB = "FCCR"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CCR-NUM-DCCR IN CCR-FLD TO CCR-NUM-DCCR IN CCR.
           MOVE CCR-NUM-MCCR IN CCR-FLD TO CCR-NUM-MCCR IN CCR.
           MOVE CCR-NUM-SCCR IN CCR-FLD TO CCR-NUM-SCCR IN CCR.
           MOVE CCR-NUM-ACCR IN CCR-FLD TO CCR-NUM-ACCR IN CCR.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% RREG = "CCR"
*% RREG_FRM = "CCR"
*% NOMB = "FING"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CCR-NUM-DING IN CCR-FLD TO CCR-NUM-DING IN CCR.
           MOVE CCR-NUM-MING IN CCR-FLD TO CCR-NUM-MING IN CCR.
           MOVE CCR-NUM-SING IN CCR-FLD TO CCR-NUM-SING IN CCR.
           MOVE CCR-NUM-AING IN CCR-FLD TO CCR-NUM-AING IN CCR.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CCR-IND-SCRT IN CCR-FLD TO CCR-IND-SCRT IN CCR.
           MOVE CCR-COD-RCCR IN CCR-FLD TO CCR-COD-RCCR IN CCR.
*% INCLUDE FIN_RET_CCR_CCR OF 'COLBMCCR.TXT'
       FIN-RET-CCR-CCR.
           EXIT.
       
       RET-DLC-CCR SECTION.
       INI-RET-DLC-CCR.
*% INCLUDE INI_RET_DLC_CCR OF 'COLBMCCR.TXT'
           MOVE DLC-IND-SCRT IN CCR-FLD TO DLC-IND-SCRT IN DLC.
*% INCLUDE FIN_RET_DLC_CCR OF 'COLBMCCR.TXT'
       FIN-RET-DLC-CCR.
           EXIT.
       
       KEY-ALL-CCR SECTION.
       INI-KEY-ALL-CCR.
*% INCLUDE INI_KEY_ALL_CCR OF 'COLBMCCR.TXT'
           PERFORM KEY-OPC-CCR.
           PERFORM KEY-CCR-CCR.
           PERFORM KEY-DLC-CCR.
*% INCLUDE FIN_KEY_ALL_CCR OF 'COLBMCCR.TXT'
       FIN-KEY-ALL-CCR.
           EXIT.
       
       KEY-OPC-CCR SECTION.
       INI-KEY-OPC-CCR.
*% INCLUDE INI_KEY_OPC_CCR OF 'COLBMCCR.TXT'
*% INCLUDE FIN_KEY_OPC_CCR OF 'COLBMCCR.TXT'
       FIN-KEY-OPC-CCR.
           EXIT.
       
       KEY-CCR-CCR SECTION.
       INI-KEY-CCR-CCR.
*% INCLUDE INI_KEY_CCR_CCR OF 'COLBMCCR.TXT'
           MOVE CCR-CAI-IOPC IN CCR-FLD TO CCR-CAI-IOPC IN CCR.
           MOVE CCR-IIC-IOPC IN CCR-FLD TO CCR-IIC-IOPC IN CCR.
           MOVE CCR-NUM-IDLC IN CCR-FLD TO CCR-NUM-IDLC IN CCR.
           MOVE CCR-NUM-ICCR IN CCR-FLD TO CCR-NUM-ICCR IN CCR.
*% INCLUDE FIN_KEY_CCR_CCR OF 'COLBMCCR.TXT'
       FIN-KEY-CCR-CCR.
           EXIT.
       
       KEY-DLC-CCR SECTION.
       INI-KEY-DLC-CCR.
*% INCLUDE INI_KEY_DLC_CCR OF 'COLBMCCR.TXT'
*% INCLUDE FIN_KEY_DLC_CCR OF 'COLBMCCR.TXT'
       FIN-KEY-DLC-CCR.
           EXIT.
       
       SCR-CLRA-CCR SECTION.
       INI-SCR-CLRA-CCR.
*% INCLUDE INI_SCR_CLRA_CCR OF 'COLBMCCR.TXT'
           PERFORM CLR-KEY-CCR.
           PERFORM CLR-FLD-CCR.
*% INCLUDE FIN_SCR_CLRA_CCR OF 'COLBMCCR.TXT'
       FIN-SCR-CLRA-CCR.
           EXIT.
       
       CLR-KEY-CCR SECTION.
       INI-CLR-KEY-CCR.
*% INCLUDE INI_CLR_KEY_CCR OF 'COLBMCCR.TXT'
           MOVE SPACES TO CCR-CAI-IOPC IN CCR-FLD.
           MOVE ZEROES TO CCR-IIC-IOPC IN CCR-FLD.
           MOVE ZEROES TO CCR-NUM-IDLC IN CCR-FLD.
           MOVE ZEROES TO CCR-NUM-ICCR IN CCR-FLD.
           MOVE SPACES TO FRM-GLS-NOMC IN CCR-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-ICLI IN CCR-FLD.
           MOVE SPACES TO FRM-VRF-ICLI IN CCR-FLD.
           MOVE SPACES TO FRM-IND-ICLI IN CCR-FLD.
           MOVE SPACES TO FRM-GLS-ICLI IN CCR-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-CAI-ICLI IN CCR-FLD.
           MOVE SPACES TO FRM-IIC-ICLI IN CCR-FLD.
           MOVE SPACES TO FRM-GLS-NOMA IN CCR-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-IAVA IN CCR-FLD.
           MOVE SPACES TO FRM-VRF-IAVA IN CCR-FLD.
           MOVE SPACES TO FRM-IND-IAVA IN CCR-FLD.
           MOVE SPACES TO FRM-GLS-IAVA IN CCR-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-CAI-IAVA IN CCR-FLD.
           MOVE SPACES TO FRM-IIC-IAVA IN CCR-FLD.
*% INCLUDE FIN_CLR_KEY_CCR OF 'COLBMCCR.TXT'
       FIN-CLR-KEY-CCR.
           EXIT.
       
       CLR-FLD-CCR SECTION.
       INI-CLR-FLD-CCR.
*% INCLUDE INI_CLR_FLD_CCR OF 'COLBMCCR.TXT'
           MOVE SPACES TO FRM-GLS-TVOC IN CCR-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN CCR-FLD.
           MOVE SPACES TO FRM-GLS-OFOC IN CCR-FLD.
           MOVE SPACES TO FRM-GLS-SCRA IN CCR-FLD.
           MOVE SPACES TO FRM-GLS-SCRT IN CCR-FLD.
           MOVE SPACES TO FRM-GLS-RCCR IN CCR-FLD.
           PERFORM CLR-OPC-CCR.
           PERFORM CLR-CCR-CCR.
           PERFORM CLR-DLC-CCR.
*% INCLUDE FIN_CLR_FLD_CCR OF 'COLBMCCR.TXT'
       FIN-CLR-FLD-CCR.
           EXIT.
       
       CLR-OPC-CCR SECTION.
       INI-CLR-OPC-CCR.
*% INCLUDE INI_CLR_OPC_CCR OF 'COLBMCCR.TXT'
           MOVE SPACES TO OPC-MSC-TVOC IN CCR-FLD.
           MOVE SPACES TO OPC-COD-TOOC IN CCR-FLD.
           MOVE SPACES TO OPC-COD-OFOC IN CCR-FLD.
*% INCLUDE FIN_CLR_OPC_CCR OF 'COLBMCCR.TXT'
       FIN-CLR-OPC-CCR.
           EXIT.
       
       CLR-CCR-CCR SECTION.
       INI-CLR-CCR-CCR.
*% INCLUDE INI_CLR_CCR_CCR OF 'COLBMCCR.TXT'
           MOVE SPACES TO CCR-IND-SCRA IN CCR-FLD.
*% RREG = "CCR"
*% RREG_FRM = "CCR"
*% NOMB = "FCCR"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CCR-FEC-FCCR IN CCR-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "CCR"
*% RREG_FRM = "CCR"
*% NOMB = "FING"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CCR-FEC-FING IN CCR-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO CCR-IND-SCRT IN CCR-FLD.
           MOVE SPACES TO CCR-COD-RCCR IN CCR-FLD.
*% INCLUDE FIN_CLR_CCR_CCR OF 'COLBMCCR.TXT'
       FIN-CLR-CCR-CCR.
           EXIT.
       
       CLR-DLC-CCR SECTION.
       INI-CLR-DLC-CCR.
*% INCLUDE INI_CLR_DLC_CCR OF 'COLBMCCR.TXT'
           MOVE SPACES TO DLC-IND-SCRT IN CCR-FLD.
*% INCLUDE FIN_CLR_DLC_CCR OF 'COLBMCCR.TXT'
       FIN-CLR-DLC-CCR.
           EXIT.
       
       FST-KEY-CCR SECTION.
       INI-FST-KEY-CCR.
*% INCLUDE INI_FST_KEY_CCR OF 'COLBMCCR.TXT'
           MOVE -1 TO CCR-CAI-IOPC-LEN IN CCR-FLD.
*% INCLUDE FIN_FST_KEY_CCR OF 'COLBMCCR.TXT'
       FIN-FST-KEY-CCR.
           EXIT.
       
       FST-FLD-CCR SECTION.
       INI-FST-FLD-CCR.
*% INCLUDE INI_FST_FLD_CCR OF 'COLBMCCR.TXT'
*% RREG = "CCR"
*% RREG_FRM = "CCR"
*% NOMB = "FCCR"
           MOVE FRM-POS-CURS TO CCR-FEC-FCCR-LEN IN CCR-FLD.
*% INCLUDE FIN_FST_FLD_CCR OF 'COLBMCCR.TXT'
       FIN-FST-FLD-CCR.
           EXIT.
       
       PRO-KEY-CCR SECTION.
       INI-PRO-KEY-CCR.
*% INCLUDE INI_PRO_KEY_CCR OF 'COLBMCCR.TXT'
           MOVE FRM-CPIM-AEY TO FRM-GLS-NOMC-ATR IN CCR-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-NUM-ICLI-ATR IN CCR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VRF-ICLI-ATR IN CCR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-ICLI-ATR IN CCR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-ICLI-ATR IN CCR-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-GLS-NOMA-ATR IN CCR-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-NUM-IAVA-ATR IN CCR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VRF-IAVA-ATR IN CCR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-IAVA-ATR IN CCR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-IAVA-ATR IN CCR-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           PERFORM PRO-MKY-CCR.
*% INCLUDE FIN_PRO_KEY_CCR OF 'COLBMCCR.TXT'
       FIN-PRO-KEY-CCR.
           EXIT.
       
       PRO-MKY-CCR SECTION.
       INI-PRO-MKY-CCR.
*% INCLUDE INI_PRO_MKY_CCR OF 'COLBMCCR.TXT'
*% INCLUDE FIN_PRO_MKY_CCR OF 'COLBMCCR.TXT'
       FIN-PRO-MKY-CCR.
           EXIT.
*% RREG = "FRM"
