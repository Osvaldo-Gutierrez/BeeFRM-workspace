      *
      * COLBQCEC
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 03:44 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CEC"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMCEC.TXT'
       SET-TAG-COLBQCEC SECTION.
       INI-SET-TAG-COLBQCEC.
           MOVE
           '<IDNMOD NOM=$COLBQCEC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 03:44 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCEC.
           EXIT.
           
       PUT-ALL-CEC SECTION.
       INI-PUT-ALL-CEC.
*% INCLUDE INI_PUT_ALL_CEC OF 'COLBMCEC.TXT'
           PERFORM PUT-DBC-CEC.
           PERFORM PUT-CEC-CEC.
*% INCLUDE FIN_PUT_ALL_CEC OF 'COLBMCEC.TXT'
       FIN-PUT-ALL-CEC.
           EXIT.
       
       PUT-DBC-CEC SECTION.
       INI-PUT-DBC-CEC.
*% INCLUDE INI_PUT_DBC_CEC OF 'COLBMCEC.TXT'
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-CEC.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-CEC
               GO TO FIN-PUT-DBC-CEC.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN CEC-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN CEC-FLD.
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN CEC-FLD.
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN CEC-FLD.
      * IDC-FIN --> BMCHIBCI001210.SRCAIU 
*% INCLUDE FIN_PUT_DBC_CEC OF 'COLBMCEC.TXT'
       FIN-PUT-DBC-CEC.
           EXIT.
       
       PUT-CEC-CEC SECTION.
       INI-PUT-CEC-CEC.
*% INCLUDE INI_PUT_CEC_CEC OF 'COLBMCEC.TXT'
           IF PGM-STAT-CEC-NUL
               GO TO FIN-PUT-CEC-CEC.
           IF NOT PGM-STAT-CEC-OKS
               PERFORM CLR-CEC-CEC
               GO TO FIN-PUT-CEC-CEC.
           MOVE CEC-MSC-ESTA IN CEC TO CEC-MSC-ESTA IN CEC-FLD.
           MOVE CEC-NUM-IENT IN CEC TO CEC-NUM-IENT IN CEC-FLD.
           MOVE CEC-CIC-IOPC IN CEC TO CEC-CIC-IOPC IN CEC-FLD.
           MOVE CEC-CIC-IOPV IN CEC TO CEC-CIC-IOPV IN CEC-FLD.
           MOVE CEC-COD-COPR IN CEC TO CEC-COD-COPR IN CEC-FLD.
           MOVE CEC-COD-VCAM IN CEC TO CEC-COD-VCAM IN CEC-FLD.
           MOVE CEC-COD-CCOM IN CEC TO CEC-COD-CCOM IN CEC-FLD.
           MOVE CEC-COD-COD1 IN CEC TO CEC-COD-COD1 IN CEC-FLD.
           MOVE CEC-COD-TTIT IN CEC TO CEC-COD-TTIT IN CEC-FLD.
           MOVE CEC-SGV-ARNS IN CEC TO CEC-SGV-ARNS IN CEC-FLD.
           MOVE CEC-SGV-ARNE IN CEC TO CEC-SGV-ARNE IN CEC-FLD.
           MOVE CEC-COD-TIES IN CEC TO CEC-COD-TIES IN CEC-FLD.
           MOVE CEC-IND-VIGE IN CEC TO CEC-IND-VIGE IN CEC-FLD.
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FCEC"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CEC-NUM-DCEC IN CEC TO CEC-NUM-DCEC IN CEC-FLD.
           MOVE CEC-NUM-MCEC IN CEC TO CEC-NUM-MCEC IN CEC-FLD.
           MOVE CEC-NUM-SCEC IN CEC TO CEC-NUM-SCEC IN CEC-FLD.
           MOVE CEC-NUM-ACEC IN CEC TO CEC-NUM-ACEC IN CEC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FCUA"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CEC-NUM-DCUA IN CEC TO CEC-NUM-DCUA IN CEC-FLD.
           MOVE CEC-NUM-MCUA IN CEC TO CEC-NUM-MCUA IN CEC-FLD.
           MOVE CEC-NUM-SCUA IN CEC TO CEC-NUM-SCUA IN CEC-FLD.
           MOVE CEC-NUM-ACUA IN CEC TO CEC-NUM-ACUA IN CEC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FCUC"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CEC-NUM-DCUC IN CEC TO CEC-NUM-DCUC IN CEC-FLD.
           MOVE CEC-NUM-MCUC IN CEC TO CEC-NUM-MCUC IN CEC-FLD.
           MOVE CEC-NUM-SCUC IN CEC TO CEC-NUM-SCUC IN CEC-FLD.
           MOVE CEC-NUM-ACUC IN CEC TO CEC-NUM-ACUC IN CEC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FFIA"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CEC-NUM-DFIA IN CEC TO CEC-NUM-DFIA IN CEC-FLD.
           MOVE CEC-NUM-MFIA IN CEC TO CEC-NUM-MFIA IN CEC-FLD.
           MOVE CEC-NUM-SFIA IN CEC TO CEC-NUM-SFIA IN CEC-FLD.
           MOVE CEC-NUM-AFIA IN CEC TO CEC-NUM-AFIA IN CEC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FVES"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CEC-NUM-DVES IN CEC TO CEC-NUM-DVES IN CEC-FLD.
           MOVE CEC-NUM-MVES IN CEC TO CEC-NUM-MVES IN CEC-FLD.
           MOVE CEC-NUM-SVES IN CEC TO CEC-NUM-SVES IN CEC-FLD.
           MOVE CEC-NUM-AVES IN CEC TO CEC-NUM-AVES IN CEC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FCOI"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CEC-NUM-DCOI IN CEC TO CEC-NUM-DCOI IN CEC-FLD.
           MOVE CEC-NUM-MCOI IN CEC TO CEC-NUM-MCOI IN CEC-FLD.
           MOVE CEC-NUM-SCOI IN CEC TO CEC-NUM-SCOI IN CEC-FLD.
           MOVE CEC-NUM-ACOI IN CEC TO CEC-NUM-ACOI IN CEC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FTPG"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CEC-NUM-DTPG IN CEC TO CEC-NUM-DTPG IN CEC-FLD.
           MOVE CEC-NUM-MTPG IN CEC TO CEC-NUM-MTPG IN CEC-FLD.
           MOVE CEC-NUM-STPG IN CEC TO CEC-NUM-STPG IN CEC-FLD.
           MOVE CEC-NUM-ATPG IN CEC TO CEC-NUM-ATPG IN CEC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE CEC-NUM-PZOC IN CEC TO CEC-NUM-PZOC IN CEC-FLD.
           MOVE CEC-SGV-TIMP IN CEC TO CEC-SGV-TIMP IN CEC-FLD.
           MOVE CEC-COD-TIOP IN CEC TO CEC-COD-TIOP IN CEC-FLD.
           MOVE CEC-SGV-TINT IN CEC TO CEC-SGV-TINT IN CEC-FLD.
           MOVE CEC-NUM-TADE IN CEC TO CEC-NUM-TADE IN CEC-FLD.
           MOVE CEC-CIC-IBCO IN CEC TO CEC-CIC-IBCO IN CEC-FLD.
           MOVE CEC-SGV-ARNR IN CEC TO CEC-SGV-ARNR IN CEC-FLD.
           MOVE CEC-CIC-IIES IN CEC TO CEC-CIC-IIES IN CEC-FLD.
           MOVE CEC-SGV-MFIA IN CEC TO CEC-SGV-MFIA IN CEC-FLD.
           MOVE CEC-NUM-ANOL IN CEC TO CEC-NUM-ANOL IN CEC-FLD.
           MOVE CEC-NUM-ANOO IN CEC TO CEC-NUM-ANOO IN CEC-FLD.
           MOVE CEC-NUM-ANOI IN CEC TO CEC-NUM-ANOI IN CEC-FLD.
           MOVE CEC-NUM-ANOD IN CEC TO CEC-NUM-ANOD IN CEC-FLD.
           MOVE CEC-NUM-ANOA IN CEC TO CEC-NUM-ANOA IN CEC-FLD.
           MOVE CEC-NUM-CNCA IN CEC TO CEC-NUM-CNCA IN CEC-FLD.
           MOVE CEC-IND-QNTL IN CEC TO CEC-IND-QNTL IN CEC-FLD.
           MOVE CEC-NUM-NIES IN CEC TO CEC-NUM-NIES IN CEC-FLD.
           MOVE CEC-NUM-PGAR IN CEC TO CEC-NUM-PGAR IN CEC-FLD.
*% INCLUDE FIN_PUT_CEC_CEC OF 'COLBMCEC.TXT'
       FIN-PUT-CEC-CEC.
           EXIT.
       
       KEY-ALL-CEC SECTION.
       INI-KEY-ALL-CEC.
*% INCLUDE INI_KEY_ALL_CEC OF 'COLBMCEC.TXT'
           PERFORM KEY-DBC-CEC.
           PERFORM KEY-CEC-CEC.
*% INCLUDE FIN_KEY_ALL_CEC OF 'COLBMCEC.TXT'
       FIN-KEY-ALL-CEC.
           EXIT.
       
       KEY-DBC-CEC SECTION.
       INI-KEY-DBC-CEC.
*% INCLUDE INI_KEY_DBC_CEC OF 'COLBMCEC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN CEC-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN CEC-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN CEC-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN CEC-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_KEY_DBC_CEC OF 'COLBMCEC.TXT'
       FIN-KEY-DBC-CEC.
           EXIT.
       
       KEY-CEC-CEC SECTION.
       INI-KEY-CEC-CEC.
*% INCLUDE INI_KEY_CEC_CEC OF 'COLBMCEC.TXT'
           MOVE CEC-NUM-IENT IN CEC-FLD TO CEC-NUM-IENT IN CEC.
*% INCLUDE FIN_KEY_CEC_CEC OF 'COLBMCEC.TXT'
       FIN-KEY-CEC-CEC.
           EXIT.
       
       SCR-CLRA-CEC SECTION.
       INI-SCR-CLRA-CEC.
*% INCLUDE INI_SCR_CLRA_CEC OF 'COLBMCEC.TXT'
           PERFORM CLR-KEY-CEC.
           PERFORM CLR-FLD-CEC.
*% INCLUDE FIN_SCR_CLRA_CEC OF 'COLBMCEC.TXT'
       FIN-SCR-CLRA-CEC.
           EXIT.
       
       CLR-KEY-CEC SECTION.
       INI-CLR-KEY-CEC.
*% INCLUDE INI_CLR_KEY_CEC OF 'COLBMCEC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN CEC-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN CEC-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN CEC-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN CEC-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CEC-NUM-IENT IN CEC-FLD.
*% INCLUDE FIN_CLR_KEY_CEC OF 'COLBMCEC.TXT'
       FIN-CLR-KEY-CEC.
           EXIT.
       
       CLR-FLD-CEC SECTION.
       INI-CLR-FLD-CEC.
*% INCLUDE INI_CLR_FLD_CEC OF 'COLBMCEC.TXT'
           MOVE SPACES TO FRM-GLS-ESTA IN CEC-FLD.
           MOVE SPACES TO FRM-GLS-NOMC IN CEC-FLD.
           MOVE SPACES TO FRM-GLS-CON1 IN CEC-FLD.
           PERFORM CLR-DBC-CEC.
           PERFORM CLR-CEC-CEC.
*% INCLUDE FIN_CLR_FLD_CEC OF 'COLBMCEC.TXT'
       FIN-CLR-FLD-CEC.
           EXIT.
       
       CLR-DBC-CEC SECTION.
       INI-CLR-DBC-CEC.
*% INCLUDE INI_CLR_DBC_CEC OF 'COLBMCEC.TXT'
*% INCLUDE FIN_CLR_DBC_CEC OF 'COLBMCEC.TXT'
       FIN-CLR-DBC-CEC.
           EXIT.
       
       CLR-CEC-CEC SECTION.
       INI-CLR-CEC-CEC.
*% INCLUDE INI_CLR_CEC_CEC OF 'COLBMCEC.TXT'
           MOVE SPACES TO CEC-MSC-ESTA IN CEC-FLD.
           MOVE SPACES TO CEC-CIC-IOPC IN CEC-FLD.
           MOVE SPACES TO CEC-CIC-IOPV IN CEC-FLD.
           MOVE SPACES TO CEC-COD-COPR IN CEC-FLD.
           MOVE SPACES TO CEC-COD-VCAM IN CEC-FLD.
           MOVE SPACES TO CEC-COD-CCOM IN CEC-FLD.
           MOVE SPACES TO CEC-COD-COD1 IN CEC-FLD.
           MOVE SPACES TO CEC-COD-TTIT IN CEC-FLD.
           MOVE ZEROES TO CEC-SGV-ARNS IN CEC-FLD.
           MOVE ZEROES TO CEC-SGV-ARNE IN CEC-FLD.
           MOVE SPACES TO CEC-COD-TIES IN CEC-FLD.
           MOVE SPACES TO CEC-IND-VIGE IN CEC-FLD.
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FCEC"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CEC-FEC-FCEC IN CEC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "CEC"
*% RREG_FRM = "CEC"
*% NOMB = "FCUA"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CEC-FEC-FCUA IN CEC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "CEC"
