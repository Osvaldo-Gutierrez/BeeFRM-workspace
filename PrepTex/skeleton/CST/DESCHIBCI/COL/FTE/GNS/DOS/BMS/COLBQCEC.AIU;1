           MOVE FRM-CPIM-UBY TO CEC-NUM-PGAR-ATR IN CEC-FLD.
*% INCLUDE FIN_UNP_CEC_CEC OF 'COLBMCEC.TXT'
       FIN-UNP-CEC-CEC.
           EXIT.
       
       VAL-KEY-CEC SECTION.
       INI-VAL-KEY-CEC.
           PERFORM VAL-IKY-CEC.
           PERFORM VAL-MKY-CEC.
           PERFORM VAL-AKY-CEC.
       FIN-VAL-KEY-CEC.
           EXIT.
       
       VAL-IKY-CEC SECTION.
       INI-VAL-IKY-CEC.
*% INCLUDE INI_VAL_IKY_CEC OF 'COLBMCEC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * VRF-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * VRF-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DBC-IND-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-IND-ICLI-LEN IN CEC-FLD
                GO TO FIN-VAL-IKY-CEC.
           PERFORM VAL-DBC-GLS-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-GLS-ICLI-LEN IN CEC-FLD
                GO TO FIN-VAL-IKY-CEC.
           PERFORM VAL-DBC-NUM-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-NUM-ICLI-LEN IN CEC-FLD
                GO TO FIN-VAL-IKY-CEC.
           PERFORM VAL-DBC-VRF-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-VRF-ICLI-LEN IN CEC-FLD
                GO TO FIN-VAL-IKY-CEC.
      * IDC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-CEC-NUM-IENT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CEC-NUM-IENT-LEN IN CEC-FLD
                GO TO FIN-VAL-IKY-CEC.
*% INCLUDE FIN_VAL_IKY_CEC OF 'COLBMCEC.TXT'
       FIN-VAL-IKY-CEC.
           EXIT.
       
