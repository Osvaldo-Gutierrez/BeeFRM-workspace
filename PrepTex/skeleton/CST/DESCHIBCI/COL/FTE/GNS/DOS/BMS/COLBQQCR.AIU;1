      *
      * COLBQQCR
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:53 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "QCR"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMQCR.TXT'
       SET-TAG-COLBQQCR SECTION.
       INI-SET-TAG-COLBQQCR.
           MOVE
           '<IDNMOD NOM=$COLBQQCR$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:53 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQQCR.
           EXIT.
           
       PUT-ALL-QCR SECTION.
       INI-PUT-ALL-QCR.
*% INCLUDE INI_PUT_ALL_QCR OF 'COLBMQCR.TXT'
           PERFORM PUT-CCR-QCR.
*% INCLUDE FIN_PUT_ALL_QCR OF 'COLBMQCR.TXT'
       FIN-PUT-ALL-QCR.
           EXIT.
       
       PUT-CCR-QCR SECTION.
       INI-PUT-CCR-QCR.
*% INCLUDE INI_PUT_CCR_QCR OF 'COLBMQCR.TXT'
           IF PGM-STAT-CCR-NUL
               GO TO FIN-PUT-CCR-QCR.
           IF NOT PGM-STAT-CCR-OKS
               PERFORM CLR-CCR-QCR
               GO TO FIN-PUT-CCR-QCR.
           MOVE CCR-CAI-IOPC IN CCR TO CCR-CAI-IOPC IN QCR-FLD.
           MOVE CCR-IIC-IOPC IN CCR TO CCR-IIC-IOPC IN QCR-FLD.
           MOVE CCR-NUM-IDLC IN CCR TO CCR-NUM-IDLC IN QCR-FLD.
*% INCLUDE FIN_PUT_CCR_QCR OF 'COLBMQCR.TXT'
       FIN-PUT-CCR-QCR.
           EXIT.
       
       KEY-ALL-QCR SECTION.
       INI-KEY-ALL-QCR.
*% INCLUDE INI_KEY_ALL_QCR OF 'COLBMQCR.TXT'
           PERFORM KEY-CCR-QCR.
*% INCLUDE FIN_KEY_ALL_QCR OF 'COLBMQCR.TXT'
       FIN-KEY-ALL-QCR.
           EXIT.
       
       KEY-CCR-QCR SECTION.
       INI-KEY-CCR-QCR.
*% INCLUDE INI_KEY_CCR_QCR OF 'COLBMQCR.TXT'
           MOVE CCR-CAI-IOPC IN QCR-FLD TO CCR-CAI-IOPC IN CCR.
           MOVE CCR-IIC-IOPC IN QCR-FLD TO CCR-IIC-IOPC IN CCR.
           MOVE CCR-NUM-IDLC IN QCR-FLD TO CCR-NUM-IDLC IN CCR.
*% INCLUDE FIN_KEY_CCR_QCR OF 'COLBMQCR.TXT'
       FIN-KEY-CCR-QCR.
           EXIT.
       
       SCR-CLRA-QCR SECTION.
       INI-SCR-CLRA-QCR.
*% INCLUDE INI_SCR_CLRA_QCR OF 'COLBMQCR.TXT'
           PERFORM CLR-KEY-QCR.
           PERFORM CLR-FLD-QCR.
*% INCLUDE FIN_SCR_CLRA_QCR OF 'COLBMQCR.TXT'
       FIN-SCR-CLRA-QCR.
           EXIT.
       
       CLR-KEY-QCR SECTION.
       INI-CLR-KEY-QCR.
*% INCLUDE INI_CLR_KEY_QCR OF 'COLBMQCR.TXT'
           MOVE SPACES TO CCR-CAI-IOPC IN QCR-FLD.
           MOVE ZEROES TO CCR-IIC-IOPC IN QCR-FLD.
           MOVE ZEROES TO CCR-NUM-IDLC IN QCR-FLD.
           MOVE SPACES TO FRM-IND-HMAS IN QCR-FLD.
           MOVE SPACES TO FRM-CAI-IOPC IN QCR-FLD.
           MOVE ZEROES TO FRM-IIC-IOPC IN QCR-FLD.
           MOVE ZEROES TO FRM-NUM-IDLC IN QCR-FLD.
           MOVE ZEROES TO FRM-VAL-ICCR IN QCR-FLD.
*% INCLUDE FIN_CLR_KEY_QCR OF 'COLBMQCR.TXT'
       FIN-CLR-KEY-QCR.
           EXIT.
       
       CLR-FLD-QCR SECTION.
       INI-CLR-FLD-QCR.
*% INCLUDE INI_CLR_FLD_QCR OF 'COLBMQCR.TXT'
           MOVE SPACES TO FRM-GLS-VCOC IN QCR-FLD.
           MOVE SPACES TO FRM-GLS-COOC IN QCR-FLD.
           MOVE SPACES TO FRM-GLS-OFOC IN QCR-FLD.
           MOVE ZEROES TO FRM-NUM-ICCR IN QCR-FLD(1).
           MOVE ZEROES TO FRM-NUM-ICCR IN QCR-FLD(2).
           MOVE ZEROES TO FRM-NUM-ICCR IN QCR-FLD(3).
           MOVE ZEROES TO FRM-NUM-ICCR IN QCR-FLD(4).
           MOVE ZEROES TO FRM-NUM-ICCR IN QCR-FLD(5).
           MOVE ZEROES TO FRM-NUM-ICCR IN QCR-FLD(6).
           MOVE ZEROES TO FRM-NUM-ICCR IN QCR-FLD(7).
           MOVE ZEROES TO FRM-NUM-ICCR IN QCR-FLD(8).
           MOVE ZEROES TO FRM-NUM-ICCR IN QCR-FLD(9).
           MOVE ZEROES TO FRM-NUM-ICCR IN QCR-FLD(10).
           MOVE ZEROES TO FRM-NUM-ICCR IN QCR-FLD(11).
           MOVE SPACES TO FRM-GLS-SCRA IN QCR-FLD(1).
           MOVE SPACES TO FRM-GLS-SCRA IN QCR-FLD(2).
           MOVE SPACES TO FRM-GLS-SCRA IN QCR-FLD(3).
           MOVE SPACES TO FRM-GLS-SCRA IN QCR-FLD(4).
           MOVE SPACES TO FRM-GLS-SCRA IN QCR-FLD(5).
           MOVE SPACES TO FRM-GLS-SCRA IN QCR-FLD(6).
           MOVE SPACES TO FRM-GLS-SCRA IN QCR-FLD(7).
           MOVE SPACES TO FRM-GLS-SCRA IN QCR-FLD(8).
           MOVE SPACES TO FRM-GLS-SCRA IN QCR-FLD(9).
           MOVE SPACES TO FRM-GLS-SCRA IN QCR-FLD(10).
           MOVE SPACES TO FRM-GLS-SCRA IN QCR-FLD(11).
           MOVE SPACES TO FRM-GLS-SCRT IN QCR-FLD(1).
           MOVE SPACES TO FRM-GLS-SCRT IN QCR-FLD(2).
           MOVE SPACES TO FRM-GLS-SCRT IN QCR-FLD(3).
           MOVE SPACES TO FRM-GLS-SCRT IN QCR-FLD(4).
           MOVE SPACES TO FRM-GLS-SCRT IN QCR-FLD(5).
           MOVE SPACES TO FRM-GLS-SCRT IN QCR-FLD(6).
           MOVE SPACES TO FRM-GLS-SCRT IN QCR-FLD(7).
           MOVE SPACES TO FRM-GLS-SCRT IN QCR-FLD(8).
           MOVE SPACES TO FRM-GLS-SCRT IN QCR-FLD(9).
           MOVE SPACES TO FRM-GLS-SCRT IN QCR-FLD(10).
           MOVE SPACES TO FRM-GLS-SCRT IN QCR-FLD(11).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCCR"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FCCR IN QCR-FLD(1).
           MOVE ZEROES TO FRM-FEC-FCCR IN QCR-FLD(2).
           MOVE ZEROES TO FRM-FEC-FCCR IN QCR-FLD(3).
           MOVE ZEROES TO FRM-FEC-FCCR IN QCR-FLD(4).
           MOVE ZEROES TO FRM-FEC-FCCR IN QCR-FLD(5).
           MOVE ZEROES TO FRM-FEC-FCCR IN QCR-FLD(6).
           MOVE ZEROES TO FRM-FEC-FCCR IN QCR-FLD(7).
           MOVE ZEROES TO FRM-FEC-FCCR IN QCR-FLD(8).
           MOVE ZEROES TO FRM-FEC-FCCR IN QCR-FLD(9).
           MOVE ZEROES TO FRM-FEC-FCCR IN QCR-FLD(10).
           MOVE ZEROES TO FRM-FEC-FCCR IN QCR-FLD(11).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FING"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FING IN QCR-FLD(1).
           MOVE ZEROES TO FRM-FEC-FING IN QCR-FLD(2).
           MOVE ZEROES TO FRM-FEC-FING IN QCR-FLD(3).
           MOVE ZEROES TO FRM-FEC-FING IN QCR-FLD(4).
           MOVE ZEROES TO FRM-FEC-FING IN QCR-FLD(5).
           MOVE ZEROES TO FRM-FEC-FING IN QCR-FLD(6).
           MOVE ZEROES TO FRM-FEC-FING IN QCR-FLD(7).
           MOVE ZEROES TO FRM-FEC-FING IN QCR-FLD(8).
           MOVE ZEROES TO FRM-FEC-FING IN QCR-FLD(9).
           MOVE ZEROES TO FRM-FEC-FING IN QCR-FLD(10).
           MOVE ZEROES TO FRM-FEC-FING IN QCR-FLD(11).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-COD-RCCR IN QCR-FLD(1).
           MOVE SPACES TO FRM-COD-RCCR IN QCR-FLD(2).
           MOVE SPACES TO FRM-COD-RCCR IN QCR-FLD(3).
           MOVE SPACES TO FRM-COD-RCCR IN QCR-FLD(4).
           MOVE SPACES TO FRM-COD-RCCR IN QCR-FLD(5).
           MOVE SPACES TO FRM-COD-RCCR IN QCR-FLD(6).
           MOVE SPACES TO FRM-COD-RCCR IN QCR-FLD(7).
           MOVE SPACES TO FRM-COD-RCCR IN QCR-FLD(8).
           MOVE SPACES TO FRM-COD-RCCR IN QCR-FLD(9).
           MOVE SPACES TO FRM-COD-RCCR IN QCR-FLD(10).
           MOVE SPACES TO FRM-COD-RCCR IN QCR-FLD(11).
           PERFORM CLR-CCR-QCR.
*% INCLUDE FIN_CLR_FLD_QCR OF 'COLBMQCR.TXT'
       FIN-CLR-FLD-QCR.
           EXIT.
       
       CLR-CCR-QCR SECTION.
       INI-CLR-CCR-QCR.
*% INCLUDE INI_CLR_CCR_QCR OF 'COLBMQCR.TXT'
*% INCLUDE FIN_CLR_CCR_QCR OF 'COLBMQCR.TXT'
       FIN-CLR-CCR-QCR.
           EXIT.
       
       FST-KEY-QCR SECTION.
       INI-FST-KEY-QCR.
*% INCLUDE INI_FST_KEY_QCR OF 'COLBMQCR.TXT'
           MOVE -1 TO CCR-CAI-IOPC-LEN IN QCR-FLD.
*% INCLUDE FIN_FST_KEY_QCR OF 'COLBMQCR.TXT'
       FIN-FST-KEY-QCR.
           EXIT.
       
       FST-FLD-QCR SECTION.
       INI-FST-FLD-QCR.
*% INCLUDE INI_FST_FLD_QCR OF 'COLBMQCR.TXT'
*% INCLUDE FIN_FST_FLD_QCR OF 'COLBMQCR.TXT'
       FIN-FST-FLD-QCR.
           EXIT.
       
       PRO-KEY-QCR SECTION.
       INI-PRO-KEY-QCR.
*% INCLUDE INI_PRO_KEY_QCR OF 'COLBMQCR.TXT'
           MOVE FRM-CPIM-AEY TO FRM-IND-HMAS-ATR IN QCR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-CAI-IOPC-ATR IN QCR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IIC-IOPC-ATR IN QCR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-NUM-IDLC-ATR IN QCR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VAL-ICCR-ATR IN QCR-FLD.
           PERFORM PRO-IKY-QCR.
           PERFORM PRO-AKY-QCR.
           PERFORM PRO-MKY-QCR.
*% INCLUDE FIN_PRO_KEY_QCR OF 'COLBMQCR.TXT'
       FIN-PRO-KEY-QCR.
           EXIT.
       
       PRO-IKY-QCR SECTION.
       INI-PRO-IKY-QCR.
*% INCLUDE INI_PRO_IKY_QCR OF 'COLBMQCR.TXT'
           MOVE FRM-CPIM-AEY TO CCR-CAI-IOPC-ATR IN QCR-FLD.
           MOVE FRM-CPIM-AEY TO CCR-IIC-IOPC-ATR IN QCR-FLD.
           MOVE FRM-CPIM-AEY TO CCR-NUM-IDLC-ATR IN QCR-FLD.
*% INCLUDE FIN_PRO_IKY_QCR OF 'COLBMQCR.TXT'
       FIN-PRO-IKY-QCR.
           EXIT.
       
       PRO-MKY-QCR SECTION.
       INI-PRO-MKY-QCR.
*% INCLUDE INI_PRO_MKY_QCR OF 'COLBMQCR.TXT'
*% INCLUDE FIN_PRO_MKY_QCR OF 'COLBMQCR.TXT'
       FIN-PRO-MKY-QCR.
           EXIT.
       
       PRO-AKY-QCR SECTION.
       INI-PRO-AKY-QCR.
*% INCLUDE INI_PRO_AKY_QCR OF 'COLBMQCR.TXT'
*% INCLUDE FIN_PRO_AKY_QCR OF 'COLBMQCR.TXT'
       FIN-PRO-AKY-QCR.
           EXIT.
       
       UNP-KEY-QCR SECTION.
       INI-UNP-KEY-QCR.
*% INCLUDE INI_UNP_KEY_QCR OF 'COLBMQCR.TXT'
           MOVE FRM-CPIM-UBY TO FRM-IND-HMAS-ATR IN QCR-FLD.
           MOVE FRM-CPIM-UBY TO FRM-CAI-IOPC-ATR IN QCR-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IIC-IOPC-ATR IN QCR-FLD.
           MOVE FRM-CPIM-UBY TO FRM-NUM-IDLC-ATR IN QCR-FLD.
           MOVE FRM-CPIM-UBY TO FRM-VAL-ICCR-ATR IN QCR-FLD.
           PERFORM UNP-IKY-QCR.
           PERFORM UNP-AKY-QCR.
           PERFORM UNP-MKY-QCR.
*% INCLUDE FIN_UNP_KEY_QCR OF 'COLBMQCR.TXT'
       FIN-UNP-KEY-QCR.
           EXIT.
       
       UNP-IKY-QCR SECTION.
       INI-UNP-IKY-QCR.
*% INCLUDE INI_UNP_IKY_QCR OF 'COLBMQCR.TXT'
           MOVE FRM-CPIM-UBY TO CCR-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO CCR-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO CCR-NUM-IDLC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-IND-HMAS-ATR.
           MOVE FRM-CPIM-UBY TO FRM-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-NUM-IDLC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-VAL-ICCR-ATR.
*% INCLUDE FIN_UNP_IKY_QCR OF 'COLBMQCR.TXT'
       FIN-UNP-IKY-QCR.
           EXIT.
       
       UNP-MKY-QCR SECTION.
       INI-UNP-MKY-QCR.
*% INCLUDE INI_UNP_MKY_QCR OF 'COLBMQCR.TXT'
*% INCLUDE FIN_UNP_MKY_QCR OF 'COLBMQCR.TXT'
       FIN-UNP-MKY-QCR.
           EXIT.
       
       UNP-AKY-QCR SECTION.
       INI-UNP-AKY-QCR.
*% INCLUDE INI_UNP_AKY_QCR OF 'COLBMQCR.TXT'
*% INCLUDE FIN_UNP_AKY_QCR OF 'COLBMQCR.TXT'
       FIN-UNP-AKY-QCR.
           EXIT.
       
       UNP-FLD-QCR SECTION.
       INI-UNP-FLD-QCR.
*% INCLUDE INI_UNP_FLD_QCR OF 'COLBMQCR.TXT'
           PERFORM UNP-CCR-QCR.
*% INCLUDE FIN_UNP_FLD_QCR OF 'COLBMQCR.TXT'
       FIN-UNP-FLD-QCR.
           EXIT.
       
       UNP-CCR-QCR SECTION.
       INI-UNP-CCR-QCR.
*% INCLUDE INI_UNP_CCR_QCR OF 'COLBMQCR.TXT'
*% INCLUDE FIN_UNP_CCR_QCR OF 'COLBMQCR.TXT'
       FIN-UNP-CCR-QCR.
           EXIT.
       
       VAL-KEY-QCR SECTION.
       INI-VAL-KEY-QCR.
           PERFORM VAL-IKY-QCR.
           PERFORM VAL-MKY-QCR.
           PERFORM VAL-AKY-QCR.
       FIN-VAL-KEY-QCR.
           EXIT.
       
       VAL-IKY-QCR SECTION.
       INI-VAL-IKY-QCR.
*% INCLUDE INI_VAL_IKY_QCR OF 'COLBMQCR.TXT'
           PERFORM VAL-CCR-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CCR-CAI-IOPC-LEN IN QCR-FLD
                GO TO FIN-VAL-IKY-QCR.
           PERFORM VAL-CCR-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CCR-IIC-IOPC-LEN IN QCR-FLD
                GO TO FIN-VAL-IKY-QCR.
           PERFORM VAL-CCR-NUM-IDLC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CCR-NUM-IDLC-LEN IN QCR-FLD
                GO TO FIN-VAL-IKY-QCR.
           PERFORM VAL-FRM-IND-HMAS.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IND-HMAS-LEN IN QCR-FLD
                GO TO FIN-VAL-IKY-QCR.
           PERFORM VAL-FRM-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-IOPC-LEN IN QCR-FLD
                GO TO FIN-VAL-IKY-QCR.
           PERFORM VAL-FRM-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-IOPC-LEN IN QCR-FLD
                GO TO FIN-VAL-IKY-QCR.
           PERFORM VAL-FRM-NUM-IDLC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-NUM-IDLC-LEN IN QCR-FLD
                GO TO FIN-VAL-IKY-QCR.
           PERFORM VAL-FRM-VAL-ICCR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-ICCR-LEN IN QCR-FLD
                GO TO FIN-VAL-IKY-QCR.
*% INCLUDE FIN_VAL_IKY_QCR OF 'COLBMQCR.TXT'
       FIN-VAL-IKY-QCR.
           EXIT.
       
       VAL-MKY-QCR SECTION.
       INI-VAL-MKY-QCR.
*% INCLUDE INI_VAL_MKY_QCR OF 'COLBMQCR.TXT'
*% INCLUDE FIN_VAL_MKY_QCR OF 'COLBMQCR.TXT'
       FIN-VAL-MKY-QCR.
           EXIT.
       
       VAL-AKY-QCR SECTION.
       INI-VAL-AKY-QCR.
*% INCLUDE INI_VAL_AKY_QCR OF 'COLBMQCR.TXT'
*% INCLUDE FIN_VAL_AKY_QCR OF 'COLBMQCR.TXT'
       FIN-VAL-AKY-QCR.
           EXIT.
       
       VAL-CCR-CAI-IOPC SECTION.
       INI-VAL-CCR-CAI-IOPC.
*% LBL = "CCR_CAI_IOPC OF 'COLBMQCR.TXT'"
*% INCLUDE INI_CCR_CAI_IOPC OF 'COLBMQCR.TXT'
           IF CCR-CAI-IOPC IN QCR-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-CCR-CAI-IOPC.
*% INCLUDE CCR_CAI_IOPC OF 'COLBMQCR.TXT'
*% INCLUDE FIN_CCR_CAI_IOPC OF 'COLBMQCR.TXT'
       FIN-VAL-CCR-CAI-IOPC.
           EXIT.
       
       VAL-CCR-IIC-IOPC SECTION.
       INI-VAL-CCR-IIC-IOPC.
*% LBL = "CCR_IIC_IOPC OF 'COLBMQCR.TXT'"
*% INCLUDE INI_CCR_IIC_IOPC OF 'COLBMQCR.TXT'
           IF CCR-IIC-IOPC IN QCR-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-CCR-IIC-IOPC.
*% INCLUDE CCR_IIC_IOPC OF 'COLBMQCR.TXT'
*% INCLUDE FIN_CCR_IIC_IOPC OF 'COLBMQCR.TXT'
       FIN-VAL-CCR-IIC-IOPC.
           EXIT.
       
       VAL-CCR-NUM-IDLC SECTION.
       INI-VAL-CCR-NUM-IDLC.
*% LBL = "CCR_NUM_IDLC OF 'COLBMQCR.TXT'"
*% INCLUDE INI_CCR_NUM_IDLC OF 'COLBMQCR.TXT'
           IF CCR-NUM-IDLC IN QCR-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-CCR-NUM-IDLC.
*% INCLUDE CCR_NUM_IDLC OF 'COLBMQCR.TXT'
*% INCLUDE FIN_CCR_NUM_IDLC OF 'COLBMQCR.TXT'
       FIN-VAL-CCR-NUM-IDLC.
           EXIT.
       
       VAL-FRM-IND-HMAS SECTION.
       INI-VAL-FRM-IND-HMAS.
*% LBL = "FRM_IND_HMAS OF 'COLBMQCR.TXT'"
*% INCLUDE INI_FRM_IND_HMAS OF 'COLBMQCR.TXT'
           IF FRM-IND-HMAS IN QCR-FLD NOT > SPACES
              MOVE SPACES TO FRM-IND-HMAS IN QCR-FLD.
*% INCLUDE FRM_IND_HMAS OF 'COLBMQCR.TXT'
*% INCLUDE FIN_FRM_IND_HMAS OF 'COLBMQCR.TXT'
       FIN-VAL-FRM-IND-HMAS.
           EXIT.
       
       VAL-FRM-CAI-IOPC SECTION.
       INI-VAL-FRM-CAI-IOPC.
*% LBL = "FRM_CAI_IOPC OF 'COLBMQCR.TXT'"
*% INCLUDE INI_FRM_CAI_IOPC OF 'COLBMQCR.TXT'
           IF FRM-CAI-IOPC IN QCR-FLD NOT > SPACES
              MOVE SPACES TO FRM-CAI-IOPC IN QCR-FLD.
*% INCLUDE FRM_CAI_IOPC OF 'COLBMQCR.TXT'
*% INCLUDE FIN_FRM_CAI_IOPC OF 'COLBMQCR.TXT'
       FIN-VAL-FRM-CAI-IOPC.
           EXIT.
       
       VAL-FRM-IIC-IOPC SECTION.
       INI-VAL-FRM-IIC-IOPC.
*% LBL = "FRM_IIC_IOPC OF 'COLBMQCR.TXT'"
*% INCLUDE INI_FRM_IIC_IOPC OF 'COLBMQCR.TXT'
*% INCLUDE FRM_IIC_IOPC OF 'COLBMQCR.TXT'
*% INCLUDE FIN_FRM_IIC_IOPC OF 'COLBMQCR.TXT'
       FIN-VAL-FRM-IIC-IOPC.
           EXIT.
       
       VAL-FRM-NUM-IDLC SECTION.
       INI-VAL-FRM-NUM-IDLC.
*% LBL = "FRM_NUM_IDLC OF 'COLBMQCR.TXT'"
*% INCLUDE INI_FRM_NUM_IDLC OF 'COLBMQCR.TXT'
*% INCLUDE FRM_NUM_IDLC OF 'COLBMQCR.TXT'
*% INCLUDE FIN_FRM_NUM_IDLC OF 'COLBMQCR.TXT'
       FIN-VAL-FRM-NUM-IDLC.
           EXIT.
       
       VAL-FRM-VAL-ICCR SECTION.
       INI-VAL-FRM-VAL-ICCR.
*% LBL = "FRM_VAL_ICCR OF 'COLBMQCR.TXT'"
*% INCLUDE INI_FRM_VAL_ICCR OF 'COLBMQCR.TXT'
*% INCLUDE FRM_VAL_ICCR OF 'COLBMQCR.TXT'
*% INCLUDE FIN_FRM_VAL_ICCR OF 'COLBMQCR.TXT'
       FIN-VAL-FRM-VAL-ICCR.
           EXIT.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCCR"
*% IZR = "S"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FING"
*% IZR = "S"
       
       VAL-CON-KEY-QCR SECTION.
       INI-VAL-CON-KEY-QCR.
*% INCLUDE INI_VAL_CON_KEY_QCR OF 'COLBMQCR.TXT'
           IF     CCR-CAI-IOPC IN QCR-FLD NOT > SPACES 
              AND CCR-IIC-IOPC IN QCR-FLD NOT > ZEROES 
              AND CCR-NUM-IDLC IN QCR-FLD NOT > ZEROES 
              AND FRM-IND-HMAS IN QCR-FLD NOT > SPACES 
              AND FRM-CAI-IOPC IN QCR-FLD NOT > SPACES 
              AND FRM-IIC-IOPC IN QCR-FLD NOT > ZEROES 
              AND FRM-NUM-IDLC IN QCR-FLD NOT > ZEROES 
              AND FRM-VAL-ICCR IN QCR-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-QCR.
*% INCLUDE FIN_VAL_CON_KEY_QCR OF 'COLBMQCR.TXT'
       FIN-VAL-CON-KEY-QCR.
           EXIT.
       
