      *
      * COLBQCYA
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:49 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CYA"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMCYA.TXT'
       SET-TAG-COLBQCYA SECTION.
       INI-SET-TAG-COLBQCYA.
           MOVE
           '<IDNMOD NOM=$COLBQCYA$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:49 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCYA.
           EXIT.
           
       PUT-ALL-CYA SECTION.
       INI-PUT-ALL-CYA.
*% INCLUDE INI_PUT_ALL_CYA OF 'COLBMCYA.TXT'
           PERFORM PUT-OPC-CYA.
           PERFORM PUT-CYA-CYA.
           PERFORM PUT-DBC-CYA.
           PERFORM PUT-RDC-CYA.
*% INCLUDE FIN_PUT_ALL_CYA OF 'COLBMCYA.TXT'
       FIN-PUT-ALL-CYA.
           EXIT.
       
       PUT-OPC-CYA SECTION.
       INI-PUT-OPC-CYA.
*% INCLUDE INI_PUT_OPC_CYA OF 'COLBMCYA.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-CYA.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-CYA
               GO TO FIN-PUT-OPC-CYA.
           MOVE OPC-MSC-TVOC IN OPC TO OPC-MSC-TVOC IN CYA-FLD.
           MOVE OPC-COD-TOOC IN OPC TO OPC-COD-TOOC IN CYA-FLD.
           MOVE OPC-COD-OFOC IN OPC TO OPC-COD-OFOC IN CYA-FLD.
           MOVE OPC-COD-VCOC IN OPC TO OPC-COD-VCOC IN CYA-FLD.
*% BUF = "CYA-FLD"
*% SIS = "TAB"
*% TAB = "TVAL"
*% FLD = "OPC-MSC-TVOC"
*% GLS = "FRM-GLS-TVOC"
*% TIENE_GLOSA = TRUE
*% LGL = 13
*% INCLUDE 'BUSMSC'
*% BUF = "CYA-FLD"
*% SIS = "TAB"
*% TAB = "TIO "
*% FLD = "OPC-COD-TOOC"
*% GLS = "FRM-GLS-TOOC"
*% TIENE_GLOSA = TRUE
*% LGL = 14
*% INCLUDE 'BUSTAB'
*% BUF = "CYA-FLD"
*% SIS = "TAB"
*% TAB = "OFI "
*% FLD = "OPC-COD-OFOC"
*% GLS = "FRM-GLS-OFOC"
*% TIENE_GLOSA = TRUE
*% LGL = 14
*% INCLUDE 'BUSOFI          '
*% BUF = "CYA-FLD"
*% SIS = "TAB"
*% TAB = "VLR "
*% FLD = "OPC-COD-VCOC"
*% GLS = "FRM-GLS-VCOC"
*% TIENE_GLOSA = TRUE
*% LGL = 18
*% INCLUDE 'BUSTAB'
*% INCLUDE FIN_PUT_OPC_CYA OF 'COLBMCYA.TXT'
       FIN-PUT-OPC-CYA.
           EXIT.
       
       PUT-CYA-CYA SECTION.
       INI-PUT-CYA-CYA.
*% INCLUDE INI_PUT_CYA_CYA OF 'COLBMCYA.TXT'
           IF PGM-STAT-CYA-NUL
               GO TO FIN-PUT-CYA-CYA.
           IF NOT PGM-STAT-CYA-OKS
               PERFORM CLR-CYA-CYA
               GO TO FIN-PUT-CYA-CYA.
           MOVE CYA-CAI-IOPC IN CYA TO CYA-CAI-IOPC IN CYA-FLD.
           MOVE CYA-IIC-IOPC IN CYA TO CYA-IIC-IOPC IN CYA-FLD.
           MOVE CYA-NUM-ICAN IN CYA TO CYA-NUM-ICAN IN CYA-FLD.
           MOVE CYA-NUM-ICYA IN CYA TO CYA-NUM-ICYA IN CYA-FLD.
           MOVE CYA-IND-TCYA IN CYA TO CYA-IND-TCYA IN CYA-FLD.
           MOVE CYA-CAI-ICYA IN CYA TO CYA-CAI-ICYA IN CYA-FLD.
           MOVE CYA-IIC-ICYA IN CYA TO CYA-IIC-ICYA IN CYA-FLD.
           MOVE CYA-SGV-LIQU IN CYA TO CYA-SGV-LIQU IN CYA-FLD.
*% BUF = "CYA-FLD"
*% SIS = "TAB"
*% TAB = "TCYA"
*% FLD = "CYA-IND-TCYA"
*% GLS = "FRM-GLS-TCYA"
*% TIENE_GLOSA = TRUE
*% LGL = 45
*% INCLUDE 'BUSMSC'
*% INCLUDE FIN_PUT_CYA_CYA OF 'COLBMCYA.TXT'
       FIN-PUT-CYA-CYA.
           EXIT.
       
       PUT-DBC-CYA SECTION.
       INI-PUT-DBC-CYA.
*% INCLUDE INI_PUT_DBC_CYA OF 'COLBMCYA.TXT'
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-CYA.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-CYA
               GO TO FIN-PUT-DBC-CYA.
           MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN CYA-FLD.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN CYA-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN CYA-FLD.
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN CYA-FLD.
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN CYA-FLD.
      * IDC-FIN --> BMCHIBCI001210.SRCAIU 
*% INCLUDE FIN_PUT_DBC_CYA OF 'COLBMCYA.TXT'
       FIN-PUT-DBC-CYA.
           EXIT.
       
       PUT-RDC-CYA SECTION.
       INI-PUT-RDC-CYA.
*% INCLUDE INI_PUT_RDC_CYA OF 'COLBMCYA.TXT'
           IF PGM-STAT-RDC-NUL
               GO TO FIN-PUT-RDC-CYA.
           IF NOT PGM-STAT-RDC-OKS
               PERFORM CLR-RDC-CYA
               GO TO FIN-PUT-RDC-CYA.
           MOVE RDC-CAI-ICLI IN RDC TO RDC-CAI-ICLI IN CYA-FLD.
           MOVE RDC-IIC-ICLI IN RDC TO RDC-IIC-ICLI IN CYA-FLD.
*% INCLUDE FIN_PUT_RDC_CYA OF 'COLBMCYA.TXT'
       FIN-PUT-RDC-CYA.
           EXIT.
       
       KEY-ALL-CYA SECTION.
       INI-KEY-ALL-CYA.
*% INCLUDE INI_KEY_ALL_CYA OF 'COLBMCYA.TXT'
           PERFORM KEY-OPC-CYA.
           PERFORM KEY-CYA-CYA.
           PERFORM KEY-DBC-CYA.
           PERFORM KEY-RDC-CYA.
*% INCLUDE FIN_KEY_ALL_CYA OF 'COLBMCYA.TXT'
       FIN-KEY-ALL-CYA.
           EXIT.
       
       KEY-OPC-CYA SECTION.
       INI-KEY-OPC-CYA.
*% INCLUDE INI_KEY_OPC_CYA OF 'COLBMCYA.TXT'
*% INCLUDE FIN_KEY_OPC_CYA OF 'COLBMCYA.TXT'
       FIN-KEY-OPC-CYA.
           EXIT.
       
       KEY-CYA-CYA SECTION.
       INI-KEY-CYA-CYA.
*% INCLUDE INI_KEY_CYA_CYA OF 'COLBMCYA.TXT'
           MOVE CYA-CAI-IOPC IN CYA-FLD TO CYA-CAI-IOPC IN CYA.
           MOVE CYA-IIC-IOPC IN CYA-FLD TO CYA-IIC-IOPC IN CYA.
           MOVE CYA-NUM-ICAN IN CYA-FLD TO CYA-NUM-ICAN IN CYA.
           MOVE CYA-NUM-ICYA IN CYA-FLD TO CYA-NUM-ICYA IN CYA.
*% INCLUDE FIN_KEY_CYA_CYA OF 'COLBMCYA.TXT'
       FIN-KEY-CYA-CYA.
           EXIT.
       
       KEY-DBC-CYA SECTION.
       INI-KEY-DBC-CYA.
*% INCLUDE INI_KEY_DBC_CYA OF 'COLBMCYA.TXT'
           MOVE DBC-GLS-NOMC IN CYA-FLD TO DBC-GLS-NOMC IN DBC.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN CYA-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN CYA-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN CYA-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN CYA-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_KEY_DBC_CYA OF 'COLBMCYA.TXT'
       FIN-KEY-DBC-CYA.
           EXIT.
       
       KEY-RDC-CYA SECTION.
       INI-KEY-RDC-CYA.
*% INCLUDE INI_KEY_RDC_CYA OF 'COLBMCYA.TXT'
*% INCLUDE FIN_KEY_RDC_CYA OF 'COLBMCYA.TXT'
       FIN-KEY-RDC-CYA.
           EXIT.
       
       SCR-CLRA-CYA SECTION.
       INI-SCR-CLRA-CYA.
*% INCLUDE INI_SCR_CLRA_CYA OF 'COLBMCYA.TXT'
           PERFORM CLR-KEY-CYA.
           PERFORM CLR-FLD-CYA.
*% INCLUDE FIN_SCR_CLRA_CYA OF 'COLBMCYA.TXT'
       FIN-SCR-CLRA-CYA.
           EXIT.
       
       CLR-KEY-CYA SECTION.
       INI-CLR-KEY-CYA.
*% INCLUDE INI_CLR_KEY_CYA OF 'COLBMCYA.TXT'
           MOVE SPACES TO CYA-CAI-IOPC IN CYA-FLD.
           MOVE ZEROES TO CYA-IIC-IOPC IN CYA-FLD.
           MOVE ZEROES TO CYA-NUM-ICAN IN CYA-FLD.
           MOVE ZEROES TO CYA-NUM-ICYA IN CYA-FLD.
           MOVE SPACES TO DBC-GLS-NOMC IN CYA-FLD.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN CYA-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN CYA-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN CYA-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN CYA-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_KEY_CYA OF 'COLBMCYA.TXT'
       FIN-CLR-KEY-CYA.
           EXIT.
       
       CLR-FLD-CYA SECTION.
       INI-CLR-FLD-CYA.
*% INCLUDE INI_CLR_FLD_CYA OF 'COLBMCYA.TXT'
           MOVE SPACES TO FRM-GLS-TVOC IN CYA-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN CYA-FLD.
           MOVE SPACES TO FRM-GLS-OFOC IN CYA-FLD.
           MOVE SPACES TO FRM-GLS-TM01 IN CYA-FLD.
           MOVE SPACES TO FRM-GLS-TCYA IN CYA-FLD.
           MOVE SPACES TO FRM-GLS-TM04 IN CYA-FLD.
           MOVE SPACES TO FRM-GLS-TM02 IN CYA-FLD.
           MOVE ZEROES TO FRM-VAL-MONX IN CYA-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN CYA-FLD.
           MOVE SPACES TO FRM-GLS-TM03 IN CYA-FLD.
           MOVE ZEROES TO FRM-VAL-VPES IN CYA-FLD.
           PERFORM CLR-OPC-CYA.
           PERFORM CLR-CYA-CYA.
           PERFORM CLR-DBC-CYA.
           PERFORM CLR-RDC-CYA.
*% INCLUDE FIN_CLR_FLD_CYA OF 'COLBMCYA.TXT'
       FIN-CLR-FLD-CYA.
           EXIT.
       
       CLR-OPC-CYA SECTION.
       INI-CLR-OPC-CYA.
*% INCLUDE INI_CLR_OPC_CYA OF 'COLBMCYA.TXT'
           MOVE SPACES TO OPC-MSC-TVOC IN CYA-FLD.
           MOVE SPACES TO OPC-COD-TOOC IN CYA-FLD.
           MOVE SPACES TO OPC-COD-OFOC IN CYA-FLD.
           MOVE SPACES TO OPC-COD-VCOC IN CYA-FLD.
*% INCLUDE FIN_CLR_OPC_CYA OF 'COLBMCYA.TXT'
       FIN-CLR-OPC-CYA.
           EXIT.
       
       CLR-CYA-CYA SECTION.
       INI-CLR-CYA-CYA.
*% INCLUDE INI_CLR_CYA_CYA OF 'COLBMCYA.TXT'
           MOVE SPACES TO CYA-IND-TCYA IN CYA-FLD.
           MOVE SPACES TO CYA-CAI-ICYA IN CYA-FLD.
           MOVE SPACES TO CYA-IIC-ICYA IN CYA-FLD.
           MOVE ZEROES TO CYA-SGV-LIQU IN CYA-FLD.
*% INCLUDE FIN_CLR_CYA_CYA OF 'COLBMCYA.TXT'
       FIN-CLR-CYA-CYA.
           EXIT.
       
       CLR-DBC-CYA SECTION.
       INI-CLR-DBC-CYA.
*% INCLUDE INI_CLR_DBC_CYA OF 'COLBMCYA.TXT'
*% INCLUDE FIN_CLR_DBC_CYA OF 'COLBMCYA.TXT'
       FIN-CLR-DBC-CYA.
           EXIT.
       
       CLR-RDC-CYA SECTION.
       INI-CLR-RDC-CYA.
*% INCLUDE INI_CLR_RDC_CYA OF 'COLBMCYA.TXT'
           MOVE SPACES TO RDC-CAI-ICLI IN CYA-FLD.
           MOVE SPACES TO RDC-IIC-ICLI IN CYA-FLD.
*% INCLUDE FIN_CLR_RDC_CYA OF 'COLBMCYA.TXT'
       FIN-CLR-RDC-CYA.
           EXIT.
       
       FST-KEY-CYA SECTION.
       INI-FST-KEY-CYA.
*% INCLUDE INI_FST_KEY_CYA OF 'COLBMCYA.TXT'
           MOVE -1 TO CYA-CAI-IOPC-LEN IN CYA-FLD.
*% INCLUDE FIN_FST_KEY_CYA OF 'COLBMCYA.TXT'
       FIN-FST-KEY-CYA.
           EXIT.
       
       FST-FLD-CYA SECTION.
       INI-FST-FLD-CYA.
*% INCLUDE INI_FST_FLD_CYA OF 'COLBMCYA.TXT'
           MOVE -1 TO OPC-MSC-TVOC-LEN IN CYA-FLD.
*% INCLUDE FIN_FST_FLD_CYA OF 'COLBMCYA.TXT'
       FIN-FST-FLD-CYA.
           EXIT.
       
       PRO-KEY-CYA SECTION.
       INI-PRO-KEY-CYA.
*% INCLUDE INI_PRO_KEY_CYA OF 'COLBMCYA.TXT'
           PERFORM PRO-IKY-CYA.
           PERFORM PRO-AKY-CYA.
           PERFORM PRO-MKY-CYA.
*% INCLUDE FIN_PRO_KEY_CYA OF 'COLBMCYA.TXT'
       FIN-PRO-KEY-CYA.
           EXIT.
       
       PRO-IKY-CYA SECTION.
       INI-PRO-IKY-CYA.
*% INCLUDE INI_PRO_IKY_CYA OF 'COLBMCYA.TXT'
           MOVE FRM-CPIM-AEY TO CYA-CAI-IOPC-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO CYA-IIC-IOPC-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO CYA-NUM-ICAN-ATR IN CYA-FLD.
*% INCLUDE FIN_PRO_IKY_CYA OF 'COLBMCYA.TXT'
       FIN-PRO-IKY-CYA.
           EXIT.
       
       PRO-MKY-CYA SECTION.
       INI-PRO-MKY-CYA.
*% INCLUDE INI_PRO_MKY_CYA OF 'COLBMCYA.TXT'
*% INCLUDE FIN_PRO_MKY_CYA OF 'COLBMCYA.TXT'
       FIN-PRO-MKY-CYA.
           EXIT.
       
       PRO-AKY-CYA SECTION.
       INI-PRO-AKY-CYA.
*% INCLUDE INI_PRO_AKY_CYA OF 'COLBMCYA.TXT'
           MOVE FRM-CPIM-AEY TO CYA-NUM-ICYA-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO DBC-GLS-NOMC-ATR IN CYA-FLD.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO DBC-NUM-ICLI-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO DBC-VRF-ICLI-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO DBC-IND-ICLI-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO DBC-GLS-ICLI-ATR IN CYA-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
*% INCLUDE FIN_PRO_AKY_CYA OF 'COLBMCYA.TXT'
       FIN-PRO-AKY-CYA.
           EXIT.
       
       UNP-KEY-CYA SECTION.
       INI-UNP-KEY-CYA.
*% INCLUDE INI_UNP_KEY_CYA OF 'COLBMCYA.TXT'
           PERFORM UNP-IKY-CYA.
           PERFORM UNP-AKY-CYA.
