       EDT-IKY-PCP SECTION.
       INI-EDT-IKY-PCP.
*% INCLUDE EDT_IKY_PCP OF 'COLBMPCP.TXT'
*% RREG = "PCP"
*% NOMB = "FPLA"
*% RREG = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF DBC-NUM-ICLI-LEN IN PCP-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DBC-NUM-ICLI-LEN IN PCP-FLD
                GO TO FIN-EDT-IKY-PCP.
           MOVE DBC-NUM-ICLI IN PCP-FLD TO
                DBC-NUM-ICLI-EDT IN PCP-EDT.
           MOVE DBC-NUM-ICLI-EDT IN PCP-EDT TO
                DBC-NUM-ICLI-ALF IN PCP-FLD.
      * IDC-FIN --> BMCHIBCI011.SRCAIU EDICION NUMERICA
*% INCLUDE FIN_EDT_IKY_PCP OF 'COLBMPCP.TXT'
       FIN-EDT-IKY-PCP.
           EXIT.
       
       EDT-MKY-PCP SECTION.
       INI-EDT-MKY-PCP.
*% INCLUDE EDT_MKY_PCP OF 'COLBMPCP.TXT'
*% INCLUDE FIN_EDT_MKY_PCP OF 'COLBMPCP.TXT'
       FIN-EDT-MKY-PCP.
           EXIT.
       
