       INI-EDT-AKY-PCP.
*% INCLUDE EDT_AKY_PCP OF 'COLBMPCP.TXT'
*% INCLUDE FIN_EDT_AKY_PCP OF 'COLBMPCP.TXT'
       FIN-EDT-AKY-PCP.
           EXIT.
       
       EDT-FLD-PCP SECTION.
       INI-EDT-FLD-PCP.
*% INCLUDE EDT_FLD_PCP OF 'COLBMPCP.TXT'
*% RREG = "PCP"
*% NOMB = "FTRN"
           IF PCP-HRA-HRTR-LEN IN PCP-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PCP-HRA-HRTR-LEN IN PCP-FLD
                GO TO FIN-EDT-FLD-PCP.
           MOVE PCP-HRA-HRTR IN PCP-FLD TO PCP-HRA-HRTR-EDT IN PCP-EDT.
           MOVE PCP-HRA-HRTR-EDT IN PCP-EDT TO 
                PCP-HRA-HRTR-ALF IN PCP-FLD.
           IF PCP-SGV-TIMP-LEN IN PCP-FLD(1) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PCP-SGV-TIMP-LEN IN PCP-FLD(1)
                GO TO FIN-EDT-FLD-PCP.
           MOVE PCP-SGV-TIMP IN PCP-FLD(1) TO 
                PCP-SGV-TIMP-EDT IN PCP-EDT.
           MOVE PCP-SGV-TIMP-EDT IN PCP-EDT TO 
                PCP-SGV-TIMP-ALF IN PCP-FLD(1).
           IF PCP-SGV-TIMP-LEN IN PCP-FLD(2) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PCP-SGV-TIMP-LEN IN PCP-FLD(2)
                GO TO FIN-EDT-FLD-PCP.
           MOVE PCP-SGV-TIMP IN PCP-FLD(2) TO 
                PCP-SGV-TIMP-EDT IN PCP-EDT.
           MOVE PCP-SGV-TIMP-EDT IN PCP-EDT TO 
                PCP-SGV-TIMP-ALF IN PCP-FLD(2).
           IF PCP-SGV-TIMP-LEN IN PCP-FLD(3) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PCP-SGV-TIMP-LEN IN PCP-FLD(3)
                GO TO FIN-EDT-FLD-PCP.
           MOVE PCP-SGV-TIMP IN PCP-FLD(3) TO 
                PCP-SGV-TIMP-EDT IN PCP-EDT.
           MOVE PCP-SGV-TIMP-EDT IN PCP-EDT TO 
                PCP-SGV-TIMP-ALF IN PCP-FLD(3).
           IF PCP-SGV-TIMP-LEN IN PCP-FLD(4) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PCP-SGV-TIMP-LEN IN PCP-FLD(4)
                GO TO FIN-EDT-FLD-PCP.
           MOVE PCP-SGV-TIMP IN PCP-FLD(4) TO 
                PCP-SGV-TIMP-EDT IN PCP-EDT.
           MOVE PCP-SGV-TIMP-EDT IN PCP-EDT TO 
                PCP-SGV-TIMP-ALF IN PCP-FLD(4).
           IF PCP-SGV-TIMP-LEN IN PCP-FLD(5) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PCP-SGV-TIMP-LEN IN PCP-FLD(5)
                GO TO FIN-EDT-FLD-PCP.
           MOVE PCP-SGV-TIMP IN PCP-FLD(5) TO 
                PCP-SGV-TIMP-EDT IN PCP-EDT.
           MOVE PCP-SGV-TIMP-EDT IN PCP-EDT TO 
                PCP-SGV-TIMP-ALF IN PCP-FLD(5).
           IF PCP-SGV-TIMP-LEN IN PCP-FLD(6) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PCP-SGV-TIMP-LEN IN PCP-FLD(6)
                GO TO FIN-EDT-FLD-PCP.
           MOVE PCP-SGV-TIMP IN PCP-FLD(6) TO 
                PCP-SGV-TIMP-EDT IN PCP-EDT.
           MOVE PCP-SGV-TIMP-EDT IN PCP-EDT TO 
                PCP-SGV-TIMP-ALF IN PCP-FLD(6).
           IF PCP-VAL-MTOP-LEN IN PCP-FLD(1) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PCP-VAL-MTOP-LEN IN PCP-FLD(1)
                GO TO FIN-EDT-FLD-PCP.
           MOVE PCP-VAL-MTOP IN PCP-FLD(1) TO 
                PCP-VAL-MTOP-EDT IN PCP-EDT.
           MOVE PCP-VAL-MTOP-EDT IN PCP-EDT TO 
                PCP-VAL-MTOP-ALF IN PCP-FLD(1).
           IF PCP-VAL-MTOP-LEN IN PCP-FLD(2) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PCP-VAL-MTOP-LEN IN PCP-FLD(2)
                GO TO FIN-EDT-FLD-PCP.
           MOVE PCP-VAL-MTOP IN PCP-FLD(2) TO 
                PCP-VAL-MTOP-EDT IN PCP-EDT.
           MOVE PCP-VAL-MTOP-EDT IN PCP-EDT TO 
                PCP-VAL-MTOP-ALF IN PCP-FLD(2).
           IF PCP-VAL-MTOP-LEN IN PCP-FLD(3) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PCP-VAL-MTOP-LEN IN PCP-FLD(3)
                GO TO FIN-EDT-FLD-PCP.
           MOVE PCP-VAL-MTOP IN PCP-FLD(3) TO 
                PCP-VAL-MTOP-EDT IN PCP-EDT.
           MOVE PCP-VAL-MTOP-EDT IN PCP-EDT TO 
                PCP-VAL-MTOP-ALF IN PCP-FLD(3).
           IF PCP-VAL-MTOP-LEN IN PCP-FLD(4) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PCP-VAL-MTOP-LEN IN PCP-FLD(4)
                GO TO FIN-EDT-FLD-PCP.
           MOVE PCP-VAL-MTOP IN PCP-FLD(4) TO 
                PCP-VAL-MTOP-EDT IN PCP-EDT.
           MOVE PCP-VAL-MTOP-EDT IN PCP-EDT TO 
                PCP-VAL-MTOP-ALF IN PCP-FLD(4).
           IF PCP-VAL-MTOP-LEN IN PCP-FLD(5) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PCP-VAL-MTOP-LEN IN PCP-FLD(5)
                GO TO FIN-EDT-FLD-PCP.
           MOVE PCP-VAL-MTOP IN PCP-FLD(5) TO 
                PCP-VAL-MTOP-EDT IN PCP-EDT.
           MOVE PCP-VAL-MTOP-EDT IN PCP-EDT TO 
                PCP-VAL-MTOP-ALF IN PCP-FLD(5).
           IF PCP-VAL-MTOP-LEN IN PCP-FLD(6) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PCP-VAL-MTOP-LEN IN PCP-FLD(6)
                GO TO FIN-EDT-FLD-PCP.
           MOVE PCP-VAL-MTOP IN PCP-FLD(6) TO 
                PCP-VAL-MTOP-EDT IN PCP-EDT.
           MOVE PCP-VAL-MTOP-EDT IN PCP-EDT TO 
                PCP-VAL-MTOP-ALF IN PCP-FLD(6).
           IF PCP-VAL-COMI-LEN IN PCP-FLD(1) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PCP-VAL-COMI-LEN IN PCP-FLD(1)
                GO TO FIN-EDT-FLD-PCP.
           MOVE PCP-VAL-COMI IN PCP-FLD(1) TO 
                PCP-VAL-COMI-EDT IN PCP-EDT.
           MOVE PCP-VAL-COMI-EDT IN PCP-EDT TO 
                PCP-VAL-COMI-ALF IN PCP-FLD(1).
           IF PCP-VAL-COMI-LEN IN PCP-FLD(2) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PCP-VAL-COMI-LEN IN PCP-FLD(2)
                GO TO FIN-EDT-FLD-PCP.
           MOVE PCP-VAL-COMI IN PCP-FLD(2) TO 
                PCP-VAL-COMI-EDT IN PCP-EDT.
           MOVE PCP-VAL-COMI-EDT IN PCP-EDT TO 
                PCP-VAL-COMI-ALF IN PCP-FLD(2).
           IF PCP-VAL-COMI-LEN IN PCP-FLD(3) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PCP-VAL-COMI-LEN IN PCP-FLD(3)
                GO TO FIN-EDT-FLD-PCP.
           MOVE PCP-VAL-COMI IN PCP-FLD(3) TO 
                PCP-VAL-COMI-EDT IN PCP-EDT.
           MOVE PCP-VAL-COMI-EDT IN PCP-EDT TO 
