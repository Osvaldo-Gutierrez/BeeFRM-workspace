*% RREG = "PCP"
*% NOMB = "FTRN"
*% OBL = "S"
*% LBL = "PCP_FEC_FTRN OF 'COLBMPCP.TXT'"
*% IZR = "S"
*% ZRO = "S"
*% FRM_IFLD = ""
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-PCP-FEC-FTRN SECTION.
       INI-VAL-PCP-FEC-FTRN.
           IF PCP-FEC-FTRN IN PCP-FLD  NOT > ZEROES
              MOVE ZEROES TO PCP-FEC-FTRN IN PCP-FLD.
*% INCLUDE INI_PCP_FEC_FTRN OF 'COLBMPCP.TXT'
*% INCLUDE PCP_FEC_FTRN OF 'COLBMPCP.TXT'
