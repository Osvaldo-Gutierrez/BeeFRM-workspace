      * 11-Apr-96 11:43 AM
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
       PUT-ALL-QDL SECTION.
       INI-PUT-ALL-QDL.
*% INCLUDE INI_PUT_ALL_QDL OF 'COLBMQDL.TXT'
           PERFORM PUT-DLC-QDL.
*% INCLUDE FIN_PUT_ALL_QDL OF 'COLBMQDL.TXT'
       FIN-PUT-ALL-QDL.
           EXIT.
       
       PUT-DLC-QDL SECTION.
       INI-PUT-DLC-QDL.
*% INCLUDE INI_PUT_DLC_QDL OF 'COLBMQDL.TXT'
           IF PGM-STAT-DLC-NUL
               GO TO FIN-PUT-DLC-QDL.
           IF NOT PGM-STAT-DLC-OKS
               PERFORM CLR-DLC-QDL
               GO TO FIN-PUT-DLC-QDL.
           MOVE DLC-CAI-IOPC IN DLC TO DLC-CAI-IOPC IN QDL-FLD.
           MOVE DLC-IIC-IOPC IN DLC TO DLC-IIC-IOPC IN QDL-FLD.
           MOVE DLC-NUM-IDLC IN DLC TO DLC-NUM-IDLC IN QDL-FLD.
           MOVE DLC-VAL-NDLC IN DLC TO DLC-VAL-NDLC IN QDL-FLD.
           MOVE DLC-VAL-SDLC IN DLC TO DLC-VAL-SDLC IN QDL-FLD.
           MOVE DLC-VAL-IMPU IN DLC TO DLC-VAL-IMPU IN QDL-FLD.
           MOVE DLC-VAL-GNOT IN DLC TO DLC-VAL-GNOT IN QDL-FLD.
           MOVE DLC-VAL-READ IN DLC TO DLC-VAL-READ IN QDL-FLD.
           MOVE DLC-VAL-INTD IN DLC TO DLC-VAL-INTD IN QDL-FLD.
           MOVE DLC-NUM-DDEV IN DLC TO DLC-NUM-DDEV IN QDL-FLD.
           MOVE DLC-NUM-MDEV IN DLC TO DLC-NUM-MDEV IN QDL-FLD.
           MOVE DLC-NUM-SDEV IN DLC TO DLC-NUM-SDEV IN QDL-FLD.
           MOVE DLC-NUM-ADEV IN DLC TO DLC-NUM-ADEV IN QDL-FLD.
           MOVE DLC-VAL-SINT IN DLC TO DLC-VAL-SINT IN QDL-FLD.
           MOVE DLC-VAL-SCPA IN DLC TO DLC-VAL-SCPA IN QDL-FLD.
           MOVE DLC-NUM-DAMT IN DLC TO DLC-NUM-DAMT IN QDL-FLD.
           MOVE DLC-NUM-MAMT IN DLC TO DLC-NUM-MAMT IN QDL-FLD.
           MOVE DLC-NUM-SAMT IN DLC TO DLC-NUM-SAMT IN QDL-FLD.
           MOVE DLC-NUM-AAMT IN DLC TO DLC-NUM-AAMT IN QDL-FLD.
           MOVE DLC-VAL-IPEN IN DLC TO DLC-VAL-IPEN IN QDL-FLD.
           MOVE DLC-VAL-DPES IN DLC TO DLC-VAL-DPES IN QDL-FLD.
           MOVE DLC-NUM-DMAV IN DLC TO DLC-NUM-DMAV IN QDL-FLD.
           MOVE DLC-NUM-MMAV IN DLC TO DLC-NUM-MMAV IN QDL-FLD.
           MOVE DLC-NUM-SMAV IN DLC TO DLC-NUM-SMAV IN QDL-FLD.
           MOVE DLC-NUM-AMAV IN DLC TO DLC-NUM-AMAV IN QDL-FLD.
           MOVE DLC-CAI-IOCO IN DLC TO DLC-CAI-IOCO IN QDL-FLD.
           MOVE DLC-IIC-IOCO IN DLC TO DLC-IIC-IOCO IN QDL-FLD.
           MOVE DLC-NUM-IDLO IN DLC TO DLC-NUM-IDLO IN QDL-FLD.
           MOVE DLC-IND-SCBZ IN DLC TO DLC-IND-SCBZ IN QDL-FLD.
           MOVE DLC-IND-SCRT IN DLC TO DLC-IND-SCRT IN QDL-FLD.
           MOVE DLC-IND-SCTB IN DLC TO DLC-IND-SCTB IN QDL-FLD.
           MOVE DLC-COD-UBFD IN DLC TO DLC-COD-UBFD IN QDL-FLD.
           MOVE DLC-COD-PLZA IN DLC TO DLC-COD-PLZA IN QDL-FLD.
           MOVE DLC-COD-PLZO IN DLC TO DLC-COD-PLZO IN QDL-FLD.
*% INCLUDE FIN_PUT_DLC_QDL OF 'COLBMQDL.TXT'
       FIN-PUT-DLC-QDL.
           EXIT.
