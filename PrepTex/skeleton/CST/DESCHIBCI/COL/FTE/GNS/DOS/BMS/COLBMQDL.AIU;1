       EDT-KEY-QDL SECTION.
       INI-EDT-KEY-QDL.
           PERFORM EDT-IKY-QDL.
           PERFORM EDT-MKY-QDL.
           PERFORM EDT-AKY-QDL.
*% INCLUDE EDT_KEY_QDL OF 'COLBMQDL.TXT'
       FIN-EDT-KEY-QDL.
           EXIT.
       
       EDT-IKY-QDL SECTION.
       INI-EDT-IKY-QDL.
*% INCLUDE EDT_IKY_QDL OF 'COLBMQDL.TXT'
           IF DLC-IIC-IOPC-LEN IN QDL-FLD = -9
                MOVE -1 TO DLC-IIC-IOPC-LEN IN QDL-FLD
                GO TO FIN-EDT-IKY-QDL.
           MOVE DLC-IIC-IOPC IN QDL-FLD TO DLC-IIC-IOPC-EDT IN QDL-EDT.
           MOVE DLC-IIC-IOPC-EDT IN QDL-EDT TO 
                DLC-IIC-IOPC-ALF IN QDL-FLD.
           IF DLC-NUM-IDLC-LEN IN QDL-FLD = -9
                MOVE -1 TO DLC-NUM-IDLC-LEN IN QDL-FLD
                GO TO FIN-EDT-IKY-QDL.
           MOVE DLC-NUM-IDLC IN QDL-FLD TO DLC-NUM-IDLC-EDT IN QDL-EDT.
           MOVE DLC-NUM-IDLC-EDT IN QDL-EDT TO 
                DLC-NUM-IDLC-ALF IN QDL-FLD.
*% INCLUDE FIN_EDT_IKY_QDL OF 'COLBMQDL.TXT'
       FIN-EDT-IKY-QDL.
           EXIT.
       
       EDT-MKY-QDL SECTION.
       INI-EDT-MKY-QDL.
*% INCLUDE EDT_MKY_QDL OF 'COLBMQDL.TXT'
*% INCLUDE FIN_EDT_MKY_QDL OF 'COLBMQDL.TXT'
       FIN-EDT-MKY-QDL.
           EXIT.
       
       EDT-AKY-QDL SECTION.
       INI-EDT-AKY-QDL.
*% INCLUDE EDT_AKY_QDL OF 'COLBMQDL.TXT'
*% INCLUDE FIN_EDT_AKY_QDL OF 'COLBMQDL.TXT'
       FIN-EDT-AKY-QDL.
           EXIT.
       
       EDT-FLD-QDL SECTION.
       INI-EDT-FLD-QDL.
*% INCLUDE EDT_FLD_QDL OF 'COLBMQDL.TXT'
           IF DLC-VAL-NDLC-LEN IN QDL-FLD = -9
                MOVE -1 TO DLC-VAL-NDLC-LEN IN QDL-FLD
                GO TO FIN-EDT-FLD-QDL.
           MOVE DLC-VAL-NDLC IN QDL-FLD TO DLC-VAL-NDLC-EDT IN QDL-EDT.
           MOVE DLC-VAL-NDLC-EDT IN QDL-EDT TO 
                DLC-VAL-NDLC-ALF IN QDL-FLD.
           IF DLC-VAL-SDLC-LEN IN QDL-FLD = -9
                MOVE -1 TO DLC-VAL-SDLC-LEN IN QDL-FLD
                GO TO FIN-EDT-FLD-QDL.
           MOVE DLC-VAL-SDLC IN QDL-FLD TO DLC-VAL-SDLC-EDT IN QDL-EDT.
           MOVE DLC-VAL-SDLC-EDT IN QDL-EDT TO 
                DLC-VAL-SDLC-ALF IN QDL-FLD.
           IF DLC-VAL-IMPU-LEN IN QDL-FLD = -9
                MOVE -1 TO DLC-VAL-IMPU-LEN IN QDL-FLD
                GO TO FIN-EDT-FLD-QDL.
           MOVE DLC-VAL-IMPU IN QDL-FLD TO DLC-VAL-IMPU-EDT IN QDL-EDT.
           MOVE DLC-VAL-IMPU-EDT IN QDL-EDT TO 
                DLC-VAL-IMPU-ALF IN QDL-FLD.
           IF DLC-VAL-GNOT-LEN IN QDL-FLD = -9
                MOVE -1 TO DLC-VAL-GNOT-LEN IN QDL-FLD
                GO TO FIN-EDT-FLD-QDL.
           MOVE DLC-VAL-GNOT IN QDL-FLD TO DLC-VAL-GNOT-EDT IN QDL-EDT.
           MOVE DLC-VAL-GNOT-EDT IN QDL-EDT TO 
                DLC-VAL-GNOT-ALF IN QDL-FLD.
           IF DLC-VAL-READ-LEN IN QDL-FLD = -9
                MOVE -1 TO DLC-VAL-READ-LEN IN QDL-FLD
                GO TO FIN-EDT-FLD-QDL.
           MOVE DLC-VAL-READ IN QDL-FLD TO DLC-VAL-READ-EDT IN QDL-EDT.
           MOVE DLC-VAL-READ-EDT IN QDL-EDT TO 
                DLC-VAL-READ-ALF IN QDL-FLD.
           IF DLC-VAL-INTD-LEN IN QDL-FLD = -9
                MOVE -1 TO DLC-VAL-INTD-LEN IN QDL-FLD
                GO TO FIN-EDT-FLD-QDL.
           MOVE DLC-VAL-INTD IN QDL-FLD TO DLC-VAL-INTD-EDT IN QDL-EDT.
           MOVE DLC-VAL-INTD-EDT IN QDL-EDT TO 
                DLC-VAL-INTD-ALF IN QDL-FLD.
           IF DLC-VAL-SINT-LEN IN QDL-FLD = -9
                MOVE -1 TO DLC-VAL-SINT-LEN IN QDL-FLD
                GO TO FIN-EDT-FLD-QDL.
           MOVE DLC-VAL-SINT IN QDL-FLD TO DLC-VAL-SINT-EDT IN QDL-EDT.
           MOVE DLC-VAL-SINT-EDT IN QDL-EDT TO 
                DLC-VAL-SINT-ALF IN QDL-FLD.
           IF DLC-VAL-SCPA-LEN IN QDL-FLD = -9
                MOVE -1 TO DLC-VAL-SCPA-LEN IN QDL-FLD
                GO TO FIN-EDT-FLD-QDL.
           MOVE DLC-VAL-SCPA IN QDL-FLD TO DLC-VAL-SCPA-EDT IN QDL-EDT.
           MOVE DLC-VAL-SCPA-EDT IN QDL-EDT TO 
                DLC-VAL-SCPA-ALF IN QDL-FLD.
           IF DLC-VAL-IPEN-LEN IN QDL-FLD = -9
                MOVE -1 TO DLC-VAL-IPEN-LEN IN QDL-FLD
                GO TO FIN-EDT-FLD-QDL.
           MOVE DLC-VAL-IPEN IN QDL-FLD TO DLC-VAL-IPEN-EDT IN QDL-EDT.
           MOVE DLC-VAL-IPEN-EDT IN QDL-EDT TO 
                DLC-VAL-IPEN-ALF IN QDL-FLD.
           IF DLC-VAL-DPES-LEN IN QDL-FLD = -9
                MOVE -1 TO DLC-VAL-DPES-LEN IN QDL-FLD
                GO TO FIN-EDT-FLD-QDL.
           MOVE DLC-VAL-DPES IN QDL-FLD TO DLC-VAL-DPES-EDT IN QDL-EDT.
           MOVE DLC-VAL-DPES-EDT IN QDL-EDT TO 
                DLC-VAL-DPES-ALF IN QDL-FLD.
           IF DLC-IIC-IOCO-LEN IN QDL-FLD = -9
                MOVE -1 TO DLC-IIC-IOCO-LEN IN QDL-FLD
                GO TO FIN-EDT-FLD-QDL.
           MOVE DLC-IIC-IOCO IN QDL-FLD TO DLC-IIC-IOCO-EDT IN QDL-EDT.
           MOVE DLC-IIC-IOCO-EDT IN QDL-EDT TO 
                DLC-IIC-IOCO-ALF IN QDL-FLD.
           IF DLC-NUM-IDLO-LEN IN QDL-FLD = -9
                MOVE -1 TO DLC-NUM-IDLO-LEN IN QDL-FLD
                GO TO FIN-EDT-FLD-QDL.
           MOVE DLC-NUM-IDLO IN QDL-FLD TO DLC-NUM-IDLO-EDT IN QDL-EDT.
           MOVE DLC-NUM-IDLO-EDT IN QDL-EDT TO 
                DLC-NUM-IDLO-ALF IN QDL-FLD.
*% INCLUDE FIN_EDT_FLD_QDL OF 'COLBMQDL.TXT'
       FIN-EDT-FLD-QDL.
           EXIT.
