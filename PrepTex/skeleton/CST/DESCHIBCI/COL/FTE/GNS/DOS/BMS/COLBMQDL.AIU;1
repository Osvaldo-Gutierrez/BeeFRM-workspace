       VAL-KEY-QDL SECTION.
       INI-VAL-KEY-QDL.
           PERFORM VAL-IKY-QDL.
           PERFORM VAL-MKY-QDL.
           PERFORM VAL-AKY-QDL.
       FIN-VAL-KEY-QDL.
           EXIT.
       
       VAL-IKY-QDL SECTION.
       INI-VAL-IKY-QDL.
*% INCLUDE INI_VAL_IKY_QDL OF 'COLBMQDL.TXT'
           PERFORM VAL-DLC-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-CAI-IOPC-LEN IN QDL-FLD
                GO TO FIN-VAL-IKY-QDL.
           PERFORM VAL-DLC-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-IIC-IOPC-LEN IN QDL-FLD
                GO TO FIN-VAL-IKY-QDL.
           PERFORM VAL-DLC-NUM-IDLC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-NUM-IDLC-LEN IN QDL-FLD
                GO TO FIN-VAL-IKY-QDL.
*% INCLUDE FIN_VAL_IKY_QDL OF 'COLBMQDL.TXT'
       FIN-VAL-IKY-QDL.
           EXIT.
       
       VAL-MKY-QDL SECTION.
       INI-VAL-MKY-QDL.
*% INCLUDE INI_VAL_MKY_QDL OF 'COLBMQDL.TXT'
*% INCLUDE FIN_VAL_MKY_QDL OF 'COLBMQDL.TXT'
       FIN-VAL-MKY-QDL.
           EXIT.
       
       VAL-AKY-QDL SECTION.
       INI-VAL-AKY-QDL.
*% INCLUDE INI_VAL_AKY_QDL OF 'COLBMQDL.TXT'
*% INCLUDE FIN_VAL_AKY_QDL OF 'COLBMQDL.TXT'
       FIN-VAL-AKY-QDL.
           EXIT.
       
       VAL-FLD-QDL SECTION.
       INI-VAL-FLD-QDL.
*% INCLUDE INI_VAL_FLD_QDL OF 'COLBMQDL.TXT'
           PERFORM VAL-DLC-VAL-NDLC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-VAL-NDLC-LEN IN QDL-FLD
                GO TO FIN-VAL-FLD-QDL.
           PERFORM VAL-DLC-VAL-SDLC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-VAL-SDLC-LEN IN QDL-FLD
                GO TO FIN-VAL-FLD-QDL.
           PERFORM VAL-DLC-VAL-IMPU.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-VAL-IMPU-LEN IN QDL-FLD
                GO TO FIN-VAL-FLD-QDL.
           PERFORM VAL-DLC-VAL-GNOT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-VAL-GNOT-LEN IN QDL-FLD
                GO TO FIN-VAL-FLD-QDL.
           PERFORM VAL-DLC-VAL-READ.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-VAL-READ-LEN IN QDL-FLD
                GO TO FIN-VAL-FLD-QDL.
           PERFORM VAL-DLC-VAL-INTD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-VAL-INTD-LEN IN QDL-FLD
                GO TO FIN-VAL-FLD-QDL.
           PERFORM VAL-DLC-FEC-FDEV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-FEC-FDEV-LEN IN QDL-FLD
                GO TO FIN-VAL-FLD-QDL.
           PERFORM VAL-DLC-VAL-SINT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-VAL-SINT-LEN IN QDL-FLD
                GO TO FIN-VAL-FLD-QDL.
           PERFORM VAL-DLC-VAL-SCPA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-VAL-SCPA-LEN IN QDL-FLD
                GO TO FIN-VAL-FLD-QDL.
           PERFORM VAL-DLC-FEC-FAMT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-FEC-FAMT-LEN IN QDL-FLD
                GO TO FIN-VAL-FLD-QDL.
           PERFORM VAL-DLC-VAL-IPEN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-VAL-IPEN-LEN IN QDL-FLD
                GO TO FIN-VAL-FLD-QDL.
           PERFORM VAL-DLC-VAL-DPES.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-VAL-DPES-LEN IN QDL-FLD
                GO TO FIN-VAL-FLD-QDL.
           PERFORM VAL-DLC-FEC-FMAV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-FEC-FMAV-LEN IN QDL-FLD
                GO TO FIN-VAL-FLD-QDL.
           PERFORM VAL-DLC-CAI-IOCO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-CAI-IOCO-LEN IN QDL-FLD
                GO TO FIN-VAL-FLD-QDL.
           PERFORM VAL-DLC-IIC-IOCO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-IIC-IOCO-LEN IN QDL-FLD
                GO TO FIN-VAL-FLD-QDL.
           PERFORM VAL-DLC-NUM-IDLO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-NUM-IDLO-LEN IN QDL-FLD
                GO TO FIN-VAL-FLD-QDL.
           PERFORM VAL-DLC-IND-SCBZ.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-IND-SCBZ-LEN IN QDL-FLD
                GO TO FIN-VAL-FLD-QDL.
           PERFORM VAL-DLC-IND-SCRT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-IND-SCRT-LEN IN QDL-FLD
                GO TO FIN-VAL-FLD-QDL.
           PERFORM VAL-DLC-IND-SCTB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-IND-SCTB-LEN IN QDL-FLD
                GO TO FIN-VAL-FLD-QDL.
           PERFORM VAL-DLC-COD-UBFD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-COD-UBFD-LEN IN QDL-FLD
                GO TO FIN-VAL-FLD-QDL.
           PERFORM VAL-DLC-COD-PLZA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-COD-PLZA-LEN IN QDL-FLD
                GO TO FIN-VAL-FLD-QDL.
           PERFORM VAL-DLC-COD-PLZO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-COD-PLZO-LEN IN QDL-FLD
                GO TO FIN-VAL-FLD-QDL.
*% INCLUDE FIN_VAL_FLD_QDL OF 'COLBMQDL.TXT'
       FIN-VAL-FLD-QDL.
           EXIT.
       
