      *
      * COLBQCUR
      * Version 3.0.1 Nov-2005
      * 03-Oct-14 04:03 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CUR"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMCUR.TXT'
       SET-TAG-COLBQCUR SECTION.
       INI-SET-TAG-COLBQCUR.
           MOVE
           '<IDNMOD NOM=$COLBQCUR$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$03-Oct-14 04:03 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCUR.
           EXIT.
           
       PUT-ALL-CUR SECTION.
       INI-PUT-ALL-CUR.
*% INCLUDE INI_PUT_ALL_CUR OF 'COLBMCUR.TXT'
           PERFORM PUT-LDA-CUR.
*% INCLUDE FIN_PUT_ALL_CUR OF 'COLBMCUR.TXT'
       FIN-PUT-ALL-CUR.
           EXIT.
       
       PUT-LDA-CUR SECTION.
       INI-PUT-LDA-CUR.
*% INCLUDE INI_PUT_LDA_CUR OF 'COLBMCUR.TXT'
           IF PGM-STAT-LDA-NUL
               GO TO FIN-PUT-LDA-CUR.
           IF NOT PGM-STAT-LDA-OKS
               PERFORM CLR-LDA-CUR
               GO TO FIN-PUT-LDA-CUR.
           MOVE LDA-NUM-SIS IN LDA TO LDA-NUM-SIS IN CUR-FLD.
           MOVE LDA-COD-CRR IN LDA TO LDA-COD-CRR IN CUR-FLD.
           MOVE LDA-COD-OFI IN LDA TO LDA-COD-OFI IN CUR-FLD.
           MOVE LDA-VAL-AUT IN LDA TO LDA-VAL-AUT IN CUR-FLD.
           MOVE LDA-COD-EJE IN LDA TO LDA-COD-EJE IN CUR-FLD.
*% RREG = "LDA"
*% RREG_FRM = "LDA"
*% NOMB = "ING-CMP"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE LDA-NUM-DNG-CMP IN LDA TO LDA-NUM-DNG-CMP IN CUR-FLD.
           MOVE LDA-NUM-MNG-CMP IN LDA TO LDA-NUM-MNG-CMP IN CUR-FLD.
           MOVE LDA-NUM-SNG-CMP IN LDA TO LDA-NUM-SNG-CMP IN CUR-FLD.
           MOVE LDA-NUM-ANG-CMP IN LDA TO LDA-NUM-ANG-CMP IN CUR-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% RREG = "LDA"
*% RREG_FRM = "LDA"
*% NOMB = "CUR-REA"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE LDA-NUM-DUR-REA IN LDA TO LDA-NUM-DUR-REA IN CUR-FLD.
           MOVE LDA-NUM-MUR-REA IN LDA TO LDA-NUM-MUR-REA IN CUR-FLD.
           MOVE LDA-NUM-SUR-REA IN LDA TO LDA-NUM-SUR-REA IN CUR-FLD.
           MOVE LDA-NUM-AUR-REA IN LDA TO LDA-NUM-AUR-REA IN CUR-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% RREG = "LDA"
*% RREG_FRM = "LDA"
*% NOMB = "VTO-LIN"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE LDA-NUM-DTO-LIN IN LDA TO LDA-NUM-DTO-LIN IN CUR-FLD.
           MOVE LDA-NUM-MTO-LIN IN LDA TO LDA-NUM-MTO-LIN IN CUR-FLD.
           MOVE LDA-NUM-STO-LIN IN LDA TO LDA-NUM-STO-LIN IN CUR-FLD.
           MOVE LDA-NUM-ATO-LIN IN LDA TO LDA-NUM-ATO-LIN IN CUR-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE LDA-COD-EST IN LDA TO LDA-COD-EST IN CUR-FLD.
*% BUF = "CUR-FLD"
*% SIS = "TAB"
*% TAB = "OFI "
*% FLD = "LDA-COD-OFI"
*% GLS = "FRM-GLS-OFI"
*% TIENE_GLOSA = TRUE
*% LGL = 15
*% INCLUDE 'BUSOFI          '
*% BUF = "CUR-FLD"
*% SIS = "COL"
*% TAB = "LDA "
*% FLD = "LDA-COD-EST"
*% GLS = "FRM-GLS-EST"
*% TIENE_GLOSA = TRUE
*% LGL = 12
*% INCLUDE 'BUSTAB'
*% INCLUDE FIN_PUT_LDA_CUR OF 'COLBMCUR.TXT'
       FIN-PUT-LDA-CUR.
           EXIT.
       
       KEY-ALL-CUR SECTION.
       INI-KEY-ALL-CUR.
*% INCLUDE INI_KEY_ALL_CUR OF 'COLBMCUR.TXT'
           PERFORM KEY-LDA-CUR.
*% INCLUDE FIN_KEY_ALL_CUR OF 'COLBMCUR.TXT'
       FIN-KEY-ALL-CUR.
           EXIT.
       
       KEY-LDA-CUR SECTION.
       INI-KEY-LDA-CUR.
*% INCLUDE INI_KEY_LDA_CUR OF 'COLBMCUR.TXT'
           MOVE LDA-NUM-SIS IN CUR-FLD TO LDA-NUM-SIS IN LDA.
           MOVE LDA-COD-CRR IN CUR-FLD TO LDA-COD-CRR IN LDA.
*% INCLUDE FIN_KEY_LDA_CUR OF 'COLBMCUR.TXT'
       FIN-KEY-LDA-CUR.
           EXIT.
       
       SCR-CLRA-CUR SECTION.
       INI-SCR-CLRA-CUR.
*% INCLUDE INI_SCR_CLRA_CUR OF 'COLBMCUR.TXT'
           PERFORM CLR-KEY-CUR.
           PERFORM CLR-FLD-CUR.
*% INCLUDE FIN_SCR_CLRA_CUR OF 'COLBMCUR.TXT'
       FIN-SCR-CLRA-CUR.
           EXIT.
       
       CLR-KEY-CUR SECTION.
       INI-CLR-KEY-CUR.
*% INCLUDE INI_CLR_KEY_CUR OF 'COLBMCUR.TXT'
           MOVE SPACES TO LDA-NUM-SIS IN CUR-FLD.
           MOVE ZEROES TO LDA-COD-CRR IN CUR-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * VRF-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-ICLI IN CUR-FLD.
           MOVE SPACES TO FRM-VRF-ICLI IN CUR-FLD.
      * VRF-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_KEY_CUR OF 'COLBMCUR.TXT'
       FIN-CLR-KEY-CUR.
           EXIT.
       
       CLR-FLD-CUR SECTION.
       INI-CLR-FLD-CUR.
*% INCLUDE INI_CLR_FLD_CUR OF 'COLBMCUR.TXT'
           MOVE SPACES TO FRM-GLS-OFI IN CUR-FLD.
           MOVE SPACES TO FRM-GLS-NOMC IN CUR-FLD.
           MOVE ZEROES TO FRM-COD-CRR IN CUR-FLD.
           MOVE SPACES TO FRM-GLS-EST IN CUR-FLD.
           PERFORM CLR-LDA-CUR.
*% INCLUDE FIN_CLR_FLD_CUR OF 'COLBMCUR.TXT'
       FIN-CLR-FLD-CUR.
           EXIT.
       
       CLR-LDA-CUR SECTION.
       INI-CLR-LDA-CUR.
*% INCLUDE INI_CLR_LDA_CUR OF 'COLBMCUR.TXT'
           MOVE SPACES TO LDA-COD-OFI IN CUR-FLD.
           MOVE ZEROES TO LDA-VAL-AUT IN CUR-FLD.
           MOVE SPACES TO LDA-COD-EJE IN CUR-FLD.
*% RREG = "LDA"
*% RREG_FRM = "LDA"
*% NOMB = "ING-CMP"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO LDA-FEC-ING-CMP IN CUR-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "LDA"
*% RREG_FRM = "LDA"
*% NOMB = "CUR-REA"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO LDA-FEC-CUR-REA IN CUR-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "LDA"
*% RREG_FRM = "LDA"
*% NOMB = "VTO-LIN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO LDA-FEC-VTO-LIN IN CUR-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO LDA-COD-EST IN CUR-FLD.
*% INCLUDE FIN_CLR_LDA_CUR OF 'COLBMCUR.TXT'
       FIN-CLR-LDA-CUR.
           EXIT.
       
       FST-KEY-CUR SECTION.
       INI-FST-KEY-CUR.
*% INCLUDE INI_FST_KEY_CUR OF 'COLBMCUR.TXT'
           MOVE -1 TO LDA-NUM-SIS-LEN IN CUR-FLD.
*% INCLUDE FIN_FST_KEY_CUR OF 'COLBMCUR.TXT'
       FIN-FST-KEY-CUR.
           EXIT.
       
       FST-FLD-CUR SECTION.
       INI-FST-FLD-CUR.
*% INCLUDE INI_FST_FLD_CUR OF 'COLBMCUR.TXT'
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * VRF-INI --> BMCHIBCI004.SRCAIU ( FST-KEY Y FST-FLD )
           MOVE FRM-POS-CURS TO FRM-NUM-ICLI-LEN IN CUR-FLD.
*% INCLUDE FIN_FST_FLD_CUR OF 'COLBMCUR.TXT'
       FIN-FST-FLD-CUR.
           EXIT.
       
       PRO-KEY-CUR SECTION.
       INI-PRO-KEY-CUR.
*% INCLUDE INI_PRO_KEY_CUR OF 'COLBMCUR.TXT'
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * VRF-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-NUM-ICLI-ATR IN CUR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VRF-ICLI-ATR IN CUR-FLD.
      * VRF-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           PERFORM PRO-IKY-CUR.
           PERFORM PRO-AKY-CUR.
           PERFORM PRO-MKY-CUR.
*% INCLUDE FIN_PRO_KEY_CUR OF 'COLBMCUR.TXT'
       FIN-PRO-KEY-CUR.
           EXIT.
       
       PRO-IKY-CUR SECTION.
       INI-PRO-IKY-CUR.
*% INCLUDE INI_PRO_IKY_CUR OF 'COLBMCUR.TXT'
           MOVE FRM-CPIM-AEY TO LDA-NUM-SIS-ATR IN CUR-FLD.
           MOVE FRM-CPIM-AEY TO LDA-COD-CRR-ATR IN CUR-FLD.
*% INCLUDE FIN_PRO_IKY_CUR OF 'COLBMCUR.TXT'
       FIN-PRO-IKY-CUR.
           EXIT.
       
       PRO-MKY-CUR SECTION.
       INI-PRO-MKY-CUR.
*% INCLUDE INI_PRO_MKY_CUR OF 'COLBMCUR.TXT'
*% INCLUDE FIN_PRO_MKY_CUR OF 'COLBMCUR.TXT'
       FIN-PRO-MKY-CUR.
           EXIT.
       
       PRO-AKY-CUR SECTION.
       INI-PRO-AKY-CUR.
*% INCLUDE INI_PRO_AKY_CUR OF 'COLBMCUR.TXT'
*% INCLUDE FIN_PRO_AKY_CUR OF 'COLBMCUR.TXT'
       FIN-PRO-AKY-CUR.
           EXIT.
       
       UNP-KEY-CUR SECTION.
       INI-UNP-KEY-CUR.
*% INCLUDE INI_UNP_KEY_CUR OF 'COLBMCUR.TXT'
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
           MOVE FRM-CPIM-UBY TO FRM-NUM-ICLI-ATR IN CUR-FLD.
           MOVE FRM-CPIM-UBY TO FRM-VRF-ICLI-ATR IN CUR-FLD.
           PERFORM UNP-IKY-CUR.
           PERFORM UNP-AKY-CUR.
           PERFORM UNP-MKY-CUR.
*% INCLUDE FIN_UNP_KEY_CUR OF 'COLBMCUR.TXT'
       FIN-UNP-KEY-CUR.
           EXIT.
       
       UNP-IKY-CUR SECTION.
       INI-UNP-IKY-CUR.
*% INCLUDE INI_UNP_IKY_CUR OF 'COLBMCUR.TXT'
           MOVE FRM-CPIM-UBY TO LDA-NUM-SIS-ATR.
           MOVE FRM-CPIM-UBY TO LDA-COD-CRR-ATR.
*% INCLUDE FIN_UNP_IKY_CUR OF 'COLBMCUR.TXT'
       FIN-UNP-IKY-CUR.
           EXIT.
       
       UNP-MKY-CUR SECTION.
       INI-UNP-MKY-CUR.
*% INCLUDE INI_UNP_MKY_CUR OF 'COLBMCUR.TXT'
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
           MOVE FRM-CPIM-UBY TO FRM-NUM-ICLI-ATR IN CUR-FLD.
           MOVE FRM-CPIM-UBY TO FRM-VRF-ICLI-ATR IN CUR-FLD.
*% INCLUDE FIN_UNP_MKY_CUR OF 'COLBMCUR.TXT'
       FIN-UNP-MKY-CUR.
           EXIT.
       
       UNP-AKY-CUR SECTION.
       INI-UNP-AKY-CUR.
*% INCLUDE INI_UNP_AKY_CUR OF 'COLBMCUR.TXT'
*% INCLUDE FIN_UNP_AKY_CUR OF 'COLBMCUR.TXT'
       FIN-UNP-AKY-CUR.
           EXIT.
       
       UNP-FLD-CUR SECTION.
       INI-UNP-FLD-CUR.
*% INCLUDE INI_UNP_FLD_CUR OF 'COLBMCUR.TXT'
           MOVE FRM-CPIM-UBY TO FRM-COD-CRR-ATR IN CUR-FLD.
           PERFORM UNP-LDA-CUR.
*% INCLUDE FIN_UNP_FLD_CUR OF 'COLBMCUR.TXT'
       FIN-UNP-FLD-CUR.
           EXIT.
       
       UNP-LDA-CUR SECTION.
       INI-UNP-LDA-CUR.
*% INCLUDE INI_UNP_LDA_CUR OF 'COLBMCUR.TXT'
           MOVE FRM-CPIM-UBY TO LDA-VAL-AUT-ATR IN CUR-FLD.
*% RREG = "LDA"
*% RREG_FRM = "LDA"
*% NOMB = "CUR-REA"
           MOVE FRM-CPIM-UBY TO LDA-FEC-CUR-REA-ATR IN CUR-FLD.
*% RREG = "LDA"
*% RREG_FRM = "LDA"
*% NOMB = "VTO-LIN"
           MOVE FRM-CPIM-UBY TO LDA-FEC-VTO-LIN-ATR IN CUR-FLD.
           MOVE FRM-CPIM-UBY TO LDA-COD-EST-ATR IN CUR-FLD.
*% INCLUDE FIN_UNP_LDA_CUR OF 'COLBMCUR.TXT'
       FIN-UNP-LDA-CUR.
           EXIT.
       
       VAL-KEY-CUR SECTION.
       INI-VAL-KEY-CUR.
           PERFORM VAL-IKY-CUR.
           PERFORM VAL-MKY-CUR.
           PERFORM VAL-AKY-CUR.
       FIN-VAL-KEY-CUR.
           EXIT.
       
       VAL-IKY-CUR SECTION.
       INI-VAL-IKY-CUR.
*% INCLUDE INI_VAL_IKY_CUR OF 'COLBMCUR.TXT'
           PERFORM VAL-LDA-NUM-SIS.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO LDA-NUM-SIS-LEN IN CUR-FLD
                GO TO FIN-VAL-IKY-CUR.
           PERFORM VAL-LDA-COD-CRR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO LDA-COD-CRR-LEN IN CUR-FLD
                GO TO FIN-VAL-IKY-CUR.
*% INCLUDE FIN_VAL_IKY_CUR OF 'COLBMCUR.TXT'
       FIN-VAL-IKY-CUR.
           EXIT.
       
       VAL-MKY-CUR SECTION.
       INI-VAL-MKY-CUR.
*% INCLUDE INI_VAL_MKY_CUR OF 'COLBMCUR.TXT'
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * VRF-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-NUM-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-NUM-ICLI-LEN IN CUR-FLD
                GO TO FIN-VAL-MKY-CUR.
           PERFORM VAL-FRM-VRF-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-VRF-ICLI-LEN IN CUR-FLD
                GO TO FIN-VAL-MKY-CUR.
      * VRF-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% INCLUDE FIN_VAL_MKY_CUR OF 'COLBMCUR.TXT'
       FIN-VAL-MKY-CUR.
           EXIT.
       
       VAL-AKY-CUR SECTION.
       INI-VAL-AKY-CUR.
*% INCLUDE INI_VAL_AKY_CUR OF 'COLBMCUR.TXT'
*% INCLUDE FIN_VAL_AKY_CUR OF 'COLBMCUR.TXT'
       FIN-VAL-AKY-CUR.
           EXIT.
       
       VAL-LDA-NUM-SIS SECTION.
       INI-VAL-LDA-NUM-SIS.
*% LBL = "LDA_NUM_SIS OF 'COLBMCUR.TXT'"
*% INCLUDE INI_LDA_NUM_SIS OF 'COLBMCUR.TXT'
           IF LDA-NUM-SIS IN CUR-FLD NOT > SPACES
              MOVE SPACES TO LDA-NUM-SIS IN CUR-FLD.
*% INCLUDE LDA_NUM_SIS OF 'COLBMCUR.TXT'
*% INCLUDE FIN_LDA_NUM_SIS OF 'COLBMCUR.TXT'
       FIN-VAL-LDA-NUM-SIS.
           EXIT.
       
       VAL-LDA-COD-CRR SECTION.
       INI-VAL-LDA-COD-CRR.
*% LBL = "LDA_COD_CRR OF 'COLBMCUR.TXT'"
*% INCLUDE INI_LDA_COD_CRR OF 'COLBMCUR.TXT'
*% INCLUDE LDA_COD_CRR OF 'COLBMCUR.TXT'
*% INCLUDE FIN_LDA_COD_CRR OF 'COLBMCUR.TXT'
       FIN-VAL-LDA-COD-CRR.
           EXIT.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
*% OBL = "S"
*% LBL = "FRM_VRF_ICLI OF 'COLBMCUR.TXT'"
*% IZR = "S"
*% ZRO = "S"
*% KO0 = "K"
*% ZRO = "S"
*% LMX = "7"
*% FRM_IFLD = ""
      * VRF-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-FRM-NUM-ICLI SECTION.
       INI-VAL-FRM-NUM-ICLI.
*% INCLUDE INI_FRM_NUM_ICLI  OF 'COLBMCUR.TXT'
*% INCLUDE FRM_NUM_ICLI  OF 'COLBMCUR.TXT'
*% INCLUDE FIN_FRM_NUM_ICLI  OF 'COLBMCUR.TXT'
       FIN-VAL-FRM-NUM-ICLI.
           EXIT.

       VAL-FRM-VRF-ICLI SECTION.
       INI-VAL-FRM-VRF-ICLI.
*% INCLUDE INI_FRM_VRF_ICLI  OF 'COLBMCUR.TXT'
           IF FRM-VRF-ICLI IN CUR-FLD NOT > SPACES
              MOVE SPACES TO FRM-VRF-ICLI IN CUR-FLD.
*% INCLUDE 'GNSSRC:SETVRF202.SRC'
*%! INCLUDE 'PROVRF'
*% LBL = ""
*% INCLUDE FRM_VRF_ICLI  OF 'COLBMCUR.TXT'
*% INCLUDE FIN_FRM_VRF_ICLI  OF 'COLBMCUR.TXT'
       FIN-VAL-FRM-VRF-ICLI.
           EXIT.

      * VRF-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
*% RREG = "LDA"
*% RREG_FRM = "LDA"
*% NOMB = "ING-CMP"
*% IZR = "S"
