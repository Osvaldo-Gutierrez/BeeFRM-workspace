*% IZR = "S"
       
       VAL-CON-KEY-CUR SECTION.
       INI-VAL-CON-KEY-CUR.
*% INCLUDE INI_VAL_CON_KEY_CUR OF 'COLBMCUR.TXT'
           IF     LDA-NUM-SIS IN CUR-FLD NOT > SPACES 
              AND LDA-COD-CRR IN CUR-FLD NOT > ZEROES 
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
              AND FRM-NUM-ICLI IN CUR-FLD NOT > ZEROES
              AND FRM-VRF-ICLI IN CUR-FLD NOT > SPACES
                PERFORM ERR-KEY
                PERFORM FST-KEY-CUR.
*% INCLUDE FIN_VAL_CON_KEY_CUR OF 'COLBMCUR.TXT'
       FIN-VAL-CON-KEY-CUR.
           EXIT.
       
       VAL-CON-IKY-CUR SECTION.
       INI-VAL-CON-IKY-CUR.
*% INCLUDE INI_VAL_CON_IKY_CUR OF 'COLBMCUR.TXT'
           IF     LDA-NUM-SIS IN CUR-FLD NOT > SPACES 
              AND LDA-COD-CRR IN CUR-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CUR.
*% INCLUDE FIN_VAL_CON_IKY_CUR OF 'COLBMCUR.TXT'
       FIN-VAL-CON-IKY-CUR.
           EXIT.
       
       VAL-NUM-KEY-CUR SECTION.
       INI-VAL-NUM-KEY-CUR.
           PERFORM VAL-NUM-IKY-CUR.
           PERFORM VAL-NUM-MKY-CUR.
           PERFORM VAL-NUM-AKY-CUR.
       FIN-VAL-NUM-KEY-CUR.
           EXIT.
       
       VAL-NUM-IKY-CUR SECTION.
       INI-VAL-NUM-IKY-CUR.
*% INCLUDE INI_VAL_NUM_IKY_CUR OF 'COLBMCUR.TXT'
       VAL-NUM-LDA-COD-CRR.
