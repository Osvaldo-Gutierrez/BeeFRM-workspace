*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-CUR.
*% INCLUDE FIN_VAL_NUM_FLD_CUR OF 'COLBMCUR.TXT'
       FIN-VAL-NUM-FLD-CUR.
           EXIT.
       
       EDT-KEY-CUR SECTION.
       INI-EDT-KEY-CUR.
           PERFORM EDT-IKY-CUR.
           PERFORM EDT-MKY-CUR.
           PERFORM EDT-AKY-CUR.
*% INCLUDE EDT_KEY_CUR OF 'COLBMCUR.TXT'
       FIN-EDT-KEY-CUR.
           EXIT.
       
       EDT-IKY-CUR SECTION.
       INI-EDT-IKY-CUR.
*% INCLUDE EDT_IKY_CUR OF 'COLBMCUR.TXT'
           IF LDA-COD-CRR-LEN IN CUR-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO LDA-COD-CRR-LEN IN CUR-FLD
                GO TO FIN-EDT-IKY-CUR.
           MOVE LDA-COD-CRR IN CUR-FLD TO LDA-COD-CRR-EDT IN CUR-EDT.
           MOVE LDA-COD-CRR-EDT IN CUR-EDT TO 
                LDA-COD-CRR-ALF IN CUR-FLD.
*% INCLUDE FIN_EDT_IKY_CUR OF 'COLBMCUR.TXT'
       FIN-EDT-IKY-CUR.
           EXIT.
       
       EDT-MKY-CUR SECTION.
       INI-EDT-MKY-CUR.
*% INCLUDE EDT_MKY_CUR OF 'COLBMCUR.TXT'
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF FRM-NUM-ICLI-LEN IN CUR-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-ICLI-LEN IN CUR-FLD
                GO TO FIN-EDT-MKY-CUR.
           MOVE FRM-NUM-ICLI IN CUR-FLD TO
                FRM-NUM-ICLI-EDT IN CUR-EDT.
           MOVE FRM-NUM-ICLI-EDT IN CUR-EDT TO
                FRM-NUM-ICLI-ALF IN CUR-FLD.
      * IDC-FIN --> BMCHIBCI011.SRCAIU EDICION NUMERICA
*% INCLUDE FIN_EDT_MKY_CUR OF 'COLBMCUR.TXT'
       FIN-EDT-MKY-CUR.
           EXIT.
       
       EDT-AKY-CUR SECTION.
       INI-EDT-AKY-CUR.
*% INCLUDE EDT_AKY_CUR OF 'COLBMCUR.TXT'
*% INCLUDE FIN_EDT_AKY_CUR OF 'COLBMCUR.TXT'
       FIN-EDT-AKY-CUR.
           EXIT.
       
       EDT-FLD-CUR SECTION.
       INI-EDT-FLD-CUR.
*% INCLUDE EDT_FLD_CUR OF 'COLBMCUR.TXT'
           IF FRM-COD-CRR-LEN IN CUR-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-COD-CRR-LEN IN CUR-FLD
                GO TO FIN-EDT-FLD-CUR.
           MOVE FRM-COD-CRR IN CUR-FLD TO FRM-COD-CRR-EDT IN CUR-EDT.
           MOVE FRM-COD-CRR-EDT IN CUR-EDT TO 
                FRM-COD-CRR-ALF IN CUR-FLD.
           IF LDA-VAL-AUT-LEN IN CUR-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO LDA-VAL-AUT-LEN IN CUR-FLD
                GO TO FIN-EDT-FLD-CUR.
           MOVE LDA-VAL-AUT IN CUR-FLD TO LDA-VAL-AUT-EDT IN CUR-EDT.
           MOVE LDA-VAL-AUT-EDT IN CUR-EDT TO 
                LDA-VAL-AUT-ALF IN CUR-FLD.
*% RREG = "LDA"
*% RREG_FRM = "LDA"
*% NOMB = "ING-CMP"
*% RREG = "LDA"
*% RREG_FRM = "LDA"
*% NOMB = "CUR-REA"
*% RREG = "LDA"
*% RREG_FRM = "LDA"
*% NOMB = "VTO-LIN"
*% INCLUDE FIN_EDT_FLD_CUR OF 'COLBMCUR.TXT'
       FIN-EDT-FLD-CUR.
           EXIT.
*% INCLUDE EOF_BQ OF 'COLBMCUR.TXT'
*% INCLUDE EOF OF 'COLBMCUR.TXT'
*% INCLUDE EOF_IBM OF 'COLBMCUR.TXT'
