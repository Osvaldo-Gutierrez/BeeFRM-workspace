*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "PGP"
*% PGM_BQ = TRUE
       PUT-ALL-PGP SECTION.
       INI-PUT-ALL-PGP.
*% INCLUDE INI_PUT_ALL_PGP OF 'COLBMPGP.TXT'
           PERFORM PUT-PGP-PGP.
           PERFORM PUT-DBC-PGP.
*% INCLUDE FIN_PUT_ALL_PGP OF 'COLBMPGP.TXT'
       FIN-PUT-ALL-PGP.
           EXIT.
       
       PUT-PGP-PGP SECTION.
       INI-PUT-PGP-PGP.
*% INCLUDE INI_PUT_PGP_PGP OF 'COLBMPGP.TXT'
           IF PGM-STAT-PGP-NUL
               GO TO FIN-PUT-PGP-PGP.
           IF NOT PGM-STAT-PGP-OKS
               PERFORM CLR-PGP-PGP
               GO TO FIN-PUT-PGP-PGP.
           MOVE PGP-COD-PLAN IN PGP TO PGP-COD-PLAN IN PGP-FLD.
           MOVE PGP-COD-EVEN IN PGP TO PGP-COD-EVEN IN PGP-FLD.
           MOVE PGP-COD-OFIC IN PGP TO PGP-COD-OFIC IN PGP-FLD.
*% RREG = "PGP"
*% NOMB = "FPLA"
      * FEC-INI --> BMCHIBCI001.SRCAIU 
           MOVE PGP-NUM-DPLA IN PGP TO PGP-NUM-DPLA IN PGP-FLD.
           MOVE PGP-NUM-MPLA IN PGP TO PGP-NUM-MPLA IN PGP-FLD.
           MOVE PGP-NUM-SPLA IN PGP TO PGP-NUM-SPLA IN PGP-FLD.
           MOVE PGP-NUM-APLA IN PGP TO PGP-NUM-APLA IN PGP-FLD.
      * FEC-FIN --> BMCHIBCI001.SRCAIU 
           MOVE PGP-COD-SEGM IN PGP TO PGP-COD-SEGM IN PGP-FLD.
           MOVE PGP-IND-SEGM IN PGP TO PGP-IND-SEGM IN PGP-FLD.
           MOVE PGP-IND-VIGE IN PGP TO PGP-IND-VIGE IN PGP-FLD.
           MOVE PGP-IND-DCTO IN PGP TO PGP-IND-DCTO IN PGP-FLD.
*% RREG = "PGP"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI001.SRCAIU 
           MOVE PGP-NUM-DTRN IN PGP TO PGP-NUM-DTRN IN PGP-FLD.
           MOVE PGP-NUM-MTRN IN PGP TO PGP-NUM-MTRN IN PGP-FLD.
           MOVE PGP-NUM-STRN IN PGP TO PGP-NUM-STRN IN PGP-FLD.
           MOVE PGP-NUM-ATRN IN PGP TO PGP-NUM-ATRN IN PGP-FLD.
      * FEC-FIN --> BMCHIBCI001.SRCAIU 
           MOVE PGP-HRA-HRTR IN PGP TO PGP-HRA-HRTR IN PGP-FLD.
      *
      *    CAMPO INDEXADO   PGP-COD-GSTO
           MOVE PGP-COD-GSTO IN PGP(1) TO PGP-COD-GSTO IN PGP-FLD(1).
           MOVE PGP-COD-GSTO IN PGP(2) TO PGP-COD-GSTO IN PGP-FLD(2).
           MOVE PGP-COD-GSTO IN PGP(3) TO PGP-COD-GSTO IN PGP-FLD(3).
           MOVE PGP-COD-GSTO IN PGP(4) TO PGP-COD-GSTO IN PGP-FLD(4).
           MOVE PGP-COD-GSTO IN PGP(5) TO PGP-COD-GSTO IN PGP-FLD(5).
           MOVE PGP-COD-GSTO IN PGP(6) TO PGP-COD-GSTO IN PGP-FLD(6).
      *
      *    CAMPO INDEXADO   PGP-IND-SCTB
           MOVE PGP-IND-SCTB IN PGP(1) TO PGP-IND-SCTB IN PGP-FLD(1).
           MOVE PGP-IND-SCTB IN PGP(2) TO PGP-IND-SCTB IN PGP-FLD(2).
           MOVE PGP-IND-SCTB IN PGP(3) TO PGP-IND-SCTB IN PGP-FLD(3).
           MOVE PGP-IND-SCTB IN PGP(4) TO PGP-IND-SCTB IN PGP-FLD(4).
           MOVE PGP-IND-SCTB IN PGP(5) TO PGP-IND-SCTB IN PGP-FLD(5).
           MOVE PGP-IND-SCTB IN PGP(6) TO PGP-IND-SCTB IN PGP-FLD(6).
      *
      *    CAMPO INDEXADO   PGP-IND-MTOP
           MOVE PGP-IND-MTOP IN PGP(1) TO PGP-IND-MTOP IN PGP-FLD(1).
           MOVE PGP-IND-MTOP IN PGP(2) TO PGP-IND-MTOP IN PGP-FLD(2).
           MOVE PGP-IND-MTOP IN PGP(3) TO PGP-IND-MTOP IN PGP-FLD(3).
           MOVE PGP-IND-MTOP IN PGP(4) TO PGP-IND-MTOP IN PGP-FLD(4).
           MOVE PGP-IND-MTOP IN PGP(5) TO PGP-IND-MTOP IN PGP-FLD(5).
           MOVE PGP-IND-MTOP IN PGP(6) TO PGP-IND-MTOP IN PGP-FLD(6).
      *
      *    CAMPO INDEXADO   PGP-COD-CONC
           MOVE PGP-COD-CONC IN PGP(1) TO PGP-COD-CONC IN PGP-FLD(1).
           MOVE PGP-COD-CONC IN PGP(2) TO PGP-COD-CONC IN PGP-FLD(2).
           MOVE PGP-COD-CONC IN PGP(3) TO PGP-COD-CONC IN PGP-FLD(3).
           MOVE PGP-COD-CONC IN PGP(4) TO PGP-COD-CONC IN PGP-FLD(4).
           MOVE PGP-COD-CONC IN PGP(5) TO PGP-COD-CONC IN PGP-FLD(5).
           MOVE PGP-COD-CONC IN PGP(6) TO PGP-COD-CONC IN PGP-FLD(6).
      *
      *    CAMPO INDEXADO   PGP-VAL-GSTO
           MOVE PGP-VAL-GSTO IN PGP(1) TO PGP-VAL-GSTO IN PGP-FLD(1).
           MOVE PGP-VAL-GSTO IN PGP(2) TO PGP-VAL-GSTO IN PGP-FLD(2).
           MOVE PGP-VAL-GSTO IN PGP(3) TO PGP-VAL-GSTO IN PGP-FLD(3).
           MOVE PGP-VAL-GSTO IN PGP(4) TO PGP-VAL-GSTO IN PGP-FLD(4).
           MOVE PGP-VAL-GSTO IN PGP(5) TO PGP-VAL-GSTO IN PGP-FLD(5).
           MOVE PGP-VAL-GSTO IN PGP(6) TO PGP-VAL-GSTO IN PGP-FLD(6).
      *
      *    CAMPO INDEXADO   PGP-VAL-GMIN
           MOVE PGP-VAL-GMIN IN PGP(1) TO PGP-VAL-GMIN IN PGP-FLD(1).
           MOVE PGP-VAL-GMIN IN PGP(2) TO PGP-VAL-GMIN IN PGP-FLD(2).
           MOVE PGP-VAL-GMIN IN PGP(3) TO PGP-VAL-GMIN IN PGP-FLD(3).
           MOVE PGP-VAL-GMIN IN PGP(4) TO PGP-VAL-GMIN IN PGP-FLD(4).
           MOVE PGP-VAL-GMIN IN PGP(5) TO PGP-VAL-GMIN IN PGP-FLD(5).
           MOVE PGP-VAL-GMIN IN PGP(6) TO PGP-VAL-GMIN IN PGP-FLD(6).
      *
      *    CAMPO INDEXADO   PGP-VAL-GMAX
           MOVE PGP-VAL-GMAX IN PGP(1) TO PGP-VAL-GMAX IN PGP-FLD(1).
           MOVE PGP-VAL-GMAX IN PGP(2) TO PGP-VAL-GMAX IN PGP-FLD(2).
           MOVE PGP-VAL-GMAX IN PGP(3) TO PGP-VAL-GMAX IN PGP-FLD(3).
           MOVE PGP-VAL-GMAX IN PGP(4) TO PGP-VAL-GMAX IN PGP-FLD(4).
           MOVE PGP-VAL-GMAX IN PGP(5) TO PGP-VAL-GMAX IN PGP-FLD(5).
           MOVE PGP-VAL-GMAX IN PGP(6) TO PGP-VAL-GMAX IN PGP-FLD(6).
      *
      *    CAMPO INDEXADO   PGP-COD-VCOC
           MOVE PGP-COD-VCOC IN PGP(1) TO PGP-COD-VCOC IN PGP-FLD(1).
           MOVE PGP-COD-VCOC IN PGP(2) TO PGP-COD-VCOC IN PGP-FLD(2).
           MOVE PGP-COD-VCOC IN PGP(3) TO PGP-COD-VCOC IN PGP-FLD(3).
           MOVE PGP-COD-VCOC IN PGP(4) TO PGP-COD-VCOC IN PGP-FLD(4).
           MOVE PGP-COD-VCOC IN PGP(5) TO PGP-COD-VCOC IN PGP-FLD(5).
           MOVE PGP-COD-VCOC IN PGP(6) TO PGP-COD-VCOC IN PGP-FLD(6).
           MOVE PGP-CAI-ICLI IN PGP TO PGP-CAI-ICLI IN PGP-FLD.
           MOVE PGP-IIC-ICLI IN PGP TO PGP-IIC-ICLI IN PGP-FLD.
*% BUF = "PGP-FLD"
*% SIS = "COL"
*% TAB = "MVT "
*% FLD = "PGP-COD-EVEN"
*% GLS = "FRM-GLS-EVEN"
*% TIENE_GLOSA = TRUE
*% LGL = 12
*% INCLUDE 'BUSTAB'
*% BUF = "PGP-FLD"
*% SIS = "TAB"
