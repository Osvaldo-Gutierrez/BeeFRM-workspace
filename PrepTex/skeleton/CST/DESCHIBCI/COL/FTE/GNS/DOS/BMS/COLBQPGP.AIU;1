       EDT-FLD-PGP SECTION.
       INI-EDT-FLD-PGP.
*% INCLUDE EDT_FLD_PGP OF 'COLBMPGP.TXT'
*% RREG = "PGP"
*% NOMB = "FTRN"
           IF PGP-HRA-HRTR-LEN IN PGP-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PGP-HRA-HRTR-LEN IN PGP-FLD
                GO TO FIN-EDT-FLD-PGP.
           MOVE PGP-HRA-HRTR IN PGP-FLD TO PGP-HRA-HRTR-EDT IN PGP-EDT.
           MOVE PGP-HRA-HRTR-EDT IN PGP-EDT TO 
                PGP-HRA-HRTR-ALF IN PGP-FLD.
           IF PGP-VAL-GSTO-LEN IN PGP-FLD(1) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PGP-VAL-GSTO-LEN IN PGP-FLD(1)
                GO TO FIN-EDT-FLD-PGP.
           MOVE PGP-VAL-GSTO IN PGP-FLD(1) TO 
                PGP-VAL-GSTO-EDT IN PGP-EDT.
           MOVE PGP-VAL-GSTO-EDT IN PGP-EDT TO 
                PGP-VAL-GSTO-ALF IN PGP-FLD(1).
           IF PGP-VAL-GSTO-LEN IN PGP-FLD(2) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PGP-VAL-GSTO-LEN IN PGP-FLD(2)
                GO TO FIN-EDT-FLD-PGP.
           MOVE PGP-VAL-GSTO IN PGP-FLD(2) TO 
                PGP-VAL-GSTO-EDT IN PGP-EDT.
           MOVE PGP-VAL-GSTO-EDT IN PGP-EDT TO 
                PGP-VAL-GSTO-ALF IN PGP-FLD(2).
           IF PGP-VAL-GSTO-LEN IN PGP-FLD(3) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PGP-VAL-GSTO-LEN IN PGP-FLD(3)
                GO TO FIN-EDT-FLD-PGP.
           MOVE PGP-VAL-GSTO IN PGP-FLD(3) TO 
                PGP-VAL-GSTO-EDT IN PGP-EDT.
           MOVE PGP-VAL-GSTO-EDT IN PGP-EDT TO 
                PGP-VAL-GSTO-ALF IN PGP-FLD(3).
           IF PGP-VAL-GSTO-LEN IN PGP-FLD(4) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PGP-VAL-GSTO-LEN IN PGP-FLD(4)
                GO TO FIN-EDT-FLD-PGP.
           MOVE PGP-VAL-GSTO IN PGP-FLD(4) TO 
                PGP-VAL-GSTO-EDT IN PGP-EDT.
           MOVE PGP-VAL-GSTO-EDT IN PGP-EDT TO 
                PGP-VAL-GSTO-ALF IN PGP-FLD(4).
           IF PGP-VAL-GSTO-LEN IN PGP-FLD(5) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PGP-VAL-GSTO-LEN IN PGP-FLD(5)
                GO TO FIN-EDT-FLD-PGP.
           MOVE PGP-VAL-GSTO IN PGP-FLD(5) TO 
                PGP-VAL-GSTO-EDT IN PGP-EDT.
           MOVE PGP-VAL-GSTO-EDT IN PGP-EDT TO 
                PGP-VAL-GSTO-ALF IN PGP-FLD(5).
           IF PGP-VAL-GSTO-LEN IN PGP-FLD(6) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PGP-VAL-GSTO-LEN IN PGP-FLD(6)
                GO TO FIN-EDT-FLD-PGP.
           MOVE PGP-VAL-GSTO IN PGP-FLD(6) TO 
                PGP-VAL-GSTO-EDT IN PGP-EDT.
           MOVE PGP-VAL-GSTO-EDT IN PGP-EDT TO 
                PGP-VAL-GSTO-ALF IN PGP-FLD(6).
           IF PGP-VAL-GMIN-LEN IN PGP-FLD(1) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PGP-VAL-GMIN-LEN IN PGP-FLD(1)
                GO TO FIN-EDT-FLD-PGP.
           MOVE PGP-VAL-GMIN IN PGP-FLD(1) TO 
                PGP-VAL-GMIN-EDT IN PGP-EDT.
           MOVE PGP-VAL-GMIN-EDT IN PGP-EDT TO 
                PGP-VAL-GMIN-ALF IN PGP-FLD(1).
           IF PGP-VAL-GMIN-LEN IN PGP-FLD(2) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PGP-VAL-GMIN-LEN IN PGP-FLD(2)
                GO TO FIN-EDT-FLD-PGP.
           MOVE PGP-VAL-GMIN IN PGP-FLD(2) TO 
                PGP-VAL-GMIN-EDT IN PGP-EDT.
           MOVE PGP-VAL-GMIN-EDT IN PGP-EDT TO 
                PGP-VAL-GMIN-ALF IN PGP-FLD(2).
           IF PGP-VAL-GMIN-LEN IN PGP-FLD(3) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PGP-VAL-GMIN-LEN IN PGP-FLD(3)
                GO TO FIN-EDT-FLD-PGP.
           MOVE PGP-VAL-GMIN IN PGP-FLD(3) TO 
                PGP-VAL-GMIN-EDT IN PGP-EDT.
           MOVE PGP-VAL-GMIN-EDT IN PGP-EDT TO 
                PGP-VAL-GMIN-ALF IN PGP-FLD(3).
           IF PGP-VAL-GMIN-LEN IN PGP-FLD(4) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PGP-VAL-GMIN-LEN IN PGP-FLD(4)
                GO TO FIN-EDT-FLD-PGP.
           MOVE PGP-VAL-GMIN IN PGP-FLD(4) TO 
                PGP-VAL-GMIN-EDT IN PGP-EDT.
           MOVE PGP-VAL-GMIN-EDT IN PGP-EDT TO 
                PGP-VAL-GMIN-ALF IN PGP-FLD(4).
           IF PGP-VAL-GMIN-LEN IN PGP-FLD(5) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PGP-VAL-GMIN-LEN IN PGP-FLD(5)
                GO TO FIN-EDT-FLD-PGP.
           MOVE PGP-VAL-GMIN IN PGP-FLD(5) TO 
                PGP-VAL-GMIN-EDT IN PGP-EDT.
           MOVE PGP-VAL-GMIN-EDT IN PGP-EDT TO 
                PGP-VAL-GMIN-ALF IN PGP-FLD(5).
           IF PGP-VAL-GMIN-LEN IN PGP-FLD(6) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PGP-VAL-GMIN-LEN IN PGP-FLD(6)
                GO TO FIN-EDT-FLD-PGP.
           MOVE PGP-VAL-GMIN IN PGP-FLD(6) TO 
                PGP-VAL-GMIN-EDT IN PGP-EDT.
           MOVE PGP-VAL-GMIN-EDT IN PGP-EDT TO 
                PGP-VAL-GMIN-ALF IN PGP-FLD(6).
           IF PGP-VAL-GMAX-LEN IN PGP-FLD(1) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PGP-VAL-GMAX-LEN IN PGP-FLD(1)
                GO TO FIN-EDT-FLD-PGP.
           MOVE PGP-VAL-GMAX IN PGP-FLD(1) TO 
                PGP-VAL-GMAX-EDT IN PGP-EDT.
           MOVE PGP-VAL-GMAX-EDT IN PGP-EDT TO 
                PGP-VAL-GMAX-ALF IN PGP-FLD(1).
           IF PGP-VAL-GMAX-LEN IN PGP-FLD(2) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PGP-VAL-GMAX-LEN IN PGP-FLD(2)
                GO TO FIN-EDT-FLD-PGP.
           MOVE PGP-VAL-GMAX IN PGP-FLD(2) TO 
                PGP-VAL-GMAX-EDT IN PGP-EDT.
           MOVE PGP-VAL-GMAX-EDT IN PGP-EDT TO 
                PGP-VAL-GMAX-ALF IN PGP-FLD(2).
           IF PGP-VAL-GMAX-LEN IN PGP-FLD(3) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PGP-VAL-GMAX-LEN IN PGP-FLD(3)
                GO TO FIN-EDT-FLD-PGP.
           MOVE PGP-VAL-GMAX IN PGP-FLD(3) TO 
                PGP-VAL-GMAX-EDT IN PGP-EDT.
           MOVE PGP-VAL-GMAX-EDT IN PGP-EDT TO 
                PGP-VAL-GMAX-ALF IN PGP-FLD(3).
           IF PGP-VAL-GMAX-LEN IN PGP-FLD(4) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PGP-VAL-GMAX-LEN IN PGP-FLD(4)
                GO TO FIN-EDT-FLD-PGP.
           MOVE PGP-VAL-GMAX IN PGP-FLD(4) TO 
                PGP-VAL-GMAX-EDT IN PGP-EDT.
           MOVE PGP-VAL-GMAX-EDT IN PGP-EDT TO 
                PGP-VAL-GMAX-ALF IN PGP-FLD(4).
           IF PGP-VAL-GMAX-LEN IN PGP-FLD(5) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PGP-VAL-GMAX-LEN IN PGP-FLD(5)
                GO TO FIN-EDT-FLD-PGP.
           MOVE PGP-VAL-GMAX IN PGP-FLD(5) TO 
                PGP-VAL-GMAX-EDT IN PGP-EDT.
           MOVE PGP-VAL-GMAX-EDT IN PGP-EDT TO 
                PGP-VAL-GMAX-ALF IN PGP-FLD(5).
           IF PGP-VAL-GMAX-LEN IN PGP-FLD(6) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PGP-VAL-GMAX-LEN IN PGP-FLD(6)
                GO TO FIN-EDT-FLD-PGP.
           MOVE PGP-VAL-GMAX IN PGP-FLD(6) TO 
                PGP-VAL-GMAX-EDT IN PGP-EDT.
           MOVE PGP-VAL-GMAX-EDT IN PGP-EDT TO 
                PGP-VAL-GMAX-ALF IN PGP-FLD(6).
