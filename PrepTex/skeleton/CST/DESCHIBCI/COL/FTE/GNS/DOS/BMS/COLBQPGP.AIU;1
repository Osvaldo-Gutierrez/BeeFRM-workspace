*% FLD = "PGP-COD-OFIC"
*% GLS = "FRM-GLS-OFIC"
*% TIENE_GLOSA = TRUE
*% LGL = 12
*% INCLUDE 'BUSOFI          '
*% INCLUDE FIN_PUT_PGP_PGP OF 'COLBMPGP.TXT'
       FIN-PUT-PGP-PGP.
           EXIT.
       
       PUT-DBC-PGP SECTION.
       INI-PUT-DBC-PGP.
*% INCLUDE INI_PUT_DBC_PGP OF 'COLBMPGP.TXT'
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-PGP.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-PGP
               GO TO FIN-PUT-DBC-PGP.
*% RREG = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI001.SRCAIU 
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN PGP-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN PGP-FLD.
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN PGP-FLD.
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN PGP-FLD.
      * IDC-FIN --> BMCHIBCI001.SRCAIU 
           MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN PGP-FLD.
*% INCLUDE FIN_PUT_DBC_PGP OF 'COLBMPGP.TXT'
       FIN-PUT-DBC-PGP.
           EXIT.
       
       KEY-ALL-PGP SECTION.
       INI-KEY-ALL-PGP.
*% INCLUDE INI_KEY_ALL_PGP OF 'COLBMPGP.TXT'
           PERFORM KEY-PGP-PGP.
           PERFORM KEY-DBC-PGP.
*% INCLUDE FIN_KEY_ALL_PGP OF 'COLBMPGP.TXT'
       FIN-KEY-ALL-PGP.
           EXIT.
       
       KEY-PGP-PGP SECTION.
       INI-KEY-PGP-PGP.
*% INCLUDE INI_KEY_PGP_PGP OF 'COLBMPGP.TXT'
           MOVE PGP-COD-PLAN IN PGP-FLD TO PGP-COD-PLAN IN PGP.
           MOVE PGP-COD-EVEN IN PGP-FLD TO PGP-COD-EVEN IN PGP.
           MOVE PGP-COD-OFIC IN PGP-FLD TO PGP-COD-OFIC IN PGP.
*% RREG = "PGP"
*% NOMB = "FPLA"
      * FEC-INI --> BMCHIBCI002.SRCAIU RET-ALL Y KEY-ALL
           MOVE PGP-NUM-DCLI IN PGP-FLD TO PGP-NUM-DCLI IN PGP.
           MOVE PGP-NUM-MCLI IN PGP-FLD TO PGP-NUM-MCLI IN PGP.
           MOVE PGP-NUM-SCLI IN PGP-FLD TO PGP-NUM-SCLI IN PGP.
           MOVE PGP-NUM-ACLI IN PGP-FLD TO PGP-NUM-ACLI IN PGP.
      * FEC-FIN --> BMCHIBCI002.SRCAIU RET-ALL Y KEY-ALL
           MOVE PGP-COD-SEGM IN PGP-FLD TO PGP-COD-SEGM IN PGP.
           MOVE PGP-IND-SEGM IN PGP-FLD TO PGP-IND-SEGM IN PGP.
           MOVE PGP-CAI-ICLI IN PGP-FLD TO PGP-CAI-ICLI IN PGP.
           MOVE PGP-IIC-ICLI IN PGP-FLD TO PGP-IIC-ICLI IN PGP.
*% INCLUDE FIN_KEY_PGP_PGP OF 'COLBMPGP.TXT'
       FIN-KEY-PGP-PGP.
           EXIT.
       
       KEY-DBC-PGP SECTION.
       INI-KEY-DBC-PGP.
*% INCLUDE INI_KEY_DBC_PGP OF 'COLBMPGP.TXT'
*% RREG = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI002.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN PGP-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN PGP-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN PGP-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN PGP-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-GLS-NOMC IN PGP-FLD TO DBC-GLS-NOMC IN DBC.
*% INCLUDE FIN_KEY_DBC_PGP OF 'COLBMPGP.TXT'
       FIN-KEY-DBC-PGP.
           EXIT.
       
       SCR-CLRA-PGP SECTION.
       INI-SCR-CLRA-PGP.
*% INCLUDE INI_SCR_CLRA_PGP OF 'COLBMPGP.TXT'
           PERFORM CLR-KEY-PGP.
           PERFORM CLR-FLD-PGP.
*% INCLUDE FIN_SCR_CLRA_PGP OF 'COLBMPGP.TXT'
       FIN-SCR-CLRA-PGP.
           EXIT.
       
       CLR-KEY-PGP SECTION.
       INI-CLR-KEY-PGP.
*% INCLUDE INI_CLR_KEY_PGP OF 'COLBMPGP.TXT'
           MOVE SPACES TO PGP-COD-PLAN IN PGP-FLD.
           MOVE SPACES TO PGP-COD-EVEN IN PGP-FLD.
           MOVE SPACES TO PGP-COD-OFIC IN PGP-FLD.
*% RREG = "PGP"
*% NOMB = "FPLA"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO PGP-FEC-FPLA IN PGP-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO PGP-COD-SEGM IN PGP-FLD.
           MOVE SPACES TO PGP-IND-SEGM IN PGP-FLD.
*% RREG = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN PGP-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN PGP-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN PGP-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN PGP-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO DBC-GLS-NOMC IN PGP-FLD.
           MOVE SPACES TO PGP-CAI-ICLI IN PGP-FLD.
           MOVE SPACES TO PGP-IIC-ICLI IN PGP-FLD.
*% INCLUDE FIN_CLR_KEY_PGP OF 'COLBMPGP.TXT'
       FIN-CLR-KEY-PGP.
           EXIT.
       
       CLR-FLD-PGP SECTION.
       INI-CLR-FLD-PGP.
*% INCLUDE INI_CLR_FLD_PGP OF 'COLBMPGP.TXT'
           MOVE SPACES TO FRM-GLS-EVEN IN PGP-FLD.
           MOVE SPACES TO FRM-GLS-OFIC IN PGP-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN PGP-FLD(1).
           MOVE SPACES TO FRM-GLS-VCOC IN PGP-FLD(2).
           MOVE SPACES TO FRM-GLS-VCOC IN PGP-FLD(3).
           MOVE SPACES TO FRM-GLS-VCOC IN PGP-FLD(4).
           MOVE SPACES TO FRM-GLS-VCOC IN PGP-FLD(5).
           MOVE SPACES TO FRM-GLS-VCOC IN PGP-FLD(6).
           PERFORM CLR-PGP-PGP.
           PERFORM CLR-DBC-PGP.
*% INCLUDE FIN_CLR_FLD_PGP OF 'COLBMPGP.TXT'
       FIN-CLR-FLD-PGP.
           EXIT.
       
       CLR-PGP-PGP SECTION.
       INI-CLR-PGP-PGP.
*% INCLUDE INI_CLR_PGP_PGP OF 'COLBMPGP.TXT'
           MOVE SPACES TO PGP-IND-VIGE IN PGP-FLD.
           MOVE SPACES TO PGP-IND-DCTO IN PGP-FLD.
*% RREG = "PGP"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO PGP-FEC-FTRN IN PGP-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO PGP-HRA-HRTR IN PGP-FLD.
           MOVE SPACES TO PGP-COD-GSTO IN PGP-FLD(1).
           MOVE SPACES TO PGP-COD-GSTO IN PGP-FLD(2).
           MOVE SPACES TO PGP-COD-GSTO IN PGP-FLD(3).
           MOVE SPACES TO PGP-COD-GSTO IN PGP-FLD(4).
           MOVE SPACES TO PGP-COD-GSTO IN PGP-FLD(5).
           MOVE SPACES TO PGP-COD-GSTO IN PGP-FLD(6).
           MOVE SPACES TO PGP-IND-SCTB IN PGP-FLD(1).
           MOVE SPACES TO PGP-IND-SCTB IN PGP-FLD(2).
           MOVE SPACES TO PGP-IND-SCTB IN PGP-FLD(3).
           MOVE SPACES TO PGP-IND-SCTB IN PGP-FLD(4).
           MOVE SPACES TO PGP-IND-SCTB IN PGP-FLD(5).
           MOVE SPACES TO PGP-IND-SCTB IN PGP-FLD(6).
           MOVE SPACES TO PGP-IND-MTOP IN PGP-FLD(1).
           MOVE SPACES TO PGP-IND-MTOP IN PGP-FLD(2).
           MOVE SPACES TO PGP-IND-MTOP IN PGP-FLD(3).
           MOVE SPACES TO PGP-IND-MTOP IN PGP-FLD(4).
           MOVE SPACES TO PGP-IND-MTOP IN PGP-FLD(5).
           MOVE SPACES TO PGP-IND-MTOP IN PGP-FLD(6).
           MOVE SPACES TO PGP-COD-CONC IN PGP-FLD(1).
           MOVE SPACES TO PGP-COD-CONC IN PGP-FLD(2).
           MOVE SPACES TO PGP-COD-CONC IN PGP-FLD(3).
           MOVE SPACES TO PGP-COD-CONC IN PGP-FLD(4).
           MOVE SPACES TO PGP-COD-CONC IN PGP-FLD(5).
           MOVE SPACES TO PGP-COD-CONC IN PGP-FLD(6).
           MOVE ZEROES TO PGP-VAL-GSTO IN PGP-FLD(1).
           MOVE ZEROES TO PGP-VAL-GSTO IN PGP-FLD(2).
           MOVE ZEROES TO PGP-VAL-GSTO IN PGP-FLD(3).
           MOVE ZEROES TO PGP-VAL-GSTO IN PGP-FLD(4).
           MOVE ZEROES TO PGP-VAL-GSTO IN PGP-FLD(5).
           MOVE ZEROES TO PGP-VAL-GSTO IN PGP-FLD(6).
           MOVE ZEROES TO PGP-VAL-GMIN IN PGP-FLD(1).
           MOVE ZEROES TO PGP-VAL-GMIN IN PGP-FLD(2).
           MOVE ZEROES TO PGP-VAL-GMIN IN PGP-FLD(3).
           MOVE ZEROES TO PGP-VAL-GMIN IN PGP-FLD(4).
           MOVE ZEROES TO PGP-VAL-GMIN IN PGP-FLD(5).
           MOVE ZEROES TO PGP-VAL-GMIN IN PGP-FLD(6).
           MOVE ZEROES TO PGP-VAL-GMAX IN PGP-FLD(1).
           MOVE ZEROES TO PGP-VAL-GMAX IN PGP-FLD(2).
           MOVE ZEROES TO PGP-VAL-GMAX IN PGP-FLD(3).
           MOVE ZEROES TO PGP-VAL-GMAX IN PGP-FLD(4).
           MOVE ZEROES TO PGP-VAL-GMAX IN PGP-FLD(5).
           MOVE ZEROES TO PGP-VAL-GMAX IN PGP-FLD(6).
           MOVE SPACES TO PGP-COD-VCOC IN PGP-FLD(1).
           MOVE SPACES TO PGP-COD-VCOC IN PGP-FLD(2).
           MOVE SPACES TO PGP-COD-VCOC IN PGP-FLD(3).
           MOVE SPACES TO PGP-COD-VCOC IN PGP-FLD(4).
           MOVE SPACES TO PGP-COD-VCOC IN PGP-FLD(5).
           MOVE SPACES TO PGP-COD-VCOC IN PGP-FLD(6).
*% INCLUDE FIN_CLR_PGP_PGP OF 'COLBMPGP.TXT'
       FIN-CLR-PGP-PGP.
           EXIT.
       
       CLR-DBC-PGP SECTION.
       INI-CLR-DBC-PGP.
*% INCLUDE INI_CLR_DBC_PGP OF 'COLBMPGP.TXT'
*% INCLUDE FIN_CLR_DBC_PGP OF 'COLBMPGP.TXT'
       FIN-CLR-DBC-PGP.
           EXIT.
       
       FST-KEY-PGP SECTION.
       INI-FST-KEY-PGP.
*% INCLUDE INI_FST_KEY_PGP OF 'COLBMPGP.TXT'
           MOVE -1 TO PGP-COD-PLAN-LEN IN PGP-FLD.
*% INCLUDE FIN_FST_KEY_PGP OF 'COLBMPGP.TXT'
       FIN-FST-KEY-PGP.
           EXIT.
       
       FST-FLD-PGP SECTION.
       INI-FST-FLD-PGP.
*% INCLUDE INI_FST_FLD_PGP OF 'COLBMPGP.TXT'
           MOVE -1 TO PGP-IND-VIGE-LEN IN PGP-FLD.
*% INCLUDE FIN_FST_FLD_PGP OF 'COLBMPGP.TXT'
       FIN-FST-FLD-PGP.
           EXIT.
       
       PRO-KEY-PGP SECTION.
       INI-PRO-KEY-PGP.
*% INCLUDE INI_PRO_KEY_PGP OF 'COLBMPGP.TXT'
           PERFORM PRO-IKY-PGP.
           PERFORM PRO-AKY-PGP.
           PERFORM PRO-MKY-PGP.
*% INCLUDE FIN_PRO_KEY_PGP OF 'COLBMPGP.TXT'
       FIN-PRO-KEY-PGP.
           EXIT.
       
       PRO-IKY-PGP SECTION.
       INI-PRO-IKY-PGP.
*% INCLUDE INI_PRO_IKY_PGP OF 'COLBMPGP.TXT'
           MOVE FRM-CPIM-AEY TO PGP-COD-PLAN-ATR IN PGP-FLD.
           MOVE FRM-CPIM-AEY TO PGP-COD-EVEN-ATR IN PGP-FLD.
           MOVE FRM-CPIM-AEY TO PGP-COD-OFIC-ATR IN PGP-FLD.
*% RREG = "PGP"
*% NOMB = "FPLA"
           MOVE FRM-CPIM-AEY TO PGP-FEC-FPLA-ATR IN PGP-FLD.
           MOVE FRM-CPIM-AEY TO PGP-COD-SEGM-ATR IN PGP-FLD.
           MOVE FRM-CPIM-AEY TO PGP-IND-SEGM-ATR IN PGP-FLD.
*% RREG = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO DBC-NUM-ICLI-ATR IN PGP-FLD.
           MOVE FRM-CPIM-AEY TO DBC-VRF-ICLI-ATR IN PGP-FLD.
           MOVE FRM-CPIM-AEY TO DBC-IND-ICLI-ATR IN PGP-FLD.
           MOVE FRM-CPIM-AEY TO DBC-GLS-ICLI-ATR IN PGP-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
*% INCLUDE FIN_PRO_IKY_PGP OF 'COLBMPGP.TXT'
       FIN-PRO-IKY-PGP.
           EXIT.
       
       PRO-MKY-PGP SECTION.
       INI-PRO-MKY-PGP.
*% INCLUDE INI_PRO_MKY_PGP OF 'COLBMPGP.TXT'
*% INCLUDE FIN_PRO_MKY_PGP OF 'COLBMPGP.TXT'
       FIN-PRO-MKY-PGP.
           EXIT.
       
       PRO-AKY-PGP SECTION.
       INI-PRO-AKY-PGP.
*% INCLUDE INI_PRO_AKY_PGP OF 'COLBMPGP.TXT'
*% INCLUDE FIN_PRO_AKY_PGP OF 'COLBMPGP.TXT'
       FIN-PRO-AKY-PGP.
           EXIT.
       
       UNP-KEY-PGP SECTION.
       INI-UNP-KEY-PGP.
*% INCLUDE INI_UNP_KEY_PGP OF 'COLBMPGP.TXT'
           PERFORM UNP-IKY-PGP.
           PERFORM UNP-AKY-PGP.
           PERFORM UNP-MKY-PGP.
*% INCLUDE FIN_UNP_KEY_PGP OF 'COLBMPGP.TXT'
       FIN-UNP-KEY-PGP.
           EXIT.
       
       UNP-IKY-PGP SECTION.
       INI-UNP-IKY-PGP.
*% INCLUDE INI_UNP_IKY_PGP OF 'COLBMPGP.TXT'
           MOVE FRM-CPIM-UBY TO PGP-COD-PLAN-ATR.
           MOVE FRM-CPIM-UBY TO PGP-COD-EVEN-ATR.
           MOVE FRM-CPIM-UBY TO PGP-COD-OFIC-ATR.
*% RREG = "PGP"
*% NOMB = "FPLA"
           MOVE FRM-CPIM-UBY TO PGP-FEC-FPLA-ATR IN PGP-FLD.
           MOVE FRM-CPIM-UBY TO PGP-COD-SEGM-ATR.
           MOVE FRM-CPIM-UBY TO PGP-IND-SEGM-ATR.
*% RREG = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO DBC-NUM-ICLI-ATR IN PGP-FLD.
           MOVE FRM-CPIM-UBY TO DBC-VRF-ICLI-ATR IN PGP-FLD.
           MOVE FRM-CPIM-UBY TO DBC-IND-ICLI-ATR IN PGP-FLD.
           MOVE FRM-CPIM-UBY TO DBC-GLS-ICLI-ATR IN PGP-FLD.
      * IDC-FIN --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
*% INCLUDE FIN_UNP_IKY_PGP OF 'COLBMPGP.TXT'
       FIN-UNP-IKY-PGP.
           EXIT.
       
