      *
      * COLBQPDD
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:13 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "PDD"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMPDD.TXT'
       SET-TAG-COLBQPDD SECTION.
       INI-SET-TAG-COLBQPDD.
           MOVE
           '<IDNMOD NOM=$COLBQPDD$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:13 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQPDD.
           EXIT.
           
       PUT-ALL-PDD SECTION.
       INI-PUT-ALL-PDD.
*% INCLUDE INI_PUT_ALL_PDD OF 'COLBMPDD.TXT'
           PERFORM PUT-ITK-PDD.
*% INCLUDE FIN_PUT_ALL_PDD OF 'COLBMPDD.TXT'
       FIN-PUT-ALL-PDD.
           EXIT.
       
       PUT-ITK-PDD SECTION.
       INI-PUT-ITK-PDD.
*% INCLUDE INI_PUT_ITK_PDD OF 'COLBMPDD.TXT'
           IF PGM-STAT-ITK-NUL
               GO TO FIN-PUT-ITK-PDD.
           IF NOT PGM-STAT-ITK-OKS
               PERFORM CLR-ITK-PDD
               GO TO FIN-PUT-ITK-PDD.
           MOVE ITK-CAI-IITK IN ITK TO ITK-CAI-IITK IN PDD-FLD.
           MOVE ITK-IIC-IITK IN ITK TO ITK-IIC-IITK IN PDD-FLD.
*% RREG = "ITK"
*% RREG_FRM = "ITK"
*% NOMB = "FPRO"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE ITK-NUM-DPRO IN ITK TO ITK-NUM-DPRO IN PDD-FLD.
           MOVE ITK-NUM-MPRO IN ITK TO ITK-NUM-MPRO IN PDD-FLD.
           MOVE ITK-NUM-SPRO IN ITK TO ITK-NUM-SPRO IN PDD-FLD.
           MOVE ITK-NUM-APRO IN ITK TO ITK-NUM-APRO IN PDD-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE ITK-COD-TPRO IN ITK TO ITK-COD-TPRO IN PDD-FLD.
           MOVE ITK-COD-ENTI IN ITK TO ITK-COD-ENTI IN PDD-FLD.
*% INCLUDE FIN_PUT_ITK_PDD OF 'COLBMPDD.TXT'
       FIN-PUT-ITK-PDD.
           EXIT.
       
       KEY-ALL-PDD SECTION.
       INI-KEY-ALL-PDD.
*% INCLUDE INI_KEY_ALL_PDD OF 'COLBMPDD.TXT'
           PERFORM KEY-ITK-PDD.
*% INCLUDE FIN_KEY_ALL_PDD OF 'COLBMPDD.TXT'
       FIN-KEY-ALL-PDD.
           EXIT.
       
       KEY-ITK-PDD SECTION.
       INI-KEY-ITK-PDD.
*% INCLUDE INI_KEY_ITK_PDD OF 'COLBMPDD.TXT'
           MOVE ITK-CAI-IITK IN PDD-FLD TO ITK-CAI-IITK IN ITK.
           MOVE ITK-IIC-IITK IN PDD-FLD TO ITK-IIC-IITK IN ITK.
*% RREG = "ITK"
*% RREG_FRM = "ITK"
*% NOMB = "FPRO"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE ITK-NUM-DPRO IN PDD-FLD TO ITK-NUM-DPRO IN ITK.
           MOVE ITK-NUM-MPRO IN PDD-FLD TO ITK-NUM-MPRO IN ITK.
           MOVE ITK-NUM-SPRO IN PDD-FLD TO ITK-NUM-SPRO IN ITK.
           MOVE ITK-NUM-APRO IN PDD-FLD TO ITK-NUM-APRO IN ITK.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE ITK-COD-TPRO IN PDD-FLD TO ITK-COD-TPRO IN ITK.
*% INCLUDE FIN_KEY_ITK_PDD OF 'COLBMPDD.TXT'
       FIN-KEY-ITK-PDD.
           EXIT.
       
       SCR-CLRA-PDD SECTION.
       INI-SCR-CLRA-PDD.
*% INCLUDE INI_SCR_CLRA_PDD OF 'COLBMPDD.TXT'
           PERFORM CLR-KEY-PDD.
           PERFORM CLR-FLD-PDD.
*% INCLUDE FIN_SCR_CLRA_PDD OF 'COLBMPDD.TXT'
       FIN-SCR-CLRA-PDD.
           EXIT.
       
       CLR-KEY-PDD SECTION.
       INI-CLR-KEY-PDD.
*% INCLUDE INI_CLR_KEY_PDD OF 'COLBMPDD.TXT'
           MOVE SPACES TO ITK-CAI-IITK IN PDD-FLD.
           MOVE ZEROES TO ITK-IIC-IITK IN PDD-FLD.
*% RREG = "ITK"
*% RREG_FRM = "ITK"
*% NOMB = "FPRO"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO ITK-FEC-FPRO IN PDD-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO ITK-COD-TPRO IN PDD-FLD.
*% INCLUDE FIN_CLR_KEY_PDD OF 'COLBMPDD.TXT'
       FIN-CLR-KEY-PDD.
           EXIT.
       
       CLR-FLD-PDD SECTION.
       INI-CLR-FLD-PDD.
*% INCLUDE INI_CLR_FLD_PDD OF 'COLBMPDD.TXT'
           MOVE SPACES TO FRM-GLS-IFRM IN PDD-FLD(1).
           MOVE SPACES TO FRM-GLS-IFRM IN PDD-FLD(2).
           MOVE SPACES TO FRM-GLS-IFRM IN PDD-FLD(3).
           MOVE SPACES TO FRM-GLS-IFRM IN PDD-FLD(4).
           MOVE SPACES TO FRM-GLS-IFRM IN PDD-FLD(5).
           MOVE SPACES TO FRM-GLS-IFRM IN PDD-FLD(6).
           MOVE SPACES TO FRM-GLS-IFRM IN PDD-FLD(7).
           MOVE SPACES TO FRM-GLS-IFRM IN PDD-FLD(8).
           MOVE SPACES TO FRM-GLS-IFRM IN PDD-FLD(9).
           MOVE SPACES TO FRM-GLS-IFRM IN PDD-FLD(10).
           MOVE SPACES TO FRM-GLS-IFRM IN PDD-FLD(11).
           MOVE SPACES TO FRM-GLS-IFRM IN PDD-FLD(12).
           MOVE SPACES TO FRM-GLS-IFRM IN PDD-FLD(13).
           MOVE SPACES TO FRM-GLS-IFRM IN PDD-FLD(14).
           MOVE SPACES TO FRM-GLS-IFRM IN PDD-FLD(15).
           MOVE SPACES TO FRM-GLS-IFRM IN PDD-FLD(16).
           MOVE SPACES TO FRM-GLS-IFRM IN PDD-FLD(17).
           MOVE SPACES TO FRM-GLS-IFRM IN PDD-FLD(18).
           MOVE SPACES TO FRM-GLS-IFRM IN PDD-FLD(19).
           MOVE SPACES TO FRM-GLS-IFRM IN PDD-FLD(20).
           MOVE SPACES TO FRM-GLS-DATA IN PDD-FLD(1).
           MOVE SPACES TO FRM-GLS-DATA IN PDD-FLD(2).
           MOVE SPACES TO FRM-GLS-DATA IN PDD-FLD(3).
           MOVE SPACES TO FRM-GLS-DATA IN PDD-FLD(4).
           MOVE SPACES TO FRM-GLS-DATA IN PDD-FLD(5).
           MOVE SPACES TO FRM-GLS-DATA IN PDD-FLD(6).
           MOVE SPACES TO FRM-GLS-DATA IN PDD-FLD(7).
           MOVE SPACES TO FRM-GLS-DATA IN PDD-FLD(8).
           MOVE SPACES TO FRM-GLS-DATA IN PDD-FLD(9).
           MOVE SPACES TO FRM-GLS-DATA IN PDD-FLD(10).
           MOVE SPACES TO FRM-GLS-DATA IN PDD-FLD(11).
           MOVE SPACES TO FRM-GLS-DATA IN PDD-FLD(12).
           MOVE SPACES TO FRM-GLS-DATA IN PDD-FLD(13).
           MOVE SPACES TO FRM-GLS-DATA IN PDD-FLD(14).
           MOVE SPACES TO FRM-GLS-DATA IN PDD-FLD(15).
           MOVE SPACES TO FRM-GLS-DATA IN PDD-FLD(16).
           MOVE SPACES TO FRM-GLS-DATA IN PDD-FLD(17).
           MOVE SPACES TO FRM-GLS-DATA IN PDD-FLD(18).
           MOVE SPACES TO FRM-GLS-DATA IN PDD-FLD(19).
           MOVE SPACES TO FRM-GLS-DATA IN PDD-FLD(20).
           PERFORM CLR-ITK-PDD.
*% INCLUDE FIN_CLR_FLD_PDD OF 'COLBMPDD.TXT'
       FIN-CLR-FLD-PDD.
           EXIT.
       
       CLR-ITK-PDD SECTION.
       INI-CLR-ITK-PDD.
*% INCLUDE INI_CLR_ITK_PDD OF 'COLBMPDD.TXT'
           MOVE SPACES TO ITK-COD-ENTI IN PDD-FLD.
*% INCLUDE FIN_CLR_ITK_PDD OF 'COLBMPDD.TXT'
       FIN-CLR-ITK-PDD.
           EXIT.
       
       FST-KEY-PDD SECTION.
       INI-FST-KEY-PDD.
*% INCLUDE INI_FST_KEY_PDD OF 'COLBMPDD.TXT'
           MOVE -1 TO ITK-COD-TPRO-LEN IN PDD-FLD.
*% INCLUDE FIN_FST_KEY_PDD OF 'COLBMPDD.TXT'
       FIN-FST-KEY-PDD.
           EXIT.
       
       FST-FLD-PDD SECTION.
       INI-FST-FLD-PDD.
*% INCLUDE INI_FST_FLD_PDD OF 'COLBMPDD.TXT'
           MOVE -1 TO FRM-GLS-DATA-LEN IN PDD-FLD(1).
*% INCLUDE FIN_FST_FLD_PDD OF 'COLBMPDD.TXT'
       FIN-FST-FLD-PDD.
           EXIT.
       
       PRO-KEY-PDD SECTION.
       INI-PRO-KEY-PDD.
*% INCLUDE INI_PRO_KEY_PDD OF 'COLBMPDD.TXT'
           PERFORM PRO-IKY-PDD.
           PERFORM PRO-AKY-PDD.
           PERFORM PRO-MKY-PDD.
*% INCLUDE FIN_PRO_KEY_PDD OF 'COLBMPDD.TXT'
       FIN-PRO-KEY-PDD.
           EXIT.
       
       PRO-IKY-PDD SECTION.
       INI-PRO-IKY-PDD.
*% INCLUDE INI_PRO_IKY_PDD OF 'COLBMPDD.TXT'
           MOVE FRM-CPIM-AEY TO ITK-COD-TPRO-ATR IN PDD-FLD.
*% INCLUDE FIN_PRO_IKY_PDD OF 'COLBMPDD.TXT'
       FIN-PRO-IKY-PDD.
           EXIT.
       
       PRO-MKY-PDD SECTION.
       INI-PRO-MKY-PDD.
*% INCLUDE INI_PRO_MKY_PDD OF 'COLBMPDD.TXT'
*% INCLUDE FIN_PRO_MKY_PDD OF 'COLBMPDD.TXT'
       FIN-PRO-MKY-PDD.
           EXIT.
       
       PRO-AKY-PDD SECTION.
       INI-PRO-AKY-PDD.
*% INCLUDE INI_PRO_AKY_PDD OF 'COLBMPDD.TXT'
           MOVE FRM-CPIM-AEY TO ITK-CAI-IITK-ATR IN PDD-FLD.
           MOVE FRM-CPIM-AEY TO ITK-IIC-IITK-ATR IN PDD-FLD.
*% RREG = "ITK"
*% RREG_FRM = "ITK"
*% NOMB = "FPRO"
           MOVE FRM-CPIM-AEY TO ITK-FEC-FPRO-ATR IN PDD-FLD.
*% INCLUDE FIN_PRO_AKY_PDD OF 'COLBMPDD.TXT'
       FIN-PRO-AKY-PDD.
           EXIT.
       
       UNP-KEY-PDD SECTION.
       INI-UNP-KEY-PDD.
*% INCLUDE INI_UNP_KEY_PDD OF 'COLBMPDD.TXT'
           PERFORM UNP-IKY-PDD.
           PERFORM UNP-AKY-PDD.
           PERFORM UNP-MKY-PDD.
*% INCLUDE FIN_UNP_KEY_PDD OF 'COLBMPDD.TXT'
       FIN-UNP-KEY-PDD.
           EXIT.
       
       UNP-IKY-PDD SECTION.
       INI-UNP-IKY-PDD.
*% INCLUDE INI_UNP_IKY_PDD OF 'COLBMPDD.TXT'
           MOVE FRM-CPIM-UBY TO ITK-COD-TPRO-ATR.
*% INCLUDE FIN_UNP_IKY_PDD OF 'COLBMPDD.TXT'
       FIN-UNP-IKY-PDD.
           EXIT.
       
       UNP-MKY-PDD SECTION.
       INI-UNP-MKY-PDD.
*% INCLUDE INI_UNP_MKY_PDD OF 'COLBMPDD.TXT'
*% INCLUDE FIN_UNP_MKY_PDD OF 'COLBMPDD.TXT'
       FIN-UNP-MKY-PDD.
           EXIT.
       
       UNP-AKY-PDD SECTION.
       INI-UNP-AKY-PDD.
*% INCLUDE INI_UNP_AKY_PDD OF 'COLBMPDD.TXT'
           MOVE FRM-CPIM-UBY TO ITK-CAI-IITK-ATR.
           MOVE FRM-CPIM-UBY TO ITK-IIC-IITK-ATR.
*% RREG = "ITK"
*% RREG_FRM = "ITK"
*% NOMB = "FPRO"
           MOVE FRM-CPIM-UBY TO ITK-FEC-FPRO-ATR IN PDD-FLD.
*% INCLUDE FIN_UNP_AKY_PDD OF 'COLBMPDD.TXT'
       FIN-UNP-AKY-PDD.
           EXIT.
       
       UNP-FLD-PDD SECTION.
       INI-UNP-FLD-PDD.
*% INCLUDE INI_UNP_FLD_PDD OF 'COLBMPDD.TXT'
           MOVE FRM-CPIM-UBY TO FRM-GLS-DATA-ATR IN PDD-FLD(1).
           MOVE FRM-CPIM-UBY TO FRM-GLS-DATA-ATR IN PDD-FLD(2).
           MOVE FRM-CPIM-UBY TO FRM-GLS-DATA-ATR IN PDD-FLD(3).
           MOVE FRM-CPIM-UBY TO FRM-GLS-DATA-ATR IN PDD-FLD(4).
           MOVE FRM-CPIM-UBY TO FRM-GLS-DATA-ATR IN PDD-FLD(5).
           MOVE FRM-CPIM-UBY TO FRM-GLS-DATA-ATR IN PDD-FLD(6).
           MOVE FRM-CPIM-UBY TO FRM-GLS-DATA-ATR IN PDD-FLD(7).
           MOVE FRM-CPIM-UBY TO FRM-GLS-DATA-ATR IN PDD-FLD(8).
           MOVE FRM-CPIM-UBY TO FRM-GLS-DATA-ATR IN PDD-FLD(9).
           MOVE FRM-CPIM-UBY TO FRM-GLS-DATA-ATR IN PDD-FLD(10).
           MOVE FRM-CPIM-UBY TO FRM-GLS-DATA-ATR IN PDD-FLD(11).
           MOVE FRM-CPIM-UBY TO FRM-GLS-DATA-ATR IN PDD-FLD(12).
           MOVE FRM-CPIM-UBY TO FRM-GLS-DATA-ATR IN PDD-FLD(13).
           MOVE FRM-CPIM-UBY TO FRM-GLS-DATA-ATR IN PDD-FLD(14).
           MOVE FRM-CPIM-UBY TO FRM-GLS-DATA-ATR IN PDD-FLD(15).
           MOVE FRM-CPIM-UBY TO FRM-GLS-DATA-ATR IN PDD-FLD(16).
           MOVE FRM-CPIM-UBY TO FRM-GLS-DATA-ATR IN PDD-FLD(17).
           MOVE FRM-CPIM-UBY TO FRM-GLS-DATA-ATR IN PDD-FLD(18).
           MOVE FRM-CPIM-UBY TO FRM-GLS-DATA-ATR IN PDD-FLD(19).
           MOVE FRM-CPIM-UBY TO FRM-GLS-DATA-ATR IN PDD-FLD(20).
           PERFORM UNP-ITK-PDD.
*% INCLUDE FIN_UNP_FLD_PDD OF 'COLBMPDD.TXT'
       FIN-UNP-FLD-PDD.
           EXIT.
       
       UNP-ITK-PDD SECTION.
       INI-UNP-ITK-PDD.
*% INCLUDE INI_UNP_ITK_PDD OF 'COLBMPDD.TXT'
*% INCLUDE FIN_UNP_ITK_PDD OF 'COLBMPDD.TXT'
       FIN-UNP-ITK-PDD.
           EXIT.
       
       VAL-KEY-PDD SECTION.
       INI-VAL-KEY-PDD.
           PERFORM VAL-IKY-PDD.
           PERFORM VAL-MKY-PDD.
           PERFORM VAL-AKY-PDD.
       FIN-VAL-KEY-PDD.
           EXIT.
       
       VAL-IKY-PDD SECTION.
       INI-VAL-IKY-PDD.
*% INCLUDE INI_VAL_IKY_PDD OF 'COLBMPDD.TXT'
           PERFORM VAL-ITK-COD-TPRO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO ITK-COD-TPRO-LEN IN PDD-FLD
                GO TO FIN-VAL-IKY-PDD.
*% INCLUDE FIN_VAL_IKY_PDD OF 'COLBMPDD.TXT'
       FIN-VAL-IKY-PDD.
           EXIT.
       
       VAL-MKY-PDD SECTION.
       INI-VAL-MKY-PDD.
*% INCLUDE INI_VAL_MKY_PDD OF 'COLBMPDD.TXT'
*% INCLUDE FIN_VAL_MKY_PDD OF 'COLBMPDD.TXT'
       FIN-VAL-MKY-PDD.
           EXIT.
       
       VAL-AKY-PDD SECTION.
       INI-VAL-AKY-PDD.
*% INCLUDE INI_VAL_AKY_PDD OF 'COLBMPDD.TXT'
           PERFORM VAL-ITK-CAI-IITK.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO ITK-CAI-IITK-LEN IN PDD-FLD
                GO TO FIN-VAL-AKY-PDD.
           PERFORM VAL-ITK-IIC-IITK.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO ITK-IIC-IITK-LEN IN PDD-FLD
                GO TO FIN-VAL-AKY-PDD.
*% RREG = "ITK"
*% RREG_FRM = "ITK"
*% NOMB = "FPRO"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-ITK-FEC-FPRO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                ITK-FEC-FPRO-LEN IN PDD-FLD
                GO TO FIN-VAL-AKY-PDD.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% INCLUDE FIN_VAL_AKY_PDD OF 'COLBMPDD.TXT'
       FIN-VAL-AKY-PDD.
           EXIT.
       
       VAL-ITK-CAI-IITK SECTION.
       INI-VAL-ITK-CAI-IITK.
*% LBL = "ITK_CAI_IITK OF 'COLBMPDD.TXT'"
*% INCLUDE INI_ITK_CAI_IITK OF 'COLBMPDD.TXT'
           IF ITK-CAI-IITK IN PDD-FLD NOT > SPACES
              MOVE SPACES TO ITK-CAI-IITK IN PDD-FLD.
*% INCLUDE ITK_CAI_IITK OF 'COLBMPDD.TXT'
*% INCLUDE FIN_ITK_CAI_IITK OF 'COLBMPDD.TXT'
       FIN-VAL-ITK-CAI-IITK.
           EXIT.
       
       VAL-ITK-IIC-IITK SECTION.
       INI-VAL-ITK-IIC-IITK.
*% LBL = "ITK_IIC_IITK OF 'COLBMPDD.TXT'"
*% INCLUDE INI_ITK_IIC_IITK OF 'COLBMPDD.TXT'
*% INCLUDE ITK_IIC_IITK OF 'COLBMPDD.TXT'
*% INCLUDE FIN_ITK_IIC_IITK OF 'COLBMPDD.TXT'
       FIN-VAL-ITK-IIC-IITK.
           EXIT.
*% RREG = "ITK"
*% RREG_FRM = "ITK"
*% NOMB = "FPRO"
*% OBL = "N"
*% LBL = "ITK_FEC_FPRO OF 'COLBMPDD.TXT'"
*% IZR = "N"
*% ZRO = "S"
*% BUF = "PDD-FLD"
*% FLD = "ITK-FEC-FPRO"
*% SN  = "N"
*% HOY = "LE"
*% HBL = "S"
*% TIPO = "FEC"
*% FRM_IFLD = ""
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-ITK-FEC-FPRO SECTION.
       INI-VAL-ITK-FEC-FPRO.
           IF ITK-FEC-FPRO IN PDD-FLD  NOT > ZEROES
              MOVE ZEROES TO ITK-FEC-FPRO IN PDD-FLD.
*% INCLUDE 'SETFEC'
*% INCLUDE INI_ITK_FEC_FPRO OF 'COLBMPDD.TXT'
*% INCLUDE 'PROFEC'
*% INCLUDE ITK_FEC_FPRO OF 'COLBMPDD.TXT'
*% INCLUDE FIN_ITK_FEC_FPRO OF 'COLBMPDD.TXT'
       FIN-VAL-ITK-FEC-FPRO.
           EXIT.
      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-ITK-COD-TPRO SECTION.
       INI-VAL-ITK-COD-TPRO.
*% LBL = "ITK_COD_TPRO OF 'COLBMPDD.TXT'"
*% INCLUDE INI_ITK_COD_TPRO OF 'COLBMPDD.TXT'
           IF ITK-COD-TPRO IN PDD-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-ITK-COD-TPRO.
*% INCLUDE ITK_COD_TPRO OF 'COLBMPDD.TXT'
*% INCLUDE FIN_ITK_COD_TPRO OF 'COLBMPDD.TXT'
       FIN-VAL-ITK-COD-TPRO.
           EXIT.
       
       VAL-CON-KEY-PDD SECTION.
       INI-VAL-CON-KEY-PDD.
*% INCLUDE INI_VAL_CON_KEY_PDD OF 'COLBMPDD.TXT'
           IF     ITK-CAI-IITK IN PDD-FLD NOT > SPACES 
              AND ITK-IIC-IITK IN PDD-FLD NOT > ZEROES 
*% RREG = "ITK"
*% RREG_FRM = "ITK"
*% NOMB = "FPRO"
              AND ITK-FEC-FPRO IN PDD-FLD NOT > ZEROES
              AND ITK-COD-TPRO IN PDD-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-PDD.
*% INCLUDE FIN_VAL_CON_KEY_PDD OF 'COLBMPDD.TXT'
       FIN-VAL-CON-KEY-PDD.
           EXIT.
       
       VAL-CON-IKY-PDD SECTION.
       INI-VAL-CON-IKY-PDD.
*% INCLUDE INI_VAL_CON_IKY_PDD OF 'COLBMPDD.TXT'
           IF     ITK-COD-TPRO IN PDD-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-PDD.
*% INCLUDE FIN_VAL_CON_IKY_PDD OF 'COLBMPDD.TXT'
       FIN-VAL-CON-IKY-PDD.
           EXIT.
       
       VAL-NUM-KEY-PDD SECTION.
       INI-VAL-NUM-KEY-PDD.
           PERFORM VAL-NUM-IKY-PDD.
           PERFORM VAL-NUM-MKY-PDD.
           PERFORM VAL-NUM-AKY-PDD.
       FIN-VAL-NUM-KEY-PDD.
           EXIT.
       
       VAL-NUM-IKY-PDD SECTION.
       INI-VAL-NUM-IKY-PDD.
*% INCLUDE INI_VAL_NUM_IKY_PDD OF 'COLBMPDD.TXT'
*% INCLUDE FIN_VAL_NUM_IKY_PDD OF 'COLBMPDD.TXT'
       FIN-VAL-NUM-IKY-PDD.
           EXIT.
       
       VAL-NUM-MKY-PDD SECTION.
       INI-VAL-NUM-MKY-PDD.
*% INCLUDE INI_VAL_NUM_MKY_PDD OF 'COLBMPDD.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_PDD OF 'COLBMPDD.TXT'
       FIN-VAL-NUM-MKY-PDD.
           EXIT.
       
       VAL-NUM-AKY-PDD SECTION.
       INI-VAL-NUM-AKY-PDD.
*% INCLUDE INI_VAL_NUM_AKY_PDD OF 'COLBMPDD.TXT'
       VAL-NUM-ITK-IIC-IITK.
*% BUF = "PDD-FLD"
*% FLD = "ITK-IIC-IITK"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-AKY-PDD.
*% INCLUDE FIN_VAL_NUM_AKY_PDD OF 'COLBMPDD.TXT'
       FIN-VAL-NUM-AKY-PDD.
           EXIT.
       
       VAL-NUM-FLD-PDD SECTION.
       INI-VAL-NUM-FLD-PDD.
*% INCLUDE INI_VAL_NUM_FLD_PDD OF 'COLBMPDD.TXT'
*% INCLUDE FIN_VAL_NUM_FLD_PDD OF 'COLBMPDD.TXT'
       FIN-VAL-NUM-FLD-PDD.
           EXIT.
       
       EDT-KEY-PDD SECTION.
       INI-EDT-KEY-PDD.
           PERFORM EDT-IKY-PDD.
           PERFORM EDT-MKY-PDD.
           PERFORM EDT-AKY-PDD.
*% INCLUDE EDT_KEY_PDD OF 'COLBMPDD.TXT'
       FIN-EDT-KEY-PDD.
           EXIT.
       
       EDT-IKY-PDD SECTION.
       INI-EDT-IKY-PDD.
*% INCLUDE EDT_IKY_PDD OF 'COLBMPDD.TXT'
*% INCLUDE FIN_EDT_IKY_PDD OF 'COLBMPDD.TXT'
       FIN-EDT-IKY-PDD.
           EXIT.
       
       EDT-MKY-PDD SECTION.
       INI-EDT-MKY-PDD.
*% INCLUDE EDT_MKY_PDD OF 'COLBMPDD.TXT'
*% INCLUDE FIN_EDT_MKY_PDD OF 'COLBMPDD.TXT'
       FIN-EDT-MKY-PDD.
           EXIT.
       
       EDT-AKY-PDD SECTION.
       INI-EDT-AKY-PDD.
*% INCLUDE EDT_AKY_PDD OF 'COLBMPDD.TXT'
           IF ITK-IIC-IITK-LEN IN PDD-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO ITK-IIC-IITK-LEN IN PDD-FLD
                GO TO FIN-EDT-AKY-PDD.
           MOVE ITK-IIC-IITK IN PDD-FLD TO ITK-IIC-IITK-EDT IN PDD-EDT.
           MOVE ITK-IIC-IITK-EDT IN PDD-EDT TO 
                ITK-IIC-IITK-ALF IN PDD-FLD.
*% RREG = "ITK"
*% RREG_FRM = "ITK"
*% NOMB = "FPRO"
*% INCLUDE FIN_EDT_AKY_PDD OF 'COLBMPDD.TXT'
       FIN-EDT-AKY-PDD.
           EXIT.
       
       EDT-FLD-PDD SECTION.
       INI-EDT-FLD-PDD.
*% INCLUDE EDT_FLD_PDD OF 'COLBMPDD.TXT'
*% INCLUDE FIN_EDT_FLD_PDD OF 'COLBMPDD.TXT'
       FIN-EDT-FLD-PDD.
           EXIT.
*% INCLUDE EOF_BQ OF 'COLBMPDD.TXT'
*% INCLUDE EOF OF 'COLBMPDD.TXT'
*% INCLUDE EOF_IBM OF 'COLBMPDD.TXT'
