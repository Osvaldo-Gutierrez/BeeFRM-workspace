       VAL-MKY-CIO SECTION.
       INI-VAL-MKY-CIO.
*% INCLUDE INI_VAL_MKY_CIO OF 'COLBMCIO.TXT'
*% INCLUDE FIN_VAL_MKY_CIO OF 'COLBMCIO.TXT'
       FIN-VAL-MKY-CIO.
           EXIT.
       
       VAL-FLD-CIO SECTION.
       INI-VAL-FLD-CIO.
*% INCLUDE INI_VAL_FLD_CIO OF 'COLBMCIO.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * VRF-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * VRF-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DBC-IND-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-IND-ICLI-LEN IN CIO-FLD
                GO TO FIN-VAL-FLD-CIO.
           PERFORM VAL-DBC-GLS-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-GLS-ICLI-LEN IN CIO-FLD
                GO TO FIN-VAL-FLD-CIO.
           PERFORM VAL-DBC-NUM-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-NUM-ICLI-LEN IN CIO-FLD
                GO TO FIN-VAL-FLD-CIO.
           PERFORM VAL-DBC-VRF-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-VRF-ICLI-LEN IN CIO-FLD
                GO TO FIN-VAL-FLD-CIO.
      * IDC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-GLS-NOMB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-NOMB-LEN IN CIO-FLD
                GO TO FIN-VAL-FLD-CIO.
           PERFORM VAL-FRM-GLS-GLS1
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 10
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-GLS-GLS1-LEN IN CIO-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CIO.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FMOR"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FMOR VARYING FRM-IFLD
                FROM 1 BY 1 UNTIL FRM-IFLD > 10 OR
                FRM-SUAR = FRM-SUAR-MAL OR
                FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FMOR-LEN IN CIO-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CIO.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-COD-TOPR
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 10
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-COD-TOPR-LEN IN CIO-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CIO.
           PERFORM VAL-FRM-GLS-TOPE
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 10
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-GLS-TOPE-LEN IN CIO-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CIO.
           PERFORM VAL-CEN-SGV-VAL1.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CEN-SGV-VAL1-LEN IN CIO-FLD
                GO TO FIN-VAL-FLD-CIO.
*% INCLUDE FIN_VAL_FLD_CIO OF 'COLBMCIO.TXT'
       FIN-VAL-FLD-CIO.
           EXIT.
