*% FRM = "CIO"
      *
      * COLBUCIO
      * Version 3.0.1 Nov-2005
      * 03-Dec-15 12:42 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMCIO.TXT'
       SET-TAG-COLBUCIO SECTION.
       INI-SET-TAG-COLBUCIO.
           MOVE
           '<IDNMOD NOM=$COLBUCIO$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$03-Dec-15 12:42 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCIO.
           EXIT.
           
       RET-ALL-CIO SECTION.
       INI-RET-ALL-CIO.
*% INCLUDE INI_RET_ALL_CIO OF 'COLBMCIO.TXT'
           PERFORM KEY-ALL-CIO.
           PERFORM RET-DBC-CIO.
           PERFORM RET-CEN-CIO.
*% INCLUDE FIN_RET_ALL_CIO OF 'COLBMCIO.TXT'
       FIN-RET-ALL-CIO.
           EXIT.
       
       RET-DBC-CIO SECTION.
       INI-RET-DBC-CIO.
*% INCLUDE INI_RET_DBC_CIO OF 'COLBMCIO.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN CIO-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN CIO-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN CIO-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN CIO-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_RET_DBC_CIO OF 'COLBMCIO.TXT'
       FIN-RET-DBC-CIO.
           EXIT.
       
       RET-CEN-CIO SECTION.
       INI-RET-CEN-CIO.
*% INCLUDE INI_RET_CEN_CIO OF 'COLBMCIO.TXT'
           MOVE CEN-SGV-VAL1 IN CIO-FLD TO CEN-SGV-VAL1 IN CEN.
*% INCLUDE FIN_RET_CEN_CIO OF 'COLBMCIO.TXT'
       FIN-RET-CEN-CIO.
           EXIT.
       
       KEY-ALL-CIO SECTION.
       INI-KEY-ALL-CIO.
*% INCLUDE INI_KEY_ALL_CIO OF 'COLBMCIO.TXT'
           PERFORM KEY-DBC-CIO.
           PERFORM KEY-CEN-CIO.
*% INCLUDE FIN_KEY_ALL_CIO OF 'COLBMCIO.TXT'
       FIN-KEY-ALL-CIO.
           EXIT.
       
       KEY-DBC-CIO SECTION.
       INI-KEY-DBC-CIO.
*% INCLUDE INI_KEY_DBC_CIO OF 'COLBMCIO.TXT'
*% INCLUDE FIN_KEY_DBC_CIO OF 'COLBMCIO.TXT'
       FIN-KEY-DBC-CIO.
           EXIT.
       
       KEY-CEN-CIO SECTION.
       INI-KEY-CEN-CIO.
*% INCLUDE INI_KEY_CEN_CIO OF 'COLBMCIO.TXT'
           MOVE CEN-CAI-IENT IN CIO-FLD TO CEN-CAI-IENT IN CEN.
           MOVE CEN-IIC-IENT IN CIO-FLD TO CEN-IIC-IENT IN CEN.
*% INCLUDE FIN_KEY_CEN_CIO OF 'COLBMCIO.TXT'
       FIN-KEY-CEN-CIO.
           EXIT.
       
       SCR-CLRA-CIO SECTION.
       INI-SCR-CLRA-CIO.
*% INCLUDE INI_SCR_CLRA_CIO OF 'COLBMCIO.TXT'
           PERFORM CLR-KEY-CIO.
           PERFORM CLR-FLD-CIO.
*% INCLUDE FIN_SCR_CLRA_CIO OF 'COLBMCIO.TXT'
       FIN-SCR-CLRA-CIO.
           EXIT.
       
       CLR-KEY-CIO SECTION.
       INI-CLR-KEY-CIO.
*% INCLUDE INI_CLR_KEY_CIO OF 'COLBMCIO.TXT'
           MOVE SPACES TO CEN-CAI-IENT IN CIO-FLD.
           MOVE ZEROES TO CEN-IIC-IENT IN CIO-FLD.
*% INCLUDE FIN_CLR_KEY_CIO OF 'COLBMCIO.TXT'
       FIN-CLR-KEY-CIO.
           EXIT.
       
       CLR-FLD-CIO SECTION.
       INI-CLR-FLD-CIO.
*% INCLUDE INI_CLR_FLD_CIO OF 'COLBMCIO.TXT'
           MOVE SPACES TO FRM-GLS-NOMB IN CIO-FLD.
           MOVE SPACES TO FRM-GLS-GLS1 IN CIO-FLD(1).
           MOVE SPACES TO FRM-GLS-GLS1 IN CIO-FLD(2).
           MOVE SPACES TO FRM-GLS-GLS1 IN CIO-FLD(3).
           MOVE SPACES TO FRM-GLS-GLS1 IN CIO-FLD(4).
           MOVE SPACES TO FRM-GLS-GLS1 IN CIO-FLD(5).
           MOVE SPACES TO FRM-GLS-GLS1 IN CIO-FLD(6).
           MOVE SPACES TO FRM-GLS-GLS1 IN CIO-FLD(7).
           MOVE SPACES TO FRM-GLS-GLS1 IN CIO-FLD(8).
           MOVE SPACES TO FRM-GLS-GLS1 IN CIO-FLD(9).
           MOVE SPACES TO FRM-GLS-GLS1 IN CIO-FLD(10).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FMOR"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FMOR IN CIO-FLD(1).
           MOVE ZEROES TO FRM-FEC-FMOR IN CIO-FLD(2).
           MOVE ZEROES TO FRM-FEC-FMOR IN CIO-FLD(3).
           MOVE ZEROES TO FRM-FEC-FMOR IN CIO-FLD(4).
           MOVE ZEROES TO FRM-FEC-FMOR IN CIO-FLD(5).
           MOVE ZEROES TO FRM-FEC-FMOR IN CIO-FLD(6).
           MOVE ZEROES TO FRM-FEC-FMOR IN CIO-FLD(7).
           MOVE ZEROES TO FRM-FEC-FMOR IN CIO-FLD(8).
           MOVE ZEROES TO FRM-FEC-FMOR IN CIO-FLD(9).
           MOVE ZEROES TO FRM-FEC-FMOR IN CIO-FLD(10).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-COD-TOPR IN CIO-FLD(1).
           MOVE SPACES TO FRM-COD-TOPR IN CIO-FLD(2).
           MOVE SPACES TO FRM-COD-TOPR IN CIO-FLD(3).
           MOVE SPACES TO FRM-COD-TOPR IN CIO-FLD(4).
           MOVE SPACES TO FRM-COD-TOPR IN CIO-FLD(5).
           MOVE SPACES TO FRM-COD-TOPR IN CIO-FLD(6).
           MOVE SPACES TO FRM-COD-TOPR IN CIO-FLD(7).
           MOVE SPACES TO FRM-COD-TOPR IN CIO-FLD(8).
           MOVE SPACES TO FRM-COD-TOPR IN CIO-FLD(9).
           MOVE SPACES TO FRM-COD-TOPR IN CIO-FLD(10).
           MOVE SPACES TO FRM-GLS-TOPE IN CIO-FLD(1).
           MOVE SPACES TO FRM-GLS-TOPE IN CIO-FLD(2).
           MOVE SPACES TO FRM-GLS-TOPE IN CIO-FLD(3).
           MOVE SPACES TO FRM-GLS-TOPE IN CIO-FLD(4).
           MOVE SPACES TO FRM-GLS-TOPE IN CIO-FLD(5).
           MOVE SPACES TO FRM-GLS-TOPE IN CIO-FLD(6).
           MOVE SPACES TO FRM-GLS-TOPE IN CIO-FLD(7).
           MOVE SPACES TO FRM-GLS-TOPE IN CIO-FLD(8).
           MOVE SPACES TO FRM-GLS-TOPE IN CIO-FLD(9).
           MOVE SPACES TO FRM-GLS-TOPE IN CIO-FLD(10).
           PERFORM CLR-DBC-CIO.
           PERFORM CLR-CEN-CIO.
*% INCLUDE FIN_CLR_FLD_CIO OF 'COLBMCIO.TXT'
       FIN-CLR-FLD-CIO.
           EXIT.
       
       CLR-DBC-CIO SECTION.
       INI-CLR-DBC-CIO.
*% INCLUDE INI_CLR_DBC_CIO OF 'COLBMCIO.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN CIO-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN CIO-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN CIO-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN CIO-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_DBC_CIO OF 'COLBMCIO.TXT'
       FIN-CLR-DBC-CIO.
           EXIT.
       
       CLR-CEN-CIO SECTION.
       INI-CLR-CEN-CIO.
