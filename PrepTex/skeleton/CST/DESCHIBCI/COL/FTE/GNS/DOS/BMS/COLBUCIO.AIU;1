           MOVE ZEROES TO CEN-SGV-VAL1 IN CIO-FLD.
*% INCLUDE FIN_CLR_CEN_CIO OF 'COLBMCIO.TXT'
       FIN-CLR-CEN-CIO.
           EXIT.
       
       FST-KEY-CIO SECTION.
       INI-FST-KEY-CIO.
*% INCLUDE INI_FST_KEY_CIO OF 'COLBMCIO.TXT'
           MOVE -1 TO CEN-CAI-IENT-LEN IN CIO-FLD.
*% INCLUDE FIN_FST_KEY_CIO OF 'COLBMCIO.TXT'
       FIN-FST-KEY-CIO.
           EXIT.
       
       FST-FLD-CIO SECTION.
       INI-FST-FLD-CIO.
*% INCLUDE INI_FST_FLD_CIO OF 'COLBMCIO.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
           MOVE FRM-POS-CURS TO DBC-NUM-ICLI-LEN IN CIO-FLD.
      * VRF-FIN --> BMCHIBCI004.SRCAIU ( FST-KEY Y FST-FLD )
*% INCLUDE FIN_FST_FLD_CIO OF 'COLBMCIO.TXT'
       FIN-FST-FLD-CIO.
           EXIT.
       
       PRO-KEY-CIO SECTION.
       INI-PRO-KEY-CIO.
*% INCLUDE INI_PRO_KEY_CIO OF 'COLBMCIO.TXT'
           PERFORM PRO-MKY-CIO.
*% INCLUDE FIN_PRO_KEY_CIO OF 'COLBMCIO.TXT'
       FIN-PRO-KEY-CIO.
           EXIT.
       
       PRO-MKY-CIO SECTION.
       INI-PRO-MKY-CIO.
*% INCLUDE INI_PRO_MKY_CIO OF 'COLBMCIO.TXT'
*% INCLUDE FIN_PRO_MKY_CIO OF 'COLBMCIO.TXT'
       FIN-PRO-MKY-CIO.
           EXIT.
       
       PRO-FLD-CIO SECTION.
       INI-PRO-FLD-CIO.
*% INCLUDE INI_PRO_FLD_CIO OF 'COLBMCIO.TXT'
           MOVE FRM-CPIM-AEY TO FRM-GLS-GLS1-ATR IN CIO-FLD(1).
           MOVE FRM-CPIM-AEY TO FRM-GLS-GLS1-ATR IN CIO-FLD(2).
           MOVE FRM-CPIM-AEY TO FRM-GLS-GLS1-ATR IN CIO-FLD(3).
           MOVE FRM-CPIM-AEY TO FRM-GLS-GLS1-ATR IN CIO-FLD(4).
           MOVE FRM-CPIM-AEY TO FRM-GLS-GLS1-ATR IN CIO-FLD(5).
           MOVE FRM-CPIM-AEY TO FRM-GLS-GLS1-ATR IN CIO-FLD(6).
           MOVE FRM-CPIM-AEY TO FRM-GLS-GLS1-ATR IN CIO-FLD(7).
           MOVE FRM-CPIM-AEY TO FRM-GLS-GLS1-ATR IN CIO-FLD(8).
           MOVE FRM-CPIM-AEY TO FRM-GLS-GLS1-ATR IN CIO-FLD(9).
           MOVE FRM-CPIM-AEY TO FRM-GLS-GLS1-ATR IN CIO-FLD(10).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FMOR"
           MOVE FRM-CPIM-AEY TO FRM-FEC-FMOR-ATR IN CIO-FLD(1).
           MOVE FRM-CPIM-AEY TO FRM-FEC-FMOR-ATR IN CIO-FLD(2).
           MOVE FRM-CPIM-AEY TO FRM-FEC-FMOR-ATR IN CIO-FLD(3).
           MOVE FRM-CPIM-AEY TO FRM-FEC-FMOR-ATR IN CIO-FLD(4).
           MOVE FRM-CPIM-AEY TO FRM-FEC-FMOR-ATR IN CIO-FLD(5).
           MOVE FRM-CPIM-AEY TO FRM-FEC-FMOR-ATR IN CIO-FLD(6).
           MOVE FRM-CPIM-AEY TO FRM-FEC-FMOR-ATR IN CIO-FLD(7).
           MOVE FRM-CPIM-AEY TO FRM-FEC-FMOR-ATR IN CIO-FLD(8).
           MOVE FRM-CPIM-AEY TO FRM-FEC-FMOR-ATR IN CIO-FLD(9).
           MOVE FRM-CPIM-AEY TO FRM-FEC-FMOR-ATR IN CIO-FLD(10).
           MOVE FRM-CPIM-AEY TO FRM-COD-TOPR-ATR IN CIO-FLD(1).
           MOVE FRM-CPIM-AEY TO FRM-COD-TOPR-ATR IN CIO-FLD(2).
           MOVE FRM-CPIM-AEY TO FRM-COD-TOPR-ATR IN CIO-FLD(3).
           MOVE FRM-CPIM-AEY TO FRM-COD-TOPR-ATR IN CIO-FLD(4).
           MOVE FRM-CPIM-AEY TO FRM-COD-TOPR-ATR IN CIO-FLD(5).
           MOVE FRM-CPIM-AEY TO FRM-COD-TOPR-ATR IN CIO-FLD(6).
           MOVE FRM-CPIM-AEY TO FRM-COD-TOPR-ATR IN CIO-FLD(7).
           MOVE FRM-CPIM-AEY TO FRM-COD-TOPR-ATR IN CIO-FLD(8).
           MOVE FRM-CPIM-AEY TO FRM-COD-TOPR-ATR IN CIO-FLD(9).
           MOVE FRM-CPIM-AEY TO FRM-COD-TOPR-ATR IN CIO-FLD(10).
           PERFORM PRO-DBC-CIO.
           PERFORM PRO-CEN-CIO.
*% INCLUDE FIN_PRO_FLD_CIO OF 'COLBMCIO.TXT'
       FIN-PRO-FLD-CIO.
           EXIT.
       
       PRO-DBC-CIO SECTION.
       INI-PRO-DBC-CIO.
*% INCLUDE INI_PRO_DBC_CIO OF 'COLBMCIO.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO DBC-NUM-ICLI-ATR IN CIO-FLD.
           MOVE FRM-CPIM-AEY TO DBC-VRF-ICLI-ATR IN CIO-FLD.
           MOVE FRM-CPIM-AEY TO DBC-IND-ICLI-ATR IN CIO-FLD.
           MOVE FRM-CPIM-AEY TO DBC-GLS-ICLI-ATR IN CIO-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
*% INCLUDE FIN_PRO_DBC_CIO OF 'COLBMCIO.TXT'
