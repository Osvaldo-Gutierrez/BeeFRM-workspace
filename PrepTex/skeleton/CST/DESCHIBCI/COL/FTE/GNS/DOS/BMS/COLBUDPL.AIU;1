       VAL-MKY-DPL SECTION.
       INI-VAL-MKY-DPL.
*% INCLUDE INI_VAL_MKY_DPL OF 'COLBMDPL.TXT'
*% INCLUDE FIN_VAL_MKY_DPL OF 'COLBMDPL.TXT'
       FIN-VAL-MKY-DPL.
           EXIT.
       
       VAL-FLD-DPL SECTION.
       INI-VAL-FLD-DPL.
*% INCLUDE INI_VAL_FLD_DPL OF 'COLBMDPL.TXT'
           PERFORM VAL-DPL-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DPL-CAI-IOPC-LEN IN DPL-FLD
                GO TO FIN-VAL-FLD-DPL.
           PERFORM VAL-DPL-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DPL-IIC-IOPC-LEN IN DPL-FLD
                GO TO FIN-VAL-FLD-DPL.
           PERFORM VAL-DPL-COD-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DPL-COD-VCOC-LEN IN DPL-FLD
                GO TO FIN-VAL-FLD-DPL.
           PERFORM VAL-DPL-NUM-IVEN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DPL-NUM-IVEN-LEN IN DPL-FLD
                GO TO FIN-VAL-FLD-DPL.
           PERFORM VAL-FRM-GLS-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-VCOC-LEN IN DPL-FLD
                GO TO FIN-VAL-FLD-DPL.
           PERFORM VAL-DPL-NUM-VCOR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DPL-NUM-VCOR-LEN IN DPL-FLD
                GO TO FIN-VAL-FLD-DPL.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * VRF-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * VRF-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DBC-IND-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-IND-ICLI-LEN IN DPL-FLD
                GO TO FIN-VAL-FLD-DPL.
           PERFORM VAL-DBC-GLS-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-GLS-ICLI-LEN IN DPL-FLD
                GO TO FIN-VAL-FLD-DPL.
           PERFORM VAL-DBC-NUM-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-NUM-ICLI-LEN IN DPL-FLD
                GO TO FIN-VAL-FLD-DPL.
           PERFORM VAL-DBC-VRF-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-VRF-ICLI-LEN IN DPL-FLD
                GO TO FIN-VAL-FLD-DPL.
      * IDC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DBC-GLS-NOMC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DBC-GLS-NOMC-LEN IN DPL-FLD
                GO TO FIN-VAL-FLD-DPL.
           PERFORM VAL-DPL-VAL-CUOT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DPL-VAL-CUOT-LEN IN DPL-FLD
                GO TO FIN-VAL-FLD-DPL.
*% RREG = "DPL"
*% RREG_FRM = "DPL"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DPL-FEC-FVEN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DPL-FEC-FVEN-LEN IN DPL-FLD
                GO TO FIN-VAL-FLD-DPL.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DPL-VAL-MPAG.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DPL-VAL-MPAG-LEN IN DPL-FLD
                GO TO FIN-VAL-FLD-DPL.
*% RREG = "DPL"
*% RREG_FRM = "DPL"
*% NOMB = "FING"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DPL-FEC-FING.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DPL-FEC-FING-LEN IN DPL-FLD
                GO TO FIN-VAL-FLD-DPL.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DPL-VAL-MPGO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DPL-VAL-MPGO-LEN IN DPL-FLD
                GO TO FIN-VAL-FLD-DPL.
           PERFORM VAL-DPL-COD-NOVD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DPL-COD-NOVD-LEN IN DPL-FLD
                GO TO FIN-VAL-FLD-DPL.
           PERFORM VAL-FRM-GLS-NOVD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-NOVD-LEN IN DPL-FLD
                GO TO FIN-VAL-FLD-DPL.
           PERFORM VAL-DPL-COD-ESTA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DPL-COD-ESTA-LEN IN DPL-FLD
                GO TO FIN-VAL-FLD-DPL.
           PERFORM VAL-FRM-GLS-ESTA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-ESTA-LEN IN DPL-FLD
                GO TO FIN-VAL-FLD-DPL.
