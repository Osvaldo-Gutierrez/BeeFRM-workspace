*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF DBC-NUM-ICLI-LEN IN DPL-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DBC-NUM-ICLI-LEN IN DPL-FLD
                GO TO FIN-EDT-FLD-DPL.
           MOVE DBC-NUM-ICLI IN DPL-FLD TO
                DBC-NUM-ICLI-EDT IN DPL-EDT.
           MOVE DBC-NUM-ICLI-EDT IN DPL-EDT TO
                DBC-NUM-ICLI-ALF IN DPL-FLD.
      * IDC-FIN --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF DPL-VAL-CUOT-LEN IN DPL-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DPL-VAL-CUOT-LEN IN DPL-FLD
                GO TO FIN-EDT-FLD-DPL.
           MOVE DPL-VAL-CUOT IN DPL-FLD TO DPL-VAL-CUOT-EDT IN DPL-EDT.
           MOVE DPL-VAL-CUOT-EDT IN DPL-EDT TO 
                DPL-VAL-CUOT-ALF IN DPL-FLD.
*% RREG = "DPL"
*% RREG_FRM = "DPL"
*% NOMB = "FVEN"
           IF DPL-VAL-MPAG-LEN IN DPL-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DPL-VAL-MPAG-LEN IN DPL-FLD
                GO TO FIN-EDT-FLD-DPL.
           MOVE DPL-VAL-MPAG IN DPL-FLD TO DPL-VAL-MPAG-EDT IN DPL-EDT.
           MOVE DPL-VAL-MPAG-EDT IN DPL-EDT TO 
                DPL-VAL-MPAG-ALF IN DPL-FLD.
*% RREG = "DPL"
*% RREG_FRM = "DPL"
*% NOMB = "FING"
           IF DPL-VAL-MPGO-LEN IN DPL-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DPL-VAL-MPGO-LEN IN DPL-FLD
                GO TO FIN-EDT-FLD-DPL.
           MOVE DPL-VAL-MPGO IN DPL-FLD TO DPL-VAL-MPGO-EDT IN DPL-EDT.
           MOVE DPL-VAL-MPGO-EDT IN DPL-EDT TO 
                DPL-VAL-MPGO-ALF IN DPL-FLD.
*% INCLUDE FIN_EDT_FLD_DPL OF 'COLBMDPL.TXT'
       FIN-EDT-FLD-DPL.
           EXIT.
*% INCLUDE EOF_BU OF 'COLBMDPL.TXT'
*% INCLUDE EOF OF 'COLBMDPL.TXT'
*% INCLUDE EOF_IBM OF 'COLBMDPL.TXT'
