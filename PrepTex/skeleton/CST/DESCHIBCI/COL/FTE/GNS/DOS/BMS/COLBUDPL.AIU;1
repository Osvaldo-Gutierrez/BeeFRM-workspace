*% FRM = "DPL"
      *
      * COLBUDPL
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:57 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMDPL.TXT'
       SET-TAG-COLBUDPL SECTION.
       INI-SET-TAG-COLBUDPL.
           MOVE
           '<IDNMOD NOM=$COLBUDPL$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:57 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUDPL.
           EXIT.
           
       RET-ALL-DPL SECTION.
       INI-RET-ALL-DPL.
*% INCLUDE INI_RET_ALL_DPL OF 'COLBMDPL.TXT'
           PERFORM KEY-ALL-DPL.
           PERFORM RET-DPL-DPL.
           PERFORM RET-DBC-DPL.
*% INCLUDE FIN_RET_ALL_DPL OF 'COLBMDPL.TXT'
       FIN-RET-ALL-DPL.
           EXIT.
       
       RET-DPL-DPL SECTION.
       INI-RET-DPL-DPL.
*% INCLUDE INI_RET_DPL_DPL OF 'COLBMDPL.TXT'
           MOVE DPL-CAI-IOPC IN DPL-FLD TO DPL-CAI-IOPC IN DPL.
           MOVE DPL-IIC-IOPC IN DPL-FLD TO DPL-IIC-IOPC IN DPL.
           MOVE DPL-COD-VCOC IN DPL-FLD TO DPL-COD-VCOC IN DPL.
           MOVE DPL-NUM-IVEN IN DPL-FLD TO DPL-NUM-IVEN IN DPL.
           MOVE DPL-NUM-VCOR IN DPL-FLD TO DPL-NUM-VCOR IN DPL.
           MOVE DPL-VAL-CUOT IN DPL-FLD TO DPL-VAL-CUOT IN DPL.
*% RREG = "DPL"
*% RREG_FRM = "DPL"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DPL-NUM-DVEN IN DPL-FLD TO DPL-NUM-DVEN IN DPL.
           MOVE DPL-NUM-MVEN IN DPL-FLD TO DPL-NUM-MVEN IN DPL.
           MOVE DPL-NUM-SVEN IN DPL-FLD TO DPL-NUM-SVEN IN DPL.
           MOVE DPL-NUM-AVEN IN DPL-FLD TO DPL-NUM-AVEN IN DPL.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DPL-VAL-MPAG IN DPL-FLD TO DPL-VAL-MPAG IN DPL.
*% RREG = "DPL"
*% RREG_FRM = "DPL"
*% NOMB = "FING"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DPL-NUM-DING IN DPL-FLD TO DPL-NUM-DING IN DPL.
           MOVE DPL-NUM-MING IN DPL-FLD TO DPL-NUM-MING IN DPL.
           MOVE DPL-NUM-SING IN DPL-FLD TO DPL-NUM-SING IN DPL.
           MOVE DPL-NUM-AING IN DPL-FLD TO DPL-NUM-AING IN DPL.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DPL-VAL-MPGO IN DPL-FLD TO DPL-VAL-MPGO IN DPL.
           MOVE DPL-COD-NOVD IN DPL-FLD TO DPL-COD-NOVD IN DPL.
           MOVE DPL-COD-ESTA IN DPL-FLD TO DPL-COD-ESTA IN DPL.
*% INCLUDE FIN_RET_DPL_DPL OF 'COLBMDPL.TXT'
       FIN-RET-DPL-DPL.
           EXIT.
       
       RET-DBC-DPL SECTION.
       INI-RET-DBC-DPL.
*% INCLUDE INI_RET_DBC_DPL OF 'COLBMDPL.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN DPL-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN DPL-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN DPL-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN DPL-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-GLS-NOMC IN DPL-FLD TO DBC-GLS-NOMC IN DBC.
*% INCLUDE FIN_RET_DBC_DPL OF 'COLBMDPL.TXT'
       FIN-RET-DBC-DPL.
           EXIT.
       
       KEY-ALL-DPL SECTION.
       INI-KEY-ALL-DPL.
*% INCLUDE INI_KEY_ALL_DPL OF 'COLBMDPL.TXT'
           PERFORM KEY-DPL-DPL.
           PERFORM KEY-DBC-DPL.
*% INCLUDE FIN_KEY_ALL_DPL OF 'COLBMDPL.TXT'
       FIN-KEY-ALL-DPL.
           EXIT.
       
       KEY-DPL-DPL SECTION.
       INI-KEY-DPL-DPL.
*% INCLUDE INI_KEY_DPL_DPL OF 'COLBMDPL.TXT'
           MOVE DPL-CAI-IPLL IN DPL-FLD TO DPL-CAI-IPLL IN DPL.
           MOVE DPL-IIC-IPLL IN DPL-FLD TO DPL-IIC-IPLL IN DPL.
           MOVE DPL-CAI-ICVN IN DPL-FLD TO DPL-CAI-ICVN IN DPL.
           MOVE DPL-IIC-ICVN IN DPL-FLD TO DPL-IIC-ICVN IN DPL.
           MOVE DPL-NUM-IPLL IN DPL-FLD TO DPL-NUM-IPLL IN DPL.
           MOVE DPL-NUM-IDPL IN DPL-FLD TO DPL-NUM-IDPL IN DPL.
*% INCLUDE FIN_KEY_DPL_DPL OF 'COLBMDPL.TXT'
       FIN-KEY-DPL-DPL.
           EXIT.
       
       KEY-DBC-DPL SECTION.
       INI-KEY-DBC-DPL.
*% INCLUDE INI_KEY_DBC_DPL OF 'COLBMDPL.TXT'
*% INCLUDE FIN_KEY_DBC_DPL OF 'COLBMDPL.TXT'
       FIN-KEY-DBC-DPL.
           EXIT.
       
       SCR-CLRA-DPL SECTION.
       INI-SCR-CLRA-DPL.
*% INCLUDE INI_SCR_CLRA_DPL OF 'COLBMDPL.TXT'
           PERFORM CLR-KEY-DPL.
           PERFORM CLR-FLD-DPL.
*% INCLUDE FIN_SCR_CLRA_DPL OF 'COLBMDPL.TXT'
       FIN-SCR-CLRA-DPL.
           EXIT.
       
       CLR-KEY-DPL SECTION.
       INI-CLR-KEY-DPL.
*% INCLUDE INI_CLR_KEY_DPL OF 'COLBMDPL.TXT'
           MOVE SPACES TO DPL-CAI-IPLL IN DPL-FLD.
           MOVE ZEROES TO DPL-IIC-IPLL IN DPL-FLD.
           MOVE SPACES TO DPL-CAI-ICVN IN DPL-FLD.
           MOVE ZEROES TO DPL-IIC-ICVN IN DPL-FLD.
           MOVE ZEROES TO DPL-NUM-IPLL IN DPL-FLD.
           MOVE ZEROES TO DPL-NUM-IDPL IN DPL-FLD.
*% INCLUDE FIN_CLR_KEY_DPL OF 'COLBMDPL.TXT'
       FIN-CLR-KEY-DPL.
           EXIT.
       
       CLR-FLD-DPL SECTION.
       INI-CLR-FLD-DPL.
*% INCLUDE INI_CLR_FLD_DPL OF 'COLBMDPL.TXT'
           MOVE SPACES TO FRM-GLS-VCOC IN DPL-FLD.
           MOVE SPACES TO FRM-GLS-NOVD IN DPL-FLD.
           MOVE SPACES TO FRM-GLS-ESTA IN DPL-FLD.
           PERFORM CLR-DPL-DPL.
           PERFORM CLR-DBC-DPL.
*% INCLUDE FIN_CLR_FLD_DPL OF 'COLBMDPL.TXT'
       FIN-CLR-FLD-DPL.
           EXIT.
       
       CLR-DPL-DPL SECTION.
       INI-CLR-DPL-DPL.
*% INCLUDE INI_CLR_DPL_DPL OF 'COLBMDPL.TXT'
           MOVE SPACES TO DPL-CAI-IOPC IN DPL-FLD.
           MOVE ZEROES TO DPL-IIC-IOPC IN DPL-FLD.
           MOVE SPACES TO DPL-COD-VCOC IN DPL-FLD.
           MOVE ZEROES TO DPL-NUM-IVEN IN DPL-FLD.
           MOVE ZEROES TO DPL-NUM-VCOR IN DPL-FLD.
           MOVE ZEROES TO DPL-VAL-CUOT IN DPL-FLD.
*% RREG = "DPL"
*% RREG_FRM = "DPL"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DPL-FEC-FVEN IN DPL-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DPL-VAL-MPAG IN DPL-FLD.
*% RREG = "DPL"
*% RREG_FRM = "DPL"
*% NOMB = "FING"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DPL-FEC-FING IN DPL-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
