      *
      * COLBQPTT
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:46 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "PTT"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMPTT.TXT'
       SET-TAG-COLBQPTT SECTION.
       INI-SET-TAG-COLBQPTT.
           MOVE
           '<IDNMOD NOM=$COLBQPTT$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:46 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQPTT.
           EXIT.
           
       PUT-ALL-PTT SECTION.
       INI-PUT-ALL-PTT.
*% INCLUDE INI_PUT_ALL_PTT OF 'COLBMPTT.TXT'
           PERFORM PUT-PTT-PTT.
*% INCLUDE FIN_PUT_ALL_PTT OF 'COLBMPTT.TXT'
       FIN-PUT-ALL-PTT.
           EXIT.
       
       PUT-PTT-PTT SECTION.
       INI-PUT-PTT-PTT.
*% INCLUDE INI_PUT_PTT_PTT OF 'COLBMPTT.TXT'
           IF PGM-STAT-PTT-NUL
               GO TO FIN-PUT-PTT-PTT.
           IF NOT PGM-STAT-PTT-OKS
               PERFORM CLR-PTT-PTT
               GO TO FIN-PUT-PTT-PTT.
           MOVE PTT-CAI-IPTT IN PTT TO PTT-CAI-IPTT IN PTT-FLD.
           MOVE PTT-IIC-IPTT IN PTT TO PTT-IIC-IPTT IN PTT-FLD.
           MOVE PTT-CAI-ICVN IN PTT TO PTT-CAI-ICVN IN PTT-FLD.
           MOVE PTT-IIC-ICVN IN PTT TO PTT-IIC-ICVN IN PTT-FLD.
           MOVE PTT-COD-TIPO IN PTT TO PTT-COD-TIPO IN PTT-FLD.
           MOVE PTT-IND-SEPA IN PTT TO PTT-IND-SEPA IN PTT-FLD.
           MOVE PTT-GLS-NOMP IN PTT TO PTT-GLS-NOMP IN PTT-FLD.
*% INCLUDE FIN_PUT_PTT_PTT OF 'COLBMPTT.TXT'
       FIN-PUT-PTT-PTT.
           EXIT.
       
       KEY-ALL-PTT SECTION.
       INI-KEY-ALL-PTT.
*% INCLUDE INI_KEY_ALL_PTT OF 'COLBMPTT.TXT'
           PERFORM KEY-PTT-PTT.
*% INCLUDE FIN_KEY_ALL_PTT OF 'COLBMPTT.TXT'
       FIN-KEY-ALL-PTT.
           EXIT.
       
       KEY-PTT-PTT SECTION.
       INI-KEY-PTT-PTT.
*% INCLUDE INI_KEY_PTT_PTT OF 'COLBMPTT.TXT'
           MOVE PTT-CAI-IPTT IN PTT-FLD TO PTT-CAI-IPTT IN PTT.
           MOVE PTT-IIC-IPTT IN PTT-FLD TO PTT-IIC-IPTT IN PTT.
           MOVE PTT-CAI-ICVN IN PTT-FLD TO PTT-CAI-ICVN IN PTT.
           MOVE PTT-IIC-ICVN IN PTT-FLD TO PTT-IIC-ICVN IN PTT.
*% INCLUDE FIN_KEY_PTT_PTT OF 'COLBMPTT.TXT'
       FIN-KEY-PTT-PTT.
           EXIT.
       
       SCR-CLRA-PTT SECTION.
       INI-SCR-CLRA-PTT.
*% INCLUDE INI_SCR_CLRA_PTT OF 'COLBMPTT.TXT'
           PERFORM CLR-KEY-PTT.
           PERFORM CLR-FLD-PTT.
*% INCLUDE FIN_SCR_CLRA_PTT OF 'COLBMPTT.TXT'
       FIN-SCR-CLRA-PTT.
           EXIT.
       
       CLR-KEY-PTT SECTION.
       INI-CLR-KEY-PTT.
*% INCLUDE INI_CLR_KEY_PTT OF 'COLBMPTT.TXT'
           MOVE SPACES TO PTT-CAI-IPTT IN PTT-FLD.
           MOVE ZEROES TO PTT-IIC-IPTT IN PTT-FLD.
           MOVE SPACES TO PTT-CAI-ICVN IN PTT-FLD.
           MOVE ZEROES TO PTT-IIC-ICVN IN PTT-FLD.
*% INCLUDE FIN_CLR_KEY_PTT OF 'COLBMPTT.TXT'
       FIN-CLR-KEY-PTT.
           EXIT.
       
       CLR-FLD-PTT SECTION.
       INI-CLR-FLD-PTT.
*% INCLUDE INI_CLR_FLD_PTT OF 'COLBMPTT.TXT'
           MOVE SPACES TO FRM-GLS-TIPO IN PTT-FLD.
           MOVE SPACES TO FRM-GLS-SEPA IN PTT-FLD.
           PERFORM CLR-PTT-PTT.
*% INCLUDE FIN_CLR_FLD_PTT OF 'COLBMPTT.TXT'
       FIN-CLR-FLD-PTT.
           EXIT.
       
       CLR-PTT-PTT SECTION.
       INI-CLR-PTT-PTT.
*% INCLUDE INI_CLR_PTT_PTT OF 'COLBMPTT.TXT'
           MOVE SPACES TO PTT-COD-TIPO IN PTT-FLD.
           MOVE SPACES TO PTT-IND-SEPA IN PTT-FLD.
           MOVE SPACES TO PTT-GLS-NOMP IN PTT-FLD.
*% INCLUDE FIN_CLR_PTT_PTT OF 'COLBMPTT.TXT'
       FIN-CLR-PTT-PTT.
           EXIT.
       
       FST-KEY-PTT SECTION.
       INI-FST-KEY-PTT.
*% INCLUDE INI_FST_KEY_PTT OF 'COLBMPTT.TXT'
           MOVE -1 TO PTT-CAI-IPTT-LEN IN PTT-FLD.
*% INCLUDE FIN_FST_KEY_PTT OF 'COLBMPTT.TXT'
       FIN-FST-KEY-PTT.
           EXIT.
       
       FST-FLD-PTT SECTION.
       INI-FST-FLD-PTT.
*% INCLUDE INI_FST_FLD_PTT OF 'COLBMPTT.TXT'
           MOVE -1 TO PTT-COD-TIPO-LEN IN PTT-FLD.
*% INCLUDE FIN_FST_FLD_PTT OF 'COLBMPTT.TXT'
       FIN-FST-FLD-PTT.
           EXIT.
       
       PRO-KEY-PTT SECTION.
       INI-PRO-KEY-PTT.
*% INCLUDE INI_PRO_KEY_PTT OF 'COLBMPTT.TXT'
           PERFORM PRO-IKY-PTT.
           PERFORM PRO-AKY-PTT.
           PERFORM PRO-MKY-PTT.
*% INCLUDE FIN_PRO_KEY_PTT OF 'COLBMPTT.TXT'
       FIN-PRO-KEY-PTT.
           EXIT.
       
       PRO-IKY-PTT SECTION.
       INI-PRO-IKY-PTT.
*% INCLUDE INI_PRO_IKY_PTT OF 'COLBMPTT.TXT'
           MOVE FRM-CPIM-AEY TO PTT-CAI-IPTT-ATR IN PTT-FLD.
           MOVE FRM-CPIM-AEY TO PTT-IIC-IPTT-ATR IN PTT-FLD.
           MOVE FRM-CPIM-AEY TO PTT-CAI-ICVN-ATR IN PTT-FLD.
           MOVE FRM-CPIM-AEY TO PTT-IIC-ICVN-ATR IN PTT-FLD.
*% INCLUDE FIN_PRO_IKY_PTT OF 'COLBMPTT.TXT'
       FIN-PRO-IKY-PTT.
           EXIT.
       
       PRO-MKY-PTT SECTION.
       INI-PRO-MKY-PTT.
*% INCLUDE INI_PRO_MKY_PTT OF 'COLBMPTT.TXT'
*% INCLUDE FIN_PRO_MKY_PTT OF 'COLBMPTT.TXT'
       FIN-PRO-MKY-PTT.
           EXIT.
       
       PRO-AKY-PTT SECTION.
       INI-PRO-AKY-PTT.
*% INCLUDE INI_PRO_AKY_PTT OF 'COLBMPTT.TXT'
*% INCLUDE FIN_PRO_AKY_PTT OF 'COLBMPTT.TXT'
       FIN-PRO-AKY-PTT.
           EXIT.
       
       UNP-KEY-PTT SECTION.
       INI-UNP-KEY-PTT.
*% INCLUDE INI_UNP_KEY_PTT OF 'COLBMPTT.TXT'
           PERFORM UNP-IKY-PTT.
           PERFORM UNP-AKY-PTT.
           PERFORM UNP-MKY-PTT.
*% INCLUDE FIN_UNP_KEY_PTT OF 'COLBMPTT.TXT'
       FIN-UNP-KEY-PTT.
           EXIT.
       
       UNP-IKY-PTT SECTION.
       INI-UNP-IKY-PTT.
*% INCLUDE INI_UNP_IKY_PTT OF 'COLBMPTT.TXT'
           MOVE FRM-CPIM-UBY TO PTT-CAI-IPTT-ATR.
           MOVE FRM-CPIM-UBY TO PTT-IIC-IPTT-ATR.
           MOVE FRM-CPIM-UBY TO PTT-CAI-ICVN-ATR.
           MOVE FRM-CPIM-UBY TO PTT-IIC-ICVN-ATR.
*% INCLUDE FIN_UNP_IKY_PTT OF 'COLBMPTT.TXT'
       FIN-UNP-IKY-PTT.
           EXIT.
       
       UNP-MKY-PTT SECTION.
       INI-UNP-MKY-PTT.
*% INCLUDE INI_UNP_MKY_PTT OF 'COLBMPTT.TXT'
*% INCLUDE FIN_UNP_MKY_PTT OF 'COLBMPTT.TXT'
       FIN-UNP-MKY-PTT.
           EXIT.
       
       UNP-AKY-PTT SECTION.
       INI-UNP-AKY-PTT.
*% INCLUDE INI_UNP_AKY_PTT OF 'COLBMPTT.TXT'
*% INCLUDE FIN_UNP_AKY_PTT OF 'COLBMPTT.TXT'
       FIN-UNP-AKY-PTT.
           EXIT.
       
       UNP-FLD-PTT SECTION.
       INI-UNP-FLD-PTT.
*% INCLUDE INI_UNP_FLD_PTT OF 'COLBMPTT.TXT'
           PERFORM UNP-PTT-PTT.
*% INCLUDE FIN_UNP_FLD_PTT OF 'COLBMPTT.TXT'
       FIN-UNP-FLD-PTT.
           EXIT.
       
       UNP-PTT-PTT SECTION.
       INI-UNP-PTT-PTT.
*% INCLUDE INI_UNP_PTT_PTT OF 'COLBMPTT.TXT'
           MOVE FRM-CPIM-UBY TO PTT-COD-TIPO-ATR IN PTT-FLD.
           MOVE FRM-CPIM-UBY TO PTT-IND-SEPA-ATR IN PTT-FLD.
           MOVE FRM-CPIM-UBY TO PTT-GLS-NOMP-ATR IN PTT-FLD.
*% INCLUDE FIN_UNP_PTT_PTT OF 'COLBMPTT.TXT'
       FIN-UNP-PTT-PTT.
           EXIT.
       
       VAL-KEY-PTT SECTION.
       INI-VAL-KEY-PTT.
           PERFORM VAL-IKY-PTT.
           PERFORM VAL-MKY-PTT.
           PERFORM VAL-AKY-PTT.
       FIN-VAL-KEY-PTT.
           EXIT.
       
       VAL-IKY-PTT SECTION.
       INI-VAL-IKY-PTT.
*% INCLUDE INI_VAL_IKY_PTT OF 'COLBMPTT.TXT'
           PERFORM VAL-PTT-CAI-IPTT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PTT-CAI-IPTT-LEN IN PTT-FLD
                GO TO FIN-VAL-IKY-PTT.
           PERFORM VAL-PTT-IIC-IPTT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PTT-IIC-IPTT-LEN IN PTT-FLD
                GO TO FIN-VAL-IKY-PTT.
           PERFORM VAL-PTT-CAI-ICVN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PTT-CAI-ICVN-LEN IN PTT-FLD
                GO TO FIN-VAL-IKY-PTT.
           PERFORM VAL-PTT-IIC-ICVN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PTT-IIC-ICVN-LEN IN PTT-FLD
                GO TO FIN-VAL-IKY-PTT.
*% INCLUDE FIN_VAL_IKY_PTT OF 'COLBMPTT.TXT'
       FIN-VAL-IKY-PTT.
           EXIT.
       
       VAL-MKY-PTT SECTION.
       INI-VAL-MKY-PTT.
*% INCLUDE INI_VAL_MKY_PTT OF 'COLBMPTT.TXT'
*% INCLUDE FIN_VAL_MKY_PTT OF 'COLBMPTT.TXT'
       FIN-VAL-MKY-PTT.
           EXIT.
       
       VAL-AKY-PTT SECTION.
       INI-VAL-AKY-PTT.
*% INCLUDE INI_VAL_AKY_PTT OF 'COLBMPTT.TXT'
*% INCLUDE FIN_VAL_AKY_PTT OF 'COLBMPTT.TXT'
       FIN-VAL-AKY-PTT.
           EXIT.
       
       VAL-PTT-CAI-IPTT SECTION.
       INI-VAL-PTT-CAI-IPTT.
*% LBL = "PTT_CAI_IPTT OF 'COLBMPTT.TXT'"
*% INCLUDE INI_PTT_CAI_IPTT OF 'COLBMPTT.TXT'
           IF PTT-CAI-IPTT IN PTT-FLD NOT > SPACES
              MOVE SPACES TO PTT-CAI-IPTT IN PTT-FLD.
*% INCLUDE PTT_CAI_IPTT OF 'COLBMPTT.TXT'
*% INCLUDE FIN_PTT_CAI_IPTT OF 'COLBMPTT.TXT'
       FIN-VAL-PTT-CAI-IPTT.
           EXIT.
       
       VAL-PTT-IIC-IPTT SECTION.
       INI-VAL-PTT-IIC-IPTT.
*% LBL = "PTT_IIC_IPTT OF 'COLBMPTT.TXT'"
*% INCLUDE INI_PTT_IIC_IPTT OF 'COLBMPTT.TXT'
*% INCLUDE PTT_IIC_IPTT OF 'COLBMPTT.TXT'
*% INCLUDE FIN_PTT_IIC_IPTT OF 'COLBMPTT.TXT'
       FIN-VAL-PTT-IIC-IPTT.
           EXIT.
       
       VAL-PTT-CAI-ICVN SECTION.
       INI-VAL-PTT-CAI-ICVN.
*% LBL = "PTT_CAI_ICVN OF 'COLBMPTT.TXT'"
*% INCLUDE INI_PTT_CAI_ICVN OF 'COLBMPTT.TXT'
           IF PTT-CAI-ICVN IN PTT-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-PTT-CAI-ICVN.
*% INCLUDE PTT_CAI_ICVN OF 'COLBMPTT.TXT'
*% INCLUDE FIN_PTT_CAI_ICVN OF 'COLBMPTT.TXT'
       FIN-VAL-PTT-CAI-ICVN.
           EXIT.
       
       VAL-PTT-IIC-ICVN SECTION.
       INI-VAL-PTT-IIC-ICVN.
*% LBL = "PTT_IIC_ICVN OF 'COLBMPTT.TXT'"
*% INCLUDE INI_PTT_IIC_ICVN OF 'COLBMPTT.TXT'
           IF PTT-IIC-ICVN IN PTT-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-PTT-IIC-ICVN.
*% INCLUDE PTT_IIC_ICVN OF 'COLBMPTT.TXT'
*% INCLUDE FIN_PTT_IIC_ICVN OF 'COLBMPTT.TXT'
       FIN-VAL-PTT-IIC-ICVN.
           EXIT.
       
       VAL-CON-KEY-PTT SECTION.
       INI-VAL-CON-KEY-PTT.
*% INCLUDE INI_VAL_CON_KEY_PTT OF 'COLBMPTT.TXT'
           IF     PTT-CAI-IPTT IN PTT-FLD NOT > SPACES 
              AND PTT-IIC-IPTT IN PTT-FLD NOT > ZEROES 
              AND PTT-CAI-ICVN IN PTT-FLD NOT > SPACES 
              AND PTT-IIC-ICVN IN PTT-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-PTT.
*% INCLUDE FIN_VAL_CON_KEY_PTT OF 'COLBMPTT.TXT'
       FIN-VAL-CON-KEY-PTT.
           EXIT.
       
       VAL-CON-IKY-PTT SECTION.
       INI-VAL-CON-IKY-PTT.
*% INCLUDE INI_VAL_CON_IKY_PTT OF 'COLBMPTT.TXT'
           IF     PTT-CAI-IPTT IN PTT-FLD NOT > SPACES 
              AND PTT-IIC-IPTT IN PTT-FLD NOT > ZEROES 
              AND PTT-CAI-ICVN IN PTT-FLD NOT > SPACES 
              AND PTT-IIC-ICVN IN PTT-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-PTT.
*% INCLUDE FIN_VAL_CON_IKY_PTT OF 'COLBMPTT.TXT'
       FIN-VAL-CON-IKY-PTT.
           EXIT.
       
       VAL-NUM-KEY-PTT SECTION.
       INI-VAL-NUM-KEY-PTT.
           PERFORM VAL-NUM-IKY-PTT.
           PERFORM VAL-NUM-MKY-PTT.
           PERFORM VAL-NUM-AKY-PTT.
       FIN-VAL-NUM-KEY-PTT.
           EXIT.
       
       VAL-NUM-IKY-PTT SECTION.
       INI-VAL-NUM-IKY-PTT.
*% INCLUDE INI_VAL_NUM_IKY_PTT OF 'COLBMPTT.TXT'
       VAL-NUM-PTT-IIC-IPTT.
*% BUF = "PTT-FLD"
*% FLD = "PTT-IIC-IPTT"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-PTT.
       VAL-NUM-PTT-IIC-ICVN.
*% BUF = "PTT-FLD"
*% FLD = "PTT-IIC-ICVN"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-PTT.
*% INCLUDE FIN_VAL_NUM_IKY_PTT OF 'COLBMPTT.TXT'
       FIN-VAL-NUM-IKY-PTT.
           EXIT.
       
       VAL-NUM-MKY-PTT SECTION.
       INI-VAL-NUM-MKY-PTT.
*% INCLUDE INI_VAL_NUM_MKY_PTT OF 'COLBMPTT.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_PTT OF 'COLBMPTT.TXT'
       FIN-VAL-NUM-MKY-PTT.
           EXIT.
       
       VAL-NUM-AKY-PTT SECTION.
       INI-VAL-NUM-AKY-PTT.
*% INCLUDE INI_VAL_NUM_AKY_PTT OF 'COLBMPTT.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_PTT OF 'COLBMPTT.TXT'
       FIN-VAL-NUM-AKY-PTT.
           EXIT.
       
       VAL-NUM-FLD-PTT SECTION.
       INI-VAL-NUM-FLD-PTT.
*% INCLUDE INI_VAL_NUM_FLD_PTT OF 'COLBMPTT.TXT'
*% INCLUDE FIN_VAL_NUM_FLD_PTT OF 'COLBMPTT.TXT'
       FIN-VAL-NUM-FLD-PTT.
           EXIT.
       
       EDT-KEY-PTT SECTION.
       INI-EDT-KEY-PTT.
           PERFORM EDT-IKY-PTT.
           PERFORM EDT-MKY-PTT.
           PERFORM EDT-AKY-PTT.
*% INCLUDE EDT_KEY_PTT OF 'COLBMPTT.TXT'
       FIN-EDT-KEY-PTT.
           EXIT.
       
       EDT-IKY-PTT SECTION.
       INI-EDT-IKY-PTT.
*% INCLUDE EDT_IKY_PTT OF 'COLBMPTT.TXT'
           IF PTT-IIC-IPTT-LEN IN PTT-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PTT-IIC-IPTT-LEN IN PTT-FLD
                GO TO FIN-EDT-IKY-PTT.
           MOVE PTT-IIC-IPTT IN PTT-FLD TO PTT-IIC-IPTT-EDT IN PTT-EDT.
           MOVE PTT-IIC-IPTT-EDT IN PTT-EDT TO 
                PTT-IIC-IPTT-ALF IN PTT-FLD.
           IF PTT-IIC-ICVN-LEN IN PTT-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PTT-IIC-ICVN-LEN IN PTT-FLD
                GO TO FIN-EDT-IKY-PTT.
           MOVE PTT-IIC-ICVN IN PTT-FLD TO PTT-IIC-ICVN-EDT IN PTT-EDT.
           MOVE PTT-IIC-ICVN-EDT IN PTT-EDT TO 
                PTT-IIC-ICVN-ALF IN PTT-FLD.
*% INCLUDE FIN_EDT_IKY_PTT OF 'COLBMPTT.TXT'
       FIN-EDT-IKY-PTT.
           EXIT.
       
       EDT-MKY-PTT SECTION.
       INI-EDT-MKY-PTT.
*% INCLUDE EDT_MKY_PTT OF 'COLBMPTT.TXT'
*% INCLUDE FIN_EDT_MKY_PTT OF 'COLBMPTT.TXT'
       FIN-EDT-MKY-PTT.
           EXIT.
       
       EDT-AKY-PTT SECTION.
       INI-EDT-AKY-PTT.
*% INCLUDE EDT_AKY_PTT OF 'COLBMPTT.TXT'
*% INCLUDE FIN_EDT_AKY_PTT OF 'COLBMPTT.TXT'
       FIN-EDT-AKY-PTT.
           EXIT.
       
       EDT-FLD-PTT SECTION.
       INI-EDT-FLD-PTT.
*% INCLUDE EDT_FLD_PTT OF 'COLBMPTT.TXT'
*% INCLUDE FIN_EDT_FLD_PTT OF 'COLBMPTT.TXT'
       FIN-EDT-FLD-PTT.
           EXIT.
*% INCLUDE EOF_BQ OF 'COLBMPTT.TXT'
*% INCLUDE EOF OF 'COLBMPTT.TXT'
*% INCLUDE EOF_IBM OF 'COLBMPTT.TXT'
