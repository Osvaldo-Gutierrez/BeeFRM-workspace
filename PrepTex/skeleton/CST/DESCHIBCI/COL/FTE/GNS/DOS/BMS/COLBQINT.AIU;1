*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "INT"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMINT.TXT'
       SET-TAG-COLBQINT SECTION.
       INI-SET-TAG-COLBQINT.
           MOVE
           '<IDNMOD NOM=$COLBQINT$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:04 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQINT.
           EXIT.
           
       PUT-ALL-INT SECTION.
       INI-PUT-ALL-INT.
*% INCLUDE INI_PUT_ALL_INT OF 'COLBMINT.TXT'
           PERFORM PUT-INT-INT.
*% INCLUDE FIN_PUT_ALL_INT OF 'COLBMINT.TXT'
       FIN-PUT-ALL-INT.
           EXIT.
       
       PUT-INT-INT SECTION.
       INI-PUT-INT-INT.
*% INCLUDE INI_PUT_INT_INT OF 'COLBMINT.TXT'
           IF PGM-STAT-INT-NUL
               GO TO FIN-PUT-INT-INT.
           IF NOT PGM-STAT-INT-OKS
               PERFORM CLR-INT-INT
               GO TO FIN-PUT-INT-INT.
           MOVE INT-MSC-BCTI IN INT TO INT-MSC-BCTI IN INT-FLD.
           MOVE INT-MSC-PBTI IN INT TO INT-MSC-PBTI IN INT-FLD.
           MOVE INT-MSC-BPBT IN INT TO INT-MSC-BPBT IN INT-FLD.
           MOVE INT-MSC-SPBT IN INT TO INT-MSC-SPBT IN INT-FLD.
           MOVE INT-COD-CVTI IN INT TO INT-COD-CVTI IN INT-FLD.
           MOVE INT-MSC-TFPR IN INT TO INT-MSC-TFPR IN INT-FLD.
           MOVE INT-NUM-PREP IN INT TO INT-NUM-PREP IN INT-FLD.
           MOVE INT-IND-UTPR IN INT TO INT-IND-UTPR IN INT-FLD.
           MOVE INT-CIC-TINT IN INT TO INT-CIC-TINT IN INT-FLD.
           MOVE INT-EXT-CTAB IN INT TO INT-EXT-CTAB IN INT-FLD.
           MOVE INT-GLS-DESC IN INT TO INT-GLS-DESC IN INT-FLD.
           MOVE INT-GLS-ABRV IN INT TO INT-GLS-ABRV IN INT-FLD.
           MOVE INT-GLS-DCOR IN INT TO INT-GLS-DCOR IN INT-FLD.
           MOVE INT-IND-VIGE IN INT TO INT-IND-VIGE IN INT-FLD.
*% RREG = "INT"
*% RREG_FRM = "INT"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE INT-NUM-DTRN IN INT TO INT-NUM-DTRN IN INT-FLD.
           MOVE INT-NUM-MTRN IN INT TO INT-NUM-MTRN IN INT-FLD.
           MOVE INT-NUM-STRN IN INT TO INT-NUM-STRN IN INT-FLD.
           MOVE INT-NUM-ATRN IN INT TO INT-NUM-ATRN IN INT-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% BUF = "INT-FLD"
*% SIS = "COL"
*% TAB = "BCTI"
*% FLD = "INT-MSC-BCTI"
*% GLS = "FRM-GLS-BCTI"
*% TIENE_GLOSA = TRUE
*% LGL = 34
*% INCLUDE 'BUSMSC'
*% BUF = "INT-FLD"
*% SIS = "COL"
*% TAB = "PBTI"
*% FLD = "INT-MSC-PBTI"
*% GLS = "FRM-GLS-PBTI"
*% TIENE_GLOSA = TRUE
*% LGL = 30
*% INCLUDE 'BUSMSC'
*% BUF = "INT-FLD"
*% SIS = "COL"
*% TAB = "MCTI"
*% FLD = "INT-MSC-BPBT"
*% GLS = "FRM-GLS-BPBT"
*% TIENE_GLOSA = TRUE
*% LGL = 34
*% INCLUDE 'BUSMSC'
*% BUF = "INT-FLD"
*% SIS = "COL"
*% TAB = "MCTI"
*% FLD = "INT-MSC-SPBT"
*% GLS = "FRM-GLS-SPBT"
*% TIENE_GLOSA = TRUE
*% LGL = 30
*% INCLUDE 'BUSMSC'
*% BUF = "INT-FLD"
*% SIS = "COL"
*% TAB = "TFPR"
*% FLD = "INT-MSC-TFPR"
*% GLS = "FRM-GLS-TFPR"
*% TIENE_GLOSA = TRUE
*% LGL = 30
*% INCLUDE 'BUSMSC'
*% BUF = "INT-FLD"
*% SIS = "TAB"
*% TAB = "IPER"
*% FLD = "INT-IND-UTPR"
*% GLS = "FRM-GLS-UTPR"
*% TIENE_GLOSA = TRUE
*% LGL = 7
*% INCLUDE 'BUSMSC'
*% INCLUDE FIN_PUT_INT_INT OF 'COLBMINT.TXT'
       FIN-PUT-INT-INT.
           EXIT.
       
       KEY-ALL-INT SECTION.
       INI-KEY-ALL-INT.
*% INCLUDE INI_KEY_ALL_INT OF 'COLBMINT.TXT'
           PERFORM KEY-INT-INT.
*% INCLUDE FIN_KEY_ALL_INT OF 'COLBMINT.TXT'
       FIN-KEY-ALL-INT.
           EXIT.
       
       KEY-INT-INT SECTION.
       INI-KEY-INT-INT.
*% INCLUDE INI_KEY_INT_INT OF 'COLBMINT.TXT'
           MOVE INT-MSC-BCTI IN INT-FLD TO INT-MSC-BCTI IN INT.
           MOVE INT-MSC-PBTI IN INT-FLD TO INT-MSC-PBTI IN INT.
           MOVE INT-MSC-BPBT IN INT-FLD TO INT-MSC-BPBT IN INT.
           MOVE INT-MSC-SPBT IN INT-FLD TO INT-MSC-SPBT IN INT.
           MOVE INT-COD-CVTI IN INT-FLD TO INT-COD-CVTI IN INT.
           MOVE INT-MSC-TFPR IN INT-FLD TO INT-MSC-TFPR IN INT.
           MOVE INT-NUM-PREP IN INT-FLD TO INT-NUM-PREP IN INT.
           MOVE INT-IND-UTPR IN INT-FLD TO INT-IND-UTPR IN INT.
*% INCLUDE FIN_KEY_INT_INT OF 'COLBMINT.TXT'
       FIN-KEY-INT-INT.
           EXIT.
       
       SCR-CLRA-INT SECTION.
       INI-SCR-CLRA-INT.
*% INCLUDE INI_SCR_CLRA_INT OF 'COLBMINT.TXT'
           PERFORM CLR-KEY-INT.
           PERFORM CLR-FLD-INT.
*% INCLUDE FIN_SCR_CLRA_INT OF 'COLBMINT.TXT'
       FIN-SCR-CLRA-INT.
           EXIT.
       
       CLR-KEY-INT SECTION.
       INI-CLR-KEY-INT.
*% INCLUDE INI_CLR_KEY_INT OF 'COLBMINT.TXT'
           MOVE SPACES TO INT-MSC-BCTI IN INT-FLD.
           MOVE SPACES TO INT-MSC-PBTI IN INT-FLD.
           MOVE SPACES TO INT-MSC-BPBT IN INT-FLD.
           MOVE SPACES TO INT-MSC-SPBT IN INT-FLD.
           MOVE SPACES TO INT-COD-CVTI IN INT-FLD.
           MOVE SPACES TO INT-MSC-TFPR IN INT-FLD.
           MOVE ZEROES TO INT-NUM-PREP IN INT-FLD.
           MOVE SPACES TO INT-IND-UTPR IN INT-FLD.
*% INCLUDE FIN_CLR_KEY_INT OF 'COLBMINT.TXT'
       FIN-CLR-KEY-INT.
           EXIT.
       
       CLR-FLD-INT SECTION.
       INI-CLR-FLD-INT.
*% INCLUDE INI_CLR_FLD_INT OF 'COLBMINT.TXT'
           MOVE SPACES TO FRM-GLS-BCTI IN INT-FLD.
           MOVE SPACES TO FRM-GLS-PBTI IN INT-FLD.
           MOVE SPACES TO FRM-GLS-BPBT IN INT-FLD.
           MOVE SPACES TO FRM-GLS-SPBT IN INT-FLD.
           MOVE SPACES TO FRM-GLS-CVTI IN INT-FLD.
           MOVE SPACES TO FRM-GLS-TFPR IN INT-FLD.
           MOVE SPACES TO FRM-GLS-UTPR IN INT-FLD.
           PERFORM CLR-INT-INT.
*% INCLUDE FIN_CLR_FLD_INT OF 'COLBMINT.TXT'
       FIN-CLR-FLD-INT.
           EXIT.
       
       CLR-INT-INT SECTION.
       INI-CLR-INT-INT.
*% INCLUDE INI_CLR_INT_INT OF 'COLBMINT.TXT'
           MOVE SPACES TO INT-CIC-TINT IN INT-FLD.
           MOVE SPACES TO INT-EXT-CTAB IN INT-FLD.
           MOVE SPACES TO INT-GLS-DESC IN INT-FLD.
           MOVE SPACES TO INT-GLS-ABRV IN INT-FLD.
           MOVE SPACES TO INT-GLS-DCOR IN INT-FLD.
           MOVE SPACES TO INT-IND-VIGE IN INT-FLD.
*% RREG = "INT"
*% RREG_FRM = "INT"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO INT-FEC-FTRN IN INT-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_INT_INT OF 'COLBMINT.TXT'
       FIN-CLR-INT-INT.
           EXIT.
       
       FST-KEY-INT SECTION.
       INI-FST-KEY-INT.
*% INCLUDE INI_FST_KEY_INT OF 'COLBMINT.TXT'
           MOVE -1 TO INT-MSC-BCTI-LEN IN INT-FLD.
*% INCLUDE FIN_FST_KEY_INT OF 'COLBMINT.TXT'
       FIN-FST-KEY-INT.
           EXIT.
       
       FST-FLD-INT SECTION.
       INI-FST-FLD-INT.
*% INCLUDE INI_FST_FLD_INT OF 'COLBMINT.TXT'
           MOVE -1 TO INT-CIC-TINT-LEN IN INT-FLD.
*% INCLUDE FIN_FST_FLD_INT OF 'COLBMINT.TXT'
       FIN-FST-FLD-INT.
           EXIT.
       
       PRO-KEY-INT SECTION.
       INI-PRO-KEY-INT.
*% INCLUDE INI_PRO_KEY_INT OF 'COLBMINT.TXT'
           PERFORM PRO-IKY-INT.
           PERFORM PRO-AKY-INT.
           PERFORM PRO-MKY-INT.
*% INCLUDE FIN_PRO_KEY_INT OF 'COLBMINT.TXT'
       FIN-PRO-KEY-INT.
           EXIT.
       
       PRO-IKY-INT SECTION.
       INI-PRO-IKY-INT.
*% INCLUDE INI_PRO_IKY_INT OF 'COLBMINT.TXT'
           MOVE FRM-CPIM-AEY TO INT-MSC-BCTI-ATR IN INT-FLD.
           MOVE FRM-CPIM-AEY TO INT-MSC-PBTI-ATR IN INT-FLD.
           MOVE FRM-CPIM-AEY TO INT-MSC-BPBT-ATR IN INT-FLD.
           MOVE FRM-CPIM-AEY TO INT-MSC-SPBT-ATR IN INT-FLD.
           MOVE FRM-CPIM-AEY TO INT-COD-CVTI-ATR IN INT-FLD.
           MOVE FRM-CPIM-AEY TO INT-MSC-TFPR-ATR IN INT-FLD.
           MOVE FRM-CPIM-AEY TO INT-NUM-PREP-ATR IN INT-FLD.
           MOVE FRM-CPIM-AEY TO INT-IND-UTPR-ATR IN INT-FLD.
*% INCLUDE FIN_PRO_IKY_INT OF 'COLBMINT.TXT'
       FIN-PRO-IKY-INT.
           EXIT.
       
       PRO-MKY-INT SECTION.
       INI-PRO-MKY-INT.
*% INCLUDE INI_PRO_MKY_INT OF 'COLBMINT.TXT'
*% INCLUDE FIN_PRO_MKY_INT OF 'COLBMINT.TXT'
       FIN-PRO-MKY-INT.
           EXIT.
       
       PRO-AKY-INT SECTION.
       INI-PRO-AKY-INT.
*% INCLUDE INI_PRO_AKY_INT OF 'COLBMINT.TXT'
*% INCLUDE FIN_PRO_AKY_INT OF 'COLBMINT.TXT'
       FIN-PRO-AKY-INT.
           EXIT.
       
       UNP-KEY-INT SECTION.
       INI-UNP-KEY-INT.
*% INCLUDE INI_UNP_KEY_INT OF 'COLBMINT.TXT'
           PERFORM UNP-IKY-INT.
           PERFORM UNP-AKY-INT.
           PERFORM UNP-MKY-INT.
*% INCLUDE FIN_UNP_KEY_INT OF 'COLBMINT.TXT'
       FIN-UNP-KEY-INT.
           EXIT.
       
       UNP-IKY-INT SECTION.
       INI-UNP-IKY-INT.
*% INCLUDE INI_UNP_IKY_INT OF 'COLBMINT.TXT'
           MOVE FRM-CPIM-UBY TO INT-MSC-BCTI-ATR.
           MOVE FRM-CPIM-UBY TO INT-MSC-PBTI-ATR.
           MOVE FRM-CPIM-UBY TO INT-MSC-BPBT-ATR.
           MOVE FRM-CPIM-UBY TO INT-MSC-SPBT-ATR.
           MOVE FRM-CPIM-UBY TO INT-COD-CVTI-ATR.
           MOVE FRM-CPIM-UBY TO INT-MSC-TFPR-ATR.
           MOVE FRM-CPIM-UBY TO INT-NUM-PREP-ATR.
           MOVE FRM-CPIM-UBY TO INT-IND-UTPR-ATR.
*% INCLUDE FIN_UNP_IKY_INT OF 'COLBMINT.TXT'
       FIN-UNP-IKY-INT.
           EXIT.
       
       UNP-MKY-INT SECTION.
       INI-UNP-MKY-INT.
*% INCLUDE INI_UNP_MKY_INT OF 'COLBMINT.TXT'
*% INCLUDE FIN_UNP_MKY_INT OF 'COLBMINT.TXT'
       FIN-UNP-MKY-INT.
           EXIT.
       
       UNP-AKY-INT SECTION.
       INI-UNP-AKY-INT.
*% INCLUDE INI_UNP_AKY_INT OF 'COLBMINT.TXT'
*% INCLUDE FIN_UNP_AKY_INT OF 'COLBMINT.TXT'
       FIN-UNP-AKY-INT.
           EXIT.
       
       UNP-FLD-INT SECTION.
       INI-UNP-FLD-INT.
*% INCLUDE INI_UNP_FLD_INT OF 'COLBMINT.TXT'
           PERFORM UNP-INT-INT.
*% INCLUDE FIN_UNP_FLD_INT OF 'COLBMINT.TXT'
       FIN-UNP-FLD-INT.
           EXIT.
       
       UNP-INT-INT SECTION.
       INI-UNP-INT-INT.
*% INCLUDE INI_UNP_INT_INT OF 'COLBMINT.TXT'
           MOVE FRM-CPIM-UBY TO INT-CIC-TINT-ATR IN INT-FLD.
           MOVE FRM-CPIM-UBY TO INT-EXT-CTAB-ATR IN INT-FLD.
           MOVE FRM-CPIM-UBY TO INT-GLS-DESC-ATR IN INT-FLD.
           MOVE FRM-CPIM-UBY TO INT-GLS-ABRV-ATR IN INT-FLD.
           MOVE FRM-CPIM-UBY TO INT-GLS-DCOR-ATR IN INT-FLD.
           MOVE FRM-CPIM-UBY TO INT-IND-VIGE-ATR IN INT-FLD.
*% INCLUDE FIN_UNP_INT_INT OF 'COLBMINT.TXT'
       FIN-UNP-INT-INT.
           EXIT.
       
       VAL-KEY-INT SECTION.
       INI-VAL-KEY-INT.
           PERFORM VAL-IKY-INT.
           PERFORM VAL-MKY-INT.
           PERFORM VAL-AKY-INT.
       FIN-VAL-KEY-INT.
           EXIT.
       
       VAL-IKY-INT SECTION.
       INI-VAL-IKY-INT.
*% INCLUDE INI_VAL_IKY_INT OF 'COLBMINT.TXT'
           PERFORM VAL-INT-MSC-BCTI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO INT-MSC-BCTI-LEN IN INT-FLD
                GO TO FIN-VAL-IKY-INT.
           PERFORM VAL-INT-MSC-PBTI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO INT-MSC-PBTI-LEN IN INT-FLD
                GO TO FIN-VAL-IKY-INT.
           PERFORM VAL-INT-MSC-BPBT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO INT-MSC-BPBT-LEN IN INT-FLD
                GO TO FIN-VAL-IKY-INT.
           PERFORM VAL-INT-MSC-SPBT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO INT-MSC-SPBT-LEN IN INT-FLD
                GO TO FIN-VAL-IKY-INT.
           PERFORM VAL-INT-COD-CVTI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO INT-COD-CVTI-LEN IN INT-FLD
                GO TO FIN-VAL-IKY-INT.
           PERFORM VAL-INT-MSC-TFPR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO INT-MSC-TFPR-LEN IN INT-FLD
                GO TO FIN-VAL-IKY-INT.
           PERFORM VAL-INT-NUM-PREP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO INT-NUM-PREP-LEN IN INT-FLD
                GO TO FIN-VAL-IKY-INT.
           PERFORM VAL-INT-IND-UTPR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO INT-IND-UTPR-LEN IN INT-FLD
                GO TO FIN-VAL-IKY-INT.
*% INCLUDE FIN_VAL_IKY_INT OF 'COLBMINT.TXT'
       FIN-VAL-IKY-INT.
           EXIT.
       
       VAL-MKY-INT SECTION.
       INI-VAL-MKY-INT.
*% INCLUDE INI_VAL_MKY_INT OF 'COLBMINT.TXT'
*% INCLUDE FIN_VAL_MKY_INT OF 'COLBMINT.TXT'
       FIN-VAL-MKY-INT.
           EXIT.
       
       VAL-AKY-INT SECTION.
       INI-VAL-AKY-INT.
*% INCLUDE INI_VAL_AKY_INT OF 'COLBMINT.TXT'
*% INCLUDE FIN_VAL_AKY_INT OF 'COLBMINT.TXT'
       FIN-VAL-AKY-INT.
           EXIT.
       
       VAL-INT-MSC-BCTI SECTION.
       INI-VAL-INT-MSC-BCTI.
*% LBL = "INT_MSC_BCTI OF 'COLBMINT.TXT'"
*% INCLUDE INI_INT_MSC_BCTI OF 'COLBMINT.TXT'
           IF INT-MSC-BCTI IN INT-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-INT-MSC-BCTI.
*% INCLUDE INT_MSC_BCTI OF 'COLBMINT.TXT'
*% BUF = "INT-FLD"
*% SIS = "COL"
*% TAB = "BCTI"
*% FLD = "INT-MSC-BCTI"
*% GLS = "FRM-GLS-BCTI"
*% TIENE_GLOSA = TRUE
*% LGL = 34
*% INCLUDE 'VALIND'
*% INCLUDE FIN_INT_MSC_BCTI OF 'COLBMINT.TXT'
       FIN-VAL-INT-MSC-BCTI.
           EXIT.
       
       VAL-INT-MSC-PBTI SECTION.
       INI-VAL-INT-MSC-PBTI.
*% LBL = "INT_MSC_PBTI OF 'COLBMINT.TXT'"
*% INCLUDE INI_INT_MSC_PBTI OF 'COLBMINT.TXT'
           IF INT-MSC-PBTI IN INT-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-INT-MSC-PBTI.
*% INCLUDE INT_MSC_PBTI OF 'COLBMINT.TXT'
*% BUF = "INT-FLD"
*% SIS = "COL"
*% TAB = "PBTI"
*% FLD = "INT-MSC-PBTI"
*% GLS = "FRM-GLS-PBTI"
*% TIENE_GLOSA = TRUE
*% LGL = 30
*% INCLUDE 'VALIND'
*% INCLUDE FIN_INT_MSC_PBTI OF 'COLBMINT.TXT'
       FIN-VAL-INT-MSC-PBTI.
           EXIT.
       
       VAL-INT-MSC-BPBT SECTION.
       INI-VAL-INT-MSC-BPBT.
*% LBL = "INT_MSC_BPBT OF 'COLBMINT.TXT'"
*% INCLUDE INI_INT_MSC_BPBT OF 'COLBMINT.TXT'
           IF INT-MSC-BPBT IN INT-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-INT-MSC-BPBT.
*% INCLUDE INT_MSC_BPBT OF 'COLBMINT.TXT'
*% BUF = "INT-FLD"
*% SIS = "COL"
*% TAB = "MCTI"
*% FLD = "INT-MSC-BPBT"
*% GLS = "FRM-GLS-BPBT"
*% TIENE_GLOSA = TRUE
*% LGL = 34
*% INCLUDE 'VALIND'
*% INCLUDE FIN_INT_MSC_BPBT OF 'COLBMINT.TXT'
       FIN-VAL-INT-MSC-BPBT.
           EXIT.
       
       VAL-INT-MSC-SPBT SECTION.
       INI-VAL-INT-MSC-SPBT.
*% LBL = "INT_MSC_SPBT OF 'COLBMINT.TXT'"
*% INCLUDE INI_INT_MSC_SPBT OF 'COLBMINT.TXT'
           IF INT-MSC-SPBT IN INT-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-INT-MSC-SPBT.
*% INCLUDE INT_MSC_SPBT OF 'COLBMINT.TXT'
*% BUF = "INT-FLD"
*% SIS = "COL"
*% TAB = "MCTI"
*% FLD = "INT-MSC-SPBT"
*% GLS = "FRM-GLS-SPBT"
*% TIENE_GLOSA = TRUE
*% LGL = 30
*% INCLUDE 'VALIND'
*% INCLUDE FIN_INT_MSC_SPBT OF 'COLBMINT.TXT'
       FIN-VAL-INT-MSC-SPBT.
           EXIT.
       
       VAL-INT-COD-CVTI SECTION.
       INI-VAL-INT-COD-CVTI.
*% LBL = "INT_COD_CVTI OF 'COLBMINT.TXT'"
*% INCLUDE INI_INT_COD_CVTI OF 'COLBMINT.TXT'
           IF INT-COD-CVTI IN INT-FLD NOT > SPACES
              MOVE SPACES TO INT-COD-CVTI IN INT-FLD.
*% INCLUDE INT_COD_CVTI OF 'COLBMINT.TXT'
*% INCLUDE FIN_INT_COD_CVTI OF 'COLBMINT.TXT'
       FIN-VAL-INT-COD-CVTI.
           EXIT.
       
       VAL-INT-MSC-TFPR SECTION.
       INI-VAL-INT-MSC-TFPR.
*% LBL = "INT_MSC_TFPR OF 'COLBMINT.TXT'"
*% INCLUDE INI_INT_MSC_TFPR OF 'COLBMINT.TXT'
           IF INT-MSC-TFPR IN INT-FLD NOT > SPACES
              MOVE SPACES TO INT-MSC-TFPR IN INT-FLD.
*% INCLUDE INT_MSC_TFPR OF 'COLBMINT.TXT'
*% BUF = "INT-FLD"
*% SIS = "COL"
*% TAB = "TFPR"
*% FLD = "INT-MSC-TFPR"
*% GLS = "FRM-GLS-TFPR"
*% TIENE_GLOSA = TRUE
*% LGL = 30
*% INCLUDE 'VALIND'
*% INCLUDE FIN_INT_MSC_TFPR OF 'COLBMINT.TXT'
       FIN-VAL-INT-MSC-TFPR.
           EXIT.
       
       VAL-INT-NUM-PREP SECTION.
       INI-VAL-INT-NUM-PREP.
*% LBL = "INT_NUM_PREP OF 'COLBMINT.TXT'"
*% INCLUDE INI_INT_NUM_PREP OF 'COLBMINT.TXT'
*% INCLUDE INT_NUM_PREP OF 'COLBMINT.TXT'
*% INCLUDE FIN_INT_NUM_PREP OF 'COLBMINT.TXT'
       FIN-VAL-INT-NUM-PREP.
           EXIT.
       
       VAL-INT-IND-UTPR SECTION.
       INI-VAL-INT-IND-UTPR.
*% LBL = "INT_IND_UTPR OF 'COLBMINT.TXT'"
*% INCLUDE INI_INT_IND_UTPR OF 'COLBMINT.TXT'
           IF INT-IND-UTPR IN INT-FLD NOT > SPACES
              MOVE SPACES TO INT-IND-UTPR IN INT-FLD.
*% INCLUDE INT_IND_UTPR OF 'COLBMINT.TXT'
*% BUF = "INT-FLD"
*% SIS = "TAB"
*% TAB = "IPER"
*% FLD = "INT-IND-UTPR"
*% GLS = "FRM-GLS-UTPR"
*% TIENE_GLOSA = TRUE
*% LGL = 7
*% INCLUDE 'VALIND'
*% INCLUDE FIN_INT_IND_UTPR OF 'COLBMINT.TXT'
       FIN-VAL-INT-IND-UTPR.
           EXIT.
*% RREG = "INT"
*% RREG_FRM = "INT"
*% NOMB = "FTRN"
*% IZR = "S"
       
       VAL-CON-KEY-INT SECTION.
       INI-VAL-CON-KEY-INT.
*% INCLUDE INI_VAL_CON_KEY_INT OF 'COLBMINT.TXT'
           IF     INT-MSC-BCTI IN INT-FLD NOT > SPACES 
              AND INT-MSC-PBTI IN INT-FLD NOT > SPACES 
              AND INT-MSC-BPBT IN INT-FLD NOT > SPACES 
              AND INT-MSC-SPBT IN INT-FLD NOT > SPACES 
              AND INT-COD-CVTI IN INT-FLD NOT > SPACES 
              AND INT-MSC-TFPR IN INT-FLD NOT > SPACES 
              AND INT-NUM-PREP IN INT-FLD NOT > ZEROES 
              AND INT-IND-UTPR IN INT-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-INT.
*% INCLUDE FIN_VAL_CON_KEY_INT OF 'COLBMINT.TXT'
       FIN-VAL-CON-KEY-INT.
           EXIT.
       
       VAL-CON-IKY-INT SECTION.
       INI-VAL-CON-IKY-INT.
*% INCLUDE INI_VAL_CON_IKY_INT OF 'COLBMINT.TXT'
           IF     INT-MSC-BCTI IN INT-FLD NOT > SPACES 
              AND INT-MSC-PBTI IN INT-FLD NOT > SPACES 
              AND INT-MSC-BPBT IN INT-FLD NOT > SPACES 
              AND INT-MSC-SPBT IN INT-FLD NOT > SPACES 
              AND INT-COD-CVTI IN INT-FLD NOT > SPACES 
              AND INT-MSC-TFPR IN INT-FLD NOT > SPACES 
              AND INT-NUM-PREP IN INT-FLD NOT > ZEROES 
              AND INT-IND-UTPR IN INT-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-INT.
*% INCLUDE FIN_VAL_CON_IKY_INT OF 'COLBMINT.TXT'
       FIN-VAL-CON-IKY-INT.
           EXIT.
       
       VAL-NUM-KEY-INT SECTION.
       INI-VAL-NUM-KEY-INT.
           PERFORM VAL-NUM-IKY-INT.
           PERFORM VAL-NUM-MKY-INT.
           PERFORM VAL-NUM-AKY-INT.
       FIN-VAL-NUM-KEY-INT.
           EXIT.
       
       VAL-NUM-IKY-INT SECTION.
       INI-VAL-NUM-IKY-INT.
*% INCLUDE INI_VAL_NUM_IKY_INT OF 'COLBMINT.TXT'
       VAL-NUM-INT-NUM-PREP.
*% BUF = "INT-FLD"
*% FLD = "INT-NUM-PREP"
*% ENT = "3"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-INT.
*% INCLUDE FIN_VAL_NUM_IKY_INT OF 'COLBMINT.TXT'
       FIN-VAL-NUM-IKY-INT.
           EXIT.
       
       VAL-NUM-MKY-INT SECTION.
       INI-VAL-NUM-MKY-INT.
*% INCLUDE INI_VAL_NUM_MKY_INT OF 'COLBMINT.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_INT OF 'COLBMINT.TXT'
       FIN-VAL-NUM-MKY-INT.
           EXIT.
       
       VAL-NUM-AKY-INT SECTION.
       INI-VAL-NUM-AKY-INT.
*% INCLUDE INI_VAL_NUM_AKY_INT OF 'COLBMINT.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_INT OF 'COLBMINT.TXT'
       FIN-VAL-NUM-AKY-INT.
           EXIT.
       
       VAL-NUM-FLD-INT SECTION.
       INI-VAL-NUM-FLD-INT.
*% INCLUDE INI_VAL_NUM_FLD_INT OF 'COLBMINT.TXT'
*% INCLUDE FIN_VAL_NUM_FLD_INT OF 'COLBMINT.TXT'
       FIN-VAL-NUM-FLD-INT.
           EXIT.
       
       EDT-KEY-INT SECTION.
       INI-EDT-KEY-INT.
           PERFORM EDT-IKY-INT.
           PERFORM EDT-MKY-INT.
           PERFORM EDT-AKY-INT.
*% INCLUDE EDT_KEY_INT OF 'COLBMINT.TXT'
       FIN-EDT-KEY-INT.
           EXIT.
       
       EDT-IKY-INT SECTION.
       INI-EDT-IKY-INT.
*% INCLUDE EDT_IKY_INT OF 'COLBMINT.TXT'
           IF INT-NUM-PREP-LEN IN INT-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO INT-NUM-PREP-LEN IN INT-FLD
                GO TO FIN-EDT-IKY-INT.
           MOVE INT-NUM-PREP IN INT-FLD TO INT-NUM-PREP-EDT IN INT-EDT.
           MOVE INT-NUM-PREP-EDT IN INT-EDT TO 
                INT-NUM-PREP-ALF IN INT-FLD.
*% INCLUDE FIN_EDT_IKY_INT OF 'COLBMINT.TXT'
       FIN-EDT-IKY-INT.
           EXIT.
       
       EDT-MKY-INT SECTION.
       INI-EDT-MKY-INT.
*% INCLUDE EDT_MKY_INT OF 'COLBMINT.TXT'
*% INCLUDE FIN_EDT_MKY_INT OF 'COLBMINT.TXT'
       FIN-EDT-MKY-INT.
           EXIT.
       
       EDT-AKY-INT SECTION.
       INI-EDT-AKY-INT.
*% INCLUDE EDT_AKY_INT OF 'COLBMINT.TXT'
*% INCLUDE FIN_EDT_AKY_INT OF 'COLBMINT.TXT'
       FIN-EDT-AKY-INT.
           EXIT.
       
       EDT-FLD-INT SECTION.
       INI-EDT-FLD-INT.
*% INCLUDE EDT_FLD_INT OF 'COLBMINT.TXT'
*% RREG = "INT"
*% RREG_FRM = "INT"
*% NOMB = "FTRN"
*% INCLUDE FIN_EDT_FLD_INT OF 'COLBMINT.TXT'
       FIN-EDT-FLD-INT.
           EXIT.
*% INCLUDE EOF_BQ OF 'COLBMINT.TXT'
*% INCLUDE EOF OF 'COLBMINT.TXT'
*% INCLUDE EOF_IBM OF 'COLBMINT.TXT'
