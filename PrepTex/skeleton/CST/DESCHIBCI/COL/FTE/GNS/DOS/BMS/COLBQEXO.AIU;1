      *
      * COLBQEXO
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:00 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "EXO"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMEXO.TXT'
       SET-TAG-COLBQEXO SECTION.
       INI-SET-TAG-COLBQEXO.
           MOVE
           '<IDNMOD NOM=$COLBQEXO$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:00 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQEXO.
           EXIT.
           
       PUT-ALL-EXO SECTION.
       INI-PUT-ALL-EXO.
*% INCLUDE INI_PUT_ALL_EXO OF 'COLBMEXO.TXT'
           PERFORM PUT-EXO-EXO.
*% INCLUDE FIN_PUT_ALL_EXO OF 'COLBMEXO.TXT'
       FIN-PUT-ALL-EXO.
           EXIT.
       
       PUT-EXO-EXO SECTION.
       INI-PUT-EXO-EXO.
*% INCLUDE INI_PUT_EXO_EXO OF 'COLBMEXO.TXT'
           IF PGM-STAT-EXO-NUL
               GO TO FIN-PUT-EXO-EXO.
           IF NOT PGM-STAT-EXO-OKS
               PERFORM CLR-EXO-EXO
               GO TO FIN-PUT-EXO-EXO.
           MOVE EXO-COD-SIST IN EXO TO EXO-COD-SIST IN EXO-FLD.
           MOVE EXO-COD-PROC IN EXO TO EXO-COD-PROC IN EXO-FLD.
           MOVE EXO-CAI-IEXO IN EXO TO EXO-CAI-IEXO IN EXO-FLD.
           MOVE EXO-IIC-IEXO IN EXO TO EXO-IIC-IEXO IN EXO-FLD.
           MOVE EXO-NUM-IEXO IN EXO TO EXO-NUM-IEXO IN EXO-FLD.
           MOVE EXO-COD-TABL IN EXO TO EXO-COD-TABL IN EXO-FLD.
           MOVE EXO-COD-USER IN EXO TO EXO-COD-USER IN EXO-FLD.
           MOVE EXO-GLS-CMTO IN EXO TO EXO-GLS-CMTO IN EXO-FLD.
           MOVE EXO-MSC-STAT IN EXO TO EXO-MSC-STAT IN EXO-FLD.
           MOVE EXO-HRS-FINI IN EXO TO EXO-HRS-FINI IN EXO-FLD.
*% RREG = "EXO"
*% RREG_FRM = "EXO"
*% NOMB = "FINI"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE EXO-NUM-DINI IN EXO TO EXO-NUM-DINI IN EXO-FLD.
           MOVE EXO-NUM-MINI IN EXO TO EXO-NUM-MINI IN EXO-FLD.
           MOVE EXO-NUM-SINI IN EXO TO EXO-NUM-SINI IN EXO-FLD.
           MOVE EXO-NUM-AINI IN EXO TO EXO-NUM-AINI IN EXO-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE EXO-HRS-FFIN IN EXO TO EXO-HRS-FFIN IN EXO-FLD.
*% RREG = "EXO"
*% RREG_FRM = "EXO"
*% NOMB = "FFIN"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE EXO-NUM-DFIN IN EXO TO EXO-NUM-DFIN IN EXO-FLD.
           MOVE EXO-NUM-MFIN IN EXO TO EXO-NUM-MFIN IN EXO-FLD.
           MOVE EXO-NUM-SFIN IN EXO TO EXO-NUM-SFIN IN EXO-FLD.
           MOVE EXO-NUM-AFIN IN EXO TO EXO-NUM-AFIN IN EXO-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% INCLUDE FIN_PUT_EXO_EXO OF 'COLBMEXO.TXT'
       FIN-PUT-EXO-EXO.
           EXIT.
       
       KEY-ALL-EXO SECTION.
       INI-KEY-ALL-EXO.
*% INCLUDE INI_KEY_ALL_EXO OF 'COLBMEXO.TXT'
           PERFORM KEY-EXO-EXO.
*% INCLUDE FIN_KEY_ALL_EXO OF 'COLBMEXO.TXT'
       FIN-KEY-ALL-EXO.
           EXIT.
       
       KEY-EXO-EXO SECTION.
       INI-KEY-EXO-EXO.
*% INCLUDE INI_KEY_EXO_EXO OF 'COLBMEXO.TXT'
           MOVE EXO-COD-SIST IN EXO-FLD TO EXO-COD-SIST IN EXO.
           MOVE EXO-COD-PROC IN EXO-FLD TO EXO-COD-PROC IN EXO.
           MOVE EXO-CAI-IEXO IN EXO-FLD TO EXO-CAI-IEXO IN EXO.
           MOVE EXO-IIC-IEXO IN EXO-FLD TO EXO-IIC-IEXO IN EXO.
           MOVE EXO-NUM-IEXO IN EXO-FLD TO EXO-NUM-IEXO IN EXO.
*% INCLUDE FIN_KEY_EXO_EXO OF 'COLBMEXO.TXT'
       FIN-KEY-EXO-EXO.
           EXIT.
       
       SCR-CLRA-EXO SECTION.
       INI-SCR-CLRA-EXO.
*% INCLUDE INI_SCR_CLRA_EXO OF 'COLBMEXO.TXT'
           PERFORM CLR-KEY-EXO.
           PERFORM CLR-FLD-EXO.
*% INCLUDE FIN_SCR_CLRA_EXO OF 'COLBMEXO.TXT'
       FIN-SCR-CLRA-EXO.
           EXIT.
       
       CLR-KEY-EXO SECTION.
       INI-CLR-KEY-EXO.
*% INCLUDE INI_CLR_KEY_EXO OF 'COLBMEXO.TXT'
           MOVE SPACES TO EXO-COD-SIST IN EXO-FLD.
           MOVE SPACES TO EXO-COD-PROC IN EXO-FLD.
           MOVE SPACES TO EXO-CAI-IEXO IN EXO-FLD.
           MOVE ZEROES TO EXO-IIC-IEXO IN EXO-FLD.
           MOVE ZEROES TO EXO-NUM-IEXO IN EXO-FLD.
*% INCLUDE FIN_CLR_KEY_EXO OF 'COLBMEXO.TXT'
       FIN-CLR-KEY-EXO.
           EXIT.
       
       CLR-FLD-EXO SECTION.
       INI-CLR-FLD-EXO.
*% INCLUDE INI_CLR_FLD_EXO OF 'COLBMEXO.TXT'
           PERFORM CLR-EXO-EXO.
*% INCLUDE FIN_CLR_FLD_EXO OF 'COLBMEXO.TXT'
       FIN-CLR-FLD-EXO.
           EXIT.
       
       CLR-EXO-EXO SECTION.
       INI-CLR-EXO-EXO.
*% INCLUDE INI_CLR_EXO_EXO OF 'COLBMEXO.TXT'
           MOVE SPACES TO EXO-COD-TABL IN EXO-FLD.
           MOVE SPACES TO EXO-COD-USER IN EXO-FLD.
           MOVE SPACES TO EXO-GLS-CMTO IN EXO-FLD.
           MOVE SPACES TO EXO-MSC-STAT IN EXO-FLD.
           MOVE SPACES TO EXO-HRS-FINI IN EXO-FLD.
*% RREG = "EXO"
*% RREG_FRM = "EXO"
*% NOMB = "FINI"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO EXO-FEC-FINI IN EXO-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO EXO-HRS-FFIN IN EXO-FLD.
*% RREG = "EXO"
*% RREG_FRM = "EXO"
*% NOMB = "FFIN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO EXO-FEC-FFIN IN EXO-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_EXO_EXO OF 'COLBMEXO.TXT'
       FIN-CLR-EXO-EXO.
           EXIT.
       
       FST-KEY-EXO SECTION.
       INI-FST-KEY-EXO.
*% INCLUDE INI_FST_KEY_EXO OF 'COLBMEXO.TXT'
           MOVE -1 TO EXO-COD-PROC-LEN IN EXO-FLD.
*% INCLUDE FIN_FST_KEY_EXO OF 'COLBMEXO.TXT'
       FIN-FST-KEY-EXO.
           EXIT.
       
       FST-FLD-EXO SECTION.
       INI-FST-FLD-EXO.
*% INCLUDE INI_FST_FLD_EXO OF 'COLBMEXO.TXT'
           MOVE -1 TO EXO-COD-TABL-LEN IN EXO-FLD.
*% INCLUDE FIN_FST_FLD_EXO OF 'COLBMEXO.TXT'
       FIN-FST-FLD-EXO.
           EXIT.
       
       PRO-KEY-EXO SECTION.
       INI-PRO-KEY-EXO.
*% INCLUDE INI_PRO_KEY_EXO OF 'COLBMEXO.TXT'
           PERFORM PRO-IKY-EXO.
           PERFORM PRO-AKY-EXO.
           PERFORM PRO-MKY-EXO.
*% INCLUDE FIN_PRO_KEY_EXO OF 'COLBMEXO.TXT'
       FIN-PRO-KEY-EXO.
           EXIT.
       
       PRO-IKY-EXO SECTION.
       INI-PRO-IKY-EXO.
*% INCLUDE INI_PRO_IKY_EXO OF 'COLBMEXO.TXT'
           MOVE FRM-CPIM-AEY TO EXO-COD-PROC-ATR IN EXO-FLD.
           MOVE FRM-CPIM-AEY TO EXO-CAI-IEXO-ATR IN EXO-FLD.
           MOVE FRM-CPIM-AEY TO EXO-IIC-IEXO-ATR IN EXO-FLD.
           MOVE FRM-CPIM-AEY TO EXO-NUM-IEXO-ATR IN EXO-FLD.
*% INCLUDE FIN_PRO_IKY_EXO OF 'COLBMEXO.TXT'
       FIN-PRO-IKY-EXO.
           EXIT.
       
       PRO-MKY-EXO SECTION.
       INI-PRO-MKY-EXO.
*% INCLUDE INI_PRO_MKY_EXO OF 'COLBMEXO.TXT'
*% INCLUDE FIN_PRO_MKY_EXO OF 'COLBMEXO.TXT'
       FIN-PRO-MKY-EXO.
           EXIT.
       
       PRO-AKY-EXO SECTION.
       INI-PRO-AKY-EXO.
*% INCLUDE INI_PRO_AKY_EXO OF 'COLBMEXO.TXT'
*% INCLUDE FIN_PRO_AKY_EXO OF 'COLBMEXO.TXT'
       FIN-PRO-AKY-EXO.
           EXIT.
       
       UNP-KEY-EXO SECTION.
       INI-UNP-KEY-EXO.
*% INCLUDE INI_UNP_KEY_EXO OF 'COLBMEXO.TXT'
           PERFORM UNP-IKY-EXO.
           PERFORM UNP-AKY-EXO.
           PERFORM UNP-MKY-EXO.
*% INCLUDE FIN_UNP_KEY_EXO OF 'COLBMEXO.TXT'
       FIN-UNP-KEY-EXO.
           EXIT.
       
       UNP-IKY-EXO SECTION.
       INI-UNP-IKY-EXO.
*% INCLUDE INI_UNP_IKY_EXO OF 'COLBMEXO.TXT'
           MOVE FRM-CPIM-UBY TO EXO-COD-PROC-ATR.
           MOVE FRM-CPIM-UBY TO EXO-CAI-IEXO-ATR.
           MOVE FRM-CPIM-UBY TO EXO-IIC-IEXO-ATR.
           MOVE FRM-CPIM-UBY TO EXO-NUM-IEXO-ATR.
*% INCLUDE FIN_UNP_IKY_EXO OF 'COLBMEXO.TXT'
       FIN-UNP-IKY-EXO.
           EXIT.
       
       UNP-MKY-EXO SECTION.
       INI-UNP-MKY-EXO.
*% INCLUDE INI_UNP_MKY_EXO OF 'COLBMEXO.TXT'
*% INCLUDE FIN_UNP_MKY_EXO OF 'COLBMEXO.TXT'
       FIN-UNP-MKY-EXO.
           EXIT.
       
       UNP-AKY-EXO SECTION.
       INI-UNP-AKY-EXO.
*% INCLUDE INI_UNP_AKY_EXO OF 'COLBMEXO.TXT'
*% INCLUDE FIN_UNP_AKY_EXO OF 'COLBMEXO.TXT'
       FIN-UNP-AKY-EXO.
           EXIT.
       
       UNP-FLD-EXO SECTION.
       INI-UNP-FLD-EXO.
*% INCLUDE INI_UNP_FLD_EXO OF 'COLBMEXO.TXT'
           PERFORM UNP-EXO-EXO.
*% INCLUDE FIN_UNP_FLD_EXO OF 'COLBMEXO.TXT'
       FIN-UNP-FLD-EXO.
           EXIT.
       
       UNP-EXO-EXO SECTION.
       INI-UNP-EXO-EXO.
*% INCLUDE INI_UNP_EXO_EXO OF 'COLBMEXO.TXT'
           MOVE FRM-CPIM-UBY TO EXO-COD-TABL-ATR IN EXO-FLD.
           MOVE FRM-CPIM-UBY TO EXO-COD-USER-ATR IN EXO-FLD.
           MOVE FRM-CPIM-UBY TO EXO-GLS-CMTO-ATR IN EXO-FLD.
*% INCLUDE FIN_UNP_EXO_EXO OF 'COLBMEXO.TXT'
       FIN-UNP-EXO-EXO.
           EXIT.
       
       VAL-KEY-EXO SECTION.
       INI-VAL-KEY-EXO.
           PERFORM VAL-IKY-EXO.
           PERFORM VAL-MKY-EXO.
           PERFORM VAL-AKY-EXO.
       FIN-VAL-KEY-EXO.
           EXIT.
       
       VAL-IKY-EXO SECTION.
       INI-VAL-IKY-EXO.
*% INCLUDE INI_VAL_IKY_EXO OF 'COLBMEXO.TXT'
           PERFORM VAL-EXO-COD-SIST.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EXO-COD-SIST-LEN IN EXO-FLD
                GO TO FIN-VAL-IKY-EXO.
           PERFORM VAL-EXO-COD-PROC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EXO-COD-PROC-LEN IN EXO-FLD
                GO TO FIN-VAL-IKY-EXO.
           PERFORM VAL-EXO-CAI-IEXO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EXO-CAI-IEXO-LEN IN EXO-FLD
                GO TO FIN-VAL-IKY-EXO.
           PERFORM VAL-EXO-IIC-IEXO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EXO-IIC-IEXO-LEN IN EXO-FLD
                GO TO FIN-VAL-IKY-EXO.
           PERFORM VAL-EXO-NUM-IEXO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EXO-NUM-IEXO-LEN IN EXO-FLD
                GO TO FIN-VAL-IKY-EXO.
*% INCLUDE FIN_VAL_IKY_EXO OF 'COLBMEXO.TXT'
       FIN-VAL-IKY-EXO.
           EXIT.
       
       VAL-MKY-EXO SECTION.
       INI-VAL-MKY-EXO.
*% INCLUDE INI_VAL_MKY_EXO OF 'COLBMEXO.TXT'
*% INCLUDE FIN_VAL_MKY_EXO OF 'COLBMEXO.TXT'
       FIN-VAL-MKY-EXO.
           EXIT.
       
       VAL-AKY-EXO SECTION.
       INI-VAL-AKY-EXO.
*% INCLUDE INI_VAL_AKY_EXO OF 'COLBMEXO.TXT'
*% INCLUDE FIN_VAL_AKY_EXO OF 'COLBMEXO.TXT'
       FIN-VAL-AKY-EXO.
           EXIT.
       
       VAL-EXO-COD-SIST SECTION.
       INI-VAL-EXO-COD-SIST.
*% LBL = "EXO_COD_SIST OF 'COLBMEXO.TXT'"
*% INCLUDE INI_EXO_COD_SIST OF 'COLBMEXO.TXT'
*% INCLUDE EXO_COD_SIST OF 'COLBMEXO.TXT'
*% INCLUDE FIN_EXO_COD_SIST OF 'COLBMEXO.TXT'
       FIN-VAL-EXO-COD-SIST.
           EXIT.
       
       VAL-EXO-COD-PROC SECTION.
       INI-VAL-EXO-COD-PROC.
*% LBL = "EXO_COD_PROC OF 'COLBMEXO.TXT'"
*% INCLUDE INI_EXO_COD_PROC OF 'COLBMEXO.TXT'
           IF EXO-COD-PROC IN EXO-FLD NOT > SPACES
              MOVE SPACES TO EXO-COD-PROC IN EXO-FLD.
*% INCLUDE EXO_COD_PROC OF 'COLBMEXO.TXT'
*% INCLUDE FIN_EXO_COD_PROC OF 'COLBMEXO.TXT'
       FIN-VAL-EXO-COD-PROC.
           EXIT.
       
       VAL-EXO-CAI-IEXO SECTION.
       INI-VAL-EXO-CAI-IEXO.
*% LBL = "EXO_CAI_IEXO OF 'COLBMEXO.TXT'"
*% INCLUDE INI_EXO_CAI_IEXO OF 'COLBMEXO.TXT'
           IF EXO-CAI-IEXO IN EXO-FLD NOT > SPACES
              MOVE SPACES TO EXO-CAI-IEXO IN EXO-FLD.
*% INCLUDE EXO_CAI_IEXO OF 'COLBMEXO.TXT'
*% INCLUDE FIN_EXO_CAI_IEXO OF 'COLBMEXO.TXT'
       FIN-VAL-EXO-CAI-IEXO.
           EXIT.
       
       VAL-EXO-IIC-IEXO SECTION.
       INI-VAL-EXO-IIC-IEXO.
*% LBL = "EXO_IIC_IEXO OF 'COLBMEXO.TXT'"
*% INCLUDE INI_EXO_IIC_IEXO OF 'COLBMEXO.TXT'
*% INCLUDE EXO_IIC_IEXO OF 'COLBMEXO.TXT'
*% INCLUDE FIN_EXO_IIC_IEXO OF 'COLBMEXO.TXT'
       FIN-VAL-EXO-IIC-IEXO.
           EXIT.
       
       VAL-EXO-NUM-IEXO SECTION.
       INI-VAL-EXO-NUM-IEXO.
*% LBL = "EXO_NUM_IEXO OF 'COLBMEXO.TXT'"
*% INCLUDE INI_EXO_NUM_IEXO OF 'COLBMEXO.TXT'
*% INCLUDE EXO_NUM_IEXO OF 'COLBMEXO.TXT'
*% INCLUDE FIN_EXO_NUM_IEXO OF 'COLBMEXO.TXT'
       FIN-VAL-EXO-NUM-IEXO.
           EXIT.
*% RREG = "EXO"
*% RREG_FRM = "EXO"
*% NOMB = "FINI"
*% IZR = "S"
*% RREG = "EXO"
*% RREG_FRM = "EXO"
*% NOMB = "FFIN"
*% IZR = "S"
       
       VAL-CON-KEY-EXO SECTION.
       INI-VAL-CON-KEY-EXO.
*% INCLUDE INI_VAL_CON_KEY_EXO OF 'COLBMEXO.TXT'
           IF     EXO-COD-SIST IN EXO-FLD NOT > SPACES 
              AND EXO-COD-PROC IN EXO-FLD NOT > SPACES 
              AND EXO-CAI-IEXO IN EXO-FLD NOT > SPACES 
              AND EXO-IIC-IEXO IN EXO-FLD NOT > ZEROES 
              AND EXO-NUM-IEXO IN EXO-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-EXO.
*% INCLUDE FIN_VAL_CON_KEY_EXO OF 'COLBMEXO.TXT'
       FIN-VAL-CON-KEY-EXO.
           EXIT.
       
       VAL-CON-IKY-EXO SECTION.
       INI-VAL-CON-IKY-EXO.
*% INCLUDE INI_VAL_CON_IKY_EXO OF 'COLBMEXO.TXT'
           IF     EXO-COD-SIST IN EXO-FLD NOT > SPACES 
              AND EXO-COD-PROC IN EXO-FLD NOT > SPACES 
              AND EXO-CAI-IEXO IN EXO-FLD NOT > SPACES 
              AND EXO-IIC-IEXO IN EXO-FLD NOT > ZEROES 
              AND EXO-NUM-IEXO IN EXO-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-EXO.
*% INCLUDE FIN_VAL_CON_IKY_EXO OF 'COLBMEXO.TXT'
       FIN-VAL-CON-IKY-EXO.
           EXIT.
       
       VAL-NUM-KEY-EXO SECTION.
       INI-VAL-NUM-KEY-EXO.
           PERFORM VAL-NUM-IKY-EXO.
           PERFORM VAL-NUM-MKY-EXO.
           PERFORM VAL-NUM-AKY-EXO.
       FIN-VAL-NUM-KEY-EXO.
           EXIT.
       
       VAL-NUM-IKY-EXO SECTION.
       INI-VAL-NUM-IKY-EXO.
*% INCLUDE INI_VAL_NUM_IKY_EXO OF 'COLBMEXO.TXT'
       VAL-NUM-EXO-IIC-IEXO.
*% BUF = "EXO-FLD"
*% FLD = "EXO-IIC-IEXO"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-EXO.
       VAL-NUM-EXO-NUM-IEXO.
*% BUF = "EXO-FLD"
*% FLD = "EXO-NUM-IEXO"
*% ENT = "3"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-EXO.
*% INCLUDE FIN_VAL_NUM_IKY_EXO OF 'COLBMEXO.TXT'
       FIN-VAL-NUM-IKY-EXO.
           EXIT.
       
       VAL-NUM-MKY-EXO SECTION.
       INI-VAL-NUM-MKY-EXO.
*% INCLUDE INI_VAL_NUM_MKY_EXO OF 'COLBMEXO.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_EXO OF 'COLBMEXO.TXT'
       FIN-VAL-NUM-MKY-EXO.
           EXIT.
       
       VAL-NUM-AKY-EXO SECTION.
       INI-VAL-NUM-AKY-EXO.
*% INCLUDE INI_VAL_NUM_AKY_EXO OF 'COLBMEXO.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_EXO OF 'COLBMEXO.TXT'
       FIN-VAL-NUM-AKY-EXO.
           EXIT.
       
       VAL-NUM-FLD-EXO SECTION.
       INI-VAL-NUM-FLD-EXO.
*% INCLUDE INI_VAL_NUM_FLD_EXO OF 'COLBMEXO.TXT'
*% INCLUDE FIN_VAL_NUM_FLD_EXO OF 'COLBMEXO.TXT'
       FIN-VAL-NUM-FLD-EXO.
           EXIT.
       
       EDT-KEY-EXO SECTION.
       INI-EDT-KEY-EXO.
           PERFORM EDT-IKY-EXO.
           PERFORM EDT-MKY-EXO.
           PERFORM EDT-AKY-EXO.
*% INCLUDE EDT_KEY_EXO OF 'COLBMEXO.TXT'
       FIN-EDT-KEY-EXO.
           EXIT.
       
       EDT-IKY-EXO SECTION.
       INI-EDT-IKY-EXO.
*% INCLUDE EDT_IKY_EXO OF 'COLBMEXO.TXT'
           IF EXO-IIC-IEXO-LEN IN EXO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EXO-IIC-IEXO-LEN IN EXO-FLD
                GO TO FIN-EDT-IKY-EXO.
           MOVE EXO-IIC-IEXO IN EXO-FLD TO EXO-IIC-IEXO-EDT IN EXO-EDT.
           MOVE EXO-IIC-IEXO-EDT IN EXO-EDT TO 
                EXO-IIC-IEXO-ALF IN EXO-FLD.
           IF EXO-NUM-IEXO-LEN IN EXO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EXO-NUM-IEXO-LEN IN EXO-FLD
                GO TO FIN-EDT-IKY-EXO.
           MOVE EXO-NUM-IEXO IN EXO-FLD TO EXO-NUM-IEXO-EDT IN EXO-EDT.
           MOVE EXO-NUM-IEXO-EDT IN EXO-EDT TO 
                EXO-NUM-IEXO-ALF IN EXO-FLD.
*% INCLUDE FIN_EDT_IKY_EXO OF 'COLBMEXO.TXT'
       FIN-EDT-IKY-EXO.
           EXIT.
       
       EDT-MKY-EXO SECTION.
       INI-EDT-MKY-EXO.
*% INCLUDE EDT_MKY_EXO OF 'COLBMEXO.TXT'
*% INCLUDE FIN_EDT_MKY_EXO OF 'COLBMEXO.TXT'
       FIN-EDT-MKY-EXO.
           EXIT.
       
       EDT-AKY-EXO SECTION.
       INI-EDT-AKY-EXO.
*% INCLUDE EDT_AKY_EXO OF 'COLBMEXO.TXT'
*% INCLUDE FIN_EDT_AKY_EXO OF 'COLBMEXO.TXT'
       FIN-EDT-AKY-EXO.
           EXIT.
       
       EDT-FLD-EXO SECTION.
       INI-EDT-FLD-EXO.
*% INCLUDE EDT_FLD_EXO OF 'COLBMEXO.TXT'
*% RREG = "EXO"
*% RREG_FRM = "EXO"
*% NOMB = "FINI"
*% RREG = "EXO"
*% RREG_FRM = "EXO"
*% NOMB = "FFIN"
*% INCLUDE FIN_EDT_FLD_EXO OF 'COLBMEXO.TXT'
