       EDT-KEY-DBP SECTION.
       INI-EDT-KEY-DBP.
           PERFORM EDT-IKY-DBP.
           PERFORM EDT-MKY-DBP.
           PERFORM EDT-AKY-DBP.
*% INCLUDE EDT_KEY_DBP OF 'COLBMDBP.TXT'
       FIN-EDT-KEY-DBP.
           EXIT.
       
       EDT-IKY-DBP SECTION.
       INI-EDT-IKY-DBP.
*% INCLUDE EDT_IKY_DBP OF 'COLBMDBP.TXT'
           IF DBC-NUM-ICLI-LEN IN DBP-FLD = -9
                MOVE -1 TO DBC-NUM-ICLI-LEN IN DBP-FLD
                GO TO FIN-EDT-IKY-DBP.
           MOVE DBC-NUM-ICLI IN DBP-FLD TO DBC-NUM-ICLI-EDT IN DBP-EDT.
           MOVE DBC-NUM-ICLI-EDT IN DBP-EDT TO 
                DBC-NUM-ICLI-ALF IN DBP-FLD.
*% INCLUDE FIN_EDT_IKY_DBP OF 'COLBMDBP.TXT'
       FIN-EDT-IKY-DBP.
           EXIT.
       
       EDT-MKY-DBP SECTION.
       INI-EDT-MKY-DBP.
*% INCLUDE EDT_MKY_DBP OF 'COLBMDBP.TXT'
*% INCLUDE FIN_EDT_MKY_DBP OF 'COLBMDBP.TXT'
       FIN-EDT-MKY-DBP.
           EXIT.
       
       EDT-AKY-DBP SECTION.
       INI-EDT-AKY-DBP.
*% INCLUDE EDT_AKY_DBP OF 'COLBMDBP.TXT'
*% INCLUDE FIN_EDT_AKY_DBP OF 'COLBMDBP.TXT'
       FIN-EDT-AKY-DBP.
           EXIT.
       
       EDT-FLD-DBP SECTION.
       INI-EDT-FLD-DBP.
*% INCLUDE EDT_FLD_DBP OF 'COLBMDBP.TXT'
           IF FRM-IND-WMNP-LEN IN DBP-FLD = -9
                MOVE -1 TO FRM-IND-WMNP-LEN IN DBP-FLD
                GO TO FIN-EDT-FLD-DBP.
           MOVE FRM-IND-WMNP IN DBP-FLD TO FRM-IND-WMNP-EDT IN DBP-EDT.
           MOVE FRM-IND-WMNP-EDT IN DBP-EDT TO 
                FRM-IND-WMNP-ALF IN DBP-FLD.
           IF DBC-NUM-TDIR-LEN IN DBP-FLD = -9
                MOVE -1 TO DBC-NUM-TDIR-LEN IN DBP-FLD
                GO TO FIN-EDT-FLD-DBP.
           MOVE DBC-NUM-TDIR IN DBP-FLD TO DBC-NUM-TDIR-EDT IN DBP-EDT.
           MOVE DBC-NUM-TDIR-EDT IN DBP-EDT TO 
                DBC-NUM-TDIR-ALF IN DBP-FLD.
           IF DBC-NUM-DCOR-LEN IN DBP-FLD = -9
                MOVE -1 TO DBC-NUM-DCOR-LEN IN DBP-FLD
                GO TO FIN-EDT-FLD-DBP.
           MOVE DBC-NUM-DCOR IN DBP-FLD TO DBC-NUM-DCOR-EDT IN DBP-EDT.
           MOVE DBC-NUM-DCOR-EDT IN DBP-EDT TO 
                DBC-NUM-DCOR-ALF IN DBP-FLD.
           IF DCP-NUM-CIDE-LEN IN DBP-FLD = -9
                MOVE -1 TO DCP-NUM-CIDE-LEN IN DBP-FLD
                GO TO FIN-EDT-FLD-DBP.
           MOVE DCP-NUM-CIDE IN DBP-FLD TO DCP-NUM-CIDE-EDT IN DBP-EDT.
           MOVE DCP-NUM-CIDE-EDT IN DBP-EDT TO 
                DCP-NUM-CIDE-ALF IN DBP-FLD.
           IF AEP-VAL-RLCL-LEN IN DBP-FLD = -9
                MOVE -1 TO AEP-VAL-RLCL-LEN IN DBP-FLD
                GO TO FIN-EDT-FLD-DBP.
           MOVE AEP-VAL-RLCL IN DBP-FLD TO AEP-VAL-RLCL-EDT IN DBP-EDT.
           MOVE AEP-VAL-RLCL-EDT IN DBP-EDT TO 
                AEP-VAL-RLCL-ALF IN DBP-FLD.
           IF DCP-NUM-NMSG-LEN IN DBP-FLD = -9
                MOVE -1 TO DCP-NUM-NMSG-LEN IN DBP-FLD
                GO TO FIN-EDT-FLD-DBP.
           MOVE DCP-NUM-NMSG IN DBP-FLD TO DCP-NUM-NMSG-EDT IN DBP-EDT.
           MOVE DCP-NUM-NMSG-EDT IN DBP-EDT TO 
                DCP-NUM-NMSG-ALF IN DBP-FLD.
*% INCLUDE FIN_EDT_FLD_DBP OF 'COLBMDBP.TXT'
       FIN-EDT-FLD-DBP.
           EXIT.
