*% BUF = "DBP-FLD"
*% FLD = "DBC-NUM-TDIR"
*% ENT = "2"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-DBP.
       VAL-NUM-DBC-NUM-DCOR.
*% BUF = "DBP-FLD"
*% FLD = "DBC-NUM-DCOR"
*% ENT = "2"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-DBP.
       VAL-NUM-DCP-NUM-CIDE.
