       VAL-KEY-CP2 SECTION.
       INI-VAL-KEY-CP2.
           PERFORM VAL-IKY-CP2.
           PERFORM VAL-MKY-CP2.
           PERFORM VAL-AKY-CP2.
       FIN-VAL-KEY-CP2.
           EXIT.
       
       VAL-IKY-CP2 SECTION.
       INI-VAL-IKY-CP2.
*% INCLUDE INI_VAL_IKY_CP2 OF 'COLBMCP2.TXT'
           PERFORM VAL-FRM-COD-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-VCOC-LEN IN CP2-FLD
                GO TO FIN-VAL-IKY-CP2.
           PERFORM VAL-FRM-COD-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-TOOC-LEN IN CP2-FLD
                GO TO FIN-VAL-IKY-CP2.
           PERFORM VAL-FRM-COD-AOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-AOOC-LEN IN CP2-FLD
                GO TO FIN-VAL-IKY-CP2.
           PERFORM VAL-FRM-COD-MVTO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-MVTO-LEN IN CP2-FLD
                GO TO FIN-VAL-IKY-CP2.
           PERFORM VAL-FRM-IND-SCRT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IND-SCRT-LEN IN CP2-FLD
                GO TO FIN-VAL-IKY-CP2.
           PERFORM VAL-FRM-IND-SCTB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IND-SCTB-LEN IN CP2-FLD
                GO TO FIN-VAL-IKY-CP2.
*% INCLUDE FIN_VAL_IKY_CP2 OF 'COLBMCP2.TXT'
       FIN-VAL-IKY-CP2.
           EXIT.
       
