*% INCLUDE SETTAG OF 'COLBMCP2.TXT'
       SET-TAG-COLBQCP2 SECTION.
       INI-SET-TAG-COLBQCP2.
           MOVE
           '<IDNMOD NOM=$COLBQCP2$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:29 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCP2.
           EXIT.
           
       PUT-ALL-CP2 SECTION.
       INI-PUT-ALL-CP2.
*% INCLUDE INI_PUT_ALL_CP2 OF 'COLBMCP2.TXT'
