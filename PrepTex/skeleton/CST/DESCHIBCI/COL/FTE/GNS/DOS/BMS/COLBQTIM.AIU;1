      *
      * COLBQTIM
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:58 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "TIM"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMTIM.TXT'
       SET-TAG-COLBQTIM SECTION.
       INI-SET-TAG-COLBQTIM.
           MOVE
           '<IDNMOD NOM=$COLBQTIM$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:58 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQTIM.
           EXIT.
           
       PUT-ALL-TIM SECTION.
       INI-PUT-ALL-TIM.
*% INCLUDE INI_PUT_ALL_TIM OF 'COLBMTIM.TXT'
           PERFORM PUT-TIM-TIM.
*% INCLUDE FIN_PUT_ALL_TIM OF 'COLBMTIM.TXT'
       FIN-PUT-ALL-TIM.
           EXIT.
       
       PUT-TIM-TIM SECTION.
       INI-PUT-TIM-TIM.
*% INCLUDE INI_PUT_TIM_TIM OF 'COLBMTIM.TXT'
           IF PGM-STAT-TIM-NUL
               GO TO FIN-PUT-TIM-TIM.
           IF NOT PGM-STAT-TIM-OKS
               PERFORM CLR-TIM-TIM
               GO TO FIN-PUT-TIM-TIM.
           MOVE TIM-COD-CTAB IN TIM TO TIM-COD-CTAB IN TIM-FLD.
           MOVE TIM-CIC-CTAB IN TIM TO TIM-CIC-CTAB IN TIM-FLD.
           MOVE TIM-EXT-CTAB IN TIM TO TIM-EXT-CTAB IN TIM-FLD.
           MOVE TIM-GLS-DESC IN TIM TO TIM-GLS-DESC IN TIM-FLD.
           MOVE TIM-GLS-ABRV IN TIM TO TIM-GLS-ABRV IN TIM-FLD.
           MOVE TIM-GLS-DCOR IN TIM TO TIM-GLS-DCOR IN TIM-FLD.
           MOVE TIM-IND-VIGE IN TIM TO TIM-IND-VIGE IN TIM-FLD.
           MOVE TIM-SGV-TIM1 IN TIM TO TIM-SGV-TIM1 IN TIM-FLD.
           MOVE TIM-SGV-TIM2 IN TIM TO TIM-SGV-TIM2 IN TIM-FLD.
*% INCLUDE FIN_PUT_TIM_TIM OF 'COLBMTIM.TXT'
       FIN-PUT-TIM-TIM.
           EXIT.
       
       KEY-ALL-TIM SECTION.
       INI-KEY-ALL-TIM.
*% INCLUDE INI_KEY_ALL_TIM OF 'COLBMTIM.TXT'
           PERFORM KEY-TIM-TIM.
*% INCLUDE FIN_KEY_ALL_TIM OF 'COLBMTIM.TXT'
       FIN-KEY-ALL-TIM.
           EXIT.
       
       KEY-TIM-TIM SECTION.
       INI-KEY-TIM-TIM.
*% INCLUDE INI_KEY_TIM_TIM OF 'COLBMTIM.TXT'
           MOVE TIM-COD-CTAB IN TIM-FLD TO TIM-COD-CTAB IN TIM.
*% INCLUDE FIN_KEY_TIM_TIM OF 'COLBMTIM.TXT'
       FIN-KEY-TIM-TIM.
           EXIT.
       
       SCR-CLRA-TIM SECTION.
       INI-SCR-CLRA-TIM.
*% INCLUDE INI_SCR_CLRA_TIM OF 'COLBMTIM.TXT'
           PERFORM CLR-KEY-TIM.
           PERFORM CLR-FLD-TIM.
*% INCLUDE FIN_SCR_CLRA_TIM OF 'COLBMTIM.TXT'
       FIN-SCR-CLRA-TIM.
           EXIT.
       
       CLR-KEY-TIM SECTION.
       INI-CLR-KEY-TIM.
*% INCLUDE INI_CLR_KEY_TIM OF 'COLBMTIM.TXT'
           MOVE SPACES TO TIM-COD-CTAB IN TIM-FLD.
*% INCLUDE FIN_CLR_KEY_TIM OF 'COLBMTIM.TXT'
       FIN-CLR-KEY-TIM.
           EXIT.
       
       CLR-FLD-TIM SECTION.
       INI-CLR-FLD-TIM.
*% INCLUDE INI_CLR_FLD_TIM OF 'COLBMTIM.TXT'
           PERFORM CLR-TIM-TIM.
*% INCLUDE FIN_CLR_FLD_TIM OF 'COLBMTIM.TXT'
       FIN-CLR-FLD-TIM.
           EXIT.
       
       CLR-TIM-TIM SECTION.
       INI-CLR-TIM-TIM.
*% INCLUDE INI_CLR_TIM_TIM OF 'COLBMTIM.TXT'
           MOVE SPACES TO TIM-CIC-CTAB IN TIM-FLD.
           MOVE SPACES TO TIM-EXT-CTAB IN TIM-FLD.
           MOVE SPACES TO TIM-GLS-DESC IN TIM-FLD.
           MOVE SPACES TO TIM-GLS-ABRV IN TIM-FLD.
           MOVE SPACES TO TIM-GLS-DCOR IN TIM-FLD.
           MOVE SPACES TO TIM-IND-VIGE IN TIM-FLD.
           MOVE ZEROES TO TIM-SGV-TIM1 IN TIM-FLD.
           MOVE ZEROES TO TIM-SGV-TIM2 IN TIM-FLD.
*% INCLUDE FIN_CLR_TIM_TIM OF 'COLBMTIM.TXT'
       FIN-CLR-TIM-TIM.
           EXIT.
       
       FST-KEY-TIM SECTION.
       INI-FST-KEY-TIM.
*% INCLUDE INI_FST_KEY_TIM OF 'COLBMTIM.TXT'
           MOVE -1 TO TIM-COD-CTAB-LEN IN TIM-FLD.
*% INCLUDE FIN_FST_KEY_TIM OF 'COLBMTIM.TXT'
       FIN-FST-KEY-TIM.
           EXIT.
       
       FST-FLD-TIM SECTION.
       INI-FST-FLD-TIM.
*% INCLUDE INI_FST_FLD_TIM OF 'COLBMTIM.TXT'
           MOVE -1 TO TIM-CIC-CTAB-LEN IN TIM-FLD.
*% INCLUDE FIN_FST_FLD_TIM OF 'COLBMTIM.TXT'
       FIN-FST-FLD-TIM.
           EXIT.
       
       PRO-KEY-TIM SECTION.
       INI-PRO-KEY-TIM.
*% INCLUDE INI_PRO_KEY_TIM OF 'COLBMTIM.TXT'
           PERFORM PRO-IKY-TIM.
           PERFORM PRO-AKY-TIM.
           PERFORM PRO-MKY-TIM.
*% INCLUDE FIN_PRO_KEY_TIM OF 'COLBMTIM.TXT'
       FIN-PRO-KEY-TIM.
           EXIT.
       
       PRO-IKY-TIM SECTION.
       INI-PRO-IKY-TIM.
*% INCLUDE INI_PRO_IKY_TIM OF 'COLBMTIM.TXT'
           MOVE FRM-CPIM-AEY TO TIM-COD-CTAB-ATR IN TIM-FLD.
*% INCLUDE FIN_PRO_IKY_TIM OF 'COLBMTIM.TXT'
       FIN-PRO-IKY-TIM.
           EXIT.
       
       PRO-MKY-TIM SECTION.
       INI-PRO-MKY-TIM.
*% INCLUDE INI_PRO_MKY_TIM OF 'COLBMTIM.TXT'
*% INCLUDE FIN_PRO_MKY_TIM OF 'COLBMTIM.TXT'
       FIN-PRO-MKY-TIM.
           EXIT.
       
       PRO-AKY-TIM SECTION.
       INI-PRO-AKY-TIM.
*% INCLUDE INI_PRO_AKY_TIM OF 'COLBMTIM.TXT'
*% INCLUDE FIN_PRO_AKY_TIM OF 'COLBMTIM.TXT'
       FIN-PRO-AKY-TIM.
           EXIT.
       
       UNP-KEY-TIM SECTION.
       INI-UNP-KEY-TIM.
*% INCLUDE INI_UNP_KEY_TIM OF 'COLBMTIM.TXT'
           PERFORM UNP-IKY-TIM.
           PERFORM UNP-AKY-TIM.
           PERFORM UNP-MKY-TIM.
*% INCLUDE FIN_UNP_KEY_TIM OF 'COLBMTIM.TXT'
       FIN-UNP-KEY-TIM.
           EXIT.
       
       UNP-IKY-TIM SECTION.
       INI-UNP-IKY-TIM.
*% INCLUDE INI_UNP_IKY_TIM OF 'COLBMTIM.TXT'
           MOVE FRM-CPIM-UBY TO TIM-COD-CTAB-ATR.
*% INCLUDE FIN_UNP_IKY_TIM OF 'COLBMTIM.TXT'
       FIN-UNP-IKY-TIM.
           EXIT.
       
       UNP-MKY-TIM SECTION.
       INI-UNP-MKY-TIM.
*% INCLUDE INI_UNP_MKY_TIM OF 'COLBMTIM.TXT'
*% INCLUDE FIN_UNP_MKY_TIM OF 'COLBMTIM.TXT'
       FIN-UNP-MKY-TIM.
           EXIT.
       
       UNP-AKY-TIM SECTION.
       INI-UNP-AKY-TIM.
*% INCLUDE INI_UNP_AKY_TIM OF 'COLBMTIM.TXT'
*% INCLUDE FIN_UNP_AKY_TIM OF 'COLBMTIM.TXT'
       FIN-UNP-AKY-TIM.
           EXIT.
       
       UNP-FLD-TIM SECTION.
       INI-UNP-FLD-TIM.
*% INCLUDE INI_UNP_FLD_TIM OF 'COLBMTIM.TXT'
           PERFORM UNP-TIM-TIM.
*% INCLUDE FIN_UNP_FLD_TIM OF 'COLBMTIM.TXT'
       FIN-UNP-FLD-TIM.
           EXIT.
       
       UNP-TIM-TIM SECTION.
       INI-UNP-TIM-TIM.
*% INCLUDE INI_UNP_TIM_TIM OF 'COLBMTIM.TXT'
           MOVE FRM-CPIM-UBY TO TIM-CIC-CTAB-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-EXT-CTAB-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-GLS-DESC-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-GLS-ABRV-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-GLS-DCOR-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-IND-VIGE-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-SGV-TIM1-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-SGV-TIM2-ATR IN TIM-FLD.
*% INCLUDE FIN_UNP_TIM_TIM OF 'COLBMTIM.TXT'
       FIN-UNP-TIM-TIM.
           EXIT.
       
       VAL-KEY-TIM SECTION.
       INI-VAL-KEY-TIM.
           PERFORM VAL-IKY-TIM.
           PERFORM VAL-MKY-TIM.
           PERFORM VAL-AKY-TIM.
       FIN-VAL-KEY-TIM.
           EXIT.
       
       VAL-IKY-TIM SECTION.
       INI-VAL-IKY-TIM.
*% INCLUDE INI_VAL_IKY_TIM OF 'COLBMTIM.TXT'
           PERFORM VAL-TIM-COD-CTAB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TIM-COD-CTAB-LEN IN TIM-FLD
                GO TO FIN-VAL-IKY-TIM.
*% INCLUDE FIN_VAL_IKY_TIM OF 'COLBMTIM.TXT'
       FIN-VAL-IKY-TIM.
           EXIT.
       
       VAL-MKY-TIM SECTION.
       INI-VAL-MKY-TIM.
*% INCLUDE INI_VAL_MKY_TIM OF 'COLBMTIM.TXT'
*% INCLUDE FIN_VAL_MKY_TIM OF 'COLBMTIM.TXT'
       FIN-VAL-MKY-TIM.
           EXIT.
       
       VAL-AKY-TIM SECTION.
       INI-VAL-AKY-TIM.
*% INCLUDE INI_VAL_AKY_TIM OF 'COLBMTIM.TXT'
*% INCLUDE FIN_VAL_AKY_TIM OF 'COLBMTIM.TXT'
       FIN-VAL-AKY-TIM.
           EXIT.
       
       VAL-TIM-COD-CTAB SECTION.
       INI-VAL-TIM-COD-CTAB.
*% LBL = "TIM_COD_CTAB OF 'COLBMTIM.TXT'"
*% INCLUDE INI_TIM_COD_CTAB OF 'COLBMTIM.TXT'
           IF TIM-COD-CTAB IN TIM-FLD NOT > SPACES
              MOVE SPACES TO TIM-COD-CTAB IN TIM-FLD.
*% INCLUDE TIM_COD_CTAB OF 'COLBMTIM.TXT'
*% INCLUDE FIN_TIM_COD_CTAB OF 'COLBMTIM.TXT'
       FIN-VAL-TIM-COD-CTAB.
           EXIT.
       
       VAL-CON-KEY-TIM SECTION.
       INI-VAL-CON-KEY-TIM.
*% INCLUDE INI_VAL_CON_KEY_TIM OF 'COLBMTIM.TXT'
           IF     TIM-COD-CTAB IN TIM-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-TIM.
*% INCLUDE FIN_VAL_CON_KEY_TIM OF 'COLBMTIM.TXT'
       FIN-VAL-CON-KEY-TIM.
           EXIT.
       
       VAL-CON-IKY-TIM SECTION.
       INI-VAL-CON-IKY-TIM.
*% INCLUDE INI_VAL_CON_IKY_TIM OF 'COLBMTIM.TXT'
           IF     TIM-COD-CTAB IN TIM-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-TIM.
*% INCLUDE FIN_VAL_CON_IKY_TIM OF 'COLBMTIM.TXT'
       FIN-VAL-CON-IKY-TIM.
           EXIT.
       
       VAL-NUM-KEY-TIM SECTION.
       INI-VAL-NUM-KEY-TIM.
           PERFORM VAL-NUM-IKY-TIM.
           PERFORM VAL-NUM-MKY-TIM.
           PERFORM VAL-NUM-AKY-TIM.
       FIN-VAL-NUM-KEY-TIM.
           EXIT.
       
       VAL-NUM-IKY-TIM SECTION.
       INI-VAL-NUM-IKY-TIM.
*% INCLUDE INI_VAL_NUM_IKY_TIM OF 'COLBMTIM.TXT'
*% INCLUDE FIN_VAL_NUM_IKY_TIM OF 'COLBMTIM.TXT'
       FIN-VAL-NUM-IKY-TIM.
           EXIT.
       
       VAL-NUM-MKY-TIM SECTION.
       INI-VAL-NUM-MKY-TIM.
*% INCLUDE INI_VAL_NUM_MKY_TIM OF 'COLBMTIM.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_TIM OF 'COLBMTIM.TXT'
       FIN-VAL-NUM-MKY-TIM.
           EXIT.
       
       VAL-NUM-AKY-TIM SECTION.
       INI-VAL-NUM-AKY-TIM.
*% INCLUDE INI_VAL_NUM_AKY_TIM OF 'COLBMTIM.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_TIM OF 'COLBMTIM.TXT'
       FIN-VAL-NUM-AKY-TIM.
           EXIT.
       
       VAL-NUM-FLD-TIM SECTION.
       INI-VAL-NUM-FLD-TIM.
*% INCLUDE INI_VAL_NUM_FLD_TIM OF 'COLBMTIM.TXT'
       VAL-NUM-TIM-SGV-TIM1.
*% BUF = "TIM-FLD"
*% FLD = "TIM-SGV-TIM1"
*% ENT = "3"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-TIM.
       VAL-NUM-TIM-SGV-TIM2.
*% BUF = "TIM-FLD"
*% FLD = "TIM-SGV-TIM2"
*% ENT = "7"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-TIM.
*% INCLUDE FIN_VAL_NUM_FLD_TIM OF 'COLBMTIM.TXT'
       FIN-VAL-NUM-FLD-TIM.
           EXIT.
       
       EDT-KEY-TIM SECTION.
       INI-EDT-KEY-TIM.
           PERFORM EDT-IKY-TIM.
           PERFORM EDT-MKY-TIM.
           PERFORM EDT-AKY-TIM.
*% INCLUDE EDT_KEY_TIM OF 'COLBMTIM.TXT'
       FIN-EDT-KEY-TIM.
           EXIT.
       
       EDT-IKY-TIM SECTION.
       INI-EDT-IKY-TIM.
*% INCLUDE EDT_IKY_TIM OF 'COLBMTIM.TXT'
*% INCLUDE FIN_EDT_IKY_TIM OF 'COLBMTIM.TXT'
       FIN-EDT-IKY-TIM.
           EXIT.
       
       EDT-MKY-TIM SECTION.
       INI-EDT-MKY-TIM.
*% INCLUDE EDT_MKY_TIM OF 'COLBMTIM.TXT'
*% INCLUDE FIN_EDT_MKY_TIM OF 'COLBMTIM.TXT'
       FIN-EDT-MKY-TIM.
           EXIT.
       
       EDT-AKY-TIM SECTION.
       INI-EDT-AKY-TIM.
*% INCLUDE EDT_AKY_TIM OF 'COLBMTIM.TXT'
*% INCLUDE FIN_EDT_AKY_TIM OF 'COLBMTIM.TXT'
       FIN-EDT-AKY-TIM.
           EXIT.
       
       EDT-FLD-TIM SECTION.
       INI-EDT-FLD-TIM.
*% INCLUDE EDT_FLD_TIM OF 'COLBMTIM.TXT'
           IF TIM-SGV-TIM1-LEN IN TIM-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO TIM-SGV-TIM1-LEN IN TIM-FLD
                GO TO FIN-EDT-FLD-TIM.
           MOVE TIM-SGV-TIM1 IN TIM-FLD TO TIM-SGV-TIM1-EDT IN TIM-EDT.
           MOVE TIM-SGV-TIM1-EDT IN TIM-EDT TO 
                TIM-SGV-TIM1-ALF IN TIM-FLD.
           IF TIM-SGV-TIM2-LEN IN TIM-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO TIM-SGV-TIM2-LEN IN TIM-FLD
                GO TO FIN-EDT-FLD-TIM.
           MOVE TIM-SGV-TIM2 IN TIM-FLD TO TIM-SGV-TIM2-EDT IN TIM-EDT.
           MOVE TIM-SGV-TIM2-EDT IN TIM-EDT TO 
                TIM-SGV-TIM2-ALF IN TIM-FLD.
*% INCLUDE FIN_EDT_FLD_TIM OF 'COLBMTIM.TXT'
       FIN-EDT-FLD-TIM.
           EXIT.
*% INCLUDE EOF_BQ OF 'COLBMTIM.TXT'
*% INCLUDE EOF OF 'COLBMTIM.TXT'
*% INCLUDE EOF_IBM OF 'COLBMTIM.TXT'
