*% FRM = "CNO"
      *
      * COLBUCNO
      * Version 3.0.1 Nov-2005
      * 24-May-19 06:14 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMCNO.TXT'
       SET-TAG-COLBUCNO SECTION.
       INI-SET-TAG-COLBUCNO.
           MOVE
           '<IDNMOD NOM=$COLBUCNO$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$24-May-19 06:14 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCNO.
           EXIT.
           
       RET-ALL-CNO SECTION.
       INI-RET-ALL-CNO.
*% INCLUDE INI_RET_ALL_CNO OF 'COLBMCNO.TXT'
           PERFORM KEY-ALL-CNO.
           PERFORM RET-DBC-CNO.
           PERFORM RET-NGA-CNO.
*% INCLUDE FIN_RET_ALL_CNO OF 'COLBMCNO.TXT'
       FIN-RET-ALL-CNO.
           EXIT.
       
       RET-DBC-CNO SECTION.
       INI-RET-DBC-CNO.
*% INCLUDE INI_RET_DBC_CNO OF 'COLBMCNO.TXT'
*% INCLUDE FIN_RET_DBC_CNO OF 'COLBMCNO.TXT'
       FIN-RET-DBC-CNO.
           EXIT.
       
       RET-NGA-CNO SECTION.
       INI-RET-NGA-CNO.
*% INCLUDE INI_RET_NGA_CNO OF 'COLBMCNO.TXT'
*% RREG = "NGA"
*% RREG_FRM = "NGA"
*% NOMB = "AUX1"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE NGA-NUM-DUX1 IN CNO-FLD TO NGA-NUM-DUX1 IN NGA.
           MOVE NGA-NUM-MUX1 IN CNO-FLD TO NGA-NUM-MUX1 IN NGA.
           MOVE NGA-NUM-SUX1 IN CNO-FLD TO NGA-NUM-SUX1 IN NGA.
           MOVE NGA-NUM-AUX1 IN CNO-FLD TO NGA-NUM-AUX1 IN NGA.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE NGA-SGV-MON8 IN CNO-FLD TO NGA-SGV-MON8 IN NGA.
           MOVE NGA-SGV-MON1 IN CNO-FLD TO NGA-SGV-MON1 IN NGA.
*% INCLUDE FIN_RET_NGA_CNO OF 'COLBMCNO.TXT'
       FIN-RET-NGA-CNO.
           EXIT.
       
       KEY-ALL-CNO SECTION.
       INI-KEY-ALL-CNO.
*% INCLUDE INI_KEY_ALL_CNO OF 'COLBMCNO.TXT'
           PERFORM KEY-DBC-CNO.
           PERFORM KEY-NGA-CNO.
*% INCLUDE FIN_KEY_ALL_CNO OF 'COLBMCNO.TXT'
       FIN-KEY-ALL-CNO.
           EXIT.
       
       KEY-DBC-CNO SECTION.
       INI-KEY-DBC-CNO.
*% INCLUDE INI_KEY_DBC_CNO OF 'COLBMCNO.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN CNO-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN CNO-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN CNO-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN CNO-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_KEY_DBC_CNO OF 'COLBMCNO.TXT'
       FIN-KEY-DBC-CNO.
           EXIT.
       
       KEY-NGA-CNO SECTION.
       INI-KEY-NGA-CNO.
*% INCLUDE INI_KEY_NGA_CNO OF 'COLBMCNO.TXT'
*% RREG = "NGA"
*% RREG_FRM = "NGA"
*% NOMB = "AUX2"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE NGA-NUM-DUX2 IN CNO-FLD TO NGA-NUM-DUX2 IN NGA.
           MOVE NGA-NUM-MUX2 IN CNO-FLD TO NGA-NUM-MUX2 IN NGA.
           MOVE NGA-NUM-SUX2 IN CNO-FLD TO NGA-NUM-SUX2 IN NGA.
           MOVE NGA-NUM-AUX2 IN CNO-FLD TO NGA-NUM-AUX2 IN NGA.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE NGA-KEY-CPTO IN CNO-FLD TO NGA-KEY-CPTO IN NGA.
           MOVE NGA-CAI-IOPV IN CNO-FLD TO NGA-CAI-IOPV IN NGA.
           MOVE NGA-IIC-IOPV IN CNO-FLD TO NGA-IIC-IOPV IN NGA.
*% INCLUDE FIN_KEY_NGA_CNO OF 'COLBMCNO.TXT'
       FIN-KEY-NGA-CNO.
           EXIT.
       
       SCR-CLRA-CNO SECTION.
       INI-SCR-CLRA-CNO.
*% INCLUDE INI_SCR_CLRA_CNO OF 'COLBMCNO.TXT'
           PERFORM CLR-KEY-CNO.
           PERFORM CLR-FLD-CNO.
*% INCLUDE FIN_SCR_CLRA_CNO OF 'COLBMCNO.TXT'
       FIN-SCR-CLRA-CNO.
           EXIT.
       
       CLR-KEY-CNO SECTION.
       INI-CLR-KEY-CNO.
*% INCLUDE INI_CLR_KEY_CNO OF 'COLBMCNO.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN CNO-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN CNO-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN CNO-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN CNO-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "NGA"
*% RREG_FRM = "NGA"
*% NOMB = "AUX2"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO NGA-FEC-AUX2 IN CNO-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-NVEN IN CNO-FLD.
           MOVE SPACES TO NGA-KEY-CPTO IN CNO-FLD.
           MOVE SPACES TO NGA-CAI-IOPV IN CNO-FLD.
           MOVE ZEROES TO NGA-IIC-IOPV IN CNO-FLD.
*% INCLUDE FIN_CLR_KEY_CNO OF 'COLBMCNO.TXT'
       FIN-CLR-KEY-CNO.
           EXIT.
       
       CLR-FLD-CNO SECTION.
       INI-CLR-FLD-CNO.
*% INCLUDE INI_CLR_FLD_CNO OF 'COLBMCNO.TXT'
           MOVE SPACES TO FRM-CAI-OPE1 IN CNO-FLD.
           MOVE ZEROES TO FRM-IIC-OPE1 IN CNO-FLD.
           MOVE SPACES TO FRM-CAI-OPE2 IN CNO-FLD.
           MOVE ZEROES TO FRM-IIC-OPE2 IN CNO-FLD.
           MOVE SPACES TO FRM-CAI-OPE3 IN CNO-FLD.
           MOVE ZEROES TO FRM-IIC-OPE3 IN CNO-FLD.
           MOVE SPACES TO FRM-CAI-OPE4 IN CNO-FLD.
           MOVE ZEROES TO FRM-IIC-OPE4 IN CNO-FLD.
           MOVE SPACES TO FRM-CAI-OPE5 IN CNO-FLD.
           MOVE ZEROES TO FRM-IIC-OPE5 IN CNO-FLD.
           MOVE SPACES TO FRM-CAI-OPE6 IN CNO-FLD.
           MOVE ZEROES TO FRM-IIC-OPE6 IN CNO-FLD.
           MOVE SPACES TO FRM-CAI-OPE7 IN CNO-FLD.
           MOVE ZEROES TO FRM-IIC-OPE7 IN CNO-FLD.
           MOVE SPACES TO FRM-CAI-OPE8 IN CNO-FLD.
           MOVE ZEROES TO FRM-IIC-OPE8 IN CNO-FLD.
           MOVE ZEROES TO FRM-SGV-MON9 IN CNO-FLD.
           MOVE ZEROES TO FRM-NUM-CIE1 IN CNO-FLD.
           MOVE ZEROES TO FRM-NUM-CFE1 IN CNO-FLD.
           MOVE ZEROES TO FRM-SGV-MON2 IN CNO-FLD.
           MOVE ZEROES TO FRM-NUM-CIE2 IN CNO-FLD.
           MOVE ZEROES TO FRM-NUM-CFE2 IN CNO-FLD.
           MOVE ZEROES TO FRM-NUM-CIE3 IN CNO-FLD.
           MOVE ZEROES TO FRM-NUM-CFE3 IN CNO-FLD.
           MOVE ZEROES TO FRM-SGV-MON5 IN CNO-FLD.
           PERFORM CLR-DBC-CNO.
           PERFORM CLR-NGA-CNO.
*% INCLUDE FIN_CLR_FLD_CNO OF 'COLBMCNO.TXT'
       FIN-CLR-FLD-CNO.
           EXIT.
       
       CLR-DBC-CNO SECTION.
       INI-CLR-DBC-CNO.
*% INCLUDE INI_CLR_DBC_CNO OF 'COLBMCNO.TXT'
*% INCLUDE FIN_CLR_DBC_CNO OF 'COLBMCNO.TXT'
       FIN-CLR-DBC-CNO.
           EXIT.
       
       CLR-NGA-CNO SECTION.
       INI-CLR-NGA-CNO.
*% INCLUDE INI_CLR_NGA_CNO OF 'COLBMCNO.TXT'
*% RREG = "NGA"
*% RREG_FRM = "NGA"
*% NOMB = "AUX1"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO NGA-FEC-AUX1 IN CNO-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO NGA-SGV-MON8 IN CNO-FLD.
           MOVE ZEROES TO NGA-SGV-MON1 IN CNO-FLD.
*% INCLUDE FIN_CLR_NGA_CNO OF 'COLBMCNO.TXT'
       FIN-CLR-NGA-CNO.
           EXIT.
       
       FST-KEY-CNO SECTION.
       INI-FST-KEY-CNO.
*% INCLUDE INI_FST_KEY_CNO OF 'COLBMCNO.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
           MOVE FRM-POS-CURS TO DBC-NUM-ICLI-LEN IN CNO-FLD.
      * VRF-FIN --> BMCHIBCI004.SRCAIU ( FST-KEY Y FST-FLD )
*% INCLUDE FIN_FST_KEY_CNO OF 'COLBMCNO.TXT'
       FIN-FST-KEY-CNO.
           EXIT.
       
       FST-FLD-CNO SECTION.
       INI-FST-FLD-CNO.
*% INCLUDE INI_FST_FLD_CNO OF 'COLBMCNO.TXT'
           MOVE -1 TO FRM-CAI-OPE1-LEN IN CNO-FLD.
*% INCLUDE FIN_FST_FLD_CNO OF 'COLBMCNO.TXT'
       FIN-FST-FLD-CNO.
           EXIT.
       
       PRO-KEY-CNO SECTION.
       INI-PRO-KEY-CNO.
*% INCLUDE INI_PRO_KEY_CNO OF 'COLBMCNO.TXT'
           MOVE FRM-CPIM-AEY TO FRM-NUM-NVEN-ATR IN CNO-FLD.
           PERFORM PRO-MKY-CNO.
*% INCLUDE FIN_PRO_KEY_CNO OF 'COLBMCNO.TXT'
       FIN-PRO-KEY-CNO.
           EXIT.
       
       PRO-MKY-CNO SECTION.
       INI-PRO-MKY-CNO.
*% INCLUDE INI_PRO_MKY_CNO OF 'COLBMCNO.TXT'
*% INCLUDE FIN_PRO_MKY_CNO OF 'COLBMCNO.TXT'
       FIN-PRO-MKY-CNO.
           EXIT.
       
       PRO-FLD-CNO SECTION.
       INI-PRO-FLD-CNO.
*% INCLUDE INI_PRO_FLD_CNO OF 'COLBMCNO.TXT'
           MOVE FRM-CPIM-AEY TO FRM-CAI-OPE1-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IIC-OPE1-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-CAI-OPE2-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IIC-OPE2-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-CAI-OPE3-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IIC-OPE3-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-CAI-OPE4-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IIC-OPE4-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-CAI-OPE5-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IIC-OPE5-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-CAI-OPE6-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IIC-OPE6-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-CAI-OPE7-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IIC-OPE7-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-CAI-OPE8-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IIC-OPE8-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-SGV-MON9-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-NUM-CIE1-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-NUM-CFE1-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-SGV-MON2-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-NUM-CIE2-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-NUM-CFE2-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-NUM-CIE3-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-NUM-CFE3-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-SGV-MON5-ATR IN CNO-FLD.
           PERFORM PRO-DBC-CNO.
           PERFORM PRO-NGA-CNO.
*% INCLUDE FIN_PRO_FLD_CNO OF 'COLBMCNO.TXT'
       FIN-PRO-FLD-CNO.
           EXIT.
       
       PRO-DBC-CNO SECTION.
       INI-PRO-DBC-CNO.
*% INCLUDE INI_PRO_DBC_CNO OF 'COLBMCNO.TXT'
*% INCLUDE FIN_PRO_DBC_CNO OF 'COLBMCNO.TXT'
       FIN-PRO-DBC-CNO.
           EXIT.
       
       PRO-NGA-CNO SECTION.
       INI-PRO-NGA-CNO.
*% INCLUDE INI_PRO_NGA_CNO OF 'COLBMCNO.TXT'
*% RREG = "NGA"
*% RREG_FRM = "NGA"
*% NOMB = "AUX1"
           MOVE FRM-CPIM-AEY TO NGA-FEC-AUX1-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO NGA-SGV-MON8-ATR IN CNO-FLD.
           MOVE FRM-CPIM-AEY TO NGA-SGV-MON1-ATR IN CNO-FLD.
*% INCLUDE FIN_PRO_NGA_CNO OF 'COLBMCNO.TXT'
       FIN-PRO-NGA-CNO.
           EXIT.
       
       UNP-KEY-CNO SECTION.
       INI-UNP-KEY-CNO.
*% INCLUDE INI_UNP_KEY_CNO OF 'COLBMCNO.TXT'
           MOVE FRM-CPIM-UBY TO FRM-NUM-NVEN-ATR IN CNO-FLD.
           PERFORM UNP-IKY-CNO.
           PERFORM UNP-AKY-CNO.
           PERFORM UNP-MKY-CNO.
*% INCLUDE FIN_UNP_KEY_CNO OF 'COLBMCNO.TXT'
       FIN-UNP-KEY-CNO.
           EXIT.
       
       UNP-IKY-CNO SECTION.
       INI-UNP-IKY-CNO.
*% INCLUDE INI_UNP_IKY_CNO OF 'COLBMCNO.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO DBC-NUM-ICLI-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO DBC-VRF-ICLI-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO DBC-IND-ICLI-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO DBC-GLS-ICLI-ATR IN CNO-FLD.
      * IDC-FIN --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
*% RREG = "NGA"
*% RREG_FRM = "NGA"
*% NOMB = "AUX2"
           MOVE FRM-CPIM-UBY TO NGA-FEC-AUX2-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-NUM-NVEN-ATR.
           MOVE FRM-CPIM-UBY TO NGA-KEY-CPTO-ATR.
           MOVE FRM-CPIM-UBY TO NGA-CAI-IOPV-ATR.
           MOVE FRM-CPIM-UBY TO NGA-IIC-IOPV-ATR.
*% INCLUDE FIN_UNP_IKY_CNO OF 'COLBMCNO.TXT'
       FIN-UNP-IKY-CNO.
           EXIT.
       
       UNP-MKY-CNO SECTION.
       INI-UNP-MKY-CNO.
*% INCLUDE INI_UNP_MKY_CNO OF 'COLBMCNO.TXT'
*% INCLUDE FIN_UNP_MKY_CNO OF 'COLBMCNO.TXT'
       FIN-UNP-MKY-CNO.
           EXIT.
       
       UNP-AKY-CNO SECTION.
       INI-UNP-AKY-CNO.
*% INCLUDE INI_UNP_AKY_CNO OF 'COLBMCNO.TXT'
*% INCLUDE FIN_UNP_AKY_CNO OF 'COLBMCNO.TXT'
       FIN-UNP-AKY-CNO.
           EXIT.
       
       UNP-FLD-CNO SECTION.
       INI-UNP-FLD-CNO.
*% INCLUDE INI_UNP_FLD_CNO OF 'COLBMCNO.TXT'
           MOVE FRM-CPIM-UBY TO FRM-CAI-OPE1-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IIC-OPE1-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-CAI-OPE2-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IIC-OPE2-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-CAI-OPE3-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IIC-OPE3-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-CAI-OPE4-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IIC-OPE4-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-CAI-OPE5-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IIC-OPE5-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-CAI-OPE6-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IIC-OPE6-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-CAI-OPE7-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IIC-OPE7-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-CAI-OPE8-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IIC-OPE8-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-SGV-MON9-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-NUM-CIE1-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-NUM-CFE1-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-SGV-MON2-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-NUM-CIE2-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-NUM-CFE2-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-NUM-CIE3-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-NUM-CFE3-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-SGV-MON5-ATR IN CNO-FLD.
           PERFORM UNP-DBC-CNO.
           PERFORM UNP-NGA-CNO.
*% INCLUDE FIN_UNP_FLD_CNO OF 'COLBMCNO.TXT'
       FIN-UNP-FLD-CNO.
           EXIT.
       
       UNP-DBC-CNO SECTION.
       INI-UNP-DBC-CNO.
*% INCLUDE INI_UNP_DBC_CNO OF 'COLBMCNO.TXT'
*% INCLUDE FIN_UNP_DBC_CNO OF 'COLBMCNO.TXT'
       FIN-UNP-DBC-CNO.
           EXIT.
       
       UNP-NGA-CNO SECTION.
       INI-UNP-NGA-CNO.
*% INCLUDE INI_UNP_NGA_CNO OF 'COLBMCNO.TXT'
*% RREG = "NGA"
*% RREG_FRM = "NGA"
*% NOMB = "AUX1"
           MOVE FRM-CPIM-UBY TO NGA-FEC-AUX1-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO NGA-SGV-MON8-ATR IN CNO-FLD.
           MOVE FRM-CPIM-UBY TO NGA-SGV-MON1-ATR IN CNO-FLD.
*% INCLUDE FIN_UNP_NGA_CNO OF 'COLBMCNO.TXT'
       FIN-UNP-NGA-CNO.
           EXIT.
       
       VAL-MKY-CNO SECTION.
       INI-VAL-MKY-CNO.
*% INCLUDE INI_VAL_MKY_CNO OF 'COLBMCNO.TXT'
*% INCLUDE FIN_VAL_MKY_CNO OF 'COLBMCNO.TXT'
       FIN-VAL-MKY-CNO.
           EXIT.
       
       VAL-FLD-CNO SECTION.
       INI-VAL-FLD-CNO.
*% INCLUDE INI_VAL_FLD_CNO OF 'COLBMCNO.TXT'
           PERFORM VAL-FRM-CAI-OPE1.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-OPE1-LEN IN CNO-FLD
                GO TO FIN-VAL-FLD-CNO.
           PERFORM VAL-FRM-IIC-OPE1.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-OPE1-LEN IN CNO-FLD
                GO TO FIN-VAL-FLD-CNO.
           PERFORM VAL-FRM-CAI-OPE2.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-OPE2-LEN IN CNO-FLD
                GO TO FIN-VAL-FLD-CNO.
           PERFORM VAL-FRM-IIC-OPE2.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-OPE2-LEN IN CNO-FLD
                GO TO FIN-VAL-FLD-CNO.
           PERFORM VAL-FRM-CAI-OPE3.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-OPE3-LEN IN CNO-FLD
                GO TO FIN-VAL-FLD-CNO.
           PERFORM VAL-FRM-IIC-OPE3.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-OPE3-LEN IN CNO-FLD
                GO TO FIN-VAL-FLD-CNO.
           PERFORM VAL-FRM-CAI-OPE4.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-OPE4-LEN IN CNO-FLD
                GO TO FIN-VAL-FLD-CNO.
           PERFORM VAL-FRM-IIC-OPE4.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-OPE4-LEN IN CNO-FLD
                GO TO FIN-VAL-FLD-CNO.
           PERFORM VAL-FRM-CAI-OPE5.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-OPE5-LEN IN CNO-FLD
                GO TO FIN-VAL-FLD-CNO.
           PERFORM VAL-FRM-IIC-OPE5.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-OPE5-LEN IN CNO-FLD
                GO TO FIN-VAL-FLD-CNO.
           PERFORM VAL-FRM-CAI-OPE6.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-OPE6-LEN IN CNO-FLD
                GO TO FIN-VAL-FLD-CNO.
           PERFORM VAL-FRM-IIC-OPE6.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-OPE6-LEN IN CNO-FLD
                GO TO FIN-VAL-FLD-CNO.
           PERFORM VAL-FRM-CAI-OPE7.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-OPE7-LEN IN CNO-FLD
                GO TO FIN-VAL-FLD-CNO.
           PERFORM VAL-FRM-IIC-OPE7.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-OPE7-LEN IN CNO-FLD
                GO TO FIN-VAL-FLD-CNO.
           PERFORM VAL-FRM-CAI-OPE8.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-OPE8-LEN IN CNO-FLD
                GO TO FIN-VAL-FLD-CNO.
           PERFORM VAL-FRM-IIC-OPE8.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-OPE8-LEN IN CNO-FLD
                GO TO FIN-VAL-FLD-CNO.
*% RREG = "NGA"
*% RREG_FRM = "NGA"
