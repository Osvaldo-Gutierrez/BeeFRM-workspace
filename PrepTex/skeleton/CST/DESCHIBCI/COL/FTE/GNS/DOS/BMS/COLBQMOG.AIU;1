*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% OBL = "S"
*% LBL = "DBC_VRF_ICLI OF 'COLBMMOG.TXT'"
*% IZR = "S"
*% ZRO = "S"
*% KO0 = "K"
*% ZRO = "S"
*% LMX = "7"
*% FRM_IFLD = ""
      * VRF-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
      * VRF-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% OBL = "S"
*% LBL = "DBC_IDC_ICLI OF 'COLBMMOG.TXT'"
*% IZR = "S"
*% ZRO = "S"
*% KO0 = "K"
*% ZRO = "S"
*% LMX = "7"
*% FRM_IFLD = ""
      * IDC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION

       VAL-DBC-NUM-ICLI SECTION.
       INI-VAL-DBC-NUM-ICLI.
*% INCLUDE INI_DBC_NUM_ICLI  OF 'COLBMMOG.TXT'
*% INCLUDE DBC_NUM_ICLI  OF 'COLBMMOG.TXT'
*% INCLUDE FIN_DBC_NUM_ICLI  OF 'COLBMMOG.TXT'
       FIN-VAL-DBC-NUM-ICLI.
           EXIT.

       VAL-DBC-VRF-ICLI SECTION.
       INI-VAL-DBC-VRF-ICLI.
*% INCLUDE INI_DBC_VRF_ICLI  OF 'COLBMMOG.TXT'
           IF DBC-VRF-ICLI IN MOG-FLD NOT > SPACES
              MOVE SPACES TO DBC-VRF-ICLI IN MOG-FLD.
*% INCLUDE 'GNSSRC:SETVRF202.SRC'
*%! INCLUDE 'PROVRF'
*% INCLUDE DBC_VRF_ICLI  OF 'COLBMMOG.TXT'
*% INCLUDE FIN_DBC_VRF_ICLI  OF 'COLBMMOG.TXT'
*% LBL = ""
       FIN-VAL-DBC-VRF-ICLI.
           EXIT.
       VAL-DBC-IND-ICLI SECTION.
       INI-VAL-DBC-IND-ICLI.
*% INCLUDE INI_DBC_IND_ICLI  OF 'COLBMMOG.TXT'
           IF DBC-IND-ICLI IN MOG-FLD NOT > SPACES
              MOVE SPACES TO DBC-IND-ICLI IN MOG-FLD.
*% INCLUDE 'GNSSRC:VALIDC202.SRC'
*% INCLUDE DBC_IND_ICLI  OF 'COLBMMOG.TXT'
*% INCLUDE FIN_DBC_IND_ICLI  OF 'COLBMMOG.TXT'
       FIN-VAL-DBC-IND-ICLI.
           EXIT.

       VAL-DBC-GLS-ICLI SECTION.
       INI-VAL-DBC-GLS-ICLI.
*% LBL = "DBC_GLS_ICLI  OF 'COLBMMOG.TXT'"
*% INCLUDE INI_DBC_GLS_ICLI  OF 'COLBMMOG.TXT'
           IF DBC-GLS-ICLI IN MOG-FLD NOT > IDC-INIC-GLS 
              MOVE IDC-INIC-GLS TO DBC-GLS-ICLI IN MOG-FLD.
      *VAL-GLS para IDC valida glosa ( extension ) de identificador
           MOVE DBC-GLS-ICLI IN MOG-FLD TO IDC-GLS-RUTV.
           PERFORM VAL-GLS.
           MOVE IDC-GLS-RUTV TO DBC-GLS-ICLI IN MOG-FLD.
*% INCLUDE DBC_GLS_ICLI  OF 'COLBMMOG.TXT'
*% INCLUDE FIN_DBC_GLS_ICLI  OF 'COLBMMOG.TXT'
       FIN-VAL-DBC-GLS-ICLI.
           EXIT.

      * IDC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-FRM-COD-DOPE SECTION.
       INI-VAL-FRM-COD-DOPE.
*% LBL = "FRM_COD_DOPE OF 'COLBMMOG.TXT'"
*% INCLUDE INI_FRM_COD_DOPE OF 'COLBMMOG.TXT'
           IF FRM-COD-DOPE IN MOG-FLD NOT > SPACES
              MOVE SPACES TO FRM-COD-DOPE IN MOG-FLD.
*% INCLUDE FRM_COD_DOPE OF 'COLBMMOG.TXT'
*% INCLUDE FIN_FRM_COD_DOPE OF 'COLBMMOG.TXT'
       FIN-VAL-FRM-COD-DOPE.
           EXIT.
       
