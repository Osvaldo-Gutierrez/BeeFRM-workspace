      *
      * COLBQGNP
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:02 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "GNP"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMGNP.TXT'
       SET-TAG-COLBQGNP SECTION.
       INI-SET-TAG-COLBQGNP.
           MOVE
           '<IDNMOD NOM=$COLBQGNP$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:02 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQGNP.
           EXIT.
           
       PUT-ALL-GNP SECTION.
       INI-PUT-ALL-GNP.
*% INCLUDE INI_PUT_ALL_GNP OF 'COLBMGNP.TXT'
           PERFORM PUT-OPC-GNP.
*% INCLUDE FIN_PUT_ALL_GNP OF 'COLBMGNP.TXT'
       FIN-PUT-ALL-GNP.
           EXIT.
       
       PUT-OPC-GNP SECTION.
       INI-PUT-OPC-GNP.
*% INCLUDE INI_PUT_OPC_GNP OF 'COLBMGNP.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-GNP.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-GNP
               GO TO FIN-PUT-OPC-GNP.
           MOVE OPC-CAI-IOPC IN OPC TO OPC-CAI-IOPC IN GNP-FLD.
           MOVE OPC-IIC-IOPC IN OPC TO OPC-IIC-IOPC IN GNP-FLD.
*% INCLUDE FIN_PUT_OPC_GNP OF 'COLBMGNP.TXT'
       FIN-PUT-OPC-GNP.
           EXIT.
       
       KEY-ALL-GNP SECTION.
       INI-KEY-ALL-GNP.
*% INCLUDE INI_KEY_ALL_GNP OF 'COLBMGNP.TXT'
           PERFORM KEY-OPC-GNP.
*% INCLUDE FIN_KEY_ALL_GNP OF 'COLBMGNP.TXT'
       FIN-KEY-ALL-GNP.
           EXIT.
       
       KEY-OPC-GNP SECTION.
       INI-KEY-OPC-GNP.
*% INCLUDE INI_KEY_OPC_GNP OF 'COLBMGNP.TXT'
           MOVE OPC-CAI-IOPC IN GNP-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE OPC-IIC-IOPC IN GNP-FLD TO OPC-IIC-IOPC IN OPC.
*% INCLUDE FIN_KEY_OPC_GNP OF 'COLBMGNP.TXT'
       FIN-KEY-OPC-GNP.
           EXIT.
       
       SCR-CLRA-GNP SECTION.
       INI-SCR-CLRA-GNP.
*% INCLUDE INI_SCR_CLRA_GNP OF 'COLBMGNP.TXT'
           PERFORM CLR-KEY-GNP.
           PERFORM CLR-FLD-GNP.
*% INCLUDE FIN_SCR_CLRA_GNP OF 'COLBMGNP.TXT'
       FIN-SCR-CLRA-GNP.
           EXIT.
       
       CLR-KEY-GNP SECTION.
       INI-CLR-KEY-GNP.
*% INCLUDE INI_CLR_KEY_GNP OF 'COLBMGNP.TXT'
           MOVE SPACES TO OPC-CAI-IOPC IN GNP-FLD.
           MOVE ZEROES TO OPC-IIC-IOPC IN GNP-FLD.
           MOVE ZEROES TO FRM-EXT-IOPC IN GNP-FLD.
           MOVE SPACES TO FRM-IND-MODO IN GNP-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCUR"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FCUR IN GNP-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-VIGU IN GNP-FLD.
           MOVE ZEROES TO FRM-NUM-SPVE IN GNP-FLD.
           MOVE SPACES TO FRM-IND-UTPV IN GNP-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPVC"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FPVC IN GNP-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-SEVE IN GNP-FLD.
           MOVE SPACES TO FRM-IND-UTSV IN GNP-FLD.
           MOVE SPACES TO FRM-MSC-TCVE IN GNP-FLD.
           MOVE ZEROES TO FRM-NUM-NP01 IN GNP-FLD.
           MOVE ZEROES TO FRM-NUM-NP02 IN GNP-FLD.
           MOVE ZEROES TO FRM-NUM-NP03 IN GNP-FLD.
           MOVE ZEROES TO FRM-NUM-NP04 IN GNP-FLD.
           MOVE ZEROES TO FRM-NUM-NP05 IN GNP-FLD.
           MOVE ZEROES TO FRM-NUM-NP06 IN GNP-FLD.
           MOVE ZEROES TO FRM-NUM-NP07 IN GNP-FLD.
           MOVE ZEROES TO FRM-NUM-NP08 IN GNP-FLD.
           MOVE ZEROES TO FRM-NUM-NP09 IN GNP-FLD.
           MOVE ZEROES TO FRM-NUM-NP10 IN GNP-FLD.
           MOVE ZEROES TO FRM-NUM-NP11 IN GNP-FLD.
*% INCLUDE FIN_CLR_KEY_GNP OF 'COLBMGNP.TXT'
       FIN-CLR-KEY-GNP.
           EXIT.
       
       CLR-FLD-GNP SECTION.
       INI-CLR-FLD-GNP.
*% INCLUDE INI_CLR_FLD_GNP OF 'COLBMGNP.TXT'
           MOVE ZEROES TO FRM-NUM-PLZT IN GNP-FLD.
           MOVE SPACES TO FRM-IND-TPZT IN GNP-FLD.
           MOVE SPACES TO FRM-CIC-IPVC-R IN GNP-FLD(1).
           MOVE SPACES TO FRM-CIC-IPVC-R IN GNP-FLD(2).
           MOVE SPACES TO FRM-CIC-IPVC-R IN GNP-FLD(3).
           MOVE SPACES TO FRM-CIC-IPVC-R IN GNP-FLD(4).
           MOVE SPACES TO FRM-CIC-IPVC-R IN GNP-FLD(5).
           MOVE SPACES TO FRM-CIC-IPVC-R IN GNP-FLD(6).
           MOVE SPACES TO FRM-CIC-IPVC-R IN GNP-FLD(7).
           MOVE SPACES TO FRM-CIC-IPVC-R IN GNP-FLD(8).
           MOVE SPACES TO FRM-CIC-IPVC-R IN GNP-FLD(9).
           MOVE SPACES TO FRM-CIC-IPVC-R IN GNP-FLD(10).
           MOVE ZEROES TO FRM-NUM-IDLC-R IN GNP-FLD(1).
           MOVE ZEROES TO FRM-NUM-IDLC-R IN GNP-FLD(2).
           MOVE ZEROES TO FRM-NUM-IDLC-R IN GNP-FLD(3).
           MOVE ZEROES TO FRM-NUM-IDLC-R IN GNP-FLD(4).
           MOVE ZEROES TO FRM-NUM-IDLC-R IN GNP-FLD(5).
           MOVE ZEROES TO FRM-NUM-IDLC-R IN GNP-FLD(6).
           MOVE ZEROES TO FRM-NUM-IDLC-R IN GNP-FLD(7).
           MOVE ZEROES TO FRM-NUM-IDLC-R IN GNP-FLD(8).
           MOVE ZEROES TO FRM-NUM-IDLC-R IN GNP-FLD(9).
           MOVE ZEROES TO FRM-NUM-IDLC-R IN GNP-FLD(10).
           MOVE ZEROES TO FRM-NUM-VIGU-R IN GNP-FLD(1).
           MOVE ZEROES TO FRM-NUM-VIGU-R IN GNP-FLD(2).
           MOVE ZEROES TO FRM-NUM-VIGU-R IN GNP-FLD(3).
           MOVE ZEROES TO FRM-NUM-VIGU-R IN GNP-FLD(4).
           MOVE ZEROES TO FRM-NUM-VIGU-R IN GNP-FLD(5).
           MOVE ZEROES TO FRM-NUM-VIGU-R IN GNP-FLD(6).
           MOVE ZEROES TO FRM-NUM-VIGU-R IN GNP-FLD(7).
           MOVE ZEROES TO FRM-NUM-VIGU-R IN GNP-FLD(8).
           MOVE ZEROES TO FRM-NUM-VIGU-R IN GNP-FLD(9).
           MOVE ZEROES TO FRM-NUM-VIGU-R IN GNP-FLD(10).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPVC-R"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FPVC-R IN GNP-FLD(1).
           MOVE ZEROES TO FRM-FEC-FPVC-R IN GNP-FLD(2).
           MOVE ZEROES TO FRM-FEC-FPVC-R IN GNP-FLD(3).
           MOVE ZEROES TO FRM-FEC-FPVC-R IN GNP-FLD(4).
           MOVE ZEROES TO FRM-FEC-FPVC-R IN GNP-FLD(5).
           MOVE ZEROES TO FRM-FEC-FPVC-R IN GNP-FLD(6).
           MOVE ZEROES TO FRM-FEC-FPVC-R IN GNP-FLD(7).
           MOVE ZEROES TO FRM-FEC-FPVC-R IN GNP-FLD(8).
           MOVE ZEROES TO FRM-FEC-FPVC-R IN GNP-FLD(9).
           MOVE ZEROES TO FRM-FEC-FPVC-R IN GNP-FLD(10).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-DPVE-R IN GNP-FLD(1).
           MOVE ZEROES TO FRM-NUM-DPVE-R IN GNP-FLD(2).
           MOVE ZEROES TO FRM-NUM-DPVE-R IN GNP-FLD(3).
           MOVE ZEROES TO FRM-NUM-DPVE-R IN GNP-FLD(4).
           MOVE ZEROES TO FRM-NUM-DPVE-R IN GNP-FLD(5).
           MOVE ZEROES TO FRM-NUM-DPVE-R IN GNP-FLD(6).
           MOVE ZEROES TO FRM-NUM-DPVE-R IN GNP-FLD(7).
           MOVE ZEROES TO FRM-NUM-DPVE-R IN GNP-FLD(8).
           MOVE ZEROES TO FRM-NUM-DPVE-R IN GNP-FLD(9).
           MOVE ZEROES TO FRM-NUM-DPVE-R IN GNP-FLD(10).
           MOVE ZEROES TO FRM-NUM-SPVE-R IN GNP-FLD(1).
           MOVE ZEROES TO FRM-NUM-SPVE-R IN GNP-FLD(2).
           MOVE ZEROES TO FRM-NUM-SPVE-R IN GNP-FLD(3).
           MOVE ZEROES TO FRM-NUM-SPVE-R IN GNP-FLD(4).
           MOVE ZEROES TO FRM-NUM-SPVE-R IN GNP-FLD(5).
           MOVE ZEROES TO FRM-NUM-SPVE-R IN GNP-FLD(6).
           MOVE ZEROES TO FRM-NUM-SPVE-R IN GNP-FLD(7).
