*% INCLUDE FIN_UNP_AKY_QXO OF 'COLBMQXO.TXT'
       FIN-UNP-AKY-QXO.
           EXIT.
       
       UNP-FLD-QXO SECTION.
       INI-UNP-FLD-QXO.
*% INCLUDE INI_UNP_FLD_QXO OF 'COLBMQXO.TXT'
           PERFORM UNP-OPC-QXO.
           PERFORM UNP-ITR-QXO.
           PERFORM UNP-CYA-QXO.
*% INCLUDE FIN_UNP_FLD_QXO OF 'COLBMQXO.TXT'
       FIN-UNP-FLD-QXO.
           EXIT.
       
       UNP-OPC-QXO SECTION.
       INI-UNP-OPC-QXO.
*% INCLUDE INI_UNP_OPC_QXO OF 'COLBMQXO.TXT'
           MOVE FRM-CPIM-UBY TO OPC-COD-TOOC-ATR IN QXO-FLD.
           MOVE FRM-CPIM-UBY TO OPC-COD-VCOC-ATR IN QXO-FLD.
           MOVE FRM-CPIM-UBY TO OPC-COD-AOOC-ATR IN QXO-FLD.
           MOVE FRM-CPIM-UBY TO OPC-VAL-CRED-ATR IN QXO-FLD.
           MOVE FRM-CPIM-UBY TO OPC-IND-GARA-ATR IN QXO-FLD.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
           MOVE FRM-CPIM-UBY TO OPC-FEC-FCOL-ATR IN QXO-FLD.
*% INCLUDE FIN_UNP_OPC_QXO OF 'COLBMQXO.TXT'
       FIN-UNP-OPC-QXO.
           EXIT.
       
       UNP-ITR-QXO SECTION.
       INI-UNP-ITR-QXO.
*% INCLUDE INI_UNP_ITR_QXO OF 'COLBMQXO.TXT'
           MOVE FRM-CPIM-UBY TO ITR-SGV-TINT-ATR IN QXO-FLD.
           MOVE FRM-CPIM-UBY TO ITR-COD-TINT-ATR IN QXO-FLD.
*% INCLUDE FIN_UNP_ITR_QXO OF 'COLBMQXO.TXT'
       FIN-UNP-ITR-QXO.
           EXIT.
       
       UNP-CYA-QXO SECTION.
       INI-UNP-CYA-QXO.
*% INCLUDE INI_UNP_CYA_QXO OF 'COLBMQXO.TXT'
           MOVE FRM-CPIM-UBY TO CYA-IND-TCYA-ATR IN QXO-FLD.
*% INCLUDE FIN_UNP_CYA_QXO OF 'COLBMQXO.TXT'
       FIN-UNP-CYA-QXO.
           EXIT.
       
       VAL-KEY-QXO SECTION.
       INI-VAL-KEY-QXO.
           PERFORM VAL-IKY-QXO.
           PERFORM VAL-MKY-QXO.
           PERFORM VAL-AKY-QXO.
       FIN-VAL-KEY-QXO.
           EXIT.
       
       VAL-IKY-QXO SECTION.
       INI-VAL-IKY-QXO.
*% INCLUDE INI_VAL_IKY_QXO OF 'COLBMQXO.TXT'
           PERFORM VAL-OPC-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-CAI-IOPC-LEN IN QXO-FLD
                GO TO FIN-VAL-IKY-QXO.
           PERFORM VAL-OPC-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-IIC-IOPC-LEN IN QXO-FLD
                GO TO FIN-VAL-IKY-QXO.
*% INCLUDE FIN_VAL_IKY_QXO OF 'COLBMQXO.TXT'
       FIN-VAL-IKY-QXO.
           EXIT.
       
       VAL-MKY-QXO SECTION.
       INI-VAL-MKY-QXO.
*% INCLUDE INI_VAL_MKY_QXO OF 'COLBMQXO.TXT'
*% INCLUDE FIN_VAL_MKY_QXO OF 'COLBMQXO.TXT'
       FIN-VAL-MKY-QXO.
           EXIT.
       
       VAL-AKY-QXO SECTION.
       INI-VAL-AKY-QXO.
*% INCLUDE INI_VAL_AKY_QXO OF 'COLBMQXO.TXT'
*% INCLUDE FIN_VAL_AKY_QXO OF 'COLBMQXO.TXT'
       FIN-VAL-AKY-QXO.
           EXIT.
       
       VAL-OPC-CAI-IOPC SECTION.
       INI-VAL-OPC-CAI-IOPC.
*% LBL = "OPC_CAI_IOPC OF 'COLBMQXO.TXT'"
*% INCLUDE INI_OPC_CAI_IOPC OF 'COLBMQXO.TXT'
           IF OPC-CAI-IOPC IN QXO-FLD NOT > SPACES
              MOVE SPACES TO OPC-CAI-IOPC IN QXO-FLD.
*% INCLUDE OPC_CAI_IOPC OF 'COLBMQXO.TXT'
*% INCLUDE FIN_OPC_CAI_IOPC OF 'COLBMQXO.TXT'
       FIN-VAL-OPC-CAI-IOPC.
           EXIT.
       
       VAL-OPC-IIC-IOPC SECTION.
       INI-VAL-OPC-IIC-IOPC.
*% LBL = "OPC_IIC_IOPC OF 'COLBMQXO.TXT'"
*% INCLUDE INI_OPC_IIC_IOPC OF 'COLBMQXO.TXT'
*% INCLUDE OPC_IIC_IOPC OF 'COLBMQXO.TXT'
*% INCLUDE FIN_OPC_IIC_IOPC OF 'COLBMQXO.TXT'
       FIN-VAL-OPC-IIC-IOPC.
           EXIT.
