      *
      * COLBQQXO
      * Version 3.0.1 Nov-2005
      * 22-Jan-17 10:32 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "QXO"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMQXO.TXT'
       SET-TAG-COLBQQXO SECTION.
       INI-SET-TAG-COLBQQXO.
           MOVE
           '<IDNMOD NOM=$COLBQQXO$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$22-Jan-17 10:32 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQQXO.
           EXIT.
           
       PUT-ALL-QXO SECTION.
       INI-PUT-ALL-QXO.
*% INCLUDE INI_PUT_ALL_QXO OF 'COLBMQXO.TXT'
           PERFORM PUT-OPC-QXO.
           PERFORM PUT-ITR-QXO.
           PERFORM PUT-CYA-QXO.
*% INCLUDE FIN_PUT_ALL_QXO OF 'COLBMQXO.TXT'
       FIN-PUT-ALL-QXO.
           EXIT.
       
       PUT-OPC-QXO SECTION.
       INI-PUT-OPC-QXO.
*% INCLUDE INI_PUT_OPC_QXO OF 'COLBMQXO.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-QXO.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-QXO
               GO TO FIN-PUT-OPC-QXO.
           MOVE OPC-CAI-IOPC IN OPC TO OPC-CAI-IOPC IN QXO-FLD.
           MOVE OPC-IIC-IOPC IN OPC TO OPC-IIC-IOPC IN QXO-FLD.
           MOVE OPC-COD-TOOC IN OPC TO OPC-COD-TOOC IN QXO-FLD.
           MOVE OPC-COD-VCOC IN OPC TO OPC-COD-VCOC IN QXO-FLD.
           MOVE OPC-COD-AOOC IN OPC TO OPC-COD-AOOC IN QXO-FLD.
           MOVE OPC-VAL-CRED IN OPC TO OPC-VAL-CRED IN QXO-FLD.
           MOVE OPC-VAL-SCRE IN OPC TO OPC-VAL-SCRE IN QXO-FLD.
           MOVE OPC-IND-GARA IN OPC TO OPC-IND-GARA IN QXO-FLD.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE OPC-NUM-DCOL IN OPC TO OPC-NUM-DCOL IN QXO-FLD.
           MOVE OPC-NUM-MCOL IN OPC TO OPC-NUM-MCOL IN QXO-FLD.
           MOVE OPC-NUM-SCOL IN OPC TO OPC-NUM-SCOL IN QXO-FLD.
           MOVE OPC-NUM-ACOL IN OPC TO OPC-NUM-ACOL IN QXO-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FING"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE OPC-NUM-DING IN OPC TO OPC-NUM-DING IN QXO-FLD.
           MOVE OPC-NUM-MING IN OPC TO OPC-NUM-MING IN QXO-FLD.
           MOVE OPC-NUM-SING IN OPC TO OPC-NUM-SING IN QXO-FLD.
           MOVE OPC-NUM-AING IN OPC TO OPC-NUM-AING IN QXO-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% BUF = "QXO-FLD"
*% SIS = "TAB"
*% TAB = "TIO "
*% FLD = "OPC-COD-TOOC"
*% GLS = "FRM-GLS-TOOC"
*% TIENE_GLOSA = TRUE
*% LGL = 14
*% INCLUDE 'BUSTAB'
*% BUF = "QXO-FLD"
*% SIS = "TAB"
*% TAB = "VLR "
*% FLD = "OPC-COD-VCOC"
*% GLS = "FRM-GLS-VCOC"
*% TIENE_GLOSA = TRUE
*% LGL = 15
*% INCLUDE 'BUSTAB'
*% BUF = "QXO-FLD"
*% SIS = "COL"
*% TAB = "GARA"
*% FLD = "OPC-IND-GARA"
*% GLS = "FRM-GLS-GARA"
*% TIENE_GLOSA = TRUE
*% LGL = 27
*% INCLUDE 'BUSMSC'
*% INCLUDE FIN_PUT_OPC_QXO OF 'COLBMQXO.TXT'
       FIN-PUT-OPC-QXO.
           EXIT.
       
       PUT-ITR-QXO SECTION.
       INI-PUT-ITR-QXO.
*% INCLUDE INI_PUT_ITR_QXO OF 'COLBMQXO.TXT'
           IF PGM-STAT-ITR-NUL
               GO TO FIN-PUT-ITR-QXO.
           IF NOT PGM-STAT-ITR-OKS
               PERFORM CLR-ITR-QXO
               GO TO FIN-PUT-ITR-QXO.
           MOVE ITR-SGV-TINT IN ITR TO ITR-SGV-TINT IN QXO-FLD.
           MOVE ITR-COD-TINT IN ITR TO ITR-COD-TINT IN QXO-FLD.
*% BUF = "QXO-FLD"
*% SIS = "COL"
*% TAB = "INT "
*% FLD = "ITR-COD-TINT"
*% GLS = "FRM-GLS-TINT"
*% TIENE_GLOSA = TRUE
*% LGL = 36
*% INCLUDE 'BUSTAB'
*% INCLUDE FIN_PUT_ITR_QXO OF 'COLBMQXO.TXT'
       FIN-PUT-ITR-QXO.
           EXIT.
       
       PUT-CYA-QXO SECTION.
       INI-PUT-CYA-QXO.
*% INCLUDE INI_PUT_CYA_QXO OF 'COLBMQXO.TXT'
           IF PGM-STAT-CYA-NUL
               GO TO FIN-PUT-CYA-QXO.
           IF NOT PGM-STAT-CYA-OKS
               PERFORM CLR-CYA-QXO
               GO TO FIN-PUT-CYA-QXO.
           MOVE CYA-IND-TCYA IN CYA TO CYA-IND-TCYA IN QXO-FLD.
*% BUF = "QXO-FLD"
*% SIS = "TAB"
*% TAB = "TCYA"
*% FLD = "CYA-IND-TCYA"
*% GLS = "FRM-GLS-TCYA"
*% TIENE_GLOSA = TRUE
*% LGL = 31
*% INCLUDE 'BUSMSC'
*% INCLUDE FIN_PUT_CYA_QXO OF 'COLBMQXO.TXT'
       FIN-PUT-CYA-QXO.
           EXIT.
       
       KEY-ALL-QXO SECTION.
       INI-KEY-ALL-QXO.
*% INCLUDE INI_KEY_ALL_QXO OF 'COLBMQXO.TXT'
           PERFORM KEY-OPC-QXO.
           PERFORM KEY-ITR-QXO.
           PERFORM KEY-CYA-QXO.
*% INCLUDE FIN_KEY_ALL_QXO OF 'COLBMQXO.TXT'
       FIN-KEY-ALL-QXO.
           EXIT.
       
       KEY-OPC-QXO SECTION.
       INI-KEY-OPC-QXO.
*% INCLUDE INI_KEY_OPC_QXO OF 'COLBMQXO.TXT'
           MOVE OPC-CAI-IOPC IN QXO-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE OPC-IIC-IOPC IN QXO-FLD TO OPC-IIC-IOPC IN OPC.
*% INCLUDE FIN_KEY_OPC_QXO OF 'COLBMQXO.TXT'
       FIN-KEY-OPC-QXO.
           EXIT.
       
       KEY-ITR-QXO SECTION.
       INI-KEY-ITR-QXO.
*% INCLUDE INI_KEY_ITR_QXO OF 'COLBMQXO.TXT'
*% INCLUDE FIN_KEY_ITR_QXO OF 'COLBMQXO.TXT'
       FIN-KEY-ITR-QXO.
           EXIT.
       
       KEY-CYA-QXO SECTION.
       INI-KEY-CYA-QXO.
*% INCLUDE INI_KEY_CYA_QXO OF 'COLBMQXO.TXT'
*% INCLUDE FIN_KEY_CYA_QXO OF 'COLBMQXO.TXT'
       FIN-KEY-CYA-QXO.
           EXIT.
       
       SCR-CLRA-QXO SECTION.
       INI-SCR-CLRA-QXO.
*% INCLUDE INI_SCR_CLRA_QXO OF 'COLBMQXO.TXT'
           PERFORM CLR-KEY-QXO.
           PERFORM CLR-FLD-QXO.
*% INCLUDE FIN_SCR_CLRA_QXO OF 'COLBMQXO.TXT'
       FIN-SCR-CLRA-QXO.
           EXIT.
       
       CLR-KEY-QXO SECTION.
       INI-CLR-KEY-QXO.
*% INCLUDE INI_CLR_KEY_QXO OF 'COLBMQXO.TXT'
           MOVE SPACES TO OPC-CAI-IOPC IN QXO-FLD.
           MOVE ZEROES TO OPC-IIC-IOPC IN QXO-FLD.
*% INCLUDE FIN_CLR_KEY_QXO OF 'COLBMQXO.TXT'
       FIN-CLR-KEY-QXO.
           EXIT.
       
       CLR-FLD-QXO SECTION.
       INI-CLR-FLD-QXO.
*% INCLUDE INI_CLR_FLD_QXO OF 'COLBMQXO.TXT'
           MOVE SPACES TO FRM-GLS-TINT IN QXO-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN QXO-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN QXO-FLD.
           MOVE SPACES TO FRM-GLS-AOOC IN QXO-FLD.
           MOVE ZEROES TO FRM-NUM-CUOP IN QXO-FLD.
           MOVE ZEROES TO FRM-NUM-TCUO IN QXO-FLD.
           MOVE ZEROES TO FRM-NUM-CXVC IN QXO-FLD.
           MOVE ZEROES TO FRM-NUM-PRXV IN QXO-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FVEN IN QXO-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPCU"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FPCU IN QXO-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-IND-TDEU IN QXO-FLD.
           MOVE SPACES TO FRM-GLS-TDEU IN QXO-FLD.
           MOVE SPACES TO FRM-GLS-TCYA IN QXO-FLD.
           MOVE ZEROES TO FRM-NUM-PERC IN QXO-FLD.
           MOVE SPACES TO FRM-IND-PERC IN QXO-FLD.
           MOVE SPACES TO FRM-GLS-GARA IN QXO-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FMAV"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FMAV IN QXO-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FDEV"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FDEV IN QXO-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           PERFORM CLR-OPC-QXO.
           PERFORM CLR-ITR-QXO.
           PERFORM CLR-CYA-QXO.
*% INCLUDE FIN_CLR_FLD_QXO OF 'COLBMQXO.TXT'
       FIN-CLR-FLD-QXO.
           EXIT.
       
       CLR-OPC-QXO SECTION.
       INI-CLR-OPC-QXO.
*% INCLUDE INI_CLR_OPC_QXO OF 'COLBMQXO.TXT'
           MOVE SPACES TO OPC-COD-TOOC IN QXO-FLD.
           MOVE SPACES TO OPC-COD-VCOC IN QXO-FLD.
           MOVE SPACES TO OPC-COD-AOOC IN QXO-FLD.
           MOVE ZEROES TO OPC-VAL-CRED IN QXO-FLD.
           MOVE ZEROES TO OPC-VAL-SCRE IN QXO-FLD.
           MOVE SPACES TO OPC-IND-GARA IN QXO-FLD.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO OPC-FEC-FCOL IN QXO-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FING"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO OPC-FEC-FING IN QXO-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_OPC_QXO OF 'COLBMQXO.TXT'
       FIN-CLR-OPC-QXO.
           EXIT.
       
       CLR-ITR-QXO SECTION.
       INI-CLR-ITR-QXO.
*% INCLUDE INI_CLR_ITR_QXO OF 'COLBMQXO.TXT'
           MOVE ZEROES TO ITR-SGV-TINT IN QXO-FLD.
           MOVE SPACES TO ITR-COD-TINT IN QXO-FLD.
*% INCLUDE FIN_CLR_ITR_QXO OF 'COLBMQXO.TXT'
       FIN-CLR-ITR-QXO.
           EXIT.
       
       CLR-CYA-QXO SECTION.
       INI-CLR-CYA-QXO.
*% INCLUDE INI_CLR_CYA_QXO OF 'COLBMQXO.TXT'
           MOVE SPACES TO CYA-IND-TCYA IN QXO-FLD.
*% INCLUDE FIN_CLR_CYA_QXO OF 'COLBMQXO.TXT'
       FIN-CLR-CYA-QXO.
           EXIT.
       
       FST-KEY-QXO SECTION.
       INI-FST-KEY-QXO.
*% INCLUDE INI_FST_KEY_QXO OF 'COLBMQXO.TXT'
           MOVE -1 TO OPC-CAI-IOPC-LEN IN QXO-FLD.
*% INCLUDE FIN_FST_KEY_QXO OF 'COLBMQXO.TXT'
       FIN-FST-KEY-QXO.
           EXIT.
       
       FST-FLD-QXO SECTION.
       INI-FST-FLD-QXO.
*% INCLUDE INI_FST_FLD_QXO OF 'COLBMQXO.TXT'
           MOVE -1 TO ITR-SGV-TINT-LEN IN QXO-FLD.
*% INCLUDE FIN_FST_FLD_QXO OF 'COLBMQXO.TXT'
       FIN-FST-FLD-QXO.
           EXIT.
       
       PRO-KEY-QXO SECTION.
       INI-PRO-KEY-QXO.
*% INCLUDE INI_PRO_KEY_QXO OF 'COLBMQXO.TXT'
           PERFORM PRO-IKY-QXO.
           PERFORM PRO-AKY-QXO.
           PERFORM PRO-MKY-QXO.
*% INCLUDE FIN_PRO_KEY_QXO OF 'COLBMQXO.TXT'
       FIN-PRO-KEY-QXO.
           EXIT.
       
       PRO-IKY-QXO SECTION.
       INI-PRO-IKY-QXO.
*% INCLUDE INI_PRO_IKY_QXO OF 'COLBMQXO.TXT'
           MOVE FRM-CPIM-AEY TO OPC-CAI-IOPC-ATR IN QXO-FLD.
           MOVE FRM-CPIM-AEY TO OPC-IIC-IOPC-ATR IN QXO-FLD.
*% INCLUDE FIN_PRO_IKY_QXO OF 'COLBMQXO.TXT'
       FIN-PRO-IKY-QXO.
           EXIT.
       
       PRO-MKY-QXO SECTION.
       INI-PRO-MKY-QXO.
*% INCLUDE INI_PRO_MKY_QXO OF 'COLBMQXO.TXT'
*% INCLUDE FIN_PRO_MKY_QXO OF 'COLBMQXO.TXT'
       FIN-PRO-MKY-QXO.
           EXIT.
       
       PRO-AKY-QXO SECTION.
       INI-PRO-AKY-QXO.
*% INCLUDE INI_PRO_AKY_QXO OF 'COLBMQXO.TXT'
*% INCLUDE FIN_PRO_AKY_QXO OF 'COLBMQXO.TXT'
       FIN-PRO-AKY-QXO.
           EXIT.
       
       UNP-KEY-QXO SECTION.
       INI-UNP-KEY-QXO.
*% INCLUDE INI_UNP_KEY_QXO OF 'COLBMQXO.TXT'
           PERFORM UNP-IKY-QXO.
           PERFORM UNP-AKY-QXO.
           PERFORM UNP-MKY-QXO.
*% INCLUDE FIN_UNP_KEY_QXO OF 'COLBMQXO.TXT'
       FIN-UNP-KEY-QXO.
           EXIT.
       
       UNP-IKY-QXO SECTION.
       INI-UNP-IKY-QXO.
*% INCLUDE INI_UNP_IKY_QXO OF 'COLBMQXO.TXT'
           MOVE FRM-CPIM-UBY TO OPC-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO OPC-IIC-IOPC-ATR.
*% INCLUDE FIN_UNP_IKY_QXO OF 'COLBMQXO.TXT'
       FIN-UNP-IKY-QXO.
           EXIT.
       
       UNP-MKY-QXO SECTION.
       INI-UNP-MKY-QXO.
*% INCLUDE INI_UNP_MKY_QXO OF 'COLBMQXO.TXT'
*% INCLUDE FIN_UNP_MKY_QXO OF 'COLBMQXO.TXT'
       FIN-UNP-MKY-QXO.
           EXIT.
       
