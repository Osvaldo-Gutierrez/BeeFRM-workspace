*% FRM = "EJC"
      *
      * COLBUEJC
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:59 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMEJC.TXT'
       SET-TAG-COLBUEJC SECTION.
       INI-SET-TAG-COLBUEJC.
           MOVE
           '<IDNMOD NOM=$COLBUEJC$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:59 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUEJC.
           EXIT.
           
       RET-ALL-EJC SECTION.
       INI-RET-ALL-EJC.
*% INCLUDE INI_RET_ALL_EJC OF 'COLBMEJC.TXT'
           PERFORM KEY-ALL-EJC.
           PERFORM RET-EJC-EJC.
           PERFORM RET-USR-EJC.
*% INCLUDE FIN_RET_ALL_EJC OF 'COLBMEJC.TXT'
       FIN-RET-ALL-EJC.
           EXIT.
       
       RET-EJC-EJC SECTION.
       INI-RET-EJC-EJC.
*% INCLUDE INI_RET_EJC_EJC OF 'COLBMEJC.TXT'
*% RREG = "EJC"
*% RREG_FRM = "EJC"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE EJC-NUM-DTRN IN EJC-FLD TO EJC-NUM-DTRN IN EJC.
           MOVE EJC-NUM-MTRN IN EJC-FLD TO EJC-NUM-MTRN IN EJC.
           MOVE EJC-NUM-STRN IN EJC-FLD TO EJC-NUM-STRN IN EJC.
           MOVE EJC-NUM-ATRN IN EJC-FLD TO EJC-NUM-ATRN IN EJC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE EJC-COD-ATRI IN EJC-FLD TO EJC-COD-ATRI IN EJC.
           MOVE EJC-VAL-CMAX IN EJC-FLD TO EJC-VAL-CMAX IN EJC.
           MOVE EJC-COD-VCAM IN EJC-FLD TO EJC-COD-VCAM IN EJC.
           MOVE EJC-VAL-DMAX IN EJC-FLD TO EJC-VAL-DMAX IN EJC.
*% INCLUDE FIN_RET_EJC_EJC OF 'COLBMEJC.TXT'
       FIN-RET-EJC-EJC.
           EXIT.
       
       RET-USR-EJC SECTION.
       INI-RET-USR-EJC.
*% INCLUDE INI_RET_USR_EJC OF 'COLBMEJC.TXT'
           MOVE USR-GLS-DESC IN EJC-FLD TO USR-GLS-DESC IN USR.
*% RREG = "USR"
*% RREG_FRM = "USR"
*% NOMB = "IUSR"
*% RREG = "USR"
*% RREG_FRM = "USR"
*% NOMB = "IUSR"
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE USR-NUM-IUSR IN EJC-FLD TO USR-NUM-IUSR IN USR.
           MOVE USR-VRF-IUSR IN EJC-FLD TO USR-VRF-IUSR IN USR.
           MOVE USR-IND-IUSR IN EJC-FLD TO USR-IND-IUSR IN USR.
           MOVE USR-GLS-IUSR IN EJC-FLD TO USR-GLS-IUSR IN USR.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE USR-IND-VIGE IN EJC-FLD TO USR-IND-VIGE IN USR.
           MOVE USR-COD-UBIC IN EJC-FLD TO USR-COD-UBIC IN USR.
*% INCLUDE FIN_RET_USR_EJC OF 'COLBMEJC.TXT'
       FIN-RET-USR-EJC.
           EXIT.
       
       KEY-ALL-EJC SECTION.
       INI-KEY-ALL-EJC.
*% INCLUDE INI_KEY_ALL_EJC OF 'COLBMEJC.TXT'
           PERFORM KEY-EJC-EJC.
           PERFORM KEY-USR-EJC.
*% INCLUDE FIN_KEY_ALL_EJC OF 'COLBMEJC.TXT'
       FIN-KEY-ALL-EJC.
           EXIT.
       
       KEY-EJC-EJC SECTION.
       INI-KEY-EJC-EJC.
*% INCLUDE INI_KEY_EJC_EJC OF 'COLBMEJC.TXT'
           MOVE EJC-COD-EJEC IN EJC-FLD TO EJC-COD-EJEC IN EJC.
*% INCLUDE FIN_KEY_EJC_EJC OF 'COLBMEJC.TXT'
       FIN-KEY-EJC-EJC.
           EXIT.
       
       KEY-USR-EJC SECTION.
       INI-KEY-USR-EJC.
*% INCLUDE INI_KEY_USR_EJC OF 'COLBMEJC.TXT'
*% INCLUDE FIN_KEY_USR_EJC OF 'COLBMEJC.TXT'
       FIN-KEY-USR-EJC.
           EXIT.
       
       SCR-CLRA-EJC SECTION.
       INI-SCR-CLRA-EJC.
*% INCLUDE INI_SCR_CLRA_EJC OF 'COLBMEJC.TXT'
           PERFORM CLR-KEY-EJC.
           PERFORM CLR-FLD-EJC.
*% INCLUDE FIN_SCR_CLRA_EJC OF 'COLBMEJC.TXT'
       FIN-SCR-CLRA-EJC.
           EXIT.
       
       CLR-KEY-EJC SECTION.
       INI-CLR-KEY-EJC.
*% INCLUDE INI_CLR_KEY_EJC OF 'COLBMEJC.TXT'
           MOVE SPACES TO EJC-COD-EJEC IN EJC-FLD.
*% INCLUDE FIN_CLR_KEY_EJC OF 'COLBMEJC.TXT'
       FIN-CLR-KEY-EJC.
           EXIT.
       
       CLR-FLD-EJC SECTION.
       INI-CLR-FLD-EJC.
*% INCLUDE INI_CLR_FLD_EJC OF 'COLBMEJC.TXT'
           MOVE SPACES TO FRM-GLS-ATRI IN EJC-FLD.
           MOVE SPACES TO FRM-GLS-UBIC IN EJC-FLD.
           MOVE SPACES TO FRM-GLS-VCAM IN EJC-FLD.
           PERFORM CLR-EJC-EJC.
           PERFORM CLR-USR-EJC.
*% INCLUDE FIN_CLR_FLD_EJC OF 'COLBMEJC.TXT'
       FIN-CLR-FLD-EJC.
           EXIT.
       
       CLR-EJC-EJC SECTION.
       INI-CLR-EJC-EJC.
*% INCLUDE INI_CLR_EJC_EJC OF 'COLBMEJC.TXT'
*% RREG = "EJC"
*% RREG_FRM = "EJC"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO EJC-FEC-FTRN IN EJC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO EJC-COD-ATRI IN EJC-FLD.
           MOVE ZEROES TO EJC-VAL-CMAX IN EJC-FLD.
           MOVE SPACES TO EJC-COD-VCAM IN EJC-FLD.
           MOVE ZEROES TO EJC-VAL-DMAX IN EJC-FLD.
*% INCLUDE FIN_CLR_EJC_EJC OF 'COLBMEJC.TXT'
       FIN-CLR-EJC-EJC.
           EXIT.
       
       CLR-USR-EJC SECTION.
       INI-CLR-USR-EJC.
*% INCLUDE INI_CLR_USR_EJC OF 'COLBMEJC.TXT'
           MOVE SPACES TO USR-GLS-DESC IN EJC-FLD.
*% RREG = "USR"
*% RREG_FRM = "USR"
*% NOMB = "IUSR"
*% RREG = "USR"
*% RREG_FRM = "USR"
*% NOMB = "IUSR"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO USR-NUM-IUSR IN EJC-FLD.
           MOVE SPACES TO USR-VRF-IUSR IN EJC-FLD.
           MOVE SPACES TO USR-IND-IUSR IN EJC-FLD.
           MOVE SPACES TO USR-GLS-IUSR IN EJC-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO USR-IND-VIGE IN EJC-FLD.
           MOVE SPACES TO USR-COD-UBIC IN EJC-FLD.
*% INCLUDE FIN_CLR_USR_EJC OF 'COLBMEJC.TXT'
       FIN-CLR-USR-EJC.
           EXIT.
       
       FST-KEY-EJC SECTION.
       INI-FST-KEY-EJC.
*% INCLUDE INI_FST_KEY_EJC OF 'COLBMEJC.TXT'
           MOVE -1 TO EJC-COD-EJEC-LEN IN EJC-FLD.
*% INCLUDE FIN_FST_KEY_EJC OF 'COLBMEJC.TXT'
       FIN-FST-KEY-EJC.
           EXIT.
       
       FST-FLD-EJC SECTION.
       INI-FST-FLD-EJC.
*% INCLUDE INI_FST_FLD_EJC OF 'COLBMEJC.TXT'
           MOVE -1 TO EJC-COD-ATRI-LEN IN EJC-FLD.
*% INCLUDE FIN_FST_FLD_EJC OF 'COLBMEJC.TXT'
       FIN-FST-FLD-EJC.
           EXIT.
       
       PRO-KEY-EJC SECTION.
       INI-PRO-KEY-EJC.
*% INCLUDE INI_PRO_KEY_EJC OF 'COLBMEJC.TXT'
           PERFORM PRO-MKY-EJC.
*% INCLUDE FIN_PRO_KEY_EJC OF 'COLBMEJC.TXT'
       FIN-PRO-KEY-EJC.
           EXIT.
       
       PRO-MKY-EJC SECTION.
       INI-PRO-MKY-EJC.
*% INCLUDE INI_PRO_MKY_EJC OF 'COLBMEJC.TXT'
*% INCLUDE FIN_PRO_MKY_EJC OF 'COLBMEJC.TXT'
       FIN-PRO-MKY-EJC.
           EXIT.
       
       PRO-FLD-EJC SECTION.
       INI-PRO-FLD-EJC.
