           PERFORM PRO-EJC-EJC.
           PERFORM PRO-USR-EJC.
*% INCLUDE FIN_PRO_FLD_EJC OF 'COLBMEJC.TXT'
       FIN-PRO-FLD-EJC.
           EXIT.
       
       PRO-EJC-EJC SECTION.
       INI-PRO-EJC-EJC.
*% INCLUDE INI_PRO_EJC_EJC OF 'COLBMEJC.TXT'
           MOVE FRM-CPIM-AEY TO EJC-COD-ATRI-ATR IN EJC-FLD.
           MOVE FRM-CPIM-AEY TO EJC-VAL-CMAX-ATR IN EJC-FLD.
           MOVE FRM-CPIM-AEY TO EJC-COD-VCAM-ATR IN EJC-FLD.
           MOVE FRM-CPIM-AEY TO EJC-VAL-DMAX-ATR IN EJC-FLD.
*% INCLUDE FIN_PRO_EJC_EJC OF 'COLBMEJC.TXT'
       FIN-PRO-EJC-EJC.
           EXIT.
       
       PRO-USR-EJC SECTION.
       INI-PRO-USR-EJC.
*% INCLUDE INI_PRO_USR_EJC OF 'COLBMEJC.TXT'
*% INCLUDE FIN_PRO_USR_EJC OF 'COLBMEJC.TXT'
       FIN-PRO-USR-EJC.
           EXIT.
       
       UNP-KEY-EJC SECTION.
       INI-UNP-KEY-EJC.
*% INCLUDE INI_UNP_KEY_EJC OF 'COLBMEJC.TXT'
           PERFORM UNP-IKY-EJC.
           PERFORM UNP-AKY-EJC.
           PERFORM UNP-MKY-EJC.
*% INCLUDE FIN_UNP_KEY_EJC OF 'COLBMEJC.TXT'
       FIN-UNP-KEY-EJC.
           EXIT.
       
       UNP-IKY-EJC SECTION.
       INI-UNP-IKY-EJC.
*% INCLUDE INI_UNP_IKY_EJC OF 'COLBMEJC.TXT'
           MOVE FRM-CPIM-UBY TO EJC-COD-EJEC-ATR.
*% INCLUDE FIN_UNP_IKY_EJC OF 'COLBMEJC.TXT'
       FIN-UNP-IKY-EJC.
           EXIT.
       
       UNP-MKY-EJC SECTION.
       INI-UNP-MKY-EJC.
*% INCLUDE INI_UNP_MKY_EJC OF 'COLBMEJC.TXT'
*% INCLUDE FIN_UNP_MKY_EJC OF 'COLBMEJC.TXT'
       FIN-UNP-MKY-EJC.
           EXIT.
       
