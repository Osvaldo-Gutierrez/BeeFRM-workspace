*% INCLUDE EDT_FLD_EJC OF 'COLBMEJC.TXT'
*% RREG = "USR"
*% RREG_FRM = "USR"
*% NOMB = "IUSR"
*% RREG = "USR"
*% RREG_FRM = "USR"
*% NOMB = "IUSR"
      * IDC-INI --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF USR-NUM-IUSR-LEN IN EJC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO USR-NUM-IUSR-LEN IN EJC-FLD
                GO TO FIN-EDT-FLD-EJC.
           MOVE USR-NUM-IUSR IN EJC-FLD TO
                USR-NUM-IUSR-EDT IN EJC-EDT.
           MOVE USR-NUM-IUSR-EDT IN EJC-EDT TO
                USR-NUM-IUSR-ALF IN EJC-FLD.
      * IDC-FIN --> BMCHIBCI011.SRCAIU EDICION NUMERICA
*% RREG = "EJC"
*% RREG_FRM = "EJC"
*% NOMB = "FTRN"
           IF EJC-VAL-CMAX-LEN IN EJC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EJC-VAL-CMAX-LEN IN EJC-FLD
                GO TO FIN-EDT-FLD-EJC.
           MOVE EJC-VAL-CMAX IN EJC-FLD TO EJC-VAL-CMAX-EDT IN EJC-EDT.
           MOVE EJC-VAL-CMAX-EDT IN EJC-EDT TO 
                EJC-VAL-CMAX-ALF IN EJC-FLD.
           IF EJC-VAL-DMAX-LEN IN EJC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EJC-VAL-DMAX-LEN IN EJC-FLD
                GO TO FIN-EDT-FLD-EJC.
           MOVE EJC-VAL-DMAX IN EJC-FLD TO EJC-VAL-DMAX-EDT IN EJC-EDT.
           MOVE EJC-VAL-DMAX-EDT IN EJC-EDT TO 
                EJC-VAL-DMAX-ALF IN EJC-FLD.
*% INCLUDE FIN_EDT_FLD_EJC OF 'COLBMEJC.TXT'
       FIN-EDT-FLD-EJC.
           EXIT.
