           EXIT.
       
       UNP-FLD-CPC SECTION.
       INI-UNP-FLD-CPC.
*% INCLUDE INI_UNP_FLD_CPC OF 'COLBMCPC.TXT'
           PERFORM UNP-DBC-CPC.
*% INCLUDE FIN_UNP_FLD_CPC OF 'COLBMCPC.TXT'
       FIN-UNP-FLD-CPC.
           EXIT.
       
       UNP-DBC-CPC SECTION.
       INI-UNP-DBC-CPC.
*% INCLUDE INI_UNP_DBC_CPC OF 'COLBMCPC.TXT'
*% INCLUDE FIN_UNP_DBC_CPC OF 'COLBMCPC.TXT'
       FIN-UNP-DBC-CPC.
           EXIT.
       
       VAL-MKY-CPC SECTION.
       INI-VAL-MKY-CPC.
*% INCLUDE INI_VAL_MKY_CPC OF 'COLBMCPC.TXT'
*% INCLUDE FIN_VAL_MKY_CPC OF 'COLBMCPC.TXT'
       FIN-VAL-MKY-CPC.
           EXIT.
       
       VAL-FLD-CPC SECTION.
       INI-VAL-FLD-CPC.
*% INCLUDE INI_VAL_FLD_CPC OF 'COLBMCPC.TXT'
           PERFORM VAL-FRM-GLS-NOMC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-NOMC-LEN IN CPC-FLD
                GO TO FIN-VAL-FLD-CPC.
           PERFORM VAL-FRM-VAL-CCRE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-CCRE-LEN IN CPC-FLD
                GO TO FIN-VAL-FLD-CPC.
           PERFORM VAL-FRM-CAI-IOPC
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 6
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-CAI-IOPC-LEN IN CPC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPC.
           PERFORM VAL-FRM-IIC-IOPC
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 6
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-IIC-IOPC-LEN IN CPC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPC.
           PERFORM VAL-FRM-COD-COOC
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 6
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-COD-COOC-LEN IN CPC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPC.
           PERFORM VAL-FRM-IND-CART
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 6
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-IND-CART-LEN IN CPC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPC.
           PERFORM VAL-FRM-IND-CJUD
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 6
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-IND-CJUD-LEN IN CPC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPC.
           PERFORM VAL-FRM-IND-ACEL
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 6
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-IND-ACEL-LEN IN CPC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPC.
           PERFORM VAL-FRM-NUM-AGNO
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 6
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-NUM-AGNO-LEN IN CPC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPC.
           PERFORM VAL-FRM-VAL-CRED
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 6
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-CRED-LEN IN CPC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPC.
           PERFORM VAL-FRM-IND-CIMP
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 6
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-IND-CIMP-LEN IN CPC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPC.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCUR"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FCUR VARYING FRM-IFLD
                FROM 1 BY 1 UNTIL FRM-IFLD > 6 OR
                FRM-SUAR = FRM-SUAR-MAL OR
                FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FCUR-LEN IN CPC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPC.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-SGV-COBR
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 6
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-COBR-LEN IN CPC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPC.
           PERFORM VAL-FRM-SGV-MPAR
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 6
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-MPAR-LEN IN CPC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPC.
           PERFORM VAL-FRM-IND-SDEU
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 6
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-IND-SDEU-LEN IN CPC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPC.
           PERFORM VAL-FRM-SGV-TINT
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 6
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-TINT-LEN IN CPC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPC.
           PERFORM VAL-FRM-SGV-INSO
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 6
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-INSO-LEN IN CPC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPC.
*% INCLUDE FIN_VAL_FLD_CPC OF 'COLBMCPC.TXT'
       FIN-VAL-FLD-CPC.
           EXIT.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
