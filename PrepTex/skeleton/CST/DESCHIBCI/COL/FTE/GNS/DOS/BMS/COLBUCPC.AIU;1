*% FRM = "CPC"
      *
      * COLBUCPC
      * Version 3.0.1 Nov-2005
      * 23-Jan-13 10:44 AM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMCPC.TXT'
       SET-TAG-COLBUCPC SECTION.
       INI-SET-TAG-COLBUCPC.
           MOVE
           '<IDNMOD NOM=$COLBUCPC$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$23-Jan-13 10:44 AM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCPC.
           EXIT.
           
       RET-ALL-CPC SECTION.
       INI-RET-ALL-CPC.
*% INCLUDE INI_RET_ALL_CPC OF 'COLBMCPC.TXT'
           PERFORM KEY-ALL-CPC.
           PERFORM RET-DBC-CPC.
*% INCLUDE FIN_RET_ALL_CPC OF 'COLBMCPC.TXT'
       FIN-RET-ALL-CPC.
           EXIT.
       
       RET-DBC-CPC SECTION.
       INI-RET-DBC-CPC.
*% INCLUDE INI_RET_DBC_CPC OF 'COLBMCPC.TXT'
*% INCLUDE FIN_RET_DBC_CPC OF 'COLBMCPC.TXT'
       FIN-RET-DBC-CPC.
           EXIT.
       
       KEY-ALL-CPC SECTION.
       INI-KEY-ALL-CPC.
*% INCLUDE INI_KEY_ALL_CPC OF 'COLBMCPC.TXT'
           PERFORM KEY-DBC-CPC.
*% INCLUDE FIN_KEY_ALL_CPC OF 'COLBMCPC.TXT'
       FIN-KEY-ALL-CPC.
           EXIT.
       
       KEY-DBC-CPC SECTION.
       INI-KEY-DBC-CPC.
*% INCLUDE INI_KEY_DBC_CPC OF 'COLBMCPC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN CPC-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN CPC-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN CPC-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN CPC-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_KEY_DBC_CPC OF 'COLBMCPC.TXT'
       FIN-KEY-DBC-CPC.
           EXIT.
       
       SCR-CLRA-CPC SECTION.
       INI-SCR-CLRA-CPC.
*% INCLUDE INI_SCR_CLRA_CPC OF 'COLBMCPC.TXT'
           PERFORM CLR-KEY-CPC.
           PERFORM CLR-FLD-CPC.
*% INCLUDE FIN_SCR_CLRA_CPC OF 'COLBMCPC.TXT'
       FIN-SCR-CLRA-CPC.
           EXIT.
       
       CLR-KEY-CPC SECTION.
       INI-CLR-KEY-CPC.
*% INCLUDE INI_CLR_KEY_CPC OF 'COLBMCPC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN CPC-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN CPC-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN CPC-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN CPC-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-CAI-HMAS IN CPC-FLD.
           MOVE ZEROES TO FRM-IIC-HMAS IN CPC-FLD.
           MOVE SPACES TO FRM-IND-HMAS IN CPC-FLD.
*% INCLUDE FIN_CLR_KEY_CPC OF 'COLBMCPC.TXT'
       FIN-CLR-KEY-CPC.
           EXIT.
       
       CLR-FLD-CPC SECTION.
       INI-CLR-FLD-CPC.
*% INCLUDE INI_CLR_FLD_CPC OF 'COLBMCPC.TXT'
           MOVE SPACES TO FRM-GLS-NOMC IN CPC-FLD.
           MOVE ZEROES TO FRM-VAL-CCRE IN CPC-FLD.
           MOVE SPACES TO FRM-CAI-IOPC IN CPC-FLD(1).
           MOVE SPACES TO FRM-CAI-IOPC IN CPC-FLD(2).
           MOVE SPACES TO FRM-CAI-IOPC IN CPC-FLD(3).
           MOVE SPACES TO FRM-CAI-IOPC IN CPC-FLD(4).
           MOVE SPACES TO FRM-CAI-IOPC IN CPC-FLD(5).
           MOVE SPACES TO FRM-CAI-IOPC IN CPC-FLD(6).
           MOVE ZEROES TO FRM-IIC-IOPC IN CPC-FLD(1).
           MOVE ZEROES TO FRM-IIC-IOPC IN CPC-FLD(2).
           MOVE ZEROES TO FRM-IIC-IOPC IN CPC-FLD(3).
           MOVE ZEROES TO FRM-IIC-IOPC IN CPC-FLD(4).
           MOVE ZEROES TO FRM-IIC-IOPC IN CPC-FLD(5).
           MOVE ZEROES TO FRM-IIC-IOPC IN CPC-FLD(6).
           MOVE SPACES TO FRM-COD-COOC IN CPC-FLD(1).
           MOVE SPACES TO FRM-COD-COOC IN CPC-FLD(2).
           MOVE SPACES TO FRM-COD-COOC IN CPC-FLD(3).
           MOVE SPACES TO FRM-COD-COOC IN CPC-FLD(4).
           MOVE SPACES TO FRM-COD-COOC IN CPC-FLD(5).
           MOVE SPACES TO FRM-COD-COOC IN CPC-FLD(6).
           MOVE SPACES TO FRM-IND-CART IN CPC-FLD(1).
           MOVE SPACES TO FRM-IND-CART IN CPC-FLD(2).
           MOVE SPACES TO FRM-IND-CART IN CPC-FLD(3).
           MOVE SPACES TO FRM-IND-CART IN CPC-FLD(4).
           MOVE SPACES TO FRM-IND-CART IN CPC-FLD(5).
           MOVE SPACES TO FRM-IND-CART IN CPC-FLD(6).
           MOVE SPACES TO FRM-IND-CJUD IN CPC-FLD(1).
           MOVE SPACES TO FRM-IND-CJUD IN CPC-FLD(2).
           MOVE SPACES TO FRM-IND-CJUD IN CPC-FLD(3).
           MOVE SPACES TO FRM-IND-CJUD IN CPC-FLD(4).
           MOVE SPACES TO FRM-IND-CJUD IN CPC-FLD(5).
           MOVE SPACES TO FRM-IND-CJUD IN CPC-FLD(6).
           MOVE SPACES TO FRM-IND-ACEL IN CPC-FLD(1).
           MOVE SPACES TO FRM-IND-ACEL IN CPC-FLD(2).
           MOVE SPACES TO FRM-IND-ACEL IN CPC-FLD(3).
           MOVE SPACES TO FRM-IND-ACEL IN CPC-FLD(4).
           MOVE SPACES TO FRM-IND-ACEL IN CPC-FLD(5).
           MOVE SPACES TO FRM-IND-ACEL IN CPC-FLD(6).
           MOVE ZEROES TO FRM-NUM-AGNO IN CPC-FLD(1).
           MOVE ZEROES TO FRM-NUM-AGNO IN CPC-FLD(2).
           MOVE ZEROES TO FRM-NUM-AGNO IN CPC-FLD(3).
           MOVE ZEROES TO FRM-NUM-AGNO IN CPC-FLD(4).
           MOVE ZEROES TO FRM-NUM-AGNO IN CPC-FLD(5).
           MOVE ZEROES TO FRM-NUM-AGNO IN CPC-FLD(6).
           MOVE ZEROES TO FRM-VAL-CRED IN CPC-FLD(1).
           MOVE ZEROES TO FRM-VAL-CRED IN CPC-FLD(2).
           MOVE ZEROES TO FRM-VAL-CRED IN CPC-FLD(3).
           MOVE ZEROES TO FRM-VAL-CRED IN CPC-FLD(4).
           MOVE ZEROES TO FRM-VAL-CRED IN CPC-FLD(5).
           MOVE ZEROES TO FRM-VAL-CRED IN CPC-FLD(6).
           MOVE ZEROES TO FRM-IND-CIMP IN CPC-FLD(1).
           MOVE ZEROES TO FRM-IND-CIMP IN CPC-FLD(2).
           MOVE ZEROES TO FRM-IND-CIMP IN CPC-FLD(3).
           MOVE ZEROES TO FRM-IND-CIMP IN CPC-FLD(4).
           MOVE ZEROES TO FRM-IND-CIMP IN CPC-FLD(5).
           MOVE ZEROES TO FRM-IND-CIMP IN CPC-FLD(6).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCUR"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FCUR IN CPC-FLD(1).
           MOVE ZEROES TO FRM-FEC-FCUR IN CPC-FLD(2).
           MOVE ZEROES TO FRM-FEC-FCUR IN CPC-FLD(3).
           MOVE ZEROES TO FRM-FEC-FCUR IN CPC-FLD(4).
           MOVE ZEROES TO FRM-FEC-FCUR IN CPC-FLD(5).
           MOVE ZEROES TO FRM-FEC-FCUR IN CPC-FLD(6).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-SGV-COBR IN CPC-FLD(1).
           MOVE ZEROES TO FRM-SGV-COBR IN CPC-FLD(2).
           MOVE ZEROES TO FRM-SGV-COBR IN CPC-FLD(3).
           MOVE ZEROES TO FRM-SGV-COBR IN CPC-FLD(4).
           MOVE ZEROES TO FRM-SGV-COBR IN CPC-FLD(5).
           MOVE ZEROES TO FRM-SGV-COBR IN CPC-FLD(6).
           MOVE ZEROES TO FRM-SGV-MPAR IN CPC-FLD(1).
           MOVE ZEROES TO FRM-SGV-MPAR IN CPC-FLD(2).
           MOVE ZEROES TO FRM-SGV-MPAR IN CPC-FLD(3).
           MOVE ZEROES TO FRM-SGV-MPAR IN CPC-FLD(4).
           MOVE ZEROES TO FRM-SGV-MPAR IN CPC-FLD(5).
           MOVE ZEROES TO FRM-SGV-MPAR IN CPC-FLD(6).
           MOVE SPACES TO FRM-IND-SDEU IN CPC-FLD(1).
           MOVE SPACES TO FRM-IND-SDEU IN CPC-FLD(2).
           MOVE SPACES TO FRM-IND-SDEU IN CPC-FLD(3).
           MOVE SPACES TO FRM-IND-SDEU IN CPC-FLD(4).
           MOVE SPACES TO FRM-IND-SDEU IN CPC-FLD(5).
           MOVE SPACES TO FRM-IND-SDEU IN CPC-FLD(6).
           MOVE ZEROES TO FRM-SGV-TINT IN CPC-FLD(1).
           MOVE ZEROES TO FRM-SGV-TINT IN CPC-FLD(2).
           MOVE ZEROES TO FRM-SGV-TINT IN CPC-FLD(3).
           MOVE ZEROES TO FRM-SGV-TINT IN CPC-FLD(4).
           MOVE ZEROES TO FRM-SGV-TINT IN CPC-FLD(5).
           MOVE ZEROES TO FRM-SGV-TINT IN CPC-FLD(6).
           MOVE ZEROES TO FRM-SGV-INSO IN CPC-FLD(1).
           MOVE ZEROES TO FRM-SGV-INSO IN CPC-FLD(2).
           MOVE ZEROES TO FRM-SGV-INSO IN CPC-FLD(3).
           MOVE ZEROES TO FRM-SGV-INSO IN CPC-FLD(4).
           MOVE ZEROES TO FRM-SGV-INSO IN CPC-FLD(5).
           MOVE ZEROES TO FRM-SGV-INSO IN CPC-FLD(6).
           PERFORM CLR-DBC-CPC.
*% INCLUDE FIN_CLR_FLD_CPC OF 'COLBMCPC.TXT'
       FIN-CLR-FLD-CPC.
           EXIT.
       
