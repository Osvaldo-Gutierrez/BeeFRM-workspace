       FIN-CLR-DBC-CPC.
           EXIT.
       
       FST-KEY-CPC SECTION.
       INI-FST-KEY-CPC.
*% INCLUDE INI_FST_KEY_CPC OF 'COLBMCPC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
           MOVE FRM-POS-CURS TO DBC-NUM-ICLI-LEN IN CPC-FLD.
      * VRF-FIN --> BMCHIBCI004.SRCAIU ( FST-KEY Y FST-FLD )
*% INCLUDE FIN_FST_KEY_CPC OF 'COLBMCPC.TXT'
       FIN-FST-KEY-CPC.
           EXIT.
       
       FST-FLD-CPC SECTION.
       INI-FST-FLD-CPC.
*% INCLUDE INI_FST_FLD_CPC OF 'COLBMCPC.TXT'
*% INCLUDE FIN_FST_FLD_CPC OF 'COLBMCPC.TXT'
       FIN-FST-FLD-CPC.
           EXIT.
       
       PRO-KEY-CPC SECTION.
       INI-PRO-KEY-CPC.
*% INCLUDE INI_PRO_KEY_CPC OF 'COLBMCPC.TXT'
           MOVE FRM-CPIM-AEY TO FRM-CAI-HMAS-ATR IN CPC-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IIC-HMAS-ATR IN CPC-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-HMAS-ATR IN CPC-FLD.
           PERFORM PRO-MKY-CPC.
*% INCLUDE FIN_PRO_KEY_CPC OF 'COLBMCPC.TXT'
       FIN-PRO-KEY-CPC.
           EXIT.
       
       PRO-MKY-CPC SECTION.
       INI-PRO-MKY-CPC.
*% INCLUDE INI_PRO_MKY_CPC OF 'COLBMCPC.TXT'
*% INCLUDE FIN_PRO_MKY_CPC OF 'COLBMCPC.TXT'
       FIN-PRO-MKY-CPC.
           EXIT.
       
       PRO-FLD-CPC SECTION.
       INI-PRO-FLD-CPC.
*% INCLUDE INI_PRO_FLD_CPC OF 'COLBMCPC.TXT'
           PERFORM PRO-DBC-CPC.
*% INCLUDE FIN_PRO_FLD_CPC OF 'COLBMCPC.TXT'
       FIN-PRO-FLD-CPC.
           EXIT.
       
       PRO-DBC-CPC SECTION.
       INI-PRO-DBC-CPC.
*% INCLUDE INI_PRO_DBC_CPC OF 'COLBMCPC.TXT'
*% INCLUDE FIN_PRO_DBC_CPC OF 'COLBMCPC.TXT'
       FIN-PRO-DBC-CPC.
           EXIT.
       
       UNP-KEY-CPC SECTION.
       INI-UNP-KEY-CPC.
*% INCLUDE INI_UNP_KEY_CPC OF 'COLBMCPC.TXT'
           MOVE FRM-CPIM-UBY TO FRM-CAI-HMAS-ATR IN CPC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IIC-HMAS-ATR IN CPC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IND-HMAS-ATR IN CPC-FLD.
           PERFORM UNP-IKY-CPC.
           PERFORM UNP-AKY-CPC.
           PERFORM UNP-MKY-CPC.
*% INCLUDE FIN_UNP_KEY_CPC OF 'COLBMCPC.TXT'
       FIN-UNP-KEY-CPC.
           EXIT.
       
       UNP-IKY-CPC SECTION.
       INI-UNP-IKY-CPC.
*% INCLUDE INI_UNP_IKY_CPC OF 'COLBMCPC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO DBC-NUM-ICLI-ATR IN CPC-FLD.
           MOVE FRM-CPIM-UBY TO DBC-VRF-ICLI-ATR IN CPC-FLD.
           MOVE FRM-CPIM-UBY TO DBC-IND-ICLI-ATR IN CPC-FLD.
           MOVE FRM-CPIM-UBY TO DBC-GLS-ICLI-ATR IN CPC-FLD.
      * IDC-FIN --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO FRM-CAI-HMAS-ATR.
           MOVE FRM-CPIM-UBY TO FRM-IIC-HMAS-ATR.
           MOVE FRM-CPIM-UBY TO FRM-IND-HMAS-ATR.
*% INCLUDE FIN_UNP_IKY_CPC OF 'COLBMCPC.TXT'
       FIN-UNP-IKY-CPC.
           EXIT.
       
       UNP-MKY-CPC SECTION.
       INI-UNP-MKY-CPC.
