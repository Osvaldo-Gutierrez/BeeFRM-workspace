       VAL-MKY-RDC SECTION.
       INI-VAL-MKY-RDC.
*% INCLUDE INI_VAL_MKY_RDC OF 'COLBMRDC.TXT'
*% INCLUDE FIN_VAL_MKY_RDC OF 'COLBMRDC.TXT'
       FIN-VAL-MKY-RDC.
           EXIT.
       
       VAL-FLD-RDC SECTION.
       INI-VAL-FLD-RDC.
*% INCLUDE INI_VAL_FLD_RDC OF 'COLBMRDC.TXT'
           PERFORM VAL-OPC-MSC-TVOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-MSC-TVOC-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-OPC-COD-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-TOOC-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-OPC-COD-OFOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-OFOC-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-FRM-GLS-TVOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TVOC-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-FRM-GLS-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TOOC-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-FRM-GLS-OFOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-OFOC-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-FRM-IND-TDEU.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IND-TDEU-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-RDC-NUM-DIRE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RDC-NUM-DIRE-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-RCD-GLS-DIRE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RCD-GLS-DIRE-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-RCD-COD-LUGA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RCD-COD-LUGA-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-RCD-GLS-FONO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RCD-GLS-FONO-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-FRM-GLS-LUGA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-LUGA-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-OPC-COD-COOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-COOC-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-RDC-VAL-LIMD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RDC-VAL-LIMD-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-FRM-GLS-COOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-COOC-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-OPC-COD-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-VCOC-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-FRM-VAL-MLIM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-MLIM-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-FRM-GLS-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-VCOC-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
*% INCLUDE FIN_VAL_FLD_RDC OF 'COLBMRDC.TXT'
       FIN-VAL-FLD-RDC.
           EXIT.
       
       VAL-OPC-MSC-TVOC SECTION.
       INI-VAL-OPC-MSC-TVOC.
*% LBL = "OPC_MSC_TVOC OF 'COLBMRDC.TXT'"
*% INCLUDE INI_OPC_MSC_TVOC OF 'COLBMRDC.TXT'
*% INCLUDE OPC_MSC_TVOC OF 'COLBMRDC.TXT'
*% INCLUDE FIN_OPC_MSC_TVOC OF 'COLBMRDC.TXT'
       FIN-VAL-OPC-MSC-TVOC.
           EXIT.
       
       VAL-OPC-COD-TOOC SECTION.
       INI-VAL-OPC-COD-TOOC.
