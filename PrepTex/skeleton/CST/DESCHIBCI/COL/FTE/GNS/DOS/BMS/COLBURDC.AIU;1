*% FRM = "RDC"
      *
      * COLBURDC
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:55 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMRDC.TXT'
       SET-TAG-COLBURDC SECTION.
       INI-SET-TAG-COLBURDC.
           MOVE
           '<IDNMOD NOM=$COLBURDC$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:55 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBURDC.
           EXIT.
           
       RET-ALL-RDC SECTION.
       INI-RET-ALL-RDC.
*% INCLUDE INI_RET_ALL_RDC OF 'COLBMRDC.TXT'
           PERFORM KEY-ALL-RDC.
           PERFORM RET-OPC-RDC.
           PERFORM RET-RDC-RDC.
           PERFORM RET-DBC-RDC.
           PERFORM RET-RCD-RDC.
*% INCLUDE FIN_RET_ALL_RDC OF 'COLBMRDC.TXT'
       FIN-RET-ALL-RDC.
           EXIT.
       
       RET-OPC-RDC SECTION.
       INI-RET-OPC-RDC.
*% INCLUDE INI_RET_OPC_RDC OF 'COLBMRDC.TXT'
           MOVE OPC-MSC-TVOC IN RDC-FLD TO OPC-MSC-TVOC IN OPC.
           MOVE OPC-COD-TOOC IN RDC-FLD TO OPC-COD-TOOC IN OPC.
           MOVE OPC-COD-OFOC IN RDC-FLD TO OPC-COD-OFOC IN OPC.
           MOVE OPC-COD-COOC IN RDC-FLD TO OPC-COD-COOC IN OPC.
           MOVE OPC-COD-VCOC IN RDC-FLD TO OPC-COD-VCOC IN OPC.
*% INCLUDE FIN_RET_OPC_RDC OF 'COLBMRDC.TXT'
       FIN-RET-OPC-RDC.
           EXIT.
       
       RET-RDC-RDC SECTION.
       INI-RET-RDC-RDC.
*% INCLUDE INI_RET_RDC_RDC OF 'COLBMRDC.TXT'
           MOVE RDC-NUM-DIRE IN RDC-FLD TO RDC-NUM-DIRE IN RDC.
           MOVE RDC-VAL-LIMD IN RDC-FLD TO RDC-VAL-LIMD IN RDC.
*% INCLUDE FIN_RET_RDC_RDC OF 'COLBMRDC.TXT'
       FIN-RET-RDC-RDC.
           EXIT.
       
       RET-DBC-RDC SECTION.
       INI-RET-DBC-RDC.
*% INCLUDE INI_RET_DBC_RDC OF 'COLBMRDC.TXT'
*% INCLUDE FIN_RET_DBC_RDC OF 'COLBMRDC.TXT'
       FIN-RET-DBC-RDC.
           EXIT.
       
       RET-RCD-RDC SECTION.
       INI-RET-RCD-RDC.
*% INCLUDE INI_RET_RCD_RDC OF 'COLBMRDC.TXT'
           MOVE RCD-GLS-DIRE IN RDC-FLD TO RCD-GLS-DIRE IN RCD.
           MOVE RCD-COD-LUGA IN RDC-FLD TO RCD-COD-LUGA IN RCD.
           MOVE RCD-GLS-FONO IN RDC-FLD TO RCD-GLS-FONO IN RCD.
*% INCLUDE FIN_RET_RCD_RDC OF 'COLBMRDC.TXT'
       FIN-RET-RCD-RDC.
           EXIT.
       
       KEY-ALL-RDC SECTION.
       INI-KEY-ALL-RDC.
*% INCLUDE INI_KEY_ALL_RDC OF 'COLBMRDC.TXT'
           PERFORM KEY-OPC-RDC.
           PERFORM KEY-RDC-RDC.
           PERFORM KEY-DBC-RDC.
           PERFORM KEY-RCD-RDC.
*% INCLUDE FIN_KEY_ALL_RDC OF 'COLBMRDC.TXT'
       FIN-KEY-ALL-RDC.
           EXIT.
       
       KEY-OPC-RDC SECTION.
       INI-KEY-OPC-RDC.
*% INCLUDE INI_KEY_OPC_RDC OF 'COLBMRDC.TXT'
*% INCLUDE FIN_KEY_OPC_RDC OF 'COLBMRDC.TXT'
       FIN-KEY-OPC-RDC.
           EXIT.
       
       KEY-RDC-RDC SECTION.
       INI-KEY-RDC-RDC.
*% INCLUDE INI_KEY_RDC_RDC OF 'COLBMRDC.TXT'
           MOVE RDC-CAI-IOPC IN RDC-FLD TO RDC-CAI-IOPC IN RDC.
           MOVE RDC-IIC-IOPC IN RDC-FLD TO RDC-IIC-IOPC IN RDC.
           MOVE RDC-NUM-IDLC IN RDC-FLD TO RDC-NUM-IDLC IN RDC.
           MOVE RDC-CAI-ICLI IN RDC-FLD TO RDC-CAI-ICLI IN RDC.
           MOVE RDC-IIC-ICLI IN RDC-FLD TO RDC-IIC-ICLI IN RDC.
*% INCLUDE FIN_KEY_RDC_RDC OF 'COLBMRDC.TXT'
       FIN-KEY-RDC-RDC.
           EXIT.
       
       KEY-DBC-RDC SECTION.
       INI-KEY-DBC-RDC.
*% INCLUDE INI_KEY_DBC_RDC OF 'COLBMRDC.TXT'
           MOVE DBC-GLS-NOMC IN RDC-FLD TO DBC-GLS-NOMC IN DBC.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN RDC-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN RDC-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN RDC-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN RDC-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_KEY_DBC_RDC OF 'COLBMRDC.TXT'
       FIN-KEY-DBC-RDC.
           EXIT.
       
       KEY-RCD-RDC SECTION.
       INI-KEY-RCD-RDC.
*% INCLUDE INI_KEY_RCD_RDC OF 'COLBMRDC.TXT'
*% INCLUDE FIN_KEY_RCD_RDC OF 'COLBMRDC.TXT'
       FIN-KEY-RCD-RDC.
           EXIT.
       
       SCR-CLRA-RDC SECTION.
       INI-SCR-CLRA-RDC.
*% INCLUDE INI_SCR_CLRA_RDC OF 'COLBMRDC.TXT'
           PERFORM CLR-KEY-RDC.
           PERFORM CLR-FLD-RDC.
*% INCLUDE FIN_SCR_CLRA_RDC OF 'COLBMRDC.TXT'
       FIN-SCR-CLRA-RDC.
           EXIT.
       
       CLR-KEY-RDC SECTION.
       INI-CLR-KEY-RDC.
*% INCLUDE INI_CLR_KEY_RDC OF 'COLBMRDC.TXT'
           MOVE SPACES TO RDC-CAI-IOPC IN RDC-FLD.
           MOVE ZEROES TO RDC-IIC-IOPC IN RDC-FLD.
           MOVE ZEROES TO RDC-NUM-IDLC IN RDC-FLD.
           MOVE SPACES TO DBC-GLS-NOMC IN RDC-FLD.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN RDC-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN RDC-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN RDC-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN RDC-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO RDC-CAI-ICLI IN RDC-FLD.
           MOVE SPACES TO RDC-IIC-ICLI IN RDC-FLD.
*% INCLUDE FIN_CLR_KEY_RDC OF 'COLBMRDC.TXT'
       FIN-CLR-KEY-RDC.
           EXIT.
       
       CLR-FLD-RDC SECTION.
       INI-CLR-FLD-RDC.
*% INCLUDE INI_CLR_FLD_RDC OF 'COLBMRDC.TXT'
           MOVE SPACES TO FRM-GLS-TVOC IN RDC-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN RDC-FLD.
           MOVE SPACES TO FRM-GLS-OFOC IN RDC-FLD.
           MOVE SPACES TO FRM-IND-TDEU IN RDC-FLD.
           MOVE SPACES TO FRM-GLS-LUGA IN RDC-FLD.
           MOVE SPACES TO FRM-GLS-COOC IN RDC-FLD.
           MOVE ZEROES TO FRM-VAL-MLIM IN RDC-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN RDC-FLD.
           PERFORM CLR-OPC-RDC.
           PERFORM CLR-RDC-RDC.
           PERFORM CLR-DBC-RDC.
           PERFORM CLR-RCD-RDC.
*% INCLUDE FIN_CLR_FLD_RDC OF 'COLBMRDC.TXT'
       FIN-CLR-FLD-RDC.
           EXIT.
       
       CLR-OPC-RDC SECTION.
       INI-CLR-OPC-RDC.
*% INCLUDE INI_CLR_OPC_RDC OF 'COLBMRDC.TXT'
