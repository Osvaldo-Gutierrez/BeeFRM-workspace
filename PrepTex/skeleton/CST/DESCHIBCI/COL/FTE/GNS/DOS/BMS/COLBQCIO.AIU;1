      *
      * COLBQCIO
      * Version 3.0.1 Nov-2005
      * 03-Dec-15 12:42 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CIO"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMCIO.TXT'
       SET-TAG-COLBQCIO SECTION.
       INI-SET-TAG-COLBQCIO.
           MOVE
           '<IDNMOD NOM=$COLBQCIO$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$03-Dec-15 12:42 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCIO.
           EXIT.
           
       PUT-ALL-CIO SECTION.
       INI-PUT-ALL-CIO.
*% INCLUDE INI_PUT_ALL_CIO OF 'COLBMCIO.TXT'
           PERFORM PUT-DBC-CIO.
           PERFORM PUT-CEN-CIO.
*% INCLUDE FIN_PUT_ALL_CIO OF 'COLBMCIO.TXT'
       FIN-PUT-ALL-CIO.
           EXIT.
       
       PUT-DBC-CIO SECTION.
       INI-PUT-DBC-CIO.
*% INCLUDE INI_PUT_DBC_CIO OF 'COLBMCIO.TXT'
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-CIO.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-CIO
               GO TO FIN-PUT-DBC-CIO.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN CIO-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN CIO-FLD.
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN CIO-FLD.
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN CIO-FLD.
      * IDC-FIN --> BMCHIBCI001210.SRCAIU 
*% INCLUDE FIN_PUT_DBC_CIO OF 'COLBMCIO.TXT'
       FIN-PUT-DBC-CIO.
           EXIT.
       
       PUT-CEN-CIO SECTION.
       INI-PUT-CEN-CIO.
*% INCLUDE INI_PUT_CEN_CIO OF 'COLBMCIO.TXT'
           IF PGM-STAT-CEN-NUL
               GO TO FIN-PUT-CEN-CIO.
           IF NOT PGM-STAT-CEN-OKS
               PERFORM CLR-CEN-CIO
               GO TO FIN-PUT-CEN-CIO.
           MOVE CEN-CAI-IENT IN CEN TO CEN-CAI-IENT IN CIO-FLD.
           MOVE CEN-IIC-IENT IN CEN TO CEN-IIC-IENT IN CIO-FLD.
           MOVE CEN-SGV-VAL1 IN CEN TO CEN-SGV-VAL1 IN CIO-FLD.
*% INCLUDE FIN_PUT_CEN_CIO OF 'COLBMCIO.TXT'
       FIN-PUT-CEN-CIO.
           EXIT.
       
       KEY-ALL-CIO SECTION.
       INI-KEY-ALL-CIO.
*% INCLUDE INI_KEY_ALL_CIO OF 'COLBMCIO.TXT'
           PERFORM KEY-DBC-CIO.
           PERFORM KEY-CEN-CIO.
*% INCLUDE FIN_KEY_ALL_CIO OF 'COLBMCIO.TXT'
       FIN-KEY-ALL-CIO.
           EXIT.
       
       KEY-DBC-CIO SECTION.
       INI-KEY-DBC-CIO.
*% INCLUDE INI_KEY_DBC_CIO OF 'COLBMCIO.TXT'
*% INCLUDE FIN_KEY_DBC_CIO OF 'COLBMCIO.TXT'
       FIN-KEY-DBC-CIO.
           EXIT.
       
       KEY-CEN-CIO SECTION.
       INI-KEY-CEN-CIO.
*% INCLUDE INI_KEY_CEN_CIO OF 'COLBMCIO.TXT'
           MOVE CEN-CAI-IENT IN CIO-FLD TO CEN-CAI-IENT IN CEN.
           MOVE CEN-IIC-IENT IN CIO-FLD TO CEN-IIC-IENT IN CEN.
*% INCLUDE FIN_KEY_CEN_CIO OF 'COLBMCIO.TXT'
       FIN-KEY-CEN-CIO.
           EXIT.
       
       SCR-CLRA-CIO SECTION.
       INI-SCR-CLRA-CIO.
*% INCLUDE INI_SCR_CLRA_CIO OF 'COLBMCIO.TXT'
           PERFORM CLR-KEY-CIO.
           PERFORM CLR-FLD-CIO.
*% INCLUDE FIN_SCR_CLRA_CIO OF 'COLBMCIO.TXT'
       FIN-SCR-CLRA-CIO.
           EXIT.
       
       CLR-KEY-CIO SECTION.
       INI-CLR-KEY-CIO.
*% INCLUDE INI_CLR_KEY_CIO OF 'COLBMCIO.TXT'
           MOVE SPACES TO CEN-CAI-IENT IN CIO-FLD.
           MOVE ZEROES TO CEN-IIC-IENT IN CIO-FLD.
*% INCLUDE FIN_CLR_KEY_CIO OF 'COLBMCIO.TXT'
       FIN-CLR-KEY-CIO.
           EXIT.
       
       CLR-FLD-CIO SECTION.
       INI-CLR-FLD-CIO.
*% INCLUDE INI_CLR_FLD_CIO OF 'COLBMCIO.TXT'
           MOVE SPACES TO FRM-GLS-NOMB IN CIO-FLD.
           MOVE SPACES TO FRM-GLS-GLS1 IN CIO-FLD(1).
           MOVE SPACES TO FRM-GLS-GLS1 IN CIO-FLD(2).
           MOVE SPACES TO FRM-GLS-GLS1 IN CIO-FLD(3).
           MOVE SPACES TO FRM-GLS-GLS1 IN CIO-FLD(4).
           MOVE SPACES TO FRM-GLS-GLS1 IN CIO-FLD(5).
           MOVE SPACES TO FRM-GLS-GLS1 IN CIO-FLD(6).
           MOVE SPACES TO FRM-GLS-GLS1 IN CIO-FLD(7).
           MOVE SPACES TO FRM-GLS-GLS1 IN CIO-FLD(8).
           MOVE SPACES TO FRM-GLS-GLS1 IN CIO-FLD(9).
           MOVE SPACES TO FRM-GLS-GLS1 IN CIO-FLD(10).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FMOR"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FMOR IN CIO-FLD(1).
           MOVE ZEROES TO FRM-FEC-FMOR IN CIO-FLD(2).
           MOVE ZEROES TO FRM-FEC-FMOR IN CIO-FLD(3).
           MOVE ZEROES TO FRM-FEC-FMOR IN CIO-FLD(4).
           MOVE ZEROES TO FRM-FEC-FMOR IN CIO-FLD(5).
           MOVE ZEROES TO FRM-FEC-FMOR IN CIO-FLD(6).
           MOVE ZEROES TO FRM-FEC-FMOR IN CIO-FLD(7).
           MOVE ZEROES TO FRM-FEC-FMOR IN CIO-FLD(8).
           MOVE ZEROES TO FRM-FEC-FMOR IN CIO-FLD(9).
           MOVE ZEROES TO FRM-FEC-FMOR IN CIO-FLD(10).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-COD-TOPR IN CIO-FLD(1).
           MOVE SPACES TO FRM-COD-TOPR IN CIO-FLD(2).
           MOVE SPACES TO FRM-COD-TOPR IN CIO-FLD(3).
           MOVE SPACES TO FRM-COD-TOPR IN CIO-FLD(4).
           MOVE SPACES TO FRM-COD-TOPR IN CIO-FLD(5).
           MOVE SPACES TO FRM-COD-TOPR IN CIO-FLD(6).
           MOVE SPACES TO FRM-COD-TOPR IN CIO-FLD(7).
           MOVE SPACES TO FRM-COD-TOPR IN CIO-FLD(8).
           MOVE SPACES TO FRM-COD-TOPR IN CIO-FLD(9).
           MOVE SPACES TO FRM-COD-TOPR IN CIO-FLD(10).
           MOVE SPACES TO FRM-GLS-TOPE IN CIO-FLD(1).
           MOVE SPACES TO FRM-GLS-TOPE IN CIO-FLD(2).
           MOVE SPACES TO FRM-GLS-TOPE IN CIO-FLD(3).
           MOVE SPACES TO FRM-GLS-TOPE IN CIO-FLD(4).
           MOVE SPACES TO FRM-GLS-TOPE IN CIO-FLD(5).
           MOVE SPACES TO FRM-GLS-TOPE IN CIO-FLD(6).
           MOVE SPACES TO FRM-GLS-TOPE IN CIO-FLD(7).
           MOVE SPACES TO FRM-GLS-TOPE IN CIO-FLD(8).
           MOVE SPACES TO FRM-GLS-TOPE IN CIO-FLD(9).
           MOVE SPACES TO FRM-GLS-TOPE IN CIO-FLD(10).
           PERFORM CLR-DBC-CIO.
           PERFORM CLR-CEN-CIO.
*% INCLUDE FIN_CLR_FLD_CIO OF 'COLBMCIO.TXT'
       FIN-CLR-FLD-CIO.
           EXIT.
       
       CLR-DBC-CIO SECTION.
       INI-CLR-DBC-CIO.
*% INCLUDE INI_CLR_DBC_CIO OF 'COLBMCIO.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN CIO-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN CIO-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN CIO-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN CIO-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_DBC_CIO OF 'COLBMCIO.TXT'
       FIN-CLR-DBC-CIO.
           EXIT.
       
       CLR-CEN-CIO SECTION.
       INI-CLR-CEN-CIO.
*% INCLUDE INI_CLR_CEN_CIO OF 'COLBMCIO.TXT'
           MOVE ZEROES TO CEN-SGV-VAL1 IN CIO-FLD.
*% INCLUDE FIN_CLR_CEN_CIO OF 'COLBMCIO.TXT'
       FIN-CLR-CEN-CIO.
           EXIT.
       
       FST-KEY-CIO SECTION.
       INI-FST-KEY-CIO.
*% INCLUDE INI_FST_KEY_CIO OF 'COLBMCIO.TXT'
           MOVE -1 TO CEN-CAI-IENT-LEN IN CIO-FLD.
*% INCLUDE FIN_FST_KEY_CIO OF 'COLBMCIO.TXT'
       FIN-FST-KEY-CIO.
           EXIT.
       
       FST-FLD-CIO SECTION.
       INI-FST-FLD-CIO.
*% INCLUDE INI_FST_FLD_CIO OF 'COLBMCIO.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
           MOVE FRM-POS-CURS TO DBC-NUM-ICLI-LEN IN CIO-FLD.
      * VRF-FIN --> BMCHIBCI004.SRCAIU ( FST-KEY Y FST-FLD )
*% INCLUDE FIN_FST_FLD_CIO OF 'COLBMCIO.TXT'
       FIN-FST-FLD-CIO.
           EXIT.
       
       PRO-KEY-CIO SECTION.
       INI-PRO-KEY-CIO.
*% INCLUDE INI_PRO_KEY_CIO OF 'COLBMCIO.TXT'
           PERFORM PRO-IKY-CIO.
           PERFORM PRO-AKY-CIO.
           PERFORM PRO-MKY-CIO.
*% INCLUDE FIN_PRO_KEY_CIO OF 'COLBMCIO.TXT'
       FIN-PRO-KEY-CIO.
           EXIT.
       
       PRO-IKY-CIO SECTION.
       INI-PRO-IKY-CIO.
*% INCLUDE INI_PRO_IKY_CIO OF 'COLBMCIO.TXT'
           MOVE FRM-CPIM-AEY TO CEN-CAI-IENT-ATR IN CIO-FLD.
           MOVE FRM-CPIM-AEY TO CEN-IIC-IENT-ATR IN CIO-FLD.
*% INCLUDE FIN_PRO_IKY_CIO OF 'COLBMCIO.TXT'
       FIN-PRO-IKY-CIO.
           EXIT.
       
       PRO-MKY-CIO SECTION.
       INI-PRO-MKY-CIO.
*% INCLUDE INI_PRO_MKY_CIO OF 'COLBMCIO.TXT'
*% INCLUDE FIN_PRO_MKY_CIO OF 'COLBMCIO.TXT'
       FIN-PRO-MKY-CIO.
           EXIT.
       
       PRO-AKY-CIO SECTION.
       INI-PRO-AKY-CIO.
*% INCLUDE INI_PRO_AKY_CIO OF 'COLBMCIO.TXT'
*% INCLUDE FIN_PRO_AKY_CIO OF 'COLBMCIO.TXT'
       FIN-PRO-AKY-CIO.
           EXIT.
       
       UNP-KEY-CIO SECTION.
       INI-UNP-KEY-CIO.
*% INCLUDE INI_UNP_KEY_CIO OF 'COLBMCIO.TXT'
           PERFORM UNP-IKY-CIO.
           PERFORM UNP-AKY-CIO.
           PERFORM UNP-MKY-CIO.
*% INCLUDE FIN_UNP_KEY_CIO OF 'COLBMCIO.TXT'
       FIN-UNP-KEY-CIO.
           EXIT.
       
       UNP-IKY-CIO SECTION.
       INI-UNP-IKY-CIO.
*% INCLUDE INI_UNP_IKY_CIO OF 'COLBMCIO.TXT'
           MOVE FRM-CPIM-UBY TO CEN-CAI-IENT-ATR.
           MOVE FRM-CPIM-UBY TO CEN-IIC-IENT-ATR.
*% INCLUDE FIN_UNP_IKY_CIO OF 'COLBMCIO.TXT'
       FIN-UNP-IKY-CIO.
           EXIT.
       
       UNP-MKY-CIO SECTION.
       INI-UNP-MKY-CIO.
*% INCLUDE INI_UNP_MKY_CIO OF 'COLBMCIO.TXT'
*% INCLUDE FIN_UNP_MKY_CIO OF 'COLBMCIO.TXT'
       FIN-UNP-MKY-CIO.
           EXIT.
       
       UNP-AKY-CIO SECTION.
       INI-UNP-AKY-CIO.
*% INCLUDE INI_UNP_AKY_CIO OF 'COLBMCIO.TXT'
*% INCLUDE FIN_UNP_AKY_CIO OF 'COLBMCIO.TXT'
       FIN-UNP-AKY-CIO.
           EXIT.
       
       UNP-FLD-CIO SECTION.
       INI-UNP-FLD-CIO.
*% INCLUDE INI_UNP_FLD_CIO OF 'COLBMCIO.TXT'
           MOVE FRM-CPIM-UBY TO FRM-GLS-GLS1-ATR IN CIO-FLD(1).
           MOVE FRM-CPIM-UBY TO FRM-GLS-GLS1-ATR IN CIO-FLD(2).
           MOVE FRM-CPIM-UBY TO FRM-GLS-GLS1-ATR IN CIO-FLD(3).
           MOVE FRM-CPIM-UBY TO FRM-GLS-GLS1-ATR IN CIO-FLD(4).
           MOVE FRM-CPIM-UBY TO FRM-GLS-GLS1-ATR IN CIO-FLD(5).
           MOVE FRM-CPIM-UBY TO FRM-GLS-GLS1-ATR IN CIO-FLD(6).
           MOVE FRM-CPIM-UBY TO FRM-GLS-GLS1-ATR IN CIO-FLD(7).
           MOVE FRM-CPIM-UBY TO FRM-GLS-GLS1-ATR IN CIO-FLD(8).
           MOVE FRM-CPIM-UBY TO FRM-GLS-GLS1-ATR IN CIO-FLD(9).
           MOVE FRM-CPIM-UBY TO FRM-GLS-GLS1-ATR IN CIO-FLD(10).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FMOR"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FMOR-ATR IN CIO-FLD(1).
           MOVE FRM-CPIM-UBY TO FRM-FEC-FMOR-ATR IN CIO-FLD(2).
           MOVE FRM-CPIM-UBY TO FRM-FEC-FMOR-ATR IN CIO-FLD(3).
           MOVE FRM-CPIM-UBY TO FRM-FEC-FMOR-ATR IN CIO-FLD(4).
           MOVE FRM-CPIM-UBY TO FRM-FEC-FMOR-ATR IN CIO-FLD(5).
           MOVE FRM-CPIM-UBY TO FRM-FEC-FMOR-ATR IN CIO-FLD(6).
           MOVE FRM-CPIM-UBY TO FRM-FEC-FMOR-ATR IN CIO-FLD(7).
           MOVE FRM-CPIM-UBY TO FRM-FEC-FMOR-ATR IN CIO-FLD(8).
           MOVE FRM-CPIM-UBY TO FRM-FEC-FMOR-ATR IN CIO-FLD(9).
           MOVE FRM-CPIM-UBY TO FRM-FEC-FMOR-ATR IN CIO-FLD(10).
           MOVE FRM-CPIM-UBY TO FRM-COD-TOPR-ATR IN CIO-FLD(1).
           MOVE FRM-CPIM-UBY TO FRM-COD-TOPR-ATR IN CIO-FLD(2).
           MOVE FRM-CPIM-UBY TO FRM-COD-TOPR-ATR IN CIO-FLD(3).
           MOVE FRM-CPIM-UBY TO FRM-COD-TOPR-ATR IN CIO-FLD(4).
           MOVE FRM-CPIM-UBY TO FRM-COD-TOPR-ATR IN CIO-FLD(5).
           MOVE FRM-CPIM-UBY TO FRM-COD-TOPR-ATR IN CIO-FLD(6).
           MOVE FRM-CPIM-UBY TO FRM-COD-TOPR-ATR IN CIO-FLD(7).
           MOVE FRM-CPIM-UBY TO FRM-COD-TOPR-ATR IN CIO-FLD(8).
           MOVE FRM-CPIM-UBY TO FRM-COD-TOPR-ATR IN CIO-FLD(9).
           MOVE FRM-CPIM-UBY TO FRM-COD-TOPR-ATR IN CIO-FLD(10).
           PERFORM UNP-DBC-CIO.
           PERFORM UNP-CEN-CIO.
*% INCLUDE FIN_UNP_FLD_CIO OF 'COLBMCIO.TXT'
       FIN-UNP-FLD-CIO.
           EXIT.
       
       UNP-DBC-CIO SECTION.
       INI-UNP-DBC-CIO.
*% INCLUDE INI_UNP_DBC_CIO OF 'COLBMCIO.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO DBC-NUM-ICLI-ATR IN CIO-FLD.
           MOVE FRM-CPIM-UBY TO DBC-VRF-ICLI-ATR IN CIO-FLD.
           MOVE FRM-CPIM-UBY TO DBC-IND-ICLI-ATR IN CIO-FLD.
           MOVE FRM-CPIM-UBY TO DBC-GLS-ICLI-ATR IN CIO-FLD.
      * IDC-FIN --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
*% INCLUDE FIN_UNP_DBC_CIO OF 'COLBMCIO.TXT'
       FIN-UNP-DBC-CIO.
           EXIT.
       
       UNP-CEN-CIO SECTION.
       INI-UNP-CEN-CIO.
*% INCLUDE INI_UNP_CEN_CIO OF 'COLBMCIO.TXT'
           MOVE FRM-CPIM-UBY TO CEN-SGV-VAL1-ATR IN CIO-FLD.
*% INCLUDE FIN_UNP_CEN_CIO OF 'COLBMCIO.TXT'
       FIN-UNP-CEN-CIO.
           EXIT.
       
       VAL-KEY-CIO SECTION.
       INI-VAL-KEY-CIO.
           PERFORM VAL-IKY-CIO.
           PERFORM VAL-MKY-CIO.
           PERFORM VAL-AKY-CIO.
       FIN-VAL-KEY-CIO.
           EXIT.
       
       VAL-IKY-CIO SECTION.
       INI-VAL-IKY-CIO.
*% INCLUDE INI_VAL_IKY_CIO OF 'COLBMCIO.TXT'
           PERFORM VAL-CEN-CAI-IENT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CEN-CAI-IENT-LEN IN CIO-FLD
                GO TO FIN-VAL-IKY-CIO.
           PERFORM VAL-CEN-IIC-IENT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CEN-IIC-IENT-LEN IN CIO-FLD
                GO TO FIN-VAL-IKY-CIO.
*% INCLUDE FIN_VAL_IKY_CIO OF 'COLBMCIO.TXT'
       FIN-VAL-IKY-CIO.
           EXIT.
       
       VAL-MKY-CIO SECTION.
       INI-VAL-MKY-CIO.
*% INCLUDE INI_VAL_MKY_CIO OF 'COLBMCIO.TXT'
*% INCLUDE FIN_VAL_MKY_CIO OF 'COLBMCIO.TXT'
       FIN-VAL-MKY-CIO.
           EXIT.
       
       VAL-AKY-CIO SECTION.
       INI-VAL-AKY-CIO.
*% INCLUDE INI_VAL_AKY_CIO OF 'COLBMCIO.TXT'
*% INCLUDE FIN_VAL_AKY_CIO OF 'COLBMCIO.TXT'
       FIN-VAL-AKY-CIO.
           EXIT.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% IZR = "S"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% IZR = "S"
       
       VAL-CEN-CAI-IENT SECTION.
       INI-VAL-CEN-CAI-IENT.
*% LBL = "CEN_CAI_IENT OF 'COLBMCIO.TXT'"
*% INCLUDE INI_CEN_CAI_IENT OF 'COLBMCIO.TXT'
           IF CEN-CAI-IENT IN CIO-FLD NOT > SPACES
              MOVE SPACES TO CEN-CAI-IENT IN CIO-FLD.
*% INCLUDE CEN_CAI_IENT OF 'COLBMCIO.TXT'
*% INCLUDE FIN_CEN_CAI_IENT OF 'COLBMCIO.TXT'
       FIN-VAL-CEN-CAI-IENT.
           EXIT.
       
       VAL-CEN-IIC-IENT SECTION.
       INI-VAL-CEN-IIC-IENT.
*% LBL = "CEN_IIC_IENT OF 'COLBMCIO.TXT'"
*% INCLUDE INI_CEN_IIC_IENT OF 'COLBMCIO.TXT'
*% INCLUDE CEN_IIC_IENT OF 'COLBMCIO.TXT'
*% INCLUDE FIN_CEN_IIC_IENT OF 'COLBMCIO.TXT'
       FIN-VAL-CEN-IIC-IENT.
           EXIT.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FMOR"
*% IZR = "S"
       
       VAL-CON-KEY-CIO SECTION.
       INI-VAL-CON-KEY-CIO.
*% INCLUDE INI_VAL_CON_KEY_CIO OF 'COLBMCIO.TXT'
           IF     CEN-CAI-IENT IN CIO-FLD NOT > SPACES 
              AND CEN-IIC-IENT IN CIO-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CIO.
*% INCLUDE FIN_VAL_CON_KEY_CIO OF 'COLBMCIO.TXT'
       FIN-VAL-CON-KEY-CIO.
           EXIT.
       
       VAL-CON-IKY-CIO SECTION.
       INI-VAL-CON-IKY-CIO.
*% INCLUDE INI_VAL_CON_IKY_CIO OF 'COLBMCIO.TXT'
           IF     CEN-CAI-IENT IN CIO-FLD NOT > SPACES 
              AND CEN-IIC-IENT IN CIO-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CIO.
*% INCLUDE FIN_VAL_CON_IKY_CIO OF 'COLBMCIO.TXT'
       FIN-VAL-CON-IKY-CIO.
           EXIT.
       
       VAL-NUM-KEY-CIO SECTION.
       INI-VAL-NUM-KEY-CIO.
           PERFORM VAL-NUM-IKY-CIO.
           PERFORM VAL-NUM-MKY-CIO.
           PERFORM VAL-NUM-AKY-CIO.
       FIN-VAL-NUM-KEY-CIO.
           EXIT.
       
       VAL-NUM-IKY-CIO SECTION.
       INI-VAL-NUM-IKY-CIO.
*% INCLUDE INI_VAL_NUM_IKY_CIO OF 'COLBMCIO.TXT'
       VAL-NUM-CEN-IIC-IENT.
*% BUF = "CIO-FLD"
*% FLD = "CEN-IIC-IENT"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-CIO.
*% INCLUDE FIN_VAL_NUM_IKY_CIO OF 'COLBMCIO.TXT'
       FIN-VAL-NUM-IKY-CIO.
           EXIT.
       
       VAL-NUM-MKY-CIO SECTION.
       INI-VAL-NUM-MKY-CIO.
*% INCLUDE INI_VAL_NUM_MKY_CIO OF 'COLBMCIO.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_CIO OF 'COLBMCIO.TXT'
       FIN-VAL-NUM-MKY-CIO.
           EXIT.
       
       VAL-NUM-AKY-CIO SECTION.
       INI-VAL-NUM-AKY-CIO.
*% INCLUDE INI_VAL_NUM_AKY_CIO OF 'COLBMCIO.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_CIO OF 'COLBMCIO.TXT'
       FIN-VAL-NUM-AKY-CIO.
           EXIT.
       
       VAL-NUM-FLD-CIO SECTION.
       INI-VAL-NUM-FLD-CIO.
*% INCLUDE INI_VAL_NUM_FLD_CIO OF 'COLBMCIO.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% IZR = "S"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% IZR = "S"
      * IDC-INI --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
*% FLD = "DBC-NUM-ICLI"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% BUF = "CIO-FLD"
       VAL-NUM-{FLD}.
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-CIO.
      * IDC-FIN --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
       VAL-NUM-CEN-SGV-VAL1.
*% BUF = "CIO-FLD"
*% FLD = "CEN-SGV-VAL1"
*% ENT = "11"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-CIO.
*% INCLUDE FIN_VAL_NUM_FLD_CIO OF 'COLBMCIO.TXT'
       FIN-VAL-NUM-FLD-CIO.
           EXIT.
       
       EDT-KEY-CIO SECTION.
       INI-EDT-KEY-CIO.
           PERFORM EDT-IKY-CIO.
           PERFORM EDT-MKY-CIO.
           PERFORM EDT-AKY-CIO.
*% INCLUDE EDT_KEY_CIO OF 'COLBMCIO.TXT'
       FIN-EDT-KEY-CIO.
           EXIT.
       
       EDT-IKY-CIO SECTION.
       INI-EDT-IKY-CIO.
*% INCLUDE EDT_IKY_CIO OF 'COLBMCIO.TXT'
           IF CEN-IIC-IENT-LEN IN CIO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CEN-IIC-IENT-LEN IN CIO-FLD
                GO TO FIN-EDT-IKY-CIO.
           MOVE CEN-IIC-IENT IN CIO-FLD TO CEN-IIC-IENT-EDT IN CIO-EDT.
           MOVE CEN-IIC-IENT-EDT IN CIO-EDT TO 
                CEN-IIC-IENT-ALF IN CIO-FLD.
*% INCLUDE FIN_EDT_IKY_CIO OF 'COLBMCIO.TXT'
       FIN-EDT-IKY-CIO.
           EXIT.
       
       EDT-MKY-CIO SECTION.
       INI-EDT-MKY-CIO.
*% INCLUDE EDT_MKY_CIO OF 'COLBMCIO.TXT'
*% INCLUDE FIN_EDT_MKY_CIO OF 'COLBMCIO.TXT'
       FIN-EDT-MKY-CIO.
           EXIT.
       
       EDT-AKY-CIO SECTION.
       INI-EDT-AKY-CIO.
*% INCLUDE EDT_AKY_CIO OF 'COLBMCIO.TXT'
*% INCLUDE FIN_EDT_AKY_CIO OF 'COLBMCIO.TXT'
       FIN-EDT-AKY-CIO.
           EXIT.
       
       EDT-FLD-CIO SECTION.
       INI-EDT-FLD-CIO.
*% INCLUDE EDT_FLD_CIO OF 'COLBMCIO.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF DBC-NUM-ICLI-LEN IN CIO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DBC-NUM-ICLI-LEN IN CIO-FLD
                GO TO FIN-EDT-FLD-CIO.
           MOVE DBC-NUM-ICLI IN CIO-FLD TO
                DBC-NUM-ICLI-EDT IN CIO-EDT.
           MOVE DBC-NUM-ICLI-EDT IN CIO-EDT TO
                DBC-NUM-ICLI-ALF IN CIO-FLD.
      * IDC-FIN --> BMCHIBCI011.SRCAIU EDICION NUMERICA
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FMOR"
           IF CEN-SGV-VAL1-LEN IN CIO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CEN-SGV-VAL1-LEN IN CIO-FLD
                GO TO FIN-EDT-FLD-CIO.
           MOVE CEN-SGV-VAL1 IN CIO-FLD TO CEN-SGV-VAL1-EDT IN CIO-EDT.
           MOVE CEN-SGV-VAL1-EDT IN CIO-EDT TO 
                CEN-SGV-VAL1-ALF IN CIO-FLD.
*% INCLUDE FIN_EDT_FLD_CIO OF 'COLBMCIO.TXT'
       FIN-EDT-FLD-CIO.
           EXIT.
