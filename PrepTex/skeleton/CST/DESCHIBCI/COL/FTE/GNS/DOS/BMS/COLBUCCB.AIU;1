       PRO-OPC-CCB SECTION.
       INI-PRO-OPC-CCB.
*% INCLUDE INI_PRO_OPC_CCB OF 'COLBMCCB.TXT'
*% INCLUDE FIN_PRO_OPC_CCB OF 'COLBMCCB.TXT'
       FIN-PRO-OPC-CCB.
           EXIT.
       
       PRO-CCB-CCB SECTION.
       INI-PRO-CCB-CCB.
*% INCLUDE INI_PRO_CCB_CCB OF 'COLBMCCB.TXT'
*% RREG = "CCB"
*% RREG_FRM = "CCB"
*% NOMB = "FCCB"
           MOVE FRM-CPIM-AEY TO CCB-FEC-FCCB-ATR IN CCB-FLD.
*% RREG = "CCB"
*% RREG_FRM = "CCB"
*% NOMB = "FING"
           MOVE FRM-CPIM-AEY TO CCB-FEC-FING-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO CCB-IND-SCBZ-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO CCB-SGV-GAST-ATR IN CCB-FLD.
*% INCLUDE FIN_PRO_CCB_CCB OF 'COLBMCCB.TXT'
       FIN-PRO-CCB-CCB.
           EXIT.
       
