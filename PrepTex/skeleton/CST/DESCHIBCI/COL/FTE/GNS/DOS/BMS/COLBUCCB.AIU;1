*% FRM = "CCB"
      *
      * COLBUCCB
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 03:20 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMCCB.TXT'
       SET-TAG-COLBUCCB SECTION.
       INI-SET-TAG-COLBUCCB.
           MOVE
           '<IDNMOD NOM=$COLBUCCB$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 03:20 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCCB.
           EXIT.
           
       RET-ALL-CCB SECTION.
       INI-RET-ALL-CCB.
*% INCLUDE INI_RET_ALL_CCB OF 'COLBMCCB.TXT'
           PERFORM KEY-ALL-CCB.
           PERFORM RET-OPC-CCB.
           PERFORM RET-CCB-CCB.
           PERFORM RET-DLC-CCB.
*% INCLUDE FIN_RET_ALL_CCB OF 'COLBMCCB.TXT'
       FIN-RET-ALL-CCB.
           EXIT.
       
       RET-OPC-CCB SECTION.
       INI-RET-OPC-CCB.
*% INCLUDE INI_RET_OPC_CCB OF 'COLBMCCB.TXT'
           MOVE OPC-MSC-TVOC IN CCB-FLD TO OPC-MSC-TVOC IN OPC.
           MOVE OPC-COD-TOOC IN CCB-FLD TO OPC-COD-TOOC IN OPC.
           MOVE OPC-COD-OFOC IN CCB-FLD TO OPC-COD-OFOC IN OPC.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE OPC-NUM-DCOL IN CCB-FLD TO OPC-NUM-DCOL IN OPC.
           MOVE OPC-NUM-MCOL IN CCB-FLD TO OPC-NUM-MCOL IN OPC.
           MOVE OPC-NUM-SCOL IN CCB-FLD TO OPC-NUM-SCOL IN OPC.
           MOVE OPC-NUM-ACOL IN CCB-FLD TO OPC-NUM-ACOL IN OPC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE OPC-COD-VCOC IN CCB-FLD TO OPC-COD-VCOC IN OPC.
*% INCLUDE FIN_RET_OPC_CCB OF 'COLBMCCB.TXT'
       FIN-RET-OPC-CCB.
           EXIT.
       
       RET-CCB-CCB SECTION.
       INI-RET-CCB-CCB.
*% INCLUDE INI_RET_CCB_CCB OF 'COLBMCCB.TXT'
           MOVE CCB-IND-SCBA IN CCB-FLD TO CCB-IND-SCBA IN CCB.
*% RREG = "CCB"
*% RREG_FRM = "CCB"
*% NOMB = "FCCB"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CCB-NUM-DCCB IN CCB-FLD TO CCB-NUM-DCCB IN CCB.
           MOVE CCB-NUM-MCCB IN CCB-FLD TO CCB-NUM-MCCB IN CCB.
           MOVE CCB-NUM-SCCB IN CCB-FLD TO CCB-NUM-SCCB IN CCB.
           MOVE CCB-NUM-ACCB IN CCB-FLD TO CCB-NUM-ACCB IN CCB.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% RREG = "CCB"
*% RREG_FRM = "CCB"
*% NOMB = "FING"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CCB-NUM-DING IN CCB-FLD TO CCB-NUM-DING IN CCB.
           MOVE CCB-NUM-MING IN CCB-FLD TO CCB-NUM-MING IN CCB.
           MOVE CCB-NUM-SING IN CCB-FLD TO CCB-NUM-SING IN CCB.
           MOVE CCB-NUM-AING IN CCB-FLD TO CCB-NUM-AING IN CCB.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE CCB-IND-SCBZ IN CCB-FLD TO CCB-IND-SCBZ IN CCB.
           MOVE CCB-COD-RCCB IN CCB-FLD TO CCB-COD-RCCB IN CCB.
           MOVE CCB-SGV-GAST IN CCB-FLD TO CCB-SGV-GAST IN CCB.
*% INCLUDE FIN_RET_CCB_CCB OF 'COLBMCCB.TXT'
       FIN-RET-CCB-CCB.
           EXIT.
       
       RET-DLC-CCB SECTION.
       INI-RET-DLC-CCB.
*% INCLUDE INI_RET_DLC_CCB OF 'COLBMCCB.TXT'
           MOVE DLC-VAL-SDLC IN CCB-FLD TO DLC-VAL-SDLC IN DLC.
           MOVE DLC-COD-UBFD IN CCB-FLD TO DLC-COD-UBFD IN DLC.
           MOVE DLC-IND-SCTB IN CCB-FLD TO DLC-IND-SCTB IN DLC.
           MOVE DLC-IND-SCBZ IN CCB-FLD TO DLC-IND-SCBZ IN DLC.
*% INCLUDE FIN_RET_DLC_CCB OF 'COLBMCCB.TXT'
       FIN-RET-DLC-CCB.
           EXIT.
       
       KEY-ALL-CCB SECTION.
       INI-KEY-ALL-CCB.
*% INCLUDE INI_KEY_ALL_CCB OF 'COLBMCCB.TXT'
           PERFORM KEY-OPC-CCB.
           PERFORM KEY-CCB-CCB.
           PERFORM KEY-DLC-CCB.
*% INCLUDE FIN_KEY_ALL_CCB OF 'COLBMCCB.TXT'
       FIN-KEY-ALL-CCB.
           EXIT.
       
       KEY-OPC-CCB SECTION.
       INI-KEY-OPC-CCB.
*% INCLUDE INI_KEY_OPC_CCB OF 'COLBMCCB.TXT'
*% INCLUDE FIN_KEY_OPC_CCB OF 'COLBMCCB.TXT'
       FIN-KEY-OPC-CCB.
           EXIT.
       
       KEY-CCB-CCB SECTION.
       INI-KEY-CCB-CCB.
*% INCLUDE INI_KEY_CCB_CCB OF 'COLBMCCB.TXT'
           MOVE CCB-CAI-IOPC IN CCB-FLD TO CCB-CAI-IOPC IN CCB.
           MOVE CCB-IIC-IOPC IN CCB-FLD TO CCB-IIC-IOPC IN CCB.
           MOVE CCB-NUM-IDLC IN CCB-FLD TO CCB-NUM-IDLC IN CCB.
           MOVE CCB-NUM-ICCB IN CCB-FLD TO CCB-NUM-ICCB IN CCB.
*% INCLUDE FIN_KEY_CCB_CCB OF 'COLBMCCB.TXT'
       FIN-KEY-CCB-CCB.
           EXIT.
       
       KEY-DLC-CCB SECTION.
       INI-KEY-DLC-CCB.
*% INCLUDE INI_KEY_DLC_CCB OF 'COLBMCCB.TXT'
*% INCLUDE FIN_KEY_DLC_CCB OF 'COLBMCCB.TXT'
       FIN-KEY-DLC-CCB.
           EXIT.
       
       SCR-CLRA-CCB SECTION.
       INI-SCR-CLRA-CCB.
*% INCLUDE INI_SCR_CLRA_CCB OF 'COLBMCCB.TXT'
           PERFORM CLR-KEY-CCB.
           PERFORM CLR-FLD-CCB.
*% INCLUDE FIN_SCR_CLRA_CCB OF 'COLBMCCB.TXT'
       FIN-SCR-CLRA-CCB.
           EXIT.
       
       CLR-KEY-CCB SECTION.
       INI-CLR-KEY-CCB.
*% INCLUDE INI_CLR_KEY_CCB OF 'COLBMCCB.TXT'
           MOVE SPACES TO CCB-CAI-IOPC IN CCB-FLD.
           MOVE ZEROES TO CCB-IIC-IOPC IN CCB-FLD.
           MOVE ZEROES TO CCB-NUM-IDLC IN CCB-FLD.
           MOVE ZEROES TO CCB-NUM-ICCB IN CCB-FLD.
           MOVE SPACES TO FRM-GLS-NOMC IN CCB-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-ICLI IN CCB-FLD.
           MOVE SPACES TO FRM-VRF-ICLI IN CCB-FLD.
           MOVE SPACES TO FRM-IND-ICLI IN CCB-FLD.
           MOVE SPACES TO FRM-GLS-ICLI IN CCB-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-CAI-ICLI IN CCB-FLD.
           MOVE SPACES TO FRM-IIC-ICLI IN CCB-FLD.
           MOVE SPACES TO FRM-GLS-NOMA IN CCB-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-IAVA IN CCB-FLD.
           MOVE SPACES TO FRM-VRF-IAVA IN CCB-FLD.
           MOVE SPACES TO FRM-IND-IAVA IN CCB-FLD.
           MOVE SPACES TO FRM-GLS-IAVA IN CCB-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-CAI-IAVA IN CCB-FLD.
           MOVE SPACES TO FRM-IIC-IAVA IN CCB-FLD.
*% INCLUDE FIN_CLR_KEY_CCB OF 'COLBMCCB.TXT'
       FIN-CLR-KEY-CCB.
           EXIT.
       
       CLR-FLD-CCB SECTION.
       INI-CLR-FLD-CCB.
*% INCLUDE INI_CLR_FLD_CCB OF 'COLBMCCB.TXT'
           MOVE SPACES TO FRM-GLS-TVOC IN CCB-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN CCB-FLD.
           MOVE SPACES TO FRM-GLS-OFOC IN CCB-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN CCB-FLD.
           MOVE SPACES TO FRM-GLS-UBFD IN CCB-FLD.
           MOVE SPACES TO FRM-GLS-SCTB IN CCB-FLD.
           MOVE SPACES TO FRM-GLS-SCBA IN CCB-FLD.
           MOVE SPACES TO FRM-GLS-SCBZ IN CCB-FLD.
           MOVE SPACES TO FRM-GLS-RCCB IN CCB-FLD.
           PERFORM CLR-OPC-CCB.
           PERFORM CLR-CCB-CCB.
           PERFORM CLR-DLC-CCB.
*% INCLUDE FIN_CLR_FLD_CCB OF 'COLBMCCB.TXT'
       FIN-CLR-FLD-CCB.
           EXIT.
       
       CLR-OPC-CCB SECTION.
       INI-CLR-OPC-CCB.
*% INCLUDE INI_CLR_OPC_CCB OF 'COLBMCCB.TXT'
           MOVE SPACES TO OPC-MSC-TVOC IN CCB-FLD.
           MOVE SPACES TO OPC-COD-TOOC IN CCB-FLD.
           MOVE SPACES TO OPC-COD-OFOC IN CCB-FLD.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO OPC-FEC-FCOL IN CCB-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO OPC-COD-VCOC IN CCB-FLD.
*% INCLUDE FIN_CLR_OPC_CCB OF 'COLBMCCB.TXT'
       FIN-CLR-OPC-CCB.
           EXIT.
       
       CLR-CCB-CCB SECTION.
       INI-CLR-CCB-CCB.
*% INCLUDE INI_CLR_CCB_CCB OF 'COLBMCCB.TXT'
           MOVE SPACES TO CCB-IND-SCBA IN CCB-FLD.
*% RREG = "CCB"
*% RREG_FRM = "CCB"
*% NOMB = "FCCB"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CCB-FEC-FCCB IN CCB-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "CCB"
*% RREG_FRM = "CCB"
*% NOMB = "FING"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CCB-FEC-FING IN CCB-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO CCB-IND-SCBZ IN CCB-FLD.
           MOVE SPACES TO CCB-COD-RCCB IN CCB-FLD.
           MOVE ZEROES TO CCB-SGV-GAST IN CCB-FLD.
*% INCLUDE FIN_CLR_CCB_CCB OF 'COLBMCCB.TXT'
       FIN-CLR-CCB-CCB.
           EXIT.
       
       CLR-DLC-CCB SECTION.
       INI-CLR-DLC-CCB.
*% INCLUDE INI_CLR_DLC_CCB OF 'COLBMCCB.TXT'
           MOVE ZEROES TO DLC-VAL-SDLC IN CCB-FLD.
           MOVE SPACES TO DLC-COD-UBFD IN CCB-FLD.
           MOVE SPACES TO DLC-IND-SCTB IN CCB-FLD.
           MOVE SPACES TO DLC-IND-SCBZ IN CCB-FLD.
*% INCLUDE FIN_CLR_DLC_CCB OF 'COLBMCCB.TXT'
       FIN-CLR-DLC-CCB.
           EXIT.
       
       FST-KEY-CCB SECTION.
       INI-FST-KEY-CCB.
*% INCLUDE INI_FST_KEY_CCB OF 'COLBMCCB.TXT'
           MOVE -1 TO CCB-CAI-IOPC-LEN IN CCB-FLD.
*% INCLUDE FIN_FST_KEY_CCB OF 'COLBMCCB.TXT'
       FIN-FST-KEY-CCB.
           EXIT.
       
       FST-FLD-CCB SECTION.
       INI-FST-FLD-CCB.
*% INCLUDE INI_FST_FLD_CCB OF 'COLBMCCB.TXT'
*% RREG = "CCB"
*% RREG_FRM = "CCB"
*% NOMB = "FCCB"
           MOVE FRM-POS-CURS TO CCB-FEC-FCCB-LEN IN CCB-FLD.
*% INCLUDE FIN_FST_FLD_CCB OF 'COLBMCCB.TXT'
       FIN-FST-FLD-CCB.
           EXIT.
       
       PRO-KEY-CCB SECTION.
       INI-PRO-KEY-CCB.
*% INCLUDE INI_PRO_KEY_CCB OF 'COLBMCCB.TXT'
           MOVE FRM-CPIM-AEY TO FRM-GLS-NOMC-ATR IN CCB-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-NUM-ICLI-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VRF-ICLI-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-ICLI-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-ICLI-ATR IN CCB-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-GLS-NOMA-ATR IN CCB-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-NUM-IAVA-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VRF-IAVA-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-IAVA-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-IAVA-ATR IN CCB-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           PERFORM PRO-MKY-CCB.
*% INCLUDE FIN_PRO_KEY_CCB OF 'COLBMCCB.TXT'
       FIN-PRO-KEY-CCB.
           EXIT.
       
       PRO-MKY-CCB SECTION.
       INI-PRO-MKY-CCB.
*% INCLUDE INI_PRO_MKY_CCB OF 'COLBMCCB.TXT'
*% INCLUDE FIN_PRO_MKY_CCB OF 'COLBMCCB.TXT'
       FIN-PRO-MKY-CCB.
           EXIT.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-NUM-ICLI-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VRF-ICLI-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-ICLI-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-ICLI-ATR IN CCB-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-NUM-IAVA-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VRF-IAVA-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-IAVA-ATR IN CCB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-IAVA-ATR IN CCB-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
       
       PRO-FLD-CCB SECTION.
       INI-PRO-FLD-CCB.
*% INCLUDE INI_PRO_FLD_CCB OF 'COLBMCCB.TXT'
           PERFORM PRO-OPC-CCB.
           PERFORM PRO-CCB-CCB.
