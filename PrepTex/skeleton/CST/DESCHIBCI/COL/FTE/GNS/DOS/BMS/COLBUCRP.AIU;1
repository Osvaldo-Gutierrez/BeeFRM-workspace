*% NOMB = "FVIN"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVTR"
*% INCLUDE FIN_EDT_MKY_CRP OF 'COLBMCRP.TXT'
       FIN-EDT-MKY-CRP.
           EXIT.
       
       EDT-AKY-CRP SECTION.
       INI-EDT-AKY-CRP.
