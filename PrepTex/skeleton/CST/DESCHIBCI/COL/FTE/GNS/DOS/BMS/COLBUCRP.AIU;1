*% FRM = "CRP"
      *
      * COLBUCRP
      * Version 3.0.1 Nov-2005
      * 11-Dec-12 12:43 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMCRP.TXT'
       SET-TAG-COLBUCRP SECTION.
       INI-SET-TAG-COLBUCRP.
           MOVE
           '<IDNMOD NOM=$COLBUCRP$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$11-Dec-12 12:43 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCRP.
           EXIT.
           
       RET-ALL-CRP SECTION.
       INI-RET-ALL-CRP.
*% INCLUDE INI_RET_ALL_CRP OF 'COLBMCRP.TXT'
           PERFORM KEY-ALL-CRP.
           PERFORM RET-PLL-CRP.
*% INCLUDE FIN_RET_ALL_CRP OF 'COLBMCRP.TXT'
       FIN-RET-ALL-CRP.
           EXIT.
       
       RET-PLL-CRP SECTION.
       INI-RET-PLL-CRP.
*% INCLUDE INI_RET_PLL_CRP OF 'COLBMCRP.TXT'
*% INCLUDE FIN_RET_PLL_CRP OF 'COLBMCRP.TXT'
       FIN-RET-PLL-CRP.
           EXIT.
       
       KEY-ALL-CRP SECTION.
       INI-KEY-ALL-CRP.
*% INCLUDE INI_KEY_ALL_CRP OF 'COLBMCRP.TXT'
           PERFORM KEY-PLL-CRP.
*% INCLUDE FIN_KEY_ALL_CRP OF 'COLBMCRP.TXT'
       FIN-KEY-ALL-CRP.
           EXIT.
       
       KEY-PLL-CRP SECTION.
       INI-KEY-PLL-CRP.
*% INCLUDE INI_KEY_PLL_CRP OF 'COLBMCRP.TXT'
           MOVE PLL-CAI-IPLL IN CRP-FLD TO PLL-CAI-IPLL IN PLL.
           MOVE PLL-IIC-IPLL IN CRP-FLD TO PLL-IIC-IPLL IN PLL.
           MOVE PLL-MSC-STAT IN CRP-FLD TO PLL-MSC-STAT IN PLL.
           MOVE PLL-IND-FPAG IN CRP-FLD TO PLL-IND-FPAG IN PLL.
*% INCLUDE FIN_KEY_PLL_CRP OF 'COLBMCRP.TXT'
       FIN-KEY-PLL-CRP.
           EXIT.
       
       SCR-CLRA-CRP SECTION.
       INI-SCR-CLRA-CRP.
*% INCLUDE INI_SCR_CLRA_CRP OF 'COLBMCRP.TXT'
           PERFORM CLR-KEY-CRP.
           PERFORM CLR-FLD-CRP.
*% INCLUDE FIN_SCR_CLRA_CRP OF 'COLBMCRP.TXT'
       FIN-SCR-CLRA-CRP.
           EXIT.
       
       CLR-KEY-CRP SECTION.
       INI-CLR-KEY-CRP.
*% INCLUDE INI_CLR_KEY_CRP OF 'COLBMCRP.TXT'
           MOVE SPACES TO PLL-CAI-IPLL IN CRP-FLD.
           MOVE ZEROES TO PLL-IIC-IPLL IN CRP-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPIN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FPIN IN CRP-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO PLL-MSC-STAT IN CRP-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPTR"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FPTR IN CRP-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO PLL-IND-FPAG IN CRP-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVIN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FVIN IN CRP-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-IND-SGTE IN CRP-FLD.
           MOVE SPACES TO FRM-CAI-YPLL IN CRP-FLD.
           MOVE SPACES TO FRM-IIC-YPLL IN CRP-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVTR"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FVTR IN CRP-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_KEY_CRP OF 'COLBMCRP.TXT'
       FIN-CLR-KEY-CRP.
           EXIT.
       
       CLR-FLD-CRP SECTION.
       INI-CLR-FLD-CRP.
*% INCLUDE INI_CLR_FLD_CRP OF 'COLBMCRP.TXT'
           MOVE SPACES TO FRM-GLS-STAT IN CRP-FLD.
           MOVE SPACES TO FRM-GLS-FPAG IN CRP-FLD.
           MOVE SPACES TO FRM-CAI-IPLL IN CRP-FLD(1).
           MOVE SPACES TO FRM-CAI-IPLL IN CRP-FLD(2).
           MOVE SPACES TO FRM-CAI-IPLL IN CRP-FLD(3).
           MOVE SPACES TO FRM-CAI-IPLL IN CRP-FLD(4).
           MOVE SPACES TO FRM-CAI-IPLL IN CRP-FLD(5).
           MOVE ZEROES TO FRM-IIC-IPLL IN CRP-FLD(1).
           MOVE ZEROES TO FRM-IIC-IPLL IN CRP-FLD(2).
           MOVE ZEROES TO FRM-IIC-IPLL IN CRP-FLD(3).
           MOVE ZEROES TO FRM-IIC-IPLL IN CRP-FLD(4).
           MOVE ZEROES TO FRM-IIC-IPLL IN CRP-FLD(5).
           MOVE ZEROES TO FRM-NUM-NREG IN CRP-FLD(1).
           MOVE ZEROES TO FRM-NUM-NREG IN CRP-FLD(2).
           MOVE ZEROES TO FRM-NUM-NREG IN CRP-FLD(3).
           MOVE ZEROES TO FRM-NUM-NREG IN CRP-FLD(4).
           MOVE ZEROES TO FRM-NUM-NREG IN CRP-FLD(5).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPAG"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FPAG IN CRP-FLD(1).
           MOVE ZEROES TO FRM-FEC-FPAG IN CRP-FLD(2).
           MOVE ZEROES TO FRM-FEC-FPAG IN CRP-FLD(3).
           MOVE ZEROES TO FRM-FEC-FPAG IN CRP-FLD(4).
           MOVE ZEROES TO FRM-FEC-FPAG IN CRP-FLD(5).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FVEN IN CRP-FLD(1).
           MOVE ZEROES TO FRM-FEC-FVEN IN CRP-FLD(2).
           MOVE ZEROES TO FRM-FEC-FVEN IN CRP-FLD(3).
           MOVE ZEROES TO FRM-FEC-FVEN IN CRP-FLD(4).
           MOVE ZEROES TO FRM-FEC-FVEN IN CRP-FLD(5).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-VAL-TOTA IN CRP-FLD(1).
           MOVE ZEROES TO FRM-VAL-TOTA IN CRP-FLD(2).
           MOVE ZEROES TO FRM-VAL-TOTA IN CRP-FLD(3).
           MOVE ZEROES TO FRM-VAL-TOTA IN CRP-FLD(4).
           MOVE ZEROES TO FRM-VAL-TOTA IN CRP-FLD(5).
           MOVE SPACES TO FRM-MSC-STAT IN CRP-FLD(1).
           MOVE SPACES TO FRM-MSC-STAT IN CRP-FLD(2).
           MOVE SPACES TO FRM-MSC-STAT IN CRP-FLD(3).
           MOVE SPACES TO FRM-MSC-STAT IN CRP-FLD(4).
           MOVE SPACES TO FRM-MSC-STAT IN CRP-FLD(5).
           MOVE SPACES TO FRM-IND-FPAG IN CRP-FLD(1).
           MOVE SPACES TO FRM-IND-FPAG IN CRP-FLD(2).
           MOVE SPACES TO FRM-IND-FPAG IN CRP-FLD(3).
           MOVE SPACES TO FRM-IND-FPAG IN CRP-FLD(4).
           MOVE SPACES TO FRM-IND-FPAG IN CRP-FLD(5).
           MOVE SPACES TO FRM-IND-TCAR IN CRP-FLD(1).
           MOVE SPACES TO FRM-IND-TCAR IN CRP-FLD(2).
           MOVE SPACES TO FRM-IND-TCAR IN CRP-FLD(3).
           MOVE SPACES TO FRM-IND-TCAR IN CRP-FLD(4).
           MOVE SPACES TO FRM-IND-TCAR IN CRP-FLD(5).
           MOVE SPACES TO FRM-CAI-ICYA IN CRP-FLD(1).
           MOVE SPACES TO FRM-CAI-ICYA IN CRP-FLD(2).
           MOVE SPACES TO FRM-CAI-ICYA IN CRP-FLD(3).
           MOVE SPACES TO FRM-CAI-ICYA IN CRP-FLD(4).
           MOVE SPACES TO FRM-CAI-ICYA IN CRP-FLD(5).
           MOVE SPACES TO FRM-IIC-ICYA IN CRP-FLD(1).
           MOVE SPACES TO FRM-IIC-ICYA IN CRP-FLD(2).
           MOVE SPACES TO FRM-IIC-ICYA IN CRP-FLD(3).
           MOVE SPACES TO FRM-IIC-ICYA IN CRP-FLD(4).
           MOVE SPACES TO FRM-IIC-ICYA IN CRP-FLD(5).
           MOVE SPACES TO FRM-CAI-ICVN IN CRP-FLD(1).
           MOVE SPACES TO FRM-CAI-ICVN IN CRP-FLD(2).
           MOVE SPACES TO FRM-CAI-ICVN IN CRP-FLD(3).
           MOVE SPACES TO FRM-CAI-ICVN IN CRP-FLD(4).
           MOVE SPACES TO FRM-CAI-ICVN IN CRP-FLD(5).
           MOVE ZEROES TO FRM-IIC-ICVN IN CRP-FLD(1).
           MOVE ZEROES TO FRM-IIC-ICVN IN CRP-FLD(2).
           MOVE ZEROES TO FRM-IIC-ICVN IN CRP-FLD(3).
           MOVE ZEROES TO FRM-IIC-ICVN IN CRP-FLD(4).
           MOVE ZEROES TO FRM-IIC-ICVN IN CRP-FLD(5).
           PERFORM CLR-PLL-CRP.
*% INCLUDE FIN_CLR_FLD_CRP OF 'COLBMCRP.TXT'
       FIN-CLR-FLD-CRP.
           EXIT.
       
       CLR-PLL-CRP SECTION.
       INI-CLR-PLL-CRP.
*% INCLUDE INI_CLR_PLL_CRP OF 'COLBMCRP.TXT'
*% INCLUDE FIN_CLR_PLL_CRP OF 'COLBMCRP.TXT'
       FIN-CLR-PLL-CRP.
           EXIT.
       
