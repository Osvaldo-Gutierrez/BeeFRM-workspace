       INI-FST-KEY-CRP.
*% INCLUDE INI_FST_KEY_CRP OF 'COLBMCRP.TXT'
*% INCLUDE FIN_FST_KEY_CRP OF 'COLBMCRP.TXT'
       FIN-FST-KEY-CRP.
           EXIT.
       
       FST-FLD-CRP SECTION.
       INI-FST-FLD-CRP.
*% INCLUDE INI_FST_FLD_CRP OF 'COLBMCRP.TXT'
           MOVE -1 TO PLL-CAI-IPLL-LEN IN CRP-FLD.
*% INCLUDE FIN_FST_FLD_CRP OF 'COLBMCRP.TXT'
       FIN-FST-FLD-CRP.
           EXIT.
       
       PRO-KEY-CRP SECTION.
       INI-PRO-KEY-CRP.
*% INCLUDE INI_PRO_KEY_CRP OF 'COLBMCRP.TXT'
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPIN"
           MOVE FRM-CPIM-AEY TO FRM-FEC-FPIN-ATR IN CRP-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPTR"
           MOVE FRM-CPIM-AEY TO FRM-FEC-FPTR-ATR IN CRP-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVIN"
           MOVE FRM-CPIM-AEY TO FRM-FEC-FVIN-ATR IN CRP-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-SGTE-ATR IN CRP-FLD.
           MOVE FRM-CPIM-AEY TO FRM-CAI-YPLL-ATR IN CRP-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IIC-YPLL-ATR IN CRP-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVTR"
           MOVE FRM-CPIM-AEY TO FRM-FEC-FVTR-ATR IN CRP-FLD.
           PERFORM PRO-MKY-CRP.
*% INCLUDE FIN_PRO_KEY_CRP OF 'COLBMCRP.TXT'
       FIN-PRO-KEY-CRP.
           EXIT.
       
       PRO-MKY-CRP SECTION.
       INI-PRO-MKY-CRP.
*% INCLUDE INI_PRO_MKY_CRP OF 'COLBMCRP.TXT'
           MOVE FRM-CPIM-AEY TO PLL-CAI-IPLL-ATR IN CRP-FLD.
           MOVE FRM-CPIM-AEY TO PLL-IIC-IPLL-ATR IN CRP-FLD.
           MOVE FRM-CPIM-AEY TO PLL-MSC-STAT-ATR IN CRP-FLD.
           MOVE FRM-CPIM-AEY TO PLL-IND-FPAG-ATR IN CRP-FLD.
*% INCLUDE FIN_PRO_MKY_CRP OF 'COLBMCRP.TXT'
       FIN-PRO-MKY-CRP.
           EXIT.
       
       PRO-FLD-CRP SECTION.
       INI-PRO-FLD-CRP.
*% INCLUDE INI_PRO_FLD_CRP OF 'COLBMCRP.TXT'
           PERFORM PRO-PLL-CRP.
*% INCLUDE FIN_PRO_FLD_CRP OF 'COLBMCRP.TXT'
       FIN-PRO-FLD-CRP.
           EXIT.
       
       PRO-PLL-CRP SECTION.
       INI-PRO-PLL-CRP.
*% INCLUDE INI_PRO_PLL_CRP OF 'COLBMCRP.TXT'
*% INCLUDE FIN_PRO_PLL_CRP OF 'COLBMCRP.TXT'
       FIN-PRO-PLL-CRP.
           EXIT.
       
       UNP-KEY-CRP SECTION.
       INI-UNP-KEY-CRP.
*% INCLUDE INI_UNP_KEY_CRP OF 'COLBMCRP.TXT'
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPIN"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FPIN-ATR IN CRP-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPTR"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FPTR-ATR IN CRP-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVIN"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FVIN-ATR IN CRP-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IND-SGTE-ATR IN CRP-FLD.
           MOVE FRM-CPIM-UBY TO FRM-CAI-YPLL-ATR IN CRP-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IIC-YPLL-ATR IN CRP-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVTR"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FVTR-ATR IN CRP-FLD.
           PERFORM UNP-IKY-CRP.
           PERFORM UNP-AKY-CRP.
           PERFORM UNP-MKY-CRP.
*% INCLUDE FIN_UNP_KEY_CRP OF 'COLBMCRP.TXT'
       FIN-UNP-KEY-CRP.
           EXIT.
       
       UNP-IKY-CRP SECTION.
       INI-UNP-IKY-CRP.
*% INCLUDE INI_UNP_IKY_CRP OF 'COLBMCRP.TXT'
*% INCLUDE FIN_UNP_IKY_CRP OF 'COLBMCRP.TXT'
       FIN-UNP-IKY-CRP.
           EXIT.
       
       UNP-MKY-CRP SECTION.
       INI-UNP-MKY-CRP.
*% INCLUDE INI_UNP_MKY_CRP OF 'COLBMCRP.TXT'
           MOVE FRM-CPIM-UBY TO PLL-CAI-IPLL-ATR.
           MOVE FRM-CPIM-UBY TO PLL-IIC-IPLL-ATR.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPIN"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FPIN-ATR IN CRP-FLD.
           MOVE FRM-CPIM-UBY TO PLL-MSC-STAT-ATR.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPTR"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FPTR-ATR IN CRP-FLD.
           MOVE FRM-CPIM-UBY TO PLL-IND-FPAG-ATR.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVIN"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FVIN-ATR IN CRP-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IND-SGTE-ATR.
           MOVE FRM-CPIM-UBY TO FRM-CAI-YPLL-ATR.
           MOVE FRM-CPIM-UBY TO FRM-IIC-YPLL-ATR.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVTR"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FVTR-ATR IN CRP-FLD.
*% INCLUDE FIN_UNP_MKY_CRP OF 'COLBMCRP.TXT'
       FIN-UNP-MKY-CRP.
           EXIT.
       
       UNP-AKY-CRP SECTION.
       INI-UNP-AKY-CRP.
*% INCLUDE INI_UNP_AKY_CRP OF 'COLBMCRP.TXT'
*% INCLUDE FIN_UNP_AKY_CRP OF 'COLBMCRP.TXT'
       FIN-UNP-AKY-CRP.
           EXIT.
       
       UNP-FLD-CRP SECTION.
       INI-UNP-FLD-CRP.
*% INCLUDE INI_UNP_FLD_CRP OF 'COLBMCRP.TXT'
           PERFORM UNP-PLL-CRP.
*% INCLUDE FIN_UNP_FLD_CRP OF 'COLBMCRP.TXT'
       FIN-UNP-FLD-CRP.
           EXIT.
       
       UNP-PLL-CRP SECTION.
       INI-UNP-PLL-CRP.
*% INCLUDE INI_UNP_PLL_CRP OF 'COLBMCRP.TXT'
*% INCLUDE FIN_UNP_PLL_CRP OF 'COLBMCRP.TXT'
       FIN-UNP-PLL-CRP.
           EXIT.
       
       VAL-MKY-CRP SECTION.
       INI-VAL-MKY-CRP.
*% INCLUDE INI_VAL_MKY_CRP OF 'COLBMCRP.TXT'
*% INCLUDE FIN_VAL_MKY_CRP OF 'COLBMCRP.TXT'
       FIN-VAL-MKY-CRP.
           EXIT.
       
       VAL-FLD-CRP SECTION.
       INI-VAL-FLD-CRP.
*% INCLUDE INI_VAL_FLD_CRP OF 'COLBMCRP.TXT'
           PERFORM VAL-PLL-CAI-IPLL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PLL-CAI-IPLL-LEN IN CRP-FLD
                GO TO FIN-VAL-FLD-CRP.
           PERFORM VAL-PLL-IIC-IPLL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PLL-IIC-IPLL-LEN IN CRP-FLD
                GO TO FIN-VAL-FLD-CRP.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPIN"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FPIN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FPIN-LEN IN CRP-FLD
                GO TO FIN-VAL-FLD-CRP.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-PLL-MSC-STAT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PLL-MSC-STAT-LEN IN CRP-FLD
                GO TO FIN-VAL-FLD-CRP.
           PERFORM VAL-FRM-GLS-STAT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-STAT-LEN IN CRP-FLD
                GO TO FIN-VAL-FLD-CRP.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPTR"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FPTR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FPTR-LEN IN CRP-FLD
                GO TO FIN-VAL-FLD-CRP.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-PLL-IND-FPAG.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PLL-IND-FPAG-LEN IN CRP-FLD
                GO TO FIN-VAL-FLD-CRP.
           PERFORM VAL-FRM-GLS-FPAG.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-FPAG-LEN IN CRP-FLD
                GO TO FIN-VAL-FLD-CRP.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVIN"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FVIN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FVIN-LEN IN CRP-FLD
                GO TO FIN-VAL-FLD-CRP.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-IND-SGTE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IND-SGTE-LEN IN CRP-FLD
                GO TO FIN-VAL-FLD-CRP.
           PERFORM VAL-FRM-CAI-YPLL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-YPLL-LEN IN CRP-FLD
                GO TO FIN-VAL-FLD-CRP.
           PERFORM VAL-FRM-IIC-YPLL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-YPLL-LEN IN CRP-FLD
                GO TO FIN-VAL-FLD-CRP.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVTR"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FVTR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FVTR-LEN IN CRP-FLD
                GO TO FIN-VAL-FLD-CRP.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-CAI-IPLL
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-CAI-IPLL-LEN IN CRP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CRP.
           PERFORM VAL-FRM-IIC-IPLL
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-IIC-IPLL-LEN IN CRP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CRP.
           PERFORM VAL-FRM-NUM-NREG
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-NUM-NREG-LEN IN CRP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CRP.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPAG"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FPAG VARYING FRM-IFLD
                FROM 1 BY 1 UNTIL FRM-IFLD > 5 OR
                FRM-SUAR = FRM-SUAR-MAL OR
                FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FPAG-LEN IN CRP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CRP.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FVEN VARYING FRM-IFLD
                FROM 1 BY 1 UNTIL FRM-IFLD > 5 OR
                FRM-SUAR = FRM-SUAR-MAL OR
                FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FVEN-LEN IN CRP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CRP.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-VAL-TOTA
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-TOTA-LEN IN CRP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CRP.
           PERFORM VAL-FRM-MSC-STAT
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-MSC-STAT-LEN IN CRP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CRP.
           PERFORM VAL-FRM-IND-FPAG
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-IND-FPAG-LEN IN CRP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CRP.
           PERFORM VAL-FRM-IND-TCAR
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-IND-TCAR-LEN IN CRP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CRP.
           PERFORM VAL-FRM-CAI-ICYA
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-CAI-ICYA-LEN IN CRP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CRP.
           PERFORM VAL-FRM-IIC-ICYA
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-IIC-ICYA-LEN IN CRP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CRP.
           PERFORM VAL-FRM-CAI-ICVN
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-CAI-ICVN-LEN IN CRP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CRP.
           PERFORM VAL-FRM-IIC-ICVN
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-IIC-ICVN-LEN IN CRP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CRP.
*% INCLUDE FIN_VAL_FLD_CRP OF 'COLBMCRP.TXT'
       FIN-VAL-FLD-CRP.
           EXIT.
       
