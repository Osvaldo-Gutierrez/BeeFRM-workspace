       VAL-KEY-CPP SECTION.
       INI-VAL-KEY-CPP.
           PERFORM VAL-IKY-CPP.
           PERFORM VAL-MKY-CPP.
           PERFORM VAL-AKY-CPP.
       FIN-VAL-KEY-CPP.
           EXIT.
       
       VAL-IKY-CPP SECTION.
       INI-VAL-IKY-CPP.
*% INCLUDE INI_VAL_IKY_CPP OF 'COLBMCPP.TXT'
           PERFORM VAL-CPP-COD-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CPP-COD-VCOC-LEN IN CPP-FLD
                GO TO FIN-VAL-IKY-CPP.
           PERFORM VAL-CPP-COD-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CPP-COD-TOOC-LEN IN CPP-FLD
                GO TO FIN-VAL-IKY-CPP.
           PERFORM VAL-CPP-COD-AOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CPP-COD-AOOC-LEN IN CPP-FLD
                GO TO FIN-VAL-IKY-CPP.
           PERFORM VAL-CPP-COD-MVTO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CPP-COD-MVTO-LEN IN CPP-FLD
                GO TO FIN-VAL-IKY-CPP.
           PERFORM VAL-CPP-IND-SCRT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CPP-IND-SCRT-LEN IN CPP-FLD
                GO TO FIN-VAL-IKY-CPP.
           PERFORM VAL-CPP-IND-SCTB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CPP-IND-SCTB-LEN IN CPP-FLD
                GO TO FIN-VAL-IKY-CPP.
*% INCLUDE FIN_VAL_IKY_CPP OF 'COLBMCPP.TXT'
       FIN-VAL-IKY-CPP.
           EXIT.
       
       VAL-MKY-CPP SECTION.
       INI-VAL-MKY-CPP.
*% INCLUDE INI_VAL_MKY_CPP OF 'COLBMCPP.TXT'
*% INCLUDE FIN_VAL_MKY_CPP OF 'COLBMCPP.TXT'
       FIN-VAL-MKY-CPP.
           EXIT.
       
       VAL-AKY-CPP SECTION.
       INI-VAL-AKY-CPP.
*% INCLUDE INI_VAL_AKY_CPP OF 'COLBMCPP.TXT'
*% INCLUDE FIN_VAL_AKY_CPP OF 'COLBMCPP.TXT'
       FIN-VAL-AKY-CPP.
           EXIT.
       
       VAL-CPP-COD-VCOC SECTION.
       INI-VAL-CPP-COD-VCOC.
*% LBL = "CPP_COD_VCOC OF 'COLBMCPP.TXT'"
*% INCLUDE INI_CPP_COD_VCOC OF 'COLBMCPP.TXT'
           IF CPP-COD-VCOC IN CPP-FLD NOT > SPACES
              MOVE SPACES TO CPP-COD-VCOC IN CPP-FLD.
*% INCLUDE CPP_COD_VCOC OF 'COLBMCPP.TXT'
*% BUF = "CPP-FLD"
*% SIS = "TAB"
*% TAB = "VLR "
*% FLD = "CPP-COD-VCOC"
*% GLS = "FRM-GLS-VCOC"
*% TIENE_GLOSA = TRUE
*% LGL = 20
*% INCLUDE 'VALCOD'
*% INCLUDE FIN_CPP_COD_VCOC OF 'COLBMCPP.TXT'
       FIN-VAL-CPP-COD-VCOC.
           EXIT.
       
       VAL-CPP-COD-TOOC SECTION.
       INI-VAL-CPP-COD-TOOC.
*% LBL = "CPP_COD_TOOC OF 'COLBMCPP.TXT'"
*% INCLUDE INI_CPP_COD_TOOC OF 'COLBMCPP.TXT'
           IF CPP-COD-TOOC IN CPP-FLD NOT > SPACES
              MOVE SPACES TO CPP-COD-TOOC IN CPP-FLD.
*% INCLUDE CPP_COD_TOOC OF 'COLBMCPP.TXT'
*% BUF = "CPP-FLD"
*% SIS = "TAB"
*% TAB = "TIO "
*% FLD = "CPP-COD-TOOC"
*% GLS = "FRM-GLS-TOOC"
*% TIENE_GLOSA = TRUE
*% LGL = 21
*% INCLUDE 'VALCOD'
*% INCLUDE FIN_CPP_COD_TOOC OF 'COLBMCPP.TXT'
       FIN-VAL-CPP-COD-TOOC.
           EXIT.
       
       VAL-CPP-COD-AOOC SECTION.
       INI-VAL-CPP-COD-AOOC.
*% LBL = "CPP_COD_AOOC OF 'COLBMCPP.TXT'"
*% INCLUDE INI_CPP_COD_AOOC OF 'COLBMCPP.TXT'
           IF CPP-COD-AOOC IN CPP-FLD NOT > SPACES
              MOVE SPACES TO CPP-COD-AOOC IN CPP-FLD.
*% INCLUDE CPP_COD_AOOC OF 'COLBMCPP.TXT'
*% INCLUDE FIN_CPP_COD_AOOC OF 'COLBMCPP.TXT'
       FIN-VAL-CPP-COD-AOOC.
           EXIT.
       
       VAL-CPP-COD-MVTO SECTION.
       INI-VAL-CPP-COD-MVTO.
*% LBL = "CPP_COD_MVTO OF 'COLBMCPP.TXT'"
*% INCLUDE INI_CPP_COD_MVTO OF 'COLBMCPP.TXT'
           IF CPP-COD-MVTO IN CPP-FLD NOT > SPACES
              MOVE SPACES TO CPP-COD-MVTO IN CPP-FLD.
*% INCLUDE CPP_COD_MVTO OF 'COLBMCPP.TXT'
