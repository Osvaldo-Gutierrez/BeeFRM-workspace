       VAL-CON-KEY-CPP SECTION.
       INI-VAL-CON-KEY-CPP.
*% INCLUDE INI_VAL_CON_KEY_CPP OF 'COLBMCPP.TXT'
           IF     CPP-COD-VCOC IN CPP-FLD NOT > SPACES 
              AND CPP-COD-TOOC IN CPP-FLD NOT > SPACES 
              AND CPP-COD-AOOC IN CPP-FLD NOT > SPACES 
              AND CPP-COD-MVTO IN CPP-FLD NOT > SPACES 
              AND CPP-IND-SCRT IN CPP-FLD NOT > SPACES 
              AND CPP-IND-SCTB IN CPP-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CPP.
*% INCLUDE FIN_VAL_CON_KEY_CPP OF 'COLBMCPP.TXT'
       FIN-VAL-CON-KEY-CPP.
           EXIT.
       
       VAL-CON-IKY-CPP SECTION.
       INI-VAL-CON-IKY-CPP.
*% INCLUDE INI_VAL_CON_IKY_CPP OF 'COLBMCPP.TXT'
           IF     CPP-COD-VCOC IN CPP-FLD NOT > SPACES 
              AND CPP-COD-TOOC IN CPP-FLD NOT > SPACES 
              AND CPP-COD-AOOC IN CPP-FLD NOT > SPACES 
              AND CPP-COD-MVTO IN CPP-FLD NOT > SPACES 
              AND CPP-IND-SCRT IN CPP-FLD NOT > SPACES 
              AND CPP-IND-SCTB IN CPP-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CPP.
*% INCLUDE FIN_VAL_CON_IKY_CPP OF 'COLBMCPP.TXT'
       FIN-VAL-CON-IKY-CPP.
           EXIT.
       
       VAL-NUM-KEY-CPP SECTION.
       INI-VAL-NUM-KEY-CPP.
           PERFORM VAL-NUM-IKY-CPP.
           PERFORM VAL-NUM-MKY-CPP.
           PERFORM VAL-NUM-AKY-CPP.
       FIN-VAL-NUM-KEY-CPP.
           EXIT.
       
       VAL-NUM-IKY-CPP SECTION.
       INI-VAL-NUM-IKY-CPP.
*% INCLUDE INI_VAL_NUM_IKY_CPP OF 'COLBMCPP.TXT'
