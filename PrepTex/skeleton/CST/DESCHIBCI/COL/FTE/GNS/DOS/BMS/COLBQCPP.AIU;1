*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CPP"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMCPP.TXT'
       SET-TAG-COLBQCPP SECTION.
       INI-SET-TAG-COLBQCPP.
           MOVE
           '<IDNMOD NOM=$COLBQCPP$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:31 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCPP.
           EXIT.
           
       PUT-ALL-CPP SECTION.
       INI-PUT-ALL-CPP.
*% INCLUDE INI_PUT_ALL_CPP OF 'COLBMCPP.TXT'
           PERFORM PUT-CPP-CPP.
*% INCLUDE FIN_PUT_ALL_CPP OF 'COLBMCPP.TXT'
       FIN-PUT-ALL-CPP.
           EXIT.
       
       PUT-CPP-CPP SECTION.
       INI-PUT-CPP-CPP.
*% INCLUDE INI_PUT_CPP_CPP OF 'COLBMCPP.TXT'
           IF PGM-STAT-CPP-NUL
               GO TO FIN-PUT-CPP-CPP.
           IF NOT PGM-STAT-CPP-OKS
               PERFORM CLR-CPP-CPP
               GO TO FIN-PUT-CPP-CPP.
           MOVE CPP-COD-VCOC IN CPP TO CPP-COD-VCOC IN CPP-FLD.
           MOVE CPP-COD-TOOC IN CPP TO CPP-COD-TOOC IN CPP-FLD.
           MOVE CPP-COD-AOOC IN CPP TO CPP-COD-AOOC IN CPP-FLD.
           MOVE CPP-COD-MVTO IN CPP TO CPP-COD-MVTO IN CPP-FLD.
           MOVE CPP-IND-SCRT IN CPP TO CPP-IND-SCRT IN CPP-FLD.
           MOVE CPP-IND-SCTB IN CPP TO CPP-IND-SCTB IN CPP-FLD.
