                FRM-VAL-SCAP-ALF IN PL1-FLD(7).
           IF FRM-VAL-SCAP-LEN IN PL1-FLD(8) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-SCAP-LEN IN PL1-FLD(8)
                GO TO FIN-EDT-FLD-PL1.
           MOVE FRM-VAL-SCAP IN PL1-FLD(8) TO 
                FRM-VAL-SCAP-EDT IN PL1-EDT.
           MOVE FRM-VAL-SCAP-EDT IN PL1-EDT TO 
                FRM-VAL-SCAP-ALF IN PL1-FLD(8).
           IF FRM-VAL-SCAP-LEN IN PL1-FLD(9) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-SCAP-LEN IN PL1-FLD(9)
                GO TO FIN-EDT-FLD-PL1.
           MOVE FRM-VAL-SCAP IN PL1-FLD(9) TO 
                FRM-VAL-SCAP-EDT IN PL1-EDT.
           MOVE FRM-VAL-SCAP-EDT IN PL1-EDT TO 
                FRM-VAL-SCAP-ALF IN PL1-FLD(9).
           IF FRM-VAL-TOTA-LEN IN PL1-FLD(1) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-TOTA-LEN IN PL1-FLD(1)
                GO TO FIN-EDT-FLD-PL1.
           MOVE FRM-VAL-TOTA IN PL1-FLD(1) TO 
                FRM-VAL-TOTA-EDT IN PL1-EDT.
           MOVE FRM-VAL-TOTA-EDT IN PL1-EDT TO 
                FRM-VAL-TOTA-ALF IN PL1-FLD(1).
           IF FRM-VAL-TOTA-LEN IN PL1-FLD(2) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-TOTA-LEN IN PL1-FLD(2)
                GO TO FIN-EDT-FLD-PL1.
           MOVE FRM-VAL-TOTA IN PL1-FLD(2) TO 
                FRM-VAL-TOTA-EDT IN PL1-EDT.
           MOVE FRM-VAL-TOTA-EDT IN PL1-EDT TO 
                FRM-VAL-TOTA-ALF IN PL1-FLD(2).
           IF FRM-VAL-TOTA-LEN IN PL1-FLD(3) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-TOTA-LEN IN PL1-FLD(3)
                GO TO FIN-EDT-FLD-PL1.
           MOVE FRM-VAL-TOTA IN PL1-FLD(3) TO 
                FRM-VAL-TOTA-EDT IN PL1-EDT.
           MOVE FRM-VAL-TOTA-EDT IN PL1-EDT TO 
                FRM-VAL-TOTA-ALF IN PL1-FLD(3).
           IF FRM-VAL-TOTA-LEN IN PL1-FLD(4) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-TOTA-LEN IN PL1-FLD(4)
                GO TO FIN-EDT-FLD-PL1.
           MOVE FRM-VAL-TOTA IN PL1-FLD(4) TO 
                FRM-VAL-TOTA-EDT IN PL1-EDT.
           MOVE FRM-VAL-TOTA-EDT IN PL1-EDT TO 
                FRM-VAL-TOTA-ALF IN PL1-FLD(4).
           IF FRM-VAL-TOTA-LEN IN PL1-FLD(5) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-TOTA-LEN IN PL1-FLD(5)
                GO TO FIN-EDT-FLD-PL1.
           MOVE FRM-VAL-TOTA IN PL1-FLD(5) TO 
                FRM-VAL-TOTA-EDT IN PL1-EDT.
           MOVE FRM-VAL-TOTA-EDT IN PL1-EDT TO 
                FRM-VAL-TOTA-ALF IN PL1-FLD(5).
           IF FRM-VAL-TOTA-LEN IN PL1-FLD(6) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-TOTA-LEN IN PL1-FLD(6)
                GO TO FIN-EDT-FLD-PL1.
           MOVE FRM-VAL-TOTA IN PL1-FLD(6) TO 
                FRM-VAL-TOTA-EDT IN PL1-EDT.
           MOVE FRM-VAL-TOTA-EDT IN PL1-EDT TO 
                FRM-VAL-TOTA-ALF IN PL1-FLD(6).
           IF FRM-VAL-TOTA-LEN IN PL1-FLD(7) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-TOTA-LEN IN PL1-FLD(7)
                GO TO FIN-EDT-FLD-PL1.
           MOVE FRM-VAL-TOTA IN PL1-FLD(7) TO 
                FRM-VAL-TOTA-EDT IN PL1-EDT.
           MOVE FRM-VAL-TOTA-EDT IN PL1-EDT TO 
                FRM-VAL-TOTA-ALF IN PL1-FLD(7).
           IF FRM-VAL-TOTA-LEN IN PL1-FLD(8) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-TOTA-LEN IN PL1-FLD(8)
                GO TO FIN-EDT-FLD-PL1.
           MOVE FRM-VAL-TOTA IN PL1-FLD(8) TO 
                FRM-VAL-TOTA-EDT IN PL1-EDT.
           MOVE FRM-VAL-TOTA-EDT IN PL1-EDT TO 
