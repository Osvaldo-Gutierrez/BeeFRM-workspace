*% FRM = "PL1"
      *
      * COLBUPL1
      * Version 3.0.1 Nov-2005
      * 25-Aug-16 11:36 AM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMPL1.TXT'
       SET-TAG-COLBUPL1 SECTION.
       INI-SET-TAG-COLBUPL1.
           MOVE
           '<IDNMOD NOM=$COLBUPL1$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$25-Aug-16 11:36 AM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUPL1.
           EXIT.
           
       RET-ALL-PL1 SECTION.
       INI-RET-ALL-PL1.
*% INCLUDE INI_RET_ALL_PL1 OF 'COLBMPL1.TXT'
           PERFORM KEY-ALL-PL1.
           PERFORM RET-OPC-PL1.
           PERFORM RET-DBC-PL1.
           PERFORM RET-ITR-PL1.
*% INCLUDE FIN_RET_ALL_PL1 OF 'COLBMPL1.TXT'
       FIN-RET-ALL-PL1.
           EXIT.
       
       RET-OPC-PL1 SECTION.
       INI-RET-OPC-PL1.
*% INCLUDE INI_RET_OPC_PL1 OF 'COLBMPL1.TXT'
           MOVE OPC-COD-VCOC IN PL1-FLD TO OPC-COD-VCOC IN OPC.
           MOVE OPC-COD-TOOC IN PL1-FLD TO OPC-COD-TOOC IN OPC.
           MOVE OPC-COD-AOOC IN PL1-FLD TO OPC-COD-AOOC IN OPC.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE OPC-NUM-DCOL IN PL1-FLD TO OPC-NUM-DCOL IN OPC.
           MOVE OPC-NUM-MCOL IN PL1-FLD TO OPC-NUM-MCOL IN OPC.
           MOVE OPC-NUM-SCOL IN PL1-FLD TO OPC-NUM-SCOL IN OPC.
           MOVE OPC-NUM-ACOL IN PL1-FLD TO OPC-NUM-ACOL IN OPC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE OPC-VAL-SEGD IN PL1-FLD TO OPC-VAL-SEGD IN OPC.
           MOVE OPC-SGV-LIQU IN PL1-FLD TO OPC-SGV-LIQU IN OPC.
*% INCLUDE FIN_RET_OPC_PL1 OF 'COLBMPL1.TXT'
       FIN-RET-OPC-PL1.
           EXIT.
       
       RET-DBC-PL1 SECTION.
       INI-RET-DBC-PL1.
*% INCLUDE INI_RET_DBC_PL1 OF 'COLBMPL1.TXT'
           MOVE DBC-GLS-NOMC IN PL1-FLD TO DBC-GLS-NOMC IN DBC.
*% INCLUDE FIN_RET_DBC_PL1 OF 'COLBMPL1.TXT'
       FIN-RET-DBC-PL1.
           EXIT.
       
       RET-ITR-PL1 SECTION.
       INI-RET-ITR-PL1.
*% INCLUDE INI_RET_ITR_PL1 OF 'COLBMPL1.TXT'
           MOVE ITR-SGV-TINT IN PL1-FLD TO ITR-SGV-TINT IN ITR.
*% INCLUDE FIN_RET_ITR_PL1 OF 'COLBMPL1.TXT'
       FIN-RET-ITR-PL1.
           EXIT.
       
       KEY-ALL-PL1 SECTION.
       INI-KEY-ALL-PL1.
*% INCLUDE INI_KEY_ALL_PL1 OF 'COLBMPL1.TXT'
           PERFORM KEY-OPC-PL1.
           PERFORM KEY-DBC-PL1.
           PERFORM KEY-ITR-PL1.
*% INCLUDE FIN_KEY_ALL_PL1 OF 'COLBMPL1.TXT'
       FIN-KEY-ALL-PL1.
           EXIT.
       
       KEY-OPC-PL1 SECTION.
       INI-KEY-OPC-PL1.
*% INCLUDE INI_KEY_OPC_PL1 OF 'COLBMPL1.TXT'
           MOVE OPC-CAI-IOPC IN PL1-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE OPC-IIC-IOPC IN PL1-FLD TO OPC-IIC-IOPC IN OPC.
*% INCLUDE FIN_KEY_OPC_PL1 OF 'COLBMPL1.TXT'
       FIN-KEY-OPC-PL1.
           EXIT.
       
       KEY-DBC-PL1 SECTION.
       INI-KEY-DBC-PL1.
*% INCLUDE INI_KEY_DBC_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_KEY_DBC_PL1 OF 'COLBMPL1.TXT'
       FIN-KEY-DBC-PL1.
           EXIT.
       
       KEY-ITR-PL1 SECTION.
       INI-KEY-ITR-PL1.
*% INCLUDE INI_KEY_ITR_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_KEY_ITR_PL1 OF 'COLBMPL1.TXT'
       FIN-KEY-ITR-PL1.
           EXIT.
       
       SCR-CLRA-PL1 SECTION.
       INI-SCR-CLRA-PL1.
*% INCLUDE INI_SCR_CLRA_PL1 OF 'COLBMPL1.TXT'
           PERFORM CLR-KEY-PL1.
           PERFORM CLR-FLD-PL1.
*% INCLUDE FIN_SCR_CLRA_PL1 OF 'COLBMPL1.TXT'
       FIN-SCR-CLRA-PL1.
           EXIT.
       
       CLR-KEY-PL1 SECTION.
       INI-CLR-KEY-PL1.
*% INCLUDE INI_CLR_KEY_PL1 OF 'COLBMPL1.TXT'
           MOVE SPACES TO OPC-CAI-IOPC IN PL1-FLD.
           MOVE ZEROES TO OPC-IIC-IOPC IN PL1-FLD.
*% INCLUDE FIN_CLR_KEY_PL1 OF 'COLBMPL1.TXT'
       FIN-CLR-KEY-PL1.
           EXIT.
       
       CLR-FLD-PL1 SECTION.
       INI-CLR-FLD-PL1.
*% INCLUDE INI_CLR_FLD_PL1 OF 'COLBMPL1.TXT'
           MOVE SPACES TO FRM-GLS-VEXP IN PL1-FLD.
           MOVE ZEROES TO FRM-VAL-CRED IN PL1-FLD.
           MOVE ZEROES TO FRM-VAL-INTE IN PL1-FLD.
           MOVE ZEROES TO FRM-VAL-VCOM IN PL1-FLD.
           MOVE ZEROES TO FRM-VAL-GAST IN PL1-FLD.
           MOVE ZEROES TO FRM-VAL-IMPU IN PL1-FLD.
           MOVE SPACES TO FRM-GLS-TINT IN PL1-FLD.
           MOVE ZEROES TO FRM-NUM-IVCT IN PL1-FLD(1).
           MOVE ZEROES TO FRM-NUM-IVCT IN PL1-FLD(2).
           MOVE ZEROES TO FRM-NUM-IVCT IN PL1-FLD(3).
           MOVE ZEROES TO FRM-NUM-IVCT IN PL1-FLD(4).
           MOVE ZEROES TO FRM-NUM-IVCT IN PL1-FLD(5).
           MOVE ZEROES TO FRM-NUM-IVCT IN PL1-FLD(6).
           MOVE ZEROES TO FRM-NUM-IVCT IN PL1-FLD(7).
           MOVE ZEROES TO FRM-NUM-IVCT IN PL1-FLD(8).
           MOVE ZEROES TO FRM-NUM-IVCT IN PL1-FLD(9).
           MOVE SPACES TO FRM-IND-CANC IN PL1-FLD(1).
           MOVE SPACES TO FRM-IND-CANC IN PL1-FLD(2).
           MOVE SPACES TO FRM-IND-CANC IN PL1-FLD(3).
           MOVE SPACES TO FRM-IND-CANC IN PL1-FLD(4).
           MOVE SPACES TO FRM-IND-CANC IN PL1-FLD(5).
           MOVE SPACES TO FRM-IND-CANC IN PL1-FLD(6).
           MOVE SPACES TO FRM-IND-CANC IN PL1-FLD(7).
           MOVE SPACES TO FRM-IND-CANC IN PL1-FLD(8).
           MOVE SPACES TO FRM-IND-CANC IN PL1-FLD(9).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FVEN IN PL1-FLD(1).
           MOVE ZEROES TO FRM-FEC-FVEN IN PL1-FLD(2).
           MOVE ZEROES TO FRM-FEC-FVEN IN PL1-FLD(3).
           MOVE ZEROES TO FRM-FEC-FVEN IN PL1-FLD(4).
           MOVE ZEROES TO FRM-FEC-FVEN IN PL1-FLD(5).
           MOVE ZEROES TO FRM-FEC-FVEN IN PL1-FLD(6).
           MOVE ZEROES TO FRM-FEC-FVEN IN PL1-FLD(7).
           MOVE ZEROES TO FRM-FEC-FVEN IN PL1-FLD(8).
           MOVE ZEROES TO FRM-FEC-FVEN IN PL1-FLD(9).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-VAL-INTC IN PL1-FLD(1).
           MOVE ZEROES TO FRM-VAL-INTC IN PL1-FLD(2).
           MOVE ZEROES TO FRM-VAL-INTC IN PL1-FLD(3).
           MOVE ZEROES TO FRM-VAL-INTC IN PL1-FLD(4).
           MOVE ZEROES TO FRM-VAL-INTC IN PL1-FLD(5).
           MOVE ZEROES TO FRM-VAL-INTC IN PL1-FLD(6).
           MOVE ZEROES TO FRM-VAL-INTC IN PL1-FLD(7).
           MOVE ZEROES TO FRM-VAL-INTC IN PL1-FLD(8).
           MOVE ZEROES TO FRM-VAL-INTC IN PL1-FLD(9).
           MOVE ZEROES TO FRM-VAL-SCAP IN PL1-FLD(1).
           MOVE ZEROES TO FRM-VAL-SCAP IN PL1-FLD(2).
           MOVE ZEROES TO FRM-VAL-SCAP IN PL1-FLD(3).
           MOVE ZEROES TO FRM-VAL-SCAP IN PL1-FLD(4).
           MOVE ZEROES TO FRM-VAL-SCAP IN PL1-FLD(5).
           MOVE ZEROES TO FRM-VAL-SCAP IN PL1-FLD(6).
           MOVE ZEROES TO FRM-VAL-SCAP IN PL1-FLD(7).
           MOVE ZEROES TO FRM-VAL-SCAP IN PL1-FLD(8).
           MOVE ZEROES TO FRM-VAL-SCAP IN PL1-FLD(9).
           MOVE ZEROES TO FRM-VAL-TOTA IN PL1-FLD(1).
           MOVE ZEROES TO FRM-VAL-TOTA IN PL1-FLD(2).
           MOVE ZEROES TO FRM-VAL-TOTA IN PL1-FLD(3).
           MOVE ZEROES TO FRM-VAL-TOTA IN PL1-FLD(4).
           MOVE ZEROES TO FRM-VAL-TOTA IN PL1-FLD(5).
           MOVE ZEROES TO FRM-VAL-TOTA IN PL1-FLD(6).
           MOVE ZEROES TO FRM-VAL-TOTA IN PL1-FLD(7).
           MOVE ZEROES TO FRM-VAL-TOTA IN PL1-FLD(8).
           MOVE ZEROES TO FRM-VAL-TOTA IN PL1-FLD(9).
           PERFORM CLR-OPC-PL1.
           PERFORM CLR-DBC-PL1.
           PERFORM CLR-ITR-PL1.
*% INCLUDE FIN_CLR_FLD_PL1 OF 'COLBMPL1.TXT'
       FIN-CLR-FLD-PL1.
           EXIT.
       
       CLR-OPC-PL1 SECTION.
       INI-CLR-OPC-PL1.
*% INCLUDE INI_CLR_OPC_PL1 OF 'COLBMPL1.TXT'
           MOVE SPACES TO OPC-COD-VCOC IN PL1-FLD.
           MOVE SPACES TO OPC-COD-TOOC IN PL1-FLD.
           MOVE SPACES TO OPC-COD-AOOC IN PL1-FLD.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO OPC-FEC-FCOL IN PL1-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO OPC-VAL-SEGD IN PL1-FLD.
           MOVE ZEROES TO OPC-SGV-LIQU IN PL1-FLD.
*% INCLUDE FIN_CLR_OPC_PL1 OF 'COLBMPL1.TXT'
       FIN-CLR-OPC-PL1.
           EXIT.
       
       CLR-DBC-PL1 SECTION.
       INI-CLR-DBC-PL1.
*% INCLUDE INI_CLR_DBC_PL1 OF 'COLBMPL1.TXT'
           MOVE SPACES TO DBC-GLS-NOMC IN PL1-FLD.
*% INCLUDE FIN_CLR_DBC_PL1 OF 'COLBMPL1.TXT'
       FIN-CLR-DBC-PL1.
           EXIT.
       
       CLR-ITR-PL1 SECTION.
       INI-CLR-ITR-PL1.
*% INCLUDE INI_CLR_ITR_PL1 OF 'COLBMPL1.TXT'
           MOVE ZEROES TO ITR-SGV-TINT IN PL1-FLD.
*% INCLUDE FIN_CLR_ITR_PL1 OF 'COLBMPL1.TXT'
       FIN-CLR-ITR-PL1.
           EXIT.
       
       FST-KEY-PL1 SECTION.
       INI-FST-KEY-PL1.
*% INCLUDE INI_FST_KEY_PL1 OF 'COLBMPL1.TXT'
           MOVE -1 TO OPC-CAI-IOPC-LEN IN PL1-FLD.
*% INCLUDE FIN_FST_KEY_PL1 OF 'COLBMPL1.TXT'
       FIN-FST-KEY-PL1.
           EXIT.
       
       FST-FLD-PL1 SECTION.
       INI-FST-FLD-PL1.
*% INCLUDE INI_FST_FLD_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_FST_FLD_PL1 OF 'COLBMPL1.TXT'
       FIN-FST-FLD-PL1.
           EXIT.
       
       PRO-KEY-PL1 SECTION.
       INI-PRO-KEY-PL1.
*% INCLUDE INI_PRO_KEY_PL1 OF 'COLBMPL1.TXT'
           PERFORM PRO-MKY-PL1.
*% INCLUDE FIN_PRO_KEY_PL1 OF 'COLBMPL1.TXT'
       FIN-PRO-KEY-PL1.
           EXIT.
       
       PRO-MKY-PL1 SECTION.
       INI-PRO-MKY-PL1.
*% INCLUDE INI_PRO_MKY_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_PRO_MKY_PL1 OF 'COLBMPL1.TXT'
       FIN-PRO-MKY-PL1.
           EXIT.
       
       PRO-FLD-PL1 SECTION.
       INI-PRO-FLD-PL1.
*% INCLUDE INI_PRO_FLD_PL1 OF 'COLBMPL1.TXT'
           PERFORM PRO-OPC-PL1.
           PERFORM PRO-DBC-PL1.
           PERFORM PRO-ITR-PL1.
*% INCLUDE FIN_PRO_FLD_PL1 OF 'COLBMPL1.TXT'
       FIN-PRO-FLD-PL1.
           EXIT.
       
       PRO-OPC-PL1 SECTION.
       INI-PRO-OPC-PL1.
*% INCLUDE INI_PRO_OPC_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_PRO_OPC_PL1 OF 'COLBMPL1.TXT'
       FIN-PRO-OPC-PL1.
           EXIT.
       
       PRO-DBC-PL1 SECTION.
       INI-PRO-DBC-PL1.
*% INCLUDE INI_PRO_DBC_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_PRO_DBC_PL1 OF 'COLBMPL1.TXT'
       FIN-PRO-DBC-PL1.
           EXIT.
       
       PRO-ITR-PL1 SECTION.
       INI-PRO-ITR-PL1.
*% INCLUDE INI_PRO_ITR_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_PRO_ITR_PL1 OF 'COLBMPL1.TXT'
       FIN-PRO-ITR-PL1.
           EXIT.
       
       UNP-KEY-PL1 SECTION.
       INI-UNP-KEY-PL1.
*% INCLUDE INI_UNP_KEY_PL1 OF 'COLBMPL1.TXT'
           PERFORM UNP-IKY-PL1.
           PERFORM UNP-AKY-PL1.
           PERFORM UNP-MKY-PL1.
*% INCLUDE FIN_UNP_KEY_PL1 OF 'COLBMPL1.TXT'
       FIN-UNP-KEY-PL1.
           EXIT.
       
       UNP-IKY-PL1 SECTION.
       INI-UNP-IKY-PL1.
*% INCLUDE INI_UNP_IKY_PL1 OF 'COLBMPL1.TXT'
           MOVE FRM-CPIM-UBY TO OPC-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO OPC-IIC-IOPC-ATR.
*% INCLUDE FIN_UNP_IKY_PL1 OF 'COLBMPL1.TXT'
       FIN-UNP-IKY-PL1.
           EXIT.
       
       UNP-MKY-PL1 SECTION.
       INI-UNP-MKY-PL1.
*% INCLUDE INI_UNP_MKY_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_UNP_MKY_PL1 OF 'COLBMPL1.TXT'
       FIN-UNP-MKY-PL1.
           EXIT.
       
       UNP-AKY-PL1 SECTION.
       INI-UNP-AKY-PL1.
*% INCLUDE INI_UNP_AKY_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_UNP_AKY_PL1 OF 'COLBMPL1.TXT'
       FIN-UNP-AKY-PL1.
           EXIT.
       
       UNP-FLD-PL1 SECTION.
       INI-UNP-FLD-PL1.
*% INCLUDE INI_UNP_FLD_PL1 OF 'COLBMPL1.TXT'
           PERFORM UNP-OPC-PL1.
           PERFORM UNP-DBC-PL1.
           PERFORM UNP-ITR-PL1.
*% INCLUDE FIN_UNP_FLD_PL1 OF 'COLBMPL1.TXT'
       FIN-UNP-FLD-PL1.
           EXIT.
       
       UNP-OPC-PL1 SECTION.
       INI-UNP-OPC-PL1.
*% INCLUDE INI_UNP_OPC_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_UNP_OPC_PL1 OF 'COLBMPL1.TXT'
       FIN-UNP-OPC-PL1.
           EXIT.
       
       UNP-DBC-PL1 SECTION.
       INI-UNP-DBC-PL1.
*% INCLUDE INI_UNP_DBC_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_UNP_DBC_PL1 OF 'COLBMPL1.TXT'
       FIN-UNP-DBC-PL1.
           EXIT.
       
       UNP-ITR-PL1 SECTION.
       INI-UNP-ITR-PL1.
*% INCLUDE INI_UNP_ITR_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_UNP_ITR_PL1 OF 'COLBMPL1.TXT'
       FIN-UNP-ITR-PL1.
           EXIT.
       
       VAL-MKY-PL1 SECTION.
       INI-VAL-MKY-PL1.
*% INCLUDE INI_VAL_MKY_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_VAL_MKY_PL1 OF 'COLBMPL1.TXT'
       FIN-VAL-MKY-PL1.
           EXIT.
       
       VAL-FLD-PL1 SECTION.
       INI-VAL-FLD-PL1.
*% INCLUDE INI_VAL_FLD_PL1 OF 'COLBMPL1.TXT'
           PERFORM VAL-OPC-COD-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-VCOC-LEN IN PL1-FLD
                GO TO FIN-VAL-FLD-PL1.
           PERFORM VAL-OPC-COD-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-TOOC-LEN IN PL1-FLD
                GO TO FIN-VAL-FLD-PL1.
           PERFORM VAL-OPC-COD-AOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-AOOC-LEN IN PL1-FLD
                GO TO FIN-VAL-FLD-PL1.
           PERFORM VAL-DBC-GLS-NOMC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DBC-GLS-NOMC-LEN IN PL1-FLD
                GO TO FIN-VAL-FLD-PL1.
           PERFORM VAL-FRM-GLS-VEXP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-VEXP-LEN IN PL1-FLD
                GO TO FIN-VAL-FLD-PL1.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-OPC-FEC-FCOL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                OPC-FEC-FCOL-LEN IN PL1-FLD
                GO TO FIN-VAL-FLD-PL1.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-VAL-CRED.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-CRED-LEN IN PL1-FLD
                GO TO FIN-VAL-FLD-PL1.
           PERFORM VAL-FRM-VAL-INTE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-INTE-LEN IN PL1-FLD
                GO TO FIN-VAL-FLD-PL1.
           PERFORM VAL-FRM-VAL-VCOM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-VCOM-LEN IN PL1-FLD
                GO TO FIN-VAL-FLD-PL1.
           PERFORM VAL-OPC-VAL-SEGD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-VAL-SEGD-LEN IN PL1-FLD
                GO TO FIN-VAL-FLD-PL1.
           PERFORM VAL-FRM-VAL-GAST.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-GAST-LEN IN PL1-FLD
                GO TO FIN-VAL-FLD-PL1.
           PERFORM VAL-FRM-VAL-IMPU.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-IMPU-LEN IN PL1-FLD
                GO TO FIN-VAL-FLD-PL1.
           PERFORM VAL-OPC-SGV-LIQU.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-SGV-LIQU-LEN IN PL1-FLD
                GO TO FIN-VAL-FLD-PL1.
           PERFORM VAL-FRM-GLS-TINT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TINT-LEN IN PL1-FLD
                GO TO FIN-VAL-FLD-PL1.
           PERFORM VAL-ITR-SGV-TINT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO ITR-SGV-TINT-LEN IN PL1-FLD
                GO TO FIN-VAL-FLD-PL1.
           PERFORM VAL-FRM-NUM-IVCT
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 9
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-NUM-IVCT-LEN IN PL1-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-PL1.
           PERFORM VAL-FRM-IND-CANC
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 9
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-IND-CANC-LEN IN PL1-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-PL1.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FVEN VARYING FRM-IFLD
                FROM 1 BY 1 UNTIL FRM-IFLD > 9 OR
                FRM-SUAR = FRM-SUAR-MAL OR
                FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FVEN-LEN IN PL1-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-PL1.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-VAL-INTC
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 9
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-INTC-LEN IN PL1-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-PL1.
           PERFORM VAL-FRM-VAL-SCAP
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 9
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-SCAP-LEN IN PL1-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-PL1.
           PERFORM VAL-FRM-VAL-TOTA
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 9
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-TOTA-LEN IN PL1-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-PL1.
*% INCLUDE FIN_VAL_FLD_PL1 OF 'COLBMPL1.TXT'
       FIN-VAL-FLD-PL1.
           EXIT.
       
