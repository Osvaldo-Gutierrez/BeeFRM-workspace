*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
      * IDC-INI --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO FRM-NUM-IAVA-ATR IN CCR-FLD.
           MOVE FRM-CPIM-UBY TO FRM-VRF-IAVA-ATR IN CCR-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IND-IAVA-ATR IN CCR-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-IAVA-ATR IN CCR-FLD.
      * IDC-FIN --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
*% INCLUDE FIN_UNP_AKY_CCR OF 'COLBMCCR.TXT'
       FIN-UNP-AKY-CCR.
           EXIT.
       
       UNP-FLD-CCR SECTION.
       INI-UNP-FLD-CCR.
*% INCLUDE INI_UNP_FLD_CCR OF 'COLBMCCR.TXT'
           PERFORM UNP-OPC-CCR.
           PERFORM UNP-CCR-CCR.
           PERFORM UNP-DLC-CCR.
*% INCLUDE FIN_UNP_FLD_CCR OF 'COLBMCCR.TXT'
       FIN-UNP-FLD-CCR.
           EXIT.
       
       UNP-OPC-CCR SECTION.
       INI-UNP-OPC-CCR.
*% INCLUDE INI_UNP_OPC_CCR OF 'COLBMCCR.TXT'
*% INCLUDE FIN_UNP_OPC_CCR OF 'COLBMCCR.TXT'
       FIN-UNP-OPC-CCR.
           EXIT.
       
       UNP-CCR-CCR SECTION.
       INI-UNP-CCR-CCR.
*% INCLUDE INI_UNP_CCR_CCR OF 'COLBMCCR.TXT'
*% RREG = "CCR"
*% RREG_FRM = "CCR"
*% NOMB = "FCCR"
           MOVE FRM-CPIM-UBY TO CCR-FEC-FCCR-ATR IN CCR-FLD.
*% RREG = "CCR"
*% RREG_FRM = "CCR"
*% NOMB = "FING"
           MOVE FRM-CPIM-UBY TO CCR-FEC-FING-ATR IN CCR-FLD.
           MOVE FRM-CPIM-UBY TO CCR-IND-SCRT-ATR IN CCR-FLD.
*% INCLUDE FIN_UNP_CCR_CCR OF 'COLBMCCR.TXT'
       FIN-UNP-CCR-CCR.
           EXIT.
       
       UNP-DLC-CCR SECTION.
       INI-UNP-DLC-CCR.
*% INCLUDE INI_UNP_DLC_CCR OF 'COLBMCCR.TXT'
*% INCLUDE FIN_UNP_DLC_CCR OF 'COLBMCCR.TXT'
       FIN-UNP-DLC-CCR.
           EXIT.
       
       VAL-KEY-CCR SECTION.
       INI-VAL-KEY-CCR.
           PERFORM VAL-IKY-CCR.
           PERFORM VAL-MKY-CCR.
           PERFORM VAL-AKY-CCR.
       FIN-VAL-KEY-CCR.
           EXIT.
       
       VAL-IKY-CCR SECTION.
       INI-VAL-IKY-CCR.
*% INCLUDE INI_VAL_IKY_CCR OF 'COLBMCCR.TXT'
           PERFORM VAL-CCR-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CCR-CAI-IOPC-LEN IN CCR-FLD
                GO TO FIN-VAL-IKY-CCR.
           PERFORM VAL-CCR-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CCR-IIC-IOPC-LEN IN CCR-FLD
                GO TO FIN-VAL-IKY-CCR.
           PERFORM VAL-CCR-NUM-IDLC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CCR-NUM-IDLC-LEN IN CCR-FLD
                GO TO FIN-VAL-IKY-CCR.
*% INCLUDE FIN_VAL_IKY_CCR OF 'COLBMCCR.TXT'
       FIN-VAL-IKY-CCR.
           EXIT.
       
       VAL-MKY-CCR SECTION.
       INI-VAL-MKY-CCR.
*% INCLUDE INI_VAL_MKY_CCR OF 'COLBMCCR.TXT'
           PERFORM VAL-FRM-CAI-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-ICLI-LEN IN CCR-FLD
                GO TO FIN-VAL-MKY-CCR.
           PERFORM VAL-FRM-IIC-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-ICLI-LEN IN CCR-FLD
                GO TO FIN-VAL-MKY-CCR.
           PERFORM VAL-FRM-CAI-IAVA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-IAVA-LEN IN CCR-FLD
                GO TO FIN-VAL-MKY-CCR.
           PERFORM VAL-FRM-IIC-IAVA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-IAVA-LEN IN CCR-FLD
                GO TO FIN-VAL-MKY-CCR.
*% INCLUDE FIN_VAL_MKY_CCR OF 'COLBMCCR.TXT'
       FIN-VAL-MKY-CCR.
           EXIT.
       
       VAL-AKY-CCR SECTION.
       INI-VAL-AKY-CCR.
