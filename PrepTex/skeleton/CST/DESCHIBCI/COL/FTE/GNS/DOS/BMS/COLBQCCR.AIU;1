      *
      * COLBQCCR
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 03:23 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CCR"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMCCR.TXT'
       SET-TAG-COLBQCCR SECTION.
       INI-SET-TAG-COLBQCCR.
           MOVE
           '<IDNMOD NOM=$COLBQCCR$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 03:23 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCCR.
           EXIT.
           
       PUT-ALL-CCR SECTION.
       INI-PUT-ALL-CCR.
*% INCLUDE INI_PUT_ALL_CCR OF 'COLBMCCR.TXT'
           PERFORM PUT-OPC-CCR.
           PERFORM PUT-CCR-CCR.
           PERFORM PUT-DLC-CCR.
*% INCLUDE FIN_PUT_ALL_CCR OF 'COLBMCCR.TXT'
       FIN-PUT-ALL-CCR.
           EXIT.
       
       PUT-OPC-CCR SECTION.
       INI-PUT-OPC-CCR.
*% INCLUDE INI_PUT_OPC_CCR OF 'COLBMCCR.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-CCR.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-CCR
               GO TO FIN-PUT-OPC-CCR.
           MOVE OPC-MSC-TVOC IN OPC TO OPC-MSC-TVOC IN CCR-FLD.
           MOVE OPC-COD-TOOC IN OPC TO OPC-COD-TOOC IN CCR-FLD.
           MOVE OPC-COD-OFOC IN OPC TO OPC-COD-OFOC IN CCR-FLD.
*% BUF = "CCR-FLD"
*% SIS = "TAB"
*% TAB = "TVAL"
*% FLD = "OPC-MSC-TVOC"
*% GLS = "FRM-GLS-TVOC"
*% TIENE_GLOSA = TRUE
*% LGL = 13
*% INCLUDE 'BUSMSC'
*% BUF = "CCR-FLD"
*% SIS = "TAB"
*% TAB = "TIO "
*% FLD = "OPC-COD-TOOC"
*% GLS = "FRM-GLS-TOOC"
*% TIENE_GLOSA = TRUE
*% LGL = 14
*% INCLUDE 'BUSTAB'
*% BUF = "CCR-FLD"
*% SIS = "TAB"
*% TAB = "OFI "
*% FLD = "OPC-COD-OFOC"
*% GLS = "FRM-GLS-OFOC"
*% TIENE_GLOSA = TRUE
*% LGL = 13
*% INCLUDE 'BUSOFI          '
*% INCLUDE FIN_PUT_OPC_CCR OF 'COLBMCCR.TXT'
       FIN-PUT-OPC-CCR.
           EXIT.
       
       PUT-CCR-CCR SECTION.
       INI-PUT-CCR-CCR.
*% INCLUDE INI_PUT_CCR_CCR OF 'COLBMCCR.TXT'
           IF PGM-STAT-CCR-NUL
               GO TO FIN-PUT-CCR-CCR.
           IF NOT PGM-STAT-CCR-OKS
               PERFORM CLR-CCR-CCR
               GO TO FIN-PUT-CCR-CCR.
           MOVE CCR-CAI-IOPC IN CCR TO CCR-CAI-IOPC IN CCR-FLD.
           MOVE CCR-IIC-IOPC IN CCR TO CCR-IIC-IOPC IN CCR-FLD.
           MOVE CCR-NUM-IDLC IN CCR TO CCR-NUM-IDLC IN CCR-FLD.
           MOVE CCR-NUM-ICCR IN CCR TO CCR-NUM-ICCR IN CCR-FLD.
           MOVE CCR-IND-SCRA IN CCR TO CCR-IND-SCRA IN CCR-FLD.
*% RREG = "CCR"
*% RREG_FRM = "CCR"
*% NOMB = "FCCR"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CCR-NUM-DCCR IN CCR TO CCR-NUM-DCCR IN CCR-FLD.
           MOVE CCR-NUM-MCCR IN CCR TO CCR-NUM-MCCR IN CCR-FLD.
           MOVE CCR-NUM-SCCR IN CCR TO CCR-NUM-SCCR IN CCR-FLD.
           MOVE CCR-NUM-ACCR IN CCR TO CCR-NUM-ACCR IN CCR-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% RREG = "CCR"
*% RREG_FRM = "CCR"
*% NOMB = "FING"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CCR-NUM-DING IN CCR TO CCR-NUM-DING IN CCR-FLD.
           MOVE CCR-NUM-MING IN CCR TO CCR-NUM-MING IN CCR-FLD.
           MOVE CCR-NUM-SING IN CCR TO CCR-NUM-SING IN CCR-FLD.
           MOVE CCR-NUM-AING IN CCR TO CCR-NUM-AING IN CCR-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE CCR-IND-SCRT IN CCR TO CCR-IND-SCRT IN CCR-FLD.
           MOVE CCR-COD-RCCR IN CCR TO CCR-COD-RCCR IN CCR-FLD.
*% BUF = "CCR-FLD"
*% SIS = "COL"
*% TAB = "SCRT"
*% FLD = "CCR-IND-SCRA"
*% GLS = "FRM-GLS-SCRA"
*% TIENE_GLOSA = TRUE
*% LGL = 33
*% INCLUDE 'BUSMSC'
*% BUF = "CCR-FLD"
*% SIS = "COL"
*% TAB = "SCRT"
*% FLD = "CCR-IND-SCRT"
*% GLS = "FRM-GLS-SCRT"
*% TIENE_GLOSA = TRUE
*% LGL = 33
*% INCLUDE 'BUSMSC'
*% BUF = "CCR-FLD"
*% SIS = "TAB"
*% TAB = "USR "
*% FLD = "CCR-COD-RCCR"
*% GLS = "FRM-GLS-RCCR"
*% TIENE_GLOSA = TRUE
*% LGL = 31
*% INCLUDE 'BUSUSR          '
*% INCLUDE FIN_PUT_CCR_CCR OF 'COLBMCCR.TXT'
       FIN-PUT-CCR-CCR.
           EXIT.
       
       PUT-DLC-CCR SECTION.
       INI-PUT-DLC-CCR.
*% INCLUDE INI_PUT_DLC_CCR OF 'COLBMCCR.TXT'
           IF PGM-STAT-DLC-NUL
               GO TO FIN-PUT-DLC-CCR.
           IF NOT PGM-STAT-DLC-OKS
               PERFORM CLR-DLC-CCR
               GO TO FIN-PUT-DLC-CCR.
           MOVE DLC-IND-SCRT IN DLC TO DLC-IND-SCRT IN CCR-FLD.
*% BUF = "CCR-FLD"
*% SIS = "COL"
*% TAB = "SCRT"
*% FLD = "DLC-IND-SCRT"
*% GLS = "FRM-GLS-SCRT"
*% TIENE_GLOSA = TRUE
*% LGL = 33
*% INCLUDE 'BUSMSC'
*% INCLUDE FIN_PUT_DLC_CCR OF 'COLBMCCR.TXT'
       FIN-PUT-DLC-CCR.
           EXIT.
       
       KEY-ALL-CCR SECTION.
       INI-KEY-ALL-CCR.
*% INCLUDE INI_KEY_ALL_CCR OF 'COLBMCCR.TXT'
           PERFORM KEY-OPC-CCR.
           PERFORM KEY-CCR-CCR.
           PERFORM KEY-DLC-CCR.
*% INCLUDE FIN_KEY_ALL_CCR OF 'COLBMCCR.TXT'
       FIN-KEY-ALL-CCR.
           EXIT.
       
       KEY-OPC-CCR SECTION.
       INI-KEY-OPC-CCR.
*% INCLUDE INI_KEY_OPC_CCR OF 'COLBMCCR.TXT'
*% INCLUDE FIN_KEY_OPC_CCR OF 'COLBMCCR.TXT'
       FIN-KEY-OPC-CCR.
           EXIT.
       
       KEY-CCR-CCR SECTION.
       INI-KEY-CCR-CCR.
*% INCLUDE INI_KEY_CCR_CCR OF 'COLBMCCR.TXT'
           MOVE CCR-CAI-IOPC IN CCR-FLD TO CCR-CAI-IOPC IN CCR.
           MOVE CCR-IIC-IOPC IN CCR-FLD TO CCR-IIC-IOPC IN CCR.
           MOVE CCR-NUM-IDLC IN CCR-FLD TO CCR-NUM-IDLC IN CCR.
           MOVE CCR-NUM-ICCR IN CCR-FLD TO CCR-NUM-ICCR IN CCR.
*% INCLUDE FIN_KEY_CCR_CCR OF 'COLBMCCR.TXT'
       FIN-KEY-CCR-CCR.
           EXIT.
       
       KEY-DLC-CCR SECTION.
       INI-KEY-DLC-CCR.
*% INCLUDE INI_KEY_DLC_CCR OF 'COLBMCCR.TXT'
*% INCLUDE FIN_KEY_DLC_CCR OF 'COLBMCCR.TXT'
       FIN-KEY-DLC-CCR.
           EXIT.
       
       SCR-CLRA-CCR SECTION.
       INI-SCR-CLRA-CCR.
*% INCLUDE INI_SCR_CLRA_CCR OF 'COLBMCCR.TXT'
           PERFORM CLR-KEY-CCR.
           PERFORM CLR-FLD-CCR.
*% INCLUDE FIN_SCR_CLRA_CCR OF 'COLBMCCR.TXT'
       FIN-SCR-CLRA-CCR.
           EXIT.
       
       CLR-KEY-CCR SECTION.
       INI-CLR-KEY-CCR.
*% INCLUDE INI_CLR_KEY_CCR OF 'COLBMCCR.TXT'
           MOVE SPACES TO CCR-CAI-IOPC IN CCR-FLD.
           MOVE ZEROES TO CCR-IIC-IOPC IN CCR-FLD.
           MOVE ZEROES TO CCR-NUM-IDLC IN CCR-FLD.
           MOVE ZEROES TO CCR-NUM-ICCR IN CCR-FLD.
           MOVE SPACES TO FRM-GLS-NOMC IN CCR-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-ICLI IN CCR-FLD.
           MOVE SPACES TO FRM-VRF-ICLI IN CCR-FLD.
           MOVE SPACES TO FRM-IND-ICLI IN CCR-FLD.
           MOVE SPACES TO FRM-GLS-ICLI IN CCR-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-CAI-ICLI IN CCR-FLD.
           MOVE SPACES TO FRM-IIC-ICLI IN CCR-FLD.
           MOVE SPACES TO FRM-GLS-NOMA IN CCR-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-IAVA IN CCR-FLD.
           MOVE SPACES TO FRM-VRF-IAVA IN CCR-FLD.
           MOVE SPACES TO FRM-IND-IAVA IN CCR-FLD.
           MOVE SPACES TO FRM-GLS-IAVA IN CCR-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-CAI-IAVA IN CCR-FLD.
           MOVE SPACES TO FRM-IIC-IAVA IN CCR-FLD.
*% INCLUDE FIN_CLR_KEY_CCR OF 'COLBMCCR.TXT'
       FIN-CLR-KEY-CCR.
           EXIT.
       
       CLR-FLD-CCR SECTION.
       INI-CLR-FLD-CCR.
*% INCLUDE INI_CLR_FLD_CCR OF 'COLBMCCR.TXT'
           MOVE SPACES TO FRM-GLS-TVOC IN CCR-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN CCR-FLD.
           MOVE SPACES TO FRM-GLS-OFOC IN CCR-FLD.
           MOVE SPACES TO FRM-GLS-SCRA IN CCR-FLD.
           MOVE SPACES TO FRM-GLS-SCRT IN CCR-FLD.
           MOVE SPACES TO FRM-GLS-RCCR IN CCR-FLD.
           PERFORM CLR-OPC-CCR.
           PERFORM CLR-CCR-CCR.
           PERFORM CLR-DLC-CCR.
*% INCLUDE FIN_CLR_FLD_CCR OF 'COLBMCCR.TXT'
       FIN-CLR-FLD-CCR.
           EXIT.
       
       CLR-OPC-CCR SECTION.
       INI-CLR-OPC-CCR.
*% INCLUDE INI_CLR_OPC_CCR OF 'COLBMCCR.TXT'
           MOVE SPACES TO OPC-MSC-TVOC IN CCR-FLD.
           MOVE SPACES TO OPC-COD-TOOC IN CCR-FLD.
           MOVE SPACES TO OPC-COD-OFOC IN CCR-FLD.
*% INCLUDE FIN_CLR_OPC_CCR OF 'COLBMCCR.TXT'
       FIN-CLR-OPC-CCR.
           EXIT.
       
       CLR-CCR-CCR SECTION.
       INI-CLR-CCR-CCR.
*% INCLUDE INI_CLR_CCR_CCR OF 'COLBMCCR.TXT'
           MOVE SPACES TO CCR-IND-SCRA IN CCR-FLD.
*% RREG = "CCR"
*% RREG_FRM = "CCR"
*% NOMB = "FCCR"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CCR-FEC-FCCR IN CCR-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "CCR"
*% RREG_FRM = "CCR"
*% NOMB = "FING"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CCR-FEC-FING IN CCR-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO CCR-IND-SCRT IN CCR-FLD.
           MOVE SPACES TO CCR-COD-RCCR IN CCR-FLD.
*% INCLUDE FIN_CLR_CCR_CCR OF 'COLBMCCR.TXT'
       FIN-CLR-CCR-CCR.
           EXIT.
       
       CLR-DLC-CCR SECTION.
       INI-CLR-DLC-CCR.
*% INCLUDE INI_CLR_DLC_CCR OF 'COLBMCCR.TXT'
           MOVE SPACES TO DLC-IND-SCRT IN CCR-FLD.
*% INCLUDE FIN_CLR_DLC_CCR OF 'COLBMCCR.TXT'
       FIN-CLR-DLC-CCR.
           EXIT.
       
       FST-KEY-CCR SECTION.
       INI-FST-KEY-CCR.
*% INCLUDE INI_FST_KEY_CCR OF 'COLBMCCR.TXT'
           MOVE -1 TO CCR-CAI-IOPC-LEN IN CCR-FLD.
*% INCLUDE FIN_FST_KEY_CCR OF 'COLBMCCR.TXT'
       FIN-FST-KEY-CCR.
           EXIT.
       
       FST-FLD-CCR SECTION.
       INI-FST-FLD-CCR.
*% INCLUDE INI_FST_FLD_CCR OF 'COLBMCCR.TXT'
*% RREG = "CCR"
*% RREG_FRM = "CCR"
*% NOMB = "FCCR"
           MOVE FRM-POS-CURS TO CCR-FEC-FCCR-LEN IN CCR-FLD.
*% INCLUDE FIN_FST_FLD_CCR OF 'COLBMCCR.TXT'
       FIN-FST-FLD-CCR.
           EXIT.
       
       PRO-KEY-CCR SECTION.
       INI-PRO-KEY-CCR.
*% INCLUDE INI_PRO_KEY_CCR OF 'COLBMCCR.TXT'
           MOVE FRM-CPIM-AEY TO FRM-GLS-NOMC-ATR IN CCR-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-NUM-ICLI-ATR IN CCR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VRF-ICLI-ATR IN CCR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-ICLI-ATR IN CCR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-ICLI-ATR IN CCR-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-GLS-NOMA-ATR IN CCR-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-NUM-IAVA-ATR IN CCR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VRF-IAVA-ATR IN CCR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-IAVA-ATR IN CCR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-IAVA-ATR IN CCR-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           PERFORM PRO-IKY-CCR.
           PERFORM PRO-AKY-CCR.
           PERFORM PRO-MKY-CCR.
*% INCLUDE FIN_PRO_KEY_CCR OF 'COLBMCCR.TXT'
       FIN-PRO-KEY-CCR.
           EXIT.
       
       PRO-IKY-CCR SECTION.
       INI-PRO-IKY-CCR.
*% INCLUDE INI_PRO_IKY_CCR OF 'COLBMCCR.TXT'
           MOVE FRM-CPIM-AEY TO CCR-CAI-IOPC-ATR IN CCR-FLD.
           MOVE FRM-CPIM-AEY TO CCR-IIC-IOPC-ATR IN CCR-FLD.
           MOVE FRM-CPIM-AEY TO CCR-NUM-IDLC-ATR IN CCR-FLD.
*% INCLUDE FIN_PRO_IKY_CCR OF 'COLBMCCR.TXT'
       FIN-PRO-IKY-CCR.
           EXIT.
       
       PRO-MKY-CCR SECTION.
       INI-PRO-MKY-CCR.
*% INCLUDE INI_PRO_MKY_CCR OF 'COLBMCCR.TXT'
*% INCLUDE FIN_PRO_MKY_CCR OF 'COLBMCCR.TXT'
       FIN-PRO-MKY-CCR.
           EXIT.
       
       PRO-AKY-CCR SECTION.
       INI-PRO-AKY-CCR.
*% INCLUDE INI_PRO_AKY_CCR OF 'COLBMCCR.TXT'
           MOVE FRM-CPIM-AEY TO CCR-NUM-ICCR-ATR IN CCR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-NOMC-ATR IN CCR-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-NUM-ICLI-ATR IN CCR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VRF-ICLI-ATR IN CCR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-ICLI-ATR IN CCR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-ICLI-ATR IN CCR-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-GLS-NOMA-ATR IN CCR-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-NUM-IAVA-ATR IN CCR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VRF-IAVA-ATR IN CCR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-IAVA-ATR IN CCR-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-IAVA-ATR IN CCR-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
*% INCLUDE FIN_PRO_AKY_CCR OF 'COLBMCCR.TXT'
       FIN-PRO-AKY-CCR.
           EXIT.
       
       UNP-KEY-CCR SECTION.
       INI-UNP-KEY-CCR.
*% INCLUDE INI_UNP_KEY_CCR OF 'COLBMCCR.TXT'
           MOVE FRM-CPIM-UBY TO FRM-GLS-NOMC-ATR IN CCR-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO FRM-NUM-ICLI-ATR IN CCR-FLD.
           MOVE FRM-CPIM-UBY TO FRM-VRF-ICLI-ATR IN CCR-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IND-ICLI-ATR IN CCR-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-ICLI-ATR IN CCR-FLD.
      * IDC-FIN --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO FRM-GLS-NOMA-ATR IN CCR-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "IAVA"
