*% FRM = "CDP"
      *
      * COLBUCDP
      * Version 3.0.1 Nov-2005
      * 13-Sep-12 07:05 AM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMCDP.TXT'
       SET-TAG-COLBUCDP SECTION.
       INI-SET-TAG-COLBUCDP.
           MOVE
           '<IDNMOD NOM=$COLBUCDP$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$13-Sep-12 07:05 AM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCDP.
           EXIT.
           
       RET-ALL-CDP SECTION.
       INI-RET-ALL-CDP.
*% INCLUDE INI_RET_ALL_CDP OF 'COLBMCDP.TXT'
           PERFORM KEY-ALL-CDP.
           PERFORM RET-PLL-CDP.
*% INCLUDE FIN_RET_ALL_CDP OF 'COLBMCDP.TXT'
       FIN-RET-ALL-CDP.
           EXIT.
       
       RET-PLL-CDP SECTION.
       INI-RET-PLL-CDP.
*% INCLUDE INI_RET_PLL_CDP OF 'COLBMCDP.TXT'
*% INCLUDE FIN_RET_PLL_CDP OF 'COLBMCDP.TXT'
       FIN-RET-PLL-CDP.
           EXIT.
       
       KEY-ALL-CDP SECTION.
       INI-KEY-ALL-CDP.
*% INCLUDE INI_KEY_ALL_CDP OF 'COLBMCDP.TXT'
           PERFORM KEY-PLL-CDP.
*% INCLUDE FIN_KEY_ALL_CDP OF 'COLBMCDP.TXT'
       FIN-KEY-ALL-CDP.
           EXIT.
       
       KEY-PLL-CDP SECTION.
       INI-KEY-PLL-CDP.
*% INCLUDE INI_KEY_PLL_CDP OF 'COLBMCDP.TXT'
           MOVE PLL-CAI-IPLL IN CDP-FLD TO PLL-CAI-IPLL IN PLL.
           MOVE PLL-IIC-IPLL IN CDP-FLD TO PLL-IIC-IPLL IN PLL.
           MOVE PLL-CAI-ICVN IN CDP-FLD TO PLL-CAI-ICVN IN PLL.
           MOVE PLL-IIC-ICVN IN CDP-FLD TO PLL-IIC-ICVN IN PLL.
           MOVE PLL-NUM-IPLL IN CDP-FLD TO PLL-NUM-IPLL IN PLL.
*% INCLUDE FIN_KEY_PLL_CDP OF 'COLBMCDP.TXT'
       FIN-KEY-PLL-CDP.
           EXIT.
       
       SCR-CLRA-CDP SECTION.
       INI-SCR-CLRA-CDP.
*% INCLUDE INI_SCR_CLRA_CDP OF 'COLBMCDP.TXT'
           PERFORM CLR-KEY-CDP.
           PERFORM CLR-FLD-CDP.
*% INCLUDE FIN_SCR_CLRA_CDP OF 'COLBMCDP.TXT'
       FIN-SCR-CLRA-CDP.
           EXIT.
       
       CLR-KEY-CDP SECTION.
       INI-CLR-KEY-CDP.
*% INCLUDE INI_CLR_KEY_CDP OF 'COLBMCDP.TXT'
           MOVE SPACES TO PLL-CAI-IPLL IN CDP-FLD.
           MOVE ZEROES TO PLL-IIC-IPLL IN CDP-FLD.
           MOVE SPACES TO PLL-CAI-ICVN IN CDP-FLD.
           MOVE ZEROES TO PLL-IIC-ICVN IN CDP-FLD.
           MOVE ZEROES TO PLL-NUM-IPLL IN CDP-FLD.
           MOVE SPACES TO FRM-IND-SGTE IN CDP-FLD.
           MOVE SPACES TO FRM-CAI-OPER IN CDP-FLD.
           MOVE SPACES TO FRM-IIC-OPER IN CDP-FLD.
*% INCLUDE FIN_CLR_KEY_CDP OF 'COLBMCDP.TXT'
       FIN-CLR-KEY-CDP.
           EXIT.
       
       CLR-FLD-CDP SECTION.
       INI-CLR-FLD-CDP.
*% INCLUDE INI_CLR_FLD_CDP OF 'COLBMCDP.TXT'
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-ICLI IN CDP-FLD.
           MOVE SPACES TO FRM-VRF-ICLI IN CDP-FLD.
           MOVE SPACES TO FRM-IND-ICLI IN CDP-FLD.
           MOVE SPACES TO FRM-GLS-ICLI IN CDP-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-GLS-NOMC IN CDP-FLD.
           MOVE ZEROES TO FRM-NUM-IDPL IN CDP-FLD(1).
           MOVE ZEROES TO FRM-NUM-IDPL IN CDP-FLD(2).
           MOVE ZEROES TO FRM-NUM-IDPL IN CDP-FLD(3).
           MOVE ZEROES TO FRM-NUM-IDPL IN CDP-FLD(4).
           MOVE ZEROES TO FRM-NUM-IDPL IN CDP-FLD(5).
           MOVE SPACES TO FRM-GLS-NOMS IN CDP-FLD(1).
           MOVE SPACES TO FRM-GLS-NOMS IN CDP-FLD(2).
           MOVE SPACES TO FRM-GLS-NOMS IN CDP-FLD(3).
           MOVE SPACES TO FRM-GLS-NOMS IN CDP-FLD(4).
           MOVE SPACES TO FRM-GLS-NOMS IN CDP-FLD(5).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLS"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLS"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-ICLS IN CDP-FLD(1).
           MOVE SPACES TO FRM-VRF-ICLS IN CDP-FLD(1).
           MOVE SPACES TO FRM-IND-ICLS IN CDP-FLD(1).
           MOVE SPACES TO FRM-GLS-ICLS IN CDP-FLD(1).
           MOVE ZEROES TO FRM-NUM-ICLS IN CDP-FLD(2).
           MOVE SPACES TO FRM-VRF-ICLS IN CDP-FLD(2).
           MOVE SPACES TO FRM-IND-ICLS IN CDP-FLD(2).
           MOVE SPACES TO FRM-GLS-ICLS IN CDP-FLD(2).
           MOVE ZEROES TO FRM-NUM-ICLS IN CDP-FLD(3).
           MOVE SPACES TO FRM-VRF-ICLS IN CDP-FLD(3).
           MOVE SPACES TO FRM-IND-ICLS IN CDP-FLD(3).
           MOVE SPACES TO FRM-GLS-ICLS IN CDP-FLD(3).
           MOVE ZEROES TO FRM-NUM-ICLS IN CDP-FLD(4).
           MOVE SPACES TO FRM-VRF-ICLS IN CDP-FLD(4).
           MOVE SPACES TO FRM-IND-ICLS IN CDP-FLD(4).
           MOVE SPACES TO FRM-GLS-ICLS IN CDP-FLD(4).
           MOVE ZEROES TO FRM-NUM-ICLS IN CDP-FLD(5).
           MOVE SPACES TO FRM-VRF-ICLS IN CDP-FLD(5).
           MOVE SPACES TO FRM-IND-ICLS IN CDP-FLD(5).
           MOVE SPACES TO FRM-GLS-ICLS IN CDP-FLD(5).
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-IND-NOVD IN CDP-FLD(1).
           MOVE SPACES TO FRM-IND-NOVD IN CDP-FLD(2).
           MOVE SPACES TO FRM-IND-NOVD IN CDP-FLD(3).
           MOVE SPACES TO FRM-IND-NOVD IN CDP-FLD(4).
           MOVE SPACES TO FRM-IND-NOVD IN CDP-FLD(5).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FVEN IN CDP-FLD(1).
           MOVE ZEROES TO FRM-FEC-FVEN IN CDP-FLD(2).
           MOVE ZEROES TO FRM-FEC-FVEN IN CDP-FLD(3).
           MOVE ZEROES TO FRM-FEC-FVEN IN CDP-FLD(4).
           MOVE ZEROES TO FRM-FEC-FVEN IN CDP-FLD(5).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-CAI-IOPC IN CDP-FLD(1).
           MOVE SPACES TO FRM-CAI-IOPC IN CDP-FLD(2).
           MOVE SPACES TO FRM-CAI-IOPC IN CDP-FLD(3).
           MOVE SPACES TO FRM-CAI-IOPC IN CDP-FLD(4).
           MOVE SPACES TO FRM-CAI-IOPC IN CDP-FLD(5).
           MOVE SPACES TO FRM-IIC-IOPC IN CDP-FLD(1).
           MOVE SPACES TO FRM-IIC-IOPC IN CDP-FLD(2).
           MOVE SPACES TO FRM-IIC-IOPC IN CDP-FLD(3).
           MOVE SPACES TO FRM-IIC-IOPC IN CDP-FLD(4).
           MOVE SPACES TO FRM-IIC-IOPC IN CDP-FLD(5).
           MOVE ZEROES TO FRM-NUM-IVEN IN CDP-FLD(1).
           MOVE ZEROES TO FRM-NUM-IVEN IN CDP-FLD(2).
           MOVE ZEROES TO FRM-NUM-IVEN IN CDP-FLD(3).
           MOVE ZEROES TO FRM-NUM-IVEN IN CDP-FLD(4).
           MOVE ZEROES TO FRM-NUM-IVEN IN CDP-FLD(5).
           MOVE ZEROES TO FRM-NUM-VCOR IN CDP-FLD(1).
           MOVE ZEROES TO FRM-NUM-VCOR IN CDP-FLD(2).
           MOVE ZEROES TO FRM-NUM-VCOR IN CDP-FLD(3).
           MOVE ZEROES TO FRM-NUM-VCOR IN CDP-FLD(4).
           MOVE ZEROES TO FRM-NUM-VCOR IN CDP-FLD(5).
           MOVE SPACES TO FRM-COD-VCOC IN CDP-FLD(1).
           MOVE SPACES TO FRM-COD-VCOC IN CDP-FLD(2).
           MOVE SPACES TO FRM-COD-VCOC IN CDP-FLD(3).
           MOVE SPACES TO FRM-COD-VCOC IN CDP-FLD(4).
           MOVE SPACES TO FRM-COD-VCOC IN CDP-FLD(5).
           MOVE ZEROES TO FRM-VAL-CUOT IN CDP-FLD(1).
           MOVE ZEROES TO FRM-VAL-CUOT IN CDP-FLD(2).
           MOVE ZEROES TO FRM-VAL-CUOT IN CDP-FLD(3).
           MOVE ZEROES TO FRM-VAL-CUOT IN CDP-FLD(4).
           MOVE ZEROES TO FRM-VAL-CUOT IN CDP-FLD(5).
           MOVE ZEROES TO FRM-VAL-MPAG IN CDP-FLD(1).
           MOVE ZEROES TO FRM-VAL-MPAG IN CDP-FLD(2).
           MOVE ZEROES TO FRM-VAL-MPAG IN CDP-FLD(3).
           MOVE ZEROES TO FRM-VAL-MPAG IN CDP-FLD(4).
           MOVE ZEROES TO FRM-VAL-MPAG IN CDP-FLD(5).
           PERFORM CLR-PLL-CDP.
*% INCLUDE FIN_CLR_FLD_CDP OF 'COLBMCDP.TXT'
       FIN-CLR-FLD-CDP.
           EXIT.
       
       CLR-PLL-CDP SECTION.
       INI-CLR-PLL-CDP.
*% INCLUDE INI_CLR_PLL_CDP OF 'COLBMCDP.TXT'
*% INCLUDE FIN_CLR_PLL_CDP OF 'COLBMCDP.TXT'
       FIN-CLR-PLL-CDP.
           EXIT.
       
       FST-KEY-CDP SECTION.
       INI-FST-KEY-CDP.
*% INCLUDE INI_FST_KEY_CDP OF 'COLBMCDP.TXT'
           MOVE -1 TO PLL-CAI-IPLL-LEN IN CDP-FLD.
*% INCLUDE FIN_FST_KEY_CDP OF 'COLBMCDP.TXT'
       FIN-FST-KEY-CDP.
           EXIT.
       
       FST-FLD-CDP SECTION.
       INI-FST-FLD-CDP.
*% INCLUDE INI_FST_FLD_CDP OF 'COLBMCDP.TXT'
*% INCLUDE FIN_FST_FLD_CDP OF 'COLBMCDP.TXT'
       FIN-FST-FLD-CDP.
           EXIT.
       
       PRO-KEY-CDP SECTION.
       INI-PRO-KEY-CDP.
*% INCLUDE INI_PRO_KEY_CDP OF 'COLBMCDP.TXT'
           MOVE FRM-CPIM-AEY TO FRM-IND-SGTE-ATR IN CDP-FLD.
           MOVE FRM-CPIM-AEY TO FRM-CAI-OPER-ATR IN CDP-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IIC-OPER-ATR IN CDP-FLD.
           PERFORM PRO-MKY-CDP.
*% INCLUDE FIN_PRO_KEY_CDP OF 'COLBMCDP.TXT'
       FIN-PRO-KEY-CDP.
           EXIT.
       
       PRO-MKY-CDP SECTION.
       INI-PRO-MKY-CDP.
*% INCLUDE INI_PRO_MKY_CDP OF 'COLBMCDP.TXT'
*% INCLUDE FIN_PRO_MKY_CDP OF 'COLBMCDP.TXT'
       FIN-PRO-MKY-CDP.
           EXIT.
       
       PRO-FLD-CDP SECTION.
       INI-PRO-FLD-CDP.
*% INCLUDE INI_PRO_FLD_CDP OF 'COLBMCDP.TXT'
           PERFORM PRO-PLL-CDP.
*% INCLUDE FIN_PRO_FLD_CDP OF 'COLBMCDP.TXT'
       FIN-PRO-FLD-CDP.
           EXIT.
       
       PRO-PLL-CDP SECTION.
       INI-PRO-PLL-CDP.
*% INCLUDE INI_PRO_PLL_CDP OF 'COLBMCDP.TXT'
*% INCLUDE FIN_PRO_PLL_CDP OF 'COLBMCDP.TXT'
       FIN-PRO-PLL-CDP.
           EXIT.
       
       UNP-KEY-CDP SECTION.
       INI-UNP-KEY-CDP.
*% INCLUDE INI_UNP_KEY_CDP OF 'COLBMCDP.TXT'
           MOVE FRM-CPIM-UBY TO FRM-IND-SGTE-ATR IN CDP-FLD.
           MOVE FRM-CPIM-UBY TO FRM-CAI-OPER-ATR IN CDP-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IIC-OPER-ATR IN CDP-FLD.
           PERFORM UNP-IKY-CDP.
           PERFORM UNP-AKY-CDP.
           PERFORM UNP-MKY-CDP.
*% INCLUDE FIN_UNP_KEY_CDP OF 'COLBMCDP.TXT'
       FIN-UNP-KEY-CDP.
           EXIT.
       
       UNP-IKY-CDP SECTION.
       INI-UNP-IKY-CDP.
*% INCLUDE INI_UNP_IKY_CDP OF 'COLBMCDP.TXT'
           MOVE FRM-CPIM-UBY TO PLL-CAI-IPLL-ATR.
           MOVE FRM-CPIM-UBY TO PLL-IIC-IPLL-ATR.
           MOVE FRM-CPIM-UBY TO PLL-CAI-ICVN-ATR.
           MOVE FRM-CPIM-UBY TO PLL-IIC-ICVN-ATR.
           MOVE FRM-CPIM-UBY TO PLL-NUM-IPLL-ATR.
           MOVE FRM-CPIM-UBY TO FRM-IND-SGTE-ATR.
           MOVE FRM-CPIM-UBY TO FRM-CAI-OPER-ATR.
           MOVE FRM-CPIM-UBY TO FRM-IIC-OPER-ATR.
*% INCLUDE FIN_UNP_IKY_CDP OF 'COLBMCDP.TXT'
       FIN-UNP-IKY-CDP.
           EXIT.
       
       UNP-MKY-CDP SECTION.
       INI-UNP-MKY-CDP.
*% INCLUDE INI_UNP_MKY_CDP OF 'COLBMCDP.TXT'
*% INCLUDE FIN_UNP_MKY_CDP OF 'COLBMCDP.TXT'
       FIN-UNP-MKY-CDP.
           EXIT.
       
       UNP-AKY-CDP SECTION.
       INI-UNP-AKY-CDP.
*% INCLUDE INI_UNP_AKY_CDP OF 'COLBMCDP.TXT'
*% INCLUDE FIN_UNP_AKY_CDP OF 'COLBMCDP.TXT'
       FIN-UNP-AKY-CDP.
           EXIT.
       
       UNP-FLD-CDP SECTION.
       INI-UNP-FLD-CDP.
*% INCLUDE INI_UNP_FLD_CDP OF 'COLBMCDP.TXT'
           PERFORM UNP-PLL-CDP.
*% INCLUDE FIN_UNP_FLD_CDP OF 'COLBMCDP.TXT'
       FIN-UNP-FLD-CDP.
           EXIT.
       
       UNP-PLL-CDP SECTION.
       INI-UNP-PLL-CDP.
*% INCLUDE INI_UNP_PLL_CDP OF 'COLBMCDP.TXT'
*% INCLUDE FIN_UNP_PLL_CDP OF 'COLBMCDP.TXT'
       FIN-UNP-PLL-CDP.
           EXIT.
       
       VAL-MKY-CDP SECTION.
       INI-VAL-MKY-CDP.
*% INCLUDE INI_VAL_MKY_CDP OF 'COLBMCDP.TXT'
*% INCLUDE FIN_VAL_MKY_CDP OF 'COLBMCDP.TXT'
       FIN-VAL-MKY-CDP.
           EXIT.
       
       VAL-FLD-CDP SECTION.
       INI-VAL-FLD-CDP.
*% INCLUDE INI_VAL_FLD_CDP OF 'COLBMCDP.TXT'
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * VRF-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * VRF-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-IND-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-IND-ICLI-LEN IN CDP-FLD
                GO TO FIN-VAL-FLD-CDP.
           PERFORM VAL-FRM-GLS-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-GLS-ICLI-LEN IN CDP-FLD
                GO TO FIN-VAL-FLD-CDP.
           PERFORM VAL-FRM-NUM-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-NUM-ICLI-LEN IN CDP-FLD
                GO TO FIN-VAL-FLD-CDP.
           PERFORM VAL-FRM-VRF-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-VRF-ICLI-LEN IN CDP-FLD
                GO TO FIN-VAL-FLD-CDP.
      * IDC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-GLS-NOMC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-NOMC-LEN IN CDP-FLD
                GO TO FIN-VAL-FLD-CDP.
           PERFORM VAL-FRM-NUM-IDPL
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-NUM-IDPL-LEN IN CDP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDP.
           PERFORM VAL-FRM-GLS-NOMS
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-GLS-NOMS-LEN IN CDP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDP.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLS"
      * VRF-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * VRF-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLS"
      * IDC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-IND-ICLS VARYING FRM-IFLD
                FROM 1 BY 1 UNTIL FRM-IFLD > 5 OR
                FRM-SUAR = FRM-SUAR-MAL OR
                FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO
                     FRM-IND-ICLS-LEN IN CDP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDP.
           PERFORM VAL-FRM-GLS-ICLS VARYING FRM-IFLD
                FROM 1 BY 1 UNTIL FRM-IFLD > 5 OR
                FRM-SUAR = FRM-SUAR-MAL OR
                FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO
                     FRM-GLS-ICLS-LEN IN CDP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDP.
           PERFORM VAL-FRM-NUM-ICLS VARYING FRM-IFLD
                FROM 1 BY 1 UNTIL FRM-IFLD > 5 OR
                FRM-SUAR = FRM-SUAR-MAL OR
                FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO
                     FRM-NUM-ICLS-LEN IN CDP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDP.
           PERFORM VAL-FRM-VRF-ICLS VARYING FRM-IFLD
                FROM 1 BY 1 UNTIL FRM-IFLD > 5 OR
                FRM-SUAR = FRM-SUAR-MAL OR
                FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO
                     FRM-VRF-ICLS-LEN IN CDP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDP.

      * IDC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-IND-NOVD
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-IND-NOVD-LEN IN CDP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDP.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FVEN VARYING FRM-IFLD
                FROM 1 BY 1 UNTIL FRM-IFLD > 5 OR
                FRM-SUAR = FRM-SUAR-MAL OR
                FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FVEN-LEN IN CDP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDP.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-CAI-IOPC
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-CAI-IOPC-LEN IN CDP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDP.
           PERFORM VAL-FRM-IIC-IOPC
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-IIC-IOPC-LEN IN CDP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDP.
           PERFORM VAL-FRM-NUM-IVEN
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-NUM-IVEN-LEN IN CDP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDP.
           PERFORM VAL-FRM-NUM-VCOR
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-NUM-VCOR-LEN IN CDP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDP.
           PERFORM VAL-FRM-COD-VCOC
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-COD-VCOC-LEN IN CDP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDP.
           PERFORM VAL-FRM-VAL-CUOT
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-CUOT-LEN IN CDP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDP.
           PERFORM VAL-FRM-VAL-MPAG
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-MPAG-LEN IN CDP-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDP.
*% INCLUDE FIN_VAL_FLD_CDP OF 'COLBMCDP.TXT'
       FIN-VAL-FLD-CDP.
           EXIT.
