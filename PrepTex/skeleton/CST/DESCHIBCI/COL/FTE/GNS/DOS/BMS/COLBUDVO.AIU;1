*% INI_LBL = TRUE
*% PGM_BU = TRUE
       RET-ALL-DVO SECTION.
       INI-RET-ALL-DVO.
*% INCLUDE INI_RET_ALL_DVO OF 'COLBMDVO.TXT'
           PERFORM KEY-ALL-DVO.
           PERFORM RET-DVO-DVO.
*% INCLUDE FIN_RET_ALL_DVO OF 'COLBMDVO.TXT'
       FIN-RET-ALL-DVO.
           EXIT.
       
       RET-DVO-DVO SECTION.
       INI-RET-DVO-DVO.
*% INCLUDE INI_RET_DVO_DVO OF 'COLBMDVO.TXT'
           MOVE DVO-SGV-ICTB-IPC IN DVO-FLD TO DVO-SGV-ICTB-IPC IN DVO.
           MOVE DVO-SGV-ICTB-ISD IN DVO-FLD TO DVO-SGV-ICTB-ISD IN DVO.
           MOVE DVO-SGV-ICTB-IMD IN DVO-FLD TO DVO-SGV-ICTB-IMD IN DVO.
           MOVE DVO-SGV-ICTB-RPC IN DVO-FLD TO DVO-SGV-ICTB-RPC IN DVO.
           MOVE DVO-SGV-ICTB-RSD IN DVO-FLD TO DVO-SGV-ICTB-RSD IN DVO.
           MOVE DVO-SGV-ICTB-RMD IN DVO-FLD TO DVO-SGV-ICTB-RMD IN DVO.
*% INCLUDE FIN_RET_DVO_DVO OF 'COLBMDVO.TXT'
       FIN-RET-DVO-DVO.
           EXIT.
       
       KEY-ALL-DVO SECTION.
       INI-KEY-ALL-DVO.
*% INCLUDE INI_KEY_ALL_DVO OF 'COLBMDVO.TXT'
           PERFORM KEY-DVO-DVO.
*% INCLUDE FIN_KEY_ALL_DVO OF 'COLBMDVO.TXT'
       FIN-KEY-ALL-DVO.
           EXIT.
       
       KEY-DVO-DVO SECTION.
       INI-KEY-DVO-DVO.
*% INCLUDE INI_KEY_DVO_DVO OF 'COLBMDVO.TXT'
           MOVE DVO-CAI-IOPC IN DVO-FLD TO DVO-CAI-IOPC IN DVO.
           MOVE DVO-IIC-IOPC IN DVO-FLD TO DVO-IIC-IOPC IN DVO.
           MOVE DVO-NUM-IDLC IN DVO-FLD TO DVO-NUM-IDLC IN DVO.
*% INCLUDE FIN_KEY_DVO_DVO OF 'COLBMDVO.TXT'
       FIN-KEY-DVO-DVO.
           EXIT.
       
       SCR-CLRA-DVO SECTION.
       INI-SCR-CLRA-DVO.
*% INCLUDE INI_SCR_CLRA_DVO OF 'COLBMDVO.TXT'
           PERFORM CLR-KEY-DVO.
           PERFORM CLR-FLD-DVO.
*% INCLUDE FIN_SCR_CLRA_DVO OF 'COLBMDVO.TXT'
       FIN-SCR-CLRA-DVO.
           EXIT.
       
       CLR-KEY-DVO SECTION.
       INI-CLR-KEY-DVO.
*% INCLUDE INI_CLR_KEY_DVO OF 'COLBMDVO.TXT'
           MOVE SPACES TO DVO-CAI-IOPC IN DVO-FLD.
           MOVE ZEROES TO DVO-IIC-IOPC IN DVO-FLD.
           MOVE ZEROES TO DVO-NUM-IDLC IN DVO-FLD.
*% INCLUDE FIN_CLR_KEY_DVO OF 'COLBMDVO.TXT'
       FIN-CLR-KEY-DVO.
           EXIT.
       
       CLR-FLD-DVO SECTION.
       INI-CLR-FLD-DVO.
*% INCLUDE INI_CLR_FLD_DVO OF 'COLBMDVO.TXT'
           PERFORM CLR-DVO-DVO.
*% INCLUDE FIN_CLR_FLD_DVO OF 'COLBMDVO.TXT'
       FIN-CLR-FLD-DVO.
           EXIT.
       
       CLR-DVO-DVO SECTION.
       INI-CLR-DVO-DVO.
*% INCLUDE INI_CLR_DVO_DVO OF 'COLBMDVO.TXT'
           MOVE ZEROES TO DVO-SGV-ICTB-IPC IN DVO-FLD.
           MOVE ZEROES TO DVO-SGV-ICTB-ISD IN DVO-FLD.
           MOVE ZEROES TO DVO-SGV-ICTB-IMD IN DVO-FLD.
           MOVE ZEROES TO DVO-SGV-ICTB-RPC IN DVO-FLD.
           MOVE ZEROES TO DVO-SGV-ICTB-RSD IN DVO-FLD.
           MOVE ZEROES TO DVO-SGV-ICTB-RMD IN DVO-FLD.
*% INCLUDE FIN_CLR_DVO_DVO OF 'COLBMDVO.TXT'
       FIN-CLR-DVO-DVO.
           EXIT.
       
       FST-KEY-DVO SECTION.
       INI-FST-KEY-DVO.
*% INCLUDE INI_FST_KEY_DVO OF 'COLBMDVO.TXT'
           MOVE -1 TO DVO-CAI-IOPC-LEN IN DVO-FLD.
*% INCLUDE FIN_FST_KEY_DVO OF 'COLBMDVO.TXT'
       FIN-FST-KEY-DVO.
           EXIT.
       
       FST-FLD-DVO SECTION.
       INI-FST-FLD-DVO.
*% INCLUDE INI_FST_FLD_DVO OF 'COLBMDVO.TXT'
           MOVE -1 TO DVO-SGV-ICTB-IPC-LEN IN DVO-FLD.
*% INCLUDE FIN_FST_FLD_DVO OF 'COLBMDVO.TXT'
       FIN-FST-FLD-DVO.
           EXIT.
       
       PRO-KEY-DVO SECTION.
       INI-PRO-KEY-DVO.
*% INCLUDE INI_PRO_KEY_DVO OF 'COLBMDVO.TXT'
           PERFORM PRO-MKY-DVO.
*% INCLUDE FIN_PRO_KEY_DVO OF 'COLBMDVO.TXT'
       FIN-PRO-KEY-DVO.
           EXIT.
       
       PRO-MKY-DVO SECTION.
       INI-PRO-MKY-DVO.
*% INCLUDE INI_PRO_MKY_DVO OF 'COLBMDVO.TXT'
*% INCLUDE FIN_PRO_MKY_DVO OF 'COLBMDVO.TXT'
       FIN-PRO-MKY-DVO.
           EXIT.
       
       PRO-FLD-DVO SECTION.
       INI-PRO-FLD-DVO.
*% INCLUDE INI_PRO_FLD_DVO OF 'COLBMDVO.TXT'
