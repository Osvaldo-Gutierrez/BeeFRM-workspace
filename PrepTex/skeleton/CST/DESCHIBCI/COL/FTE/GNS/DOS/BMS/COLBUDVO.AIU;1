*% LBL = "DVO_SGV_ICTB_RSD OF 'COLBMDVO.TXT'"
*% INCLUDE INI_DVO_SGV_ICTB_RSD OF 'COLBMDVO.TXT'
*% INCLUDE DVO_SGV_ICTB_RSD OF 'COLBMDVO.TXT'
*% INCLUDE FIN_DVO_SGV_ICTB_RSD OF 'COLBMDVO.TXT'
       FIN-VAL-DVO-SGV-ICTB-RSD.
           EXIT.
       
       VAL-DVO-SGV-ICTB-RMD SECTION.
       INI-VAL-DVO-SGV-ICTB-RMD.
*% LBL = "DVO_SGV_ICTB_RMD OF 'COLBMDVO.TXT'"
*% INCLUDE INI_DVO_SGV_ICTB_RMD OF 'COLBMDVO.TXT'
*% INCLUDE DVO_SGV_ICTB_RMD OF 'COLBMDVO.TXT'
