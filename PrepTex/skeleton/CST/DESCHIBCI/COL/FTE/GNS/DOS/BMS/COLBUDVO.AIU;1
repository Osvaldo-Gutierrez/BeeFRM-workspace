       UNP-DVO-DVO SECTION.
       INI-UNP-DVO-DVO.
*% INCLUDE INI_UNP_DVO_DVO OF 'COLBMDVO.TXT'
           MOVE FRM-CPIM-UBY TO DVO-SGV-ICTB-IPC-ATR IN DVO-FLD.
           MOVE FRM-CPIM-UBY TO DVO-SGV-ICTB-ISD-ATR IN DVO-FLD.
           MOVE FRM-CPIM-UBY TO DVO-SGV-ICTB-IMD-ATR IN DVO-FLD.
           MOVE FRM-CPIM-UBY TO DVO-SGV-ICTB-RPC-ATR IN DVO-FLD.
           MOVE FRM-CPIM-UBY TO DVO-SGV-ICTB-RSD-ATR IN DVO-FLD.
           MOVE FRM-CPIM-UBY TO DVO-SGV-ICTB-RMD-ATR IN DVO-FLD.
*% INCLUDE FIN_UNP_DVO_DVO OF 'COLBMDVO.TXT'
       FIN-UNP-DVO-DVO.
           EXIT.
       
       VAL-MKY-DVO SECTION.
       INI-VAL-MKY-DVO.
*% INCLUDE INI_VAL_MKY_DVO OF 'COLBMDVO.TXT'
*% INCLUDE FIN_VAL_MKY_DVO OF 'COLBMDVO.TXT'
       FIN-VAL-MKY-DVO.
           EXIT.
       
       VAL-FLD-DVO SECTION.
       INI-VAL-FLD-DVO.
*% INCLUDE INI_VAL_FLD_DVO OF 'COLBMDVO.TXT'
           PERFORM VAL-DVO-SGV-ICTB-IPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DVO-SGV-ICTB-IPC-LEN IN DVO-FLD
                GO TO FIN-VAL-FLD-DVO.
           PERFORM VAL-DVO-SGV-ICTB-ISD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DVO-SGV-ICTB-ISD-LEN IN DVO-FLD
                GO TO FIN-VAL-FLD-DVO.
           PERFORM VAL-DVO-SGV-ICTB-IMD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DVO-SGV-ICTB-IMD-LEN IN DVO-FLD
                GO TO FIN-VAL-FLD-DVO.
           PERFORM VAL-DVO-SGV-ICTB-RPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DVO-SGV-ICTB-RPC-LEN IN DVO-FLD
                GO TO FIN-VAL-FLD-DVO.
           PERFORM VAL-DVO-SGV-ICTB-RSD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DVO-SGV-ICTB-RSD-LEN IN DVO-FLD
                GO TO FIN-VAL-FLD-DVO.
           PERFORM VAL-DVO-SGV-ICTB-RMD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DVO-SGV-ICTB-RMD-LEN IN DVO-FLD
                GO TO FIN-VAL-FLD-DVO.
*% INCLUDE FIN_VAL_FLD_DVO OF 'COLBMDVO.TXT'
       FIN-VAL-FLD-DVO.
           EXIT.
       
       VAL-DVO-SGV-ICTB-IPC SECTION.
       INI-VAL-DVO-SGV-ICTB-IPC.
*% LBL = "DVO_SGV_ICTB_IPC OF 'COLBMDVO.TXT'"
*% INCLUDE INI_DVO_SGV_ICTB_IPC OF 'COLBMDVO.TXT'
*% INCLUDE DVO_SGV_ICTB_IPC OF 'COLBMDVO.TXT'
