*% INCLUDE VAL_CON_DVO OF 'COLBMDVO.TXT'
       FIN-VAL-CON-DVO.
           EXIT.
       
       VAL-NUM-KEY-DVO SECTION.
       INI-VAL-NUM-KEY-DVO.
           PERFORM VAL-NUM-IKY-DVO.
           PERFORM VAL-NUM-MKY-DVO.
           PERFORM VAL-NUM-AKY-DVO.
       FIN-VAL-NUM-KEY-DVO.
           EXIT.
       
       VAL-NUM-IKY-DVO SECTION.
       INI-VAL-NUM-IKY-DVO.
*% INCLUDE INI_VAL_NUM_IKY_DVO OF 'COLBMDVO.TXT'
       VAL-NUM-DVO-IIC-IOPC.
*% BUF = "DVO-FLD"
*% FLD = "DVO-IIC-IOPC"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-DVO.
       VAL-NUM-DVO-NUM-IDLC.
*% BUF = "DVO-FLD"
*% FLD = "DVO-NUM-IDLC"
*% ENT = "3"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-DVO.
*% INCLUDE FIN_VAL_NUM_IKY_DVO OF 'COLBMDVO.TXT'
       FIN-VAL-NUM-IKY-DVO.
