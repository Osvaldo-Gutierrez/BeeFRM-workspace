*% INCLUDE FIN_PRO_FLD_DVO OF 'COLBMDVO.TXT'
       FIN-PRO-FLD-DVO.
           EXIT.
       
       PRO-DVO-DVO SECTION.
       INI-PRO-DVO-DVO.
*% INCLUDE INI_PRO_DVO_DVO OF 'COLBMDVO.TXT'
           MOVE FRM-CPIM-AEY TO DVO-SGV-ICTB-IPC-ATR IN DVO-FLD.
           MOVE FRM-CPIM-AEY TO DVO-SGV-ICTB-ISD-ATR IN DVO-FLD.
           MOVE FRM-CPIM-AEY TO DVO-SGV-ICTB-IMD-ATR IN DVO-FLD.
           MOVE FRM-CPIM-AEY TO DVO-SGV-ICTB-RPC-ATR IN DVO-FLD.
           MOVE FRM-CPIM-AEY TO DVO-SGV-ICTB-RSD-ATR IN DVO-FLD.
           MOVE FRM-CPIM-AEY TO DVO-SGV-ICTB-RMD-ATR IN DVO-FLD.
*% INCLUDE FIN_PRO_DVO_DVO OF 'COLBMDVO.TXT'
       FIN-PRO-DVO-DVO.
           EXIT.
       
       UNP-KEY-DVO SECTION.
       INI-UNP-KEY-DVO.
*% INCLUDE INI_UNP_KEY_DVO OF 'COLBMDVO.TXT'
           PERFORM UNP-IKY-DVO.
           PERFORM UNP-AKY-DVO.
           PERFORM UNP-MKY-DVO.
*% INCLUDE FIN_UNP_KEY_DVO OF 'COLBMDVO.TXT'
       FIN-UNP-KEY-DVO.
           EXIT.
       
       UNP-IKY-DVO SECTION.
       INI-UNP-IKY-DVO.
*% INCLUDE INI_UNP_IKY_DVO OF 'COLBMDVO.TXT'
           MOVE FRM-CPIM-UBY TO DVO-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO DVO-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO DVO-NUM-IDLC-ATR.
*% INCLUDE FIN_UNP_IKY_DVO OF 'COLBMDVO.TXT'
       FIN-UNP-IKY-DVO.
           EXIT.
       
