      *
      * COLBQCMP
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:26 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CMP"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMCMP.TXT'
       SET-TAG-COLBQCMP SECTION.
       INI-SET-TAG-COLBQCMP.
           MOVE
           '<IDNMOD NOM=$COLBQCMP$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:26 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCMP.
           EXIT.
           
       PUT-ALL-CMP SECTION.
       INI-PUT-ALL-CMP.
*% INCLUDE INI_PUT_ALL_CMP OF 'COLBMCMP.TXT'
           PERFORM PUT-PTT-CMP.
*% INCLUDE FIN_PUT_ALL_CMP OF 'COLBMCMP.TXT'
       FIN-PUT-ALL-CMP.
           EXIT.
       
       PUT-PTT-CMP SECTION.
       INI-PUT-PTT-CMP.
*% INCLUDE INI_PUT_PTT_CMP OF 'COLBMCMP.TXT'
           IF PGM-STAT-PTT-NUL
               GO TO FIN-PUT-PTT-CMP.
           IF NOT PGM-STAT-PTT-OKS
               PERFORM CLR-PTT-CMP
               GO TO FIN-PUT-PTT-CMP.
           MOVE PTT-CAI-ICVN IN PTT TO PTT-CAI-ICVN IN CMP-FLD.
           MOVE PTT-IIC-ICVN IN PTT TO PTT-IIC-ICVN IN CMP-FLD.
           MOVE PTT-COD-TIPO IN PTT TO PTT-COD-TIPO IN CMP-FLD.
           MOVE PTT-IND-SEPA IN PTT TO PTT-IND-SEPA IN CMP-FLD.
*% INCLUDE FIN_PUT_PTT_CMP OF 'COLBMCMP.TXT'
       FIN-PUT-PTT-CMP.
           EXIT.
       
       KEY-ALL-CMP SECTION.
       INI-KEY-ALL-CMP.
*% INCLUDE INI_KEY_ALL_CMP OF 'COLBMCMP.TXT'
           PERFORM KEY-PTT-CMP.
*% INCLUDE FIN_KEY_ALL_CMP OF 'COLBMCMP.TXT'
       FIN-KEY-ALL-CMP.
           EXIT.
       
       KEY-PTT-CMP SECTION.
       INI-KEY-PTT-CMP.
*% INCLUDE INI_KEY_PTT_CMP OF 'COLBMCMP.TXT'
           MOVE PTT-CAI-ICVN IN CMP-FLD TO PTT-CAI-ICVN IN PTT.
           MOVE PTT-IIC-ICVN IN CMP-FLD TO PTT-IIC-ICVN IN PTT.
           MOVE PTT-COD-TIPO IN CMP-FLD TO PTT-COD-TIPO IN PTT.
           MOVE PTT-IND-SEPA IN CMP-FLD TO PTT-IND-SEPA IN PTT.
*% INCLUDE FIN_KEY_PTT_CMP OF 'COLBMCMP.TXT'
       FIN-KEY-PTT-CMP.
           EXIT.
       
       SCR-CLRA-CMP SECTION.
       INI-SCR-CLRA-CMP.
*% INCLUDE INI_SCR_CLRA_CMP OF 'COLBMCMP.TXT'
           PERFORM CLR-KEY-CMP.
           PERFORM CLR-FLD-CMP.
*% INCLUDE FIN_SCR_CLRA_CMP OF 'COLBMCMP.TXT'
       FIN-SCR-CLRA-CMP.
           EXIT.
       
       CLR-KEY-CMP SECTION.
       INI-CLR-KEY-CMP.
*% INCLUDE INI_CLR_KEY_CMP OF 'COLBMCMP.TXT'
           MOVE SPACES TO PTT-CAI-ICVN IN CMP-FLD.
           MOVE ZEROES TO PTT-IIC-ICVN IN CMP-FLD.
           MOVE SPACES TO FRM-NUM-CPTT IN CMP-FLD.
           MOVE ZEROES TO FRM-NUM-IPTT IN CMP-FLD.
           MOVE SPACES TO PTT-COD-TIPO IN CMP-FLD.
           MOVE SPACES TO PTT-IND-SEPA IN CMP-FLD.
*% INCLUDE FIN_CLR_KEY_CMP OF 'COLBMCMP.TXT'
       FIN-CLR-KEY-CMP.
           EXIT.
       
       CLR-FLD-CMP SECTION.
       INI-CLR-FLD-CMP.
*% INCLUDE INI_CLR_FLD_CMP OF 'COLBMCMP.TXT'
           MOVE SPACES TO FRM-GLS-TIPO IN CMP-FLD.
           MOVE SPACES TO FRM-CAI-IPTT IN CMP-FLD(1).
           MOVE SPACES TO FRM-CAI-IPTT IN CMP-FLD(2).
           MOVE SPACES TO FRM-CAI-IPTT IN CMP-FLD(3).
           MOVE SPACES TO FRM-CAI-IPTT IN CMP-FLD(4).
           MOVE SPACES TO FRM-CAI-IPTT IN CMP-FLD(5).
           MOVE SPACES TO FRM-CAI-IPTT IN CMP-FLD(6).
           MOVE SPACES TO FRM-CAI-IPTT IN CMP-FLD(7).
           MOVE SPACES TO FRM-CAI-IPTT IN CMP-FLD(8).
           MOVE SPACES TO FRM-CAI-IPTT IN CMP-FLD(9).
           MOVE ZEROES TO FRM-IIC-IPTT IN CMP-FLD(1).
           MOVE ZEROES TO FRM-IIC-IPTT IN CMP-FLD(2).
           MOVE ZEROES TO FRM-IIC-IPTT IN CMP-FLD(3).
           MOVE ZEROES TO FRM-IIC-IPTT IN CMP-FLD(4).
           MOVE ZEROES TO FRM-IIC-IPTT IN CMP-FLD(5).
           MOVE ZEROES TO FRM-IIC-IPTT IN CMP-FLD(6).
           MOVE ZEROES TO FRM-IIC-IPTT IN CMP-FLD(7).
           MOVE ZEROES TO FRM-IIC-IPTT IN CMP-FLD(8).
           MOVE ZEROES TO FRM-IIC-IPTT IN CMP-FLD(9).
           MOVE SPACES TO FRM-GLS-NOMP IN CMP-FLD(1).
           MOVE SPACES TO FRM-GLS-NOMP IN CMP-FLD(2).
           MOVE SPACES TO FRM-GLS-NOMP IN CMP-FLD(3).
           MOVE SPACES TO FRM-GLS-NOMP IN CMP-FLD(4).
           MOVE SPACES TO FRM-GLS-NOMP IN CMP-FLD(5).
           MOVE SPACES TO FRM-GLS-NOMP IN CMP-FLD(6).
           MOVE SPACES TO FRM-GLS-NOMP IN CMP-FLD(7).
           MOVE SPACES TO FRM-GLS-NOMP IN CMP-FLD(8).
           MOVE SPACES TO FRM-GLS-NOMP IN CMP-FLD(9).
           MOVE SPACES TO FRM-COD-TIPO IN CMP-FLD(1).
           MOVE SPACES TO FRM-COD-TIPO IN CMP-FLD(2).
           MOVE SPACES TO FRM-COD-TIPO IN CMP-FLD(3).
           MOVE SPACES TO FRM-COD-TIPO IN CMP-FLD(4).
           MOVE SPACES TO FRM-COD-TIPO IN CMP-FLD(5).
           MOVE SPACES TO FRM-COD-TIPO IN CMP-FLD(6).
           MOVE SPACES TO FRM-COD-TIPO IN CMP-FLD(7).
           MOVE SPACES TO FRM-COD-TIPO IN CMP-FLD(8).
           MOVE SPACES TO FRM-COD-TIPO IN CMP-FLD(9).
           MOVE SPACES TO FRM-IND-SEPA IN CMP-FLD(1).
           MOVE SPACES TO FRM-IND-SEPA IN CMP-FLD(2).
           MOVE SPACES TO FRM-IND-SEPA IN CMP-FLD(3).
           MOVE SPACES TO FRM-IND-SEPA IN CMP-FLD(4).
           MOVE SPACES TO FRM-IND-SEPA IN CMP-FLD(5).
           MOVE SPACES TO FRM-IND-SEPA IN CMP-FLD(6).
           MOVE SPACES TO FRM-IND-SEPA IN CMP-FLD(7).
           MOVE SPACES TO FRM-IND-SEPA IN CMP-FLD(8).
           MOVE SPACES TO FRM-IND-SEPA IN CMP-FLD(9).
           PERFORM CLR-PTT-CMP.
*% INCLUDE FIN_CLR_FLD_CMP OF 'COLBMCMP.TXT'
       FIN-CLR-FLD-CMP.
           EXIT.
       
       CLR-PTT-CMP SECTION.
       INI-CLR-PTT-CMP.
*% INCLUDE INI_CLR_PTT_CMP OF 'COLBMCMP.TXT'
*% INCLUDE FIN_CLR_PTT_CMP OF 'COLBMCMP.TXT'
       FIN-CLR-PTT-CMP.
           EXIT.
       
       FST-KEY-CMP SECTION.
       INI-FST-KEY-CMP.
*% INCLUDE INI_FST_KEY_CMP OF 'COLBMCMP.TXT'
           MOVE -1 TO PTT-CAI-ICVN-LEN IN CMP-FLD.
*% INCLUDE FIN_FST_KEY_CMP OF 'COLBMCMP.TXT'
       FIN-FST-KEY-CMP.
           EXIT.
       
       FST-FLD-CMP SECTION.
       INI-FST-FLD-CMP.
*% INCLUDE INI_FST_FLD_CMP OF 'COLBMCMP.TXT'
*% INCLUDE FIN_FST_FLD_CMP OF 'COLBMCMP.TXT'
       FIN-FST-FLD-CMP.
           EXIT.
       
       PRO-KEY-CMP SECTION.
       INI-PRO-KEY-CMP.
*% INCLUDE INI_PRO_KEY_CMP OF 'COLBMCMP.TXT'
           MOVE FRM-CPIM-AEY TO FRM-NUM-CPTT-ATR IN CMP-FLD.
           MOVE FRM-CPIM-AEY TO FRM-NUM-IPTT-ATR IN CMP-FLD.
           PERFORM PRO-IKY-CMP.
           PERFORM PRO-AKY-CMP.
           PERFORM PRO-MKY-CMP.
*% INCLUDE FIN_PRO_KEY_CMP OF 'COLBMCMP.TXT'
       FIN-PRO-KEY-CMP.
           EXIT.
       
       PRO-IKY-CMP SECTION.
       INI-PRO-IKY-CMP.
*% INCLUDE INI_PRO_IKY_CMP OF 'COLBMCMP.TXT'
           MOVE FRM-CPIM-AEY TO PTT-CAI-ICVN-ATR IN CMP-FLD.
           MOVE FRM-CPIM-AEY TO PTT-IIC-ICVN-ATR IN CMP-FLD.
           MOVE FRM-CPIM-AEY TO PTT-COD-TIPO-ATR IN CMP-FLD.
           MOVE FRM-CPIM-AEY TO PTT-IND-SEPA-ATR IN CMP-FLD.
*% INCLUDE FIN_PRO_IKY_CMP OF 'COLBMCMP.TXT'
       FIN-PRO-IKY-CMP.
           EXIT.
       
       PRO-MKY-CMP SECTION.
       INI-PRO-MKY-CMP.
*% INCLUDE INI_PRO_MKY_CMP OF 'COLBMCMP.TXT'
*% INCLUDE FIN_PRO_MKY_CMP OF 'COLBMCMP.TXT'
       FIN-PRO-MKY-CMP.
           EXIT.
       
       PRO-AKY-CMP SECTION.
       INI-PRO-AKY-CMP.
*% INCLUDE INI_PRO_AKY_CMP OF 'COLBMCMP.TXT'
*% INCLUDE FIN_PRO_AKY_CMP OF 'COLBMCMP.TXT'
       FIN-PRO-AKY-CMP.
           EXIT.
       
       UNP-KEY-CMP SECTION.
       INI-UNP-KEY-CMP.
*% INCLUDE INI_UNP_KEY_CMP OF 'COLBMCMP.TXT'
           MOVE FRM-CPIM-UBY TO FRM-NUM-CPTT-ATR IN CMP-FLD.
           MOVE FRM-CPIM-UBY TO FRM-NUM-IPTT-ATR IN CMP-FLD.
           PERFORM UNP-IKY-CMP.
           PERFORM UNP-AKY-CMP.
           PERFORM UNP-MKY-CMP.
*% INCLUDE FIN_UNP_KEY_CMP OF 'COLBMCMP.TXT'
       FIN-UNP-KEY-CMP.
           EXIT.
       
       UNP-IKY-CMP SECTION.
       INI-UNP-IKY-CMP.
*% INCLUDE INI_UNP_IKY_CMP OF 'COLBMCMP.TXT'
           MOVE FRM-CPIM-UBY TO PTT-CAI-ICVN-ATR.
           MOVE FRM-CPIM-UBY TO PTT-IIC-ICVN-ATR.
           MOVE FRM-CPIM-UBY TO FRM-NUM-CPTT-ATR.
           MOVE FRM-CPIM-UBY TO FRM-NUM-IPTT-ATR.
           MOVE FRM-CPIM-UBY TO PTT-COD-TIPO-ATR.
           MOVE FRM-CPIM-UBY TO PTT-IND-SEPA-ATR.
*% INCLUDE FIN_UNP_IKY_CMP OF 'COLBMCMP.TXT'
       FIN-UNP-IKY-CMP.
           EXIT.
       
       UNP-MKY-CMP SECTION.
       INI-UNP-MKY-CMP.
*% INCLUDE INI_UNP_MKY_CMP OF 'COLBMCMP.TXT'
*% INCLUDE FIN_UNP_MKY_CMP OF 'COLBMCMP.TXT'
       FIN-UNP-MKY-CMP.
           EXIT.
       
       UNP-AKY-CMP SECTION.
       INI-UNP-AKY-CMP.
*% INCLUDE INI_UNP_AKY_CMP OF 'COLBMCMP.TXT'
*% INCLUDE FIN_UNP_AKY_CMP OF 'COLBMCMP.TXT'
       FIN-UNP-AKY-CMP.
           EXIT.
       
       UNP-FLD-CMP SECTION.
       INI-UNP-FLD-CMP.
*% INCLUDE INI_UNP_FLD_CMP OF 'COLBMCMP.TXT'
           PERFORM UNP-PTT-CMP.
*% INCLUDE FIN_UNP_FLD_CMP OF 'COLBMCMP.TXT'
       FIN-UNP-FLD-CMP.
           EXIT.
       
       UNP-PTT-CMP SECTION.
       INI-UNP-PTT-CMP.
*% INCLUDE INI_UNP_PTT_CMP OF 'COLBMCMP.TXT'
*% INCLUDE FIN_UNP_PTT_CMP OF 'COLBMCMP.TXT'
       FIN-UNP-PTT-CMP.
           EXIT.
       
       VAL-KEY-CMP SECTION.
       INI-VAL-KEY-CMP.
           PERFORM VAL-IKY-CMP.
           PERFORM VAL-MKY-CMP.
           PERFORM VAL-AKY-CMP.
       FIN-VAL-KEY-CMP.
           EXIT.
       
       VAL-IKY-CMP SECTION.
       INI-VAL-IKY-CMP.
*% INCLUDE INI_VAL_IKY_CMP OF 'COLBMCMP.TXT'
           PERFORM VAL-PTT-CAI-ICVN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PTT-CAI-ICVN-LEN IN CMP-FLD
                GO TO FIN-VAL-IKY-CMP.
           PERFORM VAL-PTT-IIC-ICVN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PTT-IIC-ICVN-LEN IN CMP-FLD
                GO TO FIN-VAL-IKY-CMP.
           PERFORM VAL-FRM-NUM-CPTT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-NUM-CPTT-LEN IN CMP-FLD
                GO TO FIN-VAL-IKY-CMP.
           PERFORM VAL-FRM-NUM-IPTT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-NUM-IPTT-LEN IN CMP-FLD
                GO TO FIN-VAL-IKY-CMP.
           PERFORM VAL-PTT-COD-TIPO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PTT-COD-TIPO-LEN IN CMP-FLD
                GO TO FIN-VAL-IKY-CMP.
           PERFORM VAL-PTT-IND-SEPA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PTT-IND-SEPA-LEN IN CMP-FLD
                GO TO FIN-VAL-IKY-CMP.
*% INCLUDE FIN_VAL_IKY_CMP OF 'COLBMCMP.TXT'
       FIN-VAL-IKY-CMP.
           EXIT.
       
       VAL-MKY-CMP SECTION.
       INI-VAL-MKY-CMP.
*% INCLUDE INI_VAL_MKY_CMP OF 'COLBMCMP.TXT'
*% INCLUDE FIN_VAL_MKY_CMP OF 'COLBMCMP.TXT'
       FIN-VAL-MKY-CMP.
           EXIT.
       
       VAL-AKY-CMP SECTION.
       INI-VAL-AKY-CMP.
*% INCLUDE INI_VAL_AKY_CMP OF 'COLBMCMP.TXT'
*% INCLUDE FIN_VAL_AKY_CMP OF 'COLBMCMP.TXT'
       FIN-VAL-AKY-CMP.
           EXIT.
       
       VAL-PTT-CAI-ICVN SECTION.
       INI-VAL-PTT-CAI-ICVN.
*% LBL = "PTT_CAI_ICVN OF 'COLBMCMP.TXT'"
*% INCLUDE INI_PTT_CAI_ICVN OF 'COLBMCMP.TXT'
           IF PTT-CAI-ICVN IN CMP-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-PTT-CAI-ICVN.
*% INCLUDE PTT_CAI_ICVN OF 'COLBMCMP.TXT'
*% INCLUDE FIN_PTT_CAI_ICVN OF 'COLBMCMP.TXT'
       FIN-VAL-PTT-CAI-ICVN.
           EXIT.
       
       VAL-PTT-IIC-ICVN SECTION.
       INI-VAL-PTT-IIC-ICVN.
*% LBL = "PTT_IIC_ICVN OF 'COLBMCMP.TXT'"
*% INCLUDE INI_PTT_IIC_ICVN OF 'COLBMCMP.TXT'
           IF PTT-IIC-ICVN IN CMP-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-PTT-IIC-ICVN.
*% INCLUDE PTT_IIC_ICVN OF 'COLBMCMP.TXT'
*% INCLUDE FIN_PTT_IIC_ICVN OF 'COLBMCMP.TXT'
       FIN-VAL-PTT-IIC-ICVN.
           EXIT.
       
       VAL-FRM-NUM-CPTT SECTION.
       INI-VAL-FRM-NUM-CPTT.
*% LBL = "FRM_NUM_CPTT OF 'COLBMCMP.TXT'"
*% INCLUDE INI_FRM_NUM_CPTT OF 'COLBMCMP.TXT'
           IF FRM-NUM-CPTT IN CMP-FLD NOT > SPACES
              MOVE SPACES TO FRM-NUM-CPTT IN CMP-FLD.
*% INCLUDE FRM_NUM_CPTT OF 'COLBMCMP.TXT'
*% INCLUDE FIN_FRM_NUM_CPTT OF 'COLBMCMP.TXT'
       FIN-VAL-FRM-NUM-CPTT.
           EXIT.
       
       VAL-FRM-NUM-IPTT SECTION.
       INI-VAL-FRM-NUM-IPTT.
*% LBL = "FRM_NUM_IPTT OF 'COLBMCMP.TXT'"
*% INCLUDE INI_FRM_NUM_IPTT OF 'COLBMCMP.TXT'
*% INCLUDE FRM_NUM_IPTT OF 'COLBMCMP.TXT'
*% INCLUDE FIN_FRM_NUM_IPTT OF 'COLBMCMP.TXT'
       FIN-VAL-FRM-NUM-IPTT.
           EXIT.
       
       VAL-PTT-COD-TIPO SECTION.
       INI-VAL-PTT-COD-TIPO.
*% LBL = "PTT_COD_TIPO OF 'COLBMCMP.TXT'"
*% INCLUDE INI_PTT_COD_TIPO OF 'COLBMCMP.TXT'
           IF PTT-COD-TIPO IN CMP-FLD NOT > SPACES
              MOVE SPACES TO PTT-COD-TIPO IN CMP-FLD.
*% INCLUDE PTT_COD_TIPO OF 'COLBMCMP.TXT'
*% INCLUDE FIN_PTT_COD_TIPO OF 'COLBMCMP.TXT'
       FIN-VAL-PTT-COD-TIPO.
           EXIT.
       
       VAL-PTT-IND-SEPA SECTION.
       INI-VAL-PTT-IND-SEPA.
*% LBL = "PTT_IND_SEPA OF 'COLBMCMP.TXT'"
*% INCLUDE INI_PTT_IND_SEPA OF 'COLBMCMP.TXT'
           IF PTT-IND-SEPA IN CMP-FLD NOT > SPACES
              MOVE SPACES TO PTT-IND-SEPA IN CMP-FLD.
*% INCLUDE PTT_IND_SEPA OF 'COLBMCMP.TXT'
*% INCLUDE FIN_PTT_IND_SEPA OF 'COLBMCMP.TXT'
       FIN-VAL-PTT-IND-SEPA.
           EXIT.
       
       VAL-CON-KEY-CMP SECTION.
       INI-VAL-CON-KEY-CMP.
*% INCLUDE INI_VAL_CON_KEY_CMP OF 'COLBMCMP.TXT'
           IF     PTT-CAI-ICVN IN CMP-FLD NOT > SPACES 
              AND PTT-IIC-ICVN IN CMP-FLD NOT > ZEROES 
              AND FRM-NUM-CPTT IN CMP-FLD NOT > SPACES 
              AND FRM-NUM-IPTT IN CMP-FLD NOT > ZEROES 
              AND PTT-COD-TIPO IN CMP-FLD NOT > SPACES 
              AND PTT-IND-SEPA IN CMP-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CMP.
*% INCLUDE FIN_VAL_CON_KEY_CMP OF 'COLBMCMP.TXT'
       FIN-VAL-CON-KEY-CMP.
           EXIT.
       
       VAL-CON-IKY-CMP SECTION.
       INI-VAL-CON-IKY-CMP.
*% INCLUDE INI_VAL_CON_IKY_CMP OF 'COLBMCMP.TXT'
           IF     PTT-CAI-ICVN IN CMP-FLD NOT > SPACES 
              AND PTT-IIC-ICVN IN CMP-FLD NOT > ZEROES 
              AND FRM-NUM-CPTT IN CMP-FLD NOT > SPACES 
              AND FRM-NUM-IPTT IN CMP-FLD NOT > ZEROES 
              AND PTT-COD-TIPO IN CMP-FLD NOT > SPACES 
              AND PTT-IND-SEPA IN CMP-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CMP.
*% INCLUDE FIN_VAL_CON_IKY_CMP OF 'COLBMCMP.TXT'
       FIN-VAL-CON-IKY-CMP.
           EXIT.
       
       VAL-NUM-KEY-CMP SECTION.
       INI-VAL-NUM-KEY-CMP.
           PERFORM VAL-NUM-IKY-CMP.
           PERFORM VAL-NUM-MKY-CMP.
           PERFORM VAL-NUM-AKY-CMP.
       FIN-VAL-NUM-KEY-CMP.
           EXIT.
       
       VAL-NUM-IKY-CMP SECTION.
       INI-VAL-NUM-IKY-CMP.
*% INCLUDE INI_VAL_NUM_IKY_CMP OF 'COLBMCMP.TXT'
       VAL-NUM-PTT-IIC-ICVN.
*% BUF = "CMP-FLD"
*% FLD = "PTT-IIC-ICVN"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-CMP.
       VAL-NUM-FRM-NUM-IPTT.
*% BUF = "CMP-FLD"
*% FLD = "FRM-NUM-IPTT"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-CMP.
*% INCLUDE FIN_VAL_NUM_IKY_CMP OF 'COLBMCMP.TXT'
       FIN-VAL-NUM-IKY-CMP.
           EXIT.
       
       VAL-NUM-MKY-CMP SECTION.
       INI-VAL-NUM-MKY-CMP.
*% INCLUDE INI_VAL_NUM_MKY_CMP OF 'COLBMCMP.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_CMP OF 'COLBMCMP.TXT'
       FIN-VAL-NUM-MKY-CMP.
           EXIT.
       
       VAL-NUM-AKY-CMP SECTION.
       INI-VAL-NUM-AKY-CMP.
*% INCLUDE INI_VAL_NUM_AKY_CMP OF 'COLBMCMP.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_CMP OF 'COLBMCMP.TXT'
       FIN-VAL-NUM-AKY-CMP.
           EXIT.
       
       VAL-NUM-FLD-CMP SECTION.
       INI-VAL-NUM-FLD-CMP.
*% INCLUDE INI_VAL_NUM_FLD_CMP OF 'COLBMCMP.TXT'
       VAL-NUM-FRM-IIC-IPTT-1.
*% BUF = "CMP-FLD(1)"
*% FLD = "FRM-IIC-IPTT"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-CMP.
       VAL-NUM-FRM-IIC-IPTT-2.
*% BUF = "CMP-FLD(2)"
*% FLD = "FRM-IIC-IPTT"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-CMP.
       VAL-NUM-FRM-IIC-IPTT-3.
*% BUF = "CMP-FLD(3)"
*% FLD = "FRM-IIC-IPTT"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
