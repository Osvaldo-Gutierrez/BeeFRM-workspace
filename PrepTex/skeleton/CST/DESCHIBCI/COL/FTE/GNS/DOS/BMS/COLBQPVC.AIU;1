      *
      * COLBQPVC
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:46 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "PVC"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMPVC.TXT'
       SET-TAG-COLBQPVC SECTION.
       INI-SET-TAG-COLBQPVC.
           MOVE
           '<IDNMOD NOM=$COLBQPVC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:46 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQPVC.
           EXIT.
           
       PUT-ALL-PVC SECTION.
       INI-PUT-ALL-PVC.
*% INCLUDE INI_PUT_ALL_PVC OF 'COLBMPVC.TXT'
           PERFORM PUT-LAP-PVC.
           PERFORM PUT-PVC-PVC.
*% INCLUDE FIN_PUT_ALL_PVC OF 'COLBMPVC.TXT'
       FIN-PUT-ALL-PVC.
           EXIT.
       
       PUT-LAP-PVC SECTION.
       INI-PUT-LAP-PVC.
*% INCLUDE INI_PUT_LAP_PVC OF 'COLBMPVC.TXT'
           IF PGM-STAT-LAP-NUL
               GO TO FIN-PUT-LAP-PVC.
           IF NOT PGM-STAT-LAP-OKS
               PERFORM CLR-LAP-PVC
               GO TO FIN-PUT-LAP-PVC.
           MOVE LAP-COD-COME IN LAP TO LAP-COD-COME IN PVC-FLD.
*% INCLUDE FIN_PUT_LAP_PVC OF 'COLBMPVC.TXT'
       FIN-PUT-LAP-PVC.
           EXIT.
       
       PUT-PVC-PVC SECTION.
       INI-PUT-PVC-PVC.
*% INCLUDE INI_PUT_PVC_PVC OF 'COLBMPVC.TXT'
           IF PGM-STAT-PVC-NUL
               GO TO FIN-PUT-PVC-PVC.
           IF NOT PGM-STAT-PVC-OKS
               PERFORM CLR-PVC-PVC
               GO TO FIN-PUT-PVC-PVC.
           MOVE PVC-KEY-IPVC IN PVC TO PVC-KEY-IPVC IN PVC-FLD.
           MOVE PVC-NUM-SVMI IN PVC TO PVC-NUM-SVMI IN PVC-FLD.
           MOVE PVC-IND-SVMI IN PVC TO PVC-IND-SVMI IN PVC-FLD.
           MOVE PVC-NUM-VIMI IN PVC TO PVC-NUM-VIMI IN PVC-FLD.
           MOVE PVC-NUM-SVMA IN PVC TO PVC-NUM-SVMA IN PVC-FLD.
           MOVE PVC-NUM-VIMA IN PVC TO PVC-NUM-VIMA IN PVC-FLD.
           MOVE PVC-IND-SVMA IN PVC TO PVC-IND-SVMA IN PVC-FLD.
           MOVE PVC-NUM-DVMI IN PVC TO PVC-NUM-DVMI IN PVC-FLD.
           MOVE PVC-MSC-TCVE IN PVC TO PVC-MSC-TCVE IN PVC-FLD.
           MOVE PVC-IND-DVMI IN PVC TO PVC-IND-DVMI IN PVC-FLD.
           MOVE PVC-NUM-DVMA IN PVC TO PVC-NUM-DVMA IN PVC-FLD.
           MOVE PVC-COD-VCOC IN PVC TO PVC-COD-VCOC IN PVC-FLD.
           MOVE PVC-IND-DVMA IN PVC TO PVC-IND-DVMA IN PVC-FLD.
           MOVE PVC-VAL-VVMI IN PVC TO PVC-VAL-VVMI IN PVC-FLD.
           MOVE PVC-NUM-SPMI IN PVC TO PVC-NUM-SPMI IN PVC-FLD.
           MOVE PVC-VAL-VVMA IN PVC TO PVC-VAL-VVMA IN PVC-FLD.
           MOVE PVC-IND-SPMI IN PVC TO PVC-IND-SPMI IN PVC-FLD.
           MOVE PVC-NUM-SPMA IN PVC TO PVC-NUM-SPMA IN PVC-FLD.
           MOVE PVC-SGV-FMMI IN PVC TO PVC-SGV-FMMI IN PVC-FLD.
           MOVE PVC-IND-SPMA IN PVC TO PVC-IND-SPMA IN PVC-FLD.
           MOVE PVC-SGV-FMMA IN PVC TO PVC-SGV-FMMA IN PVC-FLD.
           MOVE PVC-SGV-FPMI IN PVC TO PVC-SGV-FPMI IN PVC-FLD.
           MOVE PVC-NUM-DESD IN PVC TO PVC-NUM-DESD IN PVC-FLD.
           MOVE PVC-SGV-FPMA IN PVC TO PVC-SGV-FPMA IN PVC-FLD.
*% BUF = "PVC-FLD"
*% SIS = "TAB"
*% TAB = "IPER"
*% FLD = "PVC-IND-SVMI"
*% GLS = "FRM-GLS-SVMI"
*% TIENE_GLOSA = TRUE
*% LGL = 7
*% INCLUDE 'BUSMSC'
*% BUF = "PVC-FLD"
*% SIS = "TAB"
*% TAB = "IPER"
*% FLD = "PVC-IND-SVMA"
*% GLS = "FRM-GLS-SVMA"
*% TIENE_GLOSA = TRUE
*% LGL = 7
*% INCLUDE 'BUSMSC'
*% BUF = "PVC-FLD"
*% SIS = "COL"
*% TAB = "CVC "
*% FLD = "PVC-MSC-TCVE"
*% GLS = "FRM-GLS-TCVE"
*% TIENE_GLOSA = TRUE
*% LGL = 41
*% INCLUDE 'BUSTAB'
*% BUF = "PVC-FLD"
*% SIS = "TAB"
*% TAB = "IPER"
*% FLD = "PVC-IND-DVMI"
*% GLS = "FRM-GLS-DVMI"
*% TIENE_GLOSA = TRUE
*% LGL = 7
*% INCLUDE 'BUSMSC'
*% BUF = "PVC-FLD"
*% SIS = "TAB"
*% TAB = "VLR "
*% FLD = "PVC-COD-VCOC"
*% GLS = "FRM-GLS-VCOC"
*% TIENE_GLOSA = TRUE
*% LGL = 41
*% INCLUDE 'BUSTAB'
*% BUF = "PVC-FLD"
*% SIS = "TAB"
*% TAB = "IPER"
*% FLD = "PVC-IND-DVMA"
*% GLS = "FRM-GLS-DVMA"
*% TIENE_GLOSA = TRUE
*% LGL = 7
*% INCLUDE 'BUSMSC'
*% BUF = "PVC-FLD"
*% SIS = "TAB"
*% TAB = "IPER"
*% FLD = "PVC-IND-SPMI"
*% GLS = "FRM-GLS-SPMI"
*% TIENE_GLOSA = TRUE
*% LGL = 7
*% INCLUDE 'BUSMSC'
*% BUF = "PVC-FLD"
*% SIS = "TAB"
*% TAB = "IPER"
*% FLD = "PVC-IND-SPMA"
*% GLS = "FRM-GLS-SPMA"
*% TIENE_GLOSA = TRUE
*% LGL = 7
*% INCLUDE 'BUSMSC'
*% INCLUDE FIN_PUT_PVC_PVC OF 'COLBMPVC.TXT'
       FIN-PUT-PVC-PVC.
           EXIT.
       
       KEY-ALL-PVC SECTION.
       INI-KEY-ALL-PVC.
*% INCLUDE INI_KEY_ALL_PVC OF 'COLBMPVC.TXT'
           PERFORM KEY-LAP-PVC.
           PERFORM KEY-PVC-PVC.
*% INCLUDE FIN_KEY_ALL_PVC OF 'COLBMPVC.TXT'
       FIN-KEY-ALL-PVC.
           EXIT.
       
       KEY-LAP-PVC SECTION.
       INI-KEY-LAP-PVC.
*% INCLUDE INI_KEY_LAP_PVC OF 'COLBMPVC.TXT'
*% INCLUDE FIN_KEY_LAP_PVC OF 'COLBMPVC.TXT'
       FIN-KEY-LAP-PVC.
           EXIT.
       
       KEY-PVC-PVC SECTION.
       INI-KEY-PVC-PVC.
*% INCLUDE INI_KEY_PVC_PVC OF 'COLBMPVC.TXT'
           MOVE PVC-KEY-IPVC IN PVC-FLD TO PVC-KEY-IPVC IN PVC.
*% INCLUDE FIN_KEY_PVC_PVC OF 'COLBMPVC.TXT'
       FIN-KEY-PVC-PVC.
           EXIT.
       
       SCR-CLRA-PVC SECTION.
       INI-SCR-CLRA-PVC.
*% INCLUDE INI_SCR_CLRA_PVC OF 'COLBMPVC.TXT'
           PERFORM CLR-KEY-PVC.
           PERFORM CLR-FLD-PVC.
*% INCLUDE FIN_SCR_CLRA_PVC OF 'COLBMPVC.TXT'
       FIN-SCR-CLRA-PVC.
           EXIT.
       
       CLR-KEY-PVC SECTION.
       INI-CLR-KEY-PVC.
*% INCLUDE INI_CLR_KEY_PVC OF 'COLBMPVC.TXT'
           MOVE SPACES TO PVC-KEY-IPVC IN PVC-FLD.
*% INCLUDE FIN_CLR_KEY_PVC OF 'COLBMPVC.TXT'
       FIN-CLR-KEY-PVC.
           EXIT.
       
       CLR-FLD-PVC SECTION.
       INI-CLR-FLD-PVC.
*% INCLUDE INI_CLR_FLD_PVC OF 'COLBMPVC.TXT'
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FHOY"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FHOY IN PVC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-GLS-SVMI IN PVC-FLD.
           MOVE SPACES TO FRM-GLS-SVMA IN PVC-FLD.
           MOVE SPACES TO FRM-GLS-DVMI IN PVC-FLD.
           MOVE SPACES TO FRM-GLS-TCVE IN PVC-FLD.
           MOVE SPACES TO FRM-GLS-DVMA IN PVC-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN PVC-FLD.
           MOVE SPACES TO FRM-GLS-SPMI IN PVC-FLD.
           MOVE SPACES TO FRM-GLS-SPMA IN PVC-FLD.
           PERFORM CLR-LAP-PVC.
           PERFORM CLR-PVC-PVC.
*% INCLUDE FIN_CLR_FLD_PVC OF 'COLBMPVC.TXT'
       FIN-CLR-FLD-PVC.
           EXIT.
       
       CLR-LAP-PVC SECTION.
       INI-CLR-LAP-PVC.
*% INCLUDE INI_CLR_LAP_PVC OF 'COLBMPVC.TXT'
           MOVE SPACES TO LAP-COD-COME IN PVC-FLD.
*% INCLUDE FIN_CLR_LAP_PVC OF 'COLBMPVC.TXT'
       FIN-CLR-LAP-PVC.
           EXIT.
       
       CLR-PVC-PVC SECTION.
       INI-CLR-PVC-PVC.
*% INCLUDE INI_CLR_PVC_PVC OF 'COLBMPVC.TXT'
           MOVE ZEROES TO PVC-NUM-SVMI IN PVC-FLD.
           MOVE SPACES TO PVC-IND-SVMI IN PVC-FLD.
           MOVE ZEROES TO PVC-NUM-VIMI IN PVC-FLD.
           MOVE ZEROES TO PVC-NUM-SVMA IN PVC-FLD.
           MOVE ZEROES TO PVC-NUM-VIMA IN PVC-FLD.
           MOVE SPACES TO PVC-IND-SVMA IN PVC-FLD.
           MOVE ZEROES TO PVC-NUM-DVMI IN PVC-FLD.
           MOVE SPACES TO PVC-MSC-TCVE IN PVC-FLD.
           MOVE SPACES TO PVC-IND-DVMI IN PVC-FLD.
           MOVE ZEROES TO PVC-NUM-DVMA IN PVC-FLD.
           MOVE SPACES TO PVC-COD-VCOC IN PVC-FLD.
           MOVE SPACES TO PVC-IND-DVMA IN PVC-FLD.
           MOVE ZEROES TO PVC-VAL-VVMI IN PVC-FLD.
           MOVE ZEROES TO PVC-NUM-SPMI IN PVC-FLD.
           MOVE ZEROES TO PVC-VAL-VVMA IN PVC-FLD.
           MOVE SPACES TO PVC-IND-SPMI IN PVC-FLD.
           MOVE ZEROES TO PVC-NUM-SPMA IN PVC-FLD.
           MOVE ZEROES TO PVC-SGV-FMMI IN PVC-FLD.
           MOVE SPACES TO PVC-IND-SPMA IN PVC-FLD.
           MOVE ZEROES TO PVC-SGV-FMMA IN PVC-FLD.
           MOVE ZEROES TO PVC-SGV-FPMI IN PVC-FLD.
           MOVE ZEROES TO PVC-NUM-DESD IN PVC-FLD.
           MOVE ZEROES TO PVC-SGV-FPMA IN PVC-FLD.
*% INCLUDE FIN_CLR_PVC_PVC OF 'COLBMPVC.TXT'
       FIN-CLR-PVC-PVC.
           EXIT.
       
       FST-KEY-PVC SECTION.
       INI-FST-KEY-PVC.
*% INCLUDE INI_FST_KEY_PVC OF 'COLBMPVC.TXT'
           MOVE -1 TO PVC-KEY-IPVC-LEN IN PVC-FLD.
*% INCLUDE FIN_FST_KEY_PVC OF 'COLBMPVC.TXT'
       FIN-FST-KEY-PVC.
           EXIT.
       
       FST-FLD-PVC SECTION.
       INI-FST-FLD-PVC.
*% INCLUDE INI_FST_FLD_PVC OF 'COLBMPVC.TXT'
           MOVE -1 TO LAP-COD-COME-LEN IN PVC-FLD.
*% INCLUDE FIN_FST_FLD_PVC OF 'COLBMPVC.TXT'
       FIN-FST-FLD-PVC.
           EXIT.
       
       PRO-KEY-PVC SECTION.
       INI-PRO-KEY-PVC.
*% INCLUDE INI_PRO_KEY_PVC OF 'COLBMPVC.TXT'
           PERFORM PRO-IKY-PVC.
           PERFORM PRO-AKY-PVC.
           PERFORM PRO-MKY-PVC.
*% INCLUDE FIN_PRO_KEY_PVC OF 'COLBMPVC.TXT'
       FIN-PRO-KEY-PVC.
           EXIT.
       
       PRO-IKY-PVC SECTION.
       INI-PRO-IKY-PVC.
*% INCLUDE INI_PRO_IKY_PVC OF 'COLBMPVC.TXT'
           MOVE FRM-CPIM-AEY TO PVC-KEY-IPVC-ATR IN PVC-FLD.
*% INCLUDE FIN_PRO_IKY_PVC OF 'COLBMPVC.TXT'
       FIN-PRO-IKY-PVC.
           EXIT.
       
       PRO-MKY-PVC SECTION.
       INI-PRO-MKY-PVC.
*% INCLUDE INI_PRO_MKY_PVC OF 'COLBMPVC.TXT'
*% INCLUDE FIN_PRO_MKY_PVC OF 'COLBMPVC.TXT'
       FIN-PRO-MKY-PVC.
           EXIT.
       
       PRO-AKY-PVC SECTION.
       INI-PRO-AKY-PVC.
*% INCLUDE INI_PRO_AKY_PVC OF 'COLBMPVC.TXT'
*% INCLUDE FIN_PRO_AKY_PVC OF 'COLBMPVC.TXT'
       FIN-PRO-AKY-PVC.
           EXIT.
       
       UNP-KEY-PVC SECTION.
       INI-UNP-KEY-PVC.
*% INCLUDE INI_UNP_KEY_PVC OF 'COLBMPVC.TXT'
           PERFORM UNP-IKY-PVC.
           PERFORM UNP-AKY-PVC.
           PERFORM UNP-MKY-PVC.
*% INCLUDE FIN_UNP_KEY_PVC OF 'COLBMPVC.TXT'
       FIN-UNP-KEY-PVC.
           EXIT.
       
       UNP-IKY-PVC SECTION.
       INI-UNP-IKY-PVC.
*% INCLUDE INI_UNP_IKY_PVC OF 'COLBMPVC.TXT'
           MOVE FRM-CPIM-UBY TO PVC-KEY-IPVC-ATR.
*% INCLUDE FIN_UNP_IKY_PVC OF 'COLBMPVC.TXT'
       FIN-UNP-IKY-PVC.
           EXIT.
       
       UNP-MKY-PVC SECTION.
       INI-UNP-MKY-PVC.
*% INCLUDE INI_UNP_MKY_PVC OF 'COLBMPVC.TXT'
*% INCLUDE FIN_UNP_MKY_PVC OF 'COLBMPVC.TXT'
       FIN-UNP-MKY-PVC.
           EXIT.
       
       UNP-AKY-PVC SECTION.
       INI-UNP-AKY-PVC.
*% INCLUDE INI_UNP_AKY_PVC OF 'COLBMPVC.TXT'
*% INCLUDE FIN_UNP_AKY_PVC OF 'COLBMPVC.TXT'
       FIN-UNP-AKY-PVC.
           EXIT.
       
       UNP-FLD-PVC SECTION.
       INI-UNP-FLD-PVC.
*% INCLUDE INI_UNP_FLD_PVC OF 'COLBMPVC.TXT'
           PERFORM UNP-LAP-PVC.
           PERFORM UNP-PVC-PVC.
*% INCLUDE FIN_UNP_FLD_PVC OF 'COLBMPVC.TXT'
       FIN-UNP-FLD-PVC.
           EXIT.
       
       UNP-LAP-PVC SECTION.
       INI-UNP-LAP-PVC.
*% INCLUDE INI_UNP_LAP_PVC OF 'COLBMPVC.TXT'
           MOVE FRM-CPIM-UBY TO LAP-COD-COME-ATR IN PVC-FLD.
*% INCLUDE FIN_UNP_LAP_PVC OF 'COLBMPVC.TXT'
       FIN-UNP-LAP-PVC.
           EXIT.
       
       UNP-PVC-PVC SECTION.
       INI-UNP-PVC-PVC.
*% INCLUDE INI_UNP_PVC_PVC OF 'COLBMPVC.TXT'
           MOVE FRM-CPIM-UBY TO PVC-NUM-SVMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-IND-SVMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-NUM-VIMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-NUM-SVMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-NUM-VIMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-IND-SVMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-NUM-DVMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-MSC-TCVE-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-IND-DVMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-NUM-DVMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-COD-VCOC-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-IND-DVMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-VAL-VVMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-NUM-SPMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-VAL-VVMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-IND-SPMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-NUM-SPMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-SGV-FMMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-IND-SPMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-SGV-FMMA-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-SGV-FPMI-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-NUM-DESD-ATR IN PVC-FLD.
           MOVE FRM-CPIM-UBY TO PVC-SGV-FPMA-ATR IN PVC-FLD.
*% INCLUDE FIN_UNP_PVC_PVC OF 'COLBMPVC.TXT'
       FIN-UNP-PVC-PVC.
           EXIT.
       
       VAL-KEY-PVC SECTION.
       INI-VAL-KEY-PVC.
           PERFORM VAL-IKY-PVC.
           PERFORM VAL-MKY-PVC.
           PERFORM VAL-AKY-PVC.
       FIN-VAL-KEY-PVC.
           EXIT.
       
       VAL-IKY-PVC SECTION.
       INI-VAL-IKY-PVC.
*% INCLUDE INI_VAL_IKY_PVC OF 'COLBMPVC.TXT'
           PERFORM VAL-PVC-KEY-IPVC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-KEY-IPVC-LEN IN PVC-FLD
                GO TO FIN-VAL-IKY-PVC.
*% INCLUDE FIN_VAL_IKY_PVC OF 'COLBMPVC.TXT'
       FIN-VAL-IKY-PVC.
           EXIT.
       
       VAL-MKY-PVC SECTION.
       INI-VAL-MKY-PVC.
*% INCLUDE INI_VAL_MKY_PVC OF 'COLBMPVC.TXT'
*% INCLUDE FIN_VAL_MKY_PVC OF 'COLBMPVC.TXT'
       FIN-VAL-MKY-PVC.
           EXIT.
       
       VAL-AKY-PVC SECTION.
       INI-VAL-AKY-PVC.
*% INCLUDE INI_VAL_AKY_PVC OF 'COLBMPVC.TXT'
*% INCLUDE FIN_VAL_AKY_PVC OF 'COLBMPVC.TXT'
       FIN-VAL-AKY-PVC.
           EXIT.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FHOY"
*% IZR = "S"
       
       VAL-PVC-KEY-IPVC SECTION.
       INI-VAL-PVC-KEY-IPVC.
*% LBL = "PVC_KEY_IPVC OF 'COLBMPVC.TXT'"
*% INCLUDE INI_PVC_KEY_IPVC OF 'COLBMPVC.TXT'
           IF PVC-KEY-IPVC IN PVC-FLD NOT > SPACES
              MOVE SPACES TO PVC-KEY-IPVC IN PVC-FLD.
*% INCLUDE PVC_KEY_IPVC OF 'COLBMPVC.TXT'
*% INCLUDE FIN_PVC_KEY_IPVC OF 'COLBMPVC.TXT'
       FIN-VAL-PVC-KEY-IPVC.
           EXIT.
       
       VAL-CON-KEY-PVC SECTION.
       INI-VAL-CON-KEY-PVC.
*% INCLUDE INI_VAL_CON_KEY_PVC OF 'COLBMPVC.TXT'
           IF     PVC-KEY-IPVC IN PVC-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-PVC.
*% INCLUDE FIN_VAL_CON_KEY_PVC OF 'COLBMPVC.TXT'
       FIN-VAL-CON-KEY-PVC.
           EXIT.
       
       VAL-CON-IKY-PVC SECTION.
       INI-VAL-CON-IKY-PVC.
*% INCLUDE INI_VAL_CON_IKY_PVC OF 'COLBMPVC.TXT'
           IF     PVC-KEY-IPVC IN PVC-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-PVC.
*% INCLUDE FIN_VAL_CON_IKY_PVC OF 'COLBMPVC.TXT'
       FIN-VAL-CON-IKY-PVC.
           EXIT.
       
       VAL-NUM-KEY-PVC SECTION.
       INI-VAL-NUM-KEY-PVC.
           PERFORM VAL-NUM-IKY-PVC.
           PERFORM VAL-NUM-MKY-PVC.
           PERFORM VAL-NUM-AKY-PVC.
       FIN-VAL-NUM-KEY-PVC.
           EXIT.
       
       VAL-NUM-IKY-PVC SECTION.
       INI-VAL-NUM-IKY-PVC.
*% INCLUDE INI_VAL_NUM_IKY_PVC OF 'COLBMPVC.TXT'
*% INCLUDE FIN_VAL_NUM_IKY_PVC OF 'COLBMPVC.TXT'
