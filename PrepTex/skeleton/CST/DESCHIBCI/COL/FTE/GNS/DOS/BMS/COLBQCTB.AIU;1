*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CTB"
*% PGM_BQ = TRUE
       PUT-ALL-CTB SECTION.
       INI-PUT-ALL-CTB.
*% INCLUDE INI_PUT_ALL_CTB OF 'COLBMCTB.TXT'
           PERFORM PUT-CTB-CTB.
*% INCLUDE FIN_PUT_ALL_CTB OF 'COLBMCTB.TXT'
       FIN-PUT-ALL-CTB.
           EXIT.
       
       PUT-CTB-CTB SECTION.
       INI-PUT-CTB-CTB.
*% INCLUDE INI_PUT_CTB_CTB OF 'COLBMCTB.TXT'
           IF PGM-STAT-CTB-NUL
               GO TO FIN-PUT-CTB-CTB.
           IF NOT PGM-STAT-CTB-OKS
               PERFORM CLR-CTB-CTB
               GO TO FIN-PUT-CTB-CTB.
           MOVE CTB-COD-VCOC IN CTB TO CTB-COD-VCOC IN CTB-FLD.
           MOVE CTB-COD-TOOC IN CTB TO CTB-COD-TOOC IN CTB-FLD.
           MOVE CTB-COD-AOOC IN CTB TO CTB-COD-AOOC IN CTB-FLD.
           MOVE CTB-CAI-IOPC IN CTB TO CTB-CAI-IOPC IN CTB-FLD.
           MOVE CTB-IIC-IOPC IN CTB TO CTB-IIC-IOPC IN CTB-FLD.
           MOVE CTB-COD-MVTO IN CTB TO CTB-COD-MVTO IN CTB-FLD.
           MOVE CTB-NUM-MVTO IN CTB TO CTB-NUM-MVTO IN CTB-FLD.
*% RREG = "CTB"
*% NOMB = "FPRO"
      * FEC-INI --> BMCHIBCI001.SRCAIU 
           MOVE CTB-NUM-DPRO IN CTB TO CTB-NUM-DPRO IN CTB-FLD.
           MOVE CTB-NUM-MPRO IN CTB TO CTB-NUM-MPRO IN CTB-FLD.
           MOVE CTB-NUM-SPRO IN CTB TO CTB-NUM-SPRO IN CTB-FLD.
           MOVE CTB-NUM-APRO IN CTB TO CTB-NUM-APRO IN CTB-FLD.
      * FEC-FIN --> BMCHIBCI001.SRCAIU 
*% INCLUDE FIN_PUT_CTB_CTB OF 'COLBMCTB.TXT'
       FIN-PUT-CTB-CTB.
           EXIT.
       
       KEY-ALL-CTB SECTION.
       INI-KEY-ALL-CTB.
*% INCLUDE INI_KEY_ALL_CTB OF 'COLBMCTB.TXT'
           PERFORM KEY-CTB-CTB.
*% INCLUDE FIN_KEY_ALL_CTB OF 'COLBMCTB.TXT'
       FIN-KEY-ALL-CTB.
           EXIT.
       
       KEY-CTB-CTB SECTION.
       INI-KEY-CTB-CTB.
*% INCLUDE INI_KEY_CTB_CTB OF 'COLBMCTB.TXT'
           MOVE CTB-CAI-IOPC IN CTB-FLD TO CTB-CAI-IOPC IN CTB.
           MOVE CTB-IIC-IOPC IN CTB-FLD TO CTB-IIC-IOPC IN CTB.
           MOVE CTB-COD-MVTO IN CTB-FLD TO CTB-COD-MVTO IN CTB.
           MOVE CTB-NUM-MVTO IN CTB-FLD TO CTB-NUM-MVTO IN CTB.
*% RREG = "CTB"
*% NOMB = "FPRO"
      * FEC-INI --> BMCHIBCI002.SRCAIU RET-ALL Y KEY-ALL
           MOVE CTB-NUM-DPRO IN CTB-FLD TO CTB-NUM-DPRO IN CTB.
           MOVE CTB-NUM-MPRO IN CTB-FLD TO CTB-NUM-MPRO IN CTB.
           MOVE CTB-NUM-SPRO IN CTB-FLD TO CTB-NUM-SPRO IN CTB.
           MOVE CTB-NUM-APRO IN CTB-FLD TO CTB-NUM-APRO IN CTB.
      * FEC-FIN --> BMCHIBCI002.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_KEY_CTB_CTB OF 'COLBMCTB.TXT'
       FIN-KEY-CTB-CTB.
           EXIT.
       
       SCR-CLRA-CTB SECTION.
       INI-SCR-CLRA-CTB.
*% INCLUDE INI_SCR_CLRA_CTB OF 'COLBMCTB.TXT'
           PERFORM CLR-KEY-CTB.
           PERFORM CLR-FLD-CTB.
*% INCLUDE FIN_SCR_CLRA_CTB OF 'COLBMCTB.TXT'
       FIN-SCR-CLRA-CTB.
           EXIT.
       
       CLR-KEY-CTB SECTION.
       INI-CLR-KEY-CTB.
*% INCLUDE INI_CLR_KEY_CTB OF 'COLBMCTB.TXT'
           MOVE SPACES TO CTB-CAI-IOPC IN CTB-FLD.
           MOVE ZEROES TO CTB-IIC-IOPC IN CTB-FLD.
           MOVE SPACES TO CTB-COD-MVTO IN CTB-FLD.
           MOVE ZEROES TO CTB-NUM-MVTO IN CTB-FLD.
*% RREG = "CTB"
*% NOMB = "FPRO"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CTB-FEC-FPRO IN CTB-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_KEY_CTB OF 'COLBMCTB.TXT'
       FIN-CLR-KEY-CTB.
           EXIT.
       
       CLR-FLD-CTB SECTION.
       INI-CLR-FLD-CTB.
*% INCLUDE INI_CLR_FLD_CTB OF 'COLBMCTB.TXT'
           MOVE SPACES TO FRM-GLS-VCOC IN CTB-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN CTB-FLD.
           MOVE SPACES TO FRM-GLS-AOOC IN CTB-FLD.
           MOVE SPACES TO FRM-CIC-CCSB IN CTB-FLD(1).
           MOVE SPACES TO FRM-CIC-CCSB IN CTB-FLD(2).
           MOVE SPACES TO FRM-CIC-CCSB IN CTB-FLD(3).
           MOVE SPACES TO FRM-CIC-CCSB IN CTB-FLD(4).
           MOVE SPACES TO FRM-CIC-CCSB IN CTB-FLD(5).
           MOVE SPACES TO FRM-CIC-CCSB IN CTB-FLD(6).
           MOVE SPACES TO FRM-CIC-CCSB IN CTB-FLD(7).
           MOVE SPACES TO FRM-CIC-CCSB IN CTB-FLD(8).
           MOVE SPACES TO FRM-CIC-CCSB IN CTB-FLD(9).
           MOVE SPACES TO FRM-CIC-CCSB IN CTB-FLD(10).
           MOVE SPACES TO FRM-CIC-CCSB IN CTB-FLD(11).
           MOVE SPACES TO FRM-GLS-CCSB IN CTB-FLD(1).
           MOVE SPACES TO FRM-GLS-CCSB IN CTB-FLD(2).
           MOVE SPACES TO FRM-GLS-CCSB IN CTB-FLD(3).
           MOVE SPACES TO FRM-GLS-CCSB IN CTB-FLD(4).
           MOVE SPACES TO FRM-GLS-CCSB IN CTB-FLD(5).
           MOVE SPACES TO FRM-GLS-CCSB IN CTB-FLD(6).
           MOVE SPACES TO FRM-GLS-CCSB IN CTB-FLD(7).
           MOVE SPACES TO FRM-GLS-CCSB IN CTB-FLD(8).
           MOVE SPACES TO FRM-GLS-CCSB IN CTB-FLD(9).
           MOVE SPACES TO FRM-GLS-CCSB IN CTB-FLD(10).
           MOVE SPACES TO FRM-GLS-CCSB IN CTB-FLD(11).
           MOVE ZEROES TO FRM-SGV-CTBD IN CTB-FLD(1).
           MOVE ZEROES TO FRM-SGV-CTBD IN CTB-FLD(2).
           MOVE ZEROES TO FRM-SGV-CTBD IN CTB-FLD(3).
           MOVE ZEROES TO FRM-SGV-CTBD IN CTB-FLD(4).
           MOVE ZEROES TO FRM-SGV-CTBD IN CTB-FLD(5).
           MOVE ZEROES TO FRM-SGV-CTBD IN CTB-FLD(6).
           MOVE ZEROES TO FRM-SGV-CTBD IN CTB-FLD(7).
           MOVE ZEROES TO FRM-SGV-CTBD IN CTB-FLD(8).
           MOVE ZEROES TO FRM-SGV-CTBD IN CTB-FLD(9).
           MOVE ZEROES TO FRM-SGV-CTBD IN CTB-FLD(10).
           MOVE ZEROES TO FRM-SGV-CTBD IN CTB-FLD(11).
           MOVE ZEROES TO FRM-SGV-CTBH IN CTB-FLD(1).
           MOVE ZEROES TO FRM-SGV-CTBH IN CTB-FLD(2).
           MOVE ZEROES TO FRM-SGV-CTBH IN CTB-FLD(3).
           MOVE ZEROES TO FRM-SGV-CTBH IN CTB-FLD(4).
           MOVE ZEROES TO FRM-SGV-CTBH IN CTB-FLD(5).
           MOVE ZEROES TO FRM-SGV-CTBH IN CTB-FLD(6).
           MOVE ZEROES TO FRM-SGV-CTBH IN CTB-FLD(7).
           MOVE ZEROES TO FRM-SGV-CTBH IN CTB-FLD(8).
           MOVE ZEROES TO FRM-SGV-CTBH IN CTB-FLD(9).
           MOVE ZEROES TO FRM-SGV-CTBH IN CTB-FLD(10).
           MOVE ZEROES TO FRM-SGV-CTBH IN CTB-FLD(11).
           MOVE ZEROES TO FRM-TOT-CTBD IN CTB-FLD.
           MOVE ZEROES TO FRM-TOT-CTBH IN CTB-FLD.
           PERFORM CLR-CTB-CTB.
*% INCLUDE FIN_CLR_FLD_CTB OF 'COLBMCTB.TXT'
       FIN-CLR-FLD-CTB.
           EXIT.
       
       CLR-CTB-CTB SECTION.
       INI-CLR-CTB-CTB.
*% INCLUDE INI_CLR_CTB_CTB OF 'COLBMCTB.TXT'
           MOVE SPACES TO CTB-COD-VCOC IN CTB-FLD.
           MOVE SPACES TO CTB-COD-TOOC IN CTB-FLD.
           MOVE SPACES TO CTB-COD-AOOC IN CTB-FLD.
*% INCLUDE FIN_CLR_CTB_CTB OF 'COLBMCTB.TXT'
       FIN-CLR-CTB-CTB.
           EXIT.
       
       FST-KEY-CTB SECTION.
       INI-FST-KEY-CTB.
*% INCLUDE INI_FST_KEY_CTB OF 'COLBMCTB.TXT'
           MOVE -1 TO CTB-CAI-IOPC-LEN IN CTB-FLD.
*% INCLUDE FIN_FST_KEY_CTB OF 'COLBMCTB.TXT'
       FIN-FST-KEY-CTB.
           EXIT.
       
       FST-FLD-CTB SECTION.
       INI-FST-FLD-CTB.
*% INCLUDE INI_FST_FLD_CTB OF 'COLBMCTB.TXT'
*% INCLUDE FIN_FST_FLD_CTB OF 'COLBMCTB.TXT'
       FIN-FST-FLD-CTB.
           EXIT.
       
       PRO-KEY-CTB SECTION.
       INI-PRO-KEY-CTB.
*% INCLUDE INI_PRO_KEY_CTB OF 'COLBMCTB.TXT'
           PERFORM PRO-IKY-CTB.
           PERFORM PRO-AKY-CTB.
           PERFORM PRO-MKY-CTB.
*% INCLUDE FIN_PRO_KEY_CTB OF 'COLBMCTB.TXT'
       FIN-PRO-KEY-CTB.
           EXIT.
       
       PRO-IKY-CTB SECTION.
       INI-PRO-IKY-CTB.
*% INCLUDE INI_PRO_IKY_CTB OF 'COLBMCTB.TXT'
           MOVE FRM-CPIM-AEY TO CTB-CAI-IOPC-ATR IN CTB-FLD.
           MOVE FRM-CPIM-AEY TO CTB-IIC-IOPC-ATR IN CTB-FLD.
           MOVE FRM-CPIM-AEY TO CTB-COD-MVTO-ATR IN CTB-FLD.
           MOVE FRM-CPIM-AEY TO CTB-NUM-MVTO-ATR IN CTB-FLD.
*% RREG = "CTB"
*% NOMB = "FPRO"
           MOVE FRM-CPIM-AEY TO CTB-FEC-FPRO-ATR IN CTB-FLD.
*% INCLUDE FIN_PRO_IKY_CTB OF 'COLBMCTB.TXT'
       FIN-PRO-IKY-CTB.
           EXIT.
       
       PRO-MKY-CTB SECTION.
       INI-PRO-MKY-CTB.
*% INCLUDE INI_PRO_MKY_CTB OF 'COLBMCTB.TXT'
*% INCLUDE FIN_PRO_MKY_CTB OF 'COLBMCTB.TXT'
       FIN-PRO-MKY-CTB.
           EXIT.
       
       PRO-AKY-CTB SECTION.
       INI-PRO-AKY-CTB.
*% INCLUDE INI_PRO_AKY_CTB OF 'COLBMCTB.TXT'
*% INCLUDE FIN_PRO_AKY_CTB OF 'COLBMCTB.TXT'
       FIN-PRO-AKY-CTB.
           EXIT.
       
       UNP-KEY-CTB SECTION.
       INI-UNP-KEY-CTB.
*% INCLUDE INI_UNP_KEY_CTB OF 'COLBMCTB.TXT'
           PERFORM UNP-IKY-CTB.
           PERFORM UNP-AKY-CTB.
           PERFORM UNP-MKY-CTB.
*% INCLUDE FIN_UNP_KEY_CTB OF 'COLBMCTB.TXT'
       FIN-UNP-KEY-CTB.
           EXIT.
       
       UNP-IKY-CTB SECTION.
       INI-UNP-IKY-CTB.
*% INCLUDE INI_UNP_IKY_CTB OF 'COLBMCTB.TXT'
           MOVE FRM-CPIM-UBY TO CTB-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO CTB-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO CTB-COD-MVTO-ATR.
           MOVE FRM-CPIM-UBY TO CTB-NUM-MVTO-ATR.
*% RREG = "CTB"
*% NOMB = "FPRO"
           MOVE FRM-CPIM-UBY TO CTB-FEC-FPRO-ATR IN CTB-FLD.
*% INCLUDE FIN_UNP_IKY_CTB OF 'COLBMCTB.TXT'
       FIN-UNP-IKY-CTB.
           EXIT.
       
       UNP-MKY-CTB SECTION.
       INI-UNP-MKY-CTB.
*% INCLUDE INI_UNP_MKY_CTB OF 'COLBMCTB.TXT'
*% INCLUDE FIN_UNP_MKY_CTB OF 'COLBMCTB.TXT'
       FIN-UNP-MKY-CTB.
           EXIT.
       
       UNP-AKY-CTB SECTION.
       INI-UNP-AKY-CTB.
*% INCLUDE INI_UNP_AKY_CTB OF 'COLBMCTB.TXT'
*% INCLUDE FIN_UNP_AKY_CTB OF 'COLBMCTB.TXT'
       FIN-UNP-AKY-CTB.
           EXIT.
       
       UNP-FLD-CTB SECTION.
       INI-UNP-FLD-CTB.
*% INCLUDE INI_UNP_FLD_CTB OF 'COLBMCTB.TXT'
           PERFORM UNP-CTB-CTB.
*% INCLUDE FIN_UNP_FLD_CTB OF 'COLBMCTB.TXT'
       FIN-UNP-FLD-CTB.
           EXIT.
       
       UNP-CTB-CTB SECTION.
       INI-UNP-CTB-CTB.
*% INCLUDE INI_UNP_CTB_CTB OF 'COLBMCTB.TXT'
*% INCLUDE FIN_UNP_CTB_CTB OF 'COLBMCTB.TXT'
       FIN-UNP-CTB-CTB.
           EXIT.
       
       VAL-KEY-CTB SECTION.
       INI-VAL-KEY-CTB.
           PERFORM VAL-IKY-CTB.
           PERFORM VAL-MKY-CTB.
           PERFORM VAL-AKY-CTB.
       FIN-VAL-KEY-CTB.
           EXIT.
       
       VAL-IKY-CTB SECTION.
       INI-VAL-IKY-CTB.
*% INCLUDE INI_VAL_IKY_CTB OF 'COLBMCTB.TXT'
           PERFORM VAL-CTB-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CTB-CAI-IOPC-LEN IN CTB-FLD
                GO TO FIN-VAL-IKY-CTB.
           PERFORM VAL-CTB-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CTB-IIC-IOPC-LEN IN CTB-FLD
                GO TO FIN-VAL-IKY-CTB.
           PERFORM VAL-CTB-COD-MVTO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CTB-COD-MVTO-LEN IN CTB-FLD
                GO TO FIN-VAL-IKY-CTB.
           PERFORM VAL-CTB-NUM-MVTO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CTB-NUM-MVTO-LEN IN CTB-FLD
                GO TO FIN-VAL-IKY-CTB.
*% RREG = "CTB"
*% NOMB = "FPRO"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-CTB-FEC-FPRO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                CTB-FEC-FPRO-LEN IN CTB-FLD
                GO TO FIN-VAL-IKY-CTB.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% INCLUDE FIN_VAL_IKY_CTB OF 'COLBMCTB.TXT'
       FIN-VAL-IKY-CTB.
           EXIT.
       
       VAL-MKY-CTB SECTION.
       INI-VAL-MKY-CTB.
*% INCLUDE INI_VAL_MKY_CTB OF 'COLBMCTB.TXT'
*% INCLUDE FIN_VAL_MKY_CTB OF 'COLBMCTB.TXT'
       FIN-VAL-MKY-CTB.
           EXIT.
       
       VAL-AKY-CTB SECTION.
       INI-VAL-AKY-CTB.
*% INCLUDE INI_VAL_AKY_CTB OF 'COLBMCTB.TXT'
*% INCLUDE FIN_VAL_AKY_CTB OF 'COLBMCTB.TXT'
       FIN-VAL-AKY-CTB.
           EXIT.
       
       VAL-CTB-CAI-IOPC SECTION.
       INI-VAL-CTB-CAI-IOPC.
*% LBL = "CTB_CAI_IOPC OF 'COLBMCTB.TXT'"
*% INCLUDE INI_CTB_CAI_IOPC OF 'COLBMCTB.TXT'
           IF CTB-CAI-IOPC IN CTB-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-CTB-CAI-IOPC.
*% INCLUDE CTB_CAI_IOPC OF 'COLBMCTB.TXT'
*% INCLUDE FIN_CTB_CAI_IOPC OF 'COLBMCTB.TXT'
       FIN-VAL-CTB-CAI-IOPC.
           EXIT.
       
       VAL-CTB-IIC-IOPC SECTION.
       INI-VAL-CTB-IIC-IOPC.
*% LBL = "CTB_IIC_IOPC OF 'COLBMCTB.TXT'"
*% INCLUDE INI_CTB_IIC_IOPC OF 'COLBMCTB.TXT'
           IF CTB-IIC-IOPC IN CTB-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-CTB-IIC-IOPC.
*% INCLUDE CTB_IIC_IOPC OF 'COLBMCTB.TXT'
*% INCLUDE FIN_CTB_IIC_IOPC OF 'COLBMCTB.TXT'
       FIN-VAL-CTB-IIC-IOPC.
           EXIT.
       
       VAL-CTB-COD-MVTO SECTION.
       INI-VAL-CTB-COD-MVTO.
*% LBL = "CTB_COD_MVTO OF 'COLBMCTB.TXT'"
*% INCLUDE INI_CTB_COD_MVTO OF 'COLBMCTB.TXT'
           IF CTB-COD-MVTO IN CTB-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-CTB-COD-MVTO.
*% INCLUDE CTB_COD_MVTO OF 'COLBMCTB.TXT'
*% INCLUDE FIN_CTB_COD_MVTO OF 'COLBMCTB.TXT'
       FIN-VAL-CTB-COD-MVTO.
           EXIT.
       
       VAL-CTB-NUM-MVTO SECTION.
       INI-VAL-CTB-NUM-MVTO.
*% LBL = "CTB_NUM_MVTO OF 'COLBMCTB.TXT'"
*% INCLUDE INI_CTB_NUM_MVTO OF 'COLBMCTB.TXT'
*% INCLUDE CTB_NUM_MVTO OF 'COLBMCTB.TXT'
*% INCLUDE FIN_CTB_NUM_MVTO OF 'COLBMCTB.TXT'
       FIN-VAL-CTB-NUM-MVTO.
           EXIT.
*% RREG = "CTB"
*% NOMB = "FPRO"
*% OBL = "N"
*% LBL = "CTB_FEC_FPRO OF 'COLBMCTB.TXT'"
*% IZR = "N"
*% ZRO = "S"
*% BUF = "CTB-FLD"
*% FLD = "CTB-FEC-FPRO"
*% SN  = "N"
*% HOY = "LE"
*% HBL = "S"
*% TIPO = "FEC"
*% FRM_IFLD = ""
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-CTB-FEC-FPRO SECTION.
       INI-VAL-CTB-FEC-FPRO.
           IF CTB-FEC-FPRO IN CTB-FLD  NOT > ZEROES
              MOVE ZEROES TO CTB-FEC-FPRO IN CTB-FLD.
*% INCLUDE 'SETFEC'
*% INCLUDE INI_CTB_FEC_FPRO OF 'COLBMCTB.TXT'
*% INCLUDE 'PROFEC'
*% INCLUDE CTB_FEC_FPRO OF 'COLBMCTB.TXT'
*% INCLUDE FIN_CTB_FEC_FPRO OF 'COLBMCTB.TXT'
       FIN-VAL-CTB-FEC-FPRO.
           EXIT.
      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-CON-KEY-CTB SECTION.
       INI-VAL-CON-KEY-CTB.
*% INCLUDE INI_VAL_CON_KEY_CTB OF 'COLBMCTB.TXT'
           IF     CTB-CAI-IOPC IN CTB-FLD NOT > SPACES 
              AND CTB-IIC-IOPC IN CTB-FLD NOT > ZEROES 
              AND CTB-COD-MVTO IN CTB-FLD NOT > SPACES 
              AND CTB-NUM-MVTO IN CTB-FLD NOT > ZEROES 
*% RREG = "CTB"
*% NOMB = "FPRO"
              AND CTB-FEC-FPRO IN CTB-FLD NOT > ZEROES
                PERFORM ERR-KEY
                PERFORM FST-KEY-CTB.
*% INCLUDE FIN_VAL_CON_KEY_CTB OF 'COLBMCTB.TXT'
       FIN-VAL-CON-KEY-CTB.
           EXIT.
       
       VAL-CON-IKY-CTB SECTION.
       INI-VAL-CON-IKY-CTB.
*% INCLUDE INI_VAL_CON_IKY_CTB OF 'COLBMCTB.TXT'
           IF     CTB-CAI-IOPC IN CTB-FLD NOT > SPACES 
              AND CTB-IIC-IOPC IN CTB-FLD NOT > ZEROES 
              AND CTB-COD-MVTO IN CTB-FLD NOT > SPACES 
              AND CTB-NUM-MVTO IN CTB-FLD NOT > ZEROES 
*% RREG = "CTB"
*% NOMB = "FPRO"
              AND CTB-FEC-FPRO IN CTB-FLD NOT > ZEROES
                PERFORM ERR-KEY
                PERFORM FST-KEY-CTB.
*% INCLUDE FIN_VAL_CON_IKY_CTB OF 'COLBMCTB.TXT'
       FIN-VAL-CON-IKY-CTB.
           EXIT.
       
       VAL-NUM-KEY-CTB SECTION.
       INI-VAL-NUM-KEY-CTB.
           PERFORM VAL-NUM-IKY-CTB.
           PERFORM VAL-NUM-MKY-CTB.
           PERFORM VAL-NUM-AKY-CTB.
       FIN-VAL-NUM-KEY-CTB.
           EXIT.
       
       VAL-NUM-IKY-CTB SECTION.
       INI-VAL-NUM-IKY-CTB.
*% INCLUDE INI_VAL_NUM_IKY_CTB OF 'COLBMCTB.TXT'
       VAL-NUM-CTB-IIC-IOPC.
*% BUF = "CTB-FLD"
*% FLD = "CTB-IIC-IOPC"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-CTB.
       VAL-NUM-CTB-NUM-MVTO.
*% BUF = "CTB-FLD"
*% FLD = "CTB-NUM-MVTO"
*% ENT = "3"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-CTB.
*% INCLUDE FIN_VAL_NUM_IKY_CTB OF 'COLBMCTB.TXT'
       FIN-VAL-NUM-IKY-CTB.
           EXIT.
       
       VAL-NUM-MKY-CTB SECTION.
       INI-VAL-NUM-MKY-CTB.
*% INCLUDE INI_VAL_NUM_MKY_CTB OF 'COLBMCTB.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_CTB OF 'COLBMCTB.TXT'
       FIN-VAL-NUM-MKY-CTB.
           EXIT.
       
       VAL-NUM-AKY-CTB SECTION.
       INI-VAL-NUM-AKY-CTB.
