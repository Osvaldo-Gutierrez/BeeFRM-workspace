       EDT-KEY-CEN SECTION.
       INI-EDT-KEY-CEN.
           PERFORM EDT-IKY-CEN.
           PERFORM EDT-MKY-CEN.
           PERFORM EDT-AKY-CEN.
*% INCLUDE EDT_KEY_CEN OF 'COLBMCEN.TXT'
       FIN-EDT-KEY-CEN.
           EXIT.
       
       EDT-IKY-CEN SECTION.
       INI-EDT-IKY-CEN.
*% INCLUDE EDT_IKY_CEN OF 'COLBMCEN.TXT'
           IF CEN-IIC-IENT-LEN IN CEN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CEN-IIC-IENT-LEN IN CEN-FLD
                GO TO FIN-EDT-IKY-CEN.
           MOVE CEN-IIC-IENT IN CEN-FLD TO CEN-IIC-IENT-EDT IN CEN-EDT.
           MOVE CEN-IIC-IENT-EDT IN CEN-EDT TO 
                CEN-IIC-IENT-ALF IN CEN-FLD.
*% RREG = "CEN"
*% RREG_FRM = "CEN"
*% NOMB = "FENT"
           IF CEN-NUM-IENT-LEN IN CEN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CEN-NUM-IENT-LEN IN CEN-FLD
                GO TO FIN-EDT-IKY-CEN.
           MOVE CEN-NUM-IENT IN CEN-FLD TO CEN-NUM-IENT-EDT IN CEN-EDT.
           MOVE CEN-NUM-IENT-EDT IN CEN-EDT TO 
                CEN-NUM-IENT-ALF IN CEN-FLD.
           IF CEN-IIC-ISEN-LEN IN CEN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CEN-IIC-ISEN-LEN IN CEN-FLD
                GO TO FIN-EDT-IKY-CEN.
           MOVE CEN-IIC-ISEN IN CEN-FLD TO CEN-IIC-ISEN-EDT IN CEN-EDT.
           MOVE CEN-IIC-ISEN-EDT IN CEN-EDT TO 
                CEN-IIC-ISEN-ALF IN CEN-FLD.
           IF CEN-NUM-ISEN-LEN IN CEN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CEN-NUM-ISEN-LEN IN CEN-FLD
                GO TO FIN-EDT-IKY-CEN.
           MOVE CEN-NUM-ISEN IN CEN-FLD TO CEN-NUM-ISEN-EDT IN CEN-EDT.
           MOVE CEN-NUM-ISEN-EDT IN CEN-EDT TO 
                CEN-NUM-ISEN-ALF IN CEN-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF FRM-NUM-ICLI-LEN IN CEN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-ICLI-LEN IN CEN-FLD
                GO TO FIN-EDT-IKY-CEN.
           MOVE FRM-NUM-ICLI IN CEN-FLD TO
                FRM-NUM-ICLI-EDT IN CEN-EDT.
           MOVE FRM-NUM-ICLI-EDT IN CEN-EDT TO
                FRM-NUM-ICLI-ALF IN CEN-FLD.
      * IDC-FIN --> BMCHIBCI011.SRCAIU EDICION NUMERICA
*% INCLUDE FIN_EDT_IKY_CEN OF 'COLBMCEN.TXT'
       FIN-EDT-IKY-CEN.
           EXIT.
       
       EDT-MKY-CEN SECTION.
       INI-EDT-MKY-CEN.
*% INCLUDE EDT_MKY_CEN OF 'COLBMCEN.TXT'
*% INCLUDE FIN_EDT_MKY_CEN OF 'COLBMCEN.TXT'
       FIN-EDT-MKY-CEN.
           EXIT.
       
       EDT-AKY-CEN SECTION.
       INI-EDT-AKY-CEN.
*% INCLUDE EDT_AKY_CEN OF 'COLBMCEN.TXT'
*% INCLUDE FIN_EDT_AKY_CEN OF 'COLBMCEN.TXT'
       FIN-EDT-AKY-CEN.
           EXIT.
       
       EDT-FLD-CEN SECTION.
       INI-EDT-FLD-CEN.
*% INCLUDE EDT_FLD_CEN OF 'COLBMCEN.TXT'
*% RREG = "CEN"
*% RREG_FRM = "CEN"
*% NOMB = "FFE1"
*% RREG = "CEN"
*% RREG_FRM = "CEN"
*% NOMB = "FFE2"
*% RREG = "CEN"
*% RREG_FRM = "CEN"
*% NOMB = "FFE3"
*% RREG = "CEN"
*% RREG_FRM = "CEN"
*% NOMB = "FFE4"
*% RREG = "CEN"
*% RREG_FRM = "CEN"
*% NOMB = "FFE5"
*% RREG = "CEN"
*% RREG_FRM = "CEN"
*% NOMB = "FFE6"
           IF CEN-SGV-VAL1-LEN IN CEN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CEN-SGV-VAL1-LEN IN CEN-FLD
                GO TO FIN-EDT-FLD-CEN.
           MOVE CEN-SGV-VAL1 IN CEN-FLD TO CEN-SGV-VAL1-EDT IN CEN-EDT.
           MOVE CEN-SGV-VAL1-EDT IN CEN-EDT TO 
                CEN-SGV-VAL1-ALF IN CEN-FLD.
           IF CEN-NUM-CON1-LEN IN CEN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CEN-NUM-CON1-LEN IN CEN-FLD
                GO TO FIN-EDT-FLD-CEN.
           MOVE CEN-NUM-CON1 IN CEN-FLD TO CEN-NUM-CON1-EDT IN CEN-EDT.
           MOVE CEN-NUM-CON1-EDT IN CEN-EDT TO 
                CEN-NUM-CON1-ALF IN CEN-FLD.
           IF CEN-SGV-POR1-LEN IN CEN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CEN-SGV-POR1-LEN IN CEN-FLD
                GO TO FIN-EDT-FLD-CEN.
           MOVE CEN-SGV-POR1 IN CEN-FLD TO CEN-SGV-POR1-EDT IN CEN-EDT.
           MOVE CEN-SGV-POR1-EDT IN CEN-EDT TO 
                CEN-SGV-POR1-ALF IN CEN-FLD.
           IF CEN-SGV-VAL2-LEN IN CEN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CEN-SGV-VAL2-LEN IN CEN-FLD
                GO TO FIN-EDT-FLD-CEN.
           MOVE CEN-SGV-VAL2 IN CEN-FLD TO CEN-SGV-VAL2-EDT IN CEN-EDT.
           MOVE CEN-SGV-VAL2-EDT IN CEN-EDT TO 
                CEN-SGV-VAL2-ALF IN CEN-FLD.
           IF CEN-NUM-CON2-LEN IN CEN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CEN-NUM-CON2-LEN IN CEN-FLD
                GO TO FIN-EDT-FLD-CEN.
           MOVE CEN-NUM-CON2 IN CEN-FLD TO CEN-NUM-CON2-EDT IN CEN-EDT.
           MOVE CEN-NUM-CON2-EDT IN CEN-EDT TO 
                CEN-NUM-CON2-ALF IN CEN-FLD.
           IF CEN-SGV-POR2-LEN IN CEN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CEN-SGV-POR2-LEN IN CEN-FLD
                GO TO FIN-EDT-FLD-CEN.
           MOVE CEN-SGV-POR2 IN CEN-FLD TO CEN-SGV-POR2-EDT IN CEN-EDT.
           MOVE CEN-SGV-POR2-EDT IN CEN-EDT TO 
                CEN-SGV-POR2-ALF IN CEN-FLD.
*% INCLUDE FIN_EDT_FLD_CEN OF 'COLBMCEN.TXT'
       FIN-EDT-FLD-CEN.
           EXIT.
