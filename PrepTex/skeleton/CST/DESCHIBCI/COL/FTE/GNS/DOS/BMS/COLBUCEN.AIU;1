       INI-VAL-FLD-CEN.
*% INCLUDE INI_VAL_FLD_CEN OF 'COLBMCEN.TXT'
           PERFORM VAL-FRM-GLS-ENTI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-ENTI-LEN IN CEN-FLD
                GO TO FIN-VAL-FLD-CEN.
           PERFORM VAL-FRM-GLS-RENT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-RENT-LEN IN CEN-FLD
                GO TO FIN-VAL-FLD-CEN.
           PERFORM VAL-FRM-GLS-CCSE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-CCSE-LEN IN CEN-FLD
                GO TO FIN-VAL-FLD-CEN.
           PERFORM VAL-CEN-MSC-STAT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CEN-MSC-STAT-LEN IN CEN-FLD
                GO TO FIN-VAL-FLD-CEN.
           PERFORM VAL-CEN-COD-TOPR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CEN-COD-TOPR-LEN IN CEN-FLD
                GO TO FIN-VAL-FLD-CEN.
           PERFORM VAL-CEN-COD-AOPR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CEN-COD-AOPR-LEN IN CEN-FLD
                GO TO FIN-VAL-FLD-CEN.
           PERFORM VAL-CEN-COD-VCAM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CEN-COD-VCAM-LEN IN CEN-FLD
                GO TO FIN-VAL-FLD-CEN.
           PERFORM VAL-FRM-GLS-VCAM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-VCAM-LEN IN CEN-FLD
                GO TO FIN-VAL-FLD-CEN.
           PERFORM VAL-CEN-COD-COD1.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CEN-COD-COD1-LEN IN CEN-FLD
                GO TO FIN-VAL-FLD-CEN.
           PERFORM VAL-CEN-IND-IND1.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CEN-IND-IND1-LEN IN CEN-FLD
                GO TO FIN-VAL-FLD-CEN.
           PERFORM VAL-CEN-COD-COD2.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CEN-COD-COD2-LEN IN CEN-FLD
                GO TO FIN-VAL-FLD-CEN.
           PERFORM VAL-CEN-IND-IND2.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CEN-IND-IND2-LEN IN CEN-FLD
                GO TO FIN-VAL-FLD-CEN.
           PERFORM VAL-CEN-COD-COD3.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CEN-COD-COD3-LEN IN CEN-FLD
                GO TO FIN-VAL-FLD-CEN.
