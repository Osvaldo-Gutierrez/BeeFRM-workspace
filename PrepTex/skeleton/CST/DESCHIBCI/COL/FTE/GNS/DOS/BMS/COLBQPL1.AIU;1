      *
      * COLBQPL1
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:14 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "PL1"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMPL1.TXT'
       SET-TAG-COLBQPL1 SECTION.
       INI-SET-TAG-COLBQPL1.
           MOVE
           '<IDNMOD NOM=$COLBQPL1$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:14 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQPL1.
           EXIT.
           
       PUT-ALL-PL1 SECTION.
       INI-PUT-ALL-PL1.
*% INCLUDE INI_PUT_ALL_PL1 OF 'COLBMPL1.TXT'
           PERFORM PUT-OPC-PL1.
           PERFORM PUT-DBC-PL1.
           PERFORM PUT-ITR-PL1.
*% INCLUDE FIN_PUT_ALL_PL1 OF 'COLBMPL1.TXT'
       FIN-PUT-ALL-PL1.
           EXIT.
       
       PUT-OPC-PL1 SECTION.
       INI-PUT-OPC-PL1.
*% INCLUDE INI_PUT_OPC_PL1 OF 'COLBMPL1.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-PL1.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-PL1
               GO TO FIN-PUT-OPC-PL1.
           MOVE OPC-CAI-IOPC IN OPC TO OPC-CAI-IOPC IN PL1-FLD.
           MOVE OPC-IIC-IOPC IN OPC TO OPC-IIC-IOPC IN PL1-FLD.
           MOVE OPC-COD-VCOC IN OPC TO OPC-COD-VCOC IN PL1-FLD.
           MOVE OPC-COD-TOOC IN OPC TO OPC-COD-TOOC IN PL1-FLD.
           MOVE OPC-COD-AOOC IN OPC TO OPC-COD-AOOC IN PL1-FLD.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE OPC-NUM-DCOL IN OPC TO OPC-NUM-DCOL IN PL1-FLD.
           MOVE OPC-NUM-MCOL IN OPC TO OPC-NUM-MCOL IN PL1-FLD.
           MOVE OPC-NUM-SCOL IN OPC TO OPC-NUM-SCOL IN PL1-FLD.
           MOVE OPC-NUM-ACOL IN OPC TO OPC-NUM-ACOL IN PL1-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE OPC-VAL-SEGD IN OPC TO OPC-VAL-SEGD IN PL1-FLD.
           MOVE OPC-SGV-LIQU IN OPC TO OPC-SGV-LIQU IN PL1-FLD.
*% INCLUDE FIN_PUT_OPC_PL1 OF 'COLBMPL1.TXT'
       FIN-PUT-OPC-PL1.
           EXIT.
       
       PUT-DBC-PL1 SECTION.
       INI-PUT-DBC-PL1.
*% INCLUDE INI_PUT_DBC_PL1 OF 'COLBMPL1.TXT'
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-PL1.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-PL1
               GO TO FIN-PUT-DBC-PL1.
           MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN PL1-FLD.
*% INCLUDE FIN_PUT_DBC_PL1 OF 'COLBMPL1.TXT'
       FIN-PUT-DBC-PL1.
           EXIT.
       
       PUT-ITR-PL1 SECTION.
       INI-PUT-ITR-PL1.
*% INCLUDE INI_PUT_ITR_PL1 OF 'COLBMPL1.TXT'
           IF PGM-STAT-ITR-NUL
               GO TO FIN-PUT-ITR-PL1.
           IF NOT PGM-STAT-ITR-OKS
               PERFORM CLR-ITR-PL1
               GO TO FIN-PUT-ITR-PL1.
           MOVE ITR-SGV-TINT IN ITR TO ITR-SGV-TINT IN PL1-FLD.
*% INCLUDE FIN_PUT_ITR_PL1 OF 'COLBMPL1.TXT'
       FIN-PUT-ITR-PL1.
           EXIT.
       
       KEY-ALL-PL1 SECTION.
       INI-KEY-ALL-PL1.
*% INCLUDE INI_KEY_ALL_PL1 OF 'COLBMPL1.TXT'
           PERFORM KEY-OPC-PL1.
           PERFORM KEY-DBC-PL1.
           PERFORM KEY-ITR-PL1.
*% INCLUDE FIN_KEY_ALL_PL1 OF 'COLBMPL1.TXT'
       FIN-KEY-ALL-PL1.
           EXIT.
       
       KEY-OPC-PL1 SECTION.
       INI-KEY-OPC-PL1.
*% INCLUDE INI_KEY_OPC_PL1 OF 'COLBMPL1.TXT'
           MOVE OPC-CAI-IOPC IN PL1-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE OPC-IIC-IOPC IN PL1-FLD TO OPC-IIC-IOPC IN OPC.
*% INCLUDE FIN_KEY_OPC_PL1 OF 'COLBMPL1.TXT'
       FIN-KEY-OPC-PL1.
           EXIT.
       
       KEY-DBC-PL1 SECTION.
       INI-KEY-DBC-PL1.
*% INCLUDE INI_KEY_DBC_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_KEY_DBC_PL1 OF 'COLBMPL1.TXT'
       FIN-KEY-DBC-PL1.
           EXIT.
       
       KEY-ITR-PL1 SECTION.
       INI-KEY-ITR-PL1.
*% INCLUDE INI_KEY_ITR_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_KEY_ITR_PL1 OF 'COLBMPL1.TXT'
       FIN-KEY-ITR-PL1.
           EXIT.
       
       SCR-CLRA-PL1 SECTION.
       INI-SCR-CLRA-PL1.
*% INCLUDE INI_SCR_CLRA_PL1 OF 'COLBMPL1.TXT'
           PERFORM CLR-KEY-PL1.
           PERFORM CLR-FLD-PL1.
*% INCLUDE FIN_SCR_CLRA_PL1 OF 'COLBMPL1.TXT'
       FIN-SCR-CLRA-PL1.
           EXIT.
       
       CLR-KEY-PL1 SECTION.
       INI-CLR-KEY-PL1.
*% INCLUDE INI_CLR_KEY_PL1 OF 'COLBMPL1.TXT'
           MOVE SPACES TO OPC-CAI-IOPC IN PL1-FLD.
           MOVE ZEROES TO OPC-IIC-IOPC IN PL1-FLD.
*% INCLUDE FIN_CLR_KEY_PL1 OF 'COLBMPL1.TXT'
       FIN-CLR-KEY-PL1.
           EXIT.
       
       CLR-FLD-PL1 SECTION.
       INI-CLR-FLD-PL1.
*% INCLUDE INI_CLR_FLD_PL1 OF 'COLBMPL1.TXT'
           MOVE SPACES TO FRM-GLS-VEXP IN PL1-FLD.
           MOVE ZEROES TO FRM-VAL-CRED IN PL1-FLD.
           MOVE ZEROES TO FRM-VAL-INTE IN PL1-FLD.
           MOVE ZEROES TO FRM-VAL-VCOM IN PL1-FLD.
           MOVE ZEROES TO FRM-VAL-GAST IN PL1-FLD.
           MOVE ZEROES TO FRM-VAL-IMPU IN PL1-FLD.
           MOVE SPACES TO FRM-GLS-TINT IN PL1-FLD.
           MOVE ZEROES TO FRM-NUM-IVCT IN PL1-FLD(1).
           MOVE ZEROES TO FRM-NUM-IVCT IN PL1-FLD(2).
           MOVE ZEROES TO FRM-NUM-IVCT IN PL1-FLD(3).
           MOVE ZEROES TO FRM-NUM-IVCT IN PL1-FLD(4).
           MOVE ZEROES TO FRM-NUM-IVCT IN PL1-FLD(5).
           MOVE ZEROES TO FRM-NUM-IVCT IN PL1-FLD(6).
           MOVE ZEROES TO FRM-NUM-IVCT IN PL1-FLD(7).
           MOVE ZEROES TO FRM-NUM-IVCT IN PL1-FLD(8).
           MOVE ZEROES TO FRM-NUM-IVCT IN PL1-FLD(9).
           MOVE SPACES TO FRM-IND-CANC IN PL1-FLD(1).
           MOVE SPACES TO FRM-IND-CANC IN PL1-FLD(2).
           MOVE SPACES TO FRM-IND-CANC IN PL1-FLD(3).
           MOVE SPACES TO FRM-IND-CANC IN PL1-FLD(4).
           MOVE SPACES TO FRM-IND-CANC IN PL1-FLD(5).
           MOVE SPACES TO FRM-IND-CANC IN PL1-FLD(6).
           MOVE SPACES TO FRM-IND-CANC IN PL1-FLD(7).
           MOVE SPACES TO FRM-IND-CANC IN PL1-FLD(8).
           MOVE SPACES TO FRM-IND-CANC IN PL1-FLD(9).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FVEN IN PL1-FLD(1).
           MOVE ZEROES TO FRM-FEC-FVEN IN PL1-FLD(2).
           MOVE ZEROES TO FRM-FEC-FVEN IN PL1-FLD(3).
           MOVE ZEROES TO FRM-FEC-FVEN IN PL1-FLD(4).
           MOVE ZEROES TO FRM-FEC-FVEN IN PL1-FLD(5).
           MOVE ZEROES TO FRM-FEC-FVEN IN PL1-FLD(6).
           MOVE ZEROES TO FRM-FEC-FVEN IN PL1-FLD(7).
           MOVE ZEROES TO FRM-FEC-FVEN IN PL1-FLD(8).
           MOVE ZEROES TO FRM-FEC-FVEN IN PL1-FLD(9).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-VAL-INTC IN PL1-FLD(1).
           MOVE ZEROES TO FRM-VAL-INTC IN PL1-FLD(2).
           MOVE ZEROES TO FRM-VAL-INTC IN PL1-FLD(3).
           MOVE ZEROES TO FRM-VAL-INTC IN PL1-FLD(4).
           MOVE ZEROES TO FRM-VAL-INTC IN PL1-FLD(5).
           MOVE ZEROES TO FRM-VAL-INTC IN PL1-FLD(6).
           MOVE ZEROES TO FRM-VAL-INTC IN PL1-FLD(7).
           MOVE ZEROES TO FRM-VAL-INTC IN PL1-FLD(8).
           MOVE ZEROES TO FRM-VAL-INTC IN PL1-FLD(9).
           MOVE ZEROES TO FRM-VAL-SCAP IN PL1-FLD(1).
           MOVE ZEROES TO FRM-VAL-SCAP IN PL1-FLD(2).
           MOVE ZEROES TO FRM-VAL-SCAP IN PL1-FLD(3).
           MOVE ZEROES TO FRM-VAL-SCAP IN PL1-FLD(4).
           MOVE ZEROES TO FRM-VAL-SCAP IN PL1-FLD(5).
           MOVE ZEROES TO FRM-VAL-SCAP IN PL1-FLD(6).
           MOVE ZEROES TO FRM-VAL-SCAP IN PL1-FLD(7).
           MOVE ZEROES TO FRM-VAL-SCAP IN PL1-FLD(8).
           MOVE ZEROES TO FRM-VAL-SCAP IN PL1-FLD(9).
           MOVE ZEROES TO FRM-VAL-TOTA IN PL1-FLD(1).
           MOVE ZEROES TO FRM-VAL-TOTA IN PL1-FLD(2).
           MOVE ZEROES TO FRM-VAL-TOTA IN PL1-FLD(3).
           MOVE ZEROES TO FRM-VAL-TOTA IN PL1-FLD(4).
           MOVE ZEROES TO FRM-VAL-TOTA IN PL1-FLD(5).
           MOVE ZEROES TO FRM-VAL-TOTA IN PL1-FLD(6).
           MOVE ZEROES TO FRM-VAL-TOTA IN PL1-FLD(7).
           MOVE ZEROES TO FRM-VAL-TOTA IN PL1-FLD(8).
           MOVE ZEROES TO FRM-VAL-TOTA IN PL1-FLD(9).
           PERFORM CLR-OPC-PL1.
           PERFORM CLR-DBC-PL1.
           PERFORM CLR-ITR-PL1.
*% INCLUDE FIN_CLR_FLD_PL1 OF 'COLBMPL1.TXT'
       FIN-CLR-FLD-PL1.
           EXIT.
       
       CLR-OPC-PL1 SECTION.
       INI-CLR-OPC-PL1.
*% INCLUDE INI_CLR_OPC_PL1 OF 'COLBMPL1.TXT'
           MOVE SPACES TO OPC-COD-VCOC IN PL1-FLD.
           MOVE SPACES TO OPC-COD-TOOC IN PL1-FLD.
           MOVE SPACES TO OPC-COD-AOOC IN PL1-FLD.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO OPC-FEC-FCOL IN PL1-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO OPC-VAL-SEGD IN PL1-FLD.
           MOVE ZEROES TO OPC-SGV-LIQU IN PL1-FLD.
*% INCLUDE FIN_CLR_OPC_PL1 OF 'COLBMPL1.TXT'
       FIN-CLR-OPC-PL1.
           EXIT.
       
       CLR-DBC-PL1 SECTION.
       INI-CLR-DBC-PL1.
*% INCLUDE INI_CLR_DBC_PL1 OF 'COLBMPL1.TXT'
           MOVE SPACES TO DBC-GLS-NOMC IN PL1-FLD.
*% INCLUDE FIN_CLR_DBC_PL1 OF 'COLBMPL1.TXT'
       FIN-CLR-DBC-PL1.
           EXIT.
       
       CLR-ITR-PL1 SECTION.
       INI-CLR-ITR-PL1.
*% INCLUDE INI_CLR_ITR_PL1 OF 'COLBMPL1.TXT'
           MOVE ZEROES TO ITR-SGV-TINT IN PL1-FLD.
*% INCLUDE FIN_CLR_ITR_PL1 OF 'COLBMPL1.TXT'
       FIN-CLR-ITR-PL1.
           EXIT.
       
       FST-KEY-PL1 SECTION.
       INI-FST-KEY-PL1.
*% INCLUDE INI_FST_KEY_PL1 OF 'COLBMPL1.TXT'
           MOVE -1 TO OPC-CAI-IOPC-LEN IN PL1-FLD.
*% INCLUDE FIN_FST_KEY_PL1 OF 'COLBMPL1.TXT'
       FIN-FST-KEY-PL1.
           EXIT.
       
       FST-FLD-PL1 SECTION.
       INI-FST-FLD-PL1.
*% INCLUDE INI_FST_FLD_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_FST_FLD_PL1 OF 'COLBMPL1.TXT'
       FIN-FST-FLD-PL1.
           EXIT.
       
       PRO-KEY-PL1 SECTION.
       INI-PRO-KEY-PL1.
*% INCLUDE INI_PRO_KEY_PL1 OF 'COLBMPL1.TXT'
           PERFORM PRO-IKY-PL1.
           PERFORM PRO-AKY-PL1.
           PERFORM PRO-MKY-PL1.
*% INCLUDE FIN_PRO_KEY_PL1 OF 'COLBMPL1.TXT'
       FIN-PRO-KEY-PL1.
           EXIT.
       
       PRO-IKY-PL1 SECTION.
       INI-PRO-IKY-PL1.
*% INCLUDE INI_PRO_IKY_PL1 OF 'COLBMPL1.TXT'
           MOVE FRM-CPIM-AEY TO OPC-CAI-IOPC-ATR IN PL1-FLD.
           MOVE FRM-CPIM-AEY TO OPC-IIC-IOPC-ATR IN PL1-FLD.
*% INCLUDE FIN_PRO_IKY_PL1 OF 'COLBMPL1.TXT'
       FIN-PRO-IKY-PL1.
           EXIT.
       
       PRO-MKY-PL1 SECTION.
       INI-PRO-MKY-PL1.
*% INCLUDE INI_PRO_MKY_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_PRO_MKY_PL1 OF 'COLBMPL1.TXT'
       FIN-PRO-MKY-PL1.
           EXIT.
       
       PRO-AKY-PL1 SECTION.
       INI-PRO-AKY-PL1.
*% INCLUDE INI_PRO_AKY_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_PRO_AKY_PL1 OF 'COLBMPL1.TXT'
       FIN-PRO-AKY-PL1.
           EXIT.
       
       UNP-KEY-PL1 SECTION.
       INI-UNP-KEY-PL1.
*% INCLUDE INI_UNP_KEY_PL1 OF 'COLBMPL1.TXT'
           PERFORM UNP-IKY-PL1.
           PERFORM UNP-AKY-PL1.
           PERFORM UNP-MKY-PL1.
*% INCLUDE FIN_UNP_KEY_PL1 OF 'COLBMPL1.TXT'
       FIN-UNP-KEY-PL1.
           EXIT.
       
       UNP-IKY-PL1 SECTION.
       INI-UNP-IKY-PL1.
*% INCLUDE INI_UNP_IKY_PL1 OF 'COLBMPL1.TXT'
           MOVE FRM-CPIM-UBY TO OPC-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO OPC-IIC-IOPC-ATR.
*% INCLUDE FIN_UNP_IKY_PL1 OF 'COLBMPL1.TXT'
       FIN-UNP-IKY-PL1.
           EXIT.
       
       UNP-MKY-PL1 SECTION.
       INI-UNP-MKY-PL1.
*% INCLUDE INI_UNP_MKY_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_UNP_MKY_PL1 OF 'COLBMPL1.TXT'
       FIN-UNP-MKY-PL1.
           EXIT.
       
       UNP-AKY-PL1 SECTION.
       INI-UNP-AKY-PL1.
*% INCLUDE INI_UNP_AKY_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_UNP_AKY_PL1 OF 'COLBMPL1.TXT'
       FIN-UNP-AKY-PL1.
           EXIT.
       
       UNP-FLD-PL1 SECTION.
       INI-UNP-FLD-PL1.
*% INCLUDE INI_UNP_FLD_PL1 OF 'COLBMPL1.TXT'
           PERFORM UNP-OPC-PL1.
           PERFORM UNP-DBC-PL1.
           PERFORM UNP-ITR-PL1.
*% INCLUDE FIN_UNP_FLD_PL1 OF 'COLBMPL1.TXT'
       FIN-UNP-FLD-PL1.
           EXIT.
       
       UNP-OPC-PL1 SECTION.
       INI-UNP-OPC-PL1.
*% INCLUDE INI_UNP_OPC_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_UNP_OPC_PL1 OF 'COLBMPL1.TXT'
       FIN-UNP-OPC-PL1.
           EXIT.
       
       UNP-DBC-PL1 SECTION.
       INI-UNP-DBC-PL1.
*% INCLUDE INI_UNP_DBC_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_UNP_DBC_PL1 OF 'COLBMPL1.TXT'
       FIN-UNP-DBC-PL1.
           EXIT.
       
       UNP-ITR-PL1 SECTION.
       INI-UNP-ITR-PL1.
*% INCLUDE INI_UNP_ITR_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_UNP_ITR_PL1 OF 'COLBMPL1.TXT'
       FIN-UNP-ITR-PL1.
           EXIT.
       
       VAL-KEY-PL1 SECTION.
       INI-VAL-KEY-PL1.
           PERFORM VAL-IKY-PL1.
           PERFORM VAL-MKY-PL1.
           PERFORM VAL-AKY-PL1.
       FIN-VAL-KEY-PL1.
           EXIT.
       
       VAL-IKY-PL1 SECTION.
       INI-VAL-IKY-PL1.
*% INCLUDE INI_VAL_IKY_PL1 OF 'COLBMPL1.TXT'
           PERFORM VAL-OPC-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-CAI-IOPC-LEN IN PL1-FLD
                GO TO FIN-VAL-IKY-PL1.
           PERFORM VAL-OPC-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-IIC-IOPC-LEN IN PL1-FLD
                GO TO FIN-VAL-IKY-PL1.
*% INCLUDE FIN_VAL_IKY_PL1 OF 'COLBMPL1.TXT'
       FIN-VAL-IKY-PL1.
           EXIT.
       
       VAL-MKY-PL1 SECTION.
       INI-VAL-MKY-PL1.
*% INCLUDE INI_VAL_MKY_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_VAL_MKY_PL1 OF 'COLBMPL1.TXT'
       FIN-VAL-MKY-PL1.
           EXIT.
       
       VAL-AKY-PL1 SECTION.
       INI-VAL-AKY-PL1.
*% INCLUDE INI_VAL_AKY_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_VAL_AKY_PL1 OF 'COLBMPL1.TXT'
       FIN-VAL-AKY-PL1.
           EXIT.
       
       VAL-OPC-CAI-IOPC SECTION.
       INI-VAL-OPC-CAI-IOPC.
*% LBL = "OPC_CAI_IOPC OF 'COLBMPL1.TXT'"
*% INCLUDE INI_OPC_CAI_IOPC OF 'COLBMPL1.TXT'
           IF OPC-CAI-IOPC IN PL1-FLD NOT > SPACES
              MOVE SPACES TO OPC-CAI-IOPC IN PL1-FLD.
*% INCLUDE OPC_CAI_IOPC OF 'COLBMPL1.TXT'
*% INCLUDE FIN_OPC_CAI_IOPC OF 'COLBMPL1.TXT'
       FIN-VAL-OPC-CAI-IOPC.
           EXIT.
       
       VAL-OPC-IIC-IOPC SECTION.
       INI-VAL-OPC-IIC-IOPC.
*% LBL = "OPC_IIC_IOPC OF 'COLBMPL1.TXT'"
*% INCLUDE INI_OPC_IIC_IOPC OF 'COLBMPL1.TXT'
*% INCLUDE OPC_IIC_IOPC OF 'COLBMPL1.TXT'
*% INCLUDE FIN_OPC_IIC_IOPC OF 'COLBMPL1.TXT'
       FIN-VAL-OPC-IIC-IOPC.
           EXIT.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
*% IZR = "S"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVEN"
*% IZR = "S"
       
       VAL-CON-KEY-PL1 SECTION.
       INI-VAL-CON-KEY-PL1.
*% INCLUDE INI_VAL_CON_KEY_PL1 OF 'COLBMPL1.TXT'
           IF     OPC-CAI-IOPC IN PL1-FLD NOT > SPACES 
              AND OPC-IIC-IOPC IN PL1-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-PL1.
*% INCLUDE FIN_VAL_CON_KEY_PL1 OF 'COLBMPL1.TXT'
       FIN-VAL-CON-KEY-PL1.
           EXIT.
       
       VAL-CON-IKY-PL1 SECTION.
       INI-VAL-CON-IKY-PL1.
*% INCLUDE INI_VAL_CON_IKY_PL1 OF 'COLBMPL1.TXT'
           IF     OPC-CAI-IOPC IN PL1-FLD NOT > SPACES 
              AND OPC-IIC-IOPC IN PL1-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-PL1.
*% INCLUDE FIN_VAL_CON_IKY_PL1 OF 'COLBMPL1.TXT'
       FIN-VAL-CON-IKY-PL1.
           EXIT.
       
       VAL-NUM-KEY-PL1 SECTION.
       INI-VAL-NUM-KEY-PL1.
           PERFORM VAL-NUM-IKY-PL1.
           PERFORM VAL-NUM-MKY-PL1.
           PERFORM VAL-NUM-AKY-PL1.
       FIN-VAL-NUM-KEY-PL1.
           EXIT.
       
       VAL-NUM-IKY-PL1 SECTION.
       INI-VAL-NUM-IKY-PL1.
*% INCLUDE INI_VAL_NUM_IKY_PL1 OF 'COLBMPL1.TXT'
       VAL-NUM-OPC-IIC-IOPC.
*% BUF = "PL1-FLD"
*% FLD = "OPC-IIC-IOPC"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-PL1.
*% INCLUDE FIN_VAL_NUM_IKY_PL1 OF 'COLBMPL1.TXT'
       FIN-VAL-NUM-IKY-PL1.
           EXIT.
       
       VAL-NUM-MKY-PL1 SECTION.
       INI-VAL-NUM-MKY-PL1.
*% INCLUDE INI_VAL_NUM_MKY_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_PL1 OF 'COLBMPL1.TXT'
       FIN-VAL-NUM-MKY-PL1.
           EXIT.
       
       VAL-NUM-AKY-PL1 SECTION.
       INI-VAL-NUM-AKY-PL1.
*% INCLUDE INI_VAL_NUM_AKY_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_PL1 OF 'COLBMPL1.TXT'
       FIN-VAL-NUM-AKY-PL1.
           EXIT.
       
       VAL-NUM-FLD-PL1 SECTION.
       INI-VAL-NUM-FLD-PL1.
*% INCLUDE INI_VAL_NUM_FLD_PL1 OF 'COLBMPL1.TXT'
       VAL-NUM-FRM-VAL-CRED.
*% BUF = "PL1-FLD"
*% FLD = "FRM-VAL-CRED"
*% ENT = "11"
*% DCM = "4"
