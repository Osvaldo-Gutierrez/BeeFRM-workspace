       EDT-KEY-PL1 SECTION.
       INI-EDT-KEY-PL1.
           PERFORM EDT-IKY-PL1.
           PERFORM EDT-MKY-PL1.
           PERFORM EDT-AKY-PL1.
*% INCLUDE EDT_KEY_PL1 OF 'COLBMPL1.TXT'
       FIN-EDT-KEY-PL1.
           EXIT.
       
       EDT-IKY-PL1 SECTION.
       INI-EDT-IKY-PL1.
*% INCLUDE EDT_IKY_PL1 OF 'COLBMPL1.TXT'
           IF OPC-IIC-IOPC-LEN IN PL1-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO OPC-IIC-IOPC-LEN IN PL1-FLD
                GO TO FIN-EDT-IKY-PL1.
           MOVE OPC-IIC-IOPC IN PL1-FLD TO OPC-IIC-IOPC-EDT IN PL1-EDT.
           MOVE OPC-IIC-IOPC-EDT IN PL1-EDT TO 
                OPC-IIC-IOPC-ALF IN PL1-FLD.
*% INCLUDE FIN_EDT_IKY_PL1 OF 'COLBMPL1.TXT'
       FIN-EDT-IKY-PL1.
           EXIT.
       
       EDT-MKY-PL1 SECTION.
       INI-EDT-MKY-PL1.
*% INCLUDE EDT_MKY_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_EDT_MKY_PL1 OF 'COLBMPL1.TXT'
       FIN-EDT-MKY-PL1.
           EXIT.
       
       EDT-AKY-PL1 SECTION.
       INI-EDT-AKY-PL1.
*% INCLUDE EDT_AKY_PL1 OF 'COLBMPL1.TXT'
*% INCLUDE FIN_EDT_AKY_PL1 OF 'COLBMPL1.TXT'
       FIN-EDT-AKY-PL1.
           EXIT.
       
       EDT-FLD-PL1 SECTION.
       INI-EDT-FLD-PL1.
*% INCLUDE EDT_FLD_PL1 OF 'COLBMPL1.TXT'
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
           IF FRM-VAL-CRED-LEN IN PL1-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-CRED-LEN IN PL1-FLD
                GO TO FIN-EDT-FLD-PL1.
           MOVE FRM-VAL-CRED IN PL1-FLD TO FRM-VAL-CRED-EDT IN PL1-EDT.
           MOVE FRM-VAL-CRED-EDT IN PL1-EDT TO 
                FRM-VAL-CRED-ALF IN PL1-FLD.
           IF FRM-VAL-INTE-LEN IN PL1-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-INTE-LEN IN PL1-FLD
                GO TO FIN-EDT-FLD-PL1.
           MOVE FRM-VAL-INTE IN PL1-FLD TO FRM-VAL-INTE-EDT IN PL1-EDT.
           MOVE FRM-VAL-INTE-EDT IN PL1-EDT TO 
                FRM-VAL-INTE-ALF IN PL1-FLD.
           IF FRM-VAL-VCOM-LEN IN PL1-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-VCOM-LEN IN PL1-FLD
                GO TO FIN-EDT-FLD-PL1.
           MOVE FRM-VAL-VCOM IN PL1-FLD TO FRM-VAL-VCOM-EDT IN PL1-EDT.
           MOVE FRM-VAL-VCOM-EDT IN PL1-EDT TO 
                FRM-VAL-VCOM-ALF IN PL1-FLD.
           IF OPC-VAL-SEGD-LEN IN PL1-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO OPC-VAL-SEGD-LEN IN PL1-FLD
                GO TO FIN-EDT-FLD-PL1.
           MOVE OPC-VAL-SEGD IN PL1-FLD TO OPC-VAL-SEGD-EDT IN PL1-EDT.
           MOVE OPC-VAL-SEGD-EDT IN PL1-EDT TO 
                OPC-VAL-SEGD-ALF IN PL1-FLD.
           IF FRM-VAL-GAST-LEN IN PL1-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-GAST-LEN IN PL1-FLD
                GO TO FIN-EDT-FLD-PL1.
           MOVE FRM-VAL-GAST IN PL1-FLD TO FRM-VAL-GAST-EDT IN PL1-EDT.
           MOVE FRM-VAL-GAST-EDT IN PL1-EDT TO 
                FRM-VAL-GAST-ALF IN PL1-FLD.
           IF FRM-VAL-IMPU-LEN IN PL1-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-IMPU-LEN IN PL1-FLD
                GO TO FIN-EDT-FLD-PL1.
           MOVE FRM-VAL-IMPU IN PL1-FLD TO FRM-VAL-IMPU-EDT IN PL1-EDT.
           MOVE FRM-VAL-IMPU-EDT IN PL1-EDT TO 
                FRM-VAL-IMPU-ALF IN PL1-FLD.
           IF OPC-SGV-LIQU-LEN IN PL1-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO OPC-SGV-LIQU-LEN IN PL1-FLD
                GO TO FIN-EDT-FLD-PL1.
           MOVE OPC-SGV-LIQU IN PL1-FLD TO OPC-SGV-LIQU-EDT IN PL1-EDT.
           MOVE OPC-SGV-LIQU-EDT IN PL1-EDT TO 
                OPC-SGV-LIQU-ALF IN PL1-FLD.
           IF ITR-SGV-TINT-LEN IN PL1-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO ITR-SGV-TINT-LEN IN PL1-FLD
                GO TO FIN-EDT-FLD-PL1.
           MOVE ITR-SGV-TINT IN PL1-FLD TO ITR-SGV-TINT-EDT IN PL1-EDT.
           MOVE ITR-SGV-TINT-EDT IN PL1-EDT TO 
                ITR-SGV-TINT-ALF IN PL1-FLD.
           IF FRM-NUM-IVCT-LEN IN PL1-FLD(1) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-IVCT-LEN IN PL1-FLD(1)
                GO TO FIN-EDT-FLD-PL1.
           MOVE FRM-NUM-IVCT IN PL1-FLD(1) TO 
                FRM-NUM-IVCT-EDT IN PL1-EDT.
           MOVE FRM-NUM-IVCT-EDT IN PL1-EDT TO 
                FRM-NUM-IVCT-ALF IN PL1-FLD(1).
           IF FRM-NUM-IVCT-LEN IN PL1-FLD(2) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-IVCT-LEN IN PL1-FLD(2)
                GO TO FIN-EDT-FLD-PL1.
           MOVE FRM-NUM-IVCT IN PL1-FLD(2) TO 
                FRM-NUM-IVCT-EDT IN PL1-EDT.
           MOVE FRM-NUM-IVCT-EDT IN PL1-EDT TO 
                FRM-NUM-IVCT-ALF IN PL1-FLD(2).
           IF FRM-NUM-IVCT-LEN IN PL1-FLD(3) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-IVCT-LEN IN PL1-FLD(3)
                GO TO FIN-EDT-FLD-PL1.
           MOVE FRM-NUM-IVCT IN PL1-FLD(3) TO 
                FRM-NUM-IVCT-EDT IN PL1-EDT.
           MOVE FRM-NUM-IVCT-EDT IN PL1-EDT TO 
                FRM-NUM-IVCT-ALF IN PL1-FLD(3).
           IF FRM-NUM-IVCT-LEN IN PL1-FLD(4) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-IVCT-LEN IN PL1-FLD(4)
                GO TO FIN-EDT-FLD-PL1.
           MOVE FRM-NUM-IVCT IN PL1-FLD(4) TO 
                FRM-NUM-IVCT-EDT IN PL1-EDT.
           MOVE FRM-NUM-IVCT-EDT IN PL1-EDT TO 
                FRM-NUM-IVCT-ALF IN PL1-FLD(4).
           IF FRM-NUM-IVCT-LEN IN PL1-FLD(5) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-IVCT-LEN IN PL1-FLD(5)
                GO TO FIN-EDT-FLD-PL1.
           MOVE FRM-NUM-IVCT IN PL1-FLD(5) TO 
                FRM-NUM-IVCT-EDT IN PL1-EDT.
           MOVE FRM-NUM-IVCT-EDT IN PL1-EDT TO 
                FRM-NUM-IVCT-ALF IN PL1-FLD(5).
           IF FRM-NUM-IVCT-LEN IN PL1-FLD(6) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-IVCT-LEN IN PL1-FLD(6)
                GO TO FIN-EDT-FLD-PL1.
