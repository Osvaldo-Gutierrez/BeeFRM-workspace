       EDT-KEY-DEC SECTION.
       INI-EDT-KEY-DEC.
           PERFORM EDT-IKY-DEC.
           PERFORM EDT-MKY-DEC.
           PERFORM EDT-AKY-DEC.
*% INCLUDE EDT_KEY_DEC OF 'COLBMDEC.TXT'
       FIN-EDT-KEY-DEC.
           EXIT.
       
       EDT-IKY-DEC SECTION.
       INI-EDT-IKY-DEC.
*% INCLUDE EDT_IKY_DEC OF 'COLBMDEC.TXT'
           IF CAN-IIC-IOPC-LEN IN DEC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CAN-IIC-IOPC-LEN IN DEC-FLD
                GO TO FIN-EDT-IKY-DEC.
           MOVE CAN-IIC-IOPC IN DEC-FLD TO CAN-IIC-IOPC-EDT IN DEC-EDT.
           MOVE CAN-IIC-IOPC-EDT IN DEC-EDT TO 
                CAN-IIC-IOPC-ALF IN DEC-FLD.
           IF CAN-NUM-ICAN-LEN IN DEC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CAN-NUM-ICAN-LEN IN DEC-FLD
                GO TO FIN-EDT-IKY-DEC.
           MOVE CAN-NUM-ICAN IN DEC-FLD TO CAN-NUM-ICAN-EDT IN DEC-EDT.
           MOVE CAN-NUM-ICAN-EDT IN DEC-EDT TO 
                CAN-NUM-ICAN-ALF IN DEC-FLD.
*% INCLUDE FIN_EDT_IKY_DEC OF 'COLBMDEC.TXT'
       FIN-EDT-IKY-DEC.
           EXIT.
       
       EDT-MKY-DEC SECTION.
       INI-EDT-MKY-DEC.
*% INCLUDE EDT_MKY_DEC OF 'COLBMDEC.TXT'
*% INCLUDE FIN_EDT_MKY_DEC OF 'COLBMDEC.TXT'
       FIN-EDT-MKY-DEC.
           EXIT.
       
       EDT-AKY-DEC SECTION.
       INI-EDT-AKY-DEC.
*% INCLUDE EDT_AKY_DEC OF 'COLBMDEC.TXT'
*% INCLUDE FIN_EDT_AKY_DEC OF 'COLBMDEC.TXT'
       FIN-EDT-AKY-DEC.
           EXIT.
       
       EDT-FLD-DEC SECTION.
       INI-EDT-FLD-DEC.
*% INCLUDE EDT_FLD_DEC OF 'COLBMDEC.TXT'
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
           IF FRM-VAL-VAUF-LEN IN DEC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-VAUF-LEN IN DEC-FLD
                GO TO FIN-EDT-FLD-DEC.
           MOVE FRM-VAL-VAUF IN DEC-FLD TO FRM-VAL-VAUF-EDT IN DEC-EDT.
           MOVE FRM-VAL-VAUF-EDT IN DEC-EDT TO 
                FRM-VAL-VAUF-ALF IN DEC-FLD.
           IF FRM-VAL-CPTO-LEN IN DEC-FLD(1) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-CPTO-LEN IN DEC-FLD(1)
                GO TO FIN-EDT-FLD-DEC.
           MOVE FRM-VAL-CPTO IN DEC-FLD(1) TO 
                FRM-VAL-CPTO-EDT IN DEC-EDT.
           MOVE FRM-VAL-CPTO-EDT IN DEC-EDT TO 
                FRM-VAL-CPTO-ALF IN DEC-FLD(1).
           IF FRM-VAL-CPTO-LEN IN DEC-FLD(2) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-CPTO-LEN IN DEC-FLD(2)
                GO TO FIN-EDT-FLD-DEC.
           MOVE FRM-VAL-CPTO IN DEC-FLD(2) TO 
                FRM-VAL-CPTO-EDT IN DEC-EDT.
           MOVE FRM-VAL-CPTO-EDT IN DEC-EDT TO 
                FRM-VAL-CPTO-ALF IN DEC-FLD(2).
           IF FRM-VAL-CPTO-LEN IN DEC-FLD(3) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-CPTO-LEN IN DEC-FLD(3)
                GO TO FIN-EDT-FLD-DEC.
           MOVE FRM-VAL-CPTO IN DEC-FLD(3) TO 
                FRM-VAL-CPTO-EDT IN DEC-EDT.
           MOVE FRM-VAL-CPTO-EDT IN DEC-EDT TO 
                FRM-VAL-CPTO-ALF IN DEC-FLD(3).
           IF FRM-VAL-CPTO-LEN IN DEC-FLD(4) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-CPTO-LEN IN DEC-FLD(4)
                GO TO FIN-EDT-FLD-DEC.
           MOVE FRM-VAL-CPTO IN DEC-FLD(4) TO 
                FRM-VAL-CPTO-EDT IN DEC-EDT.
           MOVE FRM-VAL-CPTO-EDT IN DEC-EDT TO 
                FRM-VAL-CPTO-ALF IN DEC-FLD(4).
           IF FRM-VAL-CPTO-LEN IN DEC-FLD(5) = FRM-ERR-NUME
