      *
      * COLBQDEC
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:53 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "DEC"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMDEC.TXT'
       SET-TAG-COLBQDEC SECTION.
       INI-SET-TAG-COLBQDEC.
           MOVE
           '<IDNMOD NOM=$COLBQDEC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:53 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQDEC.
           EXIT.
           
       PUT-ALL-DEC SECTION.
       INI-PUT-ALL-DEC.
*% INCLUDE INI_PUT_ALL_DEC OF 'COLBMDEC.TXT'
           PERFORM PUT-CAN-DEC.
           PERFORM PUT-OPC-DEC.
*% INCLUDE FIN_PUT_ALL_DEC OF 'COLBMDEC.TXT'
       FIN-PUT-ALL-DEC.
           EXIT.
       
       PUT-CAN-DEC SECTION.
       INI-PUT-CAN-DEC.
*% INCLUDE INI_PUT_CAN_DEC OF 'COLBMDEC.TXT'
           IF PGM-STAT-CAN-NUL
               GO TO FIN-PUT-CAN-DEC.
           IF NOT PGM-STAT-CAN-OKS
               PERFORM CLR-CAN-DEC
               GO TO FIN-PUT-CAN-DEC.
           MOVE CAN-CAI-IOPC IN CAN TO CAN-CAI-IOPC IN DEC-FLD.
           MOVE CAN-IIC-IOPC IN CAN TO CAN-IIC-IOPC IN DEC-FLD.
           MOVE CAN-NUM-ICAN IN CAN TO CAN-NUM-ICAN IN DEC-FLD.
*% INCLUDE FIN_PUT_CAN_DEC OF 'COLBMDEC.TXT'
       FIN-PUT-CAN-DEC.
           EXIT.
       
       PUT-OPC-DEC SECTION.
       INI-PUT-OPC-DEC.
*% INCLUDE INI_PUT_OPC_DEC OF 'COLBMDEC.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-DEC.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-DEC
               GO TO FIN-PUT-OPC-DEC.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE OPC-NUM-DCOL IN OPC TO OPC-NUM-DCOL IN DEC-FLD.
           MOVE OPC-NUM-MCOL IN OPC TO OPC-NUM-MCOL IN DEC-FLD.
           MOVE OPC-NUM-SCOL IN OPC TO OPC-NUM-SCOL IN DEC-FLD.
           MOVE OPC-NUM-ACOL IN OPC TO OPC-NUM-ACOL IN DEC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
*% INCLUDE FIN_PUT_OPC_DEC OF 'COLBMDEC.TXT'
       FIN-PUT-OPC-DEC.
           EXIT.
       
       KEY-ALL-DEC SECTION.
       INI-KEY-ALL-DEC.
*% INCLUDE INI_KEY_ALL_DEC OF 'COLBMDEC.TXT'
           PERFORM KEY-CAN-DEC.
           PERFORM KEY-OPC-DEC.
*% INCLUDE FIN_KEY_ALL_DEC OF 'COLBMDEC.TXT'
       FIN-KEY-ALL-DEC.
           EXIT.
       
       KEY-CAN-DEC SECTION.
       INI-KEY-CAN-DEC.
*% INCLUDE INI_KEY_CAN_DEC OF 'COLBMDEC.TXT'
           MOVE CAN-CAI-IOPC IN DEC-FLD TO CAN-CAI-IOPC IN CAN.
           MOVE CAN-IIC-IOPC IN DEC-FLD TO CAN-IIC-IOPC IN CAN.
           MOVE CAN-NUM-ICAN IN DEC-FLD TO CAN-NUM-ICAN IN CAN.
*% INCLUDE FIN_KEY_CAN_DEC OF 'COLBMDEC.TXT'
       FIN-KEY-CAN-DEC.
           EXIT.
       
       KEY-OPC-DEC SECTION.
       INI-KEY-OPC-DEC.
*% INCLUDE INI_KEY_OPC_DEC OF 'COLBMDEC.TXT'
*% INCLUDE FIN_KEY_OPC_DEC OF 'COLBMDEC.TXT'
       FIN-KEY-OPC-DEC.
           EXIT.
       
       SCR-CLRA-DEC SECTION.
       INI-SCR-CLRA-DEC.
*% INCLUDE INI_SCR_CLRA_DEC OF 'COLBMDEC.TXT'
           PERFORM CLR-KEY-DEC.
           PERFORM CLR-FLD-DEC.
*% INCLUDE FIN_SCR_CLRA_DEC OF 'COLBMDEC.TXT'
       FIN-SCR-CLRA-DEC.
           EXIT.
       
       CLR-KEY-DEC SECTION.
       INI-CLR-KEY-DEC.
*% INCLUDE INI_CLR_KEY_DEC OF 'COLBMDEC.TXT'
           MOVE SPACES TO CAN-CAI-IOPC IN DEC-FLD.
           MOVE ZEROES TO CAN-IIC-IOPC IN DEC-FLD.
           MOVE ZEROES TO CAN-NUM-ICAN IN DEC-FLD.
*% INCLUDE FIN_CLR_KEY_DEC OF 'COLBMDEC.TXT'
       FIN-CLR-KEY-DEC.
           EXIT.
       
       CLR-FLD-DEC SECTION.
       INI-CLR-FLD-DEC.
*% INCLUDE INI_CLR_FLD_DEC OF 'COLBMDEC.TXT'
           MOVE ZEROES TO FRM-VAL-VAUF IN DEC-FLD.
           MOVE SPACES TO FRM-COD-CPTO IN DEC-FLD(1).
           MOVE SPACES TO FRM-COD-CPTO IN DEC-FLD(2).
           MOVE SPACES TO FRM-COD-CPTO IN DEC-FLD(3).
           MOVE SPACES TO FRM-COD-CPTO IN DEC-FLD(4).
           MOVE SPACES TO FRM-COD-CPTO IN DEC-FLD(5).
           MOVE SPACES TO FRM-COD-CPTO IN DEC-FLD(6).
           MOVE SPACES TO FRM-COD-CPTO IN DEC-FLD(7).
           MOVE SPACES TO FRM-COD-CPTO IN DEC-FLD(8).
           MOVE SPACES TO FRM-COD-CPTO IN DEC-FLD(9).
           MOVE SPACES TO FRM-COD-CPTO IN DEC-FLD(10).
           MOVE SPACES TO FRM-COD-CPTO IN DEC-FLD(11).
           MOVE SPACES TO FRM-COD-CPTO IN DEC-FLD(12).
           MOVE SPACES TO FRM-COD-CPTO IN DEC-FLD(13).
           MOVE SPACES TO FRM-COD-CPTO IN DEC-FLD(14).
           MOVE SPACES TO FRM-GLS-CPTO IN DEC-FLD(1).
           MOVE SPACES TO FRM-GLS-CPTO IN DEC-FLD(2).
           MOVE SPACES TO FRM-GLS-CPTO IN DEC-FLD(3).
           MOVE SPACES TO FRM-GLS-CPTO IN DEC-FLD(4).
           MOVE SPACES TO FRM-GLS-CPTO IN DEC-FLD(5).
           MOVE SPACES TO FRM-GLS-CPTO IN DEC-FLD(6).
           MOVE SPACES TO FRM-GLS-CPTO IN DEC-FLD(7).
           MOVE SPACES TO FRM-GLS-CPTO IN DEC-FLD(8).
           MOVE SPACES TO FRM-GLS-CPTO IN DEC-FLD(9).
           MOVE SPACES TO FRM-GLS-CPTO IN DEC-FLD(10).
           MOVE SPACES TO FRM-GLS-CPTO IN DEC-FLD(11).
           MOVE SPACES TO FRM-GLS-CPTO IN DEC-FLD(12).
           MOVE SPACES TO FRM-GLS-CPTO IN DEC-FLD(13).
           MOVE SPACES TO FRM-GLS-CPTO IN DEC-FLD(14).
           MOVE ZEROES TO FRM-VAL-CPTO IN DEC-FLD(1).
           MOVE ZEROES TO FRM-VAL-CPTO IN DEC-FLD(2).
           MOVE ZEROES TO FRM-VAL-CPTO IN DEC-FLD(3).
           MOVE ZEROES TO FRM-VAL-CPTO IN DEC-FLD(4).
           MOVE ZEROES TO FRM-VAL-CPTO IN DEC-FLD(5).
           MOVE ZEROES TO FRM-VAL-CPTO IN DEC-FLD(6).
           MOVE ZEROES TO FRM-VAL-CPTO IN DEC-FLD(7).
           MOVE ZEROES TO FRM-VAL-CPTO IN DEC-FLD(8).
           MOVE ZEROES TO FRM-VAL-CPTO IN DEC-FLD(9).
           MOVE ZEROES TO FRM-VAL-CPTO IN DEC-FLD(10).
           MOVE ZEROES TO FRM-VAL-CPTO IN DEC-FLD(11).
           MOVE ZEROES TO FRM-VAL-CPTO IN DEC-FLD(12).
           MOVE ZEROES TO FRM-VAL-CPTO IN DEC-FLD(13).
           MOVE ZEROES TO FRM-VAL-CPTO IN DEC-FLD(14).
           PERFORM CLR-CAN-DEC.
           PERFORM CLR-OPC-DEC.
*% INCLUDE FIN_CLR_FLD_DEC OF 'COLBMDEC.TXT'
       FIN-CLR-FLD-DEC.
           EXIT.
       
       CLR-CAN-DEC SECTION.
       INI-CLR-CAN-DEC.
*% INCLUDE INI_CLR_CAN_DEC OF 'COLBMDEC.TXT'
*% INCLUDE FIN_CLR_CAN_DEC OF 'COLBMDEC.TXT'
       FIN-CLR-CAN-DEC.
           EXIT.
       
       CLR-OPC-DEC SECTION.
       INI-CLR-OPC-DEC.
*% INCLUDE INI_CLR_OPC_DEC OF 'COLBMDEC.TXT'
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO OPC-FEC-FCOL IN DEC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_OPC_DEC OF 'COLBMDEC.TXT'
       FIN-CLR-OPC-DEC.
           EXIT.
       
       FST-KEY-DEC SECTION.
       INI-FST-KEY-DEC.
*% INCLUDE INI_FST_KEY_DEC OF 'COLBMDEC.TXT'
           MOVE -1 TO CAN-CAI-IOPC-LEN IN DEC-FLD.
*% INCLUDE FIN_FST_KEY_DEC OF 'COLBMDEC.TXT'
       FIN-FST-KEY-DEC.
           EXIT.
       
       FST-FLD-DEC SECTION.
       INI-FST-FLD-DEC.
*% INCLUDE INI_FST_FLD_DEC OF 'COLBMDEC.TXT'
*% INCLUDE FIN_FST_FLD_DEC OF 'COLBMDEC.TXT'
       FIN-FST-FLD-DEC.
           EXIT.
       
       PRO-KEY-DEC SECTION.
       INI-PRO-KEY-DEC.
*% INCLUDE INI_PRO_KEY_DEC OF 'COLBMDEC.TXT'
           PERFORM PRO-IKY-DEC.
           PERFORM PRO-AKY-DEC.
           PERFORM PRO-MKY-DEC.
*% INCLUDE FIN_PRO_KEY_DEC OF 'COLBMDEC.TXT'
       FIN-PRO-KEY-DEC.
           EXIT.
       
       PRO-IKY-DEC SECTION.
       INI-PRO-IKY-DEC.
*% INCLUDE INI_PRO_IKY_DEC OF 'COLBMDEC.TXT'
           MOVE FRM-CPIM-AEY TO CAN-CAI-IOPC-ATR IN DEC-FLD.
           MOVE FRM-CPIM-AEY TO CAN-IIC-IOPC-ATR IN DEC-FLD.
           MOVE FRM-CPIM-AEY TO CAN-NUM-ICAN-ATR IN DEC-FLD.
*% INCLUDE FIN_PRO_IKY_DEC OF 'COLBMDEC.TXT'
       FIN-PRO-IKY-DEC.
           EXIT.
       
       PRO-MKY-DEC SECTION.
       INI-PRO-MKY-DEC.
*% INCLUDE INI_PRO_MKY_DEC OF 'COLBMDEC.TXT'
*% INCLUDE FIN_PRO_MKY_DEC OF 'COLBMDEC.TXT'
       FIN-PRO-MKY-DEC.
           EXIT.
       
       PRO-AKY-DEC SECTION.
       INI-PRO-AKY-DEC.
*% INCLUDE INI_PRO_AKY_DEC OF 'COLBMDEC.TXT'
*% INCLUDE FIN_PRO_AKY_DEC OF 'COLBMDEC.TXT'
       FIN-PRO-AKY-DEC.
           EXIT.
       
       UNP-KEY-DEC SECTION.
       INI-UNP-KEY-DEC.
*% INCLUDE INI_UNP_KEY_DEC OF 'COLBMDEC.TXT'
           PERFORM UNP-IKY-DEC.
           PERFORM UNP-AKY-DEC.
           PERFORM UNP-MKY-DEC.
*% INCLUDE FIN_UNP_KEY_DEC OF 'COLBMDEC.TXT'
       FIN-UNP-KEY-DEC.
           EXIT.
       
       UNP-IKY-DEC SECTION.
       INI-UNP-IKY-DEC.
*% INCLUDE INI_UNP_IKY_DEC OF 'COLBMDEC.TXT'
           MOVE FRM-CPIM-UBY TO CAN-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO CAN-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO CAN-NUM-ICAN-ATR.
*% INCLUDE FIN_UNP_IKY_DEC OF 'COLBMDEC.TXT'
       FIN-UNP-IKY-DEC.
           EXIT.
       
       UNP-MKY-DEC SECTION.
       INI-UNP-MKY-DEC.
*% INCLUDE INI_UNP_MKY_DEC OF 'COLBMDEC.TXT'
*% INCLUDE FIN_UNP_MKY_DEC OF 'COLBMDEC.TXT'
       FIN-UNP-MKY-DEC.
           EXIT.
       
       UNP-AKY-DEC SECTION.
       INI-UNP-AKY-DEC.
*% INCLUDE INI_UNP_AKY_DEC OF 'COLBMDEC.TXT'
*% INCLUDE FIN_UNP_AKY_DEC OF 'COLBMDEC.TXT'
       FIN-UNP-AKY-DEC.
           EXIT.
       
       UNP-FLD-DEC SECTION.
       INI-UNP-FLD-DEC.
*% INCLUDE INI_UNP_FLD_DEC OF 'COLBMDEC.TXT'
           PERFORM UNP-CAN-DEC.
           PERFORM UNP-OPC-DEC.
*% INCLUDE FIN_UNP_FLD_DEC OF 'COLBMDEC.TXT'
       FIN-UNP-FLD-DEC.
           EXIT.
       
       UNP-CAN-DEC SECTION.
       INI-UNP-CAN-DEC.
*% INCLUDE INI_UNP_CAN_DEC OF 'COLBMDEC.TXT'
*% INCLUDE FIN_UNP_CAN_DEC OF 'COLBMDEC.TXT'
       FIN-UNP-CAN-DEC.
           EXIT.
       
       UNP-OPC-DEC SECTION.
       INI-UNP-OPC-DEC.
*% INCLUDE INI_UNP_OPC_DEC OF 'COLBMDEC.TXT'
*% INCLUDE FIN_UNP_OPC_DEC OF 'COLBMDEC.TXT'
       FIN-UNP-OPC-DEC.
           EXIT.
       
       VAL-KEY-DEC SECTION.
       INI-VAL-KEY-DEC.
           PERFORM VAL-IKY-DEC.
           PERFORM VAL-MKY-DEC.
           PERFORM VAL-AKY-DEC.
       FIN-VAL-KEY-DEC.
           EXIT.
       
       VAL-IKY-DEC SECTION.
       INI-VAL-IKY-DEC.
*% INCLUDE INI_VAL_IKY_DEC OF 'COLBMDEC.TXT'
           PERFORM VAL-CAN-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAN-CAI-IOPC-LEN IN DEC-FLD
                GO TO FIN-VAL-IKY-DEC.
           PERFORM VAL-CAN-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAN-IIC-IOPC-LEN IN DEC-FLD
                GO TO FIN-VAL-IKY-DEC.
           PERFORM VAL-CAN-NUM-ICAN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAN-NUM-ICAN-LEN IN DEC-FLD
                GO TO FIN-VAL-IKY-DEC.
*% INCLUDE FIN_VAL_IKY_DEC OF 'COLBMDEC.TXT'
       FIN-VAL-IKY-DEC.
           EXIT.
       
       VAL-MKY-DEC SECTION.
       INI-VAL-MKY-DEC.
*% INCLUDE INI_VAL_MKY_DEC OF 'COLBMDEC.TXT'
*% INCLUDE FIN_VAL_MKY_DEC OF 'COLBMDEC.TXT'
       FIN-VAL-MKY-DEC.
           EXIT.
       
       VAL-AKY-DEC SECTION.
       INI-VAL-AKY-DEC.
*% INCLUDE INI_VAL_AKY_DEC OF 'COLBMDEC.TXT'
*% INCLUDE FIN_VAL_AKY_DEC OF 'COLBMDEC.TXT'
       FIN-VAL-AKY-DEC.
           EXIT.
       
       VAL-CAN-CAI-IOPC SECTION.
       INI-VAL-CAN-CAI-IOPC.
*% LBL = "CAN_CAI_IOPC OF 'COLBMDEC.TXT'"
*% INCLUDE INI_CAN_CAI_IOPC OF 'COLBMDEC.TXT'
           IF CAN-CAI-IOPC IN DEC-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-CAN-CAI-IOPC.
*% INCLUDE CAN_CAI_IOPC OF 'COLBMDEC.TXT'
*% INCLUDE FIN_CAN_CAI_IOPC OF 'COLBMDEC.TXT'
       FIN-VAL-CAN-CAI-IOPC.
           EXIT.
       
       VAL-CAN-IIC-IOPC SECTION.
       INI-VAL-CAN-IIC-IOPC.
*% LBL = "CAN_IIC_IOPC OF 'COLBMDEC.TXT'"
*% INCLUDE INI_CAN_IIC_IOPC OF 'COLBMDEC.TXT'
           IF CAN-IIC-IOPC IN DEC-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-CAN-IIC-IOPC.
*% INCLUDE CAN_IIC_IOPC OF 'COLBMDEC.TXT'
*% INCLUDE FIN_CAN_IIC_IOPC OF 'COLBMDEC.TXT'
       FIN-VAL-CAN-IIC-IOPC.
           EXIT.
       
       VAL-CAN-NUM-ICAN SECTION.
       INI-VAL-CAN-NUM-ICAN.
*% LBL = "CAN_NUM_ICAN OF 'COLBMDEC.TXT'"
*% INCLUDE INI_CAN_NUM_ICAN OF 'COLBMDEC.TXT'
*% INCLUDE CAN_NUM_ICAN OF 'COLBMDEC.TXT'
*% INCLUDE FIN_CAN_NUM_ICAN OF 'COLBMDEC.TXT'
       FIN-VAL-CAN-NUM-ICAN.
           EXIT.
*% RREG = "OPC"
*% RREG_FRM = "OPC"
*% NOMB = "FCOL"
*% IZR = "S"
       
       VAL-CON-KEY-DEC SECTION.
       INI-VAL-CON-KEY-DEC.
*% INCLUDE INI_VAL_CON_KEY_DEC OF 'COLBMDEC.TXT'
           IF     CAN-CAI-IOPC IN DEC-FLD NOT > SPACES 
              AND CAN-IIC-IOPC IN DEC-FLD NOT > ZEROES 
              AND CAN-NUM-ICAN IN DEC-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-DEC.
*% INCLUDE FIN_VAL_CON_KEY_DEC OF 'COLBMDEC.TXT'
       FIN-VAL-CON-KEY-DEC.
           EXIT.
       
       VAL-CON-IKY-DEC SECTION.
       INI-VAL-CON-IKY-DEC.
*% INCLUDE INI_VAL_CON_IKY_DEC OF 'COLBMDEC.TXT'
           IF     CAN-CAI-IOPC IN DEC-FLD NOT > SPACES 
              AND CAN-IIC-IOPC IN DEC-FLD NOT > ZEROES 
              AND CAN-NUM-ICAN IN DEC-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-DEC.
*% INCLUDE FIN_VAL_CON_IKY_DEC OF 'COLBMDEC.TXT'
       FIN-VAL-CON-IKY-DEC.
           EXIT.
       
       VAL-NUM-KEY-DEC SECTION.
       INI-VAL-NUM-KEY-DEC.
