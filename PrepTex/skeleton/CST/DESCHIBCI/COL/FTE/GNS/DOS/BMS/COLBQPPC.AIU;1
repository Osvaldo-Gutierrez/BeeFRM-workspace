*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "PPC"
*% PGM_BQ = TRUE
       PUT-ALL-PPC SECTION.
       INI-PUT-ALL-PPC.
*% INCLUDE INI_PUT_ALL_PPC OF 'COLBMPPC.TXT'
           PERFORM PUT-DBC-PPC.
           PERFORM PUT-PPC-PPC.
*% INCLUDE FIN_PUT_ALL_PPC OF 'COLBMPPC.TXT'
       FIN-PUT-ALL-PPC.
           EXIT.
       
       PUT-DBC-PPC SECTION.
       INI-PUT-DBC-PPC.
*% INCLUDE INI_PUT_DBC_PPC OF 'COLBMPPC.TXT'
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-PPC.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-PPC
               GO TO FIN-PUT-DBC-PPC.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN PPC-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN PPC-FLD.
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN PPC-FLD.
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN PPC-FLD.
      * IDC-FIN --> BMCHIBCI001210.SRCAIU 
*% INCLUDE FIN_PUT_DBC_PPC OF 'COLBMPPC.TXT'
       FIN-PUT-DBC-PPC.
           EXIT.
       
       PUT-PPC-PPC SECTION.
       INI-PUT-PPC-PPC.
*% INCLUDE INI_PUT_PPC_PPC OF 'COLBMPPC.TXT'
           IF PGM-STAT-PPC-NUL
               GO TO FIN-PUT-PPC-PPC.
           IF NOT PGM-STAT-PPC-OKS
               PERFORM CLR-PPC-PPC
               GO TO FIN-PUT-PPC-PPC.
           MOVE PPC-CAI-ICLI IN PPC TO PPC-CAI-ICLI IN PPC-FLD.
           MOVE PPC-IIC-ICLI IN PPC TO PPC-IIC-ICLI IN PPC-FLD.
           MOVE PPC-COD-VCOC IN PPC TO PPC-COD-VCOC IN PPC-FLD.
           MOVE PPC-COD-TOOC IN PPC TO PPC-COD-TOOC IN PPC-FLD.
           MOVE PPC-COD-AOOC IN PPC TO PPC-COD-AOOC IN PPC-FLD.
           MOVE PPC-MSC-STAT IN PPC TO PPC-MSC-STAT IN PPC-FLD.
           MOVE PPC-COD-OFIC IN PPC TO PPC-COD-OFIC IN PPC-FLD.
           MOVE PPC-COD-EJEC IN PPC TO PPC-COD-EJEC IN PPC-FLD.
           MOVE PPC-NUM-DIRE IN PPC TO PPC-NUM-DIRE IN PPC-FLD.
           MOVE PPC-NUM-DIAP IN PPC TO PPC-NUM-DIAP IN PPC-FLD.
           MOVE PPC-NUM-CUGR IN PPC TO PPC-NUM-CUGR IN PPC-FLD.
           MOVE PPC-NUM-MES1 IN PPC TO PPC-NUM-MES1 IN PPC-FLD.
           MOVE PPC-NUM-MES2 IN PPC TO PPC-NUM-MES2 IN PPC-FLD.
           MOVE PPC-CAI-ICAB IN PPC TO PPC-CAI-ICAB IN PPC-FLD.
           MOVE PPC-IIC-ICAB IN PPC TO PPC-IIC-ICAB IN PPC-FLD.
           MOVE PPC-CAI-ICCA IN PPC TO PPC-CAI-ICCA IN PPC-FLD.
           MOVE PPC-IIC-ICCA IN PPC TO PPC-IIC-ICCA IN PPC-FLD.
           MOVE PPC-COD-TCTC IN PPC TO PPC-COD-TCTC IN PPC-FLD.
           MOVE PPC-CAI-AVAL IN PPC TO PPC-CAI-AVAL IN PPC-FLD.
           MOVE PPC-IIC-AVAL IN PPC TO PPC-IIC-AVAL IN PPC-FLD.
*% INCLUDE FIN_PUT_PPC_PPC OF 'COLBMPPC.TXT'
       FIN-PUT-PPC-PPC.
           EXIT.
       
