       VAL-IKY-PPC SECTION.
       INI-VAL-IKY-PPC.
*% INCLUDE INI_VAL_IKY_PPC OF 'COLBMPPC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * VRF-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * VRF-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DBC-IND-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-IND-ICLI-LEN IN PPC-FLD
                GO TO FIN-VAL-IKY-PPC.
           PERFORM VAL-DBC-GLS-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-GLS-ICLI-LEN IN PPC-FLD
                GO TO FIN-VAL-IKY-PPC.
           PERFORM VAL-DBC-NUM-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-NUM-ICLI-LEN IN PPC-FLD
                GO TO FIN-VAL-IKY-PPC.
           PERFORM VAL-DBC-VRF-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-VRF-ICLI-LEN IN PPC-FLD
                GO TO FIN-VAL-IKY-PPC.
      * IDC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-PPC-CAI-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PPC-CAI-ICLI-LEN IN PPC-FLD
                GO TO FIN-VAL-IKY-PPC.
           PERFORM VAL-PPC-IIC-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PPC-IIC-ICLI-LEN IN PPC-FLD
                GO TO FIN-VAL-IKY-PPC.
*% INCLUDE FIN_VAL_IKY_PPC OF 'COLBMPPC.TXT'
       FIN-VAL-IKY-PPC.
           EXIT.
       
       VAL-MKY-PPC SECTION.
       INI-VAL-MKY-PPC.
*% INCLUDE INI_VAL_MKY_PPC OF 'COLBMPPC.TXT'
*% INCLUDE FIN_VAL_MKY_PPC OF 'COLBMPPC.TXT'
       FIN-VAL-MKY-PPC.
           EXIT.
       
       VAL-AKY-PPC SECTION.
       INI-VAL-AKY-PPC.
*% INCLUDE INI_VAL_AKY_PPC OF 'COLBMPPC.TXT'
           PERFORM VAL-PPC-COD-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PPC-COD-VCOC-LEN IN PPC-FLD
                GO TO FIN-VAL-AKY-PPC.
           PERFORM VAL-PPC-COD-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PPC-COD-TOOC-LEN IN PPC-FLD
                GO TO FIN-VAL-AKY-PPC.
           PERFORM VAL-PPC-COD-AOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PPC-COD-AOOC-LEN IN PPC-FLD
                GO TO FIN-VAL-AKY-PPC.
*% INCLUDE FIN_VAL_AKY_PPC OF 'COLBMPPC.TXT'
       FIN-VAL-AKY-PPC.
           EXIT.
