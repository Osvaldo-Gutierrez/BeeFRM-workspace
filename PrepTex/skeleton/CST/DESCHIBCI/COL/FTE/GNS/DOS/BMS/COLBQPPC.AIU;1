*% INCLUDE EDT_FLD_PPC OF 'COLBMPPC.TXT'
           IF PPC-NUM-DIRE-LEN IN PPC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PPC-NUM-DIRE-LEN IN PPC-FLD
                GO TO FIN-EDT-FLD-PPC.
           MOVE PPC-NUM-DIRE IN PPC-FLD TO PPC-NUM-DIRE-EDT IN PPC-EDT.
           MOVE PPC-NUM-DIRE-EDT IN PPC-EDT TO 
                PPC-NUM-DIRE-ALF IN PPC-FLD.
           IF PPC-NUM-DIAP-LEN IN PPC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PPC-NUM-DIAP-LEN IN PPC-FLD
                GO TO FIN-EDT-FLD-PPC.
           MOVE PPC-NUM-DIAP IN PPC-FLD TO PPC-NUM-DIAP-EDT IN PPC-EDT.
           MOVE PPC-NUM-DIAP-EDT IN PPC-EDT TO 
                PPC-NUM-DIAP-ALF IN PPC-FLD.
           IF PPC-NUM-CUGR-LEN IN PPC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PPC-NUM-CUGR-LEN IN PPC-FLD
                GO TO FIN-EDT-FLD-PPC.
           MOVE PPC-NUM-CUGR IN PPC-FLD TO PPC-NUM-CUGR-EDT IN PPC-EDT.
           MOVE PPC-NUM-CUGR-EDT IN PPC-EDT TO 
                PPC-NUM-CUGR-ALF IN PPC-FLD.
           IF PPC-NUM-MES1-LEN IN PPC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PPC-NUM-MES1-LEN IN PPC-FLD
                GO TO FIN-EDT-FLD-PPC.
           MOVE PPC-NUM-MES1 IN PPC-FLD TO PPC-NUM-MES1-EDT IN PPC-EDT.
           MOVE PPC-NUM-MES1-EDT IN PPC-EDT TO 
                PPC-NUM-MES1-ALF IN PPC-FLD.
           IF PPC-NUM-MES2-LEN IN PPC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PPC-NUM-MES2-LEN IN PPC-FLD
                GO TO FIN-EDT-FLD-PPC.
           MOVE PPC-NUM-MES2 IN PPC-FLD TO PPC-NUM-MES2-EDT IN PPC-EDT.
           MOVE PPC-NUM-MES2-EDT IN PPC-EDT TO 
                PPC-NUM-MES2-ALF IN PPC-FLD.
