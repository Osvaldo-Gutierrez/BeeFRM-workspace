*% FRM = "PO2"
      *
      * COLBUPO2
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:40 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMPO2.TXT'
       SET-TAG-COLBUPO2 SECTION.
       INI-SET-TAG-COLBUPO2.
           MOVE
           '<IDNMOD NOM=$COLBUPO2$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:40 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUPO2.
           EXIT.
           
       RET-ALL-PO2 SECTION.
       INI-RET-ALL-PO2.
*% INCLUDE INI_RET_ALL_PO2 OF 'COLBMPO2.TXT'
           PERFORM KEY-ALL-PO2.
           PERFORM RET-LAP-PO2.
           PERFORM RET-POC-PO2.
*% INCLUDE FIN_RET_ALL_PO2 OF 'COLBMPO2.TXT'
       FIN-RET-ALL-PO2.
           EXIT.
       
       RET-LAP-PO2 SECTION.
       INI-RET-LAP-PO2.
*% INCLUDE INI_RET_LAP_PO2 OF 'COLBMPO2.TXT'
           MOVE LAP-COD-COME IN PO2-FLD TO LAP-COD-COME IN LAP.
*% INCLUDE FIN_RET_LAP_PO2 OF 'COLBMPO2.TXT'
       FIN-RET-LAP-PO2.
           EXIT.
       
       RET-POC-PO2 SECTION.
       INI-RET-POC-PO2.
*% INCLUDE INI_RET_POC_PO2 OF 'COLBMPO2.TXT'
           MOVE POC-MSC-STAT IN PO2-FLD TO POC-MSC-STAT IN POC.
           MOVE POC-MSC-MIPL IN PO2-FLD TO POC-MSC-MIPL IN POC.
           MOVE POC-COD-CDOC IN PO2-FLD TO POC-COD-CDOC IN POC.
           MOVE POC-SIT-CTB1 IN PO2-FLD TO POC-SIT-CTB1 IN POC.
           MOVE POC-SIT-CTB2 IN PO2-FLD TO POC-SIT-CTB2 IN POC.
           MOVE POC-SIT-CTB3 IN PO2-FLD TO POC-SIT-CTB3 IN POC.
           MOVE POC-SIT-CRT1 IN PO2-FLD TO POC-SIT-CRT1 IN POC.
           MOVE POC-SIT-CRT2 IN PO2-FLD TO POC-SIT-CRT2 IN POC.
           MOVE POC-SIT-CRT3 IN PO2-FLD TO POC-SIT-CRT3 IN POC.
           MOVE POC-IND-GARA IN PO2-FLD TO POC-IND-GARA IN POC.
           MOVE POC-SIT-CBR1 IN PO2-FLD TO POC-SIT-CBR1 IN POC.
           MOVE POC-SIT-CBR2 IN PO2-FLD TO POC-SIT-CBR2 IN POC.
           MOVE POC-IND-GNTR IN PO2-FLD TO POC-IND-GNTR IN POC.
           MOVE POC-SIT-CBR3 IN PO2-FLD TO POC-SIT-CBR3 IN POC.
           MOVE POC-MSC-TCVE IN PO2-FLD TO POC-MSC-TCVE IN POC.
           MOVE POC-IND-AFER IN PO2-FLD TO POC-IND-AFER IN POC.
           MOVE POC-MSC-MCVF IN PO2-FLD TO POC-MSC-MCVF IN POC.
           MOVE POC-COD-ACTI IN PO2-FLD TO POC-COD-ACTI IN POC.
           MOVE POC-IND-CAUT IN PO2-FLD TO POC-IND-CAUT IN POC.
           MOVE POC-COD-COTO IN PO2-FLD TO POC-COD-COTO IN POC.
           MOVE POC-IND-TABO IN PO2-FLD TO POC-IND-TABO IN POC.
           MOVE POC-NUM-MINC IN PO2-FLD TO POC-NUM-MINC IN POC.
           MOVE POC-NUM-MINI IN PO2-FLD TO POC-NUM-MINI IN POC.
           MOVE POC-IND-MINC IN PO2-FLD TO POC-IND-MINC IN POC.
           MOVE POC-IND-MINI IN PO2-FLD TO POC-IND-MINI IN POC.
           MOVE POC-NUM-MAXC IN PO2-FLD TO POC-NUM-MAXC IN POC.
           MOVE POC-NUM-MAXI IN PO2-FLD TO POC-NUM-MAXI IN POC.
           MOVE POC-IND-MAXC IN PO2-FLD TO POC-IND-MAXC IN POC.
           MOVE POC-IND-MAXI IN PO2-FLD TO POC-IND-MAXI IN POC.
           MOVE POC-IND-LIQI IN PO2-FLD TO POC-IND-LIQI IN POC.
           MOVE POC-IND-ECDC IN PO2-FLD TO POC-IND-ECDC IN POC.
           MOVE POC-COD-IPVC IN PO2-FLD TO POC-COD-IPVC IN POC.
           MOVE POC-COD-TPAG IN PO2-FLD TO POC-COD-TPAG IN POC.
*% INCLUDE FIN_RET_POC_PO2 OF 'COLBMPO2.TXT'
       FIN-RET-POC-PO2.
           EXIT.
       
       KEY-ALL-PO2 SECTION.
       INI-KEY-ALL-PO2.
*% INCLUDE INI_KEY_ALL_PO2 OF 'COLBMPO2.TXT'
           PERFORM KEY-LAP-PO2.
           PERFORM KEY-POC-PO2.
*% INCLUDE FIN_KEY_ALL_PO2 OF 'COLBMPO2.TXT'
       FIN-KEY-ALL-PO2.
           EXIT.
       
       KEY-LAP-PO2 SECTION.
       INI-KEY-LAP-PO2.
*% INCLUDE INI_KEY_LAP_PO2 OF 'COLBMPO2.TXT'
*% INCLUDE FIN_KEY_LAP_PO2 OF 'COLBMPO2.TXT'
       FIN-KEY-LAP-PO2.
           EXIT.
       
       KEY-POC-PO2 SECTION.
       INI-KEY-POC-PO2.
*% INCLUDE INI_KEY_POC_PO2 OF 'COLBMPO2.TXT'
*% INCLUDE FIN_KEY_POC_PO2 OF 'COLBMPO2.TXT'
       FIN-KEY-POC-PO2.
           EXIT.
       
       SCR-CLRA-PO2 SECTION.
       INI-SCR-CLRA-PO2.
*% INCLUDE INI_SCR_CLRA_PO2 OF 'COLBMPO2.TXT'
           PERFORM CLR-KEY-PO2.
           PERFORM CLR-FLD-PO2.
*% INCLUDE FIN_SCR_CLRA_PO2 OF 'COLBMPO2.TXT'
       FIN-SCR-CLRA-PO2.
           EXIT.
       
       CLR-KEY-PO2 SECTION.
       INI-CLR-KEY-PO2.
*% INCLUDE INI_CLR_KEY_PO2 OF 'COLBMPO2.TXT'
*% INCLUDE FIN_CLR_KEY_PO2 OF 'COLBMPO2.TXT'
       FIN-CLR-KEY-PO2.
           EXIT.
       
       CLR-FLD-PO2 SECTION.
       INI-CLR-FLD-PO2.
*% INCLUDE INI_CLR_FLD_PO2 OF 'COLBMPO2.TXT'
           MOVE SPACES TO FRM-GLS-MIPL IN PO2-FLD.
           MOVE SPACES TO FRM-GLS-CDOC IN PO2-FLD.
           MOVE SPACES TO FRM-GLS-GARA IN PO2-FLD.
           MOVE SPACES TO FRM-GLS-TCVE IN PO2-FLD.
           MOVE SPACES TO FRM-GLS-MCVF IN PO2-FLD.
           MOVE SPACES TO FRM-GLS-ACTI IN PO2-FLD.
           MOVE SPACES TO FRM-GLS-COTO IN PO2-FLD.
           MOVE SPACES TO FRM-GLS-TABO IN PO2-FLD.
           MOVE SPACES TO FRM-GLS-MINC IN PO2-FLD.
           MOVE SPACES TO FRM-GLS-MINI IN PO2-FLD.
           MOVE SPACES TO FRM-GLS-MAXC IN PO2-FLD.
           MOVE SPACES TO FRM-GLS-MAXI IN PO2-FLD.
           MOVE SPACES TO FRM-GLS-TPAG IN PO2-FLD.
           PERFORM CLR-LAP-PO2.
           PERFORM CLR-POC-PO2.
*% INCLUDE FIN_CLR_FLD_PO2 OF 'COLBMPO2.TXT'
       FIN-CLR-FLD-PO2.
           EXIT.
       
       CLR-LAP-PO2 SECTION.
       INI-CLR-LAP-PO2.
*% INCLUDE INI_CLR_LAP_PO2 OF 'COLBMPO2.TXT'
           MOVE SPACES TO LAP-COD-COME IN PO2-FLD.
*% INCLUDE FIN_CLR_LAP_PO2 OF 'COLBMPO2.TXT'
       FIN-CLR-LAP-PO2.
           EXIT.
       
       CLR-POC-PO2 SECTION.
       INI-CLR-POC-PO2.
*% INCLUDE INI_CLR_POC_PO2 OF 'COLBMPO2.TXT'
           MOVE SPACES TO POC-MSC-STAT IN PO2-FLD.
           MOVE SPACES TO POC-MSC-MIPL IN PO2-FLD.
           MOVE SPACES TO POC-COD-CDOC IN PO2-FLD.
           MOVE SPACES TO POC-SIT-CTB1 IN PO2-FLD.
           MOVE SPACES TO POC-SIT-CTB2 IN PO2-FLD.
           MOVE SPACES TO POC-SIT-CTB3 IN PO2-FLD.
           MOVE SPACES TO POC-SIT-CRT1 IN PO2-FLD.
           MOVE SPACES TO POC-SIT-CRT2 IN PO2-FLD.
           MOVE SPACES TO POC-SIT-CRT3 IN PO2-FLD.
           MOVE SPACES TO POC-IND-GARA IN PO2-FLD.
           MOVE SPACES TO POC-SIT-CBR1 IN PO2-FLD.
           MOVE SPACES TO POC-SIT-CBR2 IN PO2-FLD.
           MOVE SPACES TO POC-IND-GNTR IN PO2-FLD.
           MOVE SPACES TO POC-SIT-CBR3 IN PO2-FLD.
           MOVE SPACES TO POC-MSC-TCVE IN PO2-FLD.
           MOVE SPACES TO POC-IND-AFER IN PO2-FLD.
           MOVE SPACES TO POC-MSC-MCVF IN PO2-FLD.
           MOVE SPACES TO POC-COD-ACTI IN PO2-FLD.
           MOVE SPACES TO POC-IND-CAUT IN PO2-FLD.
           MOVE SPACES TO POC-COD-COTO IN PO2-FLD.
           MOVE SPACES TO POC-IND-TABO IN PO2-FLD.
           MOVE ZEROES TO POC-NUM-MINC IN PO2-FLD.
           MOVE ZEROES TO POC-NUM-MINI IN PO2-FLD.
           MOVE SPACES TO POC-IND-MINC IN PO2-FLD.
           MOVE SPACES TO POC-IND-MINI IN PO2-FLD.
           MOVE ZEROES TO POC-NUM-MAXC IN PO2-FLD.
           MOVE ZEROES TO POC-NUM-MAXI IN PO2-FLD.
           MOVE SPACES TO POC-IND-MAXC IN PO2-FLD.
           MOVE SPACES TO POC-IND-MAXI IN PO2-FLD.
           MOVE SPACES TO POC-IND-LIQI IN PO2-FLD.
           MOVE SPACES TO POC-IND-ECDC IN PO2-FLD.
           MOVE SPACES TO POC-COD-IPVC IN PO2-FLD.
           MOVE SPACES TO POC-COD-TPAG IN PO2-FLD.
*% INCLUDE FIN_CLR_POC_PO2 OF 'COLBMPO2.TXT'
       FIN-CLR-POC-PO2.
           EXIT.
       
       FST-KEY-PO2 SECTION.
       INI-FST-KEY-PO2.
*% INCLUDE INI_FST_KEY_PO2 OF 'COLBMPO2.TXT'
*% INCLUDE FIN_FST_KEY_PO2 OF 'COLBMPO2.TXT'
       FIN-FST-KEY-PO2.
           EXIT.
       
       FST-FLD-PO2 SECTION.
       INI-FST-FLD-PO2.
*% INCLUDE INI_FST_FLD_PO2 OF 'COLBMPO2.TXT'
           MOVE -1 TO LAP-COD-COME-LEN IN PO2-FLD.
*% INCLUDE FIN_FST_FLD_PO2 OF 'COLBMPO2.TXT'
       FIN-FST-FLD-PO2.
           EXIT.
       
       PRO-KEY-PO2 SECTION.
       INI-PRO-KEY-PO2.
*% INCLUDE INI_PRO_KEY_PO2 OF 'COLBMPO2.TXT'
           PERFORM PRO-MKY-PO2.
*% INCLUDE FIN_PRO_KEY_PO2 OF 'COLBMPO2.TXT'
       FIN-PRO-KEY-PO2.
           EXIT.
       
       PRO-MKY-PO2 SECTION.
       INI-PRO-MKY-PO2.
*% INCLUDE INI_PRO_MKY_PO2 OF 'COLBMPO2.TXT'
*% INCLUDE FIN_PRO_MKY_PO2 OF 'COLBMPO2.TXT'
       FIN-PRO-MKY-PO2.
           EXIT.
       
       PRO-FLD-PO2 SECTION.
       INI-PRO-FLD-PO2.
*% INCLUDE INI_PRO_FLD_PO2 OF 'COLBMPO2.TXT'
           PERFORM PRO-LAP-PO2.
           PERFORM PRO-POC-PO2.
*% INCLUDE FIN_PRO_FLD_PO2 OF 'COLBMPO2.TXT'
       FIN-PRO-FLD-PO2.
           EXIT.
       
       PRO-LAP-PO2 SECTION.
       INI-PRO-LAP-PO2.
*% INCLUDE INI_PRO_LAP_PO2 OF 'COLBMPO2.TXT'
           MOVE FRM-CPIM-AEY TO LAP-COD-COME-ATR IN PO2-FLD.
*% INCLUDE FIN_PRO_LAP_PO2 OF 'COLBMPO2.TXT'
       FIN-PRO-LAP-PO2.
           EXIT.
       
       PRO-POC-PO2 SECTION.
       INI-PRO-POC-PO2.
*% INCLUDE INI_PRO_POC_PO2 OF 'COLBMPO2.TXT'
           MOVE FRM-CPIM-AEY TO POC-MSC-STAT-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-MSC-MIPL-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-COD-CDOC-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-SIT-CTB1-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-SIT-CTB2-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-SIT-CTB3-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-SIT-CRT1-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-SIT-CRT2-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-SIT-CRT3-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-IND-GARA-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-SIT-CBR1-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-SIT-CBR2-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-IND-GNTR-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-SIT-CBR3-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-MSC-TCVE-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-IND-AFER-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-MSC-MCVF-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-COD-ACTI-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-IND-CAUT-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-COD-COTO-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-IND-TABO-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-NUM-MINC-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-NUM-MINI-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-IND-MINC-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-IND-MINI-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-NUM-MAXC-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-NUM-MAXI-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-IND-MAXC-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-IND-MAXI-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-IND-LIQI-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-IND-ECDC-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-COD-IPVC-ATR IN PO2-FLD.
           MOVE FRM-CPIM-AEY TO POC-COD-TPAG-ATR IN PO2-FLD.
*% INCLUDE FIN_PRO_POC_PO2 OF 'COLBMPO2.TXT'
       FIN-PRO-POC-PO2.
           EXIT.
       
       UNP-KEY-PO2 SECTION.
       INI-UNP-KEY-PO2.
*% INCLUDE INI_UNP_KEY_PO2 OF 'COLBMPO2.TXT'
           PERFORM UNP-IKY-PO2.
           PERFORM UNP-AKY-PO2.
           PERFORM UNP-MKY-PO2.
*% INCLUDE FIN_UNP_KEY_PO2 OF 'COLBMPO2.TXT'
       FIN-UNP-KEY-PO2.
           EXIT.
       
       UNP-IKY-PO2 SECTION.
       INI-UNP-IKY-PO2.
*% INCLUDE INI_UNP_IKY_PO2 OF 'COLBMPO2.TXT'
*% INCLUDE FIN_UNP_IKY_PO2 OF 'COLBMPO2.TXT'
