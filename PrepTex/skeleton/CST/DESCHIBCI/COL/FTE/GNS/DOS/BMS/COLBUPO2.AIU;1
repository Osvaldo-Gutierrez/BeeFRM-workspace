*% INCLUDE FIN_EDT_AKY_PO2 OF 'COLBMPO2.TXT'
       FIN-EDT-AKY-PO2.
           EXIT.
       
       EDT-FLD-PO2 SECTION.
       INI-EDT-FLD-PO2.
*% INCLUDE EDT_FLD_PO2 OF 'COLBMPO2.TXT'
           IF POC-NUM-MINC-LEN IN PO2-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO POC-NUM-MINC-LEN IN PO2-FLD
                GO TO FIN-EDT-FLD-PO2.
           MOVE POC-NUM-MINC IN PO2-FLD TO POC-NUM-MINC-EDT IN PO2-EDT.
           MOVE POC-NUM-MINC-EDT IN PO2-EDT TO 
                POC-NUM-MINC-ALF IN PO2-FLD.
           IF POC-NUM-MINI-LEN IN PO2-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO POC-NUM-MINI-LEN IN PO2-FLD
                GO TO FIN-EDT-FLD-PO2.
           MOVE POC-NUM-MINI IN PO2-FLD TO POC-NUM-MINI-EDT IN PO2-EDT.
           MOVE POC-NUM-MINI-EDT IN PO2-EDT TO 
                POC-NUM-MINI-ALF IN PO2-FLD.
           IF POC-NUM-MAXC-LEN IN PO2-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO POC-NUM-MAXC-LEN IN PO2-FLD
                GO TO FIN-EDT-FLD-PO2.
           MOVE POC-NUM-MAXC IN PO2-FLD TO POC-NUM-MAXC-EDT IN PO2-EDT.
           MOVE POC-NUM-MAXC-EDT IN PO2-EDT TO 
                POC-NUM-MAXC-ALF IN PO2-FLD.
           IF POC-NUM-MAXI-LEN IN PO2-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO POC-NUM-MAXI-LEN IN PO2-FLD
                GO TO FIN-EDT-FLD-PO2.
           MOVE POC-NUM-MAXI IN PO2-FLD TO POC-NUM-MAXI-EDT IN PO2-EDT.
           MOVE POC-NUM-MAXI-EDT IN PO2-EDT TO 
                POC-NUM-MAXI-ALF IN PO2-FLD.
*% INCLUDE FIN_EDT_FLD_PO2 OF 'COLBMPO2.TXT'
       FIN-EDT-FLD-PO2.
           EXIT.
*% INCLUDE EOF_BU OF 'COLBMPO2.TXT'
*% INCLUDE EOF OF 'COLBMPO2.TXT'
*% INCLUDE EOF_IBM OF 'COLBMPO2.TXT'
