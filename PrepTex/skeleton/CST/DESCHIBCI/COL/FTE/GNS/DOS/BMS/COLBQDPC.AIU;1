*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-DPC.
       VAL-NUM-FRM-VAL-MACU.
*% BUF = "DPC-FLD"
*% FLD = "FRM-VAL-MACU"
*% ENT = "13"
*% DCM = "4"
