      *
      * COLBQDPC
      * Version 3.0.1 Nov-2005
      * 01-Apr-10 02:16 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "DPC"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMDPC.TXT'
       SET-TAG-COLBQDPC SECTION.
       INI-SET-TAG-COLBQDPC.
           MOVE
           '<IDNMOD NOM=$COLBQDPC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$01-Apr-10 02:16 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQDPC.
           EXIT.
           
       PUT-ALL-DPC SECTION.
       INI-PUT-ALL-DPC.
*% INCLUDE INI_PUT_ALL_DPC OF 'COLBMDPC.TXT'
           PERFORM PUT-DPC-DPC.
*% INCLUDE FIN_PUT_ALL_DPC OF 'COLBMDPC.TXT'
       FIN-PUT-ALL-DPC.
           EXIT.
       
       PUT-DPC-DPC SECTION.
       INI-PUT-DPC-DPC.
*% INCLUDE INI_PUT_DPC_DPC OF 'COLBMDPC.TXT'
           IF PGM-STAT-DPC-NUL
               GO TO FIN-PUT-DPC-DPC.
           IF NOT PGM-STAT-DPC-OKS
               PERFORM CLR-DPC-DPC
               GO TO FIN-PUT-DPC-DPC.
           MOVE DPC-TIP-REGI IN DPC TO DPC-TIP-REGI IN DPC-FLD.
           MOVE DPC-COD-CODI IN DPC TO DPC-COD-CODI IN DPC-FLD.
*% INCLUDE FIN_PUT_DPC_DPC OF 'COLBMDPC.TXT'
       FIN-PUT-DPC-DPC.
           EXIT.
       
       KEY-ALL-DPC SECTION.
       INI-KEY-ALL-DPC.
*% INCLUDE INI_KEY_ALL_DPC OF 'COLBMDPC.TXT'
           PERFORM KEY-DPC-DPC.
*% INCLUDE FIN_KEY_ALL_DPC OF 'COLBMDPC.TXT'
       FIN-KEY-ALL-DPC.
           EXIT.
       
       KEY-DPC-DPC SECTION.
       INI-KEY-DPC-DPC.
*% INCLUDE INI_KEY_DPC_DPC OF 'COLBMDPC.TXT'
           MOVE DPC-TIP-REGI IN DPC-FLD TO DPC-TIP-REGI IN DPC.
           MOVE DPC-COD-CODI IN DPC-FLD TO DPC-COD-CODI IN DPC.
*% INCLUDE FIN_KEY_DPC_DPC OF 'COLBMDPC.TXT'
       FIN-KEY-DPC-DPC.
           EXIT.
       
       SCR-CLRA-DPC SECTION.
       INI-SCR-CLRA-DPC.
*% INCLUDE INI_SCR_CLRA_DPC OF 'COLBMDPC.TXT'
           PERFORM CLR-KEY-DPC.
           PERFORM CLR-FLD-DPC.
*% INCLUDE FIN_SCR_CLRA_DPC OF 'COLBMDPC.TXT'
       FIN-SCR-CLRA-DPC.
           EXIT.
       
       CLR-KEY-DPC SECTION.
       INI-CLR-KEY-DPC.
*% INCLUDE INI_CLR_KEY_DPC OF 'COLBMDPC.TXT'
           MOVE SPACES TO DPC-TIP-REGI IN DPC-FLD.
           MOVE SPACES TO DPC-COD-CODI IN DPC-FLD.
*% INCLUDE FIN_CLR_KEY_DPC OF 'COLBMDPC.TXT'
       FIN-CLR-KEY-DPC.
           EXIT.
       
       CLR-FLD-DPC SECTION.
       INI-CLR-FLD-DPC.
*% INCLUDE INI_CLR_FLD_DPC OF 'COLBMDPC.TXT'
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPRO"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FPRO IN DPC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-FOI2 IN DPC-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FINI"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FINI IN DPC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-FOF2 IN DPC-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FTERM"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FTERM IN DPC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-VAL-SDAN IN DPC-FLD.
           MOVE ZEROES TO FRM-VAL-SHAN IN DPC-FLD.
           MOVE ZEROES TO FRM-VAL-MDPE IN DPC-FLD.
           MOVE ZEROES TO FRM-VAL-MHPE IN DPC-FLD.
           MOVE ZEROES TO FRM-VAL-SDNV IN DPC-FLD.
           MOVE ZEROES TO FRM-VAL-SHNV IN DPC-FLD.
           MOVE ZEROES TO FRM-VAL-MACU IN DPC-FLD.
           MOVE ZEROES TO FRM-NUM-RACU IN DPC-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FELI"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FELI IN DPC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-VAL-MELI IN DPC-FLD.
           MOVE ZEROES TO FRM-NUM-RELI IN DPC-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPR4"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FPR4 IN DPC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-FOI5 IN DPC-FLD.
           MOVE ZEROES TO FRM-NUM-FOF5 IN DPC-FLD.
           PERFORM CLR-DPC-DPC.
*% INCLUDE FIN_CLR_FLD_DPC OF 'COLBMDPC.TXT'
       FIN-CLR-FLD-DPC.
           EXIT.
       
       CLR-DPC-DPC SECTION.
       INI-CLR-DPC-DPC.
*% INCLUDE INI_CLR_DPC_DPC OF 'COLBMDPC.TXT'
*% INCLUDE FIN_CLR_DPC_DPC OF 'COLBMDPC.TXT'
       FIN-CLR-DPC-DPC.
           EXIT.
       
       FST-KEY-DPC SECTION.
       INI-FST-KEY-DPC.
*% INCLUDE INI_FST_KEY_DPC OF 'COLBMDPC.TXT'
           MOVE -1 TO DPC-TIP-REGI-LEN IN DPC-FLD.
*% INCLUDE FIN_FST_KEY_DPC OF 'COLBMDPC.TXT'
       FIN-FST-KEY-DPC.
           EXIT.
       
       FST-FLD-DPC SECTION.
       INI-FST-FLD-DPC.
*% INCLUDE INI_FST_FLD_DPC OF 'COLBMDPC.TXT'
*% INCLUDE FIN_FST_FLD_DPC OF 'COLBMDPC.TXT'
       FIN-FST-FLD-DPC.
           EXIT.
       
       PRO-KEY-DPC SECTION.
       INI-PRO-KEY-DPC.
*% INCLUDE INI_PRO_KEY_DPC OF 'COLBMDPC.TXT'
           PERFORM PRO-IKY-DPC.
           PERFORM PRO-AKY-DPC.
           PERFORM PRO-MKY-DPC.
*% INCLUDE FIN_PRO_KEY_DPC OF 'COLBMDPC.TXT'
       FIN-PRO-KEY-DPC.
           EXIT.
       
       PRO-IKY-DPC SECTION.
       INI-PRO-IKY-DPC.
*% INCLUDE INI_PRO_IKY_DPC OF 'COLBMDPC.TXT'
           MOVE FRM-CPIM-AEY TO DPC-TIP-REGI-ATR IN DPC-FLD.
           MOVE FRM-CPIM-AEY TO DPC-COD-CODI-ATR IN DPC-FLD.
*% INCLUDE FIN_PRO_IKY_DPC OF 'COLBMDPC.TXT'
       FIN-PRO-IKY-DPC.
           EXIT.
       
       PRO-MKY-DPC SECTION.
       INI-PRO-MKY-DPC.
*% INCLUDE INI_PRO_MKY_DPC OF 'COLBMDPC.TXT'
*% INCLUDE FIN_PRO_MKY_DPC OF 'COLBMDPC.TXT'
       FIN-PRO-MKY-DPC.
           EXIT.
       
       PRO-AKY-DPC SECTION.
       INI-PRO-AKY-DPC.
*% INCLUDE INI_PRO_AKY_DPC OF 'COLBMDPC.TXT'
*% INCLUDE FIN_PRO_AKY_DPC OF 'COLBMDPC.TXT'
       FIN-PRO-AKY-DPC.
           EXIT.
       
       UNP-KEY-DPC SECTION.
       INI-UNP-KEY-DPC.
*% INCLUDE INI_UNP_KEY_DPC OF 'COLBMDPC.TXT'
           PERFORM UNP-IKY-DPC.
           PERFORM UNP-AKY-DPC.
           PERFORM UNP-MKY-DPC.
*% INCLUDE FIN_UNP_KEY_DPC OF 'COLBMDPC.TXT'
       FIN-UNP-KEY-DPC.
           EXIT.
       
       UNP-IKY-DPC SECTION.
       INI-UNP-IKY-DPC.
*% INCLUDE INI_UNP_IKY_DPC OF 'COLBMDPC.TXT'
           MOVE FRM-CPIM-UBY TO DPC-TIP-REGI-ATR.
           MOVE FRM-CPIM-UBY TO DPC-COD-CODI-ATR.
*% INCLUDE FIN_UNP_IKY_DPC OF 'COLBMDPC.TXT'
       FIN-UNP-IKY-DPC.
           EXIT.
       
       UNP-MKY-DPC SECTION.
       INI-UNP-MKY-DPC.
*% INCLUDE INI_UNP_MKY_DPC OF 'COLBMDPC.TXT'
*% INCLUDE FIN_UNP_MKY_DPC OF 'COLBMDPC.TXT'
       FIN-UNP-MKY-DPC.
           EXIT.
       
       UNP-AKY-DPC SECTION.
       INI-UNP-AKY-DPC.
*% INCLUDE INI_UNP_AKY_DPC OF 'COLBMDPC.TXT'
*% INCLUDE FIN_UNP_AKY_DPC OF 'COLBMDPC.TXT'
       FIN-UNP-AKY-DPC.
           EXIT.
       
       UNP-FLD-DPC SECTION.
       INI-UNP-FLD-DPC.
*% INCLUDE INI_UNP_FLD_DPC OF 'COLBMDPC.TXT'
           PERFORM UNP-DPC-DPC.
*% INCLUDE FIN_UNP_FLD_DPC OF 'COLBMDPC.TXT'
       FIN-UNP-FLD-DPC.
           EXIT.
       
       UNP-DPC-DPC SECTION.
       INI-UNP-DPC-DPC.
*% INCLUDE INI_UNP_DPC_DPC OF 'COLBMDPC.TXT'
*% INCLUDE FIN_UNP_DPC_DPC OF 'COLBMDPC.TXT'
       FIN-UNP-DPC-DPC.
           EXIT.
       
       VAL-KEY-DPC SECTION.
       INI-VAL-KEY-DPC.
           PERFORM VAL-IKY-DPC.
           PERFORM VAL-MKY-DPC.
           PERFORM VAL-AKY-DPC.
       FIN-VAL-KEY-DPC.
           EXIT.
       
       VAL-IKY-DPC SECTION.
       INI-VAL-IKY-DPC.
*% INCLUDE INI_VAL_IKY_DPC OF 'COLBMDPC.TXT'
           PERFORM VAL-DPC-TIP-REGI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DPC-TIP-REGI-LEN IN DPC-FLD
                GO TO FIN-VAL-IKY-DPC.
           PERFORM VAL-DPC-COD-CODI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DPC-COD-CODI-LEN IN DPC-FLD
                GO TO FIN-VAL-IKY-DPC.
*% INCLUDE FIN_VAL_IKY_DPC OF 'COLBMDPC.TXT'
       FIN-VAL-IKY-DPC.
           EXIT.
       
       VAL-MKY-DPC SECTION.
       INI-VAL-MKY-DPC.
*% INCLUDE INI_VAL_MKY_DPC OF 'COLBMDPC.TXT'
*% INCLUDE FIN_VAL_MKY_DPC OF 'COLBMDPC.TXT'
       FIN-VAL-MKY-DPC.
           EXIT.
       
       VAL-AKY-DPC SECTION.
       INI-VAL-AKY-DPC.
*% INCLUDE INI_VAL_AKY_DPC OF 'COLBMDPC.TXT'
*% INCLUDE FIN_VAL_AKY_DPC OF 'COLBMDPC.TXT'
       FIN-VAL-AKY-DPC.
           EXIT.
       
       VAL-DPC-TIP-REGI SECTION.
       INI-VAL-DPC-TIP-REGI.
*% LBL = "DPC_TIP_REGI OF 'COLBMDPC.TXT'"
*% INCLUDE INI_DPC_TIP_REGI OF 'COLBMDPC.TXT'
           IF DPC-TIP-REGI IN DPC-FLD NOT > SPACES
              MOVE SPACES TO DPC-TIP-REGI IN DPC-FLD.
*% INCLUDE DPC_TIP_REGI OF 'COLBMDPC.TXT'
*% INCLUDE FIN_DPC_TIP_REGI OF 'COLBMDPC.TXT'
       FIN-VAL-DPC-TIP-REGI.
           EXIT.
       
       VAL-DPC-COD-CODI SECTION.
       INI-VAL-DPC-COD-CODI.
*% LBL = "DPC_COD_CODI OF 'COLBMDPC.TXT'"
*% INCLUDE INI_DPC_COD_CODI OF 'COLBMDPC.TXT'
           IF DPC-COD-CODI IN DPC-FLD NOT > SPACES
              MOVE SPACES TO DPC-COD-CODI IN DPC-FLD.
*% INCLUDE DPC_COD_CODI OF 'COLBMDPC.TXT'
*% INCLUDE FIN_DPC_COD_CODI OF 'COLBMDPC.TXT'
       FIN-VAL-DPC-COD-CODI.
           EXIT.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPRO"
*% IZR = "S"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FINI"
*% IZR = "S"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FTERM"
*% IZR = "S"
*% RREG = "FRM"
