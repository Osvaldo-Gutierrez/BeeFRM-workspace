*% IZR = "S"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPR4"
*% IZR = "S"
       
       VAL-CON-KEY-DPC SECTION.
       INI-VAL-CON-KEY-DPC.
*% INCLUDE INI_VAL_CON_KEY_DPC OF 'COLBMDPC.TXT'
           IF     DPC-TIP-REGI IN DPC-FLD NOT > SPACES 
              AND DPC-COD-CODI IN DPC-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-DPC.
*% INCLUDE FIN_VAL_CON_KEY_DPC OF 'COLBMDPC.TXT'
       FIN-VAL-CON-KEY-DPC.
           EXIT.
       
       VAL-CON-IKY-DPC SECTION.
       INI-VAL-CON-IKY-DPC.
*% INCLUDE INI_VAL_CON_IKY_DPC OF 'COLBMDPC.TXT'
           IF     DPC-TIP-REGI IN DPC-FLD NOT > SPACES 
              AND DPC-COD-CODI IN DPC-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-DPC.
*% INCLUDE FIN_VAL_CON_IKY_DPC OF 'COLBMDPC.TXT'
       FIN-VAL-CON-IKY-DPC.
           EXIT.
       
       VAL-NUM-KEY-DPC SECTION.
       INI-VAL-NUM-KEY-DPC.
           PERFORM VAL-NUM-IKY-DPC.
           PERFORM VAL-NUM-MKY-DPC.
           PERFORM VAL-NUM-AKY-DPC.
       FIN-VAL-NUM-KEY-DPC.
           EXIT.
       
       VAL-NUM-IKY-DPC SECTION.
       INI-VAL-NUM-IKY-DPC.
*% INCLUDE INI_VAL_NUM_IKY_DPC OF 'COLBMDPC.TXT'
*% INCLUDE FIN_VAL_NUM_IKY_DPC OF 'COLBMDPC.TXT'
       FIN-VAL-NUM-IKY-DPC.
           EXIT.
       
       VAL-NUM-MKY-DPC SECTION.
       INI-VAL-NUM-MKY-DPC.
*% INCLUDE INI_VAL_NUM_MKY_DPC OF 'COLBMDPC.TXT'
*% INCLUDE FIN_VAL_NUM_MKY_DPC OF 'COLBMDPC.TXT'
       FIN-VAL-NUM-MKY-DPC.
           EXIT.
       
       VAL-NUM-AKY-DPC SECTION.
       INI-VAL-NUM-AKY-DPC.
*% INCLUDE INI_VAL_NUM_AKY_DPC OF 'COLBMDPC.TXT'
*% INCLUDE FIN_VAL_NUM_AKY_DPC OF 'COLBMDPC.TXT'
       FIN-VAL-NUM-AKY-DPC.
           EXIT.
       
       VAL-NUM-FLD-DPC SECTION.
       INI-VAL-NUM-FLD-DPC.
*% INCLUDE INI_VAL_NUM_FLD_DPC OF 'COLBMDPC.TXT'
       VAL-NUM-FRM-NUM-FOI2.
*% BUF = "DPC-FLD"
*% FLD = "FRM-NUM-FOI2"
*% ENT = "6"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-DPC.
       VAL-NUM-FRM-NUM-FOF2.
*% BUF = "DPC-FLD"
*% FLD = "FRM-NUM-FOF2"
*% ENT = "6"
