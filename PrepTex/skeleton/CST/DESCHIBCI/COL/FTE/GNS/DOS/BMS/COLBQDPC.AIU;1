*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-DPC.
       VAL-NUM-FRM-VAL-SDAN.
*% BUF = "DPC-FLD"
*% FLD = "FRM-VAL-SDAN"
*% ENT = "10"
*% DCM = "4"
*% NMR = "NS9V"
*% ISG = "S"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-DPC.
       VAL-NUM-FRM-VAL-SHAN.
*% BUF = "DPC-FLD"
*% FLD = "FRM-VAL-SHAN"
*% ENT = "10"
