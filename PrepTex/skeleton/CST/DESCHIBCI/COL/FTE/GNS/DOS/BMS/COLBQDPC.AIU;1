       EDT-KEY-DPC SECTION.
       INI-EDT-KEY-DPC.
           PERFORM EDT-IKY-DPC.
           PERFORM EDT-MKY-DPC.
           PERFORM EDT-AKY-DPC.
*% INCLUDE EDT_KEY_DPC OF 'COLBMDPC.TXT'
       FIN-EDT-KEY-DPC.
           EXIT.
       
       EDT-IKY-DPC SECTION.
       INI-EDT-IKY-DPC.
*% INCLUDE EDT_IKY_DPC OF 'COLBMDPC.TXT'
*% INCLUDE FIN_EDT_IKY_DPC OF 'COLBMDPC.TXT'
       FIN-EDT-IKY-DPC.
           EXIT.
       
       EDT-MKY-DPC SECTION.
       INI-EDT-MKY-DPC.
*% INCLUDE EDT_MKY_DPC OF 'COLBMDPC.TXT'
*% INCLUDE FIN_EDT_MKY_DPC OF 'COLBMDPC.TXT'
       FIN-EDT-MKY-DPC.
           EXIT.
       
       EDT-AKY-DPC SECTION.
       INI-EDT-AKY-DPC.
*% INCLUDE EDT_AKY_DPC OF 'COLBMDPC.TXT'
*% INCLUDE FIN_EDT_AKY_DPC OF 'COLBMDPC.TXT'
       FIN-EDT-AKY-DPC.
           EXIT.
       
       EDT-FLD-DPC SECTION.
       INI-EDT-FLD-DPC.
*% INCLUDE EDT_FLD_DPC OF 'COLBMDPC.TXT'
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FPRO"
           IF FRM-NUM-FOI2-LEN IN DPC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-FOI2-LEN IN DPC-FLD
                GO TO FIN-EDT-FLD-DPC.
           MOVE FRM-NUM-FOI2 IN DPC-FLD TO FRM-NUM-FOI2-EDT IN DPC-EDT.
           MOVE FRM-NUM-FOI2-EDT IN DPC-EDT TO 
                FRM-NUM-FOI2-ALF IN DPC-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FINI"
           IF FRM-NUM-FOF2-LEN IN DPC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-FOF2-LEN IN DPC-FLD
                GO TO FIN-EDT-FLD-DPC.
           MOVE FRM-NUM-FOF2 IN DPC-FLD TO FRM-NUM-FOF2-EDT IN DPC-EDT.
           MOVE FRM-NUM-FOF2-EDT IN DPC-EDT TO 
                FRM-NUM-FOF2-ALF IN DPC-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FTERM"
