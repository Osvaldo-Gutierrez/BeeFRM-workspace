       FIN-EDT-KEY-PO1.
           EXIT.
       
       EDT-IKY-PO1 SECTION.
       INI-EDT-IKY-PO1.
*% INCLUDE EDT_IKY_PO1 OF 'COLBMPO1.TXT'
           IF POC-NUM-IPOC-LEN IN PO1-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO POC-NUM-IPOC-LEN IN PO1-FLD
                GO TO FIN-EDT-IKY-PO1.
           MOVE POC-NUM-IPOC IN PO1-FLD TO POC-NUM-IPOC-EDT IN PO1-EDT.
           MOVE POC-NUM-IPOC-EDT IN PO1-EDT TO 
                POC-NUM-IPOC-ALF IN PO1-FLD.
*% INCLUDE FIN_EDT_IKY_PO1 OF 'COLBMPO1.TXT'
       FIN-EDT-IKY-PO1.
           EXIT.
       
       EDT-MKY-PO1 SECTION.
       INI-EDT-MKY-PO1.
*% INCLUDE EDT_MKY_PO1 OF 'COLBMPO1.TXT'
*% INCLUDE FIN_EDT_MKY_PO1 OF 'COLBMPO1.TXT'
       FIN-EDT-MKY-PO1.
           EXIT.
       
       EDT-AKY-PO1 SECTION.
       INI-EDT-AKY-PO1.
*% INCLUDE EDT_AKY_PO1 OF 'COLBMPO1.TXT'
*% INCLUDE FIN_EDT_AKY_PO1 OF 'COLBMPO1.TXT'
       FIN-EDT-AKY-PO1.
           EXIT.
       
       EDT-FLD-PO1 SECTION.
       INI-EDT-FLD-PO1.
*% INCLUDE EDT_FLD_PO1 OF 'COLBMPO1.TXT'
           IF POC-VAL-CMIN-LEN IN PO1-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO POC-VAL-CMIN-LEN IN PO1-FLD
                GO TO FIN-EDT-FLD-PO1.
           MOVE POC-VAL-CMIN IN PO1-FLD TO POC-VAL-CMIN-EDT IN PO1-EDT.
           MOVE POC-VAL-CMIN-EDT IN PO1-EDT TO 
                POC-VAL-CMIN-ALF IN PO1-FLD.
           IF POC-NUM-PMIN-LEN IN PO1-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO POC-NUM-PMIN-LEN IN PO1-FLD
                GO TO FIN-EDT-FLD-PO1.
           MOVE POC-NUM-PMIN IN PO1-FLD TO POC-NUM-PMIN-EDT IN PO1-EDT.
           MOVE POC-NUM-PMIN-EDT IN PO1-EDT TO 
                POC-NUM-PMIN-ALF IN PO1-FLD.
           IF POC-VAL-CMAX-LEN IN PO1-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO POC-VAL-CMAX-LEN IN PO1-FLD
                GO TO FIN-EDT-FLD-PO1.
           MOVE POC-VAL-CMAX IN PO1-FLD TO POC-VAL-CMAX-EDT IN PO1-EDT.
           MOVE POC-VAL-CMAX-EDT IN PO1-EDT TO 
                POC-VAL-CMAX-ALF IN PO1-FLD.
           IF POC-NUM-PMAX-LEN IN PO1-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO POC-NUM-PMAX-LEN IN PO1-FLD
                GO TO FIN-EDT-FLD-PO1.
           MOVE POC-NUM-PMAX IN PO1-FLD TO POC-NUM-PMAX-EDT IN PO1-EDT.
           MOVE POC-NUM-PMAX-EDT IN PO1-EDT TO 
                POC-NUM-PMAX-ALF IN PO1-FLD.
           IF POC-NUM-VMIN-LEN IN PO1-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO POC-NUM-VMIN-LEN IN PO1-FLD
                GO TO FIN-EDT-FLD-PO1.
           MOVE POC-NUM-VMIN IN PO1-FLD TO POC-NUM-VMIN-EDT IN PO1-EDT.
           MOVE POC-NUM-VMIN-EDT IN PO1-EDT TO 
                POC-NUM-VMIN-ALF IN PO1-FLD.
           IF POC-NUM-VMAX-LEN IN PO1-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO POC-NUM-VMAX-LEN IN PO1-FLD
                GO TO FIN-EDT-FLD-PO1.
           MOVE POC-NUM-VMAX IN PO1-FLD TO POC-NUM-VMAX-EDT IN PO1-EDT.
           MOVE POC-NUM-VMAX-EDT IN PO1-EDT TO 
                POC-NUM-VMAX-ALF IN PO1-FLD.
           IF POC-GLS-PCMI-LEN IN PO1-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO POC-GLS-PCMI-LEN IN PO1-FLD
                GO TO FIN-EDT-FLD-PO1.
           MOVE POC-GLS-PCMI IN PO1-FLD TO POC-GLS-PCMI-EDT IN PO1-EDT.
           MOVE POC-GLS-PCMI-EDT IN PO1-EDT TO 
                POC-GLS-PCMI-ALF IN PO1-FLD.
           IF POC-GLS-PCMA-LEN IN PO1-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO POC-GLS-PCMA-LEN IN PO1-FLD
                GO TO FIN-EDT-FLD-PO1.
           MOVE POC-GLS-PCMA IN PO1-FLD TO POC-GLS-PCMA-EDT IN PO1-EDT.
           MOVE POC-GLS-PCMA-EDT IN PO1-EDT TO 
                POC-GLS-PCMA-ALF IN PO1-FLD.
           IF POC-NUM-PREP-LEN IN PO1-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO POC-NUM-PREP-LEN IN PO1-FLD
                GO TO FIN-EDT-FLD-PO1.
           MOVE POC-NUM-PREP IN PO1-FLD TO POC-NUM-PREP-EDT IN PO1-EDT.
           MOVE POC-NUM-PREP-EDT IN PO1-EDT TO 
                POC-NUM-PREP-ALF IN PO1-FLD.
*% INCLUDE FIN_EDT_FLD_PO1 OF 'COLBMPO1.TXT'
       FIN-EDT-FLD-PO1.
           EXIT.
