*% FLD = "POC-COD-VCOC"
*% GLS = "FRM-GLS-VCOC"
*% TIENE_GLOSA = TRUE
*% LGL = 15
*% INCLUDE 'VALCOD'
*% INCLUDE FIN_POC_COD_VCOC OF 'COLBMPO1.TXT'
       FIN-VAL-POC-COD-VCOC.
