*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "PO1"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMPO1.TXT'
       SET-TAG-COLBQPO1 SECTION.
       INI-SET-TAG-COLBQPO1.
           MOVE
           '<IDNMOD NOM=$COLBQPO1$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:39 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQPO1.
           EXIT.
           
       PUT-ALL-PO1 SECTION.
       INI-PUT-ALL-PO1.
*% INCLUDE INI_PUT_ALL_PO1 OF 'COLBMPO1.TXT'
           PERFORM PUT-POC-PO1.
*% INCLUDE FIN_PUT_ALL_PO1 OF 'COLBMPO1.TXT'
       FIN-PUT-ALL-PO1.
           EXIT.
       
       PUT-POC-PO1 SECTION.
       INI-PUT-POC-PO1.
*% INCLUDE INI_PUT_POC_PO1 OF 'COLBMPO1.TXT'
           IF PGM-STAT-POC-NUL
               GO TO FIN-PUT-POC-PO1.
           IF NOT PGM-STAT-POC-OKS
               PERFORM CLR-POC-PO1
               GO TO FIN-PUT-POC-PO1.
           MOVE POC-COD-VCOC IN POC TO POC-COD-VCOC IN PO1-FLD.
           MOVE POC-COD-TOOC IN POC TO POC-COD-TOOC IN PO1-FLD.
           MOVE POC-COD-AOOC IN POC TO POC-COD-AOOC IN PO1-FLD.
           MOVE POC-NUM-IPOC IN POC TO POC-NUM-IPOC IN PO1-FLD.
           MOVE POC-VAL-CMIN IN POC TO POC-VAL-CMIN IN PO1-FLD.
           MOVE POC-NUM-PMIN IN POC TO POC-NUM-PMIN IN PO1-FLD.
           MOVE POC-VAL-CMAX IN POC TO POC-VAL-CMAX IN PO1-FLD.
           MOVE POC-IND-PMIN IN POC TO POC-IND-PMIN IN PO1-FLD.
           MOVE POC-IND-TICE IN POC TO POC-IND-TICE IN PO1-FLD.
           MOVE POC-NUM-PMAX IN POC TO POC-NUM-PMAX IN PO1-FLD.
           MOVE POC-IND-TINE IN POC TO POC-IND-TINE IN PO1-FLD.
           MOVE POC-IND-PMAX IN POC TO POC-IND-PMAX IN PO1-FLD.
           MOVE POC-IND-MTIC IN POC TO POC-IND-MTIC IN PO1-FLD.
           MOVE POC-NUM-VMIN IN POC TO POC-NUM-VMIN IN PO1-FLD.
           MOVE POC-MSC-BCTI IN POC TO POC-MSC-BCTI IN PO1-FLD.
           MOVE POC-NUM-VMAX IN POC TO POC-NUM-VMAX IN PO1-FLD.
           MOVE POC-MSC-PBTI IN POC TO POC-MSC-PBTI IN PO1-FLD.
           MOVE POC-IND-RENO IN POC TO POC-IND-RENO IN PO1-FLD.
           MOVE POC-MSC-BPBT IN POC TO POC-MSC-BPBT IN PO1-FLD.
           MOVE POC-GLS-PCMI IN POC TO POC-GLS-PCMI IN PO1-FLD.
           MOVE POC-GLS-PCMA IN POC TO POC-GLS-PCMA IN PO1-FLD.
           MOVE POC-MSC-SPBT IN POC TO POC-MSC-SPBT IN PO1-FLD.
           MOVE POC-COD-SEGD IN POC TO POC-COD-SEGD IN PO1-FLD.
           MOVE POC-COD-CVTI IN POC TO POC-COD-CVTI IN PO1-FLD.
           MOVE POC-IND-TCCE IN POC TO POC-IND-TCCE IN PO1-FLD.
           MOVE POC-IND-TCNE IN POC TO POC-IND-TCNE IN PO1-FLD.
           MOVE POC-MSC-TFPR IN POC TO POC-MSC-TFPR IN PO1-FLD.
           MOVE POC-COD-TCOM IN POC TO POC-COD-TCOM IN PO1-FLD.
           MOVE POC-COD-TDOC IN POC TO POC-COD-TDOC IN PO1-FLD.
           MOVE POC-NUM-PREP IN POC TO POC-NUM-PREP IN PO1-FLD.
           MOVE POC-IND-ENBO IN POC TO POC-IND-ENBO IN PO1-FLD.
           MOVE POC-MSC-UTPR IN POC TO POC-MSC-UTPR IN PO1-FLD.
           MOVE POC-IND-AVAL IN POC TO POC-IND-AVAL IN PO1-FLD.
*% BUF = "PO1-FLD"
*% SIS = "TAB"
