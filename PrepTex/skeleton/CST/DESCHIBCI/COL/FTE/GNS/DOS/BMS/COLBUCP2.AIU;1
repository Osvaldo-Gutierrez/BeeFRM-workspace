       VAL-FLD-CP2 SECTION.
       INI-VAL-FLD-CP2.
*% INCLUDE INI_VAL_FLD_CP2 OF 'COLBMCP2.TXT'
           PERFORM VAL-FRM-GLS-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-VCOC-LEN IN CP2-FLD
                GO TO FIN-VAL-FLD-CP2.
           PERFORM VAL-FRM-GLS-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TOOC-LEN IN CP2-FLD
                GO TO FIN-VAL-FLD-CP2.
           PERFORM VAL-FRM-GLS-AOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-AOOC-LEN IN CP2-FLD
                GO TO FIN-VAL-FLD-CP2.
           PERFORM VAL-FRM-GLS-MVTO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-MVTO-LEN IN CP2-FLD
                GO TO FIN-VAL-FLD-CP2.
           PERFORM VAL-FRM-GLS-SCRT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-SCRT-LEN IN CP2-FLD
                GO TO FIN-VAL-FLD-CP2.
           PERFORM VAL-FRM-GLS-SCTB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-SCTB-LEN IN CP2-FLD
                GO TO FIN-VAL-FLD-CP2.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FTRN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FTRN-LEN IN CP2-FLD
                GO TO FIN-VAL-FLD-CP2.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-MSC-TCCT
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 39
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-MSC-TCCT-LEN IN CP2-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CP2.
           PERFORM VAL-FRM-IND-DBHB
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 39
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-IND-DBHB-LEN IN CP2-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CP2.
           PERFORM VAL-FRM-IND-TALG
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 39
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-IND-TALG-LEN IN CP2-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CP2.
