*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMCP2.TXT'
       SET-TAG-COLBUCP2 SECTION.
       INI-SET-TAG-COLBUCP2.
           MOVE
           '<IDNMOD NOM=$COLBUCP2$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:29 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCP2.
           EXIT.
           
       RET-ALL-CP2 SECTION.
       INI-RET-ALL-CP2.
*% INCLUDE INI_RET_ALL_CP2 OF 'COLBMCP2.TXT'
           PERFORM KEY-ALL-CP2.
*% INCLUDE FIN_RET_ALL_CP2 OF 'COLBMCP2.TXT'
       FIN-RET-ALL-CP2.
           EXIT.
       
       KEY-ALL-CP2 SECTION.
       INI-KEY-ALL-CP2.
*% INCLUDE INI_KEY_ALL_CP2 OF 'COLBMCP2.TXT'
*% INCLUDE FIN_KEY_ALL_CP2 OF 'COLBMCP2.TXT'
       FIN-KEY-ALL-CP2.
           EXIT.
       
       SCR-CLRA-CP2 SECTION.
       INI-SCR-CLRA-CP2.
*% INCLUDE INI_SCR_CLRA_CP2 OF 'COLBMCP2.TXT'
           PERFORM CLR-KEY-CP2.
           PERFORM CLR-FLD-CP2.
*% INCLUDE FIN_SCR_CLRA_CP2 OF 'COLBMCP2.TXT'
       FIN-SCR-CLRA-CP2.
           EXIT.
       
       CLR-KEY-CP2 SECTION.
       INI-CLR-KEY-CP2.
*% INCLUDE INI_CLR_KEY_CP2 OF 'COLBMCP2.TXT'
           MOVE SPACES TO FRM-COD-VCOC IN CP2-FLD.
           MOVE SPACES TO FRM-COD-TOOC IN CP2-FLD.
           MOVE SPACES TO FRM-COD-AOOC IN CP2-FLD.
