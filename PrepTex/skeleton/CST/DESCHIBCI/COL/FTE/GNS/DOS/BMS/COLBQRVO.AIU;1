       FIN-VAL-RDC-IIC-ICLI.
           EXIT.
       
       VAL-CON-KEY-RVO SECTION.
       INI-VAL-CON-KEY-RVO.
*% INCLUDE INI_VAL_CON_KEY_RVO OF 'COLBMRVO.TXT'
           IF     OPC-CAI-IOPC IN RVO-FLD NOT > SPACES 
              AND OPC-IIC-IOPC IN RVO-FLD NOT > ZEROES 
              AND RDC-CAI-ICLI IN RVO-FLD NOT > SPACES 
              AND RDC-IIC-ICLI IN RVO-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-RVO.
*% INCLUDE FIN_VAL_CON_KEY_RVO OF 'COLBMRVO.TXT'
       FIN-VAL-CON-KEY-RVO.
           EXIT.
       
       VAL-CON-IKY-RVO SECTION.
       INI-VAL-CON-IKY-RVO.
*% INCLUDE INI_VAL_CON_IKY_RVO OF 'COLBMRVO.TXT'
