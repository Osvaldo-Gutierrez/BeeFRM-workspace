       EDT-KEY-RVO SECTION.
       INI-EDT-KEY-RVO.
           PERFORM EDT-IKY-RVO.
           PERFORM EDT-MKY-RVO.
           PERFORM EDT-AKY-RVO.
*% INCLUDE EDT_KEY_RVO OF 'COLBMRVO.TXT'
       FIN-EDT-KEY-RVO.
           EXIT.
       
       EDT-IKY-RVO SECTION.
       INI-EDT-IKY-RVO.
*% INCLUDE EDT_IKY_RVO OF 'COLBMRVO.TXT'
           IF OPC-IIC-IOPC-LEN IN RVO-FLD = -9
                MOVE -1 TO OPC-IIC-IOPC-LEN IN RVO-FLD
                GO TO FIN-EDT-IKY-RVO.
           MOVE OPC-IIC-IOPC IN RVO-FLD TO OPC-IIC-IOPC-EDT IN RVO-EDT.
           MOVE OPC-IIC-IOPC-EDT IN RVO-EDT TO 
                OPC-IIC-IOPC-ALF IN RVO-FLD.
*% INCLUDE FIN_EDT_IKY_RVO OF 'COLBMRVO.TXT'
       FIN-EDT-IKY-RVO.
           EXIT.
       
       EDT-MKY-RVO SECTION.
       INI-EDT-MKY-RVO.
*% INCLUDE EDT_MKY_RVO OF 'COLBMRVO.TXT'
           IF RDC-IIC-ICLI-LEN IN RVO-FLD = -9
                MOVE -1 TO RDC-IIC-ICLI-LEN IN RVO-FLD
                GO TO FIN-EDT-MKY-RVO.
           MOVE RDC-IIC-ICLI IN RVO-FLD TO RDC-IIC-ICLI-EDT IN RVO-EDT.
           MOVE RDC-IIC-ICLI-EDT IN RVO-EDT TO 
                RDC-IIC-ICLI-ALF IN RVO-FLD.
*% INCLUDE FIN_EDT_MKY_RVO OF 'COLBMRVO.TXT'
       FIN-EDT-MKY-RVO.
           EXIT.
       
