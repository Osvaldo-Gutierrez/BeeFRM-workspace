       FIN-UNP-DBC-RVO.
           EXIT.
       
       UNP-RDC-RVO SECTION.
       INI-UNP-RDC-RVO.
*% INCLUDE INI_UNP_RDC_RVO OF 'COLBMRVO.TXT'
*% INCLUDE FIN_UNP_RDC_RVO OF 'COLBMRVO.TXT'
       FIN-UNP-RDC-RVO.
           EXIT.
       
       VAL-KEY-RVO SECTION.
       INI-VAL-KEY-RVO.
           PERFORM VAL-IKY-RVO.
           PERFORM VAL-MKY-RVO.
           PERFORM VAL-AKY-RVO.
       FIN-VAL-KEY-RVO.
           EXIT.
       
       VAL-IKY-RVO SECTION.
       INI-VAL-IKY-RVO.
*% INCLUDE INI_VAL_IKY_RVO OF 'COLBMRVO.TXT'
           PERFORM VAL-OPC-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPC-CAI-IOPC-LEN IN RVO-FLD
                GO TO FIN-VAL-IKY-RVO.
           PERFORM VAL-OPC-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPC-IIC-IOPC-LEN IN RVO-FLD
                GO TO FIN-VAL-IKY-RVO.
*% INCLUDE FIN_VAL_IKY_RVO OF 'COLBMRVO.TXT'
       FIN-VAL-IKY-RVO.
           EXIT.
       
       VAL-MKY-RVO SECTION.
       INI-VAL-MKY-RVO.
*% INCLUDE INI_VAL_MKY_RVO OF 'COLBMRVO.TXT'
           PERFORM VAL-RDC-CAI-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RDC-CAI-ICLI-LEN IN RVO-FLD
                GO TO FIN-VAL-MKY-RVO.
           PERFORM VAL-RDC-IIC-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RDC-IIC-ICLI-LEN IN RVO-FLD
                GO TO FIN-VAL-MKY-RVO.
*% INCLUDE FIN_VAL_MKY_RVO OF 'COLBMRVO.TXT'
       FIN-VAL-MKY-RVO.
           EXIT.
       
       VAL-AKY-RVO SECTION.
       INI-VAL-AKY-RVO.
*% INCLUDE INI_VAL_AKY_RVO OF 'COLBMRVO.TXT'
*% INCLUDE FIN_VAL_AKY_RVO OF 'COLBMRVO.TXT'
       FIN-VAL-AKY-RVO.
           EXIT.
       
       VAL-OPC-CAI-IOPC SECTION.
       INI-VAL-OPC-CAI-IOPC.
*% LBL = "OPC_CAI_IOPC OF 'COLBMRVO.TXT'"
           IF OPC-CAI-IOPC IN RVO-FLD NOT > SPACES
              MOVE SPACES TO OPC-CAI-IOPC IN RVO-FLD.
*% INCLUDE INI_OPC_CAI_IOPC OF 'COLBMRVO.TXT'
*% INCLUDE OPC_CAI_IOPC OF 'COLBMRVO.TXT'
*% INCLUDE FIN_OPC_CAI_IOPC OF 'COLBMRVO.TXT'
       FIN-VAL-OPC-CAI-IOPC.
           EXIT.
       
       VAL-OPC-IIC-IOPC SECTION.
       INI-VAL-OPC-IIC-IOPC.
*% LBL = "OPC_IIC_IOPC OF 'COLBMRVO.TXT'"
*% INCLUDE INI_OPC_IIC_IOPC OF 'COLBMRVO.TXT'
*% INCLUDE OPC_IIC_IOPC OF 'COLBMRVO.TXT'
*% INCLUDE FIN_OPC_IIC_IOPC OF 'COLBMRVO.TXT'
       FIN-VAL-OPC-IIC-IOPC.
           EXIT.
       
       VAL-RDC-CAI-ICLI SECTION.
       INI-VAL-RDC-CAI-ICLI.
*% LBL = "RDC_CAI_ICLI OF 'COLBMRVO.TXT'"
*% INCLUDE INI_RDC_CAI_ICLI OF 'COLBMRVO.TXT'
*% INCLUDE RDC_CAI_ICLI OF 'COLBMRVO.TXT'
*% INCLUDE FIN_RDC_CAI_ICLI OF 'COLBMRVO.TXT'
       FIN-VAL-RDC-CAI-ICLI.
           EXIT.
       
       VAL-RDC-IIC-ICLI SECTION.
       INI-VAL-RDC-IIC-ICLI.
*% LBL = "RDC_IIC_ICLI OF 'COLBMRVO.TXT'"
*% INCLUDE INI_RDC_IIC_ICLI OF 'COLBMRVO.TXT'
