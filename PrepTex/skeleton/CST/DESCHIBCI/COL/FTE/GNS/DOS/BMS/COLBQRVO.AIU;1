      * 04-Jan-95 08:25 PM
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
       PUT-ALL-RVO SECTION.
       INI-PUT-ALL-RVO.
*% INCLUDE INI_PUT_ALL_RVO OF 'COLBMRVO.TXT'
           PERFORM PUT-OPC-RVO.
           PERFORM PUT-DBC-RVO.
           PERFORM PUT-RDC-RVO.
*% INCLUDE FIN_PUT_ALL_RVO OF 'COLBMRVO.TXT'
       FIN-PUT-ALL-RVO.
           EXIT.
       
       PUT-OPC-RVO SECTION.
       INI-PUT-OPC-RVO.
*% INCLUDE INI_PUT_OPC_RVO OF 'COLBMRVO.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-RVO.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-RVO
               GO TO FIN-PUT-OPC-RVO.
           MOVE OPC-CAI-IOPC IN OPC TO OPC-CAI-IOPC IN RVO-FLD.
           MOVE OPC-IIC-IOPC IN OPC TO OPC-IIC-IOPC IN RVO-FLD.
           MOVE OPC-COD-TOOC IN OPC TO OPC-COD-TOOC IN RVO-FLD.
           MOVE OPC-COD-AOOC IN OPC TO OPC-COD-AOOC IN RVO-FLD.
           MOVE OPC-COD-VCOC IN OPC TO OPC-COD-VCOC IN RVO-FLD.
           MOVE OPC-NUM-DCOL IN OPC TO OPC-NUM-DCOL IN RVO-FLD.
           MOVE OPC-NUM-MCOL IN OPC TO OPC-NUM-MCOL IN RVO-FLD.
           MOVE OPC-NUM-SCOL IN OPC TO OPC-NUM-SCOL IN RVO-FLD.
           MOVE OPC-NUM-ACOL IN OPC TO OPC-NUM-ACOL IN RVO-FLD.
           MOVE OPC-VAL-CRED IN OPC TO OPC-VAL-CRED IN RVO-FLD.
*% BUF = "RVO-FLD"
*% SIS = "TAB"
*% TAB = "TIO "
*% FLD = "OPC-COD-TOOC"
*% GLS = "FRM-GLS-TOOC"
*% TIENE_GLOSA = TRUE
*% LGL = 18
*% INCLUDE 'BUSTAB'
*% BUF = "RVO-FLD"
*% SIS = "TAB"
*% TAB = "VLR "
*% FLD = "OPC-COD-VCOC"
*% GLS = "FRM-GLS-VCOC"
*% TIENE_GLOSA = TRUE
*% LGL = 15
*% INCLUDE 'BUSTAB'
*% INCLUDE FIN_PUT_OPC_RVO OF 'COLBMRVO.TXT'
       FIN-PUT-OPC-RVO.
           EXIT.
       
       PUT-DBC-RVO SECTION.
       INI-PUT-DBC-RVO.
*% INCLUDE INI_PUT_DBC_RVO OF 'COLBMRVO.TXT'
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-RVO.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-RVO
               GO TO FIN-PUT-DBC-RVO.
           MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN RVO-FLD.
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN RVO-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN RVO-FLD.
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN RVO-FLD.
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN RVO-FLD.
*% INCLUDE FIN_PUT_DBC_RVO OF 'COLBMRVO.TXT'
       FIN-PUT-DBC-RVO.
           EXIT.
       
       PUT-RDC-RVO SECTION.
       INI-PUT-RDC-RVO.
*% INCLUDE INI_PUT_RDC_RVO OF 'COLBMRVO.TXT'
           IF PGM-STAT-RDC-NUL
               GO TO FIN-PUT-RDC-RVO.
           IF NOT PGM-STAT-RDC-OKS
               PERFORM CLR-RDC-RVO
               GO TO FIN-PUT-RDC-RVO.
           MOVE RDC-CAI-ICLI IN RDC TO RDC-CAI-ICLI IN RVO-FLD.
           MOVE RDC-IIC-ICLI IN RDC TO RDC-IIC-ICLI IN RVO-FLD.
*% INCLUDE FIN_PUT_RDC_RVO OF 'COLBMRVO.TXT'
       FIN-PUT-RDC-RVO.
           EXIT.
       
       KEY-ALL-RVO SECTION.
       INI-KEY-ALL-RVO.
*% INCLUDE INI_KEY_ALL_RVO OF 'COLBMRVO.TXT'
           PERFORM KEY-OPC-RVO.
           PERFORM KEY-DBC-RVO.
           PERFORM KEY-RDC-RVO.
*% INCLUDE FIN_KEY_ALL_RVO OF 'COLBMRVO.TXT'
       FIN-KEY-ALL-RVO.
           EXIT.
       
       KEY-OPC-RVO SECTION.
       INI-KEY-OPC-RVO.
*% INCLUDE INI_KEY_OPC_RVO OF 'COLBMRVO.TXT'
           MOVE OPC-CAI-IOPC IN RVO-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE OPC-IIC-IOPC IN RVO-FLD TO OPC-IIC-IOPC IN OPC.
*% INCLUDE FIN_KEY_OPC_RVO OF 'COLBMRVO.TXT'
       FIN-KEY-OPC-RVO.
           EXIT.
       
       KEY-DBC-RVO SECTION.
       INI-KEY-DBC-RVO.
*% INCLUDE INI_KEY_DBC_RVO OF 'COLBMRVO.TXT'
*% INCLUDE FIN_KEY_DBC_RVO OF 'COLBMRVO.TXT'
       FIN-KEY-DBC-RVO.
           EXIT.
       
       KEY-RDC-RVO SECTION.
       INI-KEY-RDC-RVO.
*% INCLUDE INI_KEY_RDC_RVO OF 'COLBMRVO.TXT'
           MOVE RDC-CAI-ICLI IN RVO-FLD TO RDC-CAI-ICLI IN RDC.
           MOVE RDC-IIC-ICLI IN RVO-FLD TO RDC-IIC-ICLI IN RDC.
*% INCLUDE FIN_KEY_RDC_RVO OF 'COLBMRVO.TXT'
       FIN-KEY-RDC-RVO.
           EXIT.
       
       SCR-CLRA-RVO SECTION.
       INI-SCR-CLRA-RVO.
*% INCLUDE INI_SCR_CLRA_RVO OF 'COLBMRVO.TXT'
           PERFORM CLR-KEY-RVO.
           PERFORM CLR-FLD-RVO.
*% INCLUDE FIN_SCR_CLRA_RVO OF 'COLBMRVO.TXT'
       FIN-SCR-CLRA-RVO.
           EXIT.
       
       CLR-KEY-RVO SECTION.
       INI-CLR-KEY-RVO.
*% INCLUDE INI_CLR_KEY_RVO OF 'COLBMRVO.TXT'
           MOVE SPACES TO OPC-CAI-IOPC IN RVO-FLD.
           MOVE ZEROES TO OPC-IIC-IOPC IN RVO-FLD.
           MOVE SPACES TO RDC-CAI-ICLI IN RVO-FLD.
           MOVE ZEROES TO RDC-IIC-ICLI IN RVO-FLD.
*% INCLUDE FIN_CLR_KEY_RVO OF 'COLBMRVO.TXT'
       FIN-CLR-KEY-RVO.
           EXIT.
       
       CLR-FLD-RVO SECTION.
       INI-CLR-FLD-RVO.
*% INCLUDE INI_CLR_FLD_RVO OF 'COLBMRVO.TXT'
           MOVE SPACES TO FRM-GLS-TOOC IN RVO-FLD.
           MOVE SPACES TO FRM-GLS-AOOC IN RVO-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN RVO-FLD.
           MOVE SPACES TO FRM-CAI-IVCD IN RVO-FLD(1).
           MOVE SPACES TO FRM-CAI-IVCD IN RVO-FLD(2).
           MOVE SPACES TO FRM-CAI-IVCD IN RVO-FLD(3).
           MOVE SPACES TO FRM-CAI-IVCD IN RVO-FLD(4).
           MOVE SPACES TO FRM-CAI-IVCD IN RVO-FLD(5).
           MOVE SPACES TO FRM-CAI-IVCD IN RVO-FLD(6).
           MOVE SPACES TO FRM-CAI-IVCD IN RVO-FLD(7).
           MOVE SPACES TO FRM-CAI-IVCD IN RVO-FLD(8).
           MOVE SPACES TO FRM-CAI-IVCD IN RVO-FLD(9).
           MOVE SPACES TO FRM-CAI-IVCD IN RVO-FLD(10).
           MOVE SPACES TO FRM-CAI-IVCD IN RVO-FLD(11).
           MOVE SPACES TO FRM-CAI-IVCD IN RVO-FLD(12).
           MOVE SPACES TO FRM-CAI-IVCD IN RVO-FLD(13).
           MOVE SPACES TO FRM-CAI-IVCD IN RVO-FLD(14).
           MOVE SPACES TO FRM-CAI-IVCD IN RVO-FLD(15).
           MOVE SPACES TO FRM-CAI-IVCD IN RVO-FLD(16).
           MOVE SPACES TO FRM-CAI-IVCD IN RVO-FLD(17).
           MOVE SPACES TO FRM-CAI-IVCD IN RVO-FLD(18).
           MOVE ZEROES TO FRM-IIC-IVCD IN RVO-FLD(1).
           MOVE ZEROES TO FRM-IIC-IVCD IN RVO-FLD(2).
           MOVE ZEROES TO FRM-IIC-IVCD IN RVO-FLD(3).
           MOVE ZEROES TO FRM-IIC-IVCD IN RVO-FLD(4).
           MOVE ZEROES TO FRM-IIC-IVCD IN RVO-FLD(5).
           MOVE ZEROES TO FRM-IIC-IVCD IN RVO-FLD(6).
           MOVE ZEROES TO FRM-IIC-IVCD IN RVO-FLD(7).
           MOVE ZEROES TO FRM-IIC-IVCD IN RVO-FLD(8).
           MOVE ZEROES TO FRM-IIC-IVCD IN RVO-FLD(9).
           MOVE ZEROES TO FRM-IIC-IVCD IN RVO-FLD(10).
           MOVE ZEROES TO FRM-IIC-IVCD IN RVO-FLD(11).
           MOVE ZEROES TO FRM-IIC-IVCD IN RVO-FLD(12).
           MOVE ZEROES TO FRM-IIC-IVCD IN RVO-FLD(13).
           MOVE ZEROES TO FRM-IIC-IVCD IN RVO-FLD(14).
           MOVE ZEROES TO FRM-IIC-IVCD IN RVO-FLD(15).
           MOVE ZEROES TO FRM-IIC-IVCD IN RVO-FLD(16).
           MOVE ZEROES TO FRM-IIC-IVCD IN RVO-FLD(17).
           MOVE ZEROES TO FRM-IIC-IVCD IN RVO-FLD(18).
           PERFORM CLR-OPC-RVO.
           PERFORM CLR-DBC-RVO.
           PERFORM CLR-RDC-RVO.
*% INCLUDE FIN_CLR_FLD_RVO OF 'COLBMRVO.TXT'
       FIN-CLR-FLD-RVO.
           EXIT.
       
       CLR-OPC-RVO SECTION.
       INI-CLR-OPC-RVO.
*% INCLUDE INI_CLR_OPC_RVO OF 'COLBMRVO.TXT'
           MOVE SPACES TO OPC-COD-TOOC IN RVO-FLD.
           MOVE SPACES TO OPC-COD-AOOC IN RVO-FLD.
           MOVE SPACES TO OPC-COD-VCOC IN RVO-FLD.
           MOVE ZEROES TO OPC-FEC-FCOL IN RVO-FLD.
           MOVE ZEROES TO OPC-VAL-CRED IN RVO-FLD.
*% INCLUDE FIN_CLR_OPC_RVO OF 'COLBMRVO.TXT'
       FIN-CLR-OPC-RVO.
           EXIT.
       
       CLR-DBC-RVO SECTION.
       INI-CLR-DBC-RVO.
*% INCLUDE INI_CLR_DBC_RVO OF 'COLBMRVO.TXT'
           MOVE SPACES TO DBC-GLS-NOMC IN RVO-FLD.
           MOVE ZEROES TO DBC-NUM-ICLI IN RVO-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN RVO-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN RVO-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN RVO-FLD.
*% INCLUDE FIN_CLR_DBC_RVO OF 'COLBMRVO.TXT'
       FIN-CLR-DBC-RVO.
           EXIT.
       
       CLR-RDC-RVO SECTION.
       INI-CLR-RDC-RVO.
*% INCLUDE INI_CLR_RDC_RVO OF 'COLBMRVO.TXT'
*% INCLUDE FIN_CLR_RDC_RVO OF 'COLBMRVO.TXT'
       FIN-CLR-RDC-RVO.
           EXIT.
       
       FST-KEY-RVO SECTION.
       INI-FST-KEY-RVO.
*% INCLUDE INI_FST_KEY_RVO OF 'COLBMRVO.TXT'
           MOVE -1 TO OPC-CAI-IOPC-LEN IN RVO-FLD.
*% INCLUDE FIN_FST_KEY_RVO OF 'COLBMRVO.TXT'
       FIN-FST-KEY-RVO.
           EXIT.
       
       FST-FLD-RVO SECTION.
       INI-FST-FLD-RVO.
*% INCLUDE INI_FST_FLD_RVO OF 'COLBMRVO.TXT'
           MOVE -1 TO FRM-CAI-IVCD-LEN IN RVO-FLD(1).
*% INCLUDE FIN_FST_FLD_RVO OF 'COLBMRVO.TXT'
       FIN-FST-FLD-RVO.
           EXIT.
       
       PRO-KEY-RVO SECTION.
       INI-PRO-KEY-RVO.
*% INCLUDE INI_PRO_KEY_RVO OF 'COLBMRVO.TXT'
           PERFORM PRO-IKY-RVO.
           PERFORM PRO-AKY-RVO.
           PERFORM PRO-MKY-RVO.
*% INCLUDE FIN_PRO_KEY_RVO OF 'COLBMRVO.TXT'
       FIN-PRO-KEY-RVO.
           EXIT.
       
       PRO-IKY-RVO SECTION.
       INI-PRO-IKY-RVO.
*% INCLUDE INI_PRO_IKY_RVO OF 'COLBMRVO.TXT'
           MOVE FRM-CPIM-AEY TO OPC-CAI-IOPC-ATR IN RVO-FLD.
           MOVE FRM-CPIM-AEY TO OPC-IIC-IOPC-ATR IN RVO-FLD.
*% INCLUDE FIN_PRO_IKY_RVO OF 'COLBMRVO.TXT'
       FIN-PRO-IKY-RVO.
           EXIT.
       
