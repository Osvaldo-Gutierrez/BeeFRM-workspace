       FIN-EDT-AKY-RVO.
           EXIT.
       
       EDT-FLD-RVO SECTION.
       INI-EDT-FLD-RVO.
*% INCLUDE EDT_FLD_RVO OF 'COLBMRVO.TXT'
           IF DBC-NUM-ICLI-LEN IN RVO-FLD = -9
                MOVE -1 TO DBC-NUM-ICLI-LEN IN RVO-FLD
                GO TO FIN-EDT-FLD-RVO.
           MOVE DBC-NUM-ICLI IN RVO-FLD TO DBC-NUM-ICLI-EDT IN RVO-EDT.
           MOVE DBC-NUM-ICLI-EDT IN RVO-EDT TO 
                DBC-NUM-ICLI-ALF IN RVO-FLD.
           IF OPC-VAL-CRED-LEN IN RVO-FLD = -9
                MOVE -1 TO OPC-VAL-CRED-LEN IN RVO-FLD
                GO TO FIN-EDT-FLD-RVO.
           MOVE OPC-VAL-CRED IN RVO-FLD TO OPC-VAL-CRED-EDT IN RVO-EDT.
           MOVE OPC-VAL-CRED-EDT IN RVO-EDT TO 
                OPC-VAL-CRED-ALF IN RVO-FLD.
           IF FRM-IIC-IVCD-LEN IN RVO-FLD(1) = -9
                MOVE -1 TO FRM-IIC-IVCD-LEN IN RVO-FLD(1)
                GO TO FIN-EDT-FLD-RVO.
           MOVE FRM-IIC-IVCD IN RVO-FLD(1) TO 
                FRM-IIC-IVCD-EDT IN RVO-EDT(1).
           MOVE FRM-IIC-IVCD-EDT IN RVO-EDT(1) TO 
                FRM-IIC-IVCD-ALF IN RVO-FLD(1).
           IF FRM-IIC-IVCD-LEN IN RVO-FLD(2) = -9
                MOVE -1 TO FRM-IIC-IVCD-LEN IN RVO-FLD(2)
                GO TO FIN-EDT-FLD-RVO.
           MOVE FRM-IIC-IVCD IN RVO-FLD(2) TO 
                FRM-IIC-IVCD-EDT IN RVO-EDT(2).
           MOVE FRM-IIC-IVCD-EDT IN RVO-EDT(2) TO 
                FRM-IIC-IVCD-ALF IN RVO-FLD(2).
           IF FRM-IIC-IVCD-LEN IN RVO-FLD(3) = -9
                MOVE -1 TO FRM-IIC-IVCD-LEN IN RVO-FLD(3)
                GO TO FIN-EDT-FLD-RVO.
           MOVE FRM-IIC-IVCD IN RVO-FLD(3) TO 
                FRM-IIC-IVCD-EDT IN RVO-EDT(3).
           MOVE FRM-IIC-IVCD-EDT IN RVO-EDT(3) TO 
                FRM-IIC-IVCD-ALF IN RVO-FLD(3).
           IF FRM-IIC-IVCD-LEN IN RVO-FLD(4) = -9
                MOVE -1 TO FRM-IIC-IVCD-LEN IN RVO-FLD(4)
                GO TO FIN-EDT-FLD-RVO.
           MOVE FRM-IIC-IVCD IN RVO-FLD(4) TO 
                FRM-IIC-IVCD-EDT IN RVO-EDT(4).
           MOVE FRM-IIC-IVCD-EDT IN RVO-EDT(4) TO 
                FRM-IIC-IVCD-ALF IN RVO-FLD(4).
           IF FRM-IIC-IVCD-LEN IN RVO-FLD(5) = -9
                MOVE -1 TO FRM-IIC-IVCD-LEN IN RVO-FLD(5)
                GO TO FIN-EDT-FLD-RVO.
           MOVE FRM-IIC-IVCD IN RVO-FLD(5) TO 
                FRM-IIC-IVCD-EDT IN RVO-EDT(5).
           MOVE FRM-IIC-IVCD-EDT IN RVO-EDT(5) TO 
                FRM-IIC-IVCD-ALF IN RVO-FLD(5).
           IF FRM-IIC-IVCD-LEN IN RVO-FLD(6) = -9
                MOVE -1 TO FRM-IIC-IVCD-LEN IN RVO-FLD(6)
                GO TO FIN-EDT-FLD-RVO.
           MOVE FRM-IIC-IVCD IN RVO-FLD(6) TO 
                FRM-IIC-IVCD-EDT IN RVO-EDT(6).
           MOVE FRM-IIC-IVCD-EDT IN RVO-EDT(6) TO 
                FRM-IIC-IVCD-ALF IN RVO-FLD(6).
           IF FRM-IIC-IVCD-LEN IN RVO-FLD(7) = -9
                MOVE -1 TO FRM-IIC-IVCD-LEN IN RVO-FLD(7)
                GO TO FIN-EDT-FLD-RVO.
           MOVE FRM-IIC-IVCD IN RVO-FLD(7) TO 
                FRM-IIC-IVCD-EDT IN RVO-EDT(7).
           MOVE FRM-IIC-IVCD-EDT IN RVO-EDT(7) TO 
                FRM-IIC-IVCD-ALF IN RVO-FLD(7).
           IF FRM-IIC-IVCD-LEN IN RVO-FLD(8) = -9
                MOVE -1 TO FRM-IIC-IVCD-LEN IN RVO-FLD(8)
                GO TO FIN-EDT-FLD-RVO.
           MOVE FRM-IIC-IVCD IN RVO-FLD(8) TO 
                FRM-IIC-IVCD-EDT IN RVO-EDT(8).
           MOVE FRM-IIC-IVCD-EDT IN RVO-EDT(8) TO 
                FRM-IIC-IVCD-ALF IN RVO-FLD(8).
           IF FRM-IIC-IVCD-LEN IN RVO-FLD(9) = -9
                MOVE -1 TO FRM-IIC-IVCD-LEN IN RVO-FLD(9)
                GO TO FIN-EDT-FLD-RVO.
           MOVE FRM-IIC-IVCD IN RVO-FLD(9) TO 
                FRM-IIC-IVCD-EDT IN RVO-EDT(9).
           MOVE FRM-IIC-IVCD-EDT IN RVO-EDT(9) TO 
                FRM-IIC-IVCD-ALF IN RVO-FLD(9).
           IF FRM-IIC-IVCD-LEN IN RVO-FLD(10) = -9
                MOVE -1 TO FRM-IIC-IVCD-LEN IN RVO-FLD(10)
                GO TO FIN-EDT-FLD-RVO.
           MOVE FRM-IIC-IVCD IN RVO-FLD(10) TO 
                FRM-IIC-IVCD-EDT IN RVO-EDT(10).
           MOVE FRM-IIC-IVCD-EDT IN RVO-EDT(10) TO 
                FRM-IIC-IVCD-ALF IN RVO-FLD(10).
           IF FRM-IIC-IVCD-LEN IN RVO-FLD(11) = -9
                MOVE -1 TO FRM-IIC-IVCD-LEN IN RVO-FLD(11)
                GO TO FIN-EDT-FLD-RVO.
           MOVE FRM-IIC-IVCD IN RVO-FLD(11) TO 
                FRM-IIC-IVCD-EDT IN RVO-EDT(11).
           MOVE FRM-IIC-IVCD-EDT IN RVO-EDT(11) TO 
                FRM-IIC-IVCD-ALF IN RVO-FLD(11).
           IF FRM-IIC-IVCD-LEN IN RVO-FLD(12) = -9
                MOVE -1 TO FRM-IIC-IVCD-LEN IN RVO-FLD(12)
                GO TO FIN-EDT-FLD-RVO.
           MOVE FRM-IIC-IVCD IN RVO-FLD(12) TO 
