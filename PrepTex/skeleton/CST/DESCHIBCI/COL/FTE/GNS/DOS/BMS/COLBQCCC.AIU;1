       EDT-KEY-CCC SECTION.
       INI-EDT-KEY-CCC.
           PERFORM EDT-IKY-CCC.
           PERFORM EDT-MKY-CCC.
           PERFORM EDT-AKY-CCC.
*% INCLUDE EDT_KEY_CCC OF 'COLBMCCC.TXT'
       FIN-EDT-KEY-CCC.
           EXIT.
       
       EDT-IKY-CCC SECTION.
       INI-EDT-IKY-CCC.
*% INCLUDE EDT_IKY_CCC OF 'COLBMCCC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF DBC-NUM-ICLI-LEN IN CCC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DBC-NUM-ICLI-LEN IN CCC-FLD
                GO TO FIN-EDT-IKY-CCC.
           MOVE DBC-NUM-ICLI IN CCC-FLD TO
                DBC-NUM-ICLI-EDT IN CCC-EDT.
           MOVE DBC-NUM-ICLI-EDT IN CCC-EDT TO
                DBC-NUM-ICLI-ALF IN CCC-FLD.
      * IDC-FIN --> BMCHIBCI011.SRCAIU EDICION NUMERICA
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FINI"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FTER"
*% INCLUDE FIN_EDT_IKY_CCC OF 'COLBMCCC.TXT'
       FIN-EDT-IKY-CCC.
           EXIT.
       
       EDT-MKY-CCC SECTION.
       INI-EDT-MKY-CCC.
*% INCLUDE EDT_MKY_CCC OF 'COLBMCCC.TXT'
*% INCLUDE FIN_EDT_MKY_CCC OF 'COLBMCCC.TXT'
       FIN-EDT-MKY-CCC.
           EXIT.
       
       EDT-AKY-CCC SECTION.
       INI-EDT-AKY-CCC.
*% INCLUDE EDT_AKY_CCC OF 'COLBMCCC.TXT'
*% INCLUDE FIN_EDT_AKY_CCC OF 'COLBMCCC.TXT'
       FIN-EDT-AKY-CCC.
           EXIT.
       
       EDT-FLD-CCC SECTION.
       INI-EDT-FLD-CCC.
*% INCLUDE EDT_FLD_CCC OF 'COLBMCCC.TXT'
           IF FRM-NUM-TDLC-LEN IN CCC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-TDLC-LEN IN CCC-FLD
                GO TO FIN-EDT-FLD-CCC.
           MOVE FRM-NUM-TDLC IN CCC-FLD TO FRM-NUM-TDLC-EDT IN CCC-EDT.
           MOVE FRM-NUM-TDLC-EDT IN CCC-EDT TO 
                FRM-NUM-TDLC-ALF IN CCC-FLD.
           IF FRM-VAL-TDLC-LEN IN CCC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-TDLC-LEN IN CCC-FLD
                GO TO FIN-EDT-FLD-CCC.
           MOVE FRM-VAL-TDLC IN CCC-FLD TO FRM-VAL-TDLC-EDT IN CCC-EDT.
           MOVE FRM-VAL-TDLC-EDT IN CCC-EDT TO 
                FRM-VAL-TDLC-ALF IN CCC-FLD.
           IF FRM-VAL-CRED-LEN IN CCC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-CRED-LEN IN CCC-FLD
                GO TO FIN-EDT-FLD-CCC.
           MOVE FRM-VAL-CRED IN CCC-FLD TO FRM-VAL-CRED-EDT IN CCC-EDT.
           MOVE FRM-VAL-CRED-EDT IN CCC-EDT TO 
                FRM-VAL-CRED-ALF IN CCC-FLD.
           IF FRM-VAL-SCRE-LEN IN CCC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-SCRE-LEN IN CCC-FLD
                GO TO FIN-EDT-FLD-CCC.
           MOVE FRM-VAL-SCRE IN CCC-FLD TO FRM-VAL-SCRE-EDT IN CCC-EDT.
           MOVE FRM-VAL-SCRE-EDT IN CCC-EDT TO 
                FRM-VAL-SCRE-ALF IN CCC-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FTRN"
           IF FRM-HRA-HRTR-LEN IN CCC-FLD = FRM-ERR-NUME
