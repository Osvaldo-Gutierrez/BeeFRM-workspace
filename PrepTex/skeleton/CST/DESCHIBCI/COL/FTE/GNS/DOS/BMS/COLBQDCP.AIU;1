      *
      * COLBQDCP
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 08:52 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "DCP"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMDCP.TXT'
       SET-TAG-COLBQDCP SECTION.
       INI-SET-TAG-COLBQDCP.
           MOVE
           '<IDNMOD NOM=$COLBQDCP$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 08:52 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQDCP.
           EXIT.
           
       PUT-ALL-DCP SECTION.
       INI-PUT-ALL-DCP.
*% INCLUDE INI_PUT_ALL_DCP OF 'COLBMDCP.TXT'
           PERFORM PUT-OPC-DCP.
*% INCLUDE FIN_PUT_ALL_DCP OF 'COLBMDCP.TXT'
       FIN-PUT-ALL-DCP.
           EXIT.
       
       PUT-OPC-DCP SECTION.
       INI-PUT-OPC-DCP.
*% INCLUDE INI_PUT_OPC_DCP OF 'COLBMDCP.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-DCP.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-DCP
               GO TO FIN-PUT-OPC-DCP.
           MOVE OPC-CAI-IOPC IN OPC TO OPC-CAI-IOPC IN DCP-FLD.
           MOVE OPC-IIC-IOPC IN OPC TO OPC-IIC-IOPC IN DCP-FLD.
*% INCLUDE FIN_PUT_OPC_DCP OF 'COLBMDCP.TXT'
       FIN-PUT-OPC-DCP.
           EXIT.
       
       KEY-ALL-DCP SECTION.
       INI-KEY-ALL-DCP.
*% INCLUDE INI_KEY_ALL_DCP OF 'COLBMDCP.TXT'
           PERFORM KEY-OPC-DCP.
*% INCLUDE FIN_KEY_ALL_DCP OF 'COLBMDCP.TXT'
       FIN-KEY-ALL-DCP.
           EXIT.
       
       KEY-OPC-DCP SECTION.
       INI-KEY-OPC-DCP.
*% INCLUDE INI_KEY_OPC_DCP OF 'COLBMDCP.TXT'
           MOVE OPC-CAI-IOPC IN DCP-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE OPC-IIC-IOPC IN DCP-FLD TO OPC-IIC-IOPC IN OPC.
*% INCLUDE FIN_KEY_OPC_DCP OF 'COLBMDCP.TXT'
       FIN-KEY-OPC-DCP.
           EXIT.
       
       SCR-CLRA-DCP SECTION.
       INI-SCR-CLRA-DCP.
*% INCLUDE INI_SCR_CLRA_DCP OF 'COLBMDCP.TXT'
           PERFORM CLR-KEY-DCP.
           PERFORM CLR-FLD-DCP.
*% INCLUDE FIN_SCR_CLRA_DCP OF 'COLBMDCP.TXT'
       FIN-SCR-CLRA-DCP.
           EXIT.
       
       CLR-KEY-DCP SECTION.
       INI-CLR-KEY-DCP.
*% INCLUDE INI_CLR_KEY_DCP OF 'COLBMDCP.TXT'
           MOVE SPACES TO OPC-CAI-IOPC IN DCP-FLD.
           MOVE ZEROES TO OPC-IIC-IOPC IN DCP-FLD.
           MOVE ZEROES TO FRM-NUM-IVEN IN DCP-FLD.
           MOVE SPACES TO FRM-IND-HMAS IN DCP-FLD.
*% INCLUDE FIN_CLR_KEY_DCP OF 'COLBMDCP.TXT'
       FIN-CLR-KEY-DCP.
           EXIT.
       
       CLR-FLD-DCP SECTION.
       INI-CLR-FLD-DCP.
*% INCLUDE INI_CLR_FLD_DCP OF 'COLBMDCP.TXT'
           MOVE SPACES TO FRM-CAI-IOP2 IN DCP-FLD.
           MOVE ZEROES TO FRM-IIC-IOP2 IN DCP-FLD.
           MOVE ZEROES TO FRM-VAL-CRED IN DCP-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCOL"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FCOL IN DCP-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-COD-COOC IN DCP-FLD.
           MOVE ZEROES TO FRM-SGV-INTR IN DCP-FLD.
           MOVE ZEROES TO FRM-SGV-SPAG IN DCP-FLD.
           MOVE ZEROES TO FRM-NUM-NCRE IN DCP-FLD(1).
           MOVE ZEROES TO FRM-NUM-NCRE IN DCP-FLD(2).
           MOVE SPACES TO FRM-IND-PAGO IN DCP-FLD(1).
           MOVE SPACES TO FRM-IND-PAGO IN DCP-FLD(2).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FVEN IN DCP-FLD(1).
           MOVE ZEROES TO FRM-FEC-FVEN IN DCP-FLD(2).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-SGV-IPTO IN DCP-FLD(1).
           MOVE ZEROES TO FRM-SGV-IPTO IN DCP-FLD(2).
           MOVE ZEROES TO FRM-SGV-COMI IN DCP-FLD(1).
           MOVE ZEROES TO FRM-SGV-COMI IN DCP-FLD(2).
           MOVE ZEROES TO FRM-SGV-INTE IN DCP-FLD(1).
           MOVE ZEROES TO FRM-SGV-INTE IN DCP-FLD(2).
           MOVE ZEROES TO FRM-SGV-CAPI IN DCP-FLD(1).
           MOVE ZEROES TO FRM-SGV-CAPI IN DCP-FLD(2).
           MOVE ZEROES TO FRM-SGV-TCUO IN DCP-FLD(1).
           MOVE ZEROES TO FRM-SGV-TCUO IN DCP-FLD(2).
           MOVE ZEROES TO FRM-SGV-SINS IN DCP-FLD(1).
           MOVE ZEROES TO FRM-SGV-SINS IN DCP-FLD(2).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FCAN IN DCP-FLD(1).
           MOVE ZEROES TO FRM-FEC-FCAN IN DCP-FLD(2).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-SGV-TINT IN DCP-FLD(1).
           MOVE ZEROES TO FRM-SGV-TINT IN DCP-FLD(2).
           MOVE SPACES TO FRM-IND-TCYA IN DCP-FLD(1).
           MOVE SPACES TO FRM-IND-TCYA IN DCP-FLD(2).
           MOVE ZEROES TO FRM-SGV-MORA IN DCP-FLD(1).
           MOVE ZEROES TO FRM-SGV-MORA IN DCP-FLD(2).
           MOVE ZEROES TO FRM-SGV-GCOB IN DCP-FLD(1).
           MOVE ZEROES TO FRM-SGV-GCOB IN DCP-FLD(2).
           MOVE ZEROES TO FRM-SGV-GJUD IN DCP-FLD(1).
           MOVE ZEROES TO FRM-SGV-GJUD IN DCP-FLD(2).
           MOVE ZEROES TO FRM-SGV-PREP IN DCP-FLD(1).
           MOVE ZEROES TO FRM-SGV-PREP IN DCP-FLD(2).
           MOVE SPACES TO FRM-IND-SITU IN DCP-FLD(1).
           MOVE SPACES TO FRM-IND-SITU IN DCP-FLD(2).
           MOVE SPACES TO FRM-CAI-IOPC IN DCP-FLD(1).
           MOVE SPACES TO FRM-CAI-IOPC IN DCP-FLD(2).
           MOVE ZEROES TO FRM-IIC-IOPC IN DCP-FLD(1).
           MOVE ZEROES TO FRM-IIC-IOPC IN DCP-FLD(2).
           MOVE ZEROES TO FRM-SGV-PCTA IN DCP-FLD(1).
           MOVE ZEROES TO FRM-SGV-PCTA IN DCP-FLD(2).
           PERFORM CLR-OPC-DCP.
*% INCLUDE FIN_CLR_FLD_DCP OF 'COLBMDCP.TXT'
       FIN-CLR-FLD-DCP.
           EXIT.
       
       CLR-OPC-DCP SECTION.
       INI-CLR-OPC-DCP.
*% INCLUDE INI_CLR_OPC_DCP OF 'COLBMDCP.TXT'
*% INCLUDE FIN_CLR_OPC_DCP OF 'COLBMDCP.TXT'
       FIN-CLR-OPC-DCP.
           EXIT.
       
       FST-KEY-DCP SECTION.
       INI-FST-KEY-DCP.
*% INCLUDE INI_FST_KEY_DCP OF 'COLBMDCP.TXT'
           MOVE -1 TO OPC-CAI-IOPC-LEN IN DCP-FLD.
*% INCLUDE FIN_FST_KEY_DCP OF 'COLBMDCP.TXT'
       FIN-FST-KEY-DCP.
           EXIT.
       
       FST-FLD-DCP SECTION.
       INI-FST-FLD-DCP.
*% INCLUDE INI_FST_FLD_DCP OF 'COLBMDCP.TXT'
*% INCLUDE FIN_FST_FLD_DCP OF 'COLBMDCP.TXT'
       FIN-FST-FLD-DCP.
           EXIT.
       
       PRO-KEY-DCP SECTION.
       INI-PRO-KEY-DCP.
*% INCLUDE INI_PRO_KEY_DCP OF 'COLBMDCP.TXT'
           MOVE FRM-CPIM-AEY TO FRM-NUM-IVEN-ATR IN DCP-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-HMAS-ATR IN DCP-FLD.
           PERFORM PRO-IKY-DCP.
           PERFORM PRO-AKY-DCP.
           PERFORM PRO-MKY-DCP.
*% INCLUDE FIN_PRO_KEY_DCP OF 'COLBMDCP.TXT'
       FIN-PRO-KEY-DCP.
           EXIT.
       
       PRO-IKY-DCP SECTION.
       INI-PRO-IKY-DCP.
*% INCLUDE INI_PRO_IKY_DCP OF 'COLBMDCP.TXT'
           MOVE FRM-CPIM-AEY TO OPC-CAI-IOPC-ATR IN DCP-FLD.
           MOVE FRM-CPIM-AEY TO OPC-IIC-IOPC-ATR IN DCP-FLD.
*% INCLUDE FIN_PRO_IKY_DCP OF 'COLBMDCP.TXT'
       FIN-PRO-IKY-DCP.
           EXIT.
       
       PRO-MKY-DCP SECTION.
       INI-PRO-MKY-DCP.
*% INCLUDE INI_PRO_MKY_DCP OF 'COLBMDCP.TXT'
*% INCLUDE FIN_PRO_MKY_DCP OF 'COLBMDCP.TXT'
       FIN-PRO-MKY-DCP.
           EXIT.
       
       PRO-AKY-DCP SECTION.
       INI-PRO-AKY-DCP.
*% INCLUDE INI_PRO_AKY_DCP OF 'COLBMDCP.TXT'
*% INCLUDE FIN_PRO_AKY_DCP OF 'COLBMDCP.TXT'
       FIN-PRO-AKY-DCP.
           EXIT.
       
       UNP-KEY-DCP SECTION.
       INI-UNP-KEY-DCP.
*% INCLUDE INI_UNP_KEY_DCP OF 'COLBMDCP.TXT'
           MOVE FRM-CPIM-UBY TO FRM-NUM-IVEN-ATR IN DCP-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IND-HMAS-ATR IN DCP-FLD.
           PERFORM UNP-IKY-DCP.
           PERFORM UNP-AKY-DCP.
           PERFORM UNP-MKY-DCP.
*% INCLUDE FIN_UNP_KEY_DCP OF 'COLBMDCP.TXT'
       FIN-UNP-KEY-DCP.
           EXIT.
       
       UNP-IKY-DCP SECTION.
       INI-UNP-IKY-DCP.
*% INCLUDE INI_UNP_IKY_DCP OF 'COLBMDCP.TXT'
           MOVE FRM-CPIM-UBY TO OPC-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO OPC-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-NUM-IVEN-ATR.
           MOVE FRM-CPIM-UBY TO FRM-IND-HMAS-ATR.
*% INCLUDE FIN_UNP_IKY_DCP OF 'COLBMDCP.TXT'
       FIN-UNP-IKY-DCP.
           EXIT.
       
       UNP-MKY-DCP SECTION.
       INI-UNP-MKY-DCP.
*% INCLUDE INI_UNP_MKY_DCP OF 'COLBMDCP.TXT'
*% INCLUDE FIN_UNP_MKY_DCP OF 'COLBMDCP.TXT'
       FIN-UNP-MKY-DCP.
           EXIT.
       
       UNP-AKY-DCP SECTION.
       INI-UNP-AKY-DCP.
*% INCLUDE INI_UNP_AKY_DCP OF 'COLBMDCP.TXT'
*% INCLUDE FIN_UNP_AKY_DCP OF 'COLBMDCP.TXT'
       FIN-UNP-AKY-DCP.
           EXIT.
       
       UNP-FLD-DCP SECTION.
       INI-UNP-FLD-DCP.
*% INCLUDE INI_UNP_FLD_DCP OF 'COLBMDCP.TXT'
           PERFORM UNP-OPC-DCP.
*% INCLUDE FIN_UNP_FLD_DCP OF 'COLBMDCP.TXT'
       FIN-UNP-FLD-DCP.
           EXIT.
       
       UNP-OPC-DCP SECTION.
       INI-UNP-OPC-DCP.
*% INCLUDE INI_UNP_OPC_DCP OF 'COLBMDCP.TXT'
*% INCLUDE FIN_UNP_OPC_DCP OF 'COLBMDCP.TXT'
       FIN-UNP-OPC-DCP.
           EXIT.
       
       VAL-KEY-DCP SECTION.
       INI-VAL-KEY-DCP.
           PERFORM VAL-IKY-DCP.
           PERFORM VAL-MKY-DCP.
           PERFORM VAL-AKY-DCP.
       FIN-VAL-KEY-DCP.
           EXIT.
       
       VAL-IKY-DCP SECTION.
       INI-VAL-IKY-DCP.
*% INCLUDE INI_VAL_IKY_DCP OF 'COLBMDCP.TXT'
           PERFORM VAL-OPC-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-CAI-IOPC-LEN IN DCP-FLD
                GO TO FIN-VAL-IKY-DCP.
           PERFORM VAL-OPC-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-IIC-IOPC-LEN IN DCP-FLD
                GO TO FIN-VAL-IKY-DCP.
           PERFORM VAL-FRM-NUM-IVEN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-NUM-IVEN-LEN IN DCP-FLD
                GO TO FIN-VAL-IKY-DCP.
           PERFORM VAL-FRM-IND-HMAS.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IND-HMAS-LEN IN DCP-FLD
                GO TO FIN-VAL-IKY-DCP.
*% INCLUDE FIN_VAL_IKY_DCP OF 'COLBMDCP.TXT'
       FIN-VAL-IKY-DCP.
           EXIT.
       
       VAL-MKY-DCP SECTION.
       INI-VAL-MKY-DCP.
*% INCLUDE INI_VAL_MKY_DCP OF 'COLBMDCP.TXT'
*% INCLUDE FIN_VAL_MKY_DCP OF 'COLBMDCP.TXT'
       FIN-VAL-MKY-DCP.
           EXIT.
       
       VAL-AKY-DCP SECTION.
       INI-VAL-AKY-DCP.
*% INCLUDE INI_VAL_AKY_DCP OF 'COLBMDCP.TXT'
*% INCLUDE FIN_VAL_AKY_DCP OF 'COLBMDCP.TXT'
       FIN-VAL-AKY-DCP.
           EXIT.
       
       VAL-OPC-CAI-IOPC SECTION.
       INI-VAL-OPC-CAI-IOPC.
*% LBL = "OPC_CAI_IOPC OF 'COLBMDCP.TXT'"
*% INCLUDE INI_OPC_CAI_IOPC OF 'COLBMDCP.TXT'
           IF OPC-CAI-IOPC IN DCP-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-OPC-CAI-IOPC.
*% INCLUDE OPC_CAI_IOPC OF 'COLBMDCP.TXT'
*% INCLUDE FIN_OPC_CAI_IOPC OF 'COLBMDCP.TXT'
       FIN-VAL-OPC-CAI-IOPC.
           EXIT.
       
       VAL-OPC-IIC-IOPC SECTION.
       INI-VAL-OPC-IIC-IOPC.
*% LBL = "OPC_IIC_IOPC OF 'COLBMDCP.TXT'"
*% INCLUDE INI_OPC_IIC_IOPC OF 'COLBMDCP.TXT'
           IF OPC-IIC-IOPC IN DCP-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-OPC-IIC-IOPC.
*% INCLUDE OPC_IIC_IOPC OF 'COLBMDCP.TXT'
*% INCLUDE FIN_OPC_IIC_IOPC OF 'COLBMDCP.TXT'
       FIN-VAL-OPC-IIC-IOPC.
           EXIT.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCOL"
*% IZR = "S"
       
       VAL-FRM-NUM-IVEN SECTION.
       INI-VAL-FRM-NUM-IVEN.
*% LBL = "FRM_NUM_IVEN OF 'COLBMDCP.TXT'"
*% INCLUDE INI_FRM_NUM_IVEN OF 'COLBMDCP.TXT'
*% INCLUDE FRM_NUM_IVEN OF 'COLBMDCP.TXT'
*% INCLUDE FIN_FRM_NUM_IVEN OF 'COLBMDCP.TXT'
       FIN-VAL-FRM-NUM-IVEN.
           EXIT.
       
       VAL-FRM-IND-HMAS SECTION.
       INI-VAL-FRM-IND-HMAS.
*% LBL = "FRM_IND_HMAS OF 'COLBMDCP.TXT'"
*% INCLUDE INI_FRM_IND_HMAS OF 'COLBMDCP.TXT'
           IF FRM-IND-HMAS IN DCP-FLD NOT > SPACES
              MOVE SPACES TO FRM-IND-HMAS IN DCP-FLD.
*% INCLUDE FRM_IND_HMAS OF 'COLBMDCP.TXT'
*% INCLUDE FIN_FRM_IND_HMAS OF 'COLBMDCP.TXT'
       FIN-VAL-FRM-IND-HMAS.
           EXIT.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVEN"
*% IZR = "S"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAN"
*% IZR = "S"
       
       VAL-CON-KEY-DCP SECTION.
       INI-VAL-CON-KEY-DCP.
*% INCLUDE INI_VAL_CON_KEY_DCP OF 'COLBMDCP.TXT'
           IF     OPC-CAI-IOPC IN DCP-FLD NOT > SPACES 
              AND OPC-IIC-IOPC IN DCP-FLD NOT > ZEROES 
              AND FRM-NUM-IVEN IN DCP-FLD NOT > ZEROES 
              AND FRM-IND-HMAS IN DCP-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-DCP.
*% INCLUDE FIN_VAL_CON_KEY_DCP OF 'COLBMDCP.TXT'
       FIN-VAL-CON-KEY-DCP.
           EXIT.
       
       VAL-CON-IKY-DCP SECTION.
       INI-VAL-CON-IKY-DCP.
*% INCLUDE INI_VAL_CON_IKY_DCP OF 'COLBMDCP.TXT'
           IF     OPC-CAI-IOPC IN DCP-FLD NOT > SPACES 
              AND OPC-IIC-IOPC IN DCP-FLD NOT > ZEROES 
              AND FRM-NUM-IVEN IN DCP-FLD NOT > ZEROES 
              AND FRM-IND-HMAS IN DCP-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-DCP.
*% INCLUDE FIN_VAL_CON_IKY_DCP OF 'COLBMDCP.TXT'
       FIN-VAL-CON-IKY-DCP.
           EXIT.
       
       VAL-NUM-KEY-DCP SECTION.
       INI-VAL-NUM-KEY-DCP.
           PERFORM VAL-NUM-IKY-DCP.
           PERFORM VAL-NUM-MKY-DCP.
           PERFORM VAL-NUM-AKY-DCP.
       FIN-VAL-NUM-KEY-DCP.
           EXIT.
       
       VAL-NUM-IKY-DCP SECTION.
       INI-VAL-NUM-IKY-DCP.
*% INCLUDE INI_VAL_NUM_IKY_DCP OF 'COLBMDCP.TXT'
       VAL-NUM-OPC-IIC-IOPC.
*% BUF = "DCP-FLD"
*% FLD = "OPC-IIC-IOPC"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-DCP.
       VAL-NUM-FRM-NUM-IVEN.
*% BUF = "DCP-FLD"
*% FLD = "FRM-NUM-IVEN"
*% ENT = "3"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-DCP.
*% INCLUDE FIN_VAL_NUM_IKY_DCP OF 'COLBMDCP.TXT'
       FIN-VAL-NUM-IKY-DCP.
