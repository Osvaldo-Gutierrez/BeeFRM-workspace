*% INCLUDE FIN_UNP_DBC_CDD OF 'COLBMCDD.TXT'
       FIN-UNP-DBC-CDD.
           EXIT.
       
       VAL-MKY-CDD SECTION.
       INI-VAL-MKY-CDD.
*% INCLUDE INI_VAL_MKY_CDD OF 'COLBMCDD.TXT'
*% INCLUDE FIN_VAL_MKY_CDD OF 'COLBMCDD.TXT'
       FIN-VAL-MKY-CDD.
           EXIT.
       
       VAL-FLD-CDD SECTION.
       INI-VAL-FLD-CDD.
*% INCLUDE INI_VAL_FLD_CDD OF 'COLBMCDD.TXT'
           PERFORM VAL-FRM-GLS-NOMC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-NOMC-LEN IN CDD-FLD
                GO TO FIN-VAL-FLD-CDD.
           PERFORM VAL-FRM-VAL-CADE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-CADE-LEN IN CDD-FLD
                GO TO FIN-VAL-FLD-CDD.
           PERFORM VAL-FRM-COD-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-VCOC-LEN IN CDD-FLD
                GO TO FIN-VAL-FLD-CDD.
           PERFORM VAL-FRM-VAL-TPRE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-TPRE-LEN IN CDD-FLD
                GO TO FIN-VAL-FLD-CDD.
           PERFORM VAL-FRM-VAL-NOPE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-NOPE-LEN IN CDD-FLD
                GO TO FIN-VAL-FLD-CDD.
           PERFORM VAL-FRM-VAL-CAMA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-CAMA-LEN IN CDD-FLD
                GO TO FIN-VAL-FLD-CDD.
           PERFORM VAL-FRM-VAL-TPOR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-TPOR-LEN IN CDD-FLD
                GO TO FIN-VAL-FLD-CDD.
           PERFORM VAL-FRM-GLS-OBSE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-OBSE-LEN IN CDD-FLD
                GO TO FIN-VAL-FLD-CDD.
           PERFORM VAL-FRM-CIC-IOPC
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 4
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-CIC-IOPC-LEN IN CDD-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCUR"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FCUR VARYING FRM-IFLD
                FROM 1 BY 1 UNTIL FRM-IFLD > 4 OR
                FRM-SUAR = FRM-SUAR-MAL OR
                FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FCUR-LEN IN CDD-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDD.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-VAL-TINT
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 4
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-TINT-LEN IN CDD-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDD.
           PERFORM VAL-FRM-VAL-SKUF
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 4
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-SKUF-LEN IN CDD-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDD.
           PERFORM VAL-FRM-VAL-KPPE
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 4
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-KPPE-LEN IN CDD-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDD.
           PERFORM VAL-FRM-VAL-INPE
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 4
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-INPE-LEN IN CDD-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDD.
           PERFORM VAL-FRM-VAL-TPPE
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 4
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-TPPE-LEN IN CDD-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDD.
*% INCLUDE FIN_VAL_FLD_CDD OF 'COLBMCDD.TXT'
       FIN-VAL-FLD-CDD.
           EXIT.
