*% FRM = "CDD"
      *
      * COLBUCDD
      * Version 3.0.1 Nov-2005
      * 25-Oct-10 09:18 AM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMCDD.TXT'
       SET-TAG-COLBUCDD SECTION.
       INI-SET-TAG-COLBUCDD.
           MOVE
           '<IDNMOD NOM=$COLBUCDD$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$25-Oct-10 09:18 AM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCDD.
           EXIT.
           
       RET-ALL-CDD SECTION.
       INI-RET-ALL-CDD.
*% INCLUDE INI_RET_ALL_CDD OF 'COLBMCDD.TXT'
           PERFORM KEY-ALL-CDD.
           PERFORM RET-DBC-CDD.
*% INCLUDE FIN_RET_ALL_CDD OF 'COLBMCDD.TXT'
       FIN-RET-ALL-CDD.
           EXIT.
       
       RET-DBC-CDD SECTION.
       INI-RET-DBC-CDD.
*% INCLUDE INI_RET_DBC_CDD OF 'COLBMCDD.TXT'
*% INCLUDE FIN_RET_DBC_CDD OF 'COLBMCDD.TXT'
       FIN-RET-DBC-CDD.
           EXIT.
       
       KEY-ALL-CDD SECTION.
       INI-KEY-ALL-CDD.
*% INCLUDE INI_KEY_ALL_CDD OF 'COLBMCDD.TXT'
           PERFORM KEY-DBC-CDD.
*% INCLUDE FIN_KEY_ALL_CDD OF 'COLBMCDD.TXT'
       FIN-KEY-ALL-CDD.
           EXIT.
       
       KEY-DBC-CDD SECTION.
       INI-KEY-DBC-CDD.
*% INCLUDE INI_KEY_DBC_CDD OF 'COLBMCDD.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN CDD-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN CDD-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN CDD-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN CDD-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_KEY_DBC_CDD OF 'COLBMCDD.TXT'
       FIN-KEY-DBC-CDD.
           EXIT.
       
       SCR-CLRA-CDD SECTION.
       INI-SCR-CLRA-CDD.
*% INCLUDE INI_SCR_CLRA_CDD OF 'COLBMCDD.TXT'
           PERFORM CLR-KEY-CDD.
           PERFORM CLR-FLD-CDD.
*% INCLUDE FIN_SCR_CLRA_CDD OF 'COLBMCDD.TXT'
       FIN-SCR-CLRA-CDD.
           EXIT.
       
       CLR-KEY-CDD SECTION.
       INI-CLR-KEY-CDD.
*% INCLUDE INI_CLR_KEY_CDD OF 'COLBMCDD.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN CDD-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN CDD-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN CDD-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN CDD-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-VAL-PREP IN CDD-FLD.
*% INCLUDE FIN_CLR_KEY_CDD OF 'COLBMCDD.TXT'
       FIN-CLR-KEY-CDD.
           EXIT.
       
       CLR-FLD-CDD SECTION.
       INI-CLR-FLD-CDD.
*% INCLUDE INI_CLR_FLD_CDD OF 'COLBMCDD.TXT'
           MOVE SPACES TO FRM-GLS-NOMC IN CDD-FLD.
           MOVE ZEROES TO FRM-VAL-CADE IN CDD-FLD.
           MOVE SPACES TO FRM-COD-VCOC IN CDD-FLD.
           MOVE ZEROES TO FRM-VAL-TPRE IN CDD-FLD.
           MOVE ZEROES TO FRM-VAL-NOPE IN CDD-FLD.
           MOVE ZEROES TO FRM-VAL-CAMA IN CDD-FLD.
           MOVE ZEROES TO FRM-VAL-TPOR IN CDD-FLD.
           MOVE SPACES TO FRM-GLS-OBSE IN CDD-FLD.
           MOVE SPACES TO FRM-CIC-IOPC IN CDD-FLD(1).
           MOVE SPACES TO FRM-CIC-IOPC IN CDD-FLD(2).
           MOVE SPACES TO FRM-CIC-IOPC IN CDD-FLD(3).
           MOVE SPACES TO FRM-CIC-IOPC IN CDD-FLD(4).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCUR"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FCUR IN CDD-FLD(1).
           MOVE ZEROES TO FRM-FEC-FCUR IN CDD-FLD(2).
           MOVE ZEROES TO FRM-FEC-FCUR IN CDD-FLD(3).
           MOVE ZEROES TO FRM-FEC-FCUR IN CDD-FLD(4).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-VAL-TINT IN CDD-FLD(1).
           MOVE ZEROES TO FRM-VAL-TINT IN CDD-FLD(2).
           MOVE ZEROES TO FRM-VAL-TINT IN CDD-FLD(3).
           MOVE ZEROES TO FRM-VAL-TINT IN CDD-FLD(4).
           MOVE ZEROES TO FRM-VAL-SKUF IN CDD-FLD(1).
           MOVE ZEROES TO FRM-VAL-SKUF IN CDD-FLD(2).
           MOVE ZEROES TO FRM-VAL-SKUF IN CDD-FLD(3).
           MOVE ZEROES TO FRM-VAL-SKUF IN CDD-FLD(4).
           MOVE ZEROES TO FRM-VAL-KPPE IN CDD-FLD(1).
           MOVE ZEROES TO FRM-VAL-KPPE IN CDD-FLD(2).
           MOVE ZEROES TO FRM-VAL-KPPE IN CDD-FLD(3).
           MOVE ZEROES TO FRM-VAL-KPPE IN CDD-FLD(4).
           MOVE ZEROES TO FRM-VAL-INPE IN CDD-FLD(1).
           MOVE ZEROES TO FRM-VAL-INPE IN CDD-FLD(2).
           MOVE ZEROES TO FRM-VAL-INPE IN CDD-FLD(3).
           MOVE ZEROES TO FRM-VAL-INPE IN CDD-FLD(4).
           MOVE ZEROES TO FRM-VAL-TPPE IN CDD-FLD(1).
           MOVE ZEROES TO FRM-VAL-TPPE IN CDD-FLD(2).
           MOVE ZEROES TO FRM-VAL-TPPE IN CDD-FLD(3).
           MOVE ZEROES TO FRM-VAL-TPPE IN CDD-FLD(4).
           PERFORM CLR-DBC-CDD.
*% INCLUDE FIN_CLR_FLD_CDD OF 'COLBMCDD.TXT'
       FIN-CLR-FLD-CDD.
           EXIT.
       
       CLR-DBC-CDD SECTION.
       INI-CLR-DBC-CDD.
*% INCLUDE INI_CLR_DBC_CDD OF 'COLBMCDD.TXT'
*% INCLUDE FIN_CLR_DBC_CDD OF 'COLBMCDD.TXT'
       FIN-CLR-DBC-CDD.
           EXIT.
       
       FST-KEY-CDD SECTION.
       INI-FST-KEY-CDD.
*% INCLUDE INI_FST_KEY_CDD OF 'COLBMCDD.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
           MOVE FRM-POS-CURS TO DBC-NUM-ICLI-LEN IN CDD-FLD.
      * VRF-FIN --> BMCHIBCI004.SRCAIU ( FST-KEY Y FST-FLD )
*% INCLUDE FIN_FST_KEY_CDD OF 'COLBMCDD.TXT'
       FIN-FST-KEY-CDD.
           EXIT.
       
       FST-FLD-CDD SECTION.
       INI-FST-FLD-CDD.
*% INCLUDE INI_FST_FLD_CDD OF 'COLBMCDD.TXT'
*% INCLUDE FIN_FST_FLD_CDD OF 'COLBMCDD.TXT'
       FIN-FST-FLD-CDD.
           EXIT.
       
       PRO-KEY-CDD SECTION.
       INI-PRO-KEY-CDD.
*% INCLUDE INI_PRO_KEY_CDD OF 'COLBMCDD.TXT'
           MOVE FRM-CPIM-AEY TO FRM-VAL-PREP-ATR IN CDD-FLD.
           PERFORM PRO-MKY-CDD.
*% INCLUDE FIN_PRO_KEY_CDD OF 'COLBMCDD.TXT'
       FIN-PRO-KEY-CDD.
           EXIT.
       
       PRO-MKY-CDD SECTION.
       INI-PRO-MKY-CDD.
*% INCLUDE INI_PRO_MKY_CDD OF 'COLBMCDD.TXT'
*% INCLUDE FIN_PRO_MKY_CDD OF 'COLBMCDD.TXT'
       FIN-PRO-MKY-CDD.
           EXIT.
       
       PRO-FLD-CDD SECTION.
       INI-PRO-FLD-CDD.
*% INCLUDE INI_PRO_FLD_CDD OF 'COLBMCDD.TXT'
           PERFORM PRO-DBC-CDD.
*% INCLUDE FIN_PRO_FLD_CDD OF 'COLBMCDD.TXT'
       FIN-PRO-FLD-CDD.
           EXIT.
       
       PRO-DBC-CDD SECTION.
       INI-PRO-DBC-CDD.
*% INCLUDE INI_PRO_DBC_CDD OF 'COLBMCDD.TXT'
*% INCLUDE FIN_PRO_DBC_CDD OF 'COLBMCDD.TXT'
       FIN-PRO-DBC-CDD.
           EXIT.
       
       UNP-KEY-CDD SECTION.
       INI-UNP-KEY-CDD.
*% INCLUDE INI_UNP_KEY_CDD OF 'COLBMCDD.TXT'
           MOVE FRM-CPIM-UBY TO FRM-VAL-PREP-ATR IN CDD-FLD.
           PERFORM UNP-IKY-CDD.
           PERFORM UNP-AKY-CDD.
           PERFORM UNP-MKY-CDD.
*% INCLUDE FIN_UNP_KEY_CDD OF 'COLBMCDD.TXT'
       FIN-UNP-KEY-CDD.
           EXIT.
       
       UNP-IKY-CDD SECTION.
       INI-UNP-IKY-CDD.
*% INCLUDE INI_UNP_IKY_CDD OF 'COLBMCDD.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO DBC-NUM-ICLI-ATR IN CDD-FLD.
           MOVE FRM-CPIM-UBY TO DBC-VRF-ICLI-ATR IN CDD-FLD.
           MOVE FRM-CPIM-UBY TO DBC-IND-ICLI-ATR IN CDD-FLD.
           MOVE FRM-CPIM-UBY TO DBC-GLS-ICLI-ATR IN CDD-FLD.
      * IDC-FIN --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO FRM-VAL-PREP-ATR.
*% INCLUDE FIN_UNP_IKY_CDD OF 'COLBMCDD.TXT'
       FIN-UNP-IKY-CDD.
           EXIT.
       
       UNP-MKY-CDD SECTION.
       INI-UNP-MKY-CDD.
*% INCLUDE INI_UNP_MKY_CDD OF 'COLBMCDD.TXT'
*% INCLUDE FIN_UNP_MKY_CDD OF 'COLBMCDD.TXT'
       FIN-UNP-MKY-CDD.
           EXIT.
       
       UNP-AKY-CDD SECTION.
       INI-UNP-AKY-CDD.
