           MOVE FRM-SGV-CAPI-EDT IN CPR-EDT TO 
                FRM-SGV-CAPI-ALF IN CPR-FLD(2).
           IF FRM-SGV-CAPI-LEN IN CPR-FLD(3) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-SGV-CAPI-LEN IN CPR-FLD(3)
                GO TO FIN-EDT-FLD-CPR.
           MOVE FRM-SGV-CAPI IN CPR-FLD(3) TO 
                FRM-SGV-CAPI-EDT IN CPR-EDT.
           MOVE FRM-SGV-CAPI-EDT IN CPR-EDT TO 
                FRM-SGV-CAPI-ALF IN CPR-FLD(3).
           IF FRM-SGV-CAPI-LEN IN CPR-FLD(4) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-SGV-CAPI-LEN IN CPR-FLD(4)
                GO TO FIN-EDT-FLD-CPR.
           MOVE FRM-SGV-CAPI IN CPR-FLD(4) TO 
                FRM-SGV-CAPI-EDT IN CPR-EDT.
           MOVE FRM-SGV-CAPI-EDT IN CPR-EDT TO 
                FRM-SGV-CAPI-ALF IN CPR-FLD(4).
           IF FRM-SGV-CAPI-LEN IN CPR-FLD(5) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-SGV-CAPI-LEN IN CPR-FLD(5)
                GO TO FIN-EDT-FLD-CPR.
           MOVE FRM-SGV-CAPI IN CPR-FLD(5) TO 
                FRM-SGV-CAPI-EDT IN CPR-EDT.
           MOVE FRM-SGV-CAPI-EDT IN CPR-EDT TO 
                FRM-SGV-CAPI-ALF IN CPR-FLD(5).
           IF FRM-SGV-TINT-LEN IN CPR-FLD(1) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-SGV-TINT-LEN IN CPR-FLD(1)
                GO TO FIN-EDT-FLD-CPR.
           MOVE FRM-SGV-TINT IN CPR-FLD(1) TO 
                FRM-SGV-TINT-EDT IN CPR-EDT.
           MOVE FRM-SGV-TINT-EDT IN CPR-EDT TO 
                FRM-SGV-TINT-ALF IN CPR-FLD(1).
           IF FRM-SGV-TINT-LEN IN CPR-FLD(2) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-SGV-TINT-LEN IN CPR-FLD(2)
                GO TO FIN-EDT-FLD-CPR.
           MOVE FRM-SGV-TINT IN CPR-FLD(2) TO 
                FRM-SGV-TINT-EDT IN CPR-EDT.
           MOVE FRM-SGV-TINT-EDT IN CPR-EDT TO 
                FRM-SGV-TINT-ALF IN CPR-FLD(2).
           IF FRM-SGV-TINT-LEN IN CPR-FLD(3) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-SGV-TINT-LEN IN CPR-FLD(3)
                GO TO FIN-EDT-FLD-CPR.
           MOVE FRM-SGV-TINT IN CPR-FLD(3) TO 
                FRM-SGV-TINT-EDT IN CPR-EDT.
           MOVE FRM-SGV-TINT-EDT IN CPR-EDT TO 
                FRM-SGV-TINT-ALF IN CPR-FLD(3).
           IF FRM-SGV-TINT-LEN IN CPR-FLD(4) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-SGV-TINT-LEN IN CPR-FLD(4)
                GO TO FIN-EDT-FLD-CPR.
           MOVE FRM-SGV-TINT IN CPR-FLD(4) TO 
                FRM-SGV-TINT-EDT IN CPR-EDT.
           MOVE FRM-SGV-TINT-EDT IN CPR-EDT TO 
                FRM-SGV-TINT-ALF IN CPR-FLD(4).
           IF FRM-SGV-TINT-LEN IN CPR-FLD(5) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-SGV-TINT-LEN IN CPR-FLD(5)
                GO TO FIN-EDT-FLD-CPR.
           MOVE FRM-SGV-TINT IN CPR-FLD(5) TO 
                FRM-SGV-TINT-EDT IN CPR-EDT.
           MOVE FRM-SGV-TINT-EDT IN CPR-EDT TO 
                FRM-SGV-TINT-ALF IN CPR-FLD(5).
           IF FRM-NUM-VCOR-LEN IN CPR-FLD(1) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-VCOR-LEN IN CPR-FLD(1)
                GO TO FIN-EDT-FLD-CPR.
           MOVE FRM-NUM-VCOR IN CPR-FLD(1) TO 
                FRM-NUM-VCOR-EDT IN CPR-EDT.
           MOVE FRM-NUM-VCOR-EDT IN CPR-EDT TO 
                FRM-NUM-VCOR-ALF IN CPR-FLD(1).
           IF FRM-NUM-VCOR-LEN IN CPR-FLD(2) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-VCOR-LEN IN CPR-FLD(2)
                GO TO FIN-EDT-FLD-CPR.
           MOVE FRM-NUM-VCOR IN CPR-FLD(2) TO 
                FRM-NUM-VCOR-EDT IN CPR-EDT.
           MOVE FRM-NUM-VCOR-EDT IN CPR-EDT TO 
                FRM-NUM-VCOR-ALF IN CPR-FLD(2).
           IF FRM-NUM-VCOR-LEN IN CPR-FLD(3) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-VCOR-LEN IN CPR-FLD(3)
                GO TO FIN-EDT-FLD-CPR.
           MOVE FRM-NUM-VCOR IN CPR-FLD(3) TO 
                FRM-NUM-VCOR-EDT IN CPR-EDT.
           MOVE FRM-NUM-VCOR-EDT IN CPR-EDT TO 
                FRM-NUM-VCOR-ALF IN CPR-FLD(3).
           IF FRM-NUM-VCOR-LEN IN CPR-FLD(4) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-VCOR-LEN IN CPR-FLD(4)
                GO TO FIN-EDT-FLD-CPR.
           MOVE FRM-NUM-VCOR IN CPR-FLD(4) TO 
                FRM-NUM-VCOR-EDT IN CPR-EDT.
           MOVE FRM-NUM-VCOR-EDT IN CPR-EDT TO 
                FRM-NUM-VCOR-ALF IN CPR-FLD(4).
           IF FRM-NUM-VCOR-LEN IN CPR-FLD(5) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-VCOR-LEN IN CPR-FLD(5)
                GO TO FIN-EDT-FLD-CPR.
           MOVE FRM-NUM-VCOR IN CPR-FLD(5) TO 
                FRM-NUM-VCOR-EDT IN CPR-EDT.
           MOVE FRM-NUM-VCOR-EDT IN CPR-EDT TO 
                FRM-NUM-VCOR-ALF IN CPR-FLD(5).
           IF FRM-SGV-TPAG-LEN IN CPR-FLD(1) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-SGV-TPAG-LEN IN CPR-FLD(1)
                GO TO FIN-EDT-FLD-CPR.
           MOVE FRM-SGV-TPAG IN CPR-FLD(1) TO 
                FRM-SGV-TPAG-EDT IN CPR-EDT.
           MOVE FRM-SGV-TPAG-EDT IN CPR-EDT TO 
                FRM-SGV-TPAG-ALF IN CPR-FLD(1).
           IF FRM-SGV-TPAG-LEN IN CPR-FLD(2) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-SGV-TPAG-LEN IN CPR-FLD(2)
                GO TO FIN-EDT-FLD-CPR.
           MOVE FRM-SGV-TPAG IN CPR-FLD(2) TO 
                FRM-SGV-TPAG-EDT IN CPR-EDT.
           MOVE FRM-SGV-TPAG-EDT IN CPR-EDT TO 
