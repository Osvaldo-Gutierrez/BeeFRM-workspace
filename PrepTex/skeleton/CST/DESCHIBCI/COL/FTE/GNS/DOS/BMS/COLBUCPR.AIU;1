*% FRM = "CPR"
      *
      * COLBUCPR
      * Version 3.0.1 Nov-2005
      * 07-Dec-12 05:46 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMCPR.TXT'
       SET-TAG-COLBUCPR SECTION.
       INI-SET-TAG-COLBUCPR.
           MOVE
           '<IDNMOD NOM=$COLBUCPR$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$07-Dec-12 05:46 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCPR.
           EXIT.
           
       RET-ALL-CPR SECTION.
       INI-RET-ALL-CPR.
*% INCLUDE INI_RET_ALL_CPR OF 'COLBMCPR.TXT'
           PERFORM KEY-ALL-CPR.
           PERFORM RET-OPC-CPR.
           PERFORM RET-DBC-CPR.
*% INCLUDE FIN_RET_ALL_CPR OF 'COLBMCPR.TXT'
       FIN-RET-ALL-CPR.
           EXIT.
       
       RET-OPC-CPR SECTION.
       INI-RET-OPC-CPR.
*% INCLUDE INI_RET_OPC_CPR OF 'COLBMCPR.TXT'
*% INCLUDE FIN_RET_OPC_CPR OF 'COLBMCPR.TXT'
       FIN-RET-OPC-CPR.
           EXIT.
       
       RET-DBC-CPR SECTION.
       INI-RET-DBC-CPR.
*% INCLUDE INI_RET_DBC_CPR OF 'COLBMCPR.TXT'
           MOVE DBC-GLS-NOMC IN CPR-FLD TO DBC-GLS-NOMC IN DBC.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN CPR-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN CPR-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN CPR-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN CPR-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
*% INCLUDE FIN_RET_DBC_CPR OF 'COLBMCPR.TXT'
       FIN-RET-DBC-CPR.
           EXIT.
       
       KEY-ALL-CPR SECTION.
       INI-KEY-ALL-CPR.
*% INCLUDE INI_KEY_ALL_CPR OF 'COLBMCPR.TXT'
           PERFORM KEY-OPC-CPR.
           PERFORM KEY-DBC-CPR.
*% INCLUDE FIN_KEY_ALL_CPR OF 'COLBMCPR.TXT'
       FIN-KEY-ALL-CPR.
           EXIT.
       
       KEY-OPC-CPR SECTION.
       INI-KEY-OPC-CPR.
*% INCLUDE INI_KEY_OPC_CPR OF 'COLBMCPR.TXT'
           MOVE OPC-CAI-IOPC IN CPR-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE OPC-IIC-IOPC IN CPR-FLD TO OPC-IIC-IOPC IN OPC.
*% INCLUDE FIN_KEY_OPC_CPR OF 'COLBMCPR.TXT'
       FIN-KEY-OPC-CPR.
           EXIT.
       
       KEY-DBC-CPR SECTION.
       INI-KEY-DBC-CPR.
*% INCLUDE INI_KEY_DBC_CPR OF 'COLBMCPR.TXT'
*% INCLUDE FIN_KEY_DBC_CPR OF 'COLBMCPR.TXT'
       FIN-KEY-DBC-CPR.
           EXIT.
       
       SCR-CLRA-CPR SECTION.
       INI-SCR-CLRA-CPR.
*% INCLUDE INI_SCR_CLRA_CPR OF 'COLBMCPR.TXT'
           PERFORM CLR-KEY-CPR.
           PERFORM CLR-FLD-CPR.
*% INCLUDE FIN_SCR_CLRA_CPR OF 'COLBMCPR.TXT'
       FIN-SCR-CLRA-CPR.
           EXIT.
       
       CLR-KEY-CPR SECTION.
       INI-CLR-KEY-CPR.
*% INCLUDE INI_CLR_KEY_CPR OF 'COLBMCPR.TXT'
           MOVE SPACES TO OPC-CAI-IOPC IN CPR-FLD.
           MOVE ZEROES TO OPC-IIC-IOPC IN CPR-FLD.
           MOVE SPACES TO FRM-COD-TREN IN CPR-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FINI"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FINI IN CPR-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FTER"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FTER IN CPR-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_KEY_CPR OF 'COLBMCPR.TXT'
       FIN-CLR-KEY-CPR.
           EXIT.
       
       CLR-FLD-CPR SECTION.
       INI-CLR-FLD-CPR.
*% INCLUDE INI_CLR_FLD_CPR OF 'COLBMCPR.TXT'
           MOVE SPACES TO FRM-CAI-IOPC IN CPR-FLD(1).
           MOVE SPACES TO FRM-CAI-IOPC IN CPR-FLD(2).
           MOVE SPACES TO FRM-CAI-IOPC IN CPR-FLD(3).
           MOVE SPACES TO FRM-CAI-IOPC IN CPR-FLD(4).
           MOVE SPACES TO FRM-CAI-IOPC IN CPR-FLD(5).
           MOVE ZEROES TO FRM-IIC-IOPC IN CPR-FLD(1).
           MOVE ZEROES TO FRM-IIC-IOPC IN CPR-FLD(2).
           MOVE ZEROES TO FRM-IIC-IOPC IN CPR-FLD(3).
           MOVE ZEROES TO FRM-IIC-IOPC IN CPR-FLD(4).
           MOVE ZEROES TO FRM-IIC-IOPC IN CPR-FLD(5).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FCAN IN CPR-FLD(1).
           MOVE ZEROES TO FRM-FEC-FCAN IN CPR-FLD(2).
           MOVE ZEROES TO FRM-FEC-FCAN IN CPR-FLD(3).
           MOVE ZEROES TO FRM-FEC-FCAN IN CPR-FLD(4).
           MOVE ZEROES TO FRM-FEC-FCAN IN CPR-FLD(5).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-ICAN IN CPR-FLD(1).
           MOVE ZEROES TO FRM-NUM-ICAN IN CPR-FLD(2).
           MOVE ZEROES TO FRM-NUM-ICAN IN CPR-FLD(3).
           MOVE ZEROES TO FRM-NUM-ICAN IN CPR-FLD(4).
           MOVE ZEROES TO FRM-NUM-ICAN IN CPR-FLD(5).
           MOVE ZEROES TO FRM-NUM-IVEN IN CPR-FLD(1).
           MOVE ZEROES TO FRM-NUM-IVEN IN CPR-FLD(2).
           MOVE ZEROES TO FRM-NUM-IVEN IN CPR-FLD(3).
           MOVE ZEROES TO FRM-NUM-IVEN IN CPR-FLD(4).
           MOVE ZEROES TO FRM-NUM-IVEN IN CPR-FLD(5).
           MOVE ZEROES TO FRM-SGV-RENO IN CPR-FLD(1).
           MOVE ZEROES TO FRM-SGV-RENO IN CPR-FLD(2).
           MOVE ZEROES TO FRM-SGV-RENO IN CPR-FLD(3).
           MOVE ZEROES TO FRM-SGV-RENO IN CPR-FLD(4).
           MOVE ZEROES TO FRM-SGV-RENO IN CPR-FLD(5).
           MOVE ZEROES TO FRM-SGV-CAPI IN CPR-FLD(1).
           MOVE ZEROES TO FRM-SGV-CAPI IN CPR-FLD(2).
           MOVE ZEROES TO FRM-SGV-CAPI IN CPR-FLD(3).
           MOVE ZEROES TO FRM-SGV-CAPI IN CPR-FLD(4).
           MOVE ZEROES TO FRM-SGV-CAPI IN CPR-FLD(5).
           MOVE SPACES TO FRM-GLS-VCOC IN CPR-FLD(1).
           MOVE SPACES TO FRM-GLS-VCOC IN CPR-FLD(2).
           MOVE SPACES TO FRM-GLS-VCOC IN CPR-FLD(3).
           MOVE SPACES TO FRM-GLS-VCOC IN CPR-FLD(4).
           MOVE SPACES TO FRM-GLS-VCOC IN CPR-FLD(5).
           MOVE SPACES TO FRM-COD-COOC IN CPR-FLD(1).
           MOVE SPACES TO FRM-COD-COOC IN CPR-FLD(2).
           MOVE SPACES TO FRM-COD-COOC IN CPR-FLD(3).
           MOVE SPACES TO FRM-COD-COOC IN CPR-FLD(4).
           MOVE SPACES TO FRM-COD-COOC IN CPR-FLD(5).
           MOVE SPACES TO FRM-COD-OFIC IN CPR-FLD(1).
           MOVE SPACES TO FRM-COD-OFIC IN CPR-FLD(2).
           MOVE SPACES TO FRM-COD-OFIC IN CPR-FLD(3).
           MOVE SPACES TO FRM-COD-OFIC IN CPR-FLD(4).
           MOVE SPACES TO FRM-COD-OFIC IN CPR-FLD(5).
           MOVE ZEROES TO FRM-SGV-TINT IN CPR-FLD(1).
           MOVE ZEROES TO FRM-SGV-TINT IN CPR-FLD(2).
           MOVE ZEROES TO FRM-SGV-TINT IN CPR-FLD(3).
           MOVE ZEROES TO FRM-SGV-TINT IN CPR-FLD(4).
           MOVE ZEROES TO FRM-SGV-TINT IN CPR-FLD(5).
           MOVE ZEROES TO FRM-NUM-VCOR IN CPR-FLD(1).
           MOVE ZEROES TO FRM-NUM-VCOR IN CPR-FLD(2).
           MOVE ZEROES TO FRM-NUM-VCOR IN CPR-FLD(3).
           MOVE ZEROES TO FRM-NUM-VCOR IN CPR-FLD(4).
           MOVE ZEROES TO FRM-NUM-VCOR IN CPR-FLD(5).
           MOVE SPACES TO FRM-IND-RENO IN CPR-FLD(1).
           MOVE SPACES TO FRM-IND-RENO IN CPR-FLD(2).
           MOVE SPACES TO FRM-IND-RENO IN CPR-FLD(3).
           MOVE SPACES TO FRM-IND-RENO IN CPR-FLD(4).
           MOVE SPACES TO FRM-IND-RENO IN CPR-FLD(5).
           MOVE SPACES TO FRM-COD-RCAN IN CPR-FLD(1).
           MOVE SPACES TO FRM-COD-RCAN IN CPR-FLD(2).
           MOVE SPACES TO FRM-COD-RCAN IN CPR-FLD(3).
           MOVE SPACES TO FRM-COD-RCAN IN CPR-FLD(4).
           MOVE SPACES TO FRM-COD-RCAN IN CPR-FLD(5).
           MOVE ZEROES TO FRM-SGV-TPAG IN CPR-FLD(1).
           MOVE ZEROES TO FRM-SGV-TPAG IN CPR-FLD(2).
           MOVE ZEROES TO FRM-SGV-TPAG IN CPR-FLD(3).
           MOVE ZEROES TO FRM-SGV-TPAG IN CPR-FLD(4).
           MOVE ZEROES TO FRM-SGV-TPAG IN CPR-FLD(5).
           PERFORM CLR-OPC-CPR.
           PERFORM CLR-DBC-CPR.
*% INCLUDE FIN_CLR_FLD_CPR OF 'COLBMCPR.TXT'
       FIN-CLR-FLD-CPR.
           EXIT.
       
       CLR-OPC-CPR SECTION.
       INI-CLR-OPC-CPR.
*% INCLUDE INI_CLR_OPC_CPR OF 'COLBMCPR.TXT'
*% INCLUDE FIN_CLR_OPC_CPR OF 'COLBMCPR.TXT'
       FIN-CLR-OPC-CPR.
           EXIT.
       
       CLR-DBC-CPR SECTION.
       INI-CLR-DBC-CPR.
*% INCLUDE INI_CLR_DBC_CPR OF 'COLBMCPR.TXT'
           MOVE SPACES TO DBC-GLS-NOMC IN CPR-FLD.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN CPR-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN CPR-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN CPR-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN CPR-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_DBC_CPR OF 'COLBMCPR.TXT'
       FIN-CLR-DBC-CPR.
           EXIT.
       
       FST-KEY-CPR SECTION.
       INI-FST-KEY-CPR.
*% INCLUDE INI_FST_KEY_CPR OF 'COLBMCPR.TXT'
           MOVE -1 TO OPC-CAI-IOPC-LEN IN CPR-FLD.
*% INCLUDE FIN_FST_KEY_CPR OF 'COLBMCPR.TXT'
       FIN-FST-KEY-CPR.
           EXIT.
       
       FST-FLD-CPR SECTION.
       INI-FST-FLD-CPR.
*% INCLUDE INI_FST_FLD_CPR OF 'COLBMCPR.TXT'
*% INCLUDE FIN_FST_FLD_CPR OF 'COLBMCPR.TXT'
       FIN-FST-FLD-CPR.
           EXIT.
       
       PRO-KEY-CPR SECTION.
       INI-PRO-KEY-CPR.
*% INCLUDE INI_PRO_KEY_CPR OF 'COLBMCPR.TXT'
           MOVE FRM-CPIM-AEY TO FRM-COD-TREN-ATR IN CPR-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FINI"
           MOVE FRM-CPIM-AEY TO FRM-FEC-FINI-ATR IN CPR-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FTER"
           MOVE FRM-CPIM-AEY TO FRM-FEC-FTER-ATR IN CPR-FLD.
           PERFORM PRO-MKY-CPR.
*% INCLUDE FIN_PRO_KEY_CPR OF 'COLBMCPR.TXT'
       FIN-PRO-KEY-CPR.
           EXIT.
       
       PRO-MKY-CPR SECTION.
       INI-PRO-MKY-CPR.
*% INCLUDE INI_PRO_MKY_CPR OF 'COLBMCPR.TXT'
*% INCLUDE FIN_PRO_MKY_CPR OF 'COLBMCPR.TXT'
       FIN-PRO-MKY-CPR.
           EXIT.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FINI"
           MOVE FRM-CPIM-AEY TO FRM-FEC-FINI-ATR IN CPR-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FTER"
           MOVE FRM-CPIM-AEY TO FRM-FEC-FTER-ATR IN CPR-FLD.
       
       PRO-FLD-CPR SECTION.
       INI-PRO-FLD-CPR.
*% INCLUDE INI_PRO_FLD_CPR OF 'COLBMCPR.TXT'
           PERFORM PRO-OPC-CPR.
           PERFORM PRO-DBC-CPR.
*% INCLUDE FIN_PRO_FLD_CPR OF 'COLBMCPR.TXT'
       FIN-PRO-FLD-CPR.
           EXIT.
       
       PRO-OPC-CPR SECTION.
       INI-PRO-OPC-CPR.
*% INCLUDE INI_PRO_OPC_CPR OF 'COLBMCPR.TXT'
*% INCLUDE FIN_PRO_OPC_CPR OF 'COLBMCPR.TXT'
       FIN-PRO-OPC-CPR.
           EXIT.
       
       PRO-DBC-CPR SECTION.
       INI-PRO-DBC-CPR.
*% INCLUDE INI_PRO_DBC_CPR OF 'COLBMCPR.TXT'
*% INCLUDE FIN_PRO_DBC_CPR OF 'COLBMCPR.TXT'
       FIN-PRO-DBC-CPR.
           EXIT.
       
       UNP-KEY-CPR SECTION.
       INI-UNP-KEY-CPR.
*% INCLUDE INI_UNP_KEY_CPR OF 'COLBMCPR.TXT'
           MOVE FRM-CPIM-UBY TO FRM-COD-TREN-ATR IN CPR-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FINI"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FINI-ATR IN CPR-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FTER"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FTER-ATR IN CPR-FLD.
           PERFORM UNP-IKY-CPR.
           PERFORM UNP-AKY-CPR.
           PERFORM UNP-MKY-CPR.
*% INCLUDE FIN_UNP_KEY_CPR OF 'COLBMCPR.TXT'
       FIN-UNP-KEY-CPR.
           EXIT.
       
       UNP-IKY-CPR SECTION.
       INI-UNP-IKY-CPR.
*% INCLUDE INI_UNP_IKY_CPR OF 'COLBMCPR.TXT'
           MOVE FRM-CPIM-UBY TO OPC-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO OPC-IIC-IOPC-ATR.
*% INCLUDE FIN_UNP_IKY_CPR OF 'COLBMCPR.TXT'
       FIN-UNP-IKY-CPR.
           EXIT.
       
       UNP-MKY-CPR SECTION.
       INI-UNP-MKY-CPR.
*% INCLUDE INI_UNP_MKY_CPR OF 'COLBMCPR.TXT'
*% INCLUDE FIN_UNP_MKY_CPR OF 'COLBMCPR.TXT'
       FIN-UNP-MKY-CPR.
           EXIT.
       
       UNP-AKY-CPR SECTION.
       INI-UNP-AKY-CPR.
*% INCLUDE INI_UNP_AKY_CPR OF 'COLBMCPR.TXT'
           MOVE FRM-CPIM-UBY TO FRM-COD-TREN-ATR.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FINI"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FINI-ATR IN CPR-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FTER"
           MOVE FRM-CPIM-UBY TO FRM-FEC-FTER-ATR IN CPR-FLD.
*% INCLUDE FIN_UNP_AKY_CPR OF 'COLBMCPR.TXT'
       FIN-UNP-AKY-CPR.
           EXIT.
       
       UNP-FLD-CPR SECTION.
       INI-UNP-FLD-CPR.
*% INCLUDE INI_UNP_FLD_CPR OF 'COLBMCPR.TXT'
           PERFORM UNP-OPC-CPR.
           PERFORM UNP-DBC-CPR.
*% INCLUDE FIN_UNP_FLD_CPR OF 'COLBMCPR.TXT'
       FIN-UNP-FLD-CPR.
           EXIT.
       
       UNP-OPC-CPR SECTION.
       INI-UNP-OPC-CPR.
*% INCLUDE INI_UNP_OPC_CPR OF 'COLBMCPR.TXT'
*% INCLUDE FIN_UNP_OPC_CPR OF 'COLBMCPR.TXT'
       FIN-UNP-OPC-CPR.
           EXIT.
       
       UNP-DBC-CPR SECTION.
       INI-UNP-DBC-CPR.
*% INCLUDE INI_UNP_DBC_CPR OF 'COLBMCPR.TXT'
*% INCLUDE FIN_UNP_DBC_CPR OF 'COLBMCPR.TXT'
       FIN-UNP-DBC-CPR.
           EXIT.
       
       VAL-MKY-CPR SECTION.
       INI-VAL-MKY-CPR.
*% INCLUDE INI_VAL_MKY_CPR OF 'COLBMCPR.TXT'
*% INCLUDE FIN_VAL_MKY_CPR OF 'COLBMCPR.TXT'
       FIN-VAL-MKY-CPR.
           EXIT.
       
       VAL-FLD-CPR SECTION.
       INI-VAL-FLD-CPR.
*% INCLUDE INI_VAL_FLD_CPR OF 'COLBMCPR.TXT'
           PERFORM VAL-DBC-GLS-NOMC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DBC-GLS-NOMC-LEN IN CPR-FLD
                GO TO FIN-VAL-FLD-CPR.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * VRF-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * VRF-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DBC-IND-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-IND-ICLI-LEN IN CPR-FLD
                GO TO FIN-VAL-FLD-CPR.
           PERFORM VAL-DBC-GLS-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-GLS-ICLI-LEN IN CPR-FLD
                GO TO FIN-VAL-FLD-CPR.
           PERFORM VAL-DBC-NUM-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-NUM-ICLI-LEN IN CPR-FLD
                GO TO FIN-VAL-FLD-CPR.
           PERFORM VAL-DBC-VRF-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-VRF-ICLI-LEN IN CPR-FLD
                GO TO FIN-VAL-FLD-CPR.
      * IDC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-CAI-IOPC
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-CAI-IOPC-LEN IN CPR-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPR.
           PERFORM VAL-FRM-IIC-IOPC
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-IIC-IOPC-LEN IN CPR-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPR.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAN"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FCAN VARYING FRM-IFLD
                FROM 1 BY 1 UNTIL FRM-IFLD > 5 OR
                FRM-SUAR = FRM-SUAR-MAL OR
                FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FCAN-LEN IN CPR-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPR.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-NUM-ICAN
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-NUM-ICAN-LEN IN CPR-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPR.
           PERFORM VAL-FRM-NUM-IVEN
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-NUM-IVEN-LEN IN CPR-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPR.
           PERFORM VAL-FRM-SGV-RENO
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-RENO-LEN IN CPR-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPR.
           PERFORM VAL-FRM-SGV-CAPI
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-CAPI-LEN IN CPR-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPR.
           PERFORM VAL-FRM-GLS-VCOC
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-GLS-VCOC-LEN IN CPR-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPR.
           PERFORM VAL-FRM-COD-COOC
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-COD-COOC-LEN IN CPR-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPR.
           PERFORM VAL-FRM-COD-OFIC
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-COD-OFIC-LEN IN CPR-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPR.
           PERFORM VAL-FRM-SGV-TINT
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-TINT-LEN IN CPR-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPR.
           PERFORM VAL-FRM-NUM-VCOR
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-NUM-VCOR-LEN IN CPR-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPR.
           PERFORM VAL-FRM-IND-RENO
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-IND-RENO-LEN IN CPR-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPR.
           PERFORM VAL-FRM-COD-RCAN
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-COD-RCAN-LEN IN CPR-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPR.
           PERFORM VAL-FRM-SGV-TPAG
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 5
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-SGV-TPAG-LEN IN CPR-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CPR.
*% INCLUDE FIN_VAL_FLD_CPR OF 'COLBMCPR.TXT'
       FIN-VAL-FLD-CPR.
           EXIT.
       
