       EDT-FLD-CPR SECTION.
       INI-EDT-FLD-CPR.
*% INCLUDE EDT_FLD_CPR OF 'COLBMCPR.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF DBC-NUM-ICLI-LEN IN CPR-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DBC-NUM-ICLI-LEN IN CPR-FLD
                GO TO FIN-EDT-FLD-CPR.
           MOVE DBC-NUM-ICLI IN CPR-FLD TO
                DBC-NUM-ICLI-EDT IN CPR-EDT.
           MOVE DBC-NUM-ICLI-EDT IN CPR-EDT TO
                DBC-NUM-ICLI-ALF IN CPR-FLD.
      * IDC-FIN --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF FRM-IIC-IOPC-LEN IN CPR-FLD(1) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-IIC-IOPC-LEN IN CPR-FLD(1)
                GO TO FIN-EDT-FLD-CPR.
           MOVE FRM-IIC-IOPC IN CPR-FLD(1) TO 
                FRM-IIC-IOPC-EDT IN CPR-EDT.
           MOVE FRM-IIC-IOPC-EDT IN CPR-EDT TO 
                FRM-IIC-IOPC-ALF IN CPR-FLD(1).
           IF FRM-IIC-IOPC-LEN IN CPR-FLD(2) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-IIC-IOPC-LEN IN CPR-FLD(2)
                GO TO FIN-EDT-FLD-CPR.
           MOVE FRM-IIC-IOPC IN CPR-FLD(2) TO 
                FRM-IIC-IOPC-EDT IN CPR-EDT.
           MOVE FRM-IIC-IOPC-EDT IN CPR-EDT TO 
                FRM-IIC-IOPC-ALF IN CPR-FLD(2).
           IF FRM-IIC-IOPC-LEN IN CPR-FLD(3) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-IIC-IOPC-LEN IN CPR-FLD(3)
                GO TO FIN-EDT-FLD-CPR.
           MOVE FRM-IIC-IOPC IN CPR-FLD(3) TO 
                FRM-IIC-IOPC-EDT IN CPR-EDT.
           MOVE FRM-IIC-IOPC-EDT IN CPR-EDT TO 
                FRM-IIC-IOPC-ALF IN CPR-FLD(3).
           IF FRM-IIC-IOPC-LEN IN CPR-FLD(4) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-IIC-IOPC-LEN IN CPR-FLD(4)
                GO TO FIN-EDT-FLD-CPR.
           MOVE FRM-IIC-IOPC IN CPR-FLD(4) TO 
                FRM-IIC-IOPC-EDT IN CPR-EDT.
           MOVE FRM-IIC-IOPC-EDT IN CPR-EDT TO 
                FRM-IIC-IOPC-ALF IN CPR-FLD(4).
           IF FRM-IIC-IOPC-LEN IN CPR-FLD(5) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-IIC-IOPC-LEN IN CPR-FLD(5)
                GO TO FIN-EDT-FLD-CPR.
           MOVE FRM-IIC-IOPC IN CPR-FLD(5) TO 
                FRM-IIC-IOPC-EDT IN CPR-EDT.
           MOVE FRM-IIC-IOPC-EDT IN CPR-EDT TO 
                FRM-IIC-IOPC-ALF IN CPR-FLD(5).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCAN"
           IF FRM-NUM-ICAN-LEN IN CPR-FLD(1) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-ICAN-LEN IN CPR-FLD(1)
                GO TO FIN-EDT-FLD-CPR.
           MOVE FRM-NUM-ICAN IN CPR-FLD(1) TO 
                FRM-NUM-ICAN-EDT IN CPR-EDT.
           MOVE FRM-NUM-ICAN-EDT IN CPR-EDT TO 
                FRM-NUM-ICAN-ALF IN CPR-FLD(1).
           IF FRM-NUM-ICAN-LEN IN CPR-FLD(2) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-ICAN-LEN IN CPR-FLD(2)
                GO TO FIN-EDT-FLD-CPR.
