       VAL-MKY-CDC SECTION.
       INI-VAL-MKY-CDC.
*% INCLUDE INI_VAL_MKY_CDC OF 'COLBMCDC.TXT'
*% INCLUDE FIN_VAL_MKY_CDC OF 'COLBMCDC.TXT'
       FIN-VAL-MKY-CDC.
           EXIT.
       
       VAL-FLD-CDC SECTION.
       INI-VAL-FLD-CDC.
*% INCLUDE INI_VAL_FLD_CDC OF 'COLBMCDC.TXT'
           PERFORM VAL-OPC-COD-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-VCOC-LEN IN CDC-FLD
                GO TO FIN-VAL-FLD-CDC.
           PERFORM VAL-OPC-COD-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-TOOC-LEN IN CDC-FLD
                GO TO FIN-VAL-FLD-CDC.
           PERFORM VAL-OPC-COD-OFOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-OFOC-LEN IN CDC-FLD
                GO TO FIN-VAL-FLD-CDC.
           PERFORM VAL-FRM-GLS-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-VCOC-LEN IN CDC-FLD
                GO TO FIN-VAL-FLD-CDC.
           PERFORM VAL-FRM-GLS-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TOOC-LEN IN CDC-FLD
                GO TO FIN-VAL-FLD-CDC.
           PERFORM VAL-FRM-GLS-OFOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-OFOC-LEN IN CDC-FLD
                GO TO FIN-VAL-FLD-CDC.
           PERFORM VAL-FRM-IND-SGTE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IND-SGTE-LEN IN CDC-FLD
                GO TO FIN-VAL-FLD-CDC.
           PERFORM VAL-FRM-NUM-DEUD
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 7
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-NUM-DEUD-LEN IN CDC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDC.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * VRF-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * VRF-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-IND-ICLI VARYING FRM-IFLD
                FROM 1 BY 1 UNTIL FRM-IFLD > 7 OR
                FRM-SUAR = FRM-SUAR-MAL OR
                FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO
                     FRM-IND-ICLI-LEN IN CDC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDC.
           PERFORM VAL-FRM-GLS-ICLI VARYING FRM-IFLD
                FROM 1 BY 1 UNTIL FRM-IFLD > 7 OR
                FRM-SUAR = FRM-SUAR-MAL OR
                FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO
                     FRM-GLS-ICLI-LEN IN CDC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDC.
           PERFORM VAL-FRM-NUM-ICLI VARYING FRM-IFLD
                FROM 1 BY 1 UNTIL FRM-IFLD > 7 OR
                FRM-SUAR = FRM-SUAR-MAL OR
                FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO
                     FRM-NUM-ICLI-LEN IN CDC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDC.
           PERFORM VAL-FRM-VRF-ICLI VARYING FRM-IFLD
                FROM 1 BY 1 UNTIL FRM-IFLD > 7 OR
                FRM-SUAR = FRM-SUAR-MAL OR
                FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO
                     FRM-VRF-ICLI-LEN IN CDC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDC.

      * IDC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-VAL-LIMD
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 7
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-LIMD-LEN IN CDC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDC.
           PERFORM VAL-FRM-VAL-MONL
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 7
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-VAL-MONL-LEN IN CDC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDC.
           PERFORM VAL-FRM-NUM-DIRE
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 7
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-NUM-DIRE-LEN IN CDC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDC.
           PERFORM VAL-FRM-GLS-NOMC
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 7
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-GLS-NOMC-LEN IN CDC-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-CDC.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLT"
      * VRF-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * VRF-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLT"
      * IDC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-IND-ICLT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-IND-ICLT-LEN IN CDC-FLD
                GO TO FIN-VAL-FLD-CDC.
           PERFORM VAL-FRM-GLS-ICLT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-GLS-ICLT-LEN IN CDC-FLD
                GO TO FIN-VAL-FLD-CDC.
           PERFORM VAL-FRM-NUM-ICLT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-NUM-ICLT-LEN IN CDC-FLD
                GO TO FIN-VAL-FLD-CDC.
           PERFORM VAL-FRM-VRF-ICLT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-VRF-ICLT-LEN IN CDC-FLD
                GO TO FIN-VAL-FLD-CDC.
      * IDC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% INCLUDE FIN_VAL_FLD_CDC OF 'COLBMCDC.TXT'
       FIN-VAL-FLD-CDC.
           EXIT.
       
