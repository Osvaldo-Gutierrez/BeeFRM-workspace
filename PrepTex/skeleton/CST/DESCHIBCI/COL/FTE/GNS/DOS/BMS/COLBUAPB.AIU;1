       VAL-MKY-APB SECTION.
       INI-VAL-MKY-APB.
*% INCLUDE INI_VAL_MKY_APB OF 'COLBMAPB.TXT'
*% INCLUDE FIN_VAL_MKY_APB OF 'COLBMAPB.TXT'
       FIN-VAL-MKY-APB.
           EXIT.
       
       VAL-FLD-APB SECTION.
       INI-VAL-FLD-APB.
*% INCLUDE INI_VAL_FLD_APB OF 'COLBMAPB.TXT'
           PERFORM VAL-APB-NUM-PARA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO APB-NUM-PARA-LEN IN APB-FLD
                GO TO FIN-VAL-FLD-APB.
           PERFORM VAL-FRM-GLS-PRO1.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-PRO1-LEN IN APB-FLD
                GO TO FIN-VAL-FLD-APB.
           PERFORM VAL-FRM-GLS-PRO2.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-PRO2-LEN IN APB-FLD
                GO TO FIN-VAL-FLD-APB.
           PERFORM VAL-FRM-GLS-PARA
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 10
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-GLS-PARA-LEN IN APB-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-APB.
           PERFORM VAL-FRM-COD-PARA
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 10
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE FRM-IND-ERRO TO 
                FRM-COD-PARA-LEN IN APB-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-APB.
*% INCLUDE FIN_VAL_FLD_APB OF 'COLBMAPB.TXT'
       FIN-VAL-FLD-APB.
           EXIT.
       
       VAL-APB-NUM-PARA SECTION.
       INI-VAL-APB-NUM-PARA.
*% LBL = "APB_NUM_PARA OF 'COLBMAPB.TXT'"
*% INCLUDE INI_APB_NUM_PARA OF 'COLBMAPB.TXT'
*% INCLUDE APB_NUM_PARA OF 'COLBMAPB.TXT'
*% INCLUDE FIN_APB_NUM_PARA OF 'COLBMAPB.TXT'
       FIN-VAL-APB-NUM-PARA.
           EXIT.
       
       VAL-FRM-GLS-PRO1 SECTION.
       INI-VAL-FRM-GLS-PRO1.
*% LBL = "FRM_GLS_PRO1 OF 'COLBMAPB.TXT'"
*% INCLUDE INI_FRM_GLS_PRO1 OF 'COLBMAPB.TXT'
