*% INI_LBL = TRUE
*% PGM_BU = TRUE
       RET-ALL-APB SECTION.
       INI-RET-ALL-APB.
*% INCLUDE INI_RET_ALL_APB OF 'COLBMAPB.TXT'
           PERFORM KEY-ALL-APB.
           PERFORM RET-APB-APB.
*% INCLUDE FIN_RET_ALL_APB OF 'COLBMAPB.TXT'
       FIN-RET-ALL-APB.
           EXIT.
       
       RET-APB-APB SECTION.
       INI-RET-APB-APB.
*% INCLUDE INI_RET_APB_APB OF 'COLBMAPB.TXT'
           MOVE APB-NUM-PARA IN APB-FLD TO APB-NUM-PARA IN APB.
*% INCLUDE FIN_RET_APB_APB OF 'COLBMAPB.TXT'
       FIN-RET-APB-APB.
           EXIT.
       
       KEY-ALL-APB SECTION.
       INI-KEY-ALL-APB.
*% INCLUDE INI_KEY_ALL_APB OF 'COLBMAPB.TXT'
           PERFORM KEY-APB-APB.
*% INCLUDE FIN_KEY_ALL_APB OF 'COLBMAPB.TXT'
       FIN-KEY-ALL-APB.
           EXIT.
       
       KEY-APB-APB SECTION.
       INI-KEY-APB-APB.
*% INCLUDE INI_KEY_APB_APB OF 'COLBMAPB.TXT'
           MOVE APB-COD-PROG IN APB-FLD TO APB-COD-PROG IN APB.
*% INCLUDE FIN_KEY_APB_APB OF 'COLBMAPB.TXT'
       FIN-KEY-APB-APB.
           EXIT.
       
       SCR-CLRA-APB SECTION.
       INI-SCR-CLRA-APB.
*% INCLUDE INI_SCR_CLRA_APB OF 'COLBMAPB.TXT'
           PERFORM CLR-KEY-APB.
           PERFORM CLR-FLD-APB.
*% INCLUDE FIN_SCR_CLRA_APB OF 'COLBMAPB.TXT'
       FIN-SCR-CLRA-APB.
           EXIT.
       
       CLR-KEY-APB SECTION.
       INI-CLR-KEY-APB.
*% INCLUDE INI_CLR_KEY_APB OF 'COLBMAPB.TXT'
           MOVE SPACES TO APB-COD-PROG IN APB-FLD.
*% INCLUDE FIN_CLR_KEY_APB OF 'COLBMAPB.TXT'
       FIN-CLR-KEY-APB.
           EXIT.
       
       CLR-FLD-APB SECTION.
       INI-CLR-FLD-APB.
*% INCLUDE INI_CLR_FLD_APB OF 'COLBMAPB.TXT'
           MOVE SPACES TO FRM-GLS-PRO1 IN APB-FLD.
           MOVE SPACES TO FRM-GLS-PRO2 IN APB-FLD.
           MOVE SPACES TO FRM-GLS-PARA IN APB-FLD(1).
           MOVE SPACES TO FRM-GLS-PARA IN APB-FLD(2).
           MOVE SPACES TO FRM-GLS-PARA IN APB-FLD(3).
           MOVE SPACES TO FRM-GLS-PARA IN APB-FLD(4).
           MOVE SPACES TO FRM-GLS-PARA IN APB-FLD(5).
           MOVE SPACES TO FRM-GLS-PARA IN APB-FLD(6).
           MOVE SPACES TO FRM-GLS-PARA IN APB-FLD(7).
           MOVE SPACES TO FRM-GLS-PARA IN APB-FLD(8).
           MOVE SPACES TO FRM-GLS-PARA IN APB-FLD(9).
           MOVE SPACES TO FRM-GLS-PARA IN APB-FLD(10).
           MOVE SPACES TO FRM-COD-PARA IN APB-FLD(1).
           MOVE SPACES TO FRM-COD-PARA IN APB-FLD(2).
           MOVE SPACES TO FRM-COD-PARA IN APB-FLD(3).
           MOVE SPACES TO FRM-COD-PARA IN APB-FLD(4).
           MOVE SPACES TO FRM-COD-PARA IN APB-FLD(5).
           MOVE SPACES TO FRM-COD-PARA IN APB-FLD(6).
           MOVE SPACES TO FRM-COD-PARA IN APB-FLD(7).
           MOVE SPACES TO FRM-COD-PARA IN APB-FLD(8).
           MOVE SPACES TO FRM-COD-PARA IN APB-FLD(9).
           MOVE SPACES TO FRM-COD-PARA IN APB-FLD(10).
           PERFORM CLR-APB-APB.
*% INCLUDE FIN_CLR_FLD_APB OF 'COLBMAPB.TXT'
       FIN-CLR-FLD-APB.
           EXIT.
       
       CLR-APB-APB SECTION.
       INI-CLR-APB-APB.
*% INCLUDE INI_CLR_APB_APB OF 'COLBMAPB.TXT'
           MOVE ZEROES TO APB-NUM-PARA IN APB-FLD.
*% INCLUDE FIN_CLR_APB_APB OF 'COLBMAPB.TXT'
       FIN-CLR-APB-APB.
           EXIT.
       
       FST-KEY-APB SECTION.
       INI-FST-KEY-APB.
*% INCLUDE INI_FST_KEY_APB OF 'COLBMAPB.TXT'
           MOVE -1 TO APB-COD-PROG-LEN IN APB-FLD.
*% INCLUDE FIN_FST_KEY_APB OF 'COLBMAPB.TXT'
       FIN-FST-KEY-APB.
           EXIT.
       
       FST-FLD-APB SECTION.
       INI-FST-FLD-APB.
*% INCLUDE INI_FST_FLD_APB OF 'COLBMAPB.TXT'
           MOVE -1 TO FRM-GLS-PARA-LEN IN APB-FLD(1).
*% INCLUDE FIN_FST_FLD_APB OF 'COLBMAPB.TXT'
       FIN-FST-FLD-APB.
           EXIT.
       
       PRO-KEY-APB SECTION.
       INI-PRO-KEY-APB.
*% INCLUDE INI_PRO_KEY_APB OF 'COLBMAPB.TXT'
           PERFORM PRO-MKY-APB.
*% INCLUDE FIN_PRO_KEY_APB OF 'COLBMAPB.TXT'
       FIN-PRO-KEY-APB.
           EXIT.
       
       PRO-MKY-APB SECTION.
       INI-PRO-MKY-APB.
*% INCLUDE INI_PRO_MKY_APB OF 'COLBMAPB.TXT'
*% INCLUDE FIN_PRO_MKY_APB OF 'COLBMAPB.TXT'
       FIN-PRO-MKY-APB.
           EXIT.
       
       PRO-FLD-APB SECTION.
       INI-PRO-FLD-APB.
*% INCLUDE INI_PRO_FLD_APB OF 'COLBMAPB.TXT'
           MOVE FRM-CPIM-AEY TO FRM-GLS-PARA-ATR IN APB-FLD(1).
           MOVE FRM-CPIM-AEY TO FRM-GLS-PARA-ATR IN APB-FLD(2).
           MOVE FRM-CPIM-AEY TO FRM-GLS-PARA-ATR IN APB-FLD(3).
           MOVE FRM-CPIM-AEY TO FRM-GLS-PARA-ATR IN APB-FLD(4).
           MOVE FRM-CPIM-AEY TO FRM-GLS-PARA-ATR IN APB-FLD(5).
           MOVE FRM-CPIM-AEY TO FRM-GLS-PARA-ATR IN APB-FLD(6).
           MOVE FRM-CPIM-AEY TO FRM-GLS-PARA-ATR IN APB-FLD(7).
           MOVE FRM-CPIM-AEY TO FRM-GLS-PARA-ATR IN APB-FLD(8).
           MOVE FRM-CPIM-AEY TO FRM-GLS-PARA-ATR IN APB-FLD(9).
           MOVE FRM-CPIM-AEY TO FRM-GLS-PARA-ATR IN APB-FLD(10).
           MOVE FRM-CPIM-AEY TO FRM-COD-PARA-ATR IN APB-FLD(1).
           MOVE FRM-CPIM-AEY TO FRM-COD-PARA-ATR IN APB-FLD(2).
           MOVE FRM-CPIM-AEY TO FRM-COD-PARA-ATR IN APB-FLD(3).
           MOVE FRM-CPIM-AEY TO FRM-COD-PARA-ATR IN APB-FLD(4).
           MOVE FRM-CPIM-AEY TO FRM-COD-PARA-ATR IN APB-FLD(5).
           MOVE FRM-CPIM-AEY TO FRM-COD-PARA-ATR IN APB-FLD(6).
           MOVE FRM-CPIM-AEY TO FRM-COD-PARA-ATR IN APB-FLD(7).
           MOVE FRM-CPIM-AEY TO FRM-COD-PARA-ATR IN APB-FLD(8).
           MOVE FRM-CPIM-AEY TO FRM-COD-PARA-ATR IN APB-FLD(9).
           MOVE FRM-CPIM-AEY TO FRM-COD-PARA-ATR IN APB-FLD(10).
           PERFORM PRO-APB-APB.
*% INCLUDE FIN_PRO_FLD_APB OF 'COLBMAPB.TXT'
       FIN-PRO-FLD-APB.
           EXIT.
       
       PRO-APB-APB SECTION.
       INI-PRO-APB-APB.
*% INCLUDE INI_PRO_APB_APB OF 'COLBMAPB.TXT'
*% INCLUDE FIN_PRO_APB_APB OF 'COLBMAPB.TXT'
       FIN-PRO-APB-APB.
