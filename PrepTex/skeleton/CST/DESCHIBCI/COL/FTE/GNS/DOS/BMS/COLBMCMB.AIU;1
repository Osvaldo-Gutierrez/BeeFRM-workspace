      *
      * COLBQCMB
      * Version 3.0.1 Nov-2005
      * 24-Mar-08 05:18 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "CMB"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMCMB.TXT'
       SET-TAG-COLBQCMB SECTION.
       INI-SET-TAG-COLBQCMB.
           MOVE
           '<IDNMOD NOM=$COLBQCMB$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$24-Mar-08 05:18 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCMB.
           EXIT.
           
       PUT-ALL-CMB SECTION.
       INI-PUT-ALL-CMB.
*% INCLUDE INI_PUT_ALL_CMB OF 'COLBMCMB.TXT'
           PERFORM PUT-OPC-CMB.
           PERFORM PUT-DLC-CMB.
*% INCLUDE FIN_PUT_ALL_CMB OF 'COLBMCMB.TXT'
       FIN-PUT-ALL-CMB.
           EXIT.
       
       PUT-OPC-CMB SECTION.
       INI-PUT-OPC-CMB.
*% INCLUDE INI_PUT_OPC_CMB OF 'COLBMCMB.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-CMB.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-CMB
               GO TO FIN-PUT-OPC-CMB.
           MOVE OPC-COD-TOOC IN OPC TO OPC-COD-TOOC IN CMB-FLD.
           MOVE OPC-COD-AOOC IN OPC TO OPC-COD-AOOC IN CMB-FLD.
*% INCLUDE FIN_PUT_OPC_CMB OF 'COLBMCMB.TXT'
       FIN-PUT-OPC-CMB.
           EXIT.
       
       PUT-DLC-CMB SECTION.
       INI-PUT-DLC-CMB.
*% INCLUDE INI_PUT_DLC_CMB OF 'COLBMCMB.TXT'
           IF PGM-STAT-DLC-NUL
               GO TO FIN-PUT-DLC-CMB.
           IF NOT PGM-STAT-DLC-OKS
               PERFORM CLR-DLC-CMB
               GO TO FIN-PUT-DLC-CMB.
           MOVE DLC-IND-ESTA IN DLC TO DLC-IND-ESTA IN CMB-FLD.
           MOVE DLC-IND-ETAP IN DLC TO DLC-IND-ETAP IN CMB-FLD.
*% INCLUDE FIN_PUT_DLC_CMB OF 'COLBMCMB.TXT'
       FIN-PUT-DLC-CMB.
           EXIT.
      *
      * COLBUCMB
      * Version 3.0.1 Nov-2005
      * 24-Mar-08 05:18 PM
      *
*% INI_LBL = TRUE
*% PGM_BU = TRUE
*% INCLUDE SETTAG OF 'COLBMCMB.TXT'
       SET-TAG-COLBUCMB SECTION.
       INI-SET-TAG-COLBUCMB.
           MOVE
           '<IDNMOD NOM=$COLBUCMB$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$24-Mar-08 05:18 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCMB.
           EXIT.
           
       RET-ALL-CMB SECTION.
       INI-RET-ALL-CMB.
*% INCLUDE INI_RET_ALL_CMB OF 'COLBMCMB.TXT'
           PERFORM KEY-ALL-CMB.
           PERFORM RET-OPC-CMB.
           PERFORM RET-DLC-CMB.
*% INCLUDE FIN_RET_ALL_CMB OF 'COLBMCMB.TXT'
       FIN-RET-ALL-CMB.
           EXIT.
       
       RET-OPC-CMB SECTION.
       INI-RET-OPC-CMB.
*% INCLUDE INI_RET_OPC_CMB OF 'COLBMCMB.TXT'
*% INCLUDE FIN_RET_OPC_CMB OF 'COLBMCMB.TXT'
       FIN-RET-OPC-CMB.
           EXIT.
       
       RET-DLC-CMB SECTION.
       INI-RET-DLC-CMB.
*% INCLUDE INI_RET_DLC_CMB OF 'COLBMCMB.TXT'
*% INCLUDE FIN_RET_DLC_CMB OF 'COLBMCMB.TXT'
       FIN-RET-DLC-CMB.
           EXIT.
       
       KEY-ALL-CMB SECTION.
       INI-KEY-ALL-CMB.
*% INCLUDE INI_KEY_ALL_CMB OF 'COLBMCMB.TXT'
           PERFORM KEY-OPC-CMB.
           PERFORM KEY-DLC-CMB.
*% INCLUDE FIN_KEY_ALL_CMB OF 'COLBMCMB.TXT'
       FIN-KEY-ALL-CMB.
           EXIT.
       
       KEY-OPC-CMB SECTION.
       INI-KEY-OPC-CMB.
*% INCLUDE INI_KEY_OPC_CMB OF 'COLBMCMB.TXT'
           MOVE OPC-COD-TOOC IN CMB-FLD TO OPC-COD-TOOC IN OPC.
           MOVE OPC-COD-AOOC IN CMB-FLD TO OPC-COD-AOOC IN OPC.
*% INCLUDE FIN_KEY_OPC_CMB OF 'COLBMCMB.TXT'
       FIN-KEY-OPC-CMB.
           EXIT.
       
       KEY-DLC-CMB SECTION.
       INI-KEY-DLC-CMB.
*% INCLUDE INI_KEY_DLC_CMB OF 'COLBMCMB.TXT'
           MOVE DLC-IND-ESTA IN CMB-FLD TO DLC-IND-ESTA IN DLC.
           MOVE DLC-IND-ETAP IN CMB-FLD TO DLC-IND-ETAP IN DLC.
*% INCLUDE FIN_KEY_DLC_CMB OF 'COLBMCMB.TXT'
       FIN-KEY-DLC-CMB.
           EXIT.
       
       SCR-CLRA-CMB SECTION.
       INI-SCR-CLRA-CMB.
*% INCLUDE INI_SCR_CLRA_CMB OF 'COLBMCMB.TXT'
           PERFORM CLR-KEY-CMB.
           PERFORM CLR-FLD-CMB.
*% INCLUDE FIN_SCR_CLRA_CMB OF 'COLBMCMB.TXT'
       FIN-SCR-CLRA-CMB.
           EXIT.
       
       CLR-KEY-CMB SECTION.
       INI-CLR-KEY-CMB.
*% INCLUDE INI_CLR_KEY_CMB OF 'COLBMCMB.TXT'
           MOVE SPACES TO FRM-COD-TIPO IN CMB-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "EMIS"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "EMIS"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-EMIS IN CMB-FLD.
           MOVE SPACES TO FRM-VRF-EMIS IN CMB-FLD.
           MOVE SPACES TO FRM-IND-EMIS IN CMB-FLD.
           MOVE SPACES TO FRM-GLS-EMIS IN CMB-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "BENF"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "BENF"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-BENF IN CMB-FLD.
           MOVE SPACES TO FRM-VRF-BENF IN CMB-FLD.
           MOVE SPACES TO FRM-IND-BENF IN CMB-FLD.
           MOVE SPACES TO FRM-GLS-BENF IN CMB-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-COD-STAT IN CMB-FLD.
           MOVE SPACES TO OPC-COD-TOOC IN CMB-FLD.
           MOVE SPACES TO OPC-COD-AOOC IN CMB-FLD.
           MOVE SPACES TO DLC-IND-ESTA IN CMB-FLD.
           MOVE SPACES TO DLC-IND-ETAP IN CMB-FLD.
           MOVE SPACES TO FRM-COD-TFEC IN CMB-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FINI"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FINI IN CMB-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FFIN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FFIN IN CMB-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-COD-MONE IN CMB-FLD.
           MOVE SPACES TO FRM-COD-CNAL IN CMB-FLD.
           MOVE SPACES TO FRM-IND-SGTE IN CMB-FLD.
           MOVE SPACES TO FRM-CAI-IOPE IN CMB-FLD.
           MOVE SPACES TO FRM-IIC-IOPE IN CMB-FLD.
*% INCLUDE FIN_CLR_KEY_CMB OF 'COLBMCMB.TXT'
       FIN-CLR-KEY-CMB.
           EXIT.
       
       CLR-FLD-CMB SECTION.
       INI-CLR-FLD-CMB.
*% INCLUDE INI_CLR_FLD_CMB OF 'COLBMCMB.TXT'
           MOVE SPACES TO FRM-GLS-NEMI IN CMB-FLD.
           MOVE SPACES TO FRM-GLS-NBEN IN CMB-FLD.
           MOVE SPACES TO FRM-GLS-STUS IN CMB-FLD.
           MOVE SPACES TO FRM-GLS-ETAD IN CMB-FLD.
           MOVE SPACES TO FRM-GLS-ETPA IN CMB-FLD.
           MOVE SPACES TO FRM-GLS-MONE IN CMB-FLD.
           MOVE SPACES TO FRM-GLS-CANA IN CMB-FLD.
           MOVE SPACES TO FRM-CAI-IOPC IN CMB-FLD(1).
           MOVE SPACES TO FRM-CAI-IOPC IN CMB-FLD(2).
           MOVE SPACES TO FRM-IIC-IOPC IN CMB-FLD(1).
           MOVE SPACES TO FRM-IIC-IOPC IN CMB-FLD(2).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-ICLI IN CMB-FLD(1).
           MOVE SPACES TO FRM-VRF-ICLI IN CMB-FLD(1).
           MOVE SPACES TO FRM-IND-ICLI IN CMB-FLD(1).
           MOVE SPACES TO FRM-GLS-ICLI IN CMB-FLD(1).
           MOVE ZEROES TO FRM-NUM-ICLI IN CMB-FLD(2).
           MOVE SPACES TO FRM-VRF-ICLI IN CMB-FLD(2).
           MOVE SPACES TO FRM-IND-ICLI IN CMB-FLD(2).
           MOVE SPACES TO FRM-GLS-ICLI IN CMB-FLD(2).
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-GLS-NOMC IN CMB-FLD(1).
           MOVE SPACES TO FRM-GLS-NOMC IN CMB-FLD(2).
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FEMI"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FEMI IN CMB-FLD(1).
           MOVE ZEROES TO FRM-FEC-FEMI IN CMB-FLD(2).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FVEN IN CMB-FLD(1).
           MOVE ZEROES TO FRM-FEC-FVEN IN CMB-FLD(2).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-COD-VCOC IN CMB-FLD(1).
           MOVE SPACES TO FRM-COD-VCOC IN CMB-FLD(2).
           MOVE SPACES TO FRM-GLS-VCOC IN CMB-FLD(1).
           MOVE SPACES TO FRM-GLS-VCOC IN CMB-FLD(2).
           MOVE ZEROES TO FRM-VAL-MOPE IN CMB-FLD(1).
           MOVE ZEROES TO FRM-VAL-MOPE IN CMB-FLD(2).
           MOVE SPACES TO FRM-COD-TOOC IN CMB-FLD(1).
           MOVE SPACES TO FRM-COD-TOOC IN CMB-FLD(2).
           MOVE SPACES TO FRM-COD-AOOC IN CMB-FLD(1).
           MOVE SPACES TO FRM-COD-AOOC IN CMB-FLD(2).
           MOVE SPACES TO FRM-IND-STAT IN CMB-FLD(1).
           MOVE SPACES TO FRM-IND-STAT IN CMB-FLD(2).
           MOVE SPACES TO FRM-GLS-STAT IN CMB-FLD(1).
           MOVE SPACES TO FRM-GLS-STAT IN CMB-FLD(2).
           MOVE SPACES TO FRM-COD-ESTA IN CMB-FLD(1).
           MOVE SPACES TO FRM-COD-ESTA IN CMB-FLD(2).
           MOVE SPACES TO FRM-GLS-ESTA IN CMB-FLD(1).
           MOVE SPACES TO FRM-GLS-ESTA IN CMB-FLD(2).
           MOVE SPACES TO FRM-COD-ETAP IN CMB-FLD(1).
           MOVE SPACES TO FRM-COD-ETAP IN CMB-FLD(2).
           MOVE SPACES TO FRM-GLS-ETAP IN CMB-FLD(1).
           MOVE SPACES TO FRM-GLS-ETAP IN CMB-FLD(2).
           MOVE SPACES TO FRM-COD-CNAO IN CMB-FLD(1).
           MOVE SPACES TO FRM-COD-CNAO IN CMB-FLD(2).
           MOVE SPACES TO FRM-GLS-CNAO IN CMB-FLD(1).
           MOVE SPACES TO FRM-GLS-CNAO IN CMB-FLD(2).
           PERFORM CLR-OPC-CMB.
           PERFORM CLR-DLC-CMB.
*% INCLUDE FIN_CLR_FLD_CMB OF 'COLBMCMB.TXT'
       FIN-CLR-FLD-CMB.
           EXIT.
       
       CLR-OPC-CMB SECTION.
       INI-CLR-OPC-CMB.
*% INCLUDE INI_CLR_OPC_CMB OF 'COLBMCMB.TXT'
*% INCLUDE FIN_CLR_OPC_CMB OF 'COLBMCMB.TXT'
       FIN-CLR-OPC-CMB.
           EXIT.
       
       CLR-DLC-CMB SECTION.
       INI-CLR-DLC-CMB.
*% INCLUDE INI_CLR_DLC_CMB OF 'COLBMCMB.TXT'
*% INCLUDE FIN_CLR_DLC_CMB OF 'COLBMCMB.TXT'
       FIN-CLR-DLC-CMB.
           EXIT.
       
       FST-KEY-CMB SECTION.
       INI-FST-KEY-CMB.
*% INCLUDE INI_FST_KEY_CMB OF 'COLBMCMB.TXT'
           MOVE -1 TO FRM-COD-TIPO-LEN IN CMB-FLD.
*% INCLUDE FIN_FST_KEY_CMB OF 'COLBMCMB.TXT'
       FIN-FST-KEY-CMB.
           EXIT.
       
       FST-FLD-CMB SECTION.
       INI-FST-FLD-CMB.
*% INCLUDE INI_FST_FLD_CMB OF 'COLBMCMB.TXT'
*% INCLUDE FIN_FST_FLD_CMB OF 'COLBMCMB.TXT'
       FIN-FST-FLD-CMB.
           EXIT.
       
       PRO-KEY-CMB SECTION.
       INI-PRO-KEY-CMB.
*% INCLUDE INI_PRO_KEY_CMB OF 'COLBMCMB.TXT'
           MOVE FRM-CPIM-AEY TO FRM-COD-TIPO-ATR IN CMB-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "EMIS"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "EMIS"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-NUM-EMIS-ATR IN CMB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VRF-EMIS-ATR IN CMB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-EMIS-ATR IN CMB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-EMIS-ATR IN CMB-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "BENF"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "BENF"
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-NUM-BENF-ATR IN CMB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VRF-BENF-ATR IN CMB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-BENF-ATR IN CMB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-BENF-ATR IN CMB-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO FRM-COD-STAT-ATR IN CMB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-COD-TFEC-ATR IN CMB-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FINI"
           MOVE FRM-CPIM-AEY TO FRM-FEC-FINI-ATR IN CMB-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FFIN"
           MOVE FRM-CPIM-AEY TO FRM-FEC-FFIN-ATR IN CMB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-COD-MONE-ATR IN CMB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-COD-CNAL-ATR IN CMB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-SGTE-ATR IN CMB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-CAI-IOPE-ATR IN CMB-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IIC-IOPE-ATR IN CMB-FLD.
           PERFORM PRO-IKY-CMB.
           PERFORM PRO-AKY-CMB.
           PERFORM PRO-MKY-CMB.
*% INCLUDE FIN_PRO_KEY_CMB OF 'COLBMCMB.TXT'
       FIN-PRO-KEY-CMB.
           EXIT.
       
       PRO-IKY-CMB SECTION.
       INI-PRO-IKY-CMB.
*% INCLUDE INI_PRO_IKY_CMB OF 'COLBMCMB.TXT'
           MOVE FRM-CPIM-AEY TO OPC-COD-TOOC-ATR IN CMB-FLD.
           MOVE FRM-CPIM-AEY TO OPC-COD-AOOC-ATR IN CMB-FLD.
           MOVE FRM-CPIM-AEY TO DLC-IND-ESTA-ATR IN CMB-FLD.
           MOVE FRM-CPIM-AEY TO DLC-IND-ETAP-ATR IN CMB-FLD.
*% INCLUDE FIN_PRO_IKY_CMB OF 'COLBMCMB.TXT'
       FIN-PRO-IKY-CMB.
           EXIT.
       
       PRO-MKY-CMB SECTION.
       INI-PRO-MKY-CMB.
*% INCLUDE INI_PRO_MKY_CMB OF 'COLBMCMB.TXT'
*% INCLUDE FIN_PRO_MKY_CMB OF 'COLBMCMB.TXT'
       FIN-PRO-MKY-CMB.
           EXIT.
       
