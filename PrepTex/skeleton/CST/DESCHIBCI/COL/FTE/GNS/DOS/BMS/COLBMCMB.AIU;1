*% INCLUDE FIN_UNP_AKY_CMB OF 'COLBMCMB.TXT'
       FIN-UNP-AKY-CMB.
           EXIT.
       
       UNP-FLD-CMB SECTION.
       INI-UNP-FLD-CMB.
*% INCLUDE INI_UNP_FLD_CMB OF 'COLBMCMB.TXT'
           PERFORM UNP-OPC-CMB.
           PERFORM UNP-DLC-CMB.
*% INCLUDE FIN_UNP_FLD_CMB OF 'COLBMCMB.TXT'
       FIN-UNP-FLD-CMB.
           EXIT.
       
