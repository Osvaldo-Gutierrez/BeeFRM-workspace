*% INCLUDE FIN_UNP_DLC_CMB OF 'COLBMCMB.TXT'
       FIN-UNP-DLC-CMB.
           EXIT.
       
       VAL-KEY-CMB SECTION.
       INI-VAL-KEY-CMB.
           PERFORM VAL-IKY-CMB.
           PERFORM VAL-MKY-CMB.
           PERFORM VAL-AKY-CMB.
       FIN-VAL-KEY-CMB.
           EXIT.
       
       VAL-IKY-CMB SECTION.
       INI-VAL-IKY-CMB.
*% INCLUDE INI_VAL_IKY_CMB OF 'COLBMCMB.TXT'
           PERFORM VAL-FRM-COD-TIPO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-TIPO-LEN IN CMB-FLD
                GO TO FIN-VAL-IKY-CMB.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "EMIS"
      * VRF-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * VRF-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "EMIS"
      * IDC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-IND-EMIS.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-IND-EMIS-LEN IN CMB-FLD
                GO TO FIN-VAL-IKY-CMB.
           PERFORM VAL-FRM-GLS-EMIS.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-GLS-EMIS-LEN IN CMB-FLD
                GO TO FIN-VAL-IKY-CMB.
           PERFORM VAL-FRM-NUM-EMIS.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-NUM-EMIS-LEN IN CMB-FLD
                GO TO FIN-VAL-IKY-CMB.
           PERFORM VAL-FRM-VRF-EMIS.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-VRF-EMIS-LEN IN CMB-FLD
                GO TO FIN-VAL-IKY-CMB.
      * IDC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
*% RREG = "FRM"
*% RREG_FRM = "FRM"
