*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "PIP"
*% PGM_BQ = TRUE
       PUT-ALL-PIP SECTION.
       INI-PUT-ALL-PIP.
*% INCLUDE INI_PUT_ALL_PIP OF 'COLBMPIP.TXT'
           PERFORM PUT-PIP-PIP.
           PERFORM PUT-DBC-PIP.
*% INCLUDE FIN_PUT_ALL_PIP OF 'COLBMPIP.TXT'
       FIN-PUT-ALL-PIP.
           EXIT.
       
       PUT-PIP-PIP SECTION.
       INI-PUT-PIP-PIP.
*% INCLUDE INI_PUT_PIP_PIP OF 'COLBMPIP.TXT'
           IF PGM-STAT-PIP-NUL
               GO TO FIN-PUT-PIP-PIP.
           IF NOT PGM-STAT-PIP-OKS
               PERFORM CLR-PIP-PIP
               GO TO FIN-PUT-PIP-PIP.
           MOVE PIP-COD-PLAN IN PIP TO PIP-COD-PLAN IN PIP-FLD.
           MOVE PIP-COD-EVEN IN PIP TO PIP-COD-EVEN IN PIP-FLD.
           MOVE PIP-COD-OFIC IN PIP TO PIP-COD-OFIC IN PIP-FLD.
*% RREG = "PIP"
*% NOMB = "FPLA"
      * FEC-INI --> BMCHIBCI001.SRCAIU 
           MOVE PIP-NUM-DPLA IN PIP TO PIP-NUM-DPLA IN PIP-FLD.
           MOVE PIP-NUM-MPLA IN PIP TO PIP-NUM-MPLA IN PIP-FLD.
           MOVE PIP-NUM-SPLA IN PIP TO PIP-NUM-SPLA IN PIP-FLD.
           MOVE PIP-NUM-APLA IN PIP TO PIP-NUM-APLA IN PIP-FLD.
      * FEC-FIN --> BMCHIBCI001.SRCAIU 
           MOVE PIP-COD-SEGM IN PIP TO PIP-COD-SEGM IN PIP-FLD.
           MOVE PIP-IND-SEGM IN PIP TO PIP-IND-SEGM IN PIP-FLD.
           MOVE PIP-IND-VIGE IN PIP TO PIP-IND-VIGE IN PIP-FLD.
           MOVE PIP-IND-DCTO IN PIP TO PIP-IND-DCTO IN PIP-FLD.
*% RREG = "PIP"
*% NOMB = "FTRN"
      * FEC-INI --> BMCHIBCI001.SRCAIU 
           MOVE PIP-NUM-DTRN IN PIP TO PIP-NUM-DTRN IN PIP-FLD.
           MOVE PIP-NUM-MTRN IN PIP TO PIP-NUM-MTRN IN PIP-FLD.
           MOVE PIP-NUM-STRN IN PIP TO PIP-NUM-STRN IN PIP-FLD.
           MOVE PIP-NUM-ATRN IN PIP TO PIP-NUM-ATRN IN PIP-FLD.
      * FEC-FIN --> BMCHIBCI001.SRCAIU 
           MOVE PIP-HRA-HRTR IN PIP TO PIP-HRA-HRTR IN PIP-FLD.
      *
      *    CAMPO INDEXADO   PIP-COD-IMPU
           MOVE PIP-COD-IMPU IN PIP(1) TO PIP-COD-IMPU IN PIP-FLD(1).
           MOVE PIP-COD-IMPU IN PIP(2) TO PIP-COD-IMPU IN PIP-FLD(2).
           MOVE PIP-COD-IMPU IN PIP(3) TO PIP-COD-IMPU IN PIP-FLD(3).
           MOVE PIP-COD-IMPU IN PIP(4) TO PIP-COD-IMPU IN PIP-FLD(4).
           MOVE PIP-COD-IMPU IN PIP(5) TO PIP-COD-IMPU IN PIP-FLD(5).
           MOVE PIP-COD-IMPU IN PIP(6) TO PIP-COD-IMPU IN PIP-FLD(6).
      *
      *    CAMPO INDEXADO   PIP-COD-CONC
           MOVE PIP-COD-CONC IN PIP(1) TO PIP-COD-CONC IN PIP-FLD(1).
           MOVE PIP-COD-CONC IN PIP(2) TO PIP-COD-CONC IN PIP-FLD(2).
           MOVE PIP-COD-CONC IN PIP(3) TO PIP-COD-CONC IN PIP-FLD(3).
           MOVE PIP-COD-CONC IN PIP(4) TO PIP-COD-CONC IN PIP-FLD(4).
           MOVE PIP-COD-CONC IN PIP(5) TO PIP-COD-CONC IN PIP-FLD(5).
           MOVE PIP-COD-CONC IN PIP(6) TO PIP-COD-CONC IN PIP-FLD(6).
      *
      *    CAMPO INDEXADO   PIP-COD-SUBC
           MOVE PIP-COD-SUBC IN PIP(1) TO PIP-COD-SUBC IN PIP-FLD(1).
           MOVE PIP-COD-SUBC IN PIP(2) TO PIP-COD-SUBC IN PIP-FLD(2).
           MOVE PIP-COD-SUBC IN PIP(3) TO PIP-COD-SUBC IN PIP-FLD(3).
           MOVE PIP-COD-SUBC IN PIP(4) TO PIP-COD-SUBC IN PIP-FLD(4).
           MOVE PIP-COD-SUBC IN PIP(5) TO PIP-COD-SUBC IN PIP-FLD(5).
           MOVE PIP-COD-SUBC IN PIP(6) TO PIP-COD-SUBC IN PIP-FLD(6).
      *
      *    CAMPO INDEXADO   PIP-IND-INCL
           MOVE PIP-IND-INCL IN PIP(1) TO PIP-IND-INCL IN PIP-FLD(1).
           MOVE PIP-IND-INCL IN PIP(2) TO PIP-IND-INCL IN PIP-FLD(2).
           MOVE PIP-IND-INCL IN PIP(3) TO PIP-IND-INCL IN PIP-FLD(3).
           MOVE PIP-IND-INCL IN PIP(4) TO PIP-IND-INCL IN PIP-FLD(4).
           MOVE PIP-IND-INCL IN PIP(5) TO PIP-IND-INCL IN PIP-FLD(5).
           MOVE PIP-IND-INCL IN PIP(6) TO PIP-IND-INCL IN PIP-FLD(6).
      *
      *    CAMPO INDEXADO   PIP-SGV-TIMP
           MOVE PIP-SGV-TIMP IN PIP(1) TO PIP-SGV-TIMP IN PIP-FLD(1).
           MOVE PIP-SGV-TIMP IN PIP(2) TO PIP-SGV-TIMP IN PIP-FLD(2).
           MOVE PIP-SGV-TIMP IN PIP(3) TO PIP-SGV-TIMP IN PIP-FLD(3).
           MOVE PIP-SGV-TIMP IN PIP(4) TO PIP-SGV-TIMP IN PIP-FLD(4).
           MOVE PIP-SGV-TIMP IN PIP(5) TO PIP-SGV-TIMP IN PIP-FLD(5).
           MOVE PIP-SGV-TIMP IN PIP(6) TO PIP-SGV-TIMP IN PIP-FLD(6).
      *
      *    CAMPO INDEXADO   PIP-SGV-TTOP
           MOVE PIP-SGV-TTOP IN PIP(1) TO PIP-SGV-TTOP IN PIP-FLD(1).
           MOVE PIP-SGV-TTOP IN PIP(2) TO PIP-SGV-TTOP IN PIP-FLD(2).
           MOVE PIP-SGV-TTOP IN PIP(3) TO PIP-SGV-TTOP IN PIP-FLD(3).
           MOVE PIP-SGV-TTOP IN PIP(4) TO PIP-SGV-TTOP IN PIP-FLD(4).
           MOVE PIP-SGV-TTOP IN PIP(5) TO PIP-SGV-TTOP IN PIP-FLD(5).
           MOVE PIP-SGV-TTOP IN PIP(6) TO PIP-SGV-TTOP IN PIP-FLD(6).
      *
      *    CAMPO INDEXADO   PIP-VAL-MTOP
           MOVE PIP-VAL-MTOP IN PIP(1) TO PIP-VAL-MTOP IN PIP-FLD(1).
           MOVE PIP-VAL-MTOP IN PIP(2) TO PIP-VAL-MTOP IN PIP-FLD(2).
           MOVE PIP-VAL-MTOP IN PIP(3) TO PIP-VAL-MTOP IN PIP-FLD(3).
           MOVE PIP-VAL-MTOP IN PIP(4) TO PIP-VAL-MTOP IN PIP-FLD(4).
           MOVE PIP-VAL-MTOP IN PIP(5) TO PIP-VAL-MTOP IN PIP-FLD(5).
           MOVE PIP-VAL-MTOP IN PIP(6) TO PIP-VAL-MTOP IN PIP-FLD(6).
      *
      *    CAMPO INDEXADO   PIP-COD-VCOC
           MOVE PIP-COD-VCOC IN PIP(1) TO PIP-COD-VCOC IN PIP-FLD(1).
           MOVE PIP-COD-VCOC IN PIP(2) TO PIP-COD-VCOC IN PIP-FLD(2).
           MOVE PIP-COD-VCOC IN PIP(3) TO PIP-COD-VCOC IN PIP-FLD(3).
           MOVE PIP-COD-VCOC IN PIP(4) TO PIP-COD-VCOC IN PIP-FLD(4).
           MOVE PIP-COD-VCOC IN PIP(5) TO PIP-COD-VCOC IN PIP-FLD(5).
           MOVE PIP-COD-VCOC IN PIP(6) TO PIP-COD-VCOC IN PIP-FLD(6).
           MOVE PIP-CAI-ICLI IN PIP TO PIP-CAI-ICLI IN PIP-FLD.
           MOVE PIP-IIC-ICLI IN PIP TO PIP-IIC-ICLI IN PIP-FLD.
