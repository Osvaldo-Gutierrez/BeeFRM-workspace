           PERFORM UNP-DLC-TRM.
*% INCLUDE FIN_UNP_FLD_TRM OF 'COLBMTRM.TXT'
       FIN-UNP-FLD-TRM.
           EXIT.
       
       UNP-OPC-TRM SECTION.
       INI-UNP-OPC-TRM.
*% INCLUDE INI_UNP_OPC_TRM OF 'COLBMTRM.TXT'
*% INCLUDE FIN_UNP_OPC_TRM OF 'COLBMTRM.TXT'
       FIN-UNP-OPC-TRM.
           EXIT.
       
       UNP-VEN-TRM SECTION.
       INI-UNP-VEN-TRM.
*% INCLUDE INI_UNP_VEN_TRM OF 'COLBMTRM.TXT'
*% INCLUDE FIN_UNP_VEN_TRM OF 'COLBMTRM.TXT'
       FIN-UNP-VEN-TRM.
           EXIT.
       
       UNP-DBC-TRM SECTION.
       INI-UNP-DBC-TRM.
*% INCLUDE INI_UNP_DBC_TRM OF 'COLBMTRM.TXT'
*% INCLUDE FIN_UNP_DBC_TRM OF 'COLBMTRM.TXT'
       FIN-UNP-DBC-TRM.
           EXIT.
       
       UNP-DLC-TRM SECTION.
       INI-UNP-DLC-TRM.
*% INCLUDE INI_UNP_DLC_TRM OF 'COLBMTRM.TXT'
*% INCLUDE FIN_UNP_DLC_TRM OF 'COLBMTRM.TXT'
       FIN-UNP-DLC-TRM.
           EXIT.
       
       VAL-KEY-TRM SECTION.
       INI-VAL-KEY-TRM.
           PERFORM VAL-IKY-TRM.
           PERFORM VAL-MKY-TRM.
           PERFORM VAL-AKY-TRM.
       FIN-VAL-KEY-TRM.
           EXIT.
       
       VAL-IKY-TRM SECTION.
       INI-VAL-IKY-TRM.
*% INCLUDE INI_VAL_IKY_TRM OF 'COLBMTRM.TXT'
           PERFORM VAL-OPC-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-CAI-IOPC-LEN IN TRM-FLD
                GO TO FIN-VAL-IKY-TRM.
           PERFORM VAL-OPC-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-IIC-IOPC-LEN IN TRM-FLD
                GO TO FIN-VAL-IKY-TRM.
           PERFORM VAL-VEN-NUM-IVCT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO VEN-NUM-IVCT-LEN IN TRM-FLD
                GO TO FIN-VAL-IKY-TRM.
*% INCLUDE FIN_VAL_IKY_TRM OF 'COLBMTRM.TXT'
       FIN-VAL-IKY-TRM.
           EXIT.
       
       VAL-MKY-TRM SECTION.
       INI-VAL-MKY-TRM.
*% INCLUDE INI_VAL_MKY_TRM OF 'COLBMTRM.TXT'
*% INCLUDE FIN_VAL_MKY_TRM OF 'COLBMTRM.TXT'
       FIN-VAL-MKY-TRM.
           EXIT.
       
       VAL-AKY-TRM SECTION.
       INI-VAL-AKY-TRM.
*% INCLUDE INI_VAL_AKY_TRM OF 'COLBMTRM.TXT'
*% INCLUDE FIN_VAL_AKY_TRM OF 'COLBMTRM.TXT'
       FIN-VAL-AKY-TRM.
           EXIT.
       
       VAL-OPC-CAI-IOPC SECTION.
       INI-VAL-OPC-CAI-IOPC.
*% LBL = "OPC_CAI_IOPC OF 'COLBMTRM.TXT'"
*% INCLUDE INI_OPC_CAI_IOPC OF 'COLBMTRM.TXT'
           IF OPC-CAI-IOPC IN TRM-FLD NOT > SPACES
              MOVE SPACES TO OPC-CAI-IOPC IN TRM-FLD.
*% INCLUDE OPC_CAI_IOPC OF 'COLBMTRM.TXT'
*% INCLUDE FIN_OPC_CAI_IOPC OF 'COLBMTRM.TXT'
       FIN-VAL-OPC-CAI-IOPC.
           EXIT.
       
       VAL-OPC-IIC-IOPC SECTION.
       INI-VAL-OPC-IIC-IOPC.
*% LBL = "OPC_IIC_IOPC OF 'COLBMTRM.TXT'"
*% INCLUDE INI_OPC_IIC_IOPC OF 'COLBMTRM.TXT'
*% INCLUDE OPC_IIC_IOPC OF 'COLBMTRM.TXT'
*% INCLUDE FIN_OPC_IIC_IOPC OF 'COLBMTRM.TXT'
       FIN-VAL-OPC-IIC-IOPC.
           EXIT.
       
       VAL-VEN-NUM-IVCT SECTION.
       INI-VAL-VEN-NUM-IVCT.
*% LBL = "VEN_NUM_IVCT OF 'COLBMTRM.TXT'"
*% INCLUDE INI_VEN_NUM_IVCT OF 'COLBMTRM.TXT'
*% INCLUDE VEN_NUM_IVCT OF 'COLBMTRM.TXT'
*% INCLUDE FIN_VEN_NUM_IVCT OF 'COLBMTRM.TXT'
       FIN-VAL-VEN-NUM-IVCT.
           EXIT.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% IZR = "S"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% IZR = "S"
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCCT"
*% IZR = "S"
*% RREG = "VEN"
*% RREG_FRM = "VEN"
*% NOMB = "FVEN"
*% IZR = "S"
       
       VAL-CON-KEY-TRM SECTION.
       INI-VAL-CON-KEY-TRM.
*% INCLUDE INI_VAL_CON_KEY_TRM OF 'COLBMTRM.TXT'
           IF     OPC-CAI-IOPC IN TRM-FLD NOT > SPACES 
              AND OPC-IIC-IOPC IN TRM-FLD NOT > ZEROES 
              AND VEN-NUM-IVCT IN TRM-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-TRM.
*% INCLUDE FIN_VAL_CON_KEY_TRM OF 'COLBMTRM.TXT'
       FIN-VAL-CON-KEY-TRM.
           EXIT.
       
       VAL-CON-IKY-TRM SECTION.
       INI-VAL-CON-IKY-TRM.
*% INCLUDE INI_VAL_CON_IKY_TRM OF 'COLBMTRM.TXT'
           IF     OPC-CAI-IOPC IN TRM-FLD NOT > SPACES 
              AND OPC-IIC-IOPC IN TRM-FLD NOT > ZEROES 
              AND VEN-NUM-IVCT IN TRM-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-TRM.
