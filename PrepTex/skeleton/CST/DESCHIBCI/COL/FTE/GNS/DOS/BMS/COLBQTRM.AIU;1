      *
      * COLBQTRM
      * Version 3.0.1 Nov-2005
      * 28-Oct-19 09:59 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "TRM"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMTRM.TXT'
       SET-TAG-COLBQTRM SECTION.
       INI-SET-TAG-COLBQTRM.
           MOVE
           '<IDNMOD NOM=$COLBQTRM$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$28-Oct-19 09:59 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQTRM.
           EXIT.
           
       PUT-ALL-TRM SECTION.
       INI-PUT-ALL-TRM.
*% INCLUDE INI_PUT_ALL_TRM OF 'COLBMTRM.TXT'
           PERFORM PUT-OPC-TRM.
           PERFORM PUT-VEN-TRM.
           PERFORM PUT-DBC-TRM.
           PERFORM PUT-DLC-TRM.
*% INCLUDE FIN_PUT_ALL_TRM OF 'COLBMTRM.TXT'
       FIN-PUT-ALL-TRM.
           EXIT.
       
       PUT-OPC-TRM SECTION.
       INI-PUT-OPC-TRM.
*% INCLUDE INI_PUT_OPC_TRM OF 'COLBMTRM.TXT'
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-TRM.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-TRM
               GO TO FIN-PUT-OPC-TRM.
           MOVE OPC-MSC-TVOC IN OPC TO OPC-MSC-TVOC IN TRM-FLD.
           MOVE OPC-COD-TOOC IN OPC TO OPC-COD-TOOC IN TRM-FLD.
           MOVE OPC-COD-OFOC IN OPC TO OPC-COD-OFOC IN TRM-FLD.
           MOVE OPC-CAI-IOPC IN OPC TO OPC-CAI-IOPC IN TRM-FLD.
           MOVE OPC-IIC-IOPC IN OPC TO OPC-IIC-IOPC IN TRM-FLD.
           MOVE OPC-COD-VCOC IN OPC TO OPC-COD-VCOC IN TRM-FLD.
*% BUF = "TRM-FLD"
*% SIS = "TAB"
*% TAB = "TVAL"
*% FLD = "OPC-MSC-TVOC"
*% GLS = "FRM-GLS-TVOC"
*% TIENE_GLOSA = TRUE
*% LGL = 13
*% INCLUDE 'BUSMSC'
*% BUF = "TRM-FLD"
*% SIS = "TAB"
*% TAB = "TIO "
*% FLD = "OPC-COD-TOOC"
*% GLS = "FRM-GLS-TOOC"
*% TIENE_GLOSA = TRUE
*% LGL = 14
*% INCLUDE 'BUSTAB'
*% BUF = "TRM-FLD"
*% SIS = "TAB"
*% TAB = "OFI "
*% FLD = "OPC-COD-OFOC"
*% GLS = "FRM-GLS-OFOC"
*% TIENE_GLOSA = TRUE
*% LGL = 13
*% INCLUDE 'BUSOFI          '
*% BUF = "TRM-FLD"
*% SIS = "TAB"
*% TAB = "VLR "
*% FLD = "OPC-COD-VCOC"
*% GLS = "FRM-GLS-VCOC"
*% TIENE_GLOSA = TRUE
*% LGL = 29
*% INCLUDE 'BUSTAB'
*% INCLUDE FIN_PUT_OPC_TRM OF 'COLBMTRM.TXT'
       FIN-PUT-OPC-TRM.
           EXIT.
       
       PUT-VEN-TRM SECTION.
       INI-PUT-VEN-TRM.
*% INCLUDE INI_PUT_VEN_TRM OF 'COLBMTRM.TXT'
           IF PGM-STAT-VEN-NUL
               GO TO FIN-PUT-VEN-TRM.
           IF NOT PGM-STAT-VEN-OKS
               PERFORM CLR-VEN-TRM
               GO TO FIN-PUT-VEN-TRM.
           MOVE VEN-NUM-IVCT IN VEN TO VEN-NUM-IVCT IN TRM-FLD.
           MOVE VEN-VAL-SCAP IN VEN TO VEN-VAL-SCAP IN TRM-FLD.
*% RREG = "VEN"
*% RREG_FRM = "VEN"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE VEN-NUM-DVEN IN VEN TO VEN-NUM-DVEN IN TRM-FLD.
           MOVE VEN-NUM-MVEN IN VEN TO VEN-NUM-MVEN IN TRM-FLD.
           MOVE VEN-NUM-SVEN IN VEN TO VEN-NUM-SVEN IN TRM-FLD.
           MOVE VEN-NUM-AVEN IN VEN TO VEN-NUM-AVEN IN TRM-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE VEN-VAL-SFIN IN VEN TO VEN-VAL-SFIN IN TRM-FLD.
*% INCLUDE FIN_PUT_VEN_TRM OF 'COLBMTRM.TXT'
       FIN-PUT-VEN-TRM.
           EXIT.
       
       PUT-DBC-TRM SECTION.
       INI-PUT-DBC-TRM.
*% INCLUDE INI_PUT_DBC_TRM OF 'COLBMTRM.TXT'
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-TRM.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-TRM
               GO TO FIN-PUT-DBC-TRM.
           MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN TRM-FLD.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN TRM-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN TRM-FLD.
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN TRM-FLD.
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN TRM-FLD.
      * IDC-FIN --> BMCHIBCI001210.SRCAIU 
*% INCLUDE FIN_PUT_DBC_TRM OF 'COLBMTRM.TXT'
       FIN-PUT-DBC-TRM.
           EXIT.
       
       PUT-DLC-TRM SECTION.
       INI-PUT-DLC-TRM.
*% INCLUDE INI_PUT_DLC_TRM OF 'COLBMTRM.TXT'
           IF PGM-STAT-DLC-NUL
               GO TO FIN-PUT-DLC-TRM.
           IF NOT PGM-STAT-DLC-OKS
               PERFORM CLR-DLC-TRM
               GO TO FIN-PUT-DLC-TRM.
           MOVE DLC-VAL-SDLC IN DLC TO DLC-VAL-SDLC IN TRM-FLD.
           MOVE DLC-IND-SCTB IN DLC TO DLC-IND-SCTB IN TRM-FLD.
           MOVE DLC-IND-SCBZ IN DLC TO DLC-IND-SCBZ IN TRM-FLD.
*% BUF = "TRM-FLD"
*% SIS = "COL"
*% TAB = "SCTB"
*% FLD = "DLC-IND-SCTB"
*% GLS = "FRM-GLS-SCTB"
*% TIENE_GLOSA = TRUE
*% LGL = 37
*% INCLUDE 'BUSMSC'
*% BUF = "TRM-FLD"
*% SIS = "COL"
*% TAB = "SCBZ"
*% FLD = "DLC-IND-SCBZ"
*% GLS = "FRM-GLS-SCBZ"
*% TIENE_GLOSA = TRUE
*% LGL = 37
*% INCLUDE 'BUSMSC'
*% INCLUDE FIN_PUT_DLC_TRM OF 'COLBMTRM.TXT'
       FIN-PUT-DLC-TRM.
           EXIT.
       
       KEY-ALL-TRM SECTION.
       INI-KEY-ALL-TRM.
*% INCLUDE INI_KEY_ALL_TRM OF 'COLBMTRM.TXT'
           PERFORM KEY-OPC-TRM.
           PERFORM KEY-VEN-TRM.
           PERFORM KEY-DBC-TRM.
           PERFORM KEY-DLC-TRM.
*% INCLUDE FIN_KEY_ALL_TRM OF 'COLBMTRM.TXT'
       FIN-KEY-ALL-TRM.
           EXIT.
       
       KEY-OPC-TRM SECTION.
       INI-KEY-OPC-TRM.
*% INCLUDE INI_KEY_OPC_TRM OF 'COLBMTRM.TXT'
           MOVE OPC-CAI-IOPC IN TRM-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE OPC-IIC-IOPC IN TRM-FLD TO OPC-IIC-IOPC IN OPC.
*% INCLUDE FIN_KEY_OPC_TRM OF 'COLBMTRM.TXT'
       FIN-KEY-OPC-TRM.
           EXIT.
       
       KEY-VEN-TRM SECTION.
       INI-KEY-VEN-TRM.
*% INCLUDE INI_KEY_VEN_TRM OF 'COLBMTRM.TXT'
           MOVE VEN-NUM-IVCT IN TRM-FLD TO VEN-NUM-IVCT IN VEN.
*% INCLUDE FIN_KEY_VEN_TRM OF 'COLBMTRM.TXT'
       FIN-KEY-VEN-TRM.
           EXIT.
       
       KEY-DBC-TRM SECTION.
       INI-KEY-DBC-TRM.
*% INCLUDE INI_KEY_DBC_TRM OF 'COLBMTRM.TXT'
*% INCLUDE FIN_KEY_DBC_TRM OF 'COLBMTRM.TXT'
       FIN-KEY-DBC-TRM.
           EXIT.
       
       KEY-DLC-TRM SECTION.
       INI-KEY-DLC-TRM.
*% INCLUDE INI_KEY_DLC_TRM OF 'COLBMTRM.TXT'
*% INCLUDE FIN_KEY_DLC_TRM OF 'COLBMTRM.TXT'
       FIN-KEY-DLC-TRM.
           EXIT.
       
       SCR-CLRA-TRM SECTION.
       INI-SCR-CLRA-TRM.
*% INCLUDE INI_SCR_CLRA_TRM OF 'COLBMTRM.TXT'
           PERFORM CLR-KEY-TRM.
           PERFORM CLR-FLD-TRM.
*% INCLUDE FIN_SCR_CLRA_TRM OF 'COLBMTRM.TXT'
       FIN-SCR-CLRA-TRM.
           EXIT.
       
       CLR-KEY-TRM SECTION.
       INI-CLR-KEY-TRM.
*% INCLUDE INI_CLR_KEY_TRM OF 'COLBMTRM.TXT'
           MOVE SPACES TO OPC-CAI-IOPC IN TRM-FLD.
           MOVE ZEROES TO OPC-IIC-IOPC IN TRM-FLD.
           MOVE ZEROES TO VEN-NUM-IVCT IN TRM-FLD.
*% INCLUDE FIN_CLR_KEY_TRM OF 'COLBMTRM.TXT'
       FIN-CLR-KEY-TRM.
           EXIT.
       
       CLR-FLD-TRM SECTION.
       INI-CLR-FLD-TRM.
*% INCLUDE INI_CLR_FLD_TRM OF 'COLBMTRM.TXT'
           MOVE SPACES TO FRM-GLS-TVOC IN TRM-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN TRM-FLD.
           MOVE SPACES TO FRM-GLS-OFOC IN TRM-FLD.
           MOVE SPACES TO FRM-GLS-TRAS IN TRM-FLD.
*% RREG = "FRM"
*% RREG_FRM = "FRM"
*% NOMB = "FCCT"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FCCT IN TRM-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO FRM-GLS-SCTB IN TRM-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN TRM-FLD.
           MOVE SPACES TO FRM-GLS-SCBZ IN TRM-FLD.
           PERFORM CLR-OPC-TRM.
           PERFORM CLR-VEN-TRM.
           PERFORM CLR-DBC-TRM.
           PERFORM CLR-DLC-TRM.
*% INCLUDE FIN_CLR_FLD_TRM OF 'COLBMTRM.TXT'
       FIN-CLR-FLD-TRM.
           EXIT.
       
       CLR-OPC-TRM SECTION.
       INI-CLR-OPC-TRM.
*% INCLUDE INI_CLR_OPC_TRM OF 'COLBMTRM.TXT'
           MOVE SPACES TO OPC-MSC-TVOC IN TRM-FLD.
           MOVE SPACES TO OPC-COD-TOOC IN TRM-FLD.
           MOVE SPACES TO OPC-COD-OFOC IN TRM-FLD.
           MOVE SPACES TO OPC-COD-VCOC IN TRM-FLD.
*% INCLUDE FIN_CLR_OPC_TRM OF 'COLBMTRM.TXT'
       FIN-CLR-OPC-TRM.
           EXIT.
       
       CLR-VEN-TRM SECTION.
       INI-CLR-VEN-TRM.
*% INCLUDE INI_CLR_VEN_TRM OF 'COLBMTRM.TXT'
           MOVE ZEROES TO VEN-VAL-SCAP IN TRM-FLD.
*% RREG = "VEN"
*% RREG_FRM = "VEN"
*% NOMB = "FVEN"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO VEN-FEC-FVEN IN TRM-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO VEN-VAL-SFIN IN TRM-FLD.
*% INCLUDE FIN_CLR_VEN_TRM OF 'COLBMTRM.TXT'
       FIN-CLR-VEN-TRM.
           EXIT.
       
       CLR-DBC-TRM SECTION.
       INI-CLR-DBC-TRM.
*% INCLUDE INI_CLR_DBC_TRM OF 'COLBMTRM.TXT'
           MOVE SPACES TO DBC-GLS-NOMC IN TRM-FLD.
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN TRM-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN TRM-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN TRM-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN TRM-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
*% INCLUDE FIN_CLR_DBC_TRM OF 'COLBMTRM.TXT'
       FIN-CLR-DBC-TRM.
           EXIT.
       
       CLR-DLC-TRM SECTION.
       INI-CLR-DLC-TRM.
*% INCLUDE INI_CLR_DLC_TRM OF 'COLBMTRM.TXT'
           MOVE ZEROES TO DLC-VAL-SDLC IN TRM-FLD.
           MOVE SPACES TO DLC-IND-SCTB IN TRM-FLD.
           MOVE SPACES TO DLC-IND-SCBZ IN TRM-FLD.
*% INCLUDE FIN_CLR_DLC_TRM OF 'COLBMTRM.TXT'
       FIN-CLR-DLC-TRM.
           EXIT.
       
       FST-KEY-TRM SECTION.
       INI-FST-KEY-TRM.
*% INCLUDE INI_FST_KEY_TRM OF 'COLBMTRM.TXT'
           MOVE -1 TO OPC-CAI-IOPC-LEN IN TRM-FLD.
*% INCLUDE FIN_FST_KEY_TRM OF 'COLBMTRM.TXT'
       FIN-FST-KEY-TRM.
           EXIT.
       
       FST-FLD-TRM SECTION.
       INI-FST-FLD-TRM.
*% INCLUDE INI_FST_FLD_TRM OF 'COLBMTRM.TXT'
*% INCLUDE FIN_FST_FLD_TRM OF 'COLBMTRM.TXT'
       FIN-FST-FLD-TRM.
           EXIT.
       
       PRO-KEY-TRM SECTION.
       INI-PRO-KEY-TRM.
*% INCLUDE INI_PRO_KEY_TRM OF 'COLBMTRM.TXT'
           PERFORM PRO-IKY-TRM.
           PERFORM PRO-AKY-TRM.
           PERFORM PRO-MKY-TRM.
*% INCLUDE FIN_PRO_KEY_TRM OF 'COLBMTRM.TXT'
       FIN-PRO-KEY-TRM.
           EXIT.
       
       PRO-IKY-TRM SECTION.
       INI-PRO-IKY-TRM.
*% INCLUDE INI_PRO_IKY_TRM OF 'COLBMTRM.TXT'
           MOVE FRM-CPIM-AEY TO OPC-CAI-IOPC-ATR IN TRM-FLD.
           MOVE FRM-CPIM-AEY TO OPC-IIC-IOPC-ATR IN TRM-FLD.
           MOVE FRM-CPIM-AEY TO VEN-NUM-IVCT-ATR IN TRM-FLD.
*% INCLUDE FIN_PRO_IKY_TRM OF 'COLBMTRM.TXT'
       FIN-PRO-IKY-TRM.
           EXIT.
       
       PRO-MKY-TRM SECTION.
       INI-PRO-MKY-TRM.
*% INCLUDE INI_PRO_MKY_TRM OF 'COLBMTRM.TXT'
*% INCLUDE FIN_PRO_MKY_TRM OF 'COLBMTRM.TXT'
       FIN-PRO-MKY-TRM.
           EXIT.
       
       PRO-AKY-TRM SECTION.
       INI-PRO-AKY-TRM.
*% INCLUDE INI_PRO_AKY_TRM OF 'COLBMTRM.TXT'
*% INCLUDE FIN_PRO_AKY_TRM OF 'COLBMTRM.TXT'
       FIN-PRO-AKY-TRM.
           EXIT.
       
       UNP-KEY-TRM SECTION.
       INI-UNP-KEY-TRM.
*% INCLUDE INI_UNP_KEY_TRM OF 'COLBMTRM.TXT'
           PERFORM UNP-IKY-TRM.
           PERFORM UNP-AKY-TRM.
           PERFORM UNP-MKY-TRM.
*% INCLUDE FIN_UNP_KEY_TRM OF 'COLBMTRM.TXT'
       FIN-UNP-KEY-TRM.
           EXIT.
       
       UNP-IKY-TRM SECTION.
       INI-UNP-IKY-TRM.
*% INCLUDE INI_UNP_IKY_TRM OF 'COLBMTRM.TXT'
           MOVE FRM-CPIM-UBY TO OPC-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO OPC-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO VEN-NUM-IVCT-ATR.
*% INCLUDE FIN_UNP_IKY_TRM OF 'COLBMTRM.TXT'
       FIN-UNP-IKY-TRM.
           EXIT.
       
       UNP-MKY-TRM SECTION.
       INI-UNP-MKY-TRM.
*% INCLUDE INI_UNP_MKY_TRM OF 'COLBMTRM.TXT'
