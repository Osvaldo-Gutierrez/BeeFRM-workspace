      *
      * COLBQLO1
      * Version 3.0.1 Nov-2005
      * 17-Nov-11 05:31 PM
      *
*% INI_LBL = TRUE
*% PGM_BQ = TRUE
*% FRM = "LO1"
*% PGM_BQ = TRUE
*% INCLUDE SETTAG OF 'COLBMLO1.TXT'
       SET-TAG-COLBQLO1 SECTION.
       INI-SET-TAG-COLBQLO1.
           MOVE
           '<IDNMOD NOM=$COLBQLO1$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$17-Nov-11 05:31 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQLO1.
           EXIT.
           
       PUT-ALL-LO1 SECTION.
       INI-PUT-ALL-LO1.
*% INCLUDE INI_PUT_ALL_LO1 OF 'COLBMLO1.TXT'
           PERFORM PUT-LAP-LO1.
           PERFORM PUT-POC-LO1.
*% INCLUDE FIN_PUT_ALL_LO1 OF 'COLBMLO1.TXT'
       FIN-PUT-ALL-LO1.
           EXIT.
       
       PUT-LAP-LO1 SECTION.
       INI-PUT-LAP-LO1.
*% INCLUDE INI_PUT_LAP_LO1 OF 'COLBMLO1.TXT'
           IF PGM-STAT-LAP-NUL
               GO TO FIN-PUT-LAP-LO1.
           IF NOT PGM-STAT-LAP-OKS
               PERFORM CLR-LAP-LO1
               GO TO FIN-PUT-LAP-LO1.
*% RREG = "LAP"
*% RREG_FRM = "LAP"
*% NOMB = "FLAP"
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE LAP-NUM-DLAP IN LAP TO LAP-NUM-DLAP IN LO1-FLD.
           MOVE LAP-NUM-MLAP IN LAP TO LAP-NUM-MLAP IN LO1-FLD.
           MOVE LAP-NUM-SLAP IN LAP TO LAP-NUM-SLAP IN LO1-FLD.
           MOVE LAP-NUM-ALAP IN LAP TO LAP-NUM-ALAP IN LO1-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE LAP-HRA-HLAP IN LAP TO LAP-HRA-HLAP IN LO1-FLD.
           MOVE LAP-COD-KLAP IN LAP TO LAP-COD-KLAP IN LO1-FLD.
           MOVE LAP-COD-AUTO IN LAP TO LAP-COD-AUTO IN LO1-FLD.
           MOVE LAP-COD-COME IN LAP TO LAP-COD-COME IN LO1-FLD.
           MOVE LAP-COD-CMND IN LAP TO LAP-COD-CMND IN LO1-FLD.
           MOVE LAP-COD-OTRN IN LAP TO LAP-COD-OTRN IN LO1-FLD.
           MOVE LAP-NUM-CLAP IN LAP TO LAP-NUM-CLAP IN LO1-FLD.
*% INCLUDE FIN_PUT_LAP_LO1 OF 'COLBMLO1.TXT'
       FIN-PUT-LAP-LO1.
           EXIT.
       
       PUT-POC-LO1 SECTION.
       INI-PUT-POC-LO1.
*% INCLUDE INI_PUT_POC_LO1 OF 'COLBMLO1.TXT'
           IF PGM-STAT-POC-NUL
               GO TO FIN-PUT-POC-LO1.
           IF NOT PGM-STAT-POC-OKS
               PERFORM CLR-POC-LO1
               GO TO FIN-PUT-POC-LO1.
           MOVE POC-COD-VCOC IN POC TO POC-COD-VCOC IN LO1-FLD.
           MOVE POC-COD-TOOC IN POC TO POC-COD-TOOC IN LO1-FLD.
           MOVE POC-COD-AOOC IN POC TO POC-COD-AOOC IN LO1-FLD.
           MOVE POC-NUM-IPOC IN POC TO POC-NUM-IPOC IN LO1-FLD.
           MOVE POC-VAL-CMIN IN POC TO POC-VAL-CMIN IN LO1-FLD.
           MOVE POC-NUM-PMIN IN POC TO POC-NUM-PMIN IN LO1-FLD.
           MOVE POC-IND-PMIN IN POC TO POC-IND-PMIN IN LO1-FLD.
           MOVE POC-VAL-CMAX IN POC TO POC-VAL-CMAX IN LO1-FLD.
           MOVE POC-NUM-PMAX IN POC TO POC-NUM-PMAX IN LO1-FLD.
           MOVE POC-IND-PMAX IN POC TO POC-IND-PMAX IN LO1-FLD.
           MOVE POC-IND-TICE IN POC TO POC-IND-TICE IN LO1-FLD.
           MOVE POC-NUM-VMIN IN POC TO POC-NUM-VMIN IN LO1-FLD.
           MOVE POC-IND-TINE IN POC TO POC-IND-TINE IN LO1-FLD.
           MOVE POC-NUM-VMAX IN POC TO POC-NUM-VMAX IN LO1-FLD.
           MOVE POC-IND-MTIC IN POC TO POC-IND-MTIC IN LO1-FLD.
           MOVE POC-IND-RENO IN POC TO POC-IND-RENO IN LO1-FLD.
           MOVE POC-MSC-BCTI IN POC TO POC-MSC-BCTI IN LO1-FLD.
           MOVE POC-GLS-PCMI IN POC TO POC-GLS-PCMI IN LO1-FLD.
           MOVE POC-MSC-PBTI IN POC TO POC-MSC-PBTI IN LO1-FLD.
           MOVE POC-GLS-PCMA IN POC TO POC-GLS-PCMA IN LO1-FLD.
           MOVE POC-MSC-BPBT IN POC TO POC-MSC-BPBT IN LO1-FLD.
           MOVE POC-COD-SEGD IN POC TO POC-COD-SEGD IN LO1-FLD.
           MOVE POC-IND-TCCE IN POC TO POC-IND-TCCE IN LO1-FLD.
           MOVE POC-IND-TCNE IN POC TO POC-IND-TCNE IN LO1-FLD.
           MOVE POC-MSC-SPBT IN POC TO POC-MSC-SPBT IN LO1-FLD.
           MOVE POC-COD-TCOM IN POC TO POC-COD-TCOM IN LO1-FLD.
           MOVE POC-COD-CVTI IN POC TO POC-COD-CVTI IN LO1-FLD.
           MOVE POC-COD-TDOC IN POC TO POC-COD-TDOC IN LO1-FLD.
           MOVE POC-MSC-TFPR IN POC TO POC-MSC-TFPR IN LO1-FLD.
           MOVE POC-IND-AVAL IN POC TO POC-IND-AVAL IN LO1-FLD.
           MOVE POC-IND-ENBO IN POC TO POC-IND-ENBO IN LO1-FLD.
           MOVE POC-NUM-PREP IN POC TO POC-NUM-PREP IN LO1-FLD.
           MOVE POC-MSC-UTPR IN POC TO POC-MSC-UTPR IN LO1-FLD.
*% INCLUDE FIN_PUT_POC_LO1 OF 'COLBMLO1.TXT'
       FIN-PUT-POC-LO1.
           EXIT.
       
       KEY-ALL-LO1 SECTION.
       INI-KEY-ALL-LO1.
*% INCLUDE INI_KEY_ALL_LO1 OF 'COLBMLO1.TXT'
           PERFORM KEY-LAP-LO1.
           PERFORM KEY-POC-LO1.
*% INCLUDE FIN_KEY_ALL_LO1 OF 'COLBMLO1.TXT'
       FIN-KEY-ALL-LO1.
           EXIT.
       
       KEY-LAP-LO1 SECTION.
       INI-KEY-LAP-LO1.
*% INCLUDE INI_KEY_LAP_LO1 OF 'COLBMLO1.TXT'
*% RREG = "LAP"
*% RREG_FRM = "LAP"
*% NOMB = "FLAP"
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE LAP-NUM-DLAP IN LO1-FLD TO LAP-NUM-DLAP IN LAP.
           MOVE LAP-NUM-MLAP IN LO1-FLD TO LAP-NUM-MLAP IN LAP.
           MOVE LAP-NUM-SLAP IN LO1-FLD TO LAP-NUM-SLAP IN LAP.
           MOVE LAP-NUM-ALAP IN LO1-FLD TO LAP-NUM-ALAP IN LAP.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE LAP-HRA-HLAP IN LO1-FLD TO LAP-HRA-HLAP IN LAP.
           MOVE LAP-COD-KLAP IN LO1-FLD TO LAP-COD-KLAP IN LAP.
           MOVE LAP-COD-AUTO IN LO1-FLD TO LAP-COD-AUTO IN LAP.
           MOVE LAP-NUM-CLAP IN LO1-FLD TO LAP-NUM-CLAP IN LAP.
*% INCLUDE FIN_KEY_LAP_LO1 OF 'COLBMLO1.TXT'
       FIN-KEY-LAP-LO1.
           EXIT.
       
       KEY-POC-LO1 SECTION.
       INI-KEY-POC-LO1.
*% INCLUDE INI_KEY_POC_LO1 OF 'COLBMLO1.TXT'
*% INCLUDE FIN_KEY_POC_LO1 OF 'COLBMLO1.TXT'
       FIN-KEY-POC-LO1.
           EXIT.
       
       SCR-CLRA-LO1 SECTION.
       INI-SCR-CLRA-LO1.
*% INCLUDE INI_SCR_CLRA_LO1 OF 'COLBMLO1.TXT'
           PERFORM CLR-KEY-LO1.
           PERFORM CLR-FLD-LO1.
*% INCLUDE FIN_SCR_CLRA_LO1 OF 'COLBMLO1.TXT'
       FIN-SCR-CLRA-LO1.
           EXIT.
       
       CLR-KEY-LO1 SECTION.
       INI-CLR-KEY-LO1.
*% INCLUDE INI_CLR_KEY_LO1 OF 'COLBMLO1.TXT'
           MOVE SPACES TO FRM-COD-CREB IN LO1-FLD.
           MOVE SPACES TO FRM-IND-NEXT IN LO1-FLD.
*% RREG = "LAP"
*% RREG_FRM = "LAP"
*% NOMB = "FLAP"
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO LAP-FEC-FLAP IN LO1-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO LAP-HRA-HLAP IN LO1-FLD.
           MOVE SPACES TO LAP-COD-KLAP IN LO1-FLD.
           MOVE SPACES TO LAP-COD-AUTO IN LO1-FLD.
           MOVE ZEROES TO LAP-NUM-CLAP IN LO1-FLD.
*% INCLUDE FIN_CLR_KEY_LO1 OF 'COLBMLO1.TXT'
       FIN-CLR-KEY-LO1.
           EXIT.
       
       CLR-FLD-LO1 SECTION.
       INI-CLR-FLD-LO1.
*% INCLUDE INI_CLR_FLD_LO1 OF 'COLBMLO1.TXT'
           MOVE SPACES TO FRM-GLS-INST IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX01 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX02 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX03 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX04 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX05 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX06 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX07 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX08 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX09 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX10 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX11 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX12 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX13 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX14 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX15 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX16 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX17 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX18 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX19 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX20 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX21 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX22 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX23 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX24 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX25 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX26 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX27 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX28 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX29 IN LO1-FLD.
           MOVE SPACES TO FRM-GLS-TX30 IN LO1-FLD.
           PERFORM CLR-LAP-LO1.
           PERFORM CLR-POC-LO1.
*% INCLUDE FIN_CLR_FLD_LO1 OF 'COLBMLO1.TXT'
       FIN-CLR-FLD-LO1.
           EXIT.
       
       CLR-LAP-LO1 SECTION.
       INI-CLR-LAP-LO1.
*% INCLUDE INI_CLR_LAP_LO1 OF 'COLBMLO1.TXT'
           MOVE SPACES TO LAP-COD-COME IN LO1-FLD.
           MOVE SPACES TO LAP-COD-CMND IN LO1-FLD.
           MOVE SPACES TO LAP-COD-OTRN IN LO1-FLD.
*% INCLUDE FIN_CLR_LAP_LO1 OF 'COLBMLO1.TXT'
       FIN-CLR-LAP-LO1.
           EXIT.
       
       CLR-POC-LO1 SECTION.
       INI-CLR-POC-LO1.
*% INCLUDE INI_CLR_POC_LO1 OF 'COLBMLO1.TXT'
           MOVE SPACES TO POC-COD-VCOC IN LO1-FLD.
           MOVE SPACES TO POC-COD-TOOC IN LO1-FLD.
           MOVE SPACES TO POC-COD-AOOC IN LO1-FLD.
           MOVE ZEROES TO POC-NUM-IPOC IN LO1-FLD.
           MOVE ZEROES TO POC-VAL-CMIN IN LO1-FLD.
           MOVE ZEROES TO POC-NUM-PMIN IN LO1-FLD.
           MOVE SPACES TO POC-IND-PMIN IN LO1-FLD.
           MOVE ZEROES TO POC-VAL-CMAX IN LO1-FLD.
           MOVE ZEROES TO POC-NUM-PMAX IN LO1-FLD.
           MOVE SPACES TO POC-IND-PMAX IN LO1-FLD.
           MOVE SPACES TO POC-IND-TICE IN LO1-FLD.
           MOVE ZEROES TO POC-NUM-VMIN IN LO1-FLD.
           MOVE SPACES TO POC-IND-TINE IN LO1-FLD.
           MOVE ZEROES TO POC-NUM-VMAX IN LO1-FLD.
           MOVE SPACES TO POC-IND-MTIC IN LO1-FLD.
           MOVE SPACES TO POC-IND-RENO IN LO1-FLD.
           MOVE SPACES TO POC-MSC-BCTI IN LO1-FLD.
           MOVE ZEROES TO POC-GLS-PCMI IN LO1-FLD.
           MOVE SPACES TO POC-MSC-PBTI IN LO1-FLD.
           MOVE ZEROES TO POC-GLS-PCMA IN LO1-FLD.
           MOVE SPACES TO POC-MSC-BPBT IN LO1-FLD.
           MOVE SPACES TO POC-COD-SEGD IN LO1-FLD.
           MOVE SPACES TO POC-IND-TCCE IN LO1-FLD.
           MOVE SPACES TO POC-IND-TCNE IN LO1-FLD.
           MOVE SPACES TO POC-MSC-SPBT IN LO1-FLD.
           MOVE SPACES TO POC-COD-TCOM IN LO1-FLD.
           MOVE SPACES TO POC-COD-CVTI IN LO1-FLD.
           MOVE SPACES TO POC-COD-TDOC IN LO1-FLD.
           MOVE SPACES TO POC-MSC-TFPR IN LO1-FLD.
           MOVE SPACES TO POC-IND-AVAL IN LO1-FLD.
           MOVE SPACES TO POC-IND-ENBO IN LO1-FLD.
           MOVE ZEROES TO POC-NUM-PREP IN LO1-FLD.
           MOVE SPACES TO POC-MSC-UTPR IN LO1-FLD.
*% INCLUDE FIN_CLR_POC_LO1 OF 'COLBMLO1.TXT'
       FIN-CLR-POC-LO1.
           EXIT.
       
       FST-KEY-LO1 SECTION.
       INI-FST-KEY-LO1.
*% INCLUDE INI_FST_KEY_LO1 OF 'COLBMLO1.TXT'
           MOVE -1 TO FRM-COD-CREB-LEN IN LO1-FLD.
*% INCLUDE FIN_FST_KEY_LO1 OF 'COLBMLO1.TXT'
       FIN-FST-KEY-LO1.
           EXIT.
       
       FST-FLD-LO1 SECTION.
       INI-FST-FLD-LO1.
*% INCLUDE INI_FST_FLD_LO1 OF 'COLBMLO1.TXT'
*% INCLUDE FIN_FST_FLD_LO1 OF 'COLBMLO1.TXT'
       FIN-FST-FLD-LO1.
           EXIT.
       
       PRO-KEY-LO1 SECTION.
       INI-PRO-KEY-LO1.
*% INCLUDE INI_PRO_KEY_LO1 OF 'COLBMLO1.TXT'
           MOVE FRM-CPIM-AEY TO FRM-COD-CREB-ATR IN LO1-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-NEXT-ATR IN LO1-FLD.
           PERFORM PRO-IKY-LO1.
           PERFORM PRO-AKY-LO1.
           PERFORM PRO-MKY-LO1.
*% INCLUDE FIN_PRO_KEY_LO1 OF 'COLBMLO1.TXT'
       FIN-PRO-KEY-LO1.
           EXIT.
       
       PRO-IKY-LO1 SECTION.
       INI-PRO-IKY-LO1.
*% INCLUDE INI_PRO_IKY_LO1 OF 'COLBMLO1.TXT'
*% RREG = "LAP"
*% RREG_FRM = "LAP"
*% NOMB = "FLAP"
           MOVE FRM-CPIM-AEY TO LAP-FEC-FLAP-ATR IN LO1-FLD.
           MOVE FRM-CPIM-AEY TO LAP-HRA-HLAP-ATR IN LO1-FLD.
           MOVE FRM-CPIM-AEY TO LAP-COD-KLAP-ATR IN LO1-FLD.
           MOVE FRM-CPIM-AEY TO LAP-COD-AUTO-ATR IN LO1-FLD.
*% INCLUDE FIN_PRO_IKY_LO1 OF 'COLBMLO1.TXT'
       FIN-PRO-IKY-LO1.
           EXIT.
       
       PRO-MKY-LO1 SECTION.
       INI-PRO-MKY-LO1.
*% INCLUDE INI_PRO_MKY_LO1 OF 'COLBMLO1.TXT'
*% INCLUDE FIN_PRO_MKY_LO1 OF 'COLBMLO1.TXT'
       FIN-PRO-MKY-LO1.
           EXIT.
       
       PRO-AKY-LO1 SECTION.
       INI-PRO-AKY-LO1.
*% INCLUDE INI_PRO_AKY_LO1 OF 'COLBMLO1.TXT'
*% INCLUDE FIN_PRO_AKY_LO1 OF 'COLBMLO1.TXT'
       FIN-PRO-AKY-LO1.
           EXIT.
       
       UNP-KEY-LO1 SECTION.
       INI-UNP-KEY-LO1.
*% INCLUDE INI_UNP_KEY_LO1 OF 'COLBMLO1.TXT'
           MOVE FRM-CPIM-UBY TO FRM-COD-CREB-ATR IN LO1-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IND-NEXT-ATR IN LO1-FLD.
           PERFORM UNP-IKY-LO1.
           PERFORM UNP-AKY-LO1.
           PERFORM UNP-MKY-LO1.
*% INCLUDE FIN_UNP_KEY_LO1 OF 'COLBMLO1.TXT'
       FIN-UNP-KEY-LO1.
           EXIT.
       
       UNP-IKY-LO1 SECTION.
       INI-UNP-IKY-LO1.
*% INCLUDE INI_UNP_IKY_LO1 OF 'COLBMLO1.TXT'
           MOVE FRM-CPIM-UBY TO FRM-COD-CREB-ATR.
           MOVE FRM-CPIM-UBY TO FRM-IND-NEXT-ATR.
*% RREG = "LAP"
*% RREG_FRM = "LAP"
*% NOMB = "FLAP"
           MOVE FRM-CPIM-UBY TO LAP-FEC-FLAP-ATR IN LO1-FLD.
           MOVE FRM-CPIM-UBY TO LAP-HRA-HLAP-ATR.
           MOVE FRM-CPIM-UBY TO LAP-COD-KLAP-ATR.
           MOVE FRM-CPIM-UBY TO LAP-COD-AUTO-ATR.
*% INCLUDE FIN_UNP_IKY_LO1 OF 'COLBMLO1.TXT'
       FIN-UNP-IKY-LO1.
           EXIT.
       
       UNP-MKY-LO1 SECTION.
       INI-UNP-MKY-LO1.
