           PERFORM UNP-LAP-LO1.
           PERFORM UNP-POC-LO1.
*% INCLUDE FIN_UNP_FLD_LO1 OF 'COLBMLO1.TXT'
       FIN-UNP-FLD-LO1.
           EXIT.
       
       UNP-LAP-LO1 SECTION.
       INI-UNP-LAP-LO1.
*% INCLUDE INI_UNP_LAP_LO1 OF 'COLBMLO1.TXT'
*% INCLUDE FIN_UNP_LAP_LO1 OF 'COLBMLO1.TXT'
       FIN-UNP-LAP-LO1.
           EXIT.
       
       UNP-POC-LO1 SECTION.
       INI-UNP-POC-LO1.
*% INCLUDE INI_UNP_POC_LO1 OF 'COLBMLO1.TXT'
*% INCLUDE FIN_UNP_POC_LO1 OF 'COLBMLO1.TXT'
       FIN-UNP-POC-LO1.
           EXIT.
       
       VAL-KEY-LO1 SECTION.
       INI-VAL-KEY-LO1.
           PERFORM VAL-IKY-LO1.
           PERFORM VAL-MKY-LO1.
           PERFORM VAL-AKY-LO1.
       FIN-VAL-KEY-LO1.
           EXIT.
       
       VAL-IKY-LO1 SECTION.
       INI-VAL-IKY-LO1.
*% INCLUDE INI_VAL_IKY_LO1 OF 'COLBMLO1.TXT'
           PERFORM VAL-FRM-COD-CREB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-CREB-LEN IN LO1-FLD
                GO TO FIN-VAL-IKY-LO1.
           PERFORM VAL-FRM-IND-NEXT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IND-NEXT-LEN IN LO1-FLD
                GO TO FIN-VAL-IKY-LO1.
*% RREG = "LAP"
*% RREG_FRM = "LAP"
*% NOMB = "FLAP"
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-LAP-FEC-FLAP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                LAP-FEC-FLAP-LEN IN LO1-FLD
                GO TO FIN-VAL-IKY-LO1.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-LAP-HRA-HLAP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO LAP-HRA-HLAP-LEN IN LO1-FLD
                GO TO FIN-VAL-IKY-LO1.
           PERFORM VAL-LAP-COD-KLAP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO LAP-COD-KLAP-LEN IN LO1-FLD
                GO TO FIN-VAL-IKY-LO1.
           PERFORM VAL-LAP-COD-AUTO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO LAP-COD-AUTO-LEN IN LO1-FLD
                GO TO FIN-VAL-IKY-LO1.
           PERFORM VAL-LAP-NUM-CLAP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO LAP-NUM-CLAP-LEN IN LO1-FLD
                GO TO FIN-VAL-IKY-LO1.
*% INCLUDE FIN_VAL_IKY_LO1 OF 'COLBMLO1.TXT'
       FIN-VAL-IKY-LO1.
           EXIT.
       
       VAL-MKY-LO1 SECTION.
       INI-VAL-MKY-LO1.
*% INCLUDE INI_VAL_MKY_LO1 OF 'COLBMLO1.TXT'
*% INCLUDE FIN_VAL_MKY_LO1 OF 'COLBMLO1.TXT'
       FIN-VAL-MKY-LO1.
           EXIT.
       
       VAL-AKY-LO1 SECTION.
       INI-VAL-AKY-LO1.
*% INCLUDE INI_VAL_AKY_LO1 OF 'COLBMLO1.TXT'
*% INCLUDE FIN_VAL_AKY_LO1 OF 'COLBMLO1.TXT'
       FIN-VAL-AKY-LO1.
           EXIT.
       
       VAL-FRM-COD-CREB SECTION.
       INI-VAL-FRM-COD-CREB.
*% LBL = "FRM_COD_CREB OF 'COLBMLO1.TXT'"
*% INCLUDE INI_FRM_COD_CREB OF 'COLBMLO1.TXT'
           IF FRM-COD-CREB IN LO1-FLD NOT > SPACES
