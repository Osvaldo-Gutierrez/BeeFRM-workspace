*% IZR = "S"
       
       VAL-CON-KEY-DTC SECTION.
       INI-VAL-CON-KEY-DTC.
*% INCLUDE INI_VAL_CON_KEY_DTC OF 'COLBMDTC.TXT'
           IF     OPC-CAI-IOPC IN DTC-FLD NOT > SPACES 
              AND OPC-IIC-IOPC IN DTC-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-DTC.
*% INCLUDE FIN_VAL_CON_KEY_DTC OF 'COLBMDTC.TXT'
       FIN-VAL-CON-KEY-DTC.
           EXIT.
       
       VAL-CON-IKY-DTC SECTION.
       INI-VAL-CON-IKY-DTC.
*% INCLUDE INI_VAL_CON_IKY_DTC OF 'COLBMDTC.TXT'
           IF     OPC-CAI-IOPC IN DTC-FLD NOT > SPACES 
              AND OPC-IIC-IOPC IN DTC-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-DTC.
*% INCLUDE FIN_VAL_CON_IKY_DTC OF 'COLBMDTC.TXT'
       FIN-VAL-CON-IKY-DTC.
           EXIT.
       
       VAL-NUM-KEY-DTC SECTION.
