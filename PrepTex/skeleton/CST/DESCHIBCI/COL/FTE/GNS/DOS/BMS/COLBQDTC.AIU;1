*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% IZR = "S"
      * IDC-INI --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
*% FLD = "DBC-NUM-ICLI"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% BUF = "DTC-FLD"
       VAL-NUM-{FLD}.
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-FLD-DTC.
      * IDC-FIN --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
*% INCLUDE FIN_VAL_NUM_FLD_DTC OF 'COLBMDTC.TXT'
       FIN-VAL-NUM-FLD-DTC.
           EXIT.
       
       EDT-KEY-DTC SECTION.
       INI-EDT-KEY-DTC.
           PERFORM EDT-IKY-DTC.
           PERFORM EDT-MKY-DTC.
           PERFORM EDT-AKY-DTC.
*% INCLUDE EDT_KEY_DTC OF 'COLBMDTC.TXT'
       FIN-EDT-KEY-DTC.
           EXIT.
       
       EDT-IKY-DTC SECTION.
       INI-EDT-IKY-DTC.
*% INCLUDE EDT_IKY_DTC OF 'COLBMDTC.TXT'
           IF OPC-IIC-IOPC-LEN IN DTC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO OPC-IIC-IOPC-LEN IN DTC-FLD
                GO TO FIN-EDT-IKY-DTC.
           MOVE OPC-IIC-IOPC IN DTC-FLD TO OPC-IIC-IOPC-EDT IN DTC-EDT.
           MOVE OPC-IIC-IOPC-EDT IN DTC-EDT TO 
                OPC-IIC-IOPC-ALF IN DTC-FLD.
*% INCLUDE FIN_EDT_IKY_DTC OF 'COLBMDTC.TXT'
       FIN-EDT-IKY-DTC.
           EXIT.
       
       EDT-MKY-DTC SECTION.
       INI-EDT-MKY-DTC.
*% INCLUDE EDT_MKY_DTC OF 'COLBMDTC.TXT'
*% INCLUDE FIN_EDT_MKY_DTC OF 'COLBMDTC.TXT'
       FIN-EDT-MKY-DTC.
           EXIT.
       
       EDT-AKY-DTC SECTION.
       INI-EDT-AKY-DTC.
*% INCLUDE EDT_AKY_DTC OF 'COLBMDTC.TXT'
*% INCLUDE FIN_EDT_AKY_DTC OF 'COLBMDTC.TXT'
       FIN-EDT-AKY-DTC.
           EXIT.
       
       EDT-FLD-DTC SECTION.
       INI-EDT-FLD-DTC.
*% INCLUDE EDT_FLD_DTC OF 'COLBMDTC.TXT'
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
*% RREG = "DBC"
*% RREG_FRM = "DBC"
*% NOMB = "ICLI"
      * IDC-INI --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF DBC-NUM-ICLI-LEN IN DTC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DBC-NUM-ICLI-LEN IN DTC-FLD
                GO TO FIN-EDT-FLD-DTC.
           MOVE DBC-NUM-ICLI IN DTC-FLD TO
                DBC-NUM-ICLI-EDT IN DTC-EDT.
           MOVE DBC-NUM-ICLI-EDT IN DTC-EDT TO
                DBC-NUM-ICLI-ALF IN DTC-FLD.
      * IDC-FIN --> BMCHIBCI011.SRCAIU EDICION NUMERICA
*% INCLUDE FIN_EDT_FLD_DTC OF 'COLBMDTC.TXT'
       FIN-EDT-FLD-DTC.
           EXIT.
*% INCLUDE EOF_BQ OF 'COLBMDTC.TXT'
*% INCLUDE EOF OF 'COLBMDTC.TXT'
*% INCLUDE EOF_IBM OF 'COLBMDTC.TXT'
