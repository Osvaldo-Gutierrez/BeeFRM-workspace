           PERFORM VAL-NUM-AKY-DTC.
       FIN-VAL-NUM-KEY-DTC.
           EXIT.
       
       VAL-NUM-IKY-DTC SECTION.
       INI-VAL-NUM-IKY-DTC.
*% INCLUDE INI_VAL_NUM_IKY_DTC OF 'COLBMDTC.TXT'
       VAL-NUM-OPC-IIC-IOPC.
*% BUF = "DTC-FLD"
*% FLD = "OPC-IIC-IOPC"
*% ENT = "8"
*% DCM = "0"
*% NMR = "NN9N"
*% ISG = "N"
*% IZR = "S"
*% INCLUDE 'VALNUM'
                GO TO FIN-VAL-NUM-IKY-DTC.
*% INCLUDE FIN_VAL_NUM_IKY_DTC OF 'COLBMDTC.TXT'
