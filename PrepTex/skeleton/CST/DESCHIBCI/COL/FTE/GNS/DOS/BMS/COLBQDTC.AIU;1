*% INCLUDE INI_UNP_AKY_DTC OF 'COLBMDTC.TXT'
*% INCLUDE FIN_UNP_AKY_DTC OF 'COLBMDTC.TXT'
       FIN-UNP-AKY-DTC.
           EXIT.
       
