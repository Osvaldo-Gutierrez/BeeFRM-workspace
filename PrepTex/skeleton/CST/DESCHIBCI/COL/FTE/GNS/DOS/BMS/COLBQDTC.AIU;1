*% INCLUDE FIN_UNP_DBC_DTC OF 'COLBMDTC.TXT'
       FIN-UNP-DBC-DTC.
           EXIT.
       
       VAL-KEY-DTC SECTION.
       INI-VAL-KEY-DTC.
           PERFORM VAL-IKY-DTC.
           PERFORM VAL-MKY-DTC.
           PERFORM VAL-AKY-DTC.
       FIN-VAL-KEY-DTC.
           EXIT.
       
       VAL-IKY-DTC SECTION.
       INI-VAL-IKY-DTC.
*% INCLUDE INI_VAL_IKY_DTC OF 'COLBMDTC.TXT'
           PERFORM VAL-OPC-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-CAI-IOPC-LEN IN DTC-FLD
                GO TO FIN-VAL-IKY-DTC.
           PERFORM VAL-OPC-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-IIC-IOPC-LEN IN DTC-FLD
                GO TO FIN-VAL-IKY-DTC.
*% INCLUDE FIN_VAL_IKY_DTC OF 'COLBMDTC.TXT'
       FIN-VAL-IKY-DTC.
           EXIT.
       
       VAL-MKY-DTC SECTION.
       INI-VAL-MKY-DTC.
*% INCLUDE INI_VAL_MKY_DTC OF 'COLBMDTC.TXT'
*% INCLUDE FIN_VAL_MKY_DTC OF 'COLBMDTC.TXT'
       FIN-VAL-MKY-DTC.
           EXIT.
       
       VAL-AKY-DTC SECTION.
       INI-VAL-AKY-DTC.
*% INCLUDE INI_VAL_AKY_DTC OF 'COLBMDTC.TXT'
*% INCLUDE FIN_VAL_AKY_DTC OF 'COLBMDTC.TXT'
       FIN-VAL-AKY-DTC.
           EXIT.
       
       VAL-OPC-CAI-IOPC SECTION.
       INI-VAL-OPC-CAI-IOPC.
*% LBL = "OPC_CAI_IOPC OF 'COLBMDTC.TXT'"
*% INCLUDE INI_OPC_CAI_IOPC OF 'COLBMDTC.TXT'
           IF OPC-CAI-IOPC IN DTC-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-OPC-CAI-IOPC.
*% INCLUDE OPC_CAI_IOPC OF 'COLBMDTC.TXT'
*% INCLUDE FIN_OPC_CAI_IOPC OF 'COLBMDTC.TXT'
       FIN-VAL-OPC-CAI-IOPC.
           EXIT.
       
       VAL-OPC-IIC-IOPC SECTION.
       INI-VAL-OPC-IIC-IOPC.
*% LBL = "OPC_IIC_IOPC OF 'COLBMDTC.TXT'"
*% INCLUDE INI_OPC_IIC_IOPC OF 'COLBMDTC.TXT'
*% INCLUDE OPC_IIC_IOPC OF 'COLBMDTC.TXT'
*% INCLUDE FIN_OPC_IIC_IOPC OF 'COLBMDTC.TXT'
       FIN-VAL-OPC-IIC-IOPC.
           EXIT.
*% RREG = "DBC"
