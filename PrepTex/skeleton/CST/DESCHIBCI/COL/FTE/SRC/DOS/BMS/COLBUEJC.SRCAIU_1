      *
      * COLBUEJC
      * Version 3.0.1 Nov-2005
      * 20-Aug-19 03:50 PM
      *
       SET-TAG-COLBUEJC SECTION.
       INI-SET-TAG-COLBUEJC.
           MOVE
           '<IDNMOD NOM=$COLBUEJC$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$20-Aug-19 03:50 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUEJC.
           EXIT.
           
       RET-ALL-EJC SECTION.
       INI-RET-ALL-EJC.
           PERFORM KEY-ALL-EJC.
           PERFORM RET-EJC-EJC.
           PERFORM RET-USR-EJC.
       FIN-RET-ALL-EJC.
           EXIT.
       
       RET-EJC-EJC SECTION.
       INI-RET-EJC-EJC.
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE EJC-NUM-DTRN IN EJC-FLD TO EJC-NUM-DTRN IN EJC.
           MOVE EJC-NUM-MTRN IN EJC-FLD TO EJC-NUM-MTRN IN EJC.
           MOVE EJC-NUM-STRN IN EJC-FLD TO EJC-NUM-STRN IN EJC.
           MOVE EJC-NUM-ATRN IN EJC-FLD TO EJC-NUM-ATRN IN EJC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE EJC-COD-ATRI IN EJC-FLD TO EJC-COD-ATRI IN EJC.
           MOVE EJC-VAL-CMAX IN EJC-FLD TO EJC-VAL-CMAX IN EJC.
           MOVE EJC-COD-VCAM IN EJC-FLD TO EJC-COD-VCAM IN EJC.
           MOVE EJC-VAL-DMAX IN EJC-FLD TO EJC-VAL-DMAX IN EJC.
       FIN-RET-EJC-EJC.
           EXIT.
       
       RET-USR-EJC SECTION.
       INI-RET-USR-EJC.
           MOVE USR-GLS-DESC IN EJC-FLD TO USR-GLS-DESC IN USR.
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE USR-NUM-IUSR IN EJC-FLD TO USR-NUM-IUSR IN USR.
           MOVE USR-VRF-IUSR IN EJC-FLD TO USR-VRF-IUSR IN USR.
           MOVE USR-IND-IUSR IN EJC-FLD TO USR-IND-IUSR IN USR.
           MOVE USR-GLS-IUSR IN EJC-FLD TO USR-GLS-IUSR IN USR.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE USR-IND-VIGE IN EJC-FLD TO USR-IND-VIGE IN USR.
           MOVE USR-COD-UBIC IN EJC-FLD TO USR-COD-UBIC IN USR.
       FIN-RET-USR-EJC.
           EXIT.
       
       KEY-ALL-EJC SECTION.
       INI-KEY-ALL-EJC.
           PERFORM KEY-EJC-EJC.
           PERFORM KEY-USR-EJC.
       FIN-KEY-ALL-EJC.
           EXIT.
       
       KEY-EJC-EJC SECTION.
       INI-KEY-EJC-EJC.
           MOVE EJC-COD-EJEC IN EJC-FLD TO EJC-COD-EJEC IN EJC.
       FIN-KEY-EJC-EJC.
           EXIT.
       
       KEY-USR-EJC SECTION.
       INI-KEY-USR-EJC.
       FIN-KEY-USR-EJC.
           EXIT.
       
       SCR-CLRA-EJC SECTION.
       INI-SCR-CLRA-EJC.
           PERFORM CLR-KEY-EJC.
           PERFORM CLR-FLD-EJC.
       FIN-SCR-CLRA-EJC.
           EXIT.
       
       CLR-KEY-EJC SECTION.
       INI-CLR-KEY-EJC.
           MOVE SPACES TO EJC-COD-EJEC IN EJC-FLD.
       FIN-CLR-KEY-EJC.
           EXIT.
       
       CLR-FLD-EJC SECTION.
       INI-CLR-FLD-EJC.
           MOVE SPACES TO FRM-GLS-ATRI IN EJC-FLD.
           MOVE SPACES TO FRM-GLS-UBIC IN EJC-FLD.
           MOVE SPACES TO FRM-GLS-VCAM IN EJC-FLD.
           PERFORM CLR-EJC-EJC.
           PERFORM CLR-USR-EJC.
       FIN-CLR-FLD-EJC.
           EXIT.
       
       CLR-EJC-EJC SECTION.
       INI-CLR-EJC-EJC.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO EJC-FEC-FTRN IN EJC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO EJC-COD-ATRI IN EJC-FLD.
           MOVE ZEROES TO EJC-VAL-CMAX IN EJC-FLD.
           MOVE SPACES TO EJC-COD-VCAM IN EJC-FLD.
           MOVE ZEROES TO EJC-VAL-DMAX IN EJC-FLD.
      *<<<< FIN_CLR_EJC_EJC
           MOVE SPACES TO EJC-FEC-FTRN IN EJC-FLD.
      *>>>>
       FIN-CLR-EJC-EJC.
           EXIT.
       
       CLR-USR-EJC SECTION.
       INI-CLR-USR-EJC.
           MOVE SPACES TO USR-GLS-DESC IN EJC-FLD.
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO USR-NUM-IUSR IN EJC-FLD.
           MOVE SPACES TO USR-VRF-IUSR IN EJC-FLD.
           MOVE SPACES TO USR-IND-IUSR IN EJC-FLD.
           MOVE SPACES TO USR-GLS-IUSR IN EJC-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO USR-IND-VIGE IN EJC-FLD.
           MOVE SPACES TO USR-COD-UBIC IN EJC-FLD.
       FIN-CLR-USR-EJC.
           EXIT.
       
       FST-KEY-EJC SECTION.
       INI-FST-KEY-EJC.
           MOVE -1 TO EJC-COD-EJEC-LEN IN EJC-FLD.
       FIN-FST-KEY-EJC.
           EXIT.
       
       FST-FLD-EJC SECTION.
       INI-FST-FLD-EJC.
           MOVE -1 TO EJC-COD-ATRI-LEN IN EJC-FLD.
       FIN-FST-FLD-EJC.
           EXIT.
       
       PRO-KEY-EJC SECTION.
       INI-PRO-KEY-EJC.
           PERFORM PRO-MKY-EJC.
       FIN-PRO-KEY-EJC.
           EXIT.
       
       PRO-MKY-EJC SECTION.
       INI-PRO-MKY-EJC.
       FIN-PRO-MKY-EJC.
           EXIT.
       
       PRO-FLD-EJC SECTION.
       INI-PRO-FLD-EJC.
           PERFORM PRO-EJC-EJC.
           PERFORM PRO-USR-EJC.
       FIN-PRO-FLD-EJC.
           EXIT.
       
       PRO-EJC-EJC SECTION.
       INI-PRO-EJC-EJC.
           MOVE FRM-CPIM-AEY TO EJC-COD-ATRI-ATR IN EJC-FLD.
           MOVE FRM-CPIM-AEY TO EJC-VAL-CMAX-ATR IN EJC-FLD.
           MOVE FRM-CPIM-AEY TO EJC-COD-VCAM-ATR IN EJC-FLD.
           MOVE FRM-CPIM-AEY TO EJC-VAL-DMAX-ATR IN EJC-FLD.
       FIN-PRO-EJC-EJC.
           EXIT.
       
       PRO-USR-EJC SECTION.
       INI-PRO-USR-EJC.
       FIN-PRO-USR-EJC.
           EXIT.
       
       UNP-KEY-EJC SECTION.
       INI-UNP-KEY-EJC.
           PERFORM UNP-IKY-EJC.
           PERFORM UNP-AKY-EJC.
           PERFORM UNP-MKY-EJC.
       FIN-UNP-KEY-EJC.
           EXIT.
       
       UNP-IKY-EJC SECTION.
       INI-UNP-IKY-EJC.
           MOVE FRM-CPIM-UBY TO EJC-COD-EJEC-ATR.
       FIN-UNP-IKY-EJC.
           EXIT.
       
       UNP-MKY-EJC SECTION.
       INI-UNP-MKY-EJC.
       FIN-UNP-MKY-EJC.
           EXIT.
       
       UNP-AKY-EJC SECTION.
       INI-UNP-AKY-EJC.
       FIN-UNP-AKY-EJC.
           EXIT.
       
       UNP-FLD-EJC SECTION.
       INI-UNP-FLD-EJC.
           PERFORM UNP-EJC-EJC.
           PERFORM UNP-USR-EJC.
       FIN-UNP-FLD-EJC.
           EXIT.
       
       UNP-EJC-EJC SECTION.
       INI-UNP-EJC-EJC.
           MOVE FRM-CPIM-UBY TO EJC-COD-ATRI-ATR IN EJC-FLD.
           MOVE FRM-CPIM-UBY TO EJC-VAL-CMAX-ATR IN EJC-FLD.
           MOVE FRM-CPIM-UBY TO EJC-COD-VCAM-ATR IN EJC-FLD.
           MOVE FRM-CPIM-UBY TO EJC-VAL-DMAX-ATR IN EJC-FLD.
       FIN-UNP-EJC-EJC.
           EXIT.
       
       UNP-USR-EJC SECTION.
       INI-UNP-USR-EJC.
       FIN-UNP-USR-EJC.
           EXIT.
       
       VAL-MKY-EJC SECTION.
       INI-VAL-MKY-EJC.
       FIN-VAL-MKY-EJC.
           EXIT.
       
       VAL-FLD-EJC SECTION.
       INI-VAL-FLD-EJC.
           PERFORM VAL-USR-GLS-DESC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO USR-GLS-DESC-LEN IN EJC-FLD
                GO TO FIN-VAL-FLD-EJC.
      * VRF-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * VRF-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * IDC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-USR-IND-IUSR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                USR-IND-IUSR-LEN IN EJC-FLD
                GO TO FIN-VAL-FLD-EJC.
           PERFORM VAL-USR-GLS-IUSR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                USR-GLS-IUSR-LEN IN EJC-FLD
                GO TO FIN-VAL-FLD-EJC.
           PERFORM VAL-USR-NUM-IUSR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                USR-NUM-IUSR-LEN IN EJC-FLD
                GO TO FIN-VAL-FLD-EJC.
           PERFORM VAL-USR-VRF-IUSR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                USR-VRF-IUSR-LEN IN EJC-FLD
                GO TO FIN-VAL-FLD-EJC.
      * IDC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-USR-IND-VIGE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO USR-IND-VIGE-LEN IN EJC-FLD
                GO TO FIN-VAL-FLD-EJC.
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-EJC-FEC-FTRN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                EJC-FEC-FTRN-LEN IN EJC-FLD
                GO TO FIN-VAL-FLD-EJC.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-EJC-COD-ATRI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EJC-COD-ATRI-LEN IN EJC-FLD
                GO TO FIN-VAL-FLD-EJC.
           PERFORM VAL-USR-COD-UBIC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO USR-COD-UBIC-LEN IN EJC-FLD
                GO TO FIN-VAL-FLD-EJC.
           PERFORM VAL-FRM-GLS-ATRI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-ATRI-LEN IN EJC-FLD
                GO TO FIN-VAL-FLD-EJC.
           PERFORM VAL-FRM-GLS-UBIC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-UBIC-LEN IN EJC-FLD
                GO TO FIN-VAL-FLD-EJC.
           PERFORM VAL-EJC-VAL-CMAX.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EJC-VAL-CMAX-LEN IN EJC-FLD
                GO TO FIN-VAL-FLD-EJC.
           PERFORM VAL-EJC-COD-VCAM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EJC-COD-VCAM-LEN IN EJC-FLD
                GO TO FIN-VAL-FLD-EJC.
           PERFORM VAL-EJC-VAL-DMAX.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EJC-VAL-DMAX-LEN IN EJC-FLD
                GO TO FIN-VAL-FLD-EJC.
           PERFORM VAL-FRM-GLS-VCAM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-VCAM-LEN IN EJC-FLD
                GO TO FIN-VAL-FLD-EJC.
       FIN-VAL-FLD-EJC.
           EXIT.
       
       VAL-USR-GLS-DESC SECTION.
       INI-VAL-USR-GLS-DESC.
       FIN-VAL-USR-GLS-DESC.
           EXIT.
      * VRF-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
      * VRF-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
      * IDC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION

       VAL-USR-NUM-IUSR SECTION.
       INI-VAL-USR-NUM-IUSR.
       FIN-VAL-USR-NUM-IUSR.
           EXIT.

       VAL-USR-VRF-IUSR SECTION.
       INI-VAL-USR-VRF-IUSR.
      *<<<< INI_USR_VRF_IUSR
      *VAL-RUT Valida Rut para campo IDC
           MOVE IDC-ZERO-S TO IDC-ZERO.
           MOVE USR-NUM-IUSR IN EJC-FLD TO IDC-NUM-RUTV.
           MOVE USR-VRF-IUSR IN EJC-FLD TO IDC-VRF-RUTV.
           PERFORM VAL-RUT.
           GO TO FIN-VAL-USR-VRF-IUSR.
      *>>>>
           IF USR-VRF-IUSR IN EJC-FLD NOT > SPACES
              MOVE SPACES TO USR-VRF-IUSR IN EJC-FLD.
       FIN-VAL-USR-VRF-IUSR.
           EXIT.
       VAL-USR-IND-IUSR SECTION.
       INI-VAL-USR-IND-IUSR.
      *<<<< INI_USR_IND_IUSR
      *VAL-IDC Valida identificador
           MOVE USR-IND-IUSR IN EJC-FLD TO IDC-IND-RUTV.
           PERFORM VAL-IDC.
           MOVE IDC-IND-RUTV TO USR-IND-IUSR IN EJC-FLD.
           GO TO FIN-VAL-USR-IND-IUSR.
      *>>>>
           IF USR-IND-IUSR IN EJC-FLD NOT > SPACES
              MOVE SPACES TO USR-IND-IUSR IN EJC-FLD.
       FIN-VAL-USR-IND-IUSR.
           EXIT.

       VAL-USR-GLS-IUSR SECTION.
       INI-VAL-USR-GLS-IUSR.
      *<<<< INI_USR_GLS_IUSR
      *VAL-IDC Valida identificador
           MOVE USR-IND-IUSR IN EJC-FLD TO IDC-IND-RUTV.
           PERFORM VAL-IDC.
           MOVE IDC-IND-RUTV TO USR-IND-IUSR IN EJC-FLD.
           GO TO FIN-VAL-USR-GLS-IUSR.
      *>>>>
           IF USR-GLS-IUSR IN EJC-FLD NOT > IDC-INIC-GLS 
              MOVE IDC-INIC-GLS TO USR-GLS-IUSR IN EJC-FLD.
      *VAL-GLS para IDC valida glosa ( extension ) de identificador
           MOVE USR-GLS-IUSR IN EJC-FLD TO IDC-GLS-RUTV.
           PERFORM VAL-GLS.
           MOVE IDC-GLS-RUTV TO USR-GLS-IUSR IN EJC-FLD.
       FIN-VAL-USR-GLS-IUSR.
           EXIT.

      * IDC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-USR-IND-VIGE SECTION.
       INI-VAL-USR-IND-VIGE.
       FIN-VAL-USR-IND-VIGE.
           EXIT.
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-EJC-FEC-FTRN SECTION.
       INI-VAL-EJC-FEC-FTRN.
           IF EJC-FEC-FTRN IN EJC-FLD  NOT > ZEROES
              MOVE ZEROES TO EJC-FEC-FTRN IN EJC-FLD.
       FIN-VAL-EJC-FEC-FTRN.
           EXIT.
      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-EJC-COD-ATRI SECTION.
       INI-VAL-EJC-COD-ATRI.
           IF EJC-COD-ATRI IN EJC-FLD NOT > SPACES
              MOVE SPACES TO EJC-COD-ATRI IN EJC-FLD.
      *VAL-COD Valida codigo de tabla
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'CUS ' TO TAB-COD-TTAB IN TAB.
           MOVE EJC-COD-ATRI IN EJC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              EJC-COD-ATRI IN EJC-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-ATRI IN EJC-FLD
           ELSE
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-ATRI IN EJC-FLD.
       FIN-VAL-EJC-COD-ATRI.
           EXIT.
       
       VAL-USR-COD-UBIC SECTION.
       INI-VAL-USR-COD-UBIC.
       FIN-VAL-USR-COD-UBIC.
           EXIT.
       
       VAL-FRM-GLS-ATRI SECTION.
       INI-VAL-FRM-GLS-ATRI.
       FIN-VAL-FRM-GLS-ATRI.
           EXIT.
       
       VAL-FRM-GLS-UBIC SECTION.
       INI-VAL-FRM-GLS-UBIC.
       FIN-VAL-FRM-GLS-UBIC.
           EXIT.
       
       VAL-EJC-VAL-CMAX SECTION.
       INI-VAL-EJC-VAL-CMAX.
       FIN-VAL-EJC-VAL-CMAX.
           EXIT.
       
       VAL-EJC-COD-VCAM SECTION.
       INI-VAL-EJC-COD-VCAM.
           IF EJC-COD-VCAM IN EJC-FLD NOT > SPACES
              MOVE SPACES TO EJC-COD-VCAM IN EJC-FLD.
      *VAL-COD Valida codigo de tabla
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'VLR ' TO TAB-COD-TTAB IN TAB.
           MOVE EJC-COD-VCAM IN EJC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              EJC-COD-VCAM IN EJC-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-VCAM IN EJC-FLD
           ELSE
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-VCAM IN EJC-FLD.
       FIN-VAL-EJC-COD-VCAM.
           EXIT.
       
       VAL-EJC-VAL-DMAX SECTION.
       INI-VAL-EJC-VAL-DMAX.
      *<<<< EJC_VAL_DMAX
           IF EJC-VAL-DMAX IN EJC-FLD < EJC-VAL-CMAX IN EJC-FLD
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'VALDMAXCMAX<' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>>
       FIN-VAL-EJC-VAL-DMAX.
           EXIT.
       
       VAL-FRM-GLS-VCAM SECTION.
       INI-VAL-FRM-GLS-VCAM.
       FIN-VAL-FRM-GLS-VCAM.
           EXIT.
       
       VAL-CON-FLD-EJC SECTION.
       INI-VAL-CON-FLD-EJC.
       FIN-VAL-CON-FLD-EJC.
           EXIT.
       
       VAL-CON-EJC SECTION.
       INI-VAL-CON-EJC.
       FIN-VAL-CON-EJC.
           EXIT.
       
       VAL-NUM-KEY-EJC SECTION.
       INI-VAL-NUM-KEY-EJC.
           PERFORM VAL-NUM-IKY-EJC.
           PERFORM VAL-NUM-MKY-EJC.
           PERFORM VAL-NUM-AKY-EJC.
       FIN-VAL-NUM-KEY-EJC.
           EXIT.
       
       VAL-NUM-IKY-EJC SECTION.
       INI-VAL-NUM-IKY-EJC.
       FIN-VAL-NUM-IKY-EJC.
           EXIT.
       
       VAL-NUM-MKY-EJC SECTION.
       INI-VAL-NUM-MKY-EJC.
       FIN-VAL-NUM-MKY-EJC.
           EXIT.
       
       VAL-NUM-AKY-EJC SECTION.
       INI-VAL-NUM-AKY-EJC.
       FIN-VAL-NUM-AKY-EJC.
           EXIT.
       
       VAL-NUM-FLD-EJC SECTION.
       INI-VAL-NUM-FLD-EJC.
      * IDC-INI --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
       VAL-NUM-USR-NUM-IUSR.
           MOVE USR-NUM-IUSR-ALF IN EJC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO USR-NUM-IUSR IN EJC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO USR-NUM-IUSR-LEN IN EJC-FLD
                GO TO FIN-VAL-NUM-FLD-EJC.
      * IDC-FIN --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
       VAL-NUM-EJC-VAL-CMAX.
           MOVE EJC-VAL-CMAX-ALF IN EJC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9V TO EJC-VAL-CMAX IN EJC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO EJC-VAL-CMAX-LEN IN EJC-FLD
                GO TO FIN-VAL-NUM-FLD-EJC.
       VAL-NUM-EJC-VAL-DMAX.
           MOVE EJC-VAL-DMAX-ALF IN EJC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9V TO EJC-VAL-DMAX IN EJC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO EJC-VAL-DMAX-LEN IN EJC-FLD
                GO TO FIN-VAL-NUM-FLD-EJC.
       FIN-VAL-NUM-FLD-EJC.
           EXIT.
       
       EDT-KEY-EJC SECTION.
       INI-EDT-KEY-EJC.
           PERFORM EDT-IKY-EJC.
           PERFORM EDT-MKY-EJC.
           PERFORM EDT-AKY-EJC.
       FIN-EDT-KEY-EJC.
           EXIT.
       
       EDT-IKY-EJC SECTION.
