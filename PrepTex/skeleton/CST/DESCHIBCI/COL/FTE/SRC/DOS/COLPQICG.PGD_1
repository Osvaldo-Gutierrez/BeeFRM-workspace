           IF SCR-IND-ENG IN SCR-VEC-ENG(SCR-IPRO) = 'S'                
               PERFORM HBL-KEY-ICG.                                      
      *>>>>
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE ICG-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO ICG-FLD
               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO ICG-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO ICG-GLS-MENS.
           PERFORM UNP-KEY-ICG.
           PERFORM FST-KEY-ICG.
      *<<<< FIN_HBL_KEY
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           MOVE '1' TO FRM-CPIM-AEY. 
      *>>>>
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
           PERFORM PRO-IKY-ICG.
           PERFORM PRO-AKY-ICG.
           PERFORM UNP-MKY-ICG.
           PERFORM UNP-FLD-ICG.
           PERFORM FST-FLD-ICG.
      *<<<< FIN_HBL_FLD
      *ALD
           IF SCR-CCPP NOT = 'ING'
               GO TO FIN-HBL-FLD.
           IF NOT PGM-STAT-POC-OKS
               MOVE ICG-CAI-IOPC IN ICG-FLD TO OPC-CAI-IOPC IN OPC
               MOVE ICG-IIC-IOPC IN ICG-FLD TO OPC-IIC-IOPC IN OPC
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM COL-FIO-OPC
               IF NOT FIO-STAT-OKS
                   GO TO FIN-HBL-FLD
               ELSE
                   MOVE OPC-COD-VCOC IN OPC TO POC-COD-VCOC IN POC
                   MOVE OPC-COD-TOOC IN OPC TO POC-COD-TOOC IN POC
                   MOVE OPC-COD-AOOC IN OPC TO POC-COD-AOOC IN POC
                   MOVE ZEROES              TO POC-NUM-IPOC IN POC
                   MOVE FIO-GET-KEY TO FIO-CMND
                   PERFORM COL-FIO-POC
                   IF NOT FIO-STAT-OKS
                       GO TO FIN-HBL-FLD.

           IF POC-MSC-BCTI IN POC > SPACES
               MOVE POC-MSC-BCTI IN POC TO ICG-MSC-BCTI IN ICG-FLD.
           IF POC-MSC-PBTI IN POC > SPACES 
               MOVE POC-MSC-PBTI IN POC TO ICG-MSC-PBTI IN ICG-FLD.
           IF POC-MSC-BPBT IN POC > SPACES
               MOVE POC-MSC-BPBT IN POC TO ICG-MSC-BPBT IN ICG-FLD.
           IF POC-MSC-SPBT IN POC > SPACES
               MOVE POC-MSC-SPBT IN POC TO ICG-MSC-SPBT IN ICG-FLD.
           IF POC-COD-CVTI IN POC > SPACES
               MOVE POC-COD-CVTI IN POC TO ICG-COD-CVTI IN ICG-FLD.
           IF POC-MSC-TFPR IN POC > SPACES
               MOVE POC-MSC-TFPR IN POC TO ICG-MSC-TFPR IN ICG-FLD.
           IF POC-NUM-PREP IN POC > ZEROES
               MOVE POC-NUM-PREP IN POC TO ICG-NUM-PREP IN ICG-FLD.
           IF POC-MSC-UTPR IN POC > SPACES
               MOVE POC-MSC-UTPR IN POC TO ICG-MSC-UTPR IN ICG-FLD.
      *
      *>>>>
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO ICG-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-ICG.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
      *PTC-INI
           IF SCR-STAT NOT = SCR-STPP
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-ICG
               PERFORM EDT-FLD-ICG
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-ICG
               PERFORM EDT-FLD-ICG
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-ICG.
           PERFORM EDT-FLD-ICG.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

