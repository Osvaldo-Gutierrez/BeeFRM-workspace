      *
      * COLBQCFN
      * Version 3.0.1 Nov-2005
      * 24-May-19 03:08 PM
      *
       SET-TAG-COLBQCFN SECTION.
       INI-SET-TAG-COLBQCFN.
           MOVE
           '<IDNMOD NOM=$COLBQCFN$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$24-May-19 03:08 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCFN.
           EXIT.
           
       PUT-ALL-CFN SECTION.
       INI-PUT-ALL-CFN.
       FIN-PUT-ALL-CFN.
           EXIT.
       
       KEY-ALL-CFN SECTION.
       INI-KEY-ALL-CFN.
       FIN-KEY-ALL-CFN.
           EXIT.
       
       SCR-CLRA-CFN SECTION.
       INI-SCR-CLRA-CFN.
           PERFORM CLR-KEY-CFN.
           PERFORM CLR-FLD-CFN.
       FIN-SCR-CLRA-CFN.
           EXIT.
       
       CLR-KEY-CFN SECTION.
       INI-CLR-KEY-CFN.
           MOVE ZEROES TO FRM-NUM-PLZO IN CFN-FLD.
           MOVE SPACES TO FRM-MSC-UTPZ IN CFN-FLD.
           MOVE SPACES TO FRM-COD-VCOC IN CFN-FLD.
           MOVE SPACES TO FRM-COD-TOOC IN CFN-FLD.
           MOVE SPACES TO FRM-COD-AOOC IN CFN-FLD.
           MOVE ZEROES TO FRM-NUM-VCOR IN CFN-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FPRO IN CFN-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
      *<<<< FIN_CLR_KEY_CFN                                             
           PERFORM GET-FHOY.                                            
           MOVE HOY-DHOY TO FRM-NUM-DPRO IN CFN-FLD.                    
           MOVE HOY-MHOY TO FRM-NUM-MPRO IN CFN-FLD.                    
           MOVE HOY-SHOY TO FRM-NUM-SPRO IN CFN-FLD.                    
           MOVE HOY-AHOY TO FRM-NUM-APRO IN CFN-FLD.                    
      *>>>>                                                             
       FIN-CLR-KEY-CFN.
           EXIT.
       
       CLR-FLD-CFN SECTION.
       INI-CLR-FLD-CFN.
           MOVE SPACES TO FRM-GLS-UTPZ IN CFN-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN CFN-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN CFN-FLD.
           MOVE SPACES TO FRM-GLS-AOOC IN CFN-FLD.
           MOVE ZEROES TO FRM-SGV-CSTF IN CFN-FLD.
           MOVE SPACES TO FRM-GLS-MONE IN CFN-FLD.
       FIN-CLR-FLD-CFN.
           EXIT.
       
       FST-KEY-CFN SECTION.
       INI-FST-KEY-CFN.
           MOVE -1 TO FRM-NUM-PLZO-LEN IN CFN-FLD.
       FIN-FST-KEY-CFN.
           EXIT.
       
       FST-FLD-CFN SECTION.
       INI-FST-FLD-CFN.
       FIN-FST-FLD-CFN.
           EXIT.
       
       PRO-KEY-CFN SECTION.
       INI-PRO-KEY-CFN.
           MOVE FRM-CPIM-AEY TO FRM-NUM-PLZO-ATR IN CFN-FLD.
           MOVE FRM-CPIM-AEY TO FRM-MSC-UTPZ-ATR IN CFN-FLD.
           MOVE FRM-CPIM-AEY TO FRM-COD-VCOC-ATR IN CFN-FLD.
           MOVE FRM-CPIM-AEY TO FRM-COD-TOOC-ATR IN CFN-FLD.
           MOVE FRM-CPIM-AEY TO FRM-COD-AOOC-ATR IN CFN-FLD.
           MOVE FRM-CPIM-AEY TO FRM-NUM-VCOR-ATR IN CFN-FLD.
           MOVE FRM-CPIM-AEY TO FRM-FEC-FPRO-ATR IN CFN-FLD.
           PERFORM PRO-IKY-CFN.
           PERFORM PRO-AKY-CFN.
           PERFORM PRO-MKY-CFN.
       FIN-PRO-KEY-CFN.
           EXIT.
       
       PRO-IKY-CFN SECTION.
       INI-PRO-IKY-CFN.
       FIN-PRO-IKY-CFN.
           EXIT.
       
       PRO-MKY-CFN SECTION.
       INI-PRO-MKY-CFN.
       FIN-PRO-MKY-CFN.
           EXIT.
       
       PRO-AKY-CFN SECTION.
       INI-PRO-AKY-CFN.
       FIN-PRO-AKY-CFN.
           EXIT.
       
       UNP-KEY-CFN SECTION.
       INI-UNP-KEY-CFN.
           MOVE FRM-CPIM-UBY TO FRM-NUM-PLZO-ATR IN CFN-FLD.
           MOVE FRM-CPIM-UBY TO FRM-MSC-UTPZ-ATR IN CFN-FLD.
           MOVE FRM-CPIM-UBY TO FRM-COD-VCOC-ATR IN CFN-FLD.
           MOVE FRM-CPIM-UBY TO FRM-COD-TOOC-ATR IN CFN-FLD.
           MOVE FRM-CPIM-UBY TO FRM-COD-AOOC-ATR IN CFN-FLD.
           MOVE FRM-CPIM-UBY TO FRM-NUM-VCOR-ATR IN CFN-FLD.
           MOVE FRM-CPIM-UBY TO FRM-FEC-FPRO-ATR IN CFN-FLD.
           PERFORM UNP-IKY-CFN.
           PERFORM UNP-AKY-CFN.
           PERFORM UNP-MKY-CFN.
       FIN-UNP-KEY-CFN.
           EXIT.
       
       UNP-IKY-CFN SECTION.
       INI-UNP-IKY-CFN.
           MOVE FRM-CPIM-UBY TO FRM-NUM-PLZO-ATR.
           MOVE FRM-CPIM-UBY TO FRM-MSC-UTPZ-ATR.
           MOVE FRM-CPIM-UBY TO FRM-COD-VCOC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-COD-TOOC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-COD-AOOC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-NUM-VCOR-ATR.
           MOVE FRM-CPIM-UBY TO FRM-FEC-FPRO-ATR IN CFN-FLD.
       FIN-UNP-IKY-CFN.
           EXIT.
       
       UNP-MKY-CFN SECTION.
       INI-UNP-MKY-CFN.
       FIN-UNP-MKY-CFN.
           EXIT.
       
       UNP-AKY-CFN SECTION.
       INI-UNP-AKY-CFN.
       FIN-UNP-AKY-CFN.
           EXIT.
       
       UNP-FLD-CFN SECTION.
       INI-UNP-FLD-CFN.
       FIN-UNP-FLD-CFN.
           EXIT.
       
       VAL-KEY-CFN SECTION.
       INI-VAL-KEY-CFN.
           PERFORM VAL-IKY-CFN.
           PERFORM VAL-MKY-CFN.
           PERFORM VAL-AKY-CFN.
       FIN-VAL-KEY-CFN.
           EXIT.
       
       VAL-IKY-CFN SECTION.
       INI-VAL-IKY-CFN.
           PERFORM VAL-FRM-NUM-PLZO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-NUM-PLZO-LEN IN CFN-FLD
                GO TO FIN-VAL-IKY-CFN.
           PERFORM VAL-FRM-MSC-UTPZ.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-MSC-UTPZ-LEN IN CFN-FLD
                GO TO FIN-VAL-IKY-CFN.
           PERFORM VAL-FRM-COD-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-VCOC-LEN IN CFN-FLD
                GO TO FIN-VAL-IKY-CFN.
           PERFORM VAL-FRM-COD-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-TOOC-LEN IN CFN-FLD
                GO TO FIN-VAL-IKY-CFN.
           PERFORM VAL-FRM-COD-AOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-AOOC-LEN IN CFN-FLD
                GO TO FIN-VAL-IKY-CFN.
           PERFORM VAL-FRM-NUM-VCOR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-NUM-VCOR-LEN IN CFN-FLD
                GO TO FIN-VAL-IKY-CFN.
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-FEC-FPRO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                FRM-FEC-FPRO-LEN IN CFN-FLD
                GO TO FIN-VAL-IKY-CFN.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
       FIN-VAL-IKY-CFN.
           EXIT.
       
       VAL-MKY-CFN SECTION.
       INI-VAL-MKY-CFN.
       FIN-VAL-MKY-CFN.
           EXIT.
       
       VAL-AKY-CFN SECTION.
       INI-VAL-AKY-CFN.
       FIN-VAL-AKY-CFN.
           EXIT.
       
       VAL-FRM-NUM-PLZO SECTION.
       INI-VAL-FRM-NUM-PLZO.
           IF FRM-NUM-PLZO IN CFN-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-FRM-NUM-PLZO.
       FIN-VAL-FRM-NUM-PLZO.
           EXIT.
       
       VAL-FRM-MSC-UTPZ SECTION.
       INI-VAL-FRM-MSC-UTPZ.
           IF FRM-MSC-UTPZ IN CFN-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-FRM-MSC-UTPZ.
      *<<<< FRM_MSC_UTPZ                                                
           IF FRM-MSC-UTPZ IN CFN-FLD NOT > SPACES                      
              GO TO FIN-VAL-FRM-MSC-UTPZ.                               
                                                                        
           MOVE 'TAB'  TO MSC-COD-SIST.                                 
           MOVE 'IPER' TO MSC-COD-TMSC IN MSC.                          
           MOVE FRM-MSC-UTPZ IN CFN-FLD TO MSC-COD-CMSC IN MSC.         
           PERFORM VAL-IND.                                             
           IF MSG-COD-MENS = 'COD    NEX' OR                            
              FRM-MSC-UTPZ IN CFN-FLD NOT > SPACES                      
               MOVE SPACES TO FRM-GLS-UTPZ IN CFN-FLD                   
           ELSE                                                         
               MOVE MSC-GLS-DCOR IN MSC TO                              
                    FRM-GLS-UTPZ IN CFN-FLD.                            
                                                                        
           IF FRM-MSC-UTPZ IN CFN-FLD = 'D'                             
              MOVE FRM-NUM-PLZO IN CFN-FLD TO OCI-NUM-PLZD.             
           IF FRM-MSC-UTPZ IN CFN-FLD = 'A'                             
              MULTIPLY FRM-NUM-PLZO IN CFN-FLD BY 360 GIVING            
                                               OCI-NUM-PLZD.            
           IF FRM-MSC-UTPZ IN CFN-FLD = 'M'                             
              MULTIPLY FRM-NUM-PLZO IN CFN-FLD BY 30 GIVING             
                                               OCI-NUM-PLZD.            
      *>>>>                                                             
       FIN-VAL-FRM-MSC-UTPZ.
           EXIT.
       
       VAL-FRM-COD-VCOC SECTION.
       INI-VAL-FRM-COD-VCOC.
           IF FRM-COD-VCOC IN CFN-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-FRM-COD-VCOC.
      *<<<< FRM_COD_VCOC                                                
           IF FRM-COD-VCOC IN CFN-FLD NOT > SPACES                      
               GO TO FIN-VAL-FRM-COD-VCOC.                              
                                                                        
           MOVE 'TAB'                   TO TAB-COD-SIST.                
           MOVE 'VLR'                   TO TAB-COD-TTAB IN TAB.         
           MOVE FRM-COD-VCOC IN CFN-FLD TO TAB-COD-CTAB IN TAB.         
           PERFORM VAL-COD.                                             
           IF MSG-COD-MENS NOT > SPACES                                 
               MOVE TAB-GLS-DESC TO FRM-GLS-VCOC IN CFN-FLD             
           ELSE                                                         
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE FRM-SUAR-MAL TO FRM-SUAR                            
               GO TO FIN-VAL-FRM-COD-VCOC.                              
      *>>>>                                                             
       FIN-VAL-FRM-COD-VCOC.
           EXIT.
       
       VAL-FRM-COD-TOOC SECTION.
       INI-VAL-FRM-COD-TOOC.
           IF FRM-COD-TOOC IN CFN-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-FRM-COD-TOOC.
      *<<<< FRM_COD_TOOC                                                
           IF FRM-COD-TOOC IN CFN-FLD NOT > SPACES                      
               GO TO FIN-VAL-FRM-COD-TOOC.                              
                                                                        
           MOVE 'TAB'                   TO TAB-COD-SIST.                
           MOVE 'TIO'                   TO TAB-COD-TTAB IN TAB.         
           MOVE FRM-COD-TOOC IN CFN-FLD TO TAB-COD-CTAB IN TAB.         
           PERFORM VAL-COD.                                             
           IF MSG-COD-MENS NOT > SPACES                                 
               MOVE TAB-GLS-DESC TO FRM-GLS-TOOC IN CFN-FLD             
           ELSE                                                         
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE FRM-SUAR-MAL TO FRM-SUAR                            
               GO TO FIN-VAL-FRM-COD-TOOC.                              
      *>>>>                                                             
       FIN-VAL-FRM-COD-TOOC.
           EXIT.
       
       VAL-FRM-COD-AOOC SECTION.
       INI-VAL-FRM-COD-AOOC.
           IF FRM-COD-AOOC IN CFN-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-FRM-COD-AOOC.
      *<<<< FRM_COD_AOOC                                                
           IF FRM-COD-AOOC IN CFN-FLD NOT > SPACES                      
               GO TO FIN-VAL-FRM-COD-AOOC.                              
                                                                        
           MOVE 'TAB'                    TO TAB-COD-SIST.               
           MOVE 'AUX'                    TO TAB-COD-TTAB IN TAB.        
           MOVE FRM-COD-TOOC IN CFN-FLD  TO WSS-COD-TOOC IN WSS-VARI.   
           MOVE FRM-COD-AOOC IN CFN-FLD  TO WSS-COD-AOOC IN WSS-VARI.   
           MOVE WSS-COD-CAUX IN WSS-VARI TO TAB-COD-CTAB IN TAB.        
           PERFORM VAL-COD.                                             
           IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'               
               MOVE SPACES       TO FRM-GLS-AOOC IN CFN-FLD             
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE FRM-SUAR-MAL TO FRM-SUAR                            
           ELSE                                                         
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-AOOC IN CFN-FLD.     
      *>>>>                                                             
       FIN-VAL-FRM-COD-AOOC.
           EXIT.
       
       VAL-FRM-NUM-VCOR SECTION.
       INI-VAL-FRM-NUM-VCOR.
           IF FRM-NUM-VCOR IN CFN-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-FRM-NUM-VCOR.
      *<<<< FRM_NUM_VCOR                                                
           IF FRM-NUM-VCOR IN CFN-FLD NOT > ZEROES                      
              GO TO FIN-VAL-FRM-NUM-VCOR.                               
      *>>>>                                                             
       FIN-VAL-FRM-NUM-VCOR.
           EXIT.
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-FRM-FEC-FPRO SECTION.
       INI-VAL-FRM-FEC-FPRO.
           IF FRM-FEC-FPRO IN CFN-FLD  NOT > ZEROES
              MOVE ZEROES TO FRM-FEC-FPRO IN CFN-FLD.
      *VAL-FEC Validar Fecha
           MOVE FRM-FEC-FPRO IN CFN-FLD TO FEC-FECH.
           MOVE FEC-FORM-FEC TO FEC-FORM.
           MOVE FEC-FBLK-N  TO FEC-FBLK.
           MOVE FEC-CHOY-LE TO FEC-CHOY.
           MOVE FEC-VHBL-S TO FEC-VHBL.
           MOVE FEC-VAL-FEC TO FEC-CMND.
           PERFORM PRO-FEC.
           IF FEC-STAT-OKS
               MOVE FEC-FECH TO FRM-FEC-FPRO IN CFN-FLD
           ELSE
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE FEC-MENS     TO FRM-MENS
               GO TO FIN-VAL-FRM-FEC-FPRO.
       FIN-VAL-FRM-FEC-FPRO.
           EXIT.
      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-CON-KEY-CFN SECTION.
       INI-VAL-CON-KEY-CFN.
           IF     FRM-NUM-PLZO IN CFN-FLD NOT > ZEROES 
              AND FRM-MSC-UTPZ IN CFN-FLD NOT > SPACES 
              AND FRM-COD-VCOC IN CFN-FLD NOT > SPACES 
              AND FRM-COD-TOOC IN CFN-FLD NOT > SPACES 
              AND FRM-COD-AOOC IN CFN-FLD NOT > SPACES 
              AND FRM-NUM-VCOR IN CFN-FLD NOT > ZEROES 
              AND FRM-FEC-FPRO IN CFN-FLD NOT > ZEROES
                PERFORM ERR-KEY
                PERFORM FST-KEY-CFN.
       FIN-VAL-CON-KEY-CFN.
           EXIT.
       
       VAL-CON-IKY-CFN SECTION.
       INI-VAL-CON-IKY-CFN.
           IF     FRM-NUM-PLZO IN CFN-FLD NOT > ZEROES 
              AND FRM-MSC-UTPZ IN CFN-FLD NOT > SPACES 
              AND FRM-COD-VCOC IN CFN-FLD NOT > SPACES 
              AND FRM-COD-TOOC IN CFN-FLD NOT > SPACES 
              AND FRM-COD-AOOC IN CFN-FLD NOT > SPACES 
              AND FRM-NUM-VCOR IN CFN-FLD NOT > ZEROES 
              AND FRM-FEC-FPRO IN CFN-FLD NOT > ZEROES
                PERFORM ERR-KEY
                PERFORM FST-KEY-CFN.
       FIN-VAL-CON-IKY-CFN.
           EXIT.
       
       VAL-NUM-KEY-CFN SECTION.
       INI-VAL-NUM-KEY-CFN.
           PERFORM VAL-NUM-IKY-CFN.
           PERFORM VAL-NUM-MKY-CFN.
           PERFORM VAL-NUM-AKY-CFN.
       FIN-VAL-NUM-KEY-CFN.
           EXIT.
       
       VAL-NUM-IKY-CFN SECTION.
       INI-VAL-NUM-IKY-CFN.
       VAL-NUM-FRM-NUM-PLZO.
           MOVE FRM-NUM-PLZO-ALF IN CFN-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 5 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO FRM-NUM-PLZO IN CFN-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-NUM-PLZO-LEN IN CFN-FLD
                GO TO FIN-VAL-NUM-IKY-CFN.
       VAL-NUM-FRM-NUM-VCOR.
           MOVE FRM-NUM-VCOR-ALF IN CFN-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO FRM-NUM-VCOR IN CFN-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-NUM-VCOR-LEN IN CFN-FLD
                GO TO FIN-VAL-NUM-IKY-CFN.
       FIN-VAL-NUM-IKY-CFN.
           EXIT.
       
       VAL-NUM-MKY-CFN SECTION.
       INI-VAL-NUM-MKY-CFN.
       FIN-VAL-NUM-MKY-CFN.
           EXIT.
       
       VAL-NUM-AKY-CFN SECTION.
       INI-VAL-NUM-AKY-CFN.
       FIN-VAL-NUM-AKY-CFN.
           EXIT.
       
       VAL-NUM-FLD-CFN SECTION.
       INI-VAL-NUM-FLD-CFN.
       VAL-NUM-FRM-SGV-CSTF.
           MOVE FRM-SGV-CSTF-ALF IN CFN-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9V TO FRM-SGV-CSTF IN CFN-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-SGV-CSTF-LEN IN CFN-FLD
                GO TO FIN-VAL-NUM-FLD-CFN.
       FIN-VAL-NUM-FLD-CFN.
           EXIT.
       
       EDT-KEY-CFN SECTION.
       INI-EDT-KEY-CFN.
           PERFORM EDT-IKY-CFN.
           PERFORM EDT-MKY-CFN.
           PERFORM EDT-AKY-CFN.
       FIN-EDT-KEY-CFN.
           EXIT.
       
       EDT-IKY-CFN SECTION.
       INI-EDT-IKY-CFN.
           IF FRM-NUM-PLZO-LEN IN CFN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-PLZO-LEN IN CFN-FLD
                GO TO FIN-EDT-IKY-CFN.
           MOVE FRM-NUM-PLZO IN CFN-FLD TO FRM-NUM-PLZO-EDT IN CFN-EDT.
           MOVE FRM-NUM-PLZO-EDT IN CFN-EDT TO 
                FRM-NUM-PLZO-ALF IN CFN-FLD.
           IF FRM-NUM-VCOR-LEN IN CFN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-VCOR-LEN IN CFN-FLD
                GO TO FIN-EDT-IKY-CFN.
           MOVE FRM-NUM-VCOR IN CFN-FLD TO FRM-NUM-VCOR-EDT IN CFN-EDT.
           MOVE FRM-NUM-VCOR-EDT IN CFN-EDT TO 
                FRM-NUM-VCOR-ALF IN CFN-FLD.
       FIN-EDT-IKY-CFN.
           EXIT.
       
       EDT-MKY-CFN SECTION.
       INI-EDT-MKY-CFN.
       FIN-EDT-MKY-CFN.
           EXIT.
       
       EDT-AKY-CFN SECTION.
       INI-EDT-AKY-CFN.
       FIN-EDT-AKY-CFN.
           EXIT.
       
       EDT-FLD-CFN SECTION.
       INI-EDT-FLD-CFN.
           IF FRM-SGV-CSTF-LEN IN CFN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-SGV-CSTF-LEN IN CFN-FLD
                GO TO FIN-EDT-FLD-CFN.
           MOVE FRM-SGV-CSTF IN CFN-FLD TO FRM-SGV-CSTF-EDT IN CFN-EDT.
           MOVE FRM-SGV-CSTF-EDT IN CFN-EDT TO 
                FRM-SGV-CSTF-ALF IN CFN-FLD.
       FIN-EDT-FLD-CFN.
           EXIT.
