      ******************************************************************
      *                     ELEMENT LIST
      ******************************************************************


       01  ELEM-LIST-LDA.
           05 ELEM-1          PIC X(5) VALUE 'LDA01'.
           05 ELEM-SEC        PIC X(1).
           05 FILLER          PIC X(5) VALUE SPACES.
       PROCEDURE DIVISION.
      *------------------
       A-CONTROL SECTION.
           ENTRY  'DBMSCBL'.
       AA-INICIALIZACION.
           PERFORM B-INICIO.
       AB-PROCESAMIENTO.
           PERFORM C-PROCESAR.
       AC-TERMINACION.
           PERFORM D-TERMINAR.
       AZ-FIN-DE-PROCESO.
           GOBACK.

       B-INICIO SECTION.

       B0-CONTROL.
      *-----------
           PERFORM BA-ABRIR-ARCHIVOS.
           GO TO   BZ-FIN-SECCION.

       BA-ABRIR-ARCHIVOS.
      *-----------------
             ACCEPT   TAR-TARJETA    FROM SYSIN.
             OPEN INPUT    COLVIMO
                  EXTEND   COLVTOT
                  OUTPUT   COLVCTB.

           IF FS-COLVTOT = 35
              CLOSE COLVTOT
              OPEN OUTPUT COLVTOT
           END-IF.

             IF FS-COLVTOT NOT = 0
                MOVE 'BA-ABRIR-ARCHI'      TO WR-PARRAFO
                MOVE 'B-INICIO          '  TO WR-SECCION
                MOVE 'OPEN OUTPUT'         TO WR-COMANDO
                MOVE 'COLVTOT           '  TO WR-ARCHIVO
                MOVE FS-COLVTOT            TO WR-STATUS
                MOVE 'S'                   TO WR-TERMINAR
           END-IF.


       BZ-FIN-SECCION.
      *---------------
           EXIT.

       C-PROCESAR SECTION.
      *-----------------
       C0-CONTROL.
      *-----------
           IF WR-TERMINAR = 'S'
              GO TO CZ-FIN-SECCION
           END-IF.
           SORT SOR-SORT ON ASCENDING  KEY SOR-OFICINA
           INPUT  PROCEDURE E-ENTRADA
           OUTPUT PROCEDURE S-SALIDA.
           GO TO CZ-FIN-SECCION.

       CZ-FIN-SECCION.
      *---------------
           EXIT.


       E-ENTRADA SECTION.
      *------------------
           PERFORM EA-LEER-COLVIMO.
           PERFORM EB-LLENAR-SORT UNTIL FIN-COLVIMO.
           GO TO EZ-FIN-SECCION.

       EA-LEER-COLVIMO.
      *------------------
           READ COLVIMO AT END SET FIN-COLVIMO TO TRUE.

       EB-LLENAR-SORT.
      *------------------
           IF IMO-TIPO-MOV = 'I'
           AND IMO-MONTO > 0
               PERFORM EC-SELECCINADO.
           PERFORM EA-LEER-COLVIMO.

       EC-SELECCINADO.
      *------------------
           MOVE  IMO-CLAVE-LDA                  TO LDA-VALUE.
           PERFORM ED-ACCESAR-LDA.
           IF CON-LDA
              ADD  1                            TO WA-SEL
              PERFORM EE-LLENAR-CAMPOS
              RELEASE SORT-REG
           ELSE
              ADD  1                            TO WA-SIN-LDA
           END-IF.

       ED-ACCESAR-LDA.
      *---------------
           CALL 'DBNTRY' USING UIB REQ-AREA-LDA LDA ELEM-LIST-LDA
           IF        LDA-RET-COD = 14
                     SET SIN-LDA         TO   TRUE
           ELSE      IF     LDA-RET-COD = SPACES
                            SET CON-LDA         TO   TRUE
                     ELSE
                            MOVE 'CA-ACCESAR-LDA'      TO WR-PARRAFO
                            MOVE 'C-PROCESAR        '  TO WR-SECCION
                            MOVE LDA-COMMAND           TO WR-COMANDO
                            MOVE 'TABLA LDA         '  TO WR-ARCHIVO
                            MOVE LDA-RET-COD           TO WR-STATUS
                            MOVE 'S'                   TO WR-TERMINAR
                     END-IF
           END-IF.

       EE-LLENAR-CAMPOS.
      *------------------
           MOVE IMO-MONTO        TO SOR-MONTO.
           MOVE LDA-COD-OFI      TO SOR-OFICINA.

       EZ-FIN-SECCION.
      *------------------
           EXIT.

       S-SALIDA SECTION.
      *------------------
           PERFORM SA-LEER-SORT
           IF  NOT FIN-SORT
               MOVE SOR-OFICINA        TO W-OFICINA
               PERFORM SB-PROCESO-SORT UNTIL FIN-SORT
               PERFORM SC-GRABAR
           ELSE
               MOVE '090'            TO W-OFICINA
               MOVE ZEROES           TO W-MONTO
               PERFORM SE-GRABAR-TOT
           END-IF.
           GO TO SZ-FIN-SECCION.

       SA-LEER-SORT.
      *----------
           RETURN SOR-SORT AT END SET  FIN-SORT   TO TRUE.

       SB-PROCESO-SORT.
      *----------------
           IF W-OFICINA NOT = SOR-OFICINA
               PERFORM SC-GRABAR
               MOVE SOR-OFICINA      TO W-OFICINA
               MOVE ZEROES           TO W-MONTO
           END-IF.
               ADD SOR-MONTO         TO W-MONTO
           PERFORM SA-LEER-SORT.

       SC-GRABAR.
      *----------
           PERFORM SD-GRABAR-CTB.
           PERFORM SE-GRABAR-TOT.

       SD-GRABAR-CTB.
      *--------------
           MOVE 2                           TO CTB-TIPO-DOCTO.
           MOVE W-OFICINA                   TO CTB-SUC-MVTO.
           MOVE 301002087                   TO CTB-COD-CTA.
           MOVE 00                          TO CTB-COD-MONEDA.
           MOVE W-MONTO                     TO CTB-VALOR.
           MOVE SPACES                      TO CTB-NUM-DOCTO.
           MOVE 'MVTO. IMPTO. DIARIO LDA'   TO CTB-GLOSA.
           WRITE CTB-REGISTRO.
           ADD   1                 TO  WA-GRA-CTB.

       SE-GRABAR-TOT.
      *---------------
           MOVE W-OFICINA          TO TOT-OFICINA.
           MOVE 05                 TO TOT-SIST.
           MOVE TAR-FECHA          TO TOT-FECHA.
           MOVE SPACES             TO TOT-FILLER.
           MOVE W-MONTO            TO HABER-LIN03.
           MOVE W-MONTO            TO DEBE-LIN06.
           MOVE ZEROES             TO DEBE-LIN03.
           PERFORM CF-GRABAR-TOT VARYING I FROM 1 BY 1 UNTIL I > 7.

       CF-GRABAR-TOT.
      *---------------
           MOVE  I                 TO  TOT-NROLINEA.
           MOVE LINEAS (I)         TO  TOT-LINEA
           WRITE TOT-REGISTRO.
           IF FS-COLVTOT NOT = 0
                MOVE 'BA-ABRIR-ARCHI'      TO WR-PARRAFO
                MOVE 'B-INICIO          '  TO WR-SECCION
                MOVE 'OPEN OUTPUT'         TO WR-COMANDO
                MOVE 'COLVTOT           '  TO WR-ARCHIVO
                MOVE FS-COLVTOT            TO WR-STATUS
                MOVE 'S'                   TO WR-TERMINAR
           ELSE
                ADD   1                    TO  WA-GRA-TOT
           END-IF.

       SZ-FIN-SECCION.
      *---------------
           EXIT.
       D-TERMINAR SECTION.
      *-------------------
      *
           DISPLAY '*'
           IF WR-TERMINAR = 'S'
              DISPLAY  'PARRAFO            '     WR-PARRAFO
              DISPLAY  'SECCION            '     WR-SECCION
              DISPLAY  'PARRAFO            '     WR-COMANDO
              DISPLAY  'ARCHIVO            '     WR-ARCHIVO
              DISPLAY  'STATUS             '     WR-STATUS
              MOVE 100                       TO RETURN-CODE
           END-IF.
           DISPLAY '*'
           DISPLAY '*           ESTADISTICAS'
           DISPLAY '*'
           DISPLAY '*       PROCESO AL: '  TAR-FECHA.
           DISPLAY '*'
           DISPLAY '*'
           DISPLAY '* REGISTROS SELECCIONADOS.........' WA-SEL.
           DISPLAY '* REGISTROS GRABADOS EN CTB.......' WA-GRA-CTB.
           DISPLAY '* REGISTROS GRABADOS EN TOT.......' WA-GRA-TOT.
           DISPLAY '* REGISTROS SIN LDA...............' WA-SIN-LDA.
      *
           CLOSE COLVIMO COLVCTB COLVTOT.
      *
       DZ-FIN-SECCION.
      *---------------
           EXIT.
      *
      *
