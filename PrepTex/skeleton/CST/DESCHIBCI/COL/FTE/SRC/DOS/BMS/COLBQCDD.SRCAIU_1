       SET-TAG-COLBQCDD SECTION.
       INI-SET-TAG-COLBQCDD.
           MOVE
           '<IDNMOD NOM=$COLBQCDD$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$20-May-19 10:39 AM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCDD.
           EXIT.
           
       PUT-ALL-CDD SECTION.
       INI-PUT-ALL-CDD.
           PERFORM PUT-DBC-CDD.
       FIN-PUT-ALL-CDD.
           EXIT.
       
       PUT-DBC-CDD SECTION.
       INI-PUT-DBC-CDD.
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-CDD.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-CDD
               GO TO FIN-PUT-DBC-CDD.
      * IDC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN CDD-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN CDD-FLD.
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN CDD-FLD.
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN CDD-FLD.
      * IDC-FIN --> BMCHIBCI001210.SRCAIU 
       FIN-PUT-DBC-CDD.
           EXIT.
       
       KEY-ALL-CDD SECTION.
       INI-KEY-ALL-CDD.
           PERFORM KEY-DBC-CDD.
       FIN-KEY-ALL-CDD.
           EXIT.
       
       KEY-DBC-CDD SECTION.
       INI-KEY-DBC-CDD.
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN CDD-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN CDD-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN CDD-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN CDD-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
       FIN-KEY-DBC-CDD.
           EXIT.
       
       SCR-CLRA-CDD SECTION.
       INI-SCR-CLRA-CDD.
           PERFORM CLR-KEY-CDD.
           PERFORM CLR-FLD-CDD.
       FIN-SCR-CLRA-CDD.
           EXIT.
       
       CLR-KEY-CDD SECTION.
       INI-CLR-KEY-CDD.
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN CDD-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN CDD-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN CDD-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN CDD-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-VAL-PREP IN CDD-FLD.
       FIN-CLR-KEY-CDD.
           EXIT.
       
       CLR-FLD-CDD SECTION.
       INI-CLR-FLD-CDD.
           MOVE SPACES TO FRM-GLS-NOMC IN CDD-FLD.
           MOVE ZEROES TO FRM-VAL-CADE IN CDD-FLD.
           MOVE SPACES TO FRM-COD-VCOC IN CDD-FLD.
           MOVE ZEROES TO FRM-VAL-TPRE IN CDD-FLD.
           MOVE ZEROES TO FRM-VAL-NOPE IN CDD-FLD.
           MOVE ZEROES TO FRM-VAL-CAMA IN CDD-FLD.
           MOVE ZEROES TO FRM-VAL-TPOR IN CDD-FLD.
           MOVE SPACES TO FRM-GLS-OBSE IN CDD-FLD.
           MOVE SPACES TO FRM-CIC-IOPC IN CDD-FLD(1).
           MOVE SPACES TO FRM-CIC-IOPC IN CDD-FLD(2).
           MOVE SPACES TO FRM-CIC-IOPC IN CDD-FLD(3).
           MOVE SPACES TO FRM-CIC-IOPC IN CDD-FLD(4).
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FCUR IN CDD-FLD(1).
           MOVE ZEROES TO FRM-FEC-FCUR IN CDD-FLD(2).
           MOVE ZEROES TO FRM-FEC-FCUR IN CDD-FLD(3).
           MOVE ZEROES TO FRM-FEC-FCUR IN CDD-FLD(4).
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-VAL-TINT IN CDD-FLD(1).
           MOVE ZEROES TO FRM-VAL-TINT IN CDD-FLD(2).
           MOVE ZEROES TO FRM-VAL-TINT IN CDD-FLD(3).
           MOVE ZEROES TO FRM-VAL-TINT IN CDD-FLD(4).
           MOVE ZEROES TO FRM-VAL-SKUF IN CDD-FLD(1).
           MOVE ZEROES TO FRM-VAL-SKUF IN CDD-FLD(2).
           MOVE ZEROES TO FRM-VAL-SKUF IN CDD-FLD(3).
           MOVE ZEROES TO FRM-VAL-SKUF IN CDD-FLD(4).
           MOVE ZEROES TO FRM-VAL-KPPE IN CDD-FLD(1).
           MOVE ZEROES TO FRM-VAL-KPPE IN CDD-FLD(2).
           MOVE ZEROES TO FRM-VAL-KPPE IN CDD-FLD(3).
           MOVE ZEROES TO FRM-VAL-KPPE IN CDD-FLD(4).
           MOVE ZEROES TO FRM-VAL-INPE IN CDD-FLD(1).
           MOVE ZEROES TO FRM-VAL-INPE IN CDD-FLD(2).
           MOVE ZEROES TO FRM-VAL-INPE IN CDD-FLD(3).
           MOVE ZEROES TO FRM-VAL-INPE IN CDD-FLD(4).
           MOVE ZEROES TO FRM-VAL-TPPE IN CDD-FLD(1).
           MOVE ZEROES TO FRM-VAL-TPPE IN CDD-FLD(2).
           MOVE ZEROES TO FRM-VAL-TPPE IN CDD-FLD(3).
           MOVE ZEROES TO FRM-VAL-TPPE IN CDD-FLD(4).
           PERFORM CLR-DBC-CDD.
       FIN-CLR-FLD-CDD.
           EXIT.
       
       CLR-DBC-CDD SECTION.
       INI-CLR-DBC-CDD.
       FIN-CLR-DBC-CDD.
           EXIT.
       
       FST-KEY-CDD SECTION.
       INI-FST-KEY-CDD.
           MOVE FRM-POS-CURS TO DBC-NUM-ICLI-LEN IN CDD-FLD.
      * VRF-FIN --> BMCHIBCI004.SRCAIU ( FST-KEY Y FST-FLD )
       FIN-FST-KEY-CDD.
           EXIT.
       
       FST-FLD-CDD SECTION.
       INI-FST-FLD-CDD.
       FIN-FST-FLD-CDD.
           EXIT.
       
       PRO-KEY-CDD SECTION.
       INI-PRO-KEY-CDD.
           MOVE FRM-CPIM-AEY TO FRM-VAL-PREP-ATR IN CDD-FLD.
           PERFORM PRO-IKY-CDD.
           PERFORM PRO-AKY-CDD.
           PERFORM PRO-MKY-CDD.
       FIN-PRO-KEY-CDD.
           EXIT.
       
       PRO-IKY-CDD SECTION.
       INI-PRO-IKY-CDD.
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO DBC-NUM-ICLI-ATR IN CDD-FLD.
           MOVE FRM-CPIM-AEY TO DBC-VRF-ICLI-ATR IN CDD-FLD.
           MOVE FRM-CPIM-AEY TO DBC-IND-ICLI-ATR IN CDD-FLD.
           MOVE FRM-CPIM-AEY TO DBC-GLS-ICLI-ATR IN CDD-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
       FIN-PRO-IKY-CDD.
           EXIT.
       
       PRO-MKY-CDD SECTION.
       INI-PRO-MKY-CDD.
       FIN-PRO-MKY-CDD.
           EXIT.
       
       PRO-AKY-CDD SECTION.
       INI-PRO-AKY-CDD.
       FIN-PRO-AKY-CDD.
           EXIT.
       
       UNP-KEY-CDD SECTION.
       INI-UNP-KEY-CDD.
           MOVE FRM-CPIM-UBY TO FRM-VAL-PREP-ATR IN CDD-FLD.
           PERFORM UNP-IKY-CDD.
           PERFORM UNP-AKY-CDD.
           PERFORM UNP-MKY-CDD.
       FIN-UNP-KEY-CDD.
           EXIT.
       
       UNP-IKY-CDD SECTION.
       INI-UNP-IKY-CDD.
      * IDC-INI --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO DBC-NUM-ICLI-ATR IN CDD-FLD.
           MOVE FRM-CPIM-UBY TO DBC-VRF-ICLI-ATR IN CDD-FLD.
           MOVE FRM-CPIM-UBY TO DBC-IND-ICLI-ATR IN CDD-FLD.
           MOVE FRM-CPIM-UBY TO DBC-GLS-ICLI-ATR IN CDD-FLD.
      * IDC-FIN --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO FRM-VAL-PREP-ATR.
       FIN-UNP-IKY-CDD.
           EXIT.
       
       UNP-MKY-CDD SECTION.
       INI-UNP-MKY-CDD.
       FIN-UNP-MKY-CDD.
           EXIT.
       
       UNP-AKY-CDD SECTION.
       INI-UNP-AKY-CDD.
       FIN-UNP-AKY-CDD.
           EXIT.
       
       UNP-FLD-CDD SECTION.
       INI-UNP-FLD-CDD.
           PERFORM UNP-DBC-CDD.
       FIN-UNP-FLD-CDD.
           EXIT.
       
       UNP-DBC-CDD SECTION.
       INI-UNP-DBC-CDD.
       FIN-UNP-DBC-CDD.
           EXIT.
       
       VAL-KEY-CDD SECTION.
       INI-VAL-KEY-CDD.
           PERFORM VAL-IKY-CDD.
           PERFORM VAL-MKY-CDD.
           PERFORM VAL-AKY-CDD.
       FIN-VAL-KEY-CDD.
           EXIT.
       
       VAL-IKY-CDD SECTION.
       INI-VAL-IKY-CDD.
      * VRF-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * VRF-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
      * IDC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DBC-IND-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-IND-ICLI-LEN IN CDD-FLD
                GO TO FIN-VAL-IKY-CDD.
           PERFORM VAL-DBC-GLS-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-GLS-ICLI-LEN IN CDD-FLD
                GO TO FIN-VAL-IKY-CDD.
           PERFORM VAL-DBC-NUM-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-NUM-ICLI-LEN IN CDD-FLD
                GO TO FIN-VAL-IKY-CDD.
           PERFORM VAL-DBC-VRF-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBC-VRF-ICLI-LEN IN CDD-FLD
                GO TO FIN-VAL-IKY-CDD.
      * IDC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-VAL-PREP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-PREP-LEN IN CDD-FLD
                GO TO FIN-VAL-IKY-CDD.
       FIN-VAL-IKY-CDD.
           EXIT.
       
       VAL-MKY-CDD SECTION.
       INI-VAL-MKY-CDD.
       FIN-VAL-MKY-CDD.
           EXIT.
       
       VAL-AKY-CDD SECTION.
       INI-VAL-AKY-CDD.
       FIN-VAL-AKY-CDD.
           EXIT.
      * VRF-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
      * VRF-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
      * IDC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION

       VAL-DBC-NUM-ICLI SECTION.
       INI-VAL-DBC-NUM-ICLI.
       FIN-VAL-DBC-NUM-ICLI.
           EXIT.

       VAL-DBC-VRF-ICLI SECTION.
       INI-VAL-DBC-VRF-ICLI.
      *<<<< INI_DBC_VRF_ICLI
           IF DBC-NUM-ICLI IN CDD-FLD NOT > ZEROES
              MOVE 'SGC'        TO MSG-COD-SIST
              MOVE 'GLS    BLK' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE FRM-SUAR-MAL TO FRM-SUAR

              GO TO FIN-VAL-DBC-VRF-ICLI.
      *>>>>
           IF DBC-VRF-ICLI IN CDD-FLD NOT > SPACES
              MOVE SPACES TO DBC-VRF-ICLI IN CDD-FLD.
      *VAL-RUT Valida Rut para campo IDC
           MOVE IDC-ZERO-S TO IDC-ZERO.
           MOVE DBC-NUM-ICLI IN CDD-FLD TO IDC-NUM-RUTV.
           MOVE DBC-VRF-ICLI IN CDD-FLD TO IDC-VRF-RUTV.
           PERFORM VAL-RUT.
      *<<<< FIN_DBC_VRF_ICLI
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR-CLR
              GO TO FIN-VAL-DBC-VRF-ICLI.

           MOVE DBC-NUM-ICLI IN CDD-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN CDD-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN CDD-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN CDD-FLD TO DBC-GLS-ICLI IN DBC.
           MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY.
           MOVE FIO-GET-KEY-ALT TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           IF NOT FIO-STAT-OKS
              MOVE 'SGC'        TO MSG-COD-SIST
              MOVE 'CLI    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE FRM-SUAR-MAL TO FRM-SUAR
              GO TO FIN-VAL-DBC-VRF-ICLI.

           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.
           PERFORM CPT-BLKS.
           MOVE CPT-STRN TO FRM-GLS-NOMC IN CDD-FLD.
      *>>>>
       FIN-VAL-DBC-VRF-ICLI.
           EXIT.
       VAL-DBC-IND-ICLI SECTION.
       INI-VAL-DBC-IND-ICLI.
           IF DBC-IND-ICLI IN CDD-FLD NOT > SPACES
              MOVE SPACES TO DBC-IND-ICLI IN CDD-FLD.
      *VAL-IDC Valida identificador
           MOVE DBC-IND-ICLI IN CDD-FLD TO IDC-IND-RUTV.
           PERFORM VAL-IDC.
           MOVE IDC-IND-RUTV TO DBC-IND-ICLI IN CDD-FLD.
       FIN-VAL-DBC-IND-ICLI.
           EXIT.

       VAL-DBC-GLS-ICLI SECTION.
       INI-VAL-DBC-GLS-ICLI.
           IF DBC-GLS-ICLI IN CDD-FLD NOT > IDC-INIC-GLS 
              MOVE IDC-INIC-GLS TO DBC-GLS-ICLI IN CDD-FLD.
      *VAL-GLS para IDC valida glosa ( extension ) de identificador
           MOVE DBC-GLS-ICLI IN CDD-FLD TO IDC-GLS-RUTV.
           PERFORM VAL-GLS.
           MOVE IDC-GLS-RUTV TO DBC-GLS-ICLI IN CDD-FLD.
       FIN-VAL-DBC-GLS-ICLI.
           EXIT.

      * IDC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-FRM-VAL-PREP SECTION.
       INI-VAL-FRM-VAL-PREP.
      *<<<< INI_FRM_VAL_PREP
           IF FRM-VAL-PREP IN CDD-FLD < ZEROES
              MOVE 'COL'         TO MSG-COD-SIST
              MOVE 'CDDMTOPRENO' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE FRM-SUAR-MAL TO FRM-SUAR
              GO TO FIN-VAL-FRM-VAL-PREP
           ELSE
             MOVE FRM-VAL-PREP IN CDD-FLD TO WSS-VAL-PREP IN WSS-VARI
             MOVE WSS-VAL-PREP IN WSS-VARI TO WSS-VAL-PREE IN WSS-VARI.
      *>>>>
       FIN-VAL-FRM-VAL-PREP.
           EXIT.
       
       VAL-CON-KEY-CDD SECTION.
       INI-VAL-CON-KEY-CDD.
      * IDC-INI --> BMCHIBCI009.SRCAIU VAL-CON
           IF     DBC-NUM-ICLI IN CDD-FLD NOT > ZEROES
              AND DBC-VRF-ICLI IN CDD-FLD NOT > SPACES
              AND DBC-IND-ICLI IN CDD-FLD NOT > SPACES
              AND DBC-GLS-ICLI IN CDD-FLD NOT > IDC-INIC-GLS
      * IDC-FIN --> BMCHIBCI009.SRCAIU VAL-CON
              AND FRM-VAL-PREP IN CDD-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CDD.
       FIN-VAL-CON-KEY-CDD.
           EXIT.
       
       VAL-CON-IKY-CDD SECTION.
       INI-VAL-CON-IKY-CDD.
      * IDC-INI --> BMCHIBCI009.SRCAIU VAL-CON
           IF     DBC-NUM-ICLI IN CDD-FLD NOT > ZEROES
              AND DBC-VRF-ICLI IN CDD-FLD NOT > SPACES
              AND DBC-IND-ICLI IN CDD-FLD NOT > SPACES
              AND DBC-GLS-ICLI IN CDD-FLD NOT > IDC-INIC-GLS
      * IDC-FIN --> BMCHIBCI009.SRCAIU VAL-CON
              AND FRM-VAL-PREP IN CDD-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CDD.
       FIN-VAL-CON-IKY-CDD.
           EXIT.
       
       VAL-NUM-KEY-CDD SECTION.
       INI-VAL-NUM-KEY-CDD.
           PERFORM VAL-NUM-IKY-CDD.
           PERFORM VAL-NUM-MKY-CDD.
           PERFORM VAL-NUM-AKY-CDD.
       FIN-VAL-NUM-KEY-CDD.
           EXIT.
       
       VAL-NUM-IKY-CDD SECTION.
       INI-VAL-NUM-IKY-CDD.
      * IDC-INI --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
       VAL-NUM-DBC-NUM-ICLI.
           MOVE DBC-NUM-ICLI-ALF IN CDD-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO DBC-NUM-ICLI IN CDD-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO DBC-NUM-ICLI-LEN IN CDD-FLD
                GO TO FIN-VAL-NUM-IKY-CDD.
      * IDC-FIN --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
       VAL-NUM-FRM-VAL-PREP.
           MOVE FRM-VAL-PREP-ALF IN CDD-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 9 TO NUM-NENT.
           MOVE 2 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9V TO FRM-VAL-PREP IN CDD-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-VAL-PREP-LEN IN CDD-FLD
                GO TO FIN-VAL-NUM-IKY-CDD.
       FIN-VAL-NUM-IKY-CDD.
           EXIT.
       
       VAL-NUM-MKY-CDD SECTION.
       INI-VAL-NUM-MKY-CDD.
       FIN-VAL-NUM-MKY-CDD.
           EXIT.
       
       VAL-NUM-AKY-CDD SECTION.
       INI-VAL-NUM-AKY-CDD.
       FIN-VAL-NUM-AKY-CDD.
           EXIT.
       
       VAL-NUM-FLD-CDD SECTION.
       INI-VAL-NUM-FLD-CDD.
       VAL-NUM-FRM-VAL-CADE.
           MOVE FRM-VAL-CADE-ALF IN CDD-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9V TO FRM-VAL-CADE IN CDD-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-VAL-CADE-LEN IN CDD-FLD
                GO TO FIN-VAL-NUM-FLD-CDD.
       VAL-NUM-FRM-VAL-TPRE.
           MOVE FRM-VAL-TPRE-ALF IN CDD-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 2 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9V TO FRM-VAL-TPRE IN CDD-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-VAL-TPRE-LEN IN CDD-FLD
                GO TO FIN-VAL-NUM-FLD-CDD.
       VAL-NUM-FRM-VAL-NOPE.
           MOVE FRM-VAL-NOPE-ALF IN CDD-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 4 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO FRM-VAL-NOPE IN CDD-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-VAL-NOPE-LEN IN CDD-FLD
                GO TO FIN-VAL-NUM-FLD-CDD.
       VAL-NUM-FRM-VAL-CAMA.
           MOVE FRM-VAL-CAMA-ALF IN CDD-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 4 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO FRM-VAL-CAMA IN CDD-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-VAL-CAMA-LEN IN CDD-FLD
                GO TO FIN-VAL-NUM-FLD-CDD.
       VAL-NUM-FRM-VAL-TPOR.
           MOVE FRM-VAL-TPOR-ALF IN CDD-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9V TO FRM-VAL-TPOR IN CDD-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-VAL-TPOR-LEN IN CDD-FLD
                GO TO FIN-VAL-NUM-FLD-CDD.
       VAL-NUM-FRM-VAL-TINT-1.
           MOVE FRM-VAL-TINT-ALF IN CDD-FLD(1) TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 2 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NS9V TO FRM-VAL-TINT IN CDD-FLD(1)
           ELSE
