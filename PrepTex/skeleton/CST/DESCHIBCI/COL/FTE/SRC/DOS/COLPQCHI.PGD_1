       PROCEDURE DIVISION.
      *===================

       MAIN SECTION.
       INI-MAIN.
           PERFORM INI.
       LUP-MAIN.
           IF SCR-INST = SCR-INST-HCM
                 PERFORM SCR-COM.
           IF SCR-CCPP = 'ACC'
                 PERFORM SCR-ACC
           ELSE
                 MOVE SCR-INST-HCM TO SCR-INST.
           GO TO LUP-MAIN.
       FIN-MAIN.
           EXIT.

       INI SECTION.
       INI-INI.
       COPY GNSBGEIB.
       COPY GNSBGEDB.
           MOVE LOW-VALUES TO CHI-FLD.
           MOVE LOW-VALUES TO CHI-EDT.
           PERFORM GNS-PRO-RCK.
           PERFORM GNS-BUS-VCA.
           MOVE +270 TO SCR-TCMA.
           PERFORM GNS-HDL-SYS.
           PERFORM GNS-HDL-FRM.
           MOVE 'COLPQCHI' TO FIO-PROG.
           PERFORM GNS-BUS-IDD.
      *PTC-INI
           PERFORM GNS-ERR-QUE.
           PERFORM GET-TSK-TERM.
      *PTC-FIN
      *<<<<
           IF SCR-OFRM-SRV                                              
              MOVE TSK-TERM-ALF TO CHI-TERM                             
           ELSE                                                         
              MOVE EIBTRMID     TO CHI-TERM.                            
      *>>>>
       FIN-INI.
           EXIT.

       COPY GNSBHSYS.
       COPY GNSBHFRM.
       COPY GNSBPRCK.
       COPY GNSBBVCA.
       COPY GNSBBIDD.

       SCR-COM SECTION.
       INI-SCR-COM.
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-CLRA-CHI.
           PERFORM EDT-KEY-CHI.
           PERFORM EDT-FLD-CHI.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-HCM TO SCR-INST.
           MOVE SCR-CMNP     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       XCT-ERR.
           MOVE 'XCTERR    ' TO MSG-COD-MENS.
           MOVE 'GNS'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO SYS-MEN1.
           MOVE SYS-PROG     TO SYS-MEN2.
           MOVE SYS-ABOR     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       COPY GNSBGGBK.
       FIN-SCR-COM.
           EXIT.



       SCR-ACC SECTION.
       INI-SCR-ACC.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ACC
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-ACC.
           PERFORM DESSCRTRL.
      *<<<< PFS_SCR_ACC                                                 
           IF FRM-FFLD = FRM-FFLD-PF6                     
                    OR   FRM-FFLD-PF5                     
               PERFORM VAL-NUM-KEY-CHI                    
               PERFORM VAL-NUM-FLD-CHI                    
               PERFORM RST-STAT                           
               PERFORM FND-SCR-KEY THRU FIN-SCR-KEY       
               IF SCR-STAT NOT = SCR-STAT-ABT             
                  PERFORM SCR-TRL                        
               ELSE                                       
                  PERFORM DESSCRTRL                      
                  GO TO FIN-SCR-ACC.                     
      *>>>>
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ACC.
           EXIT.

       BRW-SCR-ACC SECTION.
       INI-BRW-SCR-ACC.
       FIN-BRW-SCR-ACC.
           EXIT.




       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               PERFORM SCR-CLRA-CHI
               PERFORM EDT-KEY-CHI
               PERFORM EDT-FLD-CHI
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-CHI
               PERFORM EDT-KEY-CHI
           ELSE
               PERFORM CLR-FLD-CHI
               PERFORM EDT-FLD-CHI.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

       SCR-KEY SECTION.
       INI-SCR-KEY.
           PERFORM SCR-RST.
           PERFORM GET-KEY.
           IF FRM-STAT-ABT
                   MOVE SPACES TO FRM-MENS
                   PERFORM SCR-MSSG
                   GO TO FIN-SCR-KEY.
       FND-SCR-KEY.
           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
           IF FIO-STAT-OKS
               PERFORM PUT-SCR
               MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
               GO TO FIN-SCR-KEY.
      *JSS ELSE
      *        PERFORM CLR-FLD-CHI.
       ERR-SCR-KEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR.
           IF FRM-MENS NOT > SPACES
               IF FIO-STAT-EOF
                   MOVE 'REGEOF    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
               IF FIO-STAT = FIO-STAT-NEX
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
           MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-SCR-KEY.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO CHI-FLD.
           MOVE LOW-VALUES TO CHI-EDT.
       FIN-SCR-RST.
           EXIT.

       GET-KEY SECTION.
       INI-GET-KEY.
           PERFORM SCR-GKEY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-KEY.
           EXIT.

       GET-IKY SECTION.
       INI-GET-IKY.
           PERFORM SCR-GIKY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-IKY.
           EXIT.

       FND-KEY SECTION.
       INI-FND-KEY.
      *<<<< INI_FND_KEY 
           MOVE SPACES TO FRM-MEN1.                           
           IF FRM-FFLD = FRM-FFLD-PF5                         
               MOVE PGM-SNUL TO PGM-STAT-OPC                  
               MOVE PGM-SNUL TO PGM-STAT-OPC                  
               SUBTRACT 1 FROM SCR-I GIVING CHI-I             
               IF CHI-I NOT > ZEROES                          
                   MOVE 'OPC PROPRV' TO MSG-COD-MENS          
                   GO TO ERR-FND-KEY                          
               ELSE                                           
                   GO TO CARGA-CHI                            
           ELSE                                               
           IF FRM-FFLD = FRM-FFLD-PF6                         
               MOVE PGM-SNUL TO PGM-STAT-OPC                  
               MOVE PGM-SNUL TO PGM-STAT-OPC                  
               ADD 1 SCR-I GIVING CHI-I                       
               IF CHI-I > SCR-IMAX                            
                   MOVE 'OPC PRONXT' TO MSG-COD-MENS          
                   GO TO ERR-FND-KEY                          
               ELSE                                           
                   GO TO CARGA-CHI.                           
           GO TO CON-FND-KEY.                                 
      *>>>>
           PERFORM KEY-ALL-CHI.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-OPC.
           IF FIO-STAT-OKS
               MOVE OPC-STP-ITRN IN OPC TO SCR-UMOD.
           MOVE FIO-STAT TO PGM-STAT-OPC.
      *<<<< FIN_FND_KEY     
           IF NOT FIO-STAT-OKS                                       
              GO TO FIN-FND-KEY.                                    
       CON-FND-KEY.                 
                                 
              PERFORM KEY-ALL-CHI.                                      
            IF OPC-IIC-IOPC IN CHI-FLD > ZEROES                         
               GO TO DSP-CAR-MOV-OPC.                                   
            MOVE DBC-NUM-ICLI IN CHI-FLD TO DBC-NUM-ICLI IN DBC        
           MOVE DBC-IND-ICLI IN CHI-FLD TO DBC-IND-ICLI IN DBC     
           MOVE DBC-GLS-ICLI IN CHI-FLD TO DBC-GLS-ICLI IN DBC     
           MOVE 'DBC-IDC-ICLI' TO FIO-AKEY.                         
           MOVE FIO-GET-KEY  TO FIO-CMND.                           
           PERFORM SGC-FIO-DBC                                     
           GO TO DSP-CAR-MOV-DBC.                                   
       DSP-CAR-MOV-OPC.                                              
           MOVE OPC-CAI-IOPC IN OPC TO RDC-CAI-IOPC IN RDC.          
           MOVE OPC-IIC-IOPC IN OPC TO RDC-IIC-IOPC IN RDC.          
           MOVE ZEROES              TO RDC-NUM-IDLC IN RDC.          
           MOVE 'RDC-KEY-IDLC' TO FIO-AKEY.                          
           MOVE FIO-GET-NLS TO FIO-CMND.                             
           PERFORM COL-FIO-RDC.                                      
           IF NOT FIO-STAT-OKS                                       
               GO TO FIN-FND-KEY.                                    
    *                                                              
           MOVE FIO-GET-KEY TO FIO-CMND.                             
           PERFORM COL-FIO-OPC.                                      
           IF OPC-IND-RENO IN OPC = SPACES OR                        
               OPC-IND-RENO IN OPC NOT = 'PRO'                        
                MOVE 'COL'         TO MSG-COD-SIST                    
                MOVE 'OPC NO PRO '    TO MSG-COD-MENS                 
                PERFORM GET-MSG                                       
                MOVE MSG-GLS-DESC IN MSG TO FRM-MENS.                 
           IF OPC-VAL-SCRE IN OPC = ZEROES                           
                MOVE 'COL'         TO MSG-COD-SIST                    
                MOVE 'OPCSINSLDO '    TO MSG-COD-MENS                 
                PERFORM GET-MSG                                       
                MOVE MSG-GLS-DESC IN MSG TO FRM-MENS.                 
           MOVE RDC-CIC-ICLI IN RDC TO DBC-CIC-ICLI IN DBC.          
           MOVE FIO-GET-KEY TO FIO-CMND.                             
           PERFORM SGC-FIO-DBC.                                      
       DSP-CAR-MOV-DBC.                                              
           IF NOT FIO-STAT-OKS                                       
               GO TO FIN-FND-KEY.                                   
           MOVE FIO-STAT TO PGM-STAT-DBC.                            
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN CHI-FLD.       
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN CHI-FLD.      
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.                     
           PERFORM CPT-BLKS.                                         
           MOVE CPT-STRN TO DBC-GLS-NOMC IN DBC.                     
           MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN CHI-FLD.      
                                                                   
           MOVE ZEROES TO SCR-IMAX.                                  
           MOVE 1 TO SCR-I.                                          
           MOVE '00' TO FIO-STAT.                                    
           PERFORM OBT-DAT-TS.                                       
           MOVE 1 TO SCR-I.                                          
           MOVE SCR-I TO CHI-I.                                      
       CARGA-CHI.                                                    
           MOVE DBC-NUM-ICLI IN CHI-FLD TO DBC-NUM-ICLI IN DBC       
           MOVE DBC-IND-ICLI IN CHI-FLD TO DBC-IND-ICLI IN DBC       
           MOVE DBC-GLS-ICLI IN CHI-FLD TO DBC-GLS-ICLI IN DBC       
           MOVE 'DBC-IDC-ICLI' TO FIO-AKEY.                          
           MOVE FIO-GET-KEY  TO FIO-CMND.                            
           PERFORM SGC-FIO-DBC                                       
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.                     
           PERFORM CPT-BLKS.                                         
           MOVE CPT-STRN TO DBC-GLS-NOMC IN DBC.                     
           MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN CHI-FLD.      
           MOVE FIO-STAT TO PGM-STAT-DBC.                            
           MOVE SPACES TO MSG-COD-MENS.                              
           IF NOT SCR-OFRM-SRV                                       
              PERFORM PUT-TAB-CHI.                                      
           MOVE '00' TO FIO-STAT.                                    
           GO TO FIN-FND-KEY.                                        
      *                                                              
       ERR-FND-KEY.                                                  
           MOVE SCR-STAT-CON TO SCR-STAT.

           MOVE 'COL'  TO MSG-COD-SIST.                              
           PERFORM GET-MSG.                                          
           MOVE MSG-GLS-DESC TO FRM-MEN1.                            
           MOVE '00' TO FIO-STAT.                                    
           GO TO FIN-FND-KEY.                                        
      *>>>>
       FIN-FND-KEY.
           IF FIO-STAT = FIO-STAT-NEX
               MOVE 'S' TO SCR-IERR.
       EXT-FND-KEY.
           EXIT.
 
       PUT-SCR SECTION.
       INI-PUT-SCR.
           IF PGM-STAT-OPC-OKS
               MOVE OPC-STP-ITRN IN OPC TO SCR-UMOD.
           PERFORM PUT-ALL-CHI.
       FIN-PUT-SCR.
           EXIT.

       STP-EXIT SECTION.
       INI-STP-EXIT.
           MOVE SCR-PROG TO PGM-PROG.
           MOVE SCR-NTRN TO PGM-NTRN.

           MOVE SCR-PROV TO SCR-PROG.
           MOVE SCR-NTRV TO SCR-NTRN.

           MOVE PGM-PROG TO SCR-PROV
           MOVE PGM-NTRN TO SCR-NTRV.
       FIN-STP-EXIT.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO SYS-NTRN.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
      *PTC-INI
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE CHI-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO CHI-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GKEY SECTION.
       INI-SCR-GKEY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-CHI
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO CHI-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GKEY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-CHI.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
