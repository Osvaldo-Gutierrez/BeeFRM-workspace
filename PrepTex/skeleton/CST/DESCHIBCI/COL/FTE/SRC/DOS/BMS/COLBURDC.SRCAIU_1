      *
      * COLBURDC
      * Version 3.0.1 Nov-2005
      * 26-Aug-19 03:10 PM
      *
       SET-TAG-COLBURDC SECTION.
       INI-SET-TAG-COLBURDC.
           MOVE
           '<IDNMOD NOM=$COLBURDC$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$26-Aug-19 03:10 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBURDC.
           EXIT.
           
       RET-ALL-RDC SECTION.
       INI-RET-ALL-RDC.
           PERFORM KEY-ALL-RDC.
           PERFORM RET-OPC-RDC.
           PERFORM RET-RDC-RDC.
           PERFORM RET-DBC-RDC.
           PERFORM RET-RCD-RDC.
       FIN-RET-ALL-RDC.
           EXIT.
       
       RET-OPC-RDC SECTION.
       INI-RET-OPC-RDC.
           MOVE OPC-MSC-TVOC IN RDC-FLD TO OPC-MSC-TVOC IN OPC.
           MOVE OPC-COD-TOOC IN RDC-FLD TO OPC-COD-TOOC IN OPC.
           MOVE OPC-COD-OFOC IN RDC-FLD TO OPC-COD-OFOC IN OPC.
           MOVE OPC-COD-COOC IN RDC-FLD TO OPC-COD-COOC IN OPC.
           MOVE OPC-COD-VCOC IN RDC-FLD TO OPC-COD-VCOC IN OPC.
       FIN-RET-OPC-RDC.
           EXIT.
       
       RET-RDC-RDC SECTION.
       INI-RET-RDC-RDC.
           MOVE RDC-NUM-DIRE IN RDC-FLD TO RDC-NUM-DIRE IN RDC.
           MOVE RDC-VAL-LIMD IN RDC-FLD TO RDC-VAL-LIMD IN RDC.
       FIN-RET-RDC-RDC.
           EXIT.
       
       RET-DBC-RDC SECTION.
       INI-RET-DBC-RDC.
       FIN-RET-DBC-RDC.
           EXIT.
       
       RET-RCD-RDC SECTION.
       INI-RET-RCD-RDC.
           MOVE RCD-GLS-DIRE IN RDC-FLD TO RCD-GLS-DIRE IN RCD.
           MOVE RCD-COD-LUGA IN RDC-FLD TO RCD-COD-LUGA IN RCD.
           MOVE RCD-GLS-FONO IN RDC-FLD TO RCD-GLS-FONO IN RCD.
       FIN-RET-RCD-RDC.
           EXIT.
       
       KEY-ALL-RDC SECTION.
       INI-KEY-ALL-RDC.
           PERFORM KEY-OPC-RDC.
           PERFORM KEY-RDC-RDC.
           PERFORM KEY-DBC-RDC.
           PERFORM KEY-RCD-RDC.
       FIN-KEY-ALL-RDC.
           EXIT.
       
       KEY-OPC-RDC SECTION.
       INI-KEY-OPC-RDC.
       FIN-KEY-OPC-RDC.
           EXIT.
       
       KEY-RDC-RDC SECTION.
       INI-KEY-RDC-RDC.
           MOVE RDC-CAI-IOPC IN RDC-FLD TO RDC-CAI-IOPC IN RDC.
           MOVE RDC-IIC-IOPC IN RDC-FLD TO RDC-IIC-IOPC IN RDC.
           MOVE RDC-NUM-IDLC IN RDC-FLD TO RDC-NUM-IDLC IN RDC.
           MOVE RDC-CAI-ICLI IN RDC-FLD TO RDC-CAI-ICLI IN RDC.
           MOVE RDC-IIC-ICLI IN RDC-FLD TO RDC-IIC-ICLI IN RDC.
       FIN-KEY-RDC-RDC.
           EXIT.
       
       KEY-DBC-RDC SECTION.
       INI-KEY-DBC-RDC.
           MOVE DBC-GLS-NOMC IN RDC-FLD TO DBC-GLS-NOMC IN DBC.
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN RDC-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN RDC-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN RDC-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN RDC-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
       FIN-KEY-DBC-RDC.
           EXIT.
       
       KEY-RCD-RDC SECTION.
       INI-KEY-RCD-RDC.
       FIN-KEY-RCD-RDC.
           EXIT.
       
       SCR-CLRA-RDC SECTION.
       INI-SCR-CLRA-RDC.
           PERFORM CLR-KEY-RDC.
           PERFORM CLR-FLD-RDC.
       FIN-SCR-CLRA-RDC.
           EXIT.
       
       CLR-KEY-RDC SECTION.
       INI-CLR-KEY-RDC.
           MOVE SPACES TO RDC-CAI-IOPC IN RDC-FLD.
           MOVE ZEROES TO RDC-IIC-IOPC IN RDC-FLD.
           MOVE ZEROES TO RDC-NUM-IDLC IN RDC-FLD.
           MOVE SPACES TO DBC-GLS-NOMC IN RDC-FLD.
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN RDC-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN RDC-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN RDC-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN RDC-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO RDC-CAI-ICLI IN RDC-FLD.
           MOVE SPACES TO RDC-IIC-ICLI IN RDC-FLD.
       FIN-CLR-KEY-RDC.
           EXIT.
       
       CLR-FLD-RDC SECTION.
       INI-CLR-FLD-RDC.
           MOVE SPACES TO FRM-GLS-TVOC IN RDC-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN RDC-FLD.
           MOVE SPACES TO FRM-GLS-OFOC IN RDC-FLD.
           MOVE SPACES TO FRM-IND-TDEU IN RDC-FLD.
           MOVE SPACES TO FRM-GLS-LUGA IN RDC-FLD.
           MOVE SPACES TO FRM-GLS-COOC IN RDC-FLD.
           MOVE ZEROES TO FRM-VAL-MLIM IN RDC-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN RDC-FLD.
           PERFORM CLR-OPC-RDC.
           PERFORM CLR-RDC-RDC.
           PERFORM CLR-DBC-RDC.
           PERFORM CLR-RCD-RDC.
       FIN-CLR-FLD-RDC.
           EXIT.
       
       CLR-OPC-RDC SECTION.
       INI-CLR-OPC-RDC.
           MOVE SPACES TO OPC-MSC-TVOC IN RDC-FLD.
           MOVE SPACES TO OPC-COD-TOOC IN RDC-FLD.
           MOVE SPACES TO OPC-COD-OFOC IN RDC-FLD.
           MOVE SPACES TO OPC-COD-COOC IN RDC-FLD.
           MOVE SPACES TO OPC-COD-VCOC IN RDC-FLD.
       FIN-CLR-OPC-RDC.
           EXIT.
       
       CLR-RDC-RDC SECTION.
       INI-CLR-RDC-RDC.
           MOVE ZEROES TO RDC-NUM-DIRE IN RDC-FLD.
           MOVE ZEROES TO RDC-VAL-LIMD IN RDC-FLD.
       FIN-CLR-RDC-RDC.
           EXIT.
       
       CLR-DBC-RDC SECTION.
       INI-CLR-DBC-RDC.
       FIN-CLR-DBC-RDC.
           EXIT.
       
       CLR-RCD-RDC SECTION.
       INI-CLR-RCD-RDC.
           MOVE SPACES TO RCD-GLS-DIRE IN RDC-FLD.
           MOVE SPACES TO RCD-COD-LUGA IN RDC-FLD.
           MOVE SPACES TO RCD-GLS-FONO IN RDC-FLD.
       FIN-CLR-RCD-RDC.
           EXIT.
       
       FST-KEY-RDC SECTION.
       INI-FST-KEY-RDC.
           MOVE -1 TO RDC-CAI-IOPC-LEN IN RDC-FLD.
       FIN-FST-KEY-RDC.
           EXIT.
       
       FST-FLD-RDC SECTION.
       INI-FST-FLD-RDC.
           MOVE -1 TO RDC-NUM-DIRE-LEN IN RDC-FLD.
       FIN-FST-FLD-RDC.
           EXIT.
       
       PRO-KEY-RDC SECTION.
       INI-PRO-KEY-RDC.
           PERFORM PRO-MKY-RDC.
       FIN-PRO-KEY-RDC.
           EXIT.
       
       PRO-MKY-RDC SECTION.
       INI-PRO-MKY-RDC.
       FIN-PRO-MKY-RDC.
           EXIT.
       
       PRO-FLD-RDC SECTION.
       INI-PRO-FLD-RDC.
           PERFORM PRO-OPC-RDC.
           PERFORM PRO-RDC-RDC.
           PERFORM PRO-DBC-RDC.
           PERFORM PRO-RCD-RDC.
       FIN-PRO-FLD-RDC.
           EXIT.
       
       PRO-OPC-RDC SECTION.
       INI-PRO-OPC-RDC.
       FIN-PRO-OPC-RDC.
           EXIT.
       
       PRO-RDC-RDC SECTION.
       INI-PRO-RDC-RDC.
           MOVE FRM-CPIM-AEY TO RDC-NUM-DIRE-ATR IN RDC-FLD.
           MOVE FRM-CPIM-AEY TO RDC-VAL-LIMD-ATR IN RDC-FLD.
       FIN-PRO-RDC-RDC.
           EXIT.
       
       PRO-DBC-RDC SECTION.
       INI-PRO-DBC-RDC.
       FIN-PRO-DBC-RDC.
           EXIT.
       
       PRO-RCD-RDC SECTION.
       INI-PRO-RCD-RDC.
       FIN-PRO-RCD-RDC.
           EXIT.
       
       UNP-KEY-RDC SECTION.
       INI-UNP-KEY-RDC.
           PERFORM UNP-IKY-RDC.
           PERFORM UNP-AKY-RDC.
           PERFORM UNP-MKY-RDC.
       FIN-UNP-KEY-RDC.
           EXIT.
       
       UNP-IKY-RDC SECTION.
       INI-UNP-IKY-RDC.
           MOVE FRM-CPIM-UBY TO RDC-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO RDC-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO RDC-NUM-IDLC-ATR.
      * IDC-INI --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO DBC-NUM-ICLI-ATR IN RDC-FLD.
           MOVE FRM-CPIM-UBY TO DBC-VRF-ICLI-ATR IN RDC-FLD.
           MOVE FRM-CPIM-UBY TO DBC-IND-ICLI-ATR IN RDC-FLD.
           MOVE FRM-CPIM-UBY TO DBC-GLS-ICLI-ATR IN RDC-FLD.
      * IDC-FIN --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
       FIN-UNP-IKY-RDC.
           EXIT.
       
       UNP-MKY-RDC SECTION.
       INI-UNP-MKY-RDC.
       FIN-UNP-MKY-RDC.
           EXIT.
       
       UNP-AKY-RDC SECTION.
       INI-UNP-AKY-RDC.
           MOVE FRM-CPIM-UBY TO DBC-GLS-NOMC-ATR.
       FIN-UNP-AKY-RDC.
           EXIT.
       
       UNP-FLD-RDC SECTION.
       INI-UNP-FLD-RDC.
           PERFORM UNP-OPC-RDC.
           PERFORM UNP-RDC-RDC.
           PERFORM UNP-DBC-RDC.
           PERFORM UNP-RCD-RDC.
       FIN-UNP-FLD-RDC.
           EXIT.
       
       UNP-OPC-RDC SECTION.
       INI-UNP-OPC-RDC.
       FIN-UNP-OPC-RDC.
           EXIT.
       
       UNP-RDC-RDC SECTION.
       INI-UNP-RDC-RDC.
           MOVE FRM-CPIM-UBY TO RDC-NUM-DIRE-ATR IN RDC-FLD.
           MOVE FRM-CPIM-UBY TO RDC-VAL-LIMD-ATR IN RDC-FLD.
       FIN-UNP-RDC-RDC.
           EXIT.
       
       UNP-DBC-RDC SECTION.
       INI-UNP-DBC-RDC.
       FIN-UNP-DBC-RDC.
           EXIT.
       
       UNP-RCD-RDC SECTION.
       INI-UNP-RCD-RDC.
       FIN-UNP-RCD-RDC.
           EXIT.
       
       VAL-MKY-RDC SECTION.
       INI-VAL-MKY-RDC.
       FIN-VAL-MKY-RDC.
           EXIT.
       
       VAL-FLD-RDC SECTION.
       INI-VAL-FLD-RDC.
           PERFORM VAL-OPC-MSC-TVOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-MSC-TVOC-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-OPC-COD-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-TOOC-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-OPC-COD-OFOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-OFOC-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-FRM-GLS-TVOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TVOC-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-FRM-GLS-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TOOC-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-FRM-GLS-OFOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-OFOC-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-FRM-IND-TDEU.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IND-TDEU-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-RDC-NUM-DIRE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RDC-NUM-DIRE-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-RCD-GLS-DIRE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RCD-GLS-DIRE-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-RCD-COD-LUGA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RCD-COD-LUGA-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-RCD-GLS-FONO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RCD-GLS-FONO-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-FRM-GLS-LUGA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-LUGA-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-OPC-COD-COOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-COOC-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-RDC-VAL-LIMD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RDC-VAL-LIMD-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-FRM-GLS-COOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-COOC-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-OPC-COD-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-VCOC-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-FRM-VAL-MLIM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-MLIM-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
           PERFORM VAL-FRM-GLS-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-VCOC-LEN IN RDC-FLD
                GO TO FIN-VAL-FLD-RDC.
       FIN-VAL-FLD-RDC.
           EXIT.
       
       VAL-OPC-MSC-TVOC SECTION.
       INI-VAL-OPC-MSC-TVOC.
       FIN-VAL-OPC-MSC-TVOC.
           EXIT.
       
       VAL-OPC-COD-TOOC SECTION.
       INI-VAL-OPC-COD-TOOC.
       FIN-VAL-OPC-COD-TOOC.
           EXIT.
       
       VAL-OPC-COD-OFOC SECTION.
       INI-VAL-OPC-COD-OFOC.
       FIN-VAL-OPC-COD-OFOC.
           EXIT.
       
       VAL-FRM-GLS-TVOC SECTION.
       INI-VAL-FRM-GLS-TVOC.
       FIN-VAL-FRM-GLS-TVOC.
           EXIT.
       
       VAL-FRM-GLS-TOOC SECTION.
       INI-VAL-FRM-GLS-TOOC.
       FIN-VAL-FRM-GLS-TOOC.
           EXIT.
       
       VAL-FRM-GLS-OFOC SECTION.
       INI-VAL-FRM-GLS-OFOC.
       FIN-VAL-FRM-GLS-OFOC.
           EXIT.
       
       VAL-FRM-IND-TDEU SECTION.
       INI-VAL-FRM-IND-TDEU.
       FIN-VAL-FRM-IND-TDEU.
           EXIT.
       
       VAL-RDC-NUM-DIRE SECTION.
       INI-VAL-RDC-NUM-DIRE.
      *<<<< RDC_NUM_DIRE
           IF RDC-NUM-DIRE IN RDC-FLD > ZEROES
               MOVE RDC-CAI-ICLI IN RDC-FLD TO RCD-CAI-ICLI IN RCD
               MOVE RDC-IIC-ICLI IN RDC-FLD TO RCD-IIC-ICLI IN RCD
               MOVE RDC-NUM-DIRE IN RDC-FLD TO RCD-NUM-DIRE IN RCD
               MOVE 'RCD-KEY-IRCD' TO FIO-AKEY
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM SGC-FIO-RCD
               IF NOT FIO-STAT-OKS
                   MOVE 'COL' TO MSG-COD-SIST
                   MOVE 'DIR    NEX' TO MSG-COD-MENS            
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   GO TO FIN-VAL-RDC-NUM-DIRE
               ELSE
                   MOVE '00' TO PGM-STAT-RCD
                   PERFORM PUT-RCD-RDC
                   PERFORM VAL-LUGA
           ELSE
               MOVE RDC-CAI-ICLI IN RDC-FLD TO DBC-CAI-ICLI IN DBC
               MOVE RDC-IIC-ICLI IN RDC-FLD TO DBC-IIC-ICLI IN DBC
               PERFORM BUS-DOM
               IF RCD-GLS-DIRE IN RCD > SPACES
                   MOVE RCD-NUM-DIRE IN RCD TO RDC-NUM-DIRE IN RDC-FLD
                   MOVE '00' TO PGM-STAT-RCD
                   PERFORM PUT-RCD-RDC
                   PERFORM VAL-LUGA.
      *>>>>
       FIN-VAL-RDC-NUM-DIRE.
           EXIT.
       
       VAL-RCD-GLS-DIRE SECTION.
       INI-VAL-RCD-GLS-DIRE.
       FIN-VAL-RCD-GLS-DIRE.
           EXIT.
       
       VAL-RCD-COD-LUGA SECTION.
       INI-VAL-RCD-COD-LUGA.
       FIN-VAL-RCD-COD-LUGA.
           EXIT.
       
       VAL-RCD-GLS-FONO SECTION.
       INI-VAL-RCD-GLS-FONO.
       FIN-VAL-RCD-GLS-FONO.
           EXIT.
       
       VAL-FRM-GLS-LUGA SECTION.
       INI-VAL-FRM-GLS-LUGA.
       FIN-VAL-FRM-GLS-LUGA.
           EXIT.
       
       VAL-OPC-COD-COOC SECTION.
       INI-VAL-OPC-COD-COOC.
       FIN-VAL-OPC-COD-COOC.
           EXIT.
       
       VAL-RDC-VAL-LIMD SECTION.
       INI-VAL-RDC-VAL-LIMD.
      *<<<< RDC_VAL_LIMD
           IF RDC-VAL-LIMD IN RDC-FLD > 100
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'VALLIMD>100' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-RDC-VAL-LIMD.
      *>>>>
       FIN-VAL-RDC-VAL-LIMD.
           EXIT.
       
       VAL-FRM-GLS-COOC SECTION.
       INI-VAL-FRM-GLS-COOC.
       FIN-VAL-FRM-GLS-COOC.
           EXIT.
       
       VAL-OPC-COD-VCOC SECTION.
       INI-VAL-OPC-COD-VCOC.
       FIN-VAL-OPC-COD-VCOC.
           EXIT.
       
       VAL-FRM-VAL-MLIM SECTION.
       INI-VAL-FRM-VAL-MLIM.
      *<<<< FRM_VAL_MLIM
           IF FRM-VAL-MLIM IN RDC-FLD > OPC-VAL-CRED IN OPC
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'VALMLIMSCRE>' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-FRM-VAL-MLIM.
      *>>>>
       FIN-VAL-FRM-VAL-MLIM.
           EXIT.
       
       VAL-FRM-GLS-VCOC SECTION.
       INI-VAL-FRM-GLS-VCOC.
       FIN-VAL-FRM-GLS-VCOC.
           EXIT.
       
       VAL-CON-FLD-RDC SECTION.
       INI-VAL-CON-FLD-RDC.
      *<<<< VAL_CON_FLD_RDC
      *ISP ESTA INSTRUCCION SE AGREGO PARA NO ELIMINAR EL CODIGO
      *    QUE MANEJA LA SITUACION COMPLETA.
           MOVE ZEROES TO FRM-VAL-MLIM.
      *
           PERFORM GET-MLIM.
           IF WSS-VAL-MLIM NOT = FRM-VAL-MLIM
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'VALLIMDMLIM' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CON-FLD-RDC.
      *>>>>
       FIN-VAL-CON-FLD-RDC.
           EXIT.
       
       VAL-CON-RDC SECTION.
       INI-VAL-CON-RDC.
       FIN-VAL-CON-RDC.
           EXIT.
       
       VAL-NUM-KEY-RDC SECTION.
       INI-VAL-NUM-KEY-RDC.
           PERFORM VAL-NUM-IKY-RDC.
           PERFORM VAL-NUM-MKY-RDC.
           PERFORM VAL-NUM-AKY-RDC.
       FIN-VAL-NUM-KEY-RDC.
           EXIT.
       
       VAL-NUM-IKY-RDC SECTION.
       INI-VAL-NUM-IKY-RDC.
       VAL-NUM-RDC-IIC-IOPC.
           MOVE RDC-IIC-IOPC-ALF IN RDC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO RDC-IIC-IOPC IN RDC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO RDC-IIC-IOPC-LEN IN RDC-FLD
                GO TO FIN-VAL-NUM-IKY-RDC.
       VAL-NUM-RDC-NUM-IDLC.
           MOVE RDC-NUM-IDLC-ALF IN RDC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO RDC-NUM-IDLC IN RDC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO RDC-NUM-IDLC-LEN IN RDC-FLD
                GO TO FIN-VAL-NUM-IKY-RDC.
      * IDC-INI --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
       VAL-NUM-DBC-NUM-ICLI.
           MOVE DBC-NUM-ICLI-ALF IN RDC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO DBC-NUM-ICLI IN RDC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO DBC-NUM-ICLI-LEN IN RDC-FLD
                GO TO FIN-VAL-NUM-IKY-RDC.
      * IDC-FIN --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
       FIN-VAL-NUM-IKY-RDC.
           EXIT.
       
       VAL-NUM-MKY-RDC SECTION.
       INI-VAL-NUM-MKY-RDC.
       FIN-VAL-NUM-MKY-RDC.
           EXIT.
       
       VAL-NUM-AKY-RDC SECTION.
       INI-VAL-NUM-AKY-RDC.
       FIN-VAL-NUM-AKY-RDC.
           EXIT.
       
       VAL-NUM-FLD-RDC SECTION.
       INI-VAL-NUM-FLD-RDC.
       VAL-NUM-RDC-NUM-DIRE.
           MOVE RDC-NUM-DIRE-ALF IN RDC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 2 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO RDC-NUM-DIRE IN RDC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO RDC-NUM-DIRE-LEN IN RDC-FLD
                GO TO FIN-VAL-NUM-FLD-RDC.
       VAL-NUM-RDC-VAL-LIMD.
           MOVE RDC-VAL-LIMD-ALF IN RDC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 5 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9V TO RDC-VAL-LIMD IN RDC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO RDC-VAL-LIMD-LEN IN RDC-FLD
                GO TO FIN-VAL-NUM-FLD-RDC.
       VAL-NUM-FRM-VAL-MLIM.
           MOVE FRM-VAL-MLIM-ALF IN RDC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9V TO FRM-VAL-MLIM IN RDC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-VAL-MLIM-LEN IN RDC-FLD
                GO TO FIN-VAL-NUM-FLD-RDC.
       FIN-VAL-NUM-FLD-RDC.
           EXIT.
       
       EDT-KEY-RDC SECTION.
       INI-EDT-KEY-RDC.
           PERFORM EDT-IKY-RDC.
           PERFORM EDT-MKY-RDC.
           PERFORM EDT-AKY-RDC.
       FIN-EDT-KEY-RDC.
           EXIT.
       
       EDT-IKY-RDC SECTION.
       INI-EDT-IKY-RDC.
           IF RDC-IIC-IOPC-LEN IN RDC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO RDC-IIC-IOPC-LEN IN RDC-FLD
                GO TO FIN-EDT-IKY-RDC.
           MOVE RDC-IIC-IOPC IN RDC-FLD TO RDC-IIC-IOPC-EDT IN RDC-EDT.
           MOVE RDC-IIC-IOPC-EDT IN RDC-EDT TO 
                RDC-IIC-IOPC-ALF IN RDC-FLD.
           IF RDC-NUM-IDLC-LEN IN RDC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO RDC-NUM-IDLC-LEN IN RDC-FLD
                GO TO FIN-EDT-IKY-RDC.
           MOVE RDC-NUM-IDLC IN RDC-FLD TO RDC-NUM-IDLC-EDT IN RDC-EDT.
           MOVE RDC-NUM-IDLC-EDT IN RDC-EDT TO 
                RDC-NUM-IDLC-ALF IN RDC-FLD.
      * IDC-INI --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF DBC-NUM-ICLI-LEN IN RDC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DBC-NUM-ICLI-LEN IN RDC-FLD
                GO TO FIN-EDT-IKY-RDC.
           MOVE DBC-NUM-ICLI IN RDC-FLD TO
                DBC-NUM-ICLI-EDT IN RDC-EDT.
           MOVE DBC-NUM-ICLI-EDT IN RDC-EDT TO
                DBC-NUM-ICLI-ALF IN RDC-FLD.
      * IDC-FIN --> BMCHIBCI011.SRCAIU EDICION NUMERICA
       FIN-EDT-IKY-RDC.
           EXIT.
       
       EDT-MKY-RDC SECTION.
       INI-EDT-MKY-RDC.
       FIN-EDT-MKY-RDC.
           EXIT.
       
       EDT-AKY-RDC SECTION.
       INI-EDT-AKY-RDC.
       FIN-EDT-AKY-RDC.
           EXIT.
       
       EDT-FLD-RDC SECTION.
       INI-EDT-FLD-RDC.
           IF RDC-NUM-DIRE-LEN IN RDC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO RDC-NUM-DIRE-LEN IN RDC-FLD
                GO TO FIN-EDT-FLD-RDC.
           MOVE RDC-NUM-DIRE IN RDC-FLD TO RDC-NUM-DIRE-EDT IN RDC-EDT.
           MOVE RDC-NUM-DIRE-EDT IN RDC-EDT TO 
                RDC-NUM-DIRE-ALF IN RDC-FLD.
           IF RDC-VAL-LIMD-LEN IN RDC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO RDC-VAL-LIMD-LEN IN RDC-FLD
                GO TO FIN-EDT-FLD-RDC.
           MOVE RDC-VAL-LIMD IN RDC-FLD TO RDC-VAL-LIMD-EDT IN RDC-EDT.
           MOVE RDC-VAL-LIMD-EDT IN RDC-EDT TO 
                RDC-VAL-LIMD-ALF IN RDC-FLD.
           IF FRM-VAL-MLIM-LEN IN RDC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-MLIM-LEN IN RDC-FLD
                GO TO FIN-EDT-FLD-RDC.
           MOVE FRM-VAL-MLIM IN RDC-FLD TO FRM-VAL-MLIM-EDT IN RDC-EDT.
           MOVE FRM-VAL-MLIM-EDT IN RDC-EDT TO 
                FRM-VAL-MLIM-ALF IN RDC-FLD.
       FIN-EDT-FLD-RDC.
           EXIT.
      *<<<< EOF
       GET-MLIM SECTION.
       INI-GET-MLIM.
           MOVE 'TAB'          TO FIO-SIST.
           MOVE 'VLR'          TO TAB-CIC-TTAB.
           MOVE '0999  '       TO TAB-CIC-CTAB.
           MOVE FIO-GET-KEY    TO FIO-CMND.
           MOVE 'TAB-CIC-TABL' TO FIO-AKEY.
           PERFORM GNS-FIO-TAB.
           IF NOT  FIO-STAT-OKS
               MOVE SPACES TO TAB-COD-CTAB IN TAB.
           IF RDC-NUM-IDLC IN RDC-FLD > ZEROES
               MOVE DLC-VAL-NDLC IN DLC TO WSS-VAL-CRED
           ELSE
               MOVE OPC-VAL-CRED IN OPC TO WSS-VAL-CRED.
           IF RDC-VAL-LIMD IN RDC-FLD NOT > ZEROES AND
              FRM-VAL-MLIM IN RDC-FLD NOT > ZEROES
               MOVE 100          TO RDC-VAL-LIMD IN RDC-FLD
               MOVE WSS-VAL-CRED TO FRM-VAL-MLIM IN RDC-FLD
               MOVE FRM-VAL-MLIM TO WSS-VAL-MLIM
           ELSE
           IF RDC-VAL-LIMD IN RDC-FLD > ZEROES AND
              FRM-VAL-MLIM IN RDC-FLD NOT > ZEROES
               COMPUTE FRM-VAL-MLIM =
               ( RDC-VAL-LIMD IN RDC-FLD * WSS-VAL-CRED ) / 100
               MOVE FRM-VAL-MLIM TO WSS-VAL-MLIM
           ELSE
           IF RDC-VAL-LIMD IN RDC-FLD NOT > ZEROES AND
              FRM-VAL-MLIM IN RDC-FLD > ZEROES
               COMPUTE RDC-VAL-LIMD IN RDC-FLD =
                       ( FRM-VAL-MLIM * 100 ) / WSS-VAL-CRED
               MOVE FRM-VAL-MLIM TO WSS-VAL-MLIM
           ELSE
               COMPUTE WSS-VAL-MLIM =
               ( RDC-VAL-LIMD IN RDC-FLD * WSS-VAL-CRED ) / 100.
           IF TAB-COD-CTAB IN TAB = OPC-COD-VCOC IN RDC-FLD
               MOVE WSS-VAL-MLIM TO PES-SGV-DECI IN PES-VARI
               PERFORM PES-SCTV
               MOVE PES-SGV-ENTE IN PES-VARI TO WSS-VAL-MLIM
               MOVE FRM-VAL-MLIM TO PES-SGV-DECI IN PES-VARI
               PERFORM PES-SCTV
               MOVE PES-SGV-ENTE TO FRM-VAL-MLIM.
       FIN-GET-MLIM.
           EXIT.
      *>>>>
