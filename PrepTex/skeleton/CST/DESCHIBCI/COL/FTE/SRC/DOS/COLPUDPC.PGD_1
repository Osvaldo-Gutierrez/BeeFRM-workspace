       IDENTIFICATION DIVISION.
      *========================
       PROGRAM-ID.   COLPUDPC.
       AUTHOR.       BEE.
       DATE-WRITTEN. 01-Apr-10 02:21 PM.

      * NOMBRE PROGRAMA                   COLPUDPC
      * VERSION PGM                       3.1
      * VERSION SKL                       08.07.21
      * OPCION                            UPD
      * ARCHIVO BASE                      COLDPC
      * PANTALLA                          COLDPC
      * INP/OUT                           DATACOM
      * TAMANO COMMAREA                   250


       ENVIRONMENT DIVISION.
      *=====================
       CONFIGURATION SECTION.
      *----------------------
       SPECIAL-NAMES.
               DECIMAL-POINT IS COMMA.

       DATA DIVISION.
      *==============

       WORKING-STORAGE SECTION.
      *------------------------
      * COPY GNSWGSCR.
       COPY GNSWGCVT.
       77  WSS-TAG-COLPUDPC-INI PIC X(125) VALUE                        
           '<IDNPGM NOM=$COLPUDPC$ SKL=$08.07.21$ VSN=${vsnpu}$        
      -    'GEN=$01-Apr-10 02:21 PM$ FNC=${fncpu}$/>'.    
       COPY GNSWCSCR.
       COPY GNSWVSCR.
       COPY COLWIDPC.
       COPY COLBRDPC.


       COPY GNSWVIDD.
       COPY GNSWGRQA.
       COPY GNSWGUIB.
       COPY GNSWGELS.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DPC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-TAB-REQA==.
       COPY GNSBRTAB.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSG-REQA==.
       COPY GNSBRMSG.
       COPY GNSWCFIO.
       COPY GNSWVFIO.
       COPY GNSWGFRM.
       COPY GNSWVNUM.
       COPY GNSWCNUM.
       COPY GNSWGFEC.
       COPY GNSWGHOY.
       COPY GNSWGSYS.

       01  PGM-VARI.
           03 PGM-ICLR                  VALUE ' '       PIC X(01).
           03 PGM-LIMPIAR-TODO          VALUE ' '       PIC X(01).
           03 PGM-LIMPIAR-LLAVES        VALUE 'K'       PIC X(01).
           03 PGM-LIMPIAR-CAMPOS        VALUE 'F'       PIC X(01).
           03 PGM-NO-LIMPIAR-NADA       VALUE 'N'       PIC X(01).
           03 PGM-PROG                                  PIC X(08).
           03 PGM-NTRN                                  PIC X(04).
           03 PGM-STAT.
              05 PGM-STAT-DPC.
                 88 PGM-STAT-DPC-NUL    VALUE '  '.
                 07 PGM-STT1-DPC        VALUE '2' PIC X(01).
                    88 PGM-STAT-DPC-OKS VALUE '0'.
                 07 PGM-STT2-DPC        VALUE '3' PIC X(01).
           03 PGM-SRNF                  VALUE '23'      PIC X(02).
           03 PGM-SOKS                  VALUE '00'      PIC X(02).
           03 PGM-SNUL                  VALUE '  '      PIC X(02).
           03 PGM-SUAR                  VALUE SPACES    PIC X(15).

       LINKAGE SECTION.
      *----------------
       01  DFHCOMMAREA.
           02 FILLER                    PIC X(250).
       COPY GNSLGFIO.

       PROCEDURE DIVISION.
      *===================

       MAIN SECTION.
       INI-MAIN.
           PERFORM INI.
       LUP-MAIN.
           IF SCR-INST = SCR-INST-HCM
                 PERFORM SCR-COM.
           IF SCR-CCPP = 'MOD'
                 PERFORM SCR-MOD
           ELSE
           IF SCR-CCPP = 'ING'
                 PERFORM SCR-ING
           ELSE
           IF SCR-CCPP = 'ACC'
                 PERFORM SCR-ACC
           ELSE
           IF SCR-CCPP = 'ELI'
                 PERFORM SCR-ELI
           ELSE
           IF SCR-CCPP = 'ACT'
                 PERFORM SCR-ACT
           ELSE
           IF SCR-CCPP = 'PGM'
                 PERFORM SCR-PGM
           ELSE
                 MOVE SCR-INST-HCM TO SCR-INST.
           GO TO LUP-MAIN.
       FIN-MAIN.
           EXIT.

       INI SECTION.
       INI-INI.
       COPY GNSBGEIB.
       COPY GNSBGEDB.
           MOVE LOW-VALUES TO DPC-FLD.
           MOVE LOW-VALUES TO DPC-EDT.
           PERFORM GNS-PRO-RCK.
           PERFORM GNS-BUS-VCA.
           PERFORM GNS-HDL-SYS.
           PERFORM GNS-HDL-FRM.
           MOVE 'COLPUDPC' TO FIO-PROG.
           PERFORM GNS-BUS-IDD.
       FIN-INI.
           EXIT.

       COPY GNSBHSYS.
       COPY GNSBHFRM.
       COPY GNSBPRCK.
       COPY GNSBBVCA.
       COPY GNSBBIDD.

       SCR-COM SECTION.
       INI-SCR-COM.
           PERFORM STP-EXIT.
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-CLRA-DPC.
           PERFORM EDT-KEY-DPC.
           PERFORM EDT-FLD-DPC.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-HCM TO SCR-INST.
           MOVE SCR-CMNP     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
           PERFORM GNS-PRO-SYS.
       XCT-ERR.
           MOVE 'XCTERR    ' TO MSG-COD-MENS.
           MOVE 'GNS'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO SYS-MEN1.
           MOVE SYS-PROG     TO SYS-MEN2.
           MOVE SYS-ABOR     TO SYS-CMND.
           PERFORM GNS-PRO-SYS.
       COPY GNSBGGBK.
       FIN-SCR-COM.
           EXIT.

       SCR-MOD SECTION.
       INI-SCR-MOD.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
               PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
               PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8
               PERFORM XCT-PRGP.
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
               GO TO MSG-SCR-MOD.
       MSG-SCR-MOD.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'MODNOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-MOD
               IF FIO-STAT-OKS
                   IF FRM-MEN1 NOT > SPACES
                       MOVE 'MODYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MEN1
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-MOD.
           EXIT.

       SCR-ING SECTION.
       INI-SCR-ING.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-IKY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
       MSG-SCR-ING.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'INGNOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-ING
               IF FIO-STAT-OKS
                   IF FRM-MEN1 NOT > SPACES
                       MOVE 'INGYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MEN1
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ING.
           EXIT.

       SCR-ACC SECTION.
       INI-SCR-ACC.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8 OR
                         FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM XCT-PRGP.
           MOVE SPACES TO FRM-MENS.
           IF SCR-STAT NOT = SCR-STAT-CON
               PERFORM CMN-ACC.
           PERFORM SCR-MSSG.
       FIN-SCR-ACC.
           EXIT.

       SCR-ELI SECTION.
       INI-SCR-ELI.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8 OR
                         FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM XCT-PRGP.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'ELINOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-ELI
               IF FIO-STAT-OKS
                   IF FRM-MEN1 NOT > SPACES
                       MOVE 'ELIYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MEN1
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ELI.
           EXIT.

       SCR-ACT SECTION.
       INI-SCR-ACT.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF ( FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8  OR
                           FRM-FFLD-PF5 OR FRM-FFLD-PF6 ) AND
              SCR-ACTU = SCR-ACTU-MOD
               PERFORM XCT-PRGP.
           IF SCR-ACTU = SCR-ACTU-MOD
                  PERFORM MSG-SCR-MOD
           ELSE
                  PERFORM MSG-SCR-ING.
       FIN-SCR-ACT.
           EXIT.

       SCR-PGM SECTION.
       INI-SCR-PGM.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
       TRL-SCR-PGM.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8 OR
                         FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM XCT-PRGP.
           MOVE SPACES TO FRM-MENS.
           IF SCR-STAT NOT = SCR-STAT-CON
               PERFORM CMN-PGM.
           PERFORM SCR-MSSG.
       FIN-SCR-PGM.
           EXIT.

       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               PERFORM SCR-CLRA-DPC
               PERFORM EDT-KEY-DPC
               PERFORM EDT-FLD-DPC
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-DPC
               PERFORM EDT-KEY-DPC
           ELSE
               PERFORM CLR-FLD-DPC
               PERFORM EDT-FLD-DPC.
           PERFORM STP-EXIT.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

       XCT-PRGP SECTION.
       INI-XCT-PRGP.
           MOVE SCR-INST-BRW TO SCR-INST.
           PERFORM STP-EXIT.
           MOVE FRM-FFLD   TO SCR-FFLD.
      *
           MOVE SCR-PROG     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
           PERFORM GNS-PRO-SYS.
           PERFORM XCT-ERR.
       FIN-XCT-PRGP.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO DPC-FLD.
           MOVE LOW-VALUES TO DPC-EDT.
       FIN-SCR-RST.
           EXIT.

       GET-FLD SECTION.
       INI-GET-FLD.
           PERFORM SCR-GFLD.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-FLD.
           EXIT.

       CMN-MOD SECTION.
       INI-CMN-MOD.
           PERFORM KEY-ALL-DPC.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM COL-FIO-DPC.
           IF FIO-STAT-OKS AND
              DPC-STP-ITRN IN DPC NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM COL-FIO-DPC
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM COL-FIO-DPC
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-MOD.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-MOD.
           PERFORM RET-STT-DPC.
           PERFORM RET-ALL-DPC.
           PERFORM RET-HDR-DPC.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM COL-FIO-DPC.
       FIN-CMN-MOD.
           EXIT.

       CMN-ING SECTION.
       INI-CMN-ING.
           MOVE SPACES TO DPC.
      *<<<< INI_CMN_ING                                                 
           MOVE ZEROES TO DPC-FEC-FTRN IN DPC.                          
           MOVE ZEROES TO DPC-HRA-HRTR IN DPC.                          
           MOVE ZEROES TO DPC-FEC-FPRO IN DPC.                          
           MOVE ZEROES TO DPC-FEC-FINI IN DPC.                          
           MOVE ZEROES TO DPC-FEC-FTER IN DPC.                          
           MOVE ZEROES TO DPC-NUM-FOI2 IN DPC.                          
           MOVE ZEROES TO DPC-NUM-FOF2 IN DPC.                          
           MOVE ZEROES TO DPC-VAL-SDAN IN DPC.                          
           MOVE ZEROES TO DPC-VAL-SHAN IN DPC.                          
           MOVE ZEROES TO DPC-VAL-MDPE IN DPC.                          
           MOVE ZEROES TO DPC-VAL-MHPE IN DPC.                          
           MOVE ZEROES TO DPC-VAL-SDNV IN DPC.                          
           MOVE ZEROES TO DPC-VAL-SHNV IN DPC.                          
           MOVE ZEROES TO DPC-VAL-MACU IN DPC.                          
           MOVE ZEROES TO DPC-NUM-RACU IN DPC.                          
           MOVE ZEROES TO DPC-VAL-MELI IN DPC.                          
           MOVE ZEROES TO DPC-NUM-RELI IN DPC.                          
           MOVE ZEROES TO DPC-FEC-FELI IN DPC.                          
           MOVE ZEROES TO DPC-FEC-FPR4 IN DPC.                          
           MOVE ZEROES TO DPC-NUM-FOI5 IN DPC.                          
           MOVE ZEROES TO DPC-NUM-FOF5 IN DPC.                          
      *>>>>                                                             
           PERFORM RET-STT-DPC.
           PERFORM RET-ALL-DPC.
           PERFORM RET-HDR-DPC.
           MOVE FIO-PUT TO FIO-CMND.
           PERFORM COL-FIO-DPC.
       FIN-CMN-ING.
           EXIT.

       CMN-ACC SECTION.
       INI-CMN-ACC.
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'ACCION NO REALIZADA' TO FRM-MENS
               GO TO FIN-CMN-ACC.
           PERFORM KEY-ALL-DPC.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM COL-FIO-DPC.
           IF FIO-STAT-OKS AND
              DPC-STP-ITRN IN DPC NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM COL-FIO-DPC
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM COL-FIO-DPC
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-ACC.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ACC.
           PERFORM RET-ALL-DPC.
       FIN-CMN-ACC.
           EXIT.

       CMN-ELI SECTION.
       INI-CMN-ELI.
           PERFORM KEY-ALL-DPC.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM COL-FIO-DPC.
           IF FIO-STAT-OKS AND
              DPC-STP-ITRN IN DPC NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM COL-FIO-DPC
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM COL-FIO-DPC
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-ELI.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ELI.
           MOVE FIO-DEL TO FIO-CMND.
           PERFORM COL-FIO-DPC.
       FIN-CMN-ELI.
           EXIT.

       CMN-PGM SECTION.
       INI-CMN-PGM.
           PERFORM RET-ALL-DPC.
       FIN-CMN-PGM.
           EXIT.

       STP-EXIT SECTION.
       INI-STP-EXIT.
           MOVE SCR-PROG TO PGM-PROG.
           MOVE SCR-NTRN TO PGM-NTRN.

           MOVE SCR-PROV TO SCR-PROG.
           MOVE SCR-NTRV TO SCR-NTRN.

           MOVE PGM-PROG TO SCR-PROV
           MOVE PGM-NTRN TO SCR-NTRV.
       FIN-STP-EXIT.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO SYS-NTRN.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE DPC-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO DPC-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GFLD SECTION.
       INI-SCR-GFLD.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-FLD-DPC
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO DPC-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GFLD.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-DPC.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-DPC
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           PERFORM VAL-NUM-FLD-DPC.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-DPC
               PERFORM EDT-FLD-DPC
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           PERFORM VAL-MKY-DPC.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GFLD.
           PERFORM VAL-FLD-DPC.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GFLD.
           PERFORM VAL-CON-FLD-DPC.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GFLD.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-FLD-DPC.
       ERR-SCR-GFLD.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-DPC.
           PERFORM EDT-FLD-DPC.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GFLD.
           MOVE SCR-STAT-MOD TO SCR-STAT.

       GET-ACC SECTION.
       INI-GET-ACC.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO DPC-GLS-MEN2
               MOVE SCR-PRMT     TO DPC-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO DPC-FLD.
           IF NOT ( ( FRM-FFLD = FRM-FFLD-PF12 OR FRM-FFLD-PF3 ) OR
                      SCR-CONF > SPACES )
               PERFORM VAL-NUM-KEY-DPC
               PERFORM VAL-NUM-FLD-DPC.
       FIN-GET-ACC.
           MOVE SCR-STAT-SON TO SCR-STAT.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO DPC-GLS-MEN2
               MOVE SCR-PRMT     TO DPC-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO DPC-FLD.
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO DPC-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
           IF SCR-DESDE-CMN
               MOVE DPC-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO DPC-FLD
      * FLB  7-NOV-1994
               PERFORM CLR-KEY-DPC

               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO DPC-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO DPC-GLS-MENS.
           PERFORM PRO-FLD-DPC.
           PERFORM PRO-MKY-DPC.
           PERFORM UNP-IKY-DPC.
           PERFORM FST-KEY-DPC.
      *<<<< FIN_HBL_IKY
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.                           
           MOVE '1' TO FRM-CPIM-AEY.                                    
                                                                        
      *>>>>
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
           IF SCR-DESDE-CMN
               MOVE DPC-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO DPC-FLD
               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO DPC-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO DPC-GLS-MENS.
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-FLD-DPC.
           MOVE '1' TO FRM-CPIM-AEY.
           PERFORM UNP-KEY-DPC.
           PERFORM FST-KEY-DPC.
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
           PERFORM UNP-MKY-DPC.
           PERFORM UNP-FLD-DPC.
           PERFORM FST-FLD-DPC.
      *<<<< FIN_HBL_FLD                                                 
                                                                        
      * ----------- SEGUN TIPO DE REGISTRO PROTEJO CAMPOS ----------*   
           IF DPC-TIP-REGI IN DPC-FLD = '1'                             
              MOVE FRM-CPIM-AEY TO                                      
      * Reg.Tipo 2                                                      
              FRM-NUM-FOI2-ATR IN DPC-FLD                               
              FRM-NUM-FOF2-ATR IN DPC-FLD                               
                                                                        
      * Reg.Tipo 3                                                      
              FRM-VAL-SDAN-ATR IN DPC-FLD                               
              FRM-VAL-SHAN-ATR IN DPC-FLD                               
              FRM-VAL-MDPE-ATR IN DPC-FLD                               
              FRM-VAL-MHPE-ATR IN DPC-FLD                               
              FRM-VAL-SDNV-ATR IN DPC-FLD                               
              FRM-VAL-SHNV-ATR IN DPC-FLD                               
                                                                        
      * Reg.Tipo 4                                                      
              FRM-VAL-MACU-ATR IN DPC-FLD                               
              FRM-NUM-RACU-ATR IN DPC-FLD                               
              FRM-VAL-MELI-ATR IN DPC-FLD                               
              FRM-NUM-RELI-ATR IN DPC-FLD                               
              FRM-FEC-FELI-ATR IN DPC-FLD                               
              FRM-FEC-FPR4-ATR IN DPC-FLD                               
                                                                        
      * Reg.Tipo 5                                                      
              FRM-NUM-FOI5-ATR IN DPC-FLD                               
              FRM-NUM-FOF5-ATR IN DPC-FLD                               
                                                                        
           ELSE IF DPC-TIP-REGI IN DPC-FLD = '2'                        
                                                                        
                   MOVE 0  TO FRM-FEC-FPRO-LEN IN DPC-FLD               
                   MOVE -1 TO FRM-NUM-FOI2-LEN IN DPC-FLD               
                                                                        
                   MOVE FRM-CPIM-AEY TO                                 
                                                                        
      * Reg.Tipo 1                                                      
                   FRM-FEC-FPRO-ATR IN DPC-FLD                          
                   FRM-FEC-FINI-ATR IN DPC-FLD                          
                   FRM-FEC-FTER-ATR IN DPC-FLD                          
                                                                        
      * Reg.Tipo 3                                                      
                   FRM-VAL-SDAN-ATR IN DPC-FLD                          
                   FRM-VAL-SHAN-ATR IN DPC-FLD                          
                   FRM-VAL-MDPE-ATR IN DPC-FLD                          
                   FRM-VAL-MHPE-ATR IN DPC-FLD                          
                   FRM-VAL-SDNV-ATR IN DPC-FLD                          
                   FRM-VAL-SHNV-ATR IN DPC-FLD                          
                                                                        
      * Reg.Tipo 4                                                      
                   FRM-VAL-MACU-ATR IN DPC-FLD                          
                   FRM-NUM-RACU-ATR IN DPC-FLD                          
                   FRM-VAL-MELI-ATR IN DPC-FLD                          
                   FRM-NUM-RELI-ATR IN DPC-FLD                          
                   FRM-FEC-FELI-ATR IN DPC-FLD                          
                   FRM-FEC-FPR4-ATR IN DPC-FLD                          
                                                                        
      * Reg.Tipo 5                                                      
                   FRM-NUM-FOI5-ATR IN DPC-FLD                          
                   FRM-NUM-FOF5-ATR IN DPC-FLD                          
                                                                        
                ELSE IF DPC-TIP-REGI IN DPC-FLD = '3'                   
                                                                        
                        MOVE 0  TO FRM-FEC-FPRO-LEN IN DPC-FLD          
                        MOVE -1 TO FRM-VAL-SDAN-LEN IN DPC-FLD          
                                                                        
                        MOVE FRM-CPIM-AEY TO                            
      * Reg.Tipo 1                                                      
                        FRM-FEC-FPRO-ATR IN DPC-FLD                     
                        FRM-FEC-FINI-ATR IN DPC-FLD                     
                        FRM-FEC-FTER-ATR IN DPC-FLD                     
                                                                        
      * Reg.Tipo 2                                                      
                        FRM-NUM-FOI2-ATR IN DPC-FLD                     
                        FRM-NUM-FOF2-ATR IN DPC-FLD                     
                                                                        
      * Reg.Tipo 4                                                      
                        FRM-VAL-MACU-ATR IN DPC-FLD                     
                        FRM-NUM-RACU-ATR IN DPC-FLD                     
                        FRM-VAL-MELI-ATR IN DPC-FLD                     
                        FRM-NUM-RELI-ATR IN DPC-FLD                     
                        FRM-FEC-FELI-ATR IN DPC-FLD                     
                        FRM-FEC-FPR4-ATR IN DPC-FLD                     
                                                                        
      * Reg.Tipo 5                                                      
                        FRM-NUM-FOI5-ATR IN DPC-FLD                     
                        FRM-NUM-FOF5-ATR IN DPC-FLD                     
                                                                        
                     ELSE IF DPC-TIP-REGI IN DPC-FLD = '4'              
                                                                        
                             MOVE 0  TO FRM-FEC-FPRO-LEN IN DPC-FLD     
                             MOVE -1 TO FRM-VAL-MACU-LEN IN DPC-FLD     
                                                                        
                             MOVE FRM-CPIM-AEY TO                       
      * Reg.Tipo 1                                                      
                             FRM-FEC-FPRO-ATR IN DPC-FLD                
                             FRM-FEC-FINI-ATR IN DPC-FLD                
                             FRM-FEC-FTER-ATR IN DPC-FLD                
                                                                        
      * Reg.Tipo 2                                                      
                             FRM-NUM-FOI2-ATR IN DPC-FLD                
                             FRM-NUM-FOF2-ATR IN DPC-FLD                
                                                                        
      * Reg.Tipo 3                                                      
                             FRM-VAL-SDAN-ATR IN DPC-FLD                
                             FRM-VAL-SHAN-ATR IN DPC-FLD                
                             FRM-VAL-MDPE-ATR IN DPC-FLD                
                             FRM-VAL-MHPE-ATR IN DPC-FLD                
                             FRM-VAL-SDNV-ATR IN DPC-FLD                
                             FRM-VAL-SHNV-ATR IN DPC-FLD                
                                                                        
      * Reg.Tipo 5                                                      
                             FRM-NUM-FOI5-ATR IN DPC-FLD                
                             FRM-NUM-FOF5-ATR IN DPC-FLD                
                                                                        
                          ELSE IF DPC-TIP-REGI IN DPC-FLD = '5'         
                                                                        
                                  MOVE 0  TO FRM-FEC-FPRO-LEN IN DPC-FLD
                                  MOVE -1 TO FRM-NUM-FOI5-LEN IN DPC-FLD
                                                                        
                                  MOVE FRM-CPIM-AEY TO                  
                                                                        
      * Reg.Tipo 1                                                      
                                  FRM-FEC-FPRO-ATR IN DPC-FLD           
                                  FRM-FEC-FINI-ATR IN DPC-FLD           
                                  FRM-FEC-FTER-ATR IN DPC-FLD           
                                                                        
      * Reg.Tipo 2                                                      
                                  FRM-NUM-FOI2-ATR IN DPC-FLD           
                                  FRM-NUM-FOF2-ATR IN DPC-FLD           
                                                                        
      * Reg.Tipo 3                                                      
                                  FRM-VAL-SDAN-ATR IN DPC-FLD           
                                  FRM-VAL-SHAN-ATR IN DPC-FLD           
                                  FRM-VAL-MDPE-ATR IN DPC-FLD           
                                  FRM-VAL-MHPE-ATR IN DPC-FLD           
                                  FRM-VAL-SDNV-ATR IN DPC-FLD           
                                  FRM-VAL-SHNV-ATR IN DPC-FLD           
                                                                        
      * Reg.Tipo 4                                                      
                                  FRM-VAL-MACU-ATR IN DPC-FLD           
                                  FRM-NUM-RACU-ATR IN DPC-FLD           
                                  FRM-VAL-MELI-ATR IN DPC-FLD           
                                  FRM-NUM-RELI-ATR IN DPC-FLD           
                                  FRM-FEC-FELI-ATR IN DPC-FLD           
                                  FRM-FEC-FPR4-ATR IN DPC-FLD.          
      *>>>>                                                              
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO DPC-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-DPC.
           PERFORM PRO-FLD-DPC.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-DPC
               PERFORM EDT-FLD-DPC
               PERFORM SCR-PUTA
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-DPC
               PERFORM EDT-FLD-DPC
               PERFORM SCR-PUTA
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-DPC.
           PERFORM EDT-FLD-DPC.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           IF SCR-INST = SCR-INST-GCF
                  PERFORM SCR-GCNF
                  GO TO FIN-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
           IF SCR-STAT = SCR-STAT-ABT OR SCR-STAT-CON
                  GO TO FIN-DESSCRTRL.
           IF SCR-FFLD-RET OR SCR-FFLD-PF10
                  MOVE SCR-VFLD-YES TO SCR-VFLD
                  PERFORM PRO-CONF.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

       PRO-CONF SECTION.
       INI-PRO-CONF.
           IF FRM-MENS NOT > SPACES
               IF SCR-CONF NOT > SPACES
                   GO TO FIN-PRO-CONF
               ELSE
                   MOVE SCR-CONF TO FRM-MENS
           ELSE
               IF FRM-MEN1 NOT > SPACES
                   MOVE SCR-CONF TO FRM-MEN1.
           PERFORM PRO-KEY-DPC.
           PERFORM PRO-FLD-DPC.
           PERFORM FST-MSG.
           PERFORM SCR-MSSG.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM EDT-KEY-DPC
               PERFORM EDT-FLD-DPC.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GCF TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-PRO-CONF.
           EXIT.

       SCR-GCNF SECTION.
       INI-SCR-GCNF.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3  OR
                               FRM-FFLD-PF4 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO DPC-GLS-MEN2
               MOVE SCR-CONF     TO DPC-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO DPC-FLD.
           MOVE FRM-FFLD TO SCR-FFLD.
           IF FRM-FFLD = FRM-FFLD-PF3
               MOVE SCR-VFLD-NOT TO SCR-VFLD
           ELSE
           IF FRM-FFLD = FRM-FFLD-RET OR FRM-FFLD-PF10 OR FRM-FFLD-PF4
               MOVE SCR-VFLD-YES TO SCR-VFLD
               PERFORM VAL-NUM-KEY-DPC
               PERFORM VAL-NUM-FLD-DPC
           ELSE
           IF FRM-FFLD = FRM-FFLD-PF12
               MOVE SCR-STPP TO SCR-STAT
               IF SCR-STAT = SCR-STAT-MOD
                   PERFORM VAL-NUM-KEY-DPC
                   PERFORM VAL-NUM-FLD-DPC
                   PERFORM SCR-TRL
               ELSE
                   MOVE SCR-VFLD-NOT TO SCR-VFLD.
       FIN-SCR-GCNF.
           EXIT.

      * PGM-EBR-ALL SECTION.
      * INI-PGM-EBR-ALL.
      *     MOVE 'GNS' TO FIO-SIST.
      *     MOVE FIO-EBR-ALL TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
      * FIN-PGM-EBR-ALL.
      *     EXIT.

       RET-STT-DPC SECTION.
       INI-RET-STT-DPC.
           IF SCR-CCPP = 'ING'
               MOVE 'I' TO DPC-MSC-TACC IN DPC
               MOVE 'I' TO DPC-MSC-STAT IN DPC
           ELSE
           IF SCR-CCPP = 'MOD'
               MOVE 'M' TO DPC-MSC-TACC IN DPC
               MOVE 'M' TO DPC-MSC-STAT IN DPC.
       FIN-RET-STT-DPC.
           EXIT.

       RET-HDR-DPC SECTION.
       INI-RET-HDR-DPC.
           PERFORM GET-FHOY.
      *    Fecha Transaccion
           MOVE HOY-FHOY TO DPC-FEC-FTRN IN DPC.
      *    Hora Transaccion
           MOVE HOY-HHOY TO DPC-HRA-HRTR IN DPC.
      *    Codigo Origen transaccion
      *<<<< NOT PGM_VMS
           MOVE SCR-OTRN TO DPC-COD-OTRN IN DPC.
      *    Autor Transaccion
           MOVE SCR-USER TO DPC-COD-ATRN IN DPC.
      *>>>>
       FIN-RET-HDR-DPC.
           EXIT.

