                MOVE -1 TO FRM-GLS-TOOC-LEN IN PSM-FLD
                GO TO FIN-VAL-FLD-PSM.
           PERFORM VAL-FRM-GLS-TBAN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-TBAN-LEN IN PSM-FLD
                GO TO FIN-VAL-FLD-PSM.
           PERFORM VAL-FRM-GLS-CLCM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-CLCM-LEN IN PSM-FLD
                GO TO FIN-VAL-FLD-PSM.
           PERFORM VAL-FRM-SGV-MTO1.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-SGV-MTO1-LEN IN PSM-FLD
                GO TO FIN-VAL-FLD-PSM.
           PERFORM VAL-FRM-SGV-MTO2.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-SGV-MTO2-LEN IN PSM-FLD
                GO TO FIN-VAL-FLD-PSM.
           PERFORM VAL-FRM-SGV-MTO3.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-SGV-MTO3-LEN IN PSM-FLD
                GO TO FIN-VAL-FLD-PSM.
           PERFORM VAL-FRM-SGV-MTO4.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-SGV-MTO4-LEN IN PSM-FLD
                GO TO FIN-VAL-FLD-PSM.
           PERFORM VAL-FRM-NUM-NDIA
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 6
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE -1 TO FRM-NUM-NDIA-LEN IN PSM-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-PSM.
           PERFORM VAL-FRM-SGV-INT1
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 6
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE -1 TO FRM-SGV-INT1-LEN IN PSM-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-PSM.
           PERFORM VAL-FRM-SGV-INT2
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 6
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE -1 TO FRM-SGV-INT2-LEN IN PSM-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-PSM.
           PERFORM VAL-FRM-SGV-INT3
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 6
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
