      *
      * COLBUTIM
      * Version 3.0.1 Nov-2005
      * 26-Aug-19 12:49 PM
      *
       SET-TAG-COLBUTIM SECTION.
       INI-SET-TAG-COLBUTIM.
           MOVE
           '<IDNMOD NOM=$COLBUTIM$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$26-Aug-19 12:49 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUTIM.
           EXIT.
           
       RET-ALL-TIM SECTION.
       INI-RET-ALL-TIM.
           PERFORM KEY-ALL-TIM.
           PERFORM RET-TIM-TIM.
       FIN-RET-ALL-TIM.
           EXIT.
       
       RET-TIM-TIM SECTION.
       INI-RET-TIM-TIM.
           MOVE TIM-CIC-CTAB IN TIM-FLD TO TIM-CIC-CTAB IN TIM.
           MOVE TIM-EXT-CTAB IN TIM-FLD TO TIM-EXT-CTAB IN TIM.
           MOVE TIM-GLS-DESC IN TIM-FLD TO TIM-GLS-DESC IN TIM.
           MOVE TIM-GLS-ABRV IN TIM-FLD TO TIM-GLS-ABRV IN TIM.
           MOVE TIM-GLS-DCOR IN TIM-FLD TO TIM-GLS-DCOR IN TIM.
           MOVE TIM-IND-VIGE IN TIM-FLD TO TIM-IND-VIGE IN TIM.
           MOVE TIM-SGV-TIM1 IN TIM-FLD TO TIM-SGV-TIM1 IN TIM.
           MOVE TIM-SGV-TIM2 IN TIM-FLD TO TIM-SGV-TIM2 IN TIM.
       FIN-RET-TIM-TIM.
           EXIT.
       
       KEY-ALL-TIM SECTION.
       INI-KEY-ALL-TIM.
           PERFORM KEY-TIM-TIM.
       FIN-KEY-ALL-TIM.
           EXIT.
       
       KEY-TIM-TIM SECTION.
       INI-KEY-TIM-TIM.
           MOVE TIM-COD-CTAB IN TIM-FLD TO TIM-COD-CTAB IN TIM.
       FIN-KEY-TIM-TIM.
           EXIT.
       
       SCR-CLRA-TIM SECTION.
       INI-SCR-CLRA-TIM.
           PERFORM CLR-KEY-TIM.
           PERFORM CLR-FLD-TIM.
       FIN-SCR-CLRA-TIM.
           EXIT.
       
       CLR-KEY-TIM SECTION.
       INI-CLR-KEY-TIM.
           MOVE SPACES TO TIM-COD-CTAB IN TIM-FLD.
       FIN-CLR-KEY-TIM.
           EXIT.
       
       CLR-FLD-TIM SECTION.
       INI-CLR-FLD-TIM.
           PERFORM CLR-TIM-TIM.
       FIN-CLR-FLD-TIM.
           EXIT.
       
       CLR-TIM-TIM SECTION.
       INI-CLR-TIM-TIM.
           MOVE SPACES TO TIM-CIC-CTAB IN TIM-FLD.
           MOVE SPACES TO TIM-EXT-CTAB IN TIM-FLD.
           MOVE SPACES TO TIM-GLS-DESC IN TIM-FLD.
           MOVE SPACES TO TIM-GLS-ABRV IN TIM-FLD.
           MOVE SPACES TO TIM-GLS-DCOR IN TIM-FLD.
           MOVE SPACES TO TIM-IND-VIGE IN TIM-FLD.
           MOVE ZEROES TO TIM-SGV-TIM1 IN TIM-FLD.
           MOVE ZEROES TO TIM-SGV-TIM2 IN TIM-FLD.
       FIN-CLR-TIM-TIM.
           EXIT.
       
       FST-KEY-TIM SECTION.
       INI-FST-KEY-TIM.
           MOVE -1 TO TIM-COD-CTAB-LEN IN TIM-FLD.
       FIN-FST-KEY-TIM.
           EXIT.
       
       FST-FLD-TIM SECTION.
       INI-FST-FLD-TIM.
           MOVE -1 TO TIM-CIC-CTAB-LEN IN TIM-FLD.
       FIN-FST-FLD-TIM.
           EXIT.
       
       PRO-KEY-TIM SECTION.
       INI-PRO-KEY-TIM.
           PERFORM PRO-MKY-TIM.
       FIN-PRO-KEY-TIM.
           EXIT.
       
       PRO-MKY-TIM SECTION.
       INI-PRO-MKY-TIM.
       FIN-PRO-MKY-TIM.
           EXIT.
       
       PRO-FLD-TIM SECTION.
       INI-PRO-FLD-TIM.
           PERFORM PRO-TIM-TIM.
       FIN-PRO-FLD-TIM.
           EXIT.
       
       PRO-TIM-TIM SECTION.
       INI-PRO-TIM-TIM.
           MOVE FRM-CPIM-AEY TO TIM-CIC-CTAB-ATR IN TIM-FLD.
           MOVE FRM-CPIM-AEY TO TIM-EXT-CTAB-ATR IN TIM-FLD.
           MOVE FRM-CPIM-AEY TO TIM-GLS-DESC-ATR IN TIM-FLD.
           MOVE FRM-CPIM-AEY TO TIM-GLS-ABRV-ATR IN TIM-FLD.
           MOVE FRM-CPIM-AEY TO TIM-GLS-DCOR-ATR IN TIM-FLD.
           MOVE FRM-CPIM-AEY TO TIM-IND-VIGE-ATR IN TIM-FLD.
           MOVE FRM-CPIM-AEY TO TIM-SGV-TIM1-ATR IN TIM-FLD.
           MOVE FRM-CPIM-AEY TO TIM-SGV-TIM2-ATR IN TIM-FLD.
       FIN-PRO-TIM-TIM.
           EXIT.
       
       UNP-KEY-TIM SECTION.
       INI-UNP-KEY-TIM.
           PERFORM UNP-IKY-TIM.
           PERFORM UNP-AKY-TIM.
           PERFORM UNP-MKY-TIM.
       FIN-UNP-KEY-TIM.
           EXIT.
       
       UNP-IKY-TIM SECTION.
       INI-UNP-IKY-TIM.
           MOVE FRM-CPIM-UBY TO TIM-COD-CTAB-ATR.
       FIN-UNP-IKY-TIM.
           EXIT.
       
       UNP-MKY-TIM SECTION.
       INI-UNP-MKY-TIM.
       FIN-UNP-MKY-TIM.
           EXIT.
       
       UNP-AKY-TIM SECTION.
       INI-UNP-AKY-TIM.
       FIN-UNP-AKY-TIM.
           EXIT.
       
       UNP-FLD-TIM SECTION.
       INI-UNP-FLD-TIM.
           PERFORM UNP-TIM-TIM.
       FIN-UNP-FLD-TIM.
           EXIT.
       
       UNP-TIM-TIM SECTION.
       INI-UNP-TIM-TIM.
           MOVE FRM-CPIM-UBY TO TIM-CIC-CTAB-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-EXT-CTAB-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-GLS-DESC-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-GLS-ABRV-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-GLS-DCOR-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-IND-VIGE-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-SGV-TIM1-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-SGV-TIM2-ATR IN TIM-FLD.
       FIN-UNP-TIM-TIM.
           EXIT.
       
       VAL-MKY-TIM SECTION.
       INI-VAL-MKY-TIM.
       FIN-VAL-MKY-TIM.
           EXIT.
       
       VAL-FLD-TIM SECTION.
       INI-VAL-FLD-TIM.
           PERFORM VAL-TIM-CIC-CTAB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TIM-CIC-CTAB-LEN IN TIM-FLD
                GO TO FIN-VAL-FLD-TIM.
           PERFORM VAL-TIM-EXT-CTAB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TIM-EXT-CTAB-LEN IN TIM-FLD
                GO TO FIN-VAL-FLD-TIM.
           PERFORM VAL-TIM-GLS-DESC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TIM-GLS-DESC-LEN IN TIM-FLD
                GO TO FIN-VAL-FLD-TIM.
           PERFORM VAL-TIM-GLS-ABRV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TIM-GLS-ABRV-LEN IN TIM-FLD
                GO TO FIN-VAL-FLD-TIM.
           PERFORM VAL-TIM-GLS-DCOR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TIM-GLS-DCOR-LEN IN TIM-FLD
                GO TO FIN-VAL-FLD-TIM.
           PERFORM VAL-TIM-IND-VIGE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TIM-IND-VIGE-LEN IN TIM-FLD
                GO TO FIN-VAL-FLD-TIM.
           PERFORM VAL-TIM-SGV-TIM1.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TIM-SGV-TIM1-LEN IN TIM-FLD
                GO TO FIN-VAL-FLD-TIM.
           PERFORM VAL-TIM-SGV-TIM2.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TIM-SGV-TIM2-LEN IN TIM-FLD
                GO TO FIN-VAL-FLD-TIM.
       FIN-VAL-FLD-TIM.
           EXIT.
       
       VAL-TIM-CIC-CTAB SECTION.
       INI-VAL-TIM-CIC-CTAB.
           IF TIM-CIC-CTAB IN TIM-FLD NOT > SPACES
              MOVE SPACES TO TIM-CIC-CTAB IN TIM-FLD.
      *>>>> TIM_CIC_CTAB
           IF TIM-CIC-CTAB IN TIM-FLD NOT > SPACES
              MOVE TIM-COD-CTAB IN TIM-FLD TO TIM-CIC-CTAB IN TIM-FLD.
      *<<<<
       FIN-VAL-TIM-CIC-CTAB.
           EXIT.
       
       VAL-TIM-EXT-CTAB SECTION.
       INI-VAL-TIM-EXT-CTAB.
           IF TIM-EXT-CTAB IN TIM-FLD NOT > SPACES
              MOVE SPACES TO TIM-EXT-CTAB IN TIM-FLD.
       FIN-VAL-TIM-EXT-CTAB.
           EXIT.
       
       VAL-TIM-GLS-DESC SECTION.
       INI-VAL-TIM-GLS-DESC.
           IF TIM-GLS-DESC IN TIM-FLD NOT > SPACES
              MOVE SPACES TO TIM-GLS-DESC IN TIM-FLD.
       FIN-VAL-TIM-GLS-DESC.
           EXIT.
       
       VAL-TIM-GLS-ABRV SECTION.
       INI-VAL-TIM-GLS-ABRV.
           IF TIM-GLS-ABRV IN TIM-FLD NOT > SPACES
              MOVE SPACES TO TIM-GLS-ABRV IN TIM-FLD.
       FIN-VAL-TIM-GLS-ABRV.
           EXIT.
       
       VAL-TIM-GLS-DCOR SECTION.
       INI-VAL-TIM-GLS-DCOR.
           IF TIM-GLS-DCOR IN TIM-FLD NOT > SPACES
              MOVE SPACES TO TIM-GLS-DCOR IN TIM-FLD.
       FIN-VAL-TIM-GLS-DCOR.
           EXIT.
       
       VAL-TIM-IND-VIGE SECTION.
       INI-VAL-TIM-IND-VIGE.
           IF TIM-IND-VIGE IN TIM-FLD NOT > SPACES
              MOVE SPACES TO TIM-IND-VIGE IN TIM-FLD.
      *<<<< TIM_IND_VIGE
           MOVE 'GNS' TO MSC-COD-SIST.
           MOVE 'VIGE' TO MSC-COD-TMSC IN MSC.
           MOVE TIM-IND-VIGE IN TIM-FLD TO MSC-COD-CMSC IN MSC.
           PERFORM VAL-IND.
      *<<<<
       FIN-VAL-TIM-IND-VIGE.
           EXIT.
       
       VAL-TIM-SGV-TIM1 SECTION.
       INI-VAL-TIM-SGV-TIM1.
       FIN-VAL-TIM-SGV-TIM1.
           EXIT.
       
       VAL-TIM-SGV-TIM2 SECTION.
       INI-VAL-TIM-SGV-TIM2.
       FIN-VAL-TIM-SGV-TIM2.
           EXIT.
       
       VAL-CON-FLD-TIM SECTION.
       INI-VAL-CON-FLD-TIM.
       FIN-VAL-CON-FLD-TIM.
           EXIT.
       
       VAL-CON-TIM SECTION.
       INI-VAL-CON-TIM.
       FIN-VAL-CON-TIM.
           EXIT.
       
       VAL-NUM-KEY-TIM SECTION.
       INI-VAL-NUM-KEY-TIM.
           PERFORM VAL-NUM-IKY-TIM.
           PERFORM VAL-NUM-MKY-TIM.
           PERFORM VAL-NUM-AKY-TIM.
       FIN-VAL-NUM-KEY-TIM.
           EXIT.
       
       VAL-NUM-IKY-TIM SECTION.
       INI-VAL-NUM-IKY-TIM.
       FIN-VAL-NUM-IKY-TIM.
           EXIT.
       
       VAL-NUM-MKY-TIM SECTION.
       INI-VAL-NUM-MKY-TIM.
       FIN-VAL-NUM-MKY-TIM.
           EXIT.
       
       VAL-NUM-AKY-TIM SECTION.
       INI-VAL-NUM-AKY-TIM.
       FIN-VAL-NUM-AKY-TIM.
           EXIT.
       
       VAL-NUM-FLD-TIM SECTION.
       INI-VAL-NUM-FLD-TIM.
       VAL-NUM-TIM-SGV-TIM1.
           MOVE TIM-SGV-TIM1-ALF IN TIM-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO TIM-SGV-TIM1 IN TIM-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO TIM-SGV-TIM1-LEN IN TIM-FLD
                GO TO FIN-VAL-NUM-FLD-TIM.
       VAL-NUM-TIM-SGV-TIM2.
           MOVE TIM-SGV-TIM2-ALF IN TIM-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 7 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO TIM-SGV-TIM2 IN TIM-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO TIM-SGV-TIM2-LEN IN TIM-FLD
                GO TO FIN-VAL-NUM-FLD-TIM.
       FIN-VAL-NUM-FLD-TIM.
           EXIT.
       
       EDT-KEY-TIM SECTION.
       INI-EDT-KEY-TIM.
           PERFORM EDT-IKY-TIM.
           PERFORM EDT-MKY-TIM.
           PERFORM EDT-AKY-TIM.
       FIN-EDT-KEY-TIM.
           EXIT.
       
       EDT-IKY-TIM SECTION.
       INI-EDT-IKY-TIM.
       FIN-EDT-IKY-TIM.
           EXIT.
       
       EDT-MKY-TIM SECTION.
       INI-EDT-MKY-TIM.
       FIN-EDT-MKY-TIM.
           EXIT.
       
       EDT-AKY-TIM SECTION.
       INI-EDT-AKY-TIM.
       FIN-EDT-AKY-TIM.
           EXIT.
       
       EDT-FLD-TIM SECTION.
       INI-EDT-FLD-TIM.
           IF TIM-SGV-TIM1-LEN IN TIM-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO TIM-SGV-TIM1-LEN IN TIM-FLD
                GO TO FIN-EDT-FLD-TIM.
           MOVE TIM-SGV-TIM1 IN TIM-FLD TO TIM-SGV-TIM1-EDT IN TIM-EDT.
           MOVE TIM-SGV-TIM1-EDT IN TIM-EDT TO 
                TIM-SGV-TIM1-ALF IN TIM-FLD.
           IF TIM-SGV-TIM2-LEN IN TIM-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO TIM-SGV-TIM2-LEN IN TIM-FLD
                GO TO FIN-EDT-FLD-TIM.
           MOVE TIM-SGV-TIM2 IN TIM-FLD TO TIM-SGV-TIM2-EDT IN TIM-EDT.
           MOVE TIM-SGV-TIM2-EDT IN TIM-EDT TO 
                TIM-SGV-TIM2-ALF IN TIM-FLD.
       FIN-EDT-FLD-TIM.
           EXIT.
