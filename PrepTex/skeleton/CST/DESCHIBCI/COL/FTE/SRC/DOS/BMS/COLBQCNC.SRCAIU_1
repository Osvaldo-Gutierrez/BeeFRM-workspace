      *
      * COLBQCNC
      * Version 3.0.1 Nov-2005
      * 24-May-19 04:44 PM
      *
       SET-TAG-COLBQCNC SECTION.
       INI-SET-TAG-COLBQCNC.
           MOVE
           '<IDNMOD NOM=$COLBQCNC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$24-May-19 04:44 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQCNC.
           EXIT.
           
       PUT-ALL-CNC SECTION.
       INI-PUT-ALL-CNC.
           PERFORM PUT-CNC-CNC.
       FIN-PUT-ALL-CNC.
           EXIT.
       
       PUT-CNC-CNC SECTION.
       INI-PUT-CNC-CNC.
           IF PGM-STAT-CNC-NUL
               GO TO FIN-PUT-CNC-CNC.
           IF NOT PGM-STAT-CNC-OKS
               PERFORM CLR-CNC-CNC
               GO TO FIN-PUT-CNC-CNC.
           MOVE CNC-MSC-TCAN IN CNC TO CNC-MSC-TCAN IN CNC-FLD.
           MOVE CNC-MSC-FCAN IN CNC TO CNC-MSC-FCAN IN CNC-FLD.
           MOVE CNC-MSC-ECAN IN CNC TO CNC-MSC-ECAN IN CNC-FLD.
           MOVE CNC-CIC-CANC IN CNC TO CNC-CIC-CANC IN CNC-FLD.
           MOVE CNC-EXT-CTAB IN CNC TO CNC-EXT-CTAB IN CNC-FLD.
           MOVE CNC-GLS-DESC IN CNC TO CNC-GLS-DESC IN CNC-FLD.
           MOVE CNC-GLS-ABRV IN CNC TO CNC-GLS-ABRV IN CNC-FLD.
           MOVE CNC-GLS-DCOR IN CNC TO CNC-GLS-DCOR IN CNC-FLD.
           MOVE CNC-IND-VIGE IN CNC TO CNC-IND-VIGE IN CNC-FLD.
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CNC-NUM-DTRN IN CNC TO CNC-NUM-DTRN IN CNC-FLD.
           MOVE CNC-NUM-MTRN IN CNC TO CNC-NUM-MTRN IN CNC-FLD.
           MOVE CNC-NUM-STRN IN CNC TO CNC-NUM-STRN IN CNC-FLD.
           MOVE CNC-NUM-ATRN IN CNC TO CNC-NUM-ATRN IN CNC-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
      *BUS-MSC busca glosa de miscelaneo en tabla
           IF CNC-MSC-TCAN IN CNC-FLD > SPACES
               MOVE 'COL' TO MSC-COD-SIST
               MOVE 'TCAN' TO MSC-COD-TMSC IN MSC
               MOVE CNC-MSC-TCAN IN CNC-FLD TO MSC-COD-CMSC IN MSC
               PERFORM BUS-MSC
               MOVE MSC-GLS-DESC IN MSC TO
                    FRM-GLS-TCAN IN CNC-FLD
           ELSE
               MOVE SPACES TO
                    FRM-GLS-TCAN IN CNC-FLD.
      *BUS-MSC busca glosa de miscelaneo en tabla
           IF CNC-MSC-FCAN IN CNC-FLD > SPACES
               MOVE 'COL' TO MSC-COD-SIST
               MOVE 'FCAN' TO MSC-COD-TMSC IN MSC
               MOVE CNC-MSC-FCAN IN CNC-FLD TO MSC-COD-CMSC IN MSC
               PERFORM BUS-MSC
               MOVE MSC-GLS-DESC IN MSC TO
                    FRM-GLS-FCAN IN CNC-FLD
           ELSE
               MOVE SPACES TO
                    FRM-GLS-FCAN IN CNC-FLD.
      *BUS-MSC busca glosa de miscelaneo en tabla
           IF CNC-MSC-ECAN IN CNC-FLD > SPACES
               MOVE 'COL' TO MSC-COD-SIST
               MOVE 'CCAN' TO MSC-COD-TMSC IN MSC
               MOVE CNC-MSC-ECAN IN CNC-FLD TO MSC-COD-CMSC IN MSC
               PERFORM BUS-MSC
               MOVE MSC-GLS-DESC IN MSC TO
                    FRM-GLS-ECAN IN CNC-FLD
           ELSE
               MOVE SPACES TO
                    FRM-GLS-ECAN IN CNC-FLD.
       FIN-PUT-CNC-CNC.
           EXIT.
       
       KEY-ALL-CNC SECTION.
       INI-KEY-ALL-CNC.
           PERFORM KEY-CNC-CNC.
       FIN-KEY-ALL-CNC.
           EXIT.
       
       KEY-CNC-CNC SECTION.
       INI-KEY-CNC-CNC.
           MOVE CNC-MSC-TCAN IN CNC-FLD TO CNC-MSC-TCAN IN CNC.
           MOVE CNC-MSC-FCAN IN CNC-FLD TO CNC-MSC-FCAN IN CNC.
           MOVE CNC-MSC-ECAN IN CNC-FLD TO CNC-MSC-ECAN IN CNC.
       FIN-KEY-CNC-CNC.
           EXIT.
       
       SCR-CLRA-CNC SECTION.
       INI-SCR-CLRA-CNC.
           PERFORM CLR-KEY-CNC.
           PERFORM CLR-FLD-CNC.
       FIN-SCR-CLRA-CNC.
           EXIT.
       
       CLR-KEY-CNC SECTION.
       INI-CLR-KEY-CNC.
           MOVE SPACES TO CNC-MSC-TCAN IN CNC-FLD.
           MOVE SPACES TO CNC-MSC-FCAN IN CNC-FLD.
           MOVE SPACES TO CNC-MSC-ECAN IN CNC-FLD.
       FIN-CLR-KEY-CNC.
           EXIT.
       
       CLR-FLD-CNC SECTION.
       INI-CLR-FLD-CNC.
           MOVE SPACES TO FRM-GLS-TCAN IN CNC-FLD.
           MOVE SPACES TO FRM-GLS-FCAN IN CNC-FLD.
           MOVE SPACES TO FRM-GLS-ECAN IN CNC-FLD.
           PERFORM CLR-CNC-CNC.
       FIN-CLR-FLD-CNC.
           EXIT.
       
       CLR-CNC-CNC SECTION.
       INI-CLR-CNC-CNC.
           MOVE SPACES TO CNC-CIC-CANC IN CNC-FLD.
           MOVE SPACES TO CNC-EXT-CTAB IN CNC-FLD.
           MOVE SPACES TO CNC-GLS-DESC IN CNC-FLD.
           MOVE SPACES TO CNC-GLS-ABRV IN CNC-FLD.
           MOVE SPACES TO CNC-GLS-DCOR IN CNC-FLD.
           MOVE SPACES TO CNC-IND-VIGE IN CNC-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CNC-FEC-FTRN IN CNC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
       FIN-CLR-CNC-CNC.
           EXIT.
       
       FST-KEY-CNC SECTION.
       INI-FST-KEY-CNC.
           MOVE -1 TO CNC-MSC-TCAN-LEN IN CNC-FLD.
       FIN-FST-KEY-CNC.
           EXIT.
       
       FST-FLD-CNC SECTION.
       INI-FST-FLD-CNC.
           MOVE -1 TO CNC-CIC-CANC-LEN IN CNC-FLD.
       FIN-FST-FLD-CNC.
           EXIT.
       
       PRO-KEY-CNC SECTION.
       INI-PRO-KEY-CNC.
           PERFORM PRO-IKY-CNC.
           PERFORM PRO-AKY-CNC.
           PERFORM PRO-MKY-CNC.
       FIN-PRO-KEY-CNC.
           EXIT.
       
       PRO-IKY-CNC SECTION.
       INI-PRO-IKY-CNC.
           MOVE FRM-CPIM-AEY TO CNC-MSC-TCAN-ATR IN CNC-FLD.
           MOVE FRM-CPIM-AEY TO CNC-MSC-FCAN-ATR IN CNC-FLD.
           MOVE FRM-CPIM-AEY TO CNC-MSC-ECAN-ATR IN CNC-FLD.
       FIN-PRO-IKY-CNC.
           EXIT.
       
       PRO-MKY-CNC SECTION.
       INI-PRO-MKY-CNC.
       FIN-PRO-MKY-CNC.
           EXIT.
       
       PRO-AKY-CNC SECTION.
       INI-PRO-AKY-CNC.
       FIN-PRO-AKY-CNC.
           EXIT.
       
       UNP-KEY-CNC SECTION.
       INI-UNP-KEY-CNC.
           PERFORM UNP-IKY-CNC.
           PERFORM UNP-AKY-CNC.
           PERFORM UNP-MKY-CNC.
       FIN-UNP-KEY-CNC.
           EXIT.
       
       UNP-IKY-CNC SECTION.
       INI-UNP-IKY-CNC.
           MOVE FRM-CPIM-UBY TO CNC-MSC-TCAN-ATR.
           MOVE FRM-CPIM-UBY TO CNC-MSC-FCAN-ATR.
           MOVE FRM-CPIM-UBY TO CNC-MSC-ECAN-ATR.
       FIN-UNP-IKY-CNC.
           EXIT.
       
       UNP-MKY-CNC SECTION.
       INI-UNP-MKY-CNC.
       FIN-UNP-MKY-CNC.
           EXIT.
       
       UNP-AKY-CNC SECTION.
       INI-UNP-AKY-CNC.
       FIN-UNP-AKY-CNC.
           EXIT.
       
       UNP-FLD-CNC SECTION.
       INI-UNP-FLD-CNC.
           PERFORM UNP-CNC-CNC.
       FIN-UNP-FLD-CNC.
           EXIT.
       
       UNP-CNC-CNC SECTION.
       INI-UNP-CNC-CNC.
           MOVE FRM-CPIM-UBY TO CNC-CIC-CANC-ATR IN CNC-FLD.
           MOVE FRM-CPIM-UBY TO CNC-EXT-CTAB-ATR IN CNC-FLD.
           MOVE FRM-CPIM-UBY TO CNC-GLS-DESC-ATR IN CNC-FLD.
           MOVE FRM-CPIM-UBY TO CNC-GLS-ABRV-ATR IN CNC-FLD.
           MOVE FRM-CPIM-UBY TO CNC-GLS-DCOR-ATR IN CNC-FLD.
           MOVE FRM-CPIM-UBY TO CNC-IND-VIGE-ATR IN CNC-FLD.
       FIN-UNP-CNC-CNC.
           EXIT.
       
       VAL-KEY-CNC SECTION.
       INI-VAL-KEY-CNC.
           PERFORM VAL-IKY-CNC.
           PERFORM VAL-MKY-CNC.
           PERFORM VAL-AKY-CNC.
       FIN-VAL-KEY-CNC.
           EXIT.
       
       VAL-IKY-CNC SECTION.
       INI-VAL-IKY-CNC.
           PERFORM VAL-CNC-MSC-TCAN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CNC-MSC-TCAN-LEN IN CNC-FLD
                GO TO FIN-VAL-IKY-CNC.
           PERFORM VAL-CNC-MSC-FCAN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CNC-MSC-FCAN-LEN IN CNC-FLD
                GO TO FIN-VAL-IKY-CNC.
           PERFORM VAL-CNC-MSC-ECAN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CNC-MSC-ECAN-LEN IN CNC-FLD
                GO TO FIN-VAL-IKY-CNC.
       FIN-VAL-IKY-CNC.
           EXIT.
       
       VAL-MKY-CNC SECTION.
       INI-VAL-MKY-CNC.
       FIN-VAL-MKY-CNC.
           EXIT.
       
       VAL-AKY-CNC SECTION.
       INI-VAL-AKY-CNC.
       FIN-VAL-AKY-CNC.
           EXIT.
       
       VAL-CNC-MSC-TCAN SECTION.
       INI-VAL-CNC-MSC-TCAN.
           IF CNC-MSC-TCAN IN CNC-FLD NOT > SPACES
              MOVE SPACES TO CNC-MSC-TCAN IN CNC-FLD.
      *VAL-IND Valida miscelaneo de tabla
           MOVE 'COL' TO MSC-COD-SIST.
           MOVE 'TCAN' TO MSC-COD-TMSC IN MSC.
           MOVE CNC-MSC-TCAN IN CNC-FLD TO MSC-COD-CMSC IN MSC.
           PERFORM VAL-IND.
           IF MSG-COD-MENS = 'COD    NEX' OR
              CNC-MSC-TCAN IN CNC-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-TCAN IN CNC-FLD
           ELSE
               MOVE MSC-GLS-DESC IN MSC TO
                    FRM-GLS-TCAN IN CNC-FLD.
       FIN-VAL-CNC-MSC-TCAN.
           EXIT.
       
       VAL-CNC-MSC-FCAN SECTION.
       INI-VAL-CNC-MSC-FCAN.
           IF CNC-MSC-FCAN IN CNC-FLD NOT > SPACES
              MOVE SPACES TO CNC-MSC-FCAN IN CNC-FLD.
      *VAL-IND Valida miscelaneo de tabla
           MOVE 'COL' TO MSC-COD-SIST.
           MOVE 'FCAN' TO MSC-COD-TMSC IN MSC.
           MOVE CNC-MSC-FCAN IN CNC-FLD TO MSC-COD-CMSC IN MSC.
           PERFORM VAL-IND.
           IF MSG-COD-MENS = 'COD    NEX' OR
              CNC-MSC-FCAN IN CNC-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-FCAN IN CNC-FLD
           ELSE
               MOVE MSC-GLS-DESC IN MSC TO
                    FRM-GLS-FCAN IN CNC-FLD.
       FIN-VAL-CNC-MSC-FCAN.
           EXIT.
       
       VAL-CNC-MSC-ECAN SECTION.
       INI-VAL-CNC-MSC-ECAN.
           IF CNC-MSC-ECAN IN CNC-FLD NOT > SPACES
              MOVE SPACES TO CNC-MSC-ECAN IN CNC-FLD.
      *VAL-IND Valida miscelaneo de tabla
           MOVE 'COL' TO MSC-COD-SIST.
           MOVE 'CCAN' TO MSC-COD-TMSC IN MSC.
           MOVE CNC-MSC-ECAN IN CNC-FLD TO MSC-COD-CMSC IN MSC.
           PERFORM VAL-IND.
           IF MSG-COD-MENS = 'COD    NEX' OR
              CNC-MSC-ECAN IN CNC-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-ECAN IN CNC-FLD
           ELSE
               MOVE MSC-GLS-DESC IN MSC TO
                    FRM-GLS-ECAN IN CNC-FLD.
       FIN-VAL-CNC-MSC-ECAN.
           EXIT.
       
       VAL-CON-KEY-CNC SECTION.
       INI-VAL-CON-KEY-CNC.
           IF     CNC-MSC-TCAN IN CNC-FLD NOT > SPACES 
              AND CNC-MSC-FCAN IN CNC-FLD NOT > SPACES 
              AND CNC-MSC-ECAN IN CNC-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CNC.
       FIN-VAL-CON-KEY-CNC.
           EXIT.
       
       VAL-CON-IKY-CNC SECTION.
       INI-VAL-CON-IKY-CNC.
           IF     CNC-MSC-TCAN IN CNC-FLD NOT > SPACES 
              AND CNC-MSC-FCAN IN CNC-FLD NOT > SPACES 
              AND CNC-MSC-ECAN IN CNC-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CNC.
       FIN-VAL-CON-IKY-CNC.
           EXIT.
       
       VAL-NUM-KEY-CNC SECTION.
       INI-VAL-NUM-KEY-CNC.
           PERFORM VAL-NUM-IKY-CNC.
           PERFORM VAL-NUM-MKY-CNC.
           PERFORM VAL-NUM-AKY-CNC.
       FIN-VAL-NUM-KEY-CNC.
           EXIT.
       
       VAL-NUM-IKY-CNC SECTION.
       INI-VAL-NUM-IKY-CNC.
       FIN-VAL-NUM-IKY-CNC.
           EXIT.
       
       VAL-NUM-MKY-CNC SECTION.
       INI-VAL-NUM-MKY-CNC.
       FIN-VAL-NUM-MKY-CNC.
           EXIT.
       
       VAL-NUM-AKY-CNC SECTION.
       INI-VAL-NUM-AKY-CNC.
       FIN-VAL-NUM-AKY-CNC.
           EXIT.
       
       VAL-NUM-FLD-CNC SECTION.
       INI-VAL-NUM-FLD-CNC.
       FIN-VAL-NUM-FLD-CNC.
           EXIT.
       
       EDT-KEY-CNC SECTION.
       INI-EDT-KEY-CNC.
           PERFORM EDT-IKY-CNC.
           PERFORM EDT-MKY-CNC.
           PERFORM EDT-AKY-CNC.
       FIN-EDT-KEY-CNC.
           EXIT.
       
       EDT-IKY-CNC SECTION.
       INI-EDT-IKY-CNC.
       FIN-EDT-IKY-CNC.
           EXIT.
       
       EDT-MKY-CNC SECTION.
       INI-EDT-MKY-CNC.
       FIN-EDT-MKY-CNC.
           EXIT.
       
       EDT-AKY-CNC SECTION.
       INI-EDT-AKY-CNC.
       FIN-EDT-AKY-CNC.
           EXIT.
       
       EDT-FLD-CNC SECTION.
       INI-EDT-FLD-CNC.
       FIN-EDT-FLD-CNC.
           EXIT.
