           MOVE WSS-NUM-MERC IN WSS-ERC        TO RPT-HEAD-NUM-MERC
           MOVE WSS-NUM-SERC IN WSS-ERC        TO RPT-HEAD-NUM-SERC
           MOVE WSS-NUM-AERC IN WSS-ERC        TO RPT-HEAD-NUM-AERC
           MOVE ERC-COD-OFIC IN SRT            TO RPT-HEAD-COD-OFIC
           MOVE OFI-GLS-DESC IN OFI            TO RPT-HEAD-GLS-DESC
           MOVE ERC-COD-EVEN IN SRT            TO RPT-DETA-COD-EVEN
           MOVE ERC-COD-PGRM IN SRT            TO RPT-DETA-COD-PGRM
           MOVE ERC-CAI-IOPC IN SRT            TO RPT-DETA-CAI-IOPC
           MOVE WSS-RAYA-IOPC IN WSS-ERC       TO RPT-DETA-RAYA-IOPC
           MOVE ERC-IIC-IOPC IN SRT            TO RPT-DETA-IIC-IOPC
           MOVE ERC-NUM-CANC IN SRT            TO RPT-DETA-NUM-CANC
           MOVE ERC-VAL-MNTO IN SRT            TO RPT-DETA-VAL-MNTO
           MOVE WSS-GLS-DESC IN WSS-ERC        TO RPT-DETA-GLS-DESC
           MOVE ERC-COD-MSGE IN SRT            TO RPT-CTRL-COD-MSGE

           IF RPT-VARI-LINEA > 59 OR RPT-HEAD-PAGE-COUNTER = 0 OR
              ERC-COD-OFIC IN SRT NOT = RPT-CTRL-COD-OFIC
              IF RPT-CTRL-COD-OFIC NOT = ERC-COD-OFIC IN SRT
                 MOVE ERC-COD-OFIC IN SRT TO RPT-CTRL-COD-OFIC
                 MOVE 0 TO RPT-VARI-PAGE-COUNTER
              END-IF
              ADD 1 TO RPT-VARI-PAGE-COUNTER
              MOVE RPT-VARI-PAGE-COUNTER TO RPT-HEAD-PAGE-COUNTER
              MOVE SPACES TO RPTERC-REG
              WRITE RPTERC-REG AFTER PAGE
              WRITE RPTERC-REG FROM  PH-ERC-01
              WRITE RPTERC-REG FROM  PH-ERC-02
              WRITE RPTERC-REG FROM  PH-ERC-03
              WRITE RPTERC-REG FROM  PH-ERC-04
              WRITE RPTERC-REG FROM  PH-ERC-05
              WRITE RPTERC-REG FROM  PH-ERC-06
              WRITE RPTERC-REG FROM  PH-ERC-07
              MOVE 9 TO RPT-VARI-LINEA
           END-IF.
           WRITE RPTERC-REG FROM  DL-ERC-08
           ADD 1 TO RPT-VARI-LINEA.

