       PROCEDURE DIVISION.
      *===================

       MAIN SECTION.
       INI-MAIN.
           PERFORM INI.
       LUP-MAIN.
           IF SCR-INST = SCR-INST-HCM
                 PERFORM SCR-COM.
           IF SCR-CCPP = 'MOD'
                 PERFORM SCR-MOD
           ELSE
           IF SCR-CCPP = 'ING'
                 PERFORM SCR-ING
           ELSE
           IF SCR-CCPP = 'ACC'
                 PERFORM SCR-ACC
           ELSE
           IF SCR-CCPP = 'ELI'
                 PERFORM SCR-ELI
           ELSE
           IF SCR-CCPP = 'ACT'
                 PERFORM SCR-ACT
           ELSE
           IF SCR-CCPP = 'PGM'
                 PERFORM SCR-PGM
           ELSE
                 MOVE SCR-INST-HCM TO SCR-INST.
           GO TO LUP-MAIN.
       FIN-MAIN.
           EXIT.

       INI SECTION.
       INI-INI.
       COPY GNSBGEIB.
       COPY GNSBGEDB.
           MOVE LOW-VALUES TO QTI-FLD.
           MOVE LOW-VALUES TO QTI-EDT.
           PERFORM GNS-PRO-RCK.
           PERFORM GNS-BUS-VCA.
           MOVE +300 TO SCR-TCMA.
           PERFORM GNS-HDL-SYS.
           PERFORM GNS-HDL-FRM.
           MOVE 'COLPQQTI' TO FIO-PROG.
           PERFORM GNS-BUS-IDD.
      *PTC-INI
           PERFORM GNS-ERR-QUE.
           PERFORM GET-TSK-TERM.
      *PTC-FIN
      *<<<<                                                      
           IF SCR-OFRM-SRV                                              
              MOVE TSK-TERM-ALF TO QTI-COD-TERM                         
              MOVE TSK-TERM-ALF TO QTI-COD-TERM-MAS                     
           ELSE                                                         
              MOVE EIBTRMID     TO QTI-COD-TERM                         
              MOVE EIBTRMID     TO QTI-COD-TERM-MAS.                       
      *>>>>
       FIN-INI.
           EXIT.

       COPY GNSBHSYS.
       COPY GNSBHFRM.
       COPY GNSBPRCK.
       COPY GNSBBVCA.
       COPY GNSBBIDD.

       SCR-COM SECTION.
       INI-SCR-COM.
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-CLRA-QTI.
           PERFORM EDT-KEY-QTI.
           PERFORM EDT-FLD-QTI.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-HCM TO SCR-INST.
           MOVE SCR-CMNP     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       XCT-ERR.
           MOVE 'XCTERR    ' TO MSG-COD-MENS.
           MOVE 'GNS'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO SYS-MEN1.
           MOVE SYS-PROG     TO SYS-MEN2.
           MOVE SYS-ABOR     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       COPY GNSBGGBK.
       FIN-SCR-COM.
           EXIT.

       SCR-MOD SECTION.
       INI-SCR-MOD.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-MOD
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-MOD.
           PERFORM DESSCRTRL.
       MSG-SCR-MOD.
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-MOD.
           EXIT.

       BRW-SCR-MOD SECTION.
       INI-BRW-SCR-MOD.
       FIN-BRW-SCR-MOD.
           EXIT.

       SCR-ING SECTION.
       INI-SCR-ING.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-IKY
               MOVE SCR-INST-GKY TO SCR-INST
      *PTC-INI
               IF NOT SCR-OFRM-SRV
                   PERFORM SCR-DMSG
               ELSE
                   PERFORM SCR-PUTA
      *PTC-FIN
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-RST
                PERFORM GET-IKY
                IF NOT FRM-STAT-ABT
                        MOVE SPACES TO FRM-MENS
                        PERFORM FND-KEY
                        IF NOT FIO-STAT-OKS AND
                           FIO-STAT NOT = FIO-STAT-NEX
      *JSS                  PERFORM CLR-FLD-QTI
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
                            PERFORM SCR-TRL
                        ELSE
      *PTC-INI
                        MOVE 'S' TO SCR-IERR
      *PTC-FIN
                        MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR
                        IF FRM-MENS NOT > SPACES
                            MOVE 'REGEXI    ' TO MSG-COD-MENS
                            MOVE 'GNS' TO MSG-COD-SIST
                            PERFORM GET-MSG
                            MOVE MSG-GLS-DESC TO FRM-MENS
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-ING
                        ELSE
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-ING
                ELSE
                        MOVE SPACES TO FRM-MENS
                        PERFORM SCR-MSSG
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ING
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON.
           PERFORM DESSCRTRL.
       MSG-SCR-ING.
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ING.
           EXIT.

       SCR-ACC SECTION.
       INI-SCR-ACC.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ACC
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-ACC.
           PERFORM DESSCRTRL.
      *<<<< PFS_SCR_ACC                                                 
           IF FRM-FFLD = FRM-FFLD-PF6                                   
                    OR   FRM-FFLD-PF5                                   
               PERFORM VAL-NUM-KEY-QTI                                  
               PERFORM VAL-NUM-FLD-QTI                                  
               PERFORM RST-STAT                                         
               PERFORM FND-SCR-KEY THRU FIN-SCR-KEY                     
               IF SCR-STAT NOT = SCR-STAT-ABT                           
                   PERFORM SCR-TRL                                      
               ELSE                  
                   PERFORM DESSCRTRL                                    
                   GO TO FIN-SCR-ACC.                                   
      *>>>>
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ACC.
           EXIT.

       BRW-SCR-ACC SECTION.
       INI-BRW-SCR-ACC.
       FIN-BRW-SCR-ACC.
           EXIT.

       SCR-ELI SECTION.
       INI-SCR-ELI.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ELI
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-ELI.
           PERFORM DESSCRTRL.
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ELI.
           EXIT.

       BRW-SCR-ELI SECTION.
       INI-BRW-SCR-ELI.
       FIN-BRW-SCR-ELI.
           EXIT.

       SCR-ACT SECTION.
       INI-SCR-ACT.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-RST
                PERFORM GET-KEY
                IF NOT FRM-STAT-ABT
                        MOVE SPACES TO FRM-MENS
                        PERFORM FND-KEY
                        IF FIO-STAT-OKS
                            MOVE SCR-ACTU-MOD TO SCR-ACTU
                            PERFORM PUT-SCR
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
                            PERFORM SCR-TRL
                        ELSE
                        MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR
                        IF FIO-STAT-EOF
                            IF FRM-MENS NOT > SPACES
                                MOVE 'REGEOF    ' TO MSG-COD-MENS
                                MOVE 'GNS' TO MSG-COD-SIST
                                PERFORM GET-MSG
                                MOVE MSG-GLS-DESC TO FRM-MENS
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                            ELSE
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                        ELSE
                        IF FIO-STAT = FIO-STAT-NEX
                            IF FRM-MENS NOT > SPACES
                                MOVE 'REGNEX    ' TO MSG-COD-MENS
                                MOVE 'GNS' TO MSG-COD-SIST
                                PERFORM GET-MSG
                                MOVE MSG-GLS-DESC TO FRM-MENS
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                            ELSE
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                        ELSE
                            MOVE SCR-ACTU-ING TO SCR-ACTU
      *JSS                  PERFORM CLR-FLD-QTI
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
                            PERFORM SCR-TRL
                ELSE
                        MOVE SPACES TO FRM-MENS
                        PERFORM SCR-MSSG
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ACT
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-ACT.
           PERFORM DESSCRTRL.
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ACT.
           EXIT.

       BRW-SCR-ACT SECTION.
       INI-BRW-SCR-ACT.
       FIN-BRW-SCR-ACT.
           EXIT.

       SCR-PGM SECTION.
       INI-SCR-PGM.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-RST
                PERFORM GET-KEY
                IF FRM-STAT-ABT
                    MOVE SPACES TO FRM-MENS
                    PERFORM SCR-MSSG
                    PERFORM DESSCRTRL
                    GO TO FIN-SCR-PGM
                ELSE
                    MOVE SPACES TO FRM-MENS
                    PERFORM FND-KEY
                    IF FIO-STAT = FIO-STAT-NEX
                        IF FRM-MENS NOT > SPACES
                            MOVE 'REGNEX    ' TO MSG-COD-MENS
                            MOVE 'GNS' TO MSG-COD-SIST
                            PERFORM GET-MSG
                            MOVE MSG-GLS-DESC TO FRM-MENS
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-PGM
                        ELSE
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-PGM
                    ELSE
                        MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
                        PERFORM SCR-TRL
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-PGM.
       TRL-SCR-PGM.
           PERFORM DESSCRTRL.
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-PGM.
           EXIT.

       BRW-SCR-PGM SECTION.
       INI-BRW-SCR-PGM.
       FIN-BRW-SCR-PGM.
           EXIT.

       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               PERFORM SCR-CLRA-QTI
               PERFORM EDT-KEY-QTI
               PERFORM EDT-FLD-QTI
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-QTI
               PERFORM EDT-KEY-QTI
           ELSE
               PERFORM CLR-FLD-QTI
               PERFORM EDT-FLD-QTI.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

       SCR-KEY SECTION.
       INI-SCR-KEY.
           PERFORM SCR-RST.
           PERFORM GET-KEY.
           IF FRM-STAT-ABT
                   MOVE SPACES TO FRM-MENS
                   PERFORM SCR-MSSG
                   GO TO FIN-SCR-KEY.
       FND-SCR-KEY.
           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
           IF FIO-STAT-OKS
               PERFORM PUT-SCR
               MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
               GO TO FIN-SCR-KEY.
      *JSS ELSE
      *        PERFORM CLR-FLD-QTI.
       ERR-SCR-KEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR.
           IF FRM-MENS NOT > SPACES
               IF FIO-STAT-EOF
                   MOVE 'REGEOF    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
               IF FIO-STAT = FIO-STAT-NEX
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
           MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-SCR-KEY.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO QTI-FLD.
           MOVE LOW-VALUES TO QTI-EDT.
       FIN-SCR-RST.
           EXIT.

       GET-KEY SECTION.
       INI-GET-KEY.
           PERFORM SCR-GKEY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-KEY.
           EXIT.

       GET-IKY SECTION.
       INI-GET-IKY.
           PERFORM SCR-GIKY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-IKY.
           EXIT.

       FND-KEY SECTION.
       INI-FND-KEY.
      *<<<< INI_FND_KEY    
           PERFORM KEY-ALL-QTI.                                                 
           MOVE IPT-CAI-IOPC IN QTI-FLD TO RDC-CAI-IOPC IN RDC.                 
           MOVE IPT-IIC-IOPC IN QTI-FLD TO RDC-IIC-IOPC IN RDC.                 
           MOVE ZEROES              TO RDC-NUM-IDLC IN RDC.                     
           MOVE 'RDC-KEY-IDLC' TO FIO-AKEY.                                     
           MOVE FIO-GET-NLS TO FIO-CMND.                                        
           PERFORM COL-FIO-RDC.                                                 
           IF NOT (FIO-STAT-OKS AND                                             
                IPT-CAI-IOPC IN QTI-FLD = RDC-CAI-IOPC IN RDC AND               
                IPT-IIC-IOPC IN QTI-FLD = RDC-IIC-IOPC IN RDC)                  
                MOVE ZEROES              TO DBC-NUM-ICLI IN QTI-FLD     
                MOVE SPACES              TO DBC-VRF-ICLI IN QTI-FLD     
                MOVE SPACES              TO DBC-GLS-NOMC IN QTI-FLD     
                MOVE '00'     TO PGM-STAT-DBC                           
                GO TO SIG-FND-KEY.                                  
                                                                                
           MOVE RDC-CIC-ICLI IN RDC TO DBC-CIC-ICLI IN DBC.                     
           MOVE FIO-GET-KEY TO FIO-CMND.                                        
           PERFORM SGC-FIO-DBC.                                                 
           IF NOT FIO-STAT-OKS                                
                MOVE ZEROES              TO DBC-NUM-ICLI IN QTI-FLD     
                MOVE SPACES              TO DBC-VRF-ICLI IN QTI-FLD     
                MOVE SPACES              TO DBC-GLS-NOMC IN QTI-FLD     
                MOVE '00'     TO PGM-STAT-DBC                       
      *         GO TO FIN-FND-KEY            
           ELSE                                                         
                MOVE FIO-STAT TO PGM-STAT-DBC                           
                MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN QTI-FLD     
                MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN QTI-FLD     
                MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN                    
                PERFORM CPT-BLKS                                        
                MOVE CPT-STRN TO DBC-GLS-NOMC IN DBC                    
                MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN QTI-FLD.      
                                  
       SIG-FND-KEY.                                                     
           MOVE SPACES TO FRM-MEN1.                                
           IF FRM-FFLD = FRM-FFLD-PF5                              
              MOVE PGM-SNUL TO PGM-STAT-IPT                      
              SUBTRACT 1 FROM SCR-I GIVING QTI-I                  
              IF QTI-I NOT > ZEROES                               
                 MOVE 'QTI IPTPRV' TO MSG-COD-MENS              
                 GO TO ERR-FND-KEY                               
              ELSE                                                
                 GO TO CARGA-QTI                                 
           ELSE                                                    
           IF FRM-FFLD = FRM-FFLD-PF6                              
              MOVE PGM-SNUL TO PGM-STAT-IPT         
              ADD 1 SCR-I GIVING QTI-I                            
              IF QTI-I > SCR-IMAX                                 
                MOVE 'QTI IPTNXT' TO MSG-COD-MENS              
                GO TO ERR-FND-KEY                               
              ELSE 
                GO TO CARGA-QTI.                                
           GO TO CON-FND-KEY.                                      
      *>>>>                 
           PERFORM KEY-ALL-QTI.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-IPT.
           IF FIO-STAT-OKS
               MOVE IPT-STP-ITRN IN IPT TO SCR-UMOD.
           MOVE FIO-STAT TO PGM-STAT-IPT.
      *<<<< FIN_FND_KEY                                            
           IF NOT FIO-STAT-OKS                                     
               GO TO FIN-FND-KEY.                                  
       CON-FND-KEY.                         

      *    PERFORM KEY-ALL-QTI.                                                 
      *    MOVE IPT-CAI-IOPC IN QTI-FLD TO RDC-CAI-IOPC IN RDC.                 
      *    MOVE IPT-IIC-IOPC IN QTI-FLD TO RDC-IIC-IOPC IN RDC.                 
      *    MOVE ZEROES              TO RDC-NUM-IDLC IN RDC.                     
      *    MOVE 'RDC-KEY-IDLC' TO FIO-AKEY.                                     
      *    MOVE FIO-GET-NLS TO FIO-CMND.                                        
      *    PERFORM COL-FIO-RDC.                                                 
      *    IF NOT (FIO-STAT-OKS AND                                             
      *         IPT-CAI-IOPC IN QTI-FLD = RDC-CAI-IOPC IN RDC AND               
      *         IPT-IIC-IOPC IN QTI-FLD = RDC-IIC-IOPC IN RDC)                  
      *         GO TO FIN-FND-KEY.                                              
      *                                                                         
      *    MOVE RDC-CIC-ICLI IN RDC TO DBC-CIC-ICLI IN DBC.                     
      *    MOVE FIO-GET-KEY TO FIO-CMND.                                        
      *    PERFORM SGC-FIO-DBC.                                                 
      *    IF NOT FIO-STAT-OKS                                                  
      *         GO TO FIN-FND-KEY.                                              
                                                                                
      *    MOVE FIO-STAT TO PGM-STAT-DBC.                                       
      *    MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN QTI-FLD.                 
      *    MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN QTI-FLD.                 
      *    MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.                                
      *    PERFORM CPT-BLKS.                                                    
      *    MOVE CPT-STRN TO DBC-GLS-NOMC IN DBC.                                
      *    MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN QTI-FLD.                 
                                                                         
           MOVE ZEROES TO SCR-IMAX.                                
           MOVE 1 TO SCR-I.                                        
           MOVE '00' TO FIO-STAT.                                  
           PERFORM OBT-DAT-TS.                                     
           MOVE 1 TO SCR-I.                                        
           MOVE SCR-I TO QTI-I.                                    
       CARGA-QTI.                                                  
           MOVE DBC-NUM-ICLI IN QTI-FLD TO DBC-NUM-ICLI IN DBC.         
           MOVE DBC-IND-ICLI IN QTI-FLD TO DBC-IND-ICLI IN DBC.         
           MOVE DBC-GLS-ICLI IN QTI-FLD TO DBC-GLS-ICLI IN DBC.         
           MOVE 'DBC-IDC-ICLI' TO FIO-AKEY.                             
           MOVE FIO-GET-KEY  TO FIO-CMND.                               
           PERFORM SGC-FIO-DBC                                          
           IF NOT FIO-STAT-OKS                                          
               MOVE SPACES              TO DBC-GLS-NOMC IN QTI-FLD      
               MOVE '00'      TO PGM-STAT-DBC                           
           ELSE                                                         
               MOVE FIO-STAT TO PGM-STAT-DBC                            
               MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN                     
               PERFORM CPT-BLKS                                         
               MOVE CPT-STRN TO DBC-GLS-NOMC IN DBC                     
               MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN QTI-FLD.     
   

           MOVE SPACES TO MSG-COD-MENS.                            
           IF NOT SCR-OFRM-SRV                                     
             PERFORM PUT-TAB-QTI.                                    
             MOVE '00' TO FIO-STAT.                                  
             GO TO FIN-FND-KEY.                                      
       ERR-FND-KEY.                                                
           MOVE SCR-STAT-CON TO SCR-STAT.                          
           MOVE 'COL'  TO MSG-COD-SIST.                            
           PERFORM GET-MSG.                                        
           MOVE MSG-GLS-DESC TO FRM-MEN1.                          
           MOVE '00' TO FIO-STAT.                                  
           GO TO FIN-FND-KEY.                                      
      *>>>>                                                        
       FIN-FND-KEY.
           IF FIO-STAT = FIO-STAT-NEX
               MOVE 'S' TO SCR-IERR.
       EXT-FND-KEY.
           EXIT.
 
       PUT-SCR SECTION.
       INI-PUT-SCR.
           IF PGM-STAT-IPT-OKS
               MOVE IPT-STP-ITRN IN IPT TO SCR-UMOD.
           PERFORM PUT-ALL-QTI.
       FIN-PUT-SCR.
           EXIT.

       STP-EXIT SECTION.
       INI-STP-EXIT.
           MOVE SCR-PROG TO PGM-PROG.
           MOVE SCR-NTRN TO PGM-NTRN.

           MOVE SCR-PROV TO SCR-PROG.
           MOVE SCR-NTRV TO SCR-NTRN.

           MOVE PGM-PROG TO SCR-PROV
           MOVE PGM-NTRN TO SCR-NTRV.
       FIN-STP-EXIT.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO SYS-NTRN.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
      *PTC-INI
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE QTI-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO QTI-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GKEY SECTION.
       INI-SCR-GKEY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-QTI
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO QTI-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GKEY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-QTI.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-QTI
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-QTI.
           PERFORM VAL-IKY-QTI.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-MKY-QTI.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-AKY-QTI.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-CON-KEY-QTI.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GKEY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-QTI.
       ERR-SCR-GKEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-QTI.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GKEY.
           PERFORM SCR-MSSG.

       SCR-GIKY SECTION.
       INI-SCR-GIKY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-QTI
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO QTI-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GIKY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-QTI.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-IKY-QTI
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-QTI.
           PERFORM VAL-IKY-QTI.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GIKY.
           PERFORM VAL-CON-IKY-QTI.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GIKY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-QTI.
       ERR-SCR-GIKY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-IKY-QTI.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GIKY.
           PERFORM SCR-MSSG.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
      *<<<< PFS_GET_CON OR PFS_GET_ACC                                  
           IF NOT (FRM-FFLD = FRM-FFLD-PF6                              
                         OR   FRM-FFLD-PF5)                             
      *>>>>
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO QTI-GLS-MEN2
               MOVE SCR-PRMT     TO QTI-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO QTI-FLD.
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO QTI-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE QTI-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO QTI-FLD
      * FLB  7-NOV-1994
               PERFORM CLR-KEY-QTI

               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO QTI-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO QTI-GLS-MENS.
           PERFORM PRO-MKY-QTI.
           PERFORM PRO-AKY-QTI.
           PERFORM UNP-IKY-QTI.
           PERFORM FST-KEY-QTI.
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE QTI-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO QTI-FLD
               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO QTI-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO QTI-GLS-MENS.
           PERFORM UNP-KEY-QTI.
           PERFORM FST-KEY-QTI.
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
           PERFORM PRO-IKY-QTI.
           PERFORM PRO-AKY-QTI.
           PERFORM UNP-MKY-QTI.
           PERFORM UNP-FLD-QTI.
           PERFORM FST-FLD-QTI.
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO QTI-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-QTI.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
      *PTC-INI
           IF SCR-STAT NOT = SCR-STPP
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-QTI
               PERFORM EDT-FLD-QTI
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-QTI
               PERFORM EDT-FLD-QTI
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-QTI.
           PERFORM EDT-FLD-QTI.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

      * PGM-EBR-ALL SECTION.
      * INI-PGM-EBR-ALL.
      *     MOVE 'GNS' TO FIO-SIST.
      *     MOVE FIO-EBR-ALL TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
      * FIN-PGM-EBR-ALL.
      *     EXIT.

       COPY COLBQQTI.
       COPY GNSBVNUM.
       COPY COLBFIPT.
       COPY GNSBFMSG.
       COPY GNSBGSYS.
       COPY GNSBGDTC.
      *PTC-INI
      *PTC-SUB
      * ISP  10-MAY-2001 13:46:35 
      * COPY GNSBGFRM.
       COPY GNSBG3CF.
      *PTC-FIN
       COPY GNSBGMSG.
       COPY GNSBEKEY.
       COPY GNSBIABT.
       COPY GNSBBTAB.
       COPY GNSBVCOD.
       COPY GNSBVTAB.
       COPY GNSBFTAB.
       COPY GNSBVIND.
       COPY GNSBVMSC.
       COPY GNSBFMSC.
       COPY GNSBVIDC.
       COPY GNSBPVRF.
       COPY GNSBEGLS.
       COPY GNSBGFEC.
       COPY GNSBGHOY.
      *PTC-INI
       COPY GNSBGQUE.
       COPY GNSBGTSK.
      *PTC-FIN

      *<<<<
       CAR-MOV-IPT SECTION.                                             
       INI-CAR-MOV-IPT.
      *<<<< 
           PERFORM KEY-ALL-QTI.                                                 
           MOVE IPT-CAI-IOPC IN QTI-FLD TO RDC-CAI-IOPC IN RDC.                 
           MOVE IPT-IIC-IOPC IN QTI-FLD TO RDC-IIC-IOPC IN RDC.                 
           MOVE ZEROES              TO RDC-NUM-IDLC IN RDC.                     
           MOVE 'RDC-KEY-IDLC' TO FIO-AKEY.                                     
           MOVE FIO-GET-NLS TO FIO-CMND.                                        
           PERFORM COL-FIO-RDC.                                                 
           IF NOT (FIO-STAT-OKS AND                                             
                IPT-CAI-IOPC IN QTI-FLD = RDC-CAI-IOPC IN RDC AND               
                IPT-IIC-IOPC IN QTI-FLD = RDC-IIC-IOPC IN RDC)                  
                MOVE ZEROES              TO DBC-NUM-ICLI IN QTI-FLD      
                MOVE SPACES              TO DBC-VRF-ICLI IN QTI-FLD      
                MOVE SPACES              TO DBC-GLS-NOMC IN QTI-FLD      
                GO TO DBC-CAR-MOV-IPT.                                  
                                                                      
           MOVE RDC-CIC-ICLI IN RDC TO DBC-CIC-ICLI IN DBC.                     
           MOVE FIO-GET-KEY TO FIO-CMND.                                        
           PERFORM SGC-FIO-DBC.                                                 
           IF NOT FIO-STAT-OKS                                                  
               MOVE ZEROES              TO DBC-NUM-ICLI IN QTI-FLD      
               MOVE SPACES              TO DBC-VRF-ICLI IN QTI-FLD      
               MOVE SPACES              TO DBC-GLS-NOMC IN QTI-FLD   
      *         GO TO FIN-CAR-MOV-IPT.  
           ELSE                                                         
               MOVE FIO-STAT TO PGM-STAT-DBC                            
               MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN QTI-FLD      
               MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN QTI-FLD      
               MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN                     
               PERFORM CPT-BLKS                                         
               MOVE CPT-STRN TO DBC-GLS-NOMC IN DBC                     
               MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN QTI-FLD.     

      *>>>>
       DBC-CAR-MOV-IPT.

           MOVE IPT-NUM-SINI IN QTI-FLD TO WSS-NUM-SINI IN WSS-VARI.            
           MOVE IPT-NUM-AINI IN QTI-FLD TO WSS-NUM-AINI IN WSS-VARI.            
           MOVE IPT-NUM-MINI IN QTI-FLD TO WSS-NUM-MINI IN WSS-VARI.            
           MOVE IPT-NUM-DINI IN QTI-FLD TO WSS-NUM-DINI IN WSS-VARI.            
           MOVE IPT-NUM-DTER IN QTI-FLD TO WSS-NUM-DTER IN WSS-VARI.            
           MOVE IPT-NUM-MTER IN QTI-FLD TO WSS-NUM-MTER IN WSS-VARI.            
           MOVE IPT-NUM-STER IN QTI-FLD TO WSS-NUM-STER IN WSS-VARI.            
           MOVE IPT-NUM-ATER IN QTI-FLD TO WSS-NUM-ATER IN WSS-VARI.
           MOVE 'N'    TO WSS-IND-EXIS IN WSS-VARI.                     
           MOVE ZEROES TO WSS-EXI-QTI  IN WSS-VARI.                      
           MOVE ZEROES TO FRM-SGV-VCAL IN QTI-FLD.                              
           MOVE ZEROES TO FRM-SGV-VIMP IN QTI-FLD.                              
           MOVE IPT-CAI-IOPC IN QTI-FLD TO IPT-CAI-IOPC IN IPT.         
           MOVE IPT-IIC-IOPC IN QTI-FLD TO IPT-IIC-IOPC IN IPT.         
           MOVE SPACES                  TO IPT-COD-EVEN IN IPT.         
           MOVE ZEROES                  TO IPT-NUM-ICAN IN IPT.         
           MOVE SPACES                  TO IPT-COD-CPTO IN IPT.         
           MOVE ZEROES                  TO IPT-NUM-CPTO IN IPT.         
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-CAR-MOV-IPT.                                                 
           PERFORM COL-FIO-IPT.                                         
           IF NOT (FIO-STAT-OKS AND                                     
              IPT-CAI-IOPC IN QTI-FLD = IPT-CAI-IOPC IN IPT AND         
              IPT-IIC-IOPC IN QTI-FLD = IPT-IIC-IOPC IN IPT)            
              GO TO FIN-CAR-MOV-IPT.                                    
                                                                        
           IF (IPT-COD-EVEN IN QTI-FLD > SPACES)                        
               IF NOT (IPT-COD-EVEN IN QTI-FLD = IPT-COD-EVEN IN IPT)   
                  GO TO SIG-CAR-MOV-IPT.                                
                                                                        
           IF (IPT-COD-CPTO IN QTI-FLD > SPACES)                        
               IF NOT (IPT-COD-CPTO IN QTI-FLD = IPT-COD-CPTO IN IPT)   
                  GO TO SIG-CAR-MOV-IPT.                                
                                                                        
           MOVE IPT-NUM-SINI IN IPT TO WSS-NUM-SINN IN WSS-VARI.        
           MOVE IPT-NUM-AINI IN IPT TO WSS-NUM-AINN IN WSS-VARI.        
           MOVE IPT-NUM-MINI IN IPT TO WSS-NUM-MINN IN WSS-VARI.        
           MOVE IPT-NUM-DINI IN IPT TO WSS-NUM-DINN IN WSS-VARI.        
                                                                        
           MOVE IPT-NUM-STER IN IPT TO WSS-NUM-STRR IN WSS-VARI.        
           MOVE IPT-NUM-ATER IN IPT TO WSS-NUM-ATRR IN WSS-VARI.        
           MOVE IPT-NUM-MTER IN IPT TO WSS-NUM-MTRR IN WSS-VARI.        
           MOVE IPT-NUM-DTER IN IPT TO WSS-NUM-DTRR IN WSS-VARI.        

           IF IPT-FEC-FINI IN QTI-FLD > ZEROES                                  
      *       IPT-FEC-FTER IN QTI-FLD NOT > ZEROES                              
              IF (WSS-FEC-FINN IN WSS-VARI < WSS-FEC-FINI IN WSS-VARI)          
                  GO TO SIG-CAR-MOV-IPT.                                        
                                                                                
           IF IPT-FEC-FTER IN QTI-FLD > ZEROES                                  
      *    IF IPT-FEC-FINI IN QTI-FLD NOT > ZEROES                              
      *       IPT-FEC-FTER IN QTI-FLD > ZEROES                                  
              IF (WSS-FEC-FTRR IN WSS-VARI > WSS-FEC-FTER IN WSS-VARI)          
                    GO TO SIG-CAR-MOV-IPT.   
                                                                     
       VAL-CAR-MOV-IPT.                                                 
           MOVE IPT-CAI-IOPC IN IPT TO QTI-CAI-IOPC(QTI-I).             
           MOVE IPT-IIC-IOPC IN IPT TO QTI-IIC-IOPC(QTI-I).             
           MOVE IPT-COD-EVEN IN IPT TO QTI-COD-EVEN(QTI-I).             
           MOVE IPT-NUM-ICAN IN IPT TO QTI-NUM-ICAN(QTI-I).             
           MOVE IPT-COD-CPTO IN IPT TO QTI-COD-CPTO(QTI-I).             
           MOVE IPT-NUM-CPTO IN IPT TO QTI-NUM-CPTO(QTI-I).             
           MOVE IPT-COD-TIPO IN IPT TO QTI-COD-TIPO(QTI-I).             
           MOVE IPT-MSC-STAT IN IPT TO QTI-MSC-STAT(QTI-I).             
           MOVE IPT-NUM-PTRM IN IPT TO QTI-NUM-PTRM(QTI-I).             
           MOVE IPT-VAL-CTRA IN IPT TO QTI-VAL-CTRA(QTI-I).             
           MOVE IPT-NUM-DINI IN IPT TO QTI-NUM-DINI(QTI-I).             
           MOVE IPT-NUM-MINI IN IPT TO QTI-NUM-MINI(QTI-I).             
           MOVE IPT-NUM-SINI IN IPT TO QTI-NUM-SINI(QTI-I).             
           MOVE IPT-NUM-AINI IN IPT TO QTI-NUM-AINI(QTI-I).             
           MOVE IPT-NUM-DTER IN IPT TO QTI-NUM-DTER(QTI-I).             
           MOVE IPT-NUM-MTER IN IPT TO QTI-NUM-MTER(QTI-I).             
           MOVE IPT-NUM-STER IN IPT TO QTI-NUM-STER(QTI-I).             
           MOVE IPT-NUM-ATER IN IPT TO QTI-NUM-ATER(QTI-I).             
           MOVE IPT-SGV-MNTO IN IPT TO QTI-SGV-MNTO(QTI-I).             
           MOVE IPT-SGV-VTOT IN IPT TO QTI-SGV-CUOT(QTI-I).             
           MOVE IPT-SGV-TINT IN IPT TO QTI-SGV-TINT(QTI-I).             
           MOVE IPT-SGV-TUNI IN IPT TO QTI-SGV-TUNI(QTI-I).             
           MOVE IPT-NUM-CPRO IN IPT TO QTI-NUM-CPRO(QTI-I).             
           MOVE IPT-NUM-DORI IN IPT TO QTI-NUM-DORI(QTI-I).             
           MOVE IPT-NUM-MORI IN IPT TO QTI-NUM-MORI(QTI-I).             
           MOVE IPT-NUM-SORI IN IPT TO QTI-NUM-SORI(QTI-I).             
           MOVE IPT-NUM-AORI IN IPT TO QTI-NUM-AORI(QTI-I).             
           IF IPT-COD-TIPO IN IPT = 'A' AND IPT-MSC-STAT IN IPT = 'A'
              IF IPT-SGV-MNTO IN IPT IS NOT NUMERIC                             
                  ADD  ZEROES              TO FRM-SGV-VCAL IN QTI-FLD           
              ELSE                                                              
                  ADD  IPT-SGV-MNTO IN IPT TO FRM-SGV-VCAL IN QTI-FLD.          
                                                                                
           IF IPT-COD-TIPO IN IPT = 'A' AND IPT-MSC-STAT IN IPT = 'A'           
              IF IPT-SGV-VTOT IN IPT IS NOT NUMERIC                             
                  ADD  ZEROES              TO FRM-SGV-VIMP IN QTI-FLD           
              ELSE                                                              
                  ADD  IPT-SGV-VTOT IN IPT TO FRM-SGV-VIMP IN QTI-FLD.          
       NXT-CAR-MOV-IPT.                                                 
           PERFORM INC-REG-TS.                                          
           ADD 1 TO WSS-EXI-QTI.                                        
       SIG-CAR-MOV-IPT.                                                 
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-CAR-MOV-IPT.                                       
       FIN-CAR-MOV-IPT.                                                 
           IF SCR-OFRM-SRV                                              
              PERFORM PUT-QTI-MAS.                                      
           EXIT.                                                        
                                                                        
       PUT-QTI-MAS SECTION.                                             
       INI-PUT-QTI-MAS.                                                 
           MOVE 1                  TO QUE-NITM.                         
           MOVE QTI-LEN-TSQTI-MAS  TO QUE-LITM.                         
           MOVE QTI-NOM-TSQTI-MAS  TO QUE-COLA.                         
           IF WSS-EXI-QTI > QTI-IMAX-MTX                                
              MOVE 'S'             TO QTI-MAS-IND-HMAS                  
              MOVE QTI-IMAX-MTX    TO QTI-MAS-NUM-TOCC                  
           ELSE                                                         
              MOVE 'N'             TO QTI-MAS-IND-HMAS                  
              MOVE WSS-EXI-QTI     TO QTI-MAS-NUM-TOCC.                 
           MOVE QTI-MAS-ITEM       TO QUE-ITEM.                         
           MOVE QUE-PUT            TO QUE-CMND.                         
           PERFORM GNS-PRO-QUE.                                         
       FIN-PUT-QTI-MAS.                                                 
           EXIT.                                                               
                                                     
   
       OBT-PARAM SECTION.                                               
       INI-OBT-PARAM.                                                   
       FIN-OBT-PARAM.                                                   
           EXIT.                                                        

                                                                        
       OBT-DAT-TS SECTION.                                              
       INI-OBT-DAT-TS.                                                  
           EXEC CICS HANDLE CONDITION QIDERR(CON-OBT-DAT-TS)            
                                     ITEMERR(CON-OBT-DAT-TS)            
           END-EXEC.                                                    
                                                                  
           MOVE QTI-NOM-TSQTI     TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
                                                                 
       CON-OBT-DAT-TS.                                                  
           MOVE QTI-SI-IMP TO QTI-IMP.                                  
           PERFORM CLR-VEC-TS VARYING QTI-I FROM 1 BY 1 UNTIL           
                   QTI-I > QTI-IMAX.                                    
           MOVE 1 TO QTI-I.                                             
           PERFORM CAR-MOV-IPT.                                         
       NXT-OBT-DAT-TS.                                                  
           IF QTI-IMP = QTI-SI-IMP                                      
               PERFORM WRT-TS.                                          
       FIN-OBT-DAT-TS.                                                  
           EXIT.                                                        
                                                                        
       PUT-TAB-QTI SECTION.                                             
       INI-PUT-TAB-QTI.                                                 
           PERFORM RED-TS.                                              
           PERFORM LLE-DAT-PNT VARYING QTI-I FROM 1 BY 1                
                   UNTIL QTI-I > QTI-IMAX.                              
       FIN-PUT-TAB-QTI.                                                 
           EXIT.                                                        
                                                                        
       LLE-DAT-PNT SECTION.                                             
       INI-LLE-DAT-PNT.                                                 
           MOVE QTI-CAI-IOPC(QTI-I) TO FRM-CAI-IOPC IN QTI-FLD(QTI-I).  
           MOVE QTI-IIC-IOPC(QTI-I) TO FRM-IIC-IOPC IN QTI-FLD(QTI-I).  
           MOVE QTI-COD-EVEN(QTI-I) TO FRM-COD-EVEN IN QTI-FLD(QTI-I).  
           MOVE QTI-NUM-ICAN(QTI-I) TO FRM-NUM-ICAN IN QTI-FLD(QTI-I).  
           MOVE QTI-COD-CPTO(QTI-I) TO FRM-COD-CPTO IN QTI-FLD(QTI-I).  
           MOVE QTI-NUM-CPTO(QTI-I) TO FRM-NUM-CPTO IN QTI-FLD(QTI-I).  
           MOVE QTI-COD-TIPO(QTI-I) TO FRM-COD-TIPO IN QTI-FLD(QTI-I).  
           MOVE QTI-MSC-STAT(QTI-I) TO FRM-MSC-STAT IN QTI-FLD(QTI-I).  
           MOVE QTI-NUM-PTRM(QTI-I) TO FRM-NUM-PTRM IN QTI-FLD(QTI-I).  
           MOVE QTI-VAL-CTRA(QTI-I) TO FRM-VAL-CTRA IN QTI-FLD(QTI-I).  
           MOVE QTI-NUM-DINI(QTI-I) TO FRM-NUM-DINI IN QTI-FLD(QTI-I).  
           MOVE QTI-NUM-MINI(QTI-I) TO FRM-NUM-MINI IN QTI-FLD(QTI-I).  
           MOVE QTI-NUM-SINI(QTI-I) TO FRM-NUM-SINI IN QTI-FLD(QTI-I).  
           MOVE QTI-NUM-AINI(QTI-I) TO FRM-NUM-AINI IN QTI-FLD(QTI-I).  
           MOVE QTI-NUM-DTER(QTI-I) TO FRM-NUM-DTER IN QTI-FLD(QTI-I).  
           MOVE QTI-NUM-MTER(QTI-I) TO FRM-NUM-MTER IN QTI-FLD(QTI-I).  
           MOVE QTI-NUM-STER(QTI-I) TO FRM-NUM-STER IN QTI-FLD(QTI-I).  
           MOVE QTI-NUM-ATER(QTI-I) TO FRM-NUM-ATER IN QTI-FLD(QTI-I).  
           MOVE QTI-SGV-MNTO(QTI-I) TO FRM-SGV-MNTO IN QTI-FLD(QTI-I).  
           MOVE QTI-SGV-CUOT(QTI-I) TO FRM-SGV-CUOT IN QTI-FLD(QTI-I).  
           MOVE QTI-SGV-TINT(QTI-I) TO FRM-SGV-TINT IN QTI-FLD(QTI-I).  
           MOVE QTI-SGV-TUNI(QTI-I) TO FRM-SGV-TUNI IN QTI-FLD(QTI-I).  
           MOVE QTI-NUM-CPRO(QTI-I) TO FRM-NUM-CPRO IN QTI-FLD(QTI-I).  
           MOVE QTI-NUM-DORI(QTI-I) TO FRM-NUM-DORI IN QTI-FLD(QTI-I).  
           MOVE QTI-NUM-MORI(QTI-I) TO FRM-NUM-MORI IN QTI-FLD(QTI-I).  
           MOVE QTI-NUM-SORI(QTI-I) TO FRM-NUM-SORI IN QTI-FLD(QTI-I).  
           MOVE QTI-NUM-AORI(QTI-I) TO FRM-NUM-AORI IN QTI-FLD(QTI-I).  
       FIN-LLE-DAT-PNT.                                                 
           EXIT.                                                        
                                                                        
       INC-REG-TS SECTION.                                              
       INI-INC-REG-TS.                                                  
           MOVE QTI-SI-IMP TO QTI-IMP.                                  
           ADD 1 TO QTI-I.                                              
           IF QTI-I > QTI-IMAX                                          
               PERFORM WRT-TS                                           
               PERFORM CLR-VEC-TS VARYING QTI-I FROM 1 BY 1             
                                     UNTIL QTI-I > QTI-IMAX             
               MOVE 1 TO QTI-I.                                         
           MOVE QTI-NO-IMP TO QTI-IMP.                                  
       FIN-INC-REG-TS.                                                  
           EXIT.                                                        
                                                                              
                                                   
                                                                        
       WRT-TS SECTION.                                                  
       INI-WRT-TS.                                                      
           ADD 1 TO SCR-IMAX.                                           
           MOVE QTI-NOM-TSQTI  TO QUE-COLA.                             
           IF QTI-TS > QTI-IMAX-MTX                                     
              MOVE QTI-IMAX-MTX    TO QUE-ITEM                          
           ELSE                                                         
              MOVE QTI-TS          TO QUE-ITEM.                         
           MOVE QTI-LEN-TSQTI  TO QUE-LITM.                             
           MOVE QTI-I          TO QUE-NITM.                             
           MOVE QUE-PUT        TO QUE-CMND.                             
           PERFORM GNS-PRO-QUE.                                         
                                                         
       FIN-WRT-TS.                                                      
           EXIT.                                                        
                                                                        
       RED-TS SECTION.                                                  
       INI-RED-TS.                                                      
           MOVE QTI-I TO SCR-I.                                         
           EXEC CICS HANDLE CONDITION QIDERR(FIN-RED-TS)                
                                     ITEMERR(FIN-RED-TS)                
           END-EXEC.                                                    
           MOVE QTI-NOM-TSQTI  TO QUE-COLA.                             
           IF QTI-TS > QTI-IMAX-MTX                                     
              MOVE QTI-IMAX-MTX    TO QUE-ITEM                          
           ELSE                                                         
              MOVE QTI-TS          TO QUE-ITEM.                         
           MOVE QTI-LEN-TSQTI  TO QUE-LITM.                             
           MOVE QTI-I          TO QUE-NITM.                             
           MOVE QUE-GET        TO QUE-CMND.                             
           PERFORM GNS-PRO-QUE.                                         
           MOVE QUE-ITEM       TO QTI-TS.                               
                                                               
       FIN-RED-TS.                                                      
           EXIT.                                                        
                                                                        
       CLR-VEC-TS SECTION.                                              
       INI-CLR-VEC-TS.                                                  
           MOVE SPACES TO QTI-CAI-IOPC(QTI-I).                          
           MOVE ZEROES TO QTI-IIC-IOPC(QTI-I).                          
           MOVE SPACES TO QTI-COD-EVEN(QTI-I).                          
           MOVE ZEROES TO QTI-NUM-ICAN(QTI-I).                          
           MOVE SPACES TO QTI-COD-CPTO(QTI-I).                          
           MOVE ZEROES TO QTI-NUM-CPTO(QTI-I).                          
           MOVE SPACES TO QTI-COD-TIPO(QTI-I).                          
           MOVE SPACES TO QTI-MSC-STAT(QTI-I).                          
           MOVE ZEROES TO QTI-NUM-PTRM(QTI-I).                          
           MOVE ZEROES TO QTI-VAL-CTRA(QTI-I).                          
           MOVE ZEROES TO QTI-FEC-FINI(QTI-I).                          
           MOVE ZEROES TO QTI-FEC-FTER(QTI-I).                          
           MOVE ZEROES TO QTI-SGV-MNTO(QTI-I).                          
           MOVE ZEROES TO QTI-SGV-CUOT(QTI-I).                          
           MOVE ZEROES TO QTI-SGV-TINT(QTI-I).                          
           MOVE ZEROES TO QTI-SGV-TUNI(QTI-I).                          
           MOVE ZEROES TO QTI-NUM-CPRO(QTI-I).                          
           MOVE ZEROES TO QTI-FEC-FORI(QTI-I).                          
       FIN-CLR-VEC-TS.                                                  
           EXIT.                                                        
                                                                        
       RST-STAT SECTION.                                                
       INI-RST-STAT.                                                    
           MOVE SCR-STPP     TO SCR-STAT.                               
           MOVE SCR-MFLD-NOT TO SCR-MFLD.                               
       FIN-RST-STAT.                                                    
           EXIT. 
                                                                              
       COPY GNSBGCPT.                                                   
       COPY SGCBFDBC.                                                   
       COPY COLBFRDC.                                                   
       COPY COLBFOPC. 
      *>>>>
