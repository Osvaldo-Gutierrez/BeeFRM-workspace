      *
      * COLBQTIM
      * Version 3.0.1 Nov-2005
      * 26-Aug-19 12:49 PM
      *
       SET-TAG-COLBQTIM SECTION.
       INI-SET-TAG-COLBQTIM.
           MOVE
           '<IDNMOD NOM=$COLBQTIM$ VSN=$06.07.25.1$ VGM=$3.0.1$
      -    'GEN=$26-Aug-19 12:49 PM$ FNC=$CNCMSV$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQTIM.
           EXIT.
           
       PUT-ALL-TIM SECTION.
       INI-PUT-ALL-TIM.
           PERFORM PUT-TIM-TIM.
       FIN-PUT-ALL-TIM.
           EXIT.
       
       PUT-TIM-TIM SECTION.
       INI-PUT-TIM-TIM.
           IF PGM-STAT-TIM-NUL
               GO TO FIN-PUT-TIM-TIM.
           IF NOT PGM-STAT-TIM-OKS
               PERFORM CLR-TIM-TIM
               GO TO FIN-PUT-TIM-TIM.
           MOVE TIM-COD-CTAB IN TIM TO TIM-COD-CTAB IN TIM-FLD.
           MOVE TIM-CIC-CTAB IN TIM TO TIM-CIC-CTAB IN TIM-FLD.
           MOVE TIM-EXT-CTAB IN TIM TO TIM-EXT-CTAB IN TIM-FLD.
           MOVE TIM-GLS-DESC IN TIM TO TIM-GLS-DESC IN TIM-FLD.
           MOVE TIM-GLS-ABRV IN TIM TO TIM-GLS-ABRV IN TIM-FLD.
           MOVE TIM-GLS-DCOR IN TIM TO TIM-GLS-DCOR IN TIM-FLD.
           MOVE TIM-IND-VIGE IN TIM TO TIM-IND-VIGE IN TIM-FLD.
           MOVE TIM-SGV-TIM1 IN TIM TO TIM-SGV-TIM1 IN TIM-FLD.
           MOVE TIM-SGV-TIM2 IN TIM TO TIM-SGV-TIM2 IN TIM-FLD.
       FIN-PUT-TIM-TIM.
           EXIT.
       
       KEY-ALL-TIM SECTION.
       INI-KEY-ALL-TIM.
           PERFORM KEY-TIM-TIM.
       FIN-KEY-ALL-TIM.
           EXIT.
       
       KEY-TIM-TIM SECTION.
       INI-KEY-TIM-TIM.
           MOVE TIM-COD-CTAB IN TIM-FLD TO TIM-COD-CTAB IN TIM.
       FIN-KEY-TIM-TIM.
           EXIT.
       
       SCR-CLRA-TIM SECTION.
       INI-SCR-CLRA-TIM.
           PERFORM CLR-KEY-TIM.
           PERFORM CLR-FLD-TIM.
       FIN-SCR-CLRA-TIM.
           EXIT.
       
       CLR-KEY-TIM SECTION.
       INI-CLR-KEY-TIM.
           MOVE SPACES TO TIM-COD-CTAB IN TIM-FLD.
       FIN-CLR-KEY-TIM.
           EXIT.
       
       CLR-FLD-TIM SECTION.
       INI-CLR-FLD-TIM.
           PERFORM CLR-TIM-TIM.
       FIN-CLR-FLD-TIM.
           EXIT.
       
       CLR-TIM-TIM SECTION.
       INI-CLR-TIM-TIM.
           MOVE SPACES TO TIM-CIC-CTAB IN TIM-FLD.
           MOVE SPACES TO TIM-EXT-CTAB IN TIM-FLD.
           MOVE SPACES TO TIM-GLS-DESC IN TIM-FLD.
           MOVE SPACES TO TIM-GLS-ABRV IN TIM-FLD.
           MOVE SPACES TO TIM-GLS-DCOR IN TIM-FLD.
           MOVE SPACES TO TIM-IND-VIGE IN TIM-FLD.
           MOVE ZEROES TO TIM-SGV-TIM1 IN TIM-FLD.
           MOVE ZEROES TO TIM-SGV-TIM2 IN TIM-FLD.
       FIN-CLR-TIM-TIM.
           EXIT.
       
       FST-KEY-TIM SECTION.
       INI-FST-KEY-TIM.
           MOVE -1 TO TIM-COD-CTAB-LEN IN TIM-FLD.
       FIN-FST-KEY-TIM.
           EXIT.
       
       FST-FLD-TIM SECTION.
       INI-FST-FLD-TIM.
           MOVE -1 TO TIM-CIC-CTAB-LEN IN TIM-FLD.
       FIN-FST-FLD-TIM.
           EXIT.
       
       PRO-KEY-TIM SECTION.
       INI-PRO-KEY-TIM.
           PERFORM PRO-IKY-TIM.
           PERFORM PRO-AKY-TIM.
           PERFORM PRO-MKY-TIM.
       FIN-PRO-KEY-TIM.
           EXIT.
       
       PRO-IKY-TIM SECTION.
       INI-PRO-IKY-TIM.
           MOVE FRM-CPIM-AEY TO TIM-COD-CTAB-ATR IN TIM-FLD.
       FIN-PRO-IKY-TIM.
           EXIT.
       
       PRO-MKY-TIM SECTION.
       INI-PRO-MKY-TIM.
       FIN-PRO-MKY-TIM.
           EXIT.
       
       PRO-AKY-TIM SECTION.
       INI-PRO-AKY-TIM.
       FIN-PRO-AKY-TIM.
           EXIT.
       
       UNP-KEY-TIM SECTION.
       INI-UNP-KEY-TIM.
           PERFORM UNP-IKY-TIM.
           PERFORM UNP-AKY-TIM.
           PERFORM UNP-MKY-TIM.
       FIN-UNP-KEY-TIM.
           EXIT.
       
       UNP-IKY-TIM SECTION.
       INI-UNP-IKY-TIM.
           MOVE FRM-CPIM-UBY TO TIM-COD-CTAB-ATR.
       FIN-UNP-IKY-TIM.
           EXIT.
       
       UNP-MKY-TIM SECTION.
       INI-UNP-MKY-TIM.
       FIN-UNP-MKY-TIM.
           EXIT.
       
       UNP-AKY-TIM SECTION.
       INI-UNP-AKY-TIM.
       FIN-UNP-AKY-TIM.
           EXIT.
       
       UNP-FLD-TIM SECTION.
       INI-UNP-FLD-TIM.
           PERFORM UNP-TIM-TIM.
       FIN-UNP-FLD-TIM.
           EXIT.
       
       UNP-TIM-TIM SECTION.
       INI-UNP-TIM-TIM.
           MOVE FRM-CPIM-UBY TO TIM-CIC-CTAB-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-EXT-CTAB-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-GLS-DESC-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-GLS-ABRV-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-GLS-DCOR-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-IND-VIGE-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-SGV-TIM1-ATR IN TIM-FLD.
           MOVE FRM-CPIM-UBY TO TIM-SGV-TIM2-ATR IN TIM-FLD.
       FIN-UNP-TIM-TIM.
           EXIT.
       
       VAL-KEY-TIM SECTION.
       INI-VAL-KEY-TIM.
           PERFORM VAL-IKY-TIM.
           PERFORM VAL-MKY-TIM.
           PERFORM VAL-AKY-TIM.
       FIN-VAL-KEY-TIM.
           EXIT.
       
       VAL-IKY-TIM SECTION.
       INI-VAL-IKY-TIM.
           PERFORM VAL-TIM-COD-CTAB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TIM-COD-CTAB-LEN IN TIM-FLD
                GO TO FIN-VAL-IKY-TIM.
       FIN-VAL-IKY-TIM.
           EXIT.
       
       VAL-MKY-TIM SECTION.
       INI-VAL-MKY-TIM.
       FIN-VAL-MKY-TIM.
           EXIT.
       
       VAL-AKY-TIM SECTION.
       INI-VAL-AKY-TIM.
       FIN-VAL-AKY-TIM.
           EXIT.
       
       VAL-TIM-COD-CTAB SECTION.
       INI-VAL-TIM-COD-CTAB.
           IF TIM-COD-CTAB IN TIM-FLD NOT > SPACES
              MOVE SPACES TO TIM-COD-CTAB IN TIM-FLD.
      *<<<< INI_VAL_NUM_IKY_TIM   VAL-FEC VALIDAR FECHA.
           IF SCR-CMND NOT = 'ING'
              GO TO FIN-VAL-TIM-COD-CTAB.

           MOVE TIM-COD-CTAB IN TIM-FLD (7:2) TO FEC-ITM1.
           MOVE TIM-COD-CTAB IN TIM-FLD (5:2) TO FEC-ITM2.
           MOVE TIM-COD-CTAB IN TIM-FLD (3:2) TO FEC-ITM4.
           MOVE TIM-COD-CTAB IN TIM-FLD (1:2) TO FEC-ITM3.
           MOVE FEC-FORM-FEC                  TO FEC-FORM.
           MOVE FEC-FBLK-N                    TO FEC-FBLK.
           MOVE FEC-CHOY-N                    TO FEC-CHOY.
           MOVE FEC-VHBL-N                    TO FEC-VHBL.
           MOVE FEC-VAL-FEC                   TO FEC-CMND.

           PERFORM PRO-FEC.

           IF FEC-STAT-OKS
               CONTINUE
           ELSE
               MOVE FRM-SUAR-MAL              TO FRM-SUAR
               MOVE FEC-MENS                  TO FRM-MENS
               GO TO FIN-VAL-TIM-COD-CTAB.
      *>>>>
       FIN-VAL-TIM-COD-CTAB.
           EXIT.
       
       VAL-CON-KEY-TIM SECTION.
       INI-VAL-CON-KEY-TIM.
           IF     TIM-COD-CTAB IN TIM-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-TIM.
       FIN-VAL-CON-KEY-TIM.
           EXIT.
       
       VAL-CON-IKY-TIM SECTION.
       INI-VAL-CON-IKY-TIM.
           IF     TIM-COD-CTAB IN TIM-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-TIM.
       FIN-VAL-CON-IKY-TIM.
           EXIT.
       
       VAL-NUM-KEY-TIM SECTION.
       INI-VAL-NUM-KEY-TIM.
           PERFORM VAL-NUM-IKY-TIM.
           PERFORM VAL-NUM-MKY-TIM.
           PERFORM VAL-NUM-AKY-TIM.
       FIN-VAL-NUM-KEY-TIM.
           EXIT.
       
       VAL-NUM-IKY-TIM SECTION.
       INI-VAL-NUM-IKY-TIM.
       FIN-VAL-NUM-IKY-TIM.
           EXIT.
       
       VAL-NUM-MKY-TIM SECTION.
       INI-VAL-NUM-MKY-TIM.
       FIN-VAL-NUM-MKY-TIM.
           EXIT.
       
       VAL-NUM-AKY-TIM SECTION.
       INI-VAL-NUM-AKY-TIM.
       FIN-VAL-NUM-AKY-TIM.
           EXIT.
       
       VAL-NUM-FLD-TIM SECTION.
       INI-VAL-NUM-FLD-TIM.
       VAL-NUM-TIM-SGV-TIM1.
           MOVE TIM-SGV-TIM1-ALF IN TIM-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO TIM-SGV-TIM1 IN TIM-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO TIM-SGV-TIM1-LEN IN TIM-FLD
                GO TO FIN-VAL-NUM-FLD-TIM.
       VAL-NUM-TIM-SGV-TIM2.
           MOVE TIM-SGV-TIM2-ALF IN TIM-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 7 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO TIM-SGV-TIM2 IN TIM-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO TIM-SGV-TIM2-LEN IN TIM-FLD
                GO TO FIN-VAL-NUM-FLD-TIM.
       FIN-VAL-NUM-FLD-TIM.
           EXIT.
       
       EDT-KEY-TIM SECTION.
       INI-EDT-KEY-TIM.
           PERFORM EDT-IKY-TIM.
           PERFORM EDT-MKY-TIM.
           PERFORM EDT-AKY-TIM.
       FIN-EDT-KEY-TIM.
           EXIT.
       
       EDT-IKY-TIM SECTION.
       INI-EDT-IKY-TIM.
       FIN-EDT-IKY-TIM.
           EXIT.
       
       EDT-MKY-TIM SECTION.
       INI-EDT-MKY-TIM.
       FIN-EDT-MKY-TIM.
           EXIT.
       
       EDT-AKY-TIM SECTION.
       INI-EDT-AKY-TIM.
       FIN-EDT-AKY-TIM.
           EXIT.
       
       EDT-FLD-TIM SECTION.
       INI-EDT-FLD-TIM.
           IF TIM-SGV-TIM1-LEN IN TIM-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO TIM-SGV-TIM1-LEN IN TIM-FLD
                GO TO FIN-EDT-FLD-TIM.
           MOVE TIM-SGV-TIM1 IN TIM-FLD TO TIM-SGV-TIM1-EDT IN TIM-EDT.
           MOVE TIM-SGV-TIM1-EDT IN TIM-EDT TO 
                TIM-SGV-TIM1-ALF IN TIM-FLD.
           IF TIM-SGV-TIM2-LEN IN TIM-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO TIM-SGV-TIM2-LEN IN TIM-FLD
                GO TO FIN-EDT-FLD-TIM.
           MOVE TIM-SGV-TIM2 IN TIM-FLD TO TIM-SGV-TIM2-EDT IN TIM-EDT.
           MOVE TIM-SGV-TIM2-EDT IN TIM-EDT TO 
                TIM-SGV-TIM2-ALF IN TIM-FLD.
       FIN-EDT-FLD-TIM.
           EXIT.
