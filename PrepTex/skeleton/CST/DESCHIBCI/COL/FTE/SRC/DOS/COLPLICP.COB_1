       COPY GNSBGFEC.
       COPY GNSBGHOY.
       COPY GNSBBMSG.
       COPY COLBFOPC.
       COPY COLBFDLC.
       COPY COLBFRDC.
       COPY COLBFRCV.
       COPY SGCBFDBC.
       COPY TABBFOFI.
       COPY TABBFUSR.
      *>>>>
