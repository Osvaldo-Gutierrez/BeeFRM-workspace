      *
      * COLBQDPC
      * Version 3.0.1 Nov-2005
      * 01-Apr-10 02:16 PM
      *
       SET-TAG-COLBQDPC SECTION.
       INI-SET-TAG-COLBQDPC.
           MOVE
           '<IDNMOD NOM=$COLBQDPC$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$01-Apr-10 02:16 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQDPC.
           EXIT.
           
       PUT-ALL-DPC SECTION.
       INI-PUT-ALL-DPC.
           PERFORM PUT-DPC-DPC.
       FIN-PUT-ALL-DPC.
           EXIT.
       
       PUT-DPC-DPC SECTION.
       INI-PUT-DPC-DPC.
           IF PGM-STAT-DPC-NUL
               GO TO FIN-PUT-DPC-DPC.
           IF NOT PGM-STAT-DPC-OKS
               PERFORM CLR-DPC-DPC
               GO TO FIN-PUT-DPC-DPC.
           MOVE DPC-TIP-REGI IN DPC TO DPC-TIP-REGI IN DPC-FLD.
           MOVE DPC-COD-CODI IN DPC TO DPC-COD-CODI IN DPC-FLD.
      *<<<< FIN_PUT_DPC_DPC
      *                      MUEVO CAMPOS DEL ARCHIVO A LA PANTALLA     
                                                                        
      * Tipo Registro 1                                                 
           IF DPC-TIP-REGI IN DPC-FLD = '1'                             
              IF DPC-FEC-FPRO IN DPC IS NOT NUMERIC                     
                 MOVE ZEROES TO DPC-FEC-FPRO IN DPC.                    
                                                                        
           IF DPC-TIP-REGI IN DPC-FLD = '1'                             
              IF DPC-FEC-FINI IN DPC IS NOT NUMERIC                     
                 MOVE ZEROES TO DPC-FEC-FINI IN DPC.                    
                                                                        
           IF DPC-TIP-REGI IN DPC-FLD = '1'                             
              IF DPC-FEC-FTER IN DPC IS NOT NUMERIC                     
                 MOVE ZEROES TO DPC-FEC-FTER IN DPC.                    
                                                                        
           IF DPC-TIP-REGI IN DPC-FLD = '1'                             
              MOVE DPC-NUM-SPRO IN DPC TO FRM-NUM-SPRO IN DPC-FLD       
              MOVE DPC-NUM-APRO IN DPC TO FRM-NUM-APRO IN DPC-FLD       
              MOVE DPC-NUM-MPRO IN DPC TO FRM-NUM-MPRO IN DPC-FLD       
              MOVE DPC-NUM-DPRO IN DPC TO FRM-NUM-DPRO IN DPC-FLD       
                                                                        
              MOVE DPC-NUM-SINI IN DPC TO FRM-NUM-SINI IN DPC-FLD       
              MOVE DPC-NUM-AINI IN DPC TO FRM-NUM-AINI IN DPC-FLD       
              MOVE DPC-NUM-MINI IN DPC TO FRM-NUM-MINI IN DPC-FLD       
              MOVE DPC-NUM-DINI IN DPC TO FRM-NUM-DINI IN DPC-FLD       
                                                                        
              MOVE DPC-NUM-STER IN DPC TO FRM-NUM-STER IN DPC-FLD       
              MOVE DPC-NUM-ATER IN DPC TO FRM-NUM-ATER IN DPC-FLD       
              MOVE DPC-NUM-MTER IN DPC TO FRM-NUM-MTER IN DPC-FLD       
              MOVE DPC-NUM-DTER IN DPC TO FRM-NUM-DTER IN DPC-FLD.      
                                                                        
      * Tipo Registro 2                                                 
           IF DPC-TIP-REGI IN DPC-FLD = '2'                             
              IF DPC-NUM-FOI2 IN DPC IS NOT NUMERIC                     
                 MOVE ZEROES TO DPC-NUM-FOI2 IN DPC.                    
                                                                        
           IF DPC-TIP-REGI IN DPC-FLD = '2'                             
              IF DPC-NUM-FOF2 IN DPC IS NOT NUMERIC                     
                 MOVE ZEROES TO DPC-NUM-FOF2 IN DPC.                    
                                                                        
           IF DPC-TIP-REGI IN DPC-FLD = '2'                             
              MOVE DPC-NUM-FOI2 IN DPC TO FRM-NUM-FOI2 IN DPC-FLD       
              MOVE DPC-NUM-FOF2 IN DPC TO FRM-NUM-FOF2 IN DPC-FLD.      
                                                                        
      * Tipo Registro 3                                                 
           IF DPC-TIP-REGI IN DPC-FLD = '3'                             
              IF DPC-VAL-SDAN IN DPC IS NOT NUMERIC                     
                 MOVE ZEROES TO DPC-VAL-SDAN IN DPC.                    
                                                                        
           IF DPC-TIP-REGI IN DPC-FLD = '3'                             
              IF DPC-VAL-SHAN IN DPC IS NOT NUMERIC                     
                 MOVE ZEROES TO DPC-VAL-SHAN IN DPC.                    
                                                                        
           IF DPC-TIP-REGI IN DPC-FLD = '3'                             
              IF DPC-VAL-MDPE IN DPC IS NOT NUMERIC                     
                  MOVE ZEROES TO DPC-VAL-MDPE IN DPC.                   
                                                                        
           IF DPC-TIP-REGI IN DPC-FLD = '3'                             
              IF DPC-VAL-MHPE IN DPC IS NOT NUMERIC                     
                 MOVE ZEROES TO DPC-VAL-MHPE IN DPC.                    
                                                                        
           IF DPC-TIP-REGI IN DPC-FLD = '3'                             
              IF DPC-VAL-SDNV IN DPC IS NOT NUMERIC                     
                 MOVE ZEROES TO DPC-VAL-SDNV IN DPC.                    
                                                                        
           IF DPC-TIP-REGI IN DPC-FLD = '3'                             
              IF DPC-VAL-SHNV IN DPC IS NOT NUMERIC                     
                 MOVE ZEROES TO DPC-VAL-SHNV IN DPC.                    
                                                                        
           IF DPC-TIP-REGI IN DPC-FLD = '3'                             
              MOVE DPC-VAL-SDAN IN DPC TO FRM-VAL-SDAN IN DPC-FLD       
              MOVE DPC-VAL-SHAN IN DPC TO FRM-VAL-SHAN IN DPC-FLD       
              MOVE DPC-VAL-MDPE IN DPC TO FRM-VAL-MDPE IN DPC-FLD       
              MOVE DPC-VAL-MHPE IN DPC TO FRM-VAL-MHPE IN DPC-FLD       
              MOVE DPC-VAL-SDNV IN DPC TO FRM-VAL-SDNV IN DPC-FLD       
              MOVE DPC-VAL-SHNV IN DPC TO FRM-VAL-SHNV IN DPC-FLD.      
                                                                        
      * Tipo Registro 4                                                 
           IF DPC-TIP-REGI IN DPC-FLD = '4'                             
              IF DPC-VAL-MACU IN DPC IS NOT NUMERIC                     
                 MOVE ZEROES TO DPC-VAL-MACU IN DPC.                    
                                                                        
           IF DPC-TIP-REGI IN DPC-FLD = '4'                             
              IF DPC-NUM-RACU IN DPC IS NOT NUMERIC                     
                 MOVE ZEROES TO DPC-NUM-RACU IN DPC.                    
                                                                        
           IF DPC-TIP-REGI IN DPC-FLD = '4'                             
              IF DPC-VAL-MELI IN DPC IS NOT NUMERIC                     
                 MOVE ZEROES TO DPC-VAL-MELI IN DPC.                    
                                                                        
           IF DPC-TIP-REGI IN DPC-FLD = '4'                             
              IF DPC-NUM-RELI IN DPC IS NOT NUMERIC                     
                 MOVE ZEROES TO DPC-NUM-RELI IN DPC.                    
                                                                        
           IF DPC-TIP-REGI IN DPC-FLD = '4'                             
              IF DPC-FEC-FELI IN DPC IS NOT NUMERIC                     
                  MOVE ZEROES TO DPC-FEC-FELI IN DPC.                   
                                                                        
           IF DPC-TIP-REGI IN DPC-FLD = '4'                             
              IF DPC-FEC-FPR4 IN DPC IS NOT NUMERIC                     
                 MOVE ZEROES TO DPC-FEC-FPR4 IN DPC.                    
                                                                        
           IF DPC-TIP-REGI IN DPC-FLD = '4'                             
              MOVE DPC-VAL-MACU IN DPC TO FRM-VAL-MACU IN DPC-FLD       
              MOVE DPC-NUM-RACU IN DPC TO FRM-NUM-RACU IN DPC-FLD       
              MOVE DPC-VAL-MELI IN DPC TO FRM-VAL-MELI IN DPC-FLD       
              MOVE DPC-NUM-RELI IN DPC TO FRM-NUM-RELI IN DPC-FLD       
                                                                        
              MOVE DPC-NUM-SELI IN DPC TO FRM-NUM-SELI IN DPC-FLD       
              MOVE DPC-NUM-AELI IN DPC TO FRM-NUM-AELI IN DPC-FLD       
              MOVE DPC-NUM-MELI IN DPC TO FRM-NUM-MELI IN DPC-FLD       
              MOVE DPC-NUM-DELI IN DPC TO FRM-NUM-DELI IN DPC-FLD       
                                                                        
              MOVE DPC-NUM-SPR4 IN DPC TO FRM-NUM-SPR4 IN DPC-FLD       
              MOVE DPC-NUM-APR4 IN DPC TO FRM-NUM-APR4 IN DPC-FLD       
              MOVE DPC-NUM-MPR4 IN DPC TO FRM-NUM-MPR4 IN DPC-FLD       
              MOVE DPC-NUM-DPR4 IN DPC TO FRM-NUM-DPR4 IN DPC-FLD.      
                                                                        
      * Tipo Registro 5                                                 
           IF DPC-TIP-REGI IN DPC-FLD = '5'                             
              IF DPC-NUM-FOI5 IN DPC IS NOT NUMERIC                     
                 MOVE ZEROES TO DPC-NUM-FOI5 IN DPC.                    
                                                                        
           IF DPC-TIP-REGI IN DPC-FLD = '5'                             
              IF DPC-NUM-FOF5 IN DPC IS NOT NUMERIC                     
                 MOVE ZEROES TO DPC-NUM-FOF5 IN DPC.                    
                                                                        
           IF DPC-TIP-REGI IN DPC-FLD = '5'                             
              MOVE DPC-NUM-FOI5 IN DPC TO FRM-NUM-FOI5 IN DPC-FLD       
              MOVE DPC-NUM-FOF5 IN DPC TO FRM-NUM-FOF5 IN DPC-FLD.      
                                                                        
      *     FIN FIN_PUT_DPC_DPC                                         

      *>>>>
       FIN-PUT-DPC-DPC.
           EXIT.
       
       KEY-ALL-DPC SECTION.
       INI-KEY-ALL-DPC.
           PERFORM KEY-DPC-DPC.
       FIN-KEY-ALL-DPC.
           EXIT.
       
       KEY-DPC-DPC SECTION.
       INI-KEY-DPC-DPC.
           MOVE DPC-TIP-REGI IN DPC-FLD TO DPC-TIP-REGI IN DPC.
           MOVE DPC-COD-CODI IN DPC-FLD TO DPC-COD-CODI IN DPC.
       FIN-KEY-DPC-DPC.
           EXIT.
       
       SCR-CLRA-DPC SECTION.
       INI-SCR-CLRA-DPC.
           PERFORM CLR-KEY-DPC.
           PERFORM CLR-FLD-DPC.
       FIN-SCR-CLRA-DPC.
           EXIT.
       
       CLR-KEY-DPC SECTION.
       INI-CLR-KEY-DPC.
           MOVE SPACES TO DPC-TIP-REGI IN DPC-FLD.
           MOVE SPACES TO DPC-COD-CODI IN DPC-FLD.
       FIN-CLR-KEY-DPC.
           EXIT.
       
       CLR-FLD-DPC SECTION.
       INI-CLR-FLD-DPC.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FPRO IN DPC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-FOI2 IN DPC-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FINI IN DPC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-FOF2 IN DPC-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FTERM IN DPC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-VAL-SDAN IN DPC-FLD.
           MOVE ZEROES TO FRM-VAL-SHAN IN DPC-FLD.
           MOVE ZEROES TO FRM-VAL-MDPE IN DPC-FLD.
           MOVE ZEROES TO FRM-VAL-MHPE IN DPC-FLD.
           MOVE ZEROES TO FRM-VAL-SDNV IN DPC-FLD.
           MOVE ZEROES TO FRM-VAL-SHNV IN DPC-FLD.
           MOVE ZEROES TO FRM-VAL-MACU IN DPC-FLD.
           MOVE ZEROES TO FRM-NUM-RACU IN DPC-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FELI IN DPC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-VAL-MELI IN DPC-FLD.
           MOVE ZEROES TO FRM-NUM-RELI IN DPC-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FPR4 IN DPC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-NUM-FOI5 IN DPC-FLD.
           MOVE ZEROES TO FRM-NUM-FOF5 IN DPC-FLD.
           PERFORM CLR-DPC-DPC.
       FIN-CLR-FLD-DPC.
           EXIT.
       
       CLR-DPC-DPC SECTION.
       INI-CLR-DPC-DPC.
       FIN-CLR-DPC-DPC.
           EXIT.
       
       FST-KEY-DPC SECTION.
       INI-FST-KEY-DPC.
           MOVE -1 TO DPC-TIP-REGI-LEN IN DPC-FLD.
       FIN-FST-KEY-DPC.
           EXIT.
       
       FST-FLD-DPC SECTION.
       INI-FST-FLD-DPC.
      *<<<< INI_FST_FLD_DPC	                                     
           MOVE -1 TO FRM-FEC-FPRO-LEN IN DPC-FLD.                      
      *>>>>
       FIN-FST-FLD-DPC.
           EXIT.
       
       PRO-KEY-DPC SECTION.
       INI-PRO-KEY-DPC.
           PERFORM PRO-IKY-DPC.
           PERFORM PRO-AKY-DPC.
           PERFORM PRO-MKY-DPC.
       FIN-PRO-KEY-DPC.
           EXIT.
       
       PRO-IKY-DPC SECTION.
       INI-PRO-IKY-DPC.
           MOVE FRM-CPIM-AEY TO DPC-TIP-REGI-ATR IN DPC-FLD.
           MOVE FRM-CPIM-AEY TO DPC-COD-CODI-ATR IN DPC-FLD.
       FIN-PRO-IKY-DPC.
           EXIT.
       
       PRO-MKY-DPC SECTION.
       INI-PRO-MKY-DPC.
       FIN-PRO-MKY-DPC.
           EXIT.
       
       PRO-AKY-DPC SECTION.
       INI-PRO-AKY-DPC.
       FIN-PRO-AKY-DPC.
           EXIT.
       
       UNP-KEY-DPC SECTION.
       INI-UNP-KEY-DPC.
           PERFORM UNP-IKY-DPC.
           PERFORM UNP-AKY-DPC.
           PERFORM UNP-MKY-DPC.
       FIN-UNP-KEY-DPC.
           EXIT.
       
       UNP-IKY-DPC SECTION.
       INI-UNP-IKY-DPC.
           MOVE FRM-CPIM-UBY TO DPC-TIP-REGI-ATR.
           MOVE FRM-CPIM-UBY TO DPC-COD-CODI-ATR.
       FIN-UNP-IKY-DPC.
           EXIT.
       
       UNP-MKY-DPC SECTION.
       INI-UNP-MKY-DPC.
       FIN-UNP-MKY-DPC.
           EXIT.
       
       UNP-AKY-DPC SECTION.
       INI-UNP-AKY-DPC.
       FIN-UNP-AKY-DPC.
           EXIT.
       
       UNP-FLD-DPC SECTION.
       INI-UNP-FLD-DPC.
      *<<<< INI_UNP_FLD_DPC                                     
                                                                        
           MOVE FRM-CPIM-UBY TO FRM-FEC-FPRO-ATR IN DPC-FLD.            
           MOVE FRM-CPIM-UBY TO FRM-NUM-FOI2-ATR IN DPC-FLD.            
           MOVE FRM-CPIM-UBY TO FRM-FEC-FINI-ATR IN DPC-FLD.            
           MOVE FRM-CPIM-UBY TO FRM-NUM-FOF2-ATR IN DPC-FLD.            
           MOVE FRM-CPIM-UBY TO FRM-FEC-FTER-ATR IN DPC-FLD.            
           MOVE FRM-CPIM-UBY TO FRM-VAL-SDAN-ATR IN DPC-FLD.            
           MOVE FRM-CPIM-UBY TO FRM-VAL-SHAN-ATR IN DPC-FLD.            
           MOVE FRM-CPIM-UBY TO FRM-VAL-MDPE-ATR IN DPC-FLD.            
           MOVE FRM-CPIM-UBY TO FRM-VAL-MHPE-ATR IN DPC-FLD.            
           MOVE FRM-CPIM-UBY TO FRM-VAL-SDNV-ATR IN DPC-FLD.            
           MOVE FRM-CPIM-UBY TO FRM-VAL-SHNV-ATR IN DPC-FLD.            
           MOVE FRM-CPIM-UBY TO FRM-FEC-FELI-ATR IN DPC-FLD.            
           MOVE FRM-CPIM-UBY TO FRM-FEC-FPR4-ATR IN DPC-FLD.            
           MOVE FRM-CPIM-UBY TO FRM-NUM-FOI5-ATR IN DPC-FLD.            
           MOVE FRM-CPIM-UBY TO FRM-NUM-FOF5-ATR IN DPC-FLD.            
           MOVE FRM-CPIM-UBY TO FRM-NUM-RACU-ATR IN DPC-FLD.            
           MOVE FRM-CPIM-UBY TO FRM-NUM-RELI-ATR IN DPC-FLD.            
           MOVE FRM-CPIM-UBY TO FRM-VAL-MACU-ATR IN DPC-FLD.            
           MOVE FRM-CPIM-UBY TO FRM-VAL-MELI-ATR IN DPC-FLD.            
      *>>>>
           PERFORM UNP-DPC-DPC.
       FIN-UNP-FLD-DPC.
           EXIT.
       
       UNP-DPC-DPC SECTION.
       INI-UNP-DPC-DPC.
       FIN-UNP-DPC-DPC.
           EXIT.
       
       VAL-KEY-DPC SECTION.
       INI-VAL-KEY-DPC.
           PERFORM VAL-IKY-DPC.
           PERFORM VAL-MKY-DPC.
           PERFORM VAL-AKY-DPC.
       FIN-VAL-KEY-DPC.
           EXIT.
       
       VAL-IKY-DPC SECTION.
       INI-VAL-IKY-DPC.
           PERFORM VAL-DPC-TIP-REGI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DPC-TIP-REGI-LEN IN DPC-FLD
                GO TO FIN-VAL-IKY-DPC.
           PERFORM VAL-DPC-COD-CODI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DPC-COD-CODI-LEN IN DPC-FLD
                GO TO FIN-VAL-IKY-DPC.
      *<<<< FIN_VAL_IKY_DPC                                             
      * Valida que Tipo Registro este entre 1 y 5                       
           IF DPC-TIP-REGI IN DPC-FLD < '1' OR                          
              DPC-TIP-REGI IN DPC-FLD > '5'                             
              MOVE FRM-SUAR-MAL TO FRM-SUAR                             
              MOVE -1 TO DPC-TIP-REGI-LEN IN DPC-FLD                    
              GO TO FIN-VAL-IKY-DPC.                                    
                                                                        
      *>>>>                                                             
       FIN-VAL-IKY-DPC.
           EXIT.
       
       VAL-MKY-DPC SECTION.
       INI-VAL-MKY-DPC.
       FIN-VAL-MKY-DPC.
           EXIT.
       
       VAL-AKY-DPC SECTION.
       INI-VAL-AKY-DPC.
       FIN-VAL-AKY-DPC.
           EXIT.
       
       VAL-DPC-TIP-REGI SECTION.
       INI-VAL-DPC-TIP-REGI.
           IF DPC-TIP-REGI IN DPC-FLD NOT > SPACES
              MOVE SPACES TO DPC-TIP-REGI IN DPC-FLD.
       FIN-VAL-DPC-TIP-REGI.
           EXIT.
       
       VAL-DPC-COD-CODI SECTION.
       INI-VAL-DPC-COD-CODI.
           IF DPC-COD-CODI IN DPC-FLD NOT > SPACES
              MOVE SPACES TO DPC-COD-CODI IN DPC-FLD.
       FIN-VAL-DPC-COD-CODI.
           EXIT.
       
       VAL-CON-KEY-DPC SECTION.
       INI-VAL-CON-KEY-DPC.
           IF     DPC-TIP-REGI IN DPC-FLD NOT > SPACES 
              AND DPC-COD-CODI IN DPC-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-DPC.
       FIN-VAL-CON-KEY-DPC.
           EXIT.
       
       VAL-CON-IKY-DPC SECTION.
       INI-VAL-CON-IKY-DPC.
           IF     DPC-TIP-REGI IN DPC-FLD NOT > SPACES 
              AND DPC-COD-CODI IN DPC-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-DPC.
       FIN-VAL-CON-IKY-DPC.
           EXIT.
       
       VAL-NUM-KEY-DPC SECTION.
       INI-VAL-NUM-KEY-DPC.
           PERFORM VAL-NUM-IKY-DPC.
           PERFORM VAL-NUM-MKY-DPC.
           PERFORM VAL-NUM-AKY-DPC.
       FIN-VAL-NUM-KEY-DPC.
           EXIT.
       
       VAL-NUM-IKY-DPC SECTION.
       INI-VAL-NUM-IKY-DPC.
       FIN-VAL-NUM-IKY-DPC.
           EXIT.
       
       VAL-NUM-MKY-DPC SECTION.
       INI-VAL-NUM-MKY-DPC.
       FIN-VAL-NUM-MKY-DPC.
           EXIT.
       
       VAL-NUM-AKY-DPC SECTION.
       INI-VAL-NUM-AKY-DPC.
       FIN-VAL-NUM-AKY-DPC.
           EXIT.
       
       VAL-NUM-FLD-DPC SECTION.
       INI-VAL-NUM-FLD-DPC.
       VAL-NUM-FRM-NUM-FOI2.
           MOVE FRM-NUM-FOI2-ALF IN DPC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 6 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO FRM-NUM-FOI2 IN DPC-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO FRM-NUM-FOI2-LEN IN DPC-FLD
                GO TO FIN-VAL-NUM-FLD-DPC.
       VAL-NUM-FRM-NUM-FOF2.
           MOVE FRM-NUM-FOF2-ALF IN DPC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 6 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO FRM-NUM-FOF2 IN DPC-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO FRM-NUM-FOF2-LEN IN DPC-FLD
                GO TO FIN-VAL-NUM-FLD-DPC.
       VAL-NUM-FRM-VAL-SDAN.
           MOVE FRM-VAL-SDAN-ALF IN DPC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 10 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NS9V TO FRM-VAL-SDAN IN DPC-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO FRM-VAL-SDAN-LEN IN DPC-FLD
                GO TO FIN-VAL-NUM-FLD-DPC.
       VAL-NUM-FRM-VAL-SHAN.
           MOVE FRM-VAL-SHAN-ALF IN DPC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 10 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NS9V TO FRM-VAL-SHAN IN DPC-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO FRM-VAL-SHAN-LEN IN DPC-FLD
                GO TO FIN-VAL-NUM-FLD-DPC.
       VAL-NUM-FRM-VAL-MDPE.
           MOVE FRM-VAL-MDPE-ALF IN DPC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 10 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NS9V TO FRM-VAL-MDPE IN DPC-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO FRM-VAL-MDPE-LEN IN DPC-FLD
                GO TO FIN-VAL-NUM-FLD-DPC.
       VAL-NUM-FRM-VAL-MHPE.
           MOVE FRM-VAL-MHPE-ALF IN DPC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
