           PERFORM VAL-FRM-IND-IAVA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-IND-IAVA-LEN IN CUF-FLD
                GO TO FIN-VAL-AKY-CUF.
           PERFORM VAL-FRM-GLS-IAVA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-IAVA-LEN IN CUF-FLD
                GO TO FIN-VAL-AKY-CUF.
           PERFORM VAL-FRM-NUM-IAVA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-NUM-IAVA-LEN IN CUF-FLD
                GO TO FIN-VAL-AKY-CUF.
           PERFORM VAL-FRM-VRF-IAVA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-VRF-IAVA-LEN IN CUF-FLD
                GO TO FIN-VAL-AKY-CUF.
       FIN-VAL-AKY-CUF.
           EXIT.
       
       VAL-FLD-CUF SECTION.
       INI-VAL-FLD-CUF.
           PERFORM VAL-OPC-MSC-TVOC.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPC-MSC-TVOC-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-OPC-COD-TOOC.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPC-COD-TOOC-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-OPC-COD-OFOC.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPC-COD-OFOC-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-FRM-GLS-TVOC.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-TVOC-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-FRM-GLS-TOOC.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-TOOC-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-FRM-GLS-OFOC.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-OFOC-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-CUF-COD-UFAD.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO CUF-COD-UFAD-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-CUF-FEC-FCUF.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO CUF-FEC-FCUF-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-FRM-GLS-UFAD.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-UFAD-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-CUF-FEC-FING.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO CUF-FEC-FING-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-CUF-COD-UBFD.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO CUF-COD-UBFD-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-CUF-COD-RCUF.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO CUF-COD-RCUF-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-FRM-GLS-UBFD.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-UBFD-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-FRM-GLS-RCUF.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-RCUF-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
           PERFORM VAL-DLC-COD-UBFD.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-COD-UBFD-LEN IN CUF-FLD
                GO TO FIN-VAL-FLD-CUF.
       FIN-VAL-FLD-CUF.
           EXIT.
       
       VAL-OPC-MSC-TVOC SECTION.
       INI-VAL-OPC-MSC-TVOC.
       FIN-VAL-OPC-MSC-TVOC.
           EXIT.
       
       VAL-OPC-COD-TOOC SECTION.
       INI-VAL-OPC-COD-TOOC.
       FIN-VAL-OPC-COD-TOOC.
           EXIT.
       
       VAL-OPC-COD-OFOC SECTION.
       INI-VAL-OPC-COD-OFOC.
       FIN-VAL-OPC-COD-OFOC.
           EXIT.
       
       VAL-CUF-CAI-IOPC SECTION.
       INI-VAL-CUF-CAI-IOPC.
           IF CUF-CAI-IOPC IN CUF-FLD NOT > SPACES
              MOVE SPACES TO CUF-CAI-IOPC IN CUF-FLD.
       FIN-VAL-CUF-CAI-IOPC.
           EXIT.
       
       VAL-CUF-IIC-IOPC SECTION.
       INI-VAL-CUF-IIC-IOPC.
       FIN-VAL-CUF-IIC-IOPC.
           EXIT.
       
       VAL-FRM-GLS-TVOC SECTION.
       INI-VAL-FRM-GLS-TVOC.
       FIN-VAL-FRM-GLS-TVOC.
           EXIT.
       
       VAL-FRM-GLS-TOOC SECTION.
       INI-VAL-FRM-GLS-TOOC.
       FIN-VAL-FRM-GLS-TOOC.
           EXIT.
       
       VAL-FRM-GLS-OFOC SECTION.
       INI-VAL-FRM-GLS-OFOC.
       FIN-VAL-FRM-GLS-OFOC.
           EXIT.
       
       VAL-CUF-NUM-IDLC SECTION.
       INI-VAL-CUF-NUM-IDLC.
       FIN-VAL-CUF-NUM-IDLC.
           EXIT.
       
       VAL-CUF-NUM-ICUF SECTION.
       INI-VAL-CUF-NUM-ICUF.
       FIN-VAL-CUF-NUM-ICUF.
           EXIT.
       
       VAL-FRM-GLS-NOMC SECTION.
       INI-VAL-FRM-GLS-NOMC.
           IF FRM-GLS-NOMC IN CUF-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-NOMC IN CUF-FLD.
       FIN-VAL-FRM-GLS-NOMC.
           EXIT.
       
       VAL-FRM-IND-ICLI SECTION.
       INI-VAL-FRM-IND-ICLI.
           IF FRM-IND-ICLI IN CUF-FLD NOT > SPACES
              MOVE SPACES TO FRM-IND-ICLI IN CUF-FLD.
      *VAL-IDC Valida identificador
           MOVE FRM-IND-ICLI IN CUF-FLD TO IDC-IND-RUTV.
           PERFORM VAL-IDC.
           MOVE IDC-IND-RUTV TO FRM-IND-ICLI IN CUF-FLD.
       FIN-VAL-FRM-IND-ICLI.
           EXIT.
       
       VAL-FRM-GLS-ICLI SECTION.
       INI-VAL-FRM-GLS-ICLI.
           IF FRM-GLS-ICLI IN CUF-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-ICLI IN CUF-FLD.
      *VAL-GLS para IDC valida glosa ( extension ) de identificador
           MOVE FRM-GLS-ICLI IN CUF-FLD TO IDC-GLS-RUTV.
           PERFORM VAL-GLS.
           MOVE IDC-GLS-RUTV TO FRM-GLS-ICLI IN CUF-FLD.
       FIN-VAL-FRM-GLS-ICLI.
           EXIT.
       
       VAL-FRM-NUM-ICLI SECTION.
       INI-VAL-FRM-NUM-ICLI.
       FIN-VAL-FRM-NUM-ICLI.
           EXIT.
       
       VAL-FRM-VRF-ICLI SECTION.
       INI-VAL-FRM-VRF-ICLI.
           IF FRM-VRF-ICLI IN CUF-FLD NOT > SPACES
              MOVE SPACES TO FRM-VRF-ICLI IN CUF-FLD.
      *VAL-RUT Valida Rut para campo IDC
           MOVE IDC-ZERO-S TO IDC-ZERO.
           MOVE FRM-NUM-ICLI IN CUF-FLD TO IDC-NUM-RUTV.
           MOVE FRM-VRF-ICLI IN CUF-FLD TO IDC-VRF-RUTV.
           PERFORM VAL-RUT.
       FIN-VAL-FRM-VRF-ICLI.
           EXIT.
       
       VAL-FRM-CAI-ICLI SECTION.
       INI-VAL-FRM-CAI-ICLI.
       FIN-VAL-FRM-CAI-ICLI.
           EXIT.
       
       VAL-FRM-IIC-ICLI SECTION.
       INI-VAL-FRM-IIC-ICLI.
       FIN-VAL-FRM-IIC-ICLI.
           EXIT.
       
       VAL-FRM-GLS-NOMA SECTION.
       INI-VAL-FRM-GLS-NOMA.
           IF FRM-GLS-NOMA IN CUF-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-NOMA IN CUF-FLD.
       FIN-VAL-FRM-GLS-NOMA.
           EXIT.
       
       VAL-FRM-IND-IAVA SECTION.
       INI-VAL-FRM-IND-IAVA.
           IF FRM-IND-IAVA IN CUF-FLD NOT > SPACES
              MOVE SPACES TO FRM-IND-IAVA IN CUF-FLD.
      *VAL-IDC Valida identificador
           MOVE FRM-IND-IAVA IN CUF-FLD TO IDC-IND-RUTV.
           PERFORM VAL-IDC.
           MOVE IDC-IND-RUTV TO FRM-IND-IAVA IN CUF-FLD.
       FIN-VAL-FRM-IND-IAVA.
           EXIT.
       
       VAL-FRM-GLS-IAVA SECTION.
       INI-VAL-FRM-GLS-IAVA.
           IF FRM-GLS-IAVA IN CUF-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-IAVA IN CUF-FLD.
      *VAL-GLS para IDC valida glosa ( extension ) de identificador
           MOVE FRM-GLS-IAVA IN CUF-FLD TO IDC-GLS-RUTV.
           PERFORM VAL-GLS.
           MOVE IDC-GLS-RUTV TO FRM-GLS-IAVA IN CUF-FLD.
       FIN-VAL-FRM-GLS-IAVA.
           EXIT.
       
       VAL-FRM-NUM-IAVA SECTION.
       INI-VAL-FRM-NUM-IAVA.
       FIN-VAL-FRM-NUM-IAVA.
           EXIT.
       
       VAL-FRM-VRF-IAVA SECTION.
       INI-VAL-FRM-VRF-IAVA.
           IF FRM-VRF-IAVA IN CUF-FLD NOT > SPACES
              MOVE SPACES TO FRM-VRF-IAVA IN CUF-FLD.
      *VAL-RUT Valida Rut para campo IDC
           MOVE IDC-ZERO-S TO IDC-ZERO.
           MOVE FRM-NUM-IAVA IN CUF-FLD TO IDC-NUM-RUTV.
           MOVE FRM-VRF-IAVA IN CUF-FLD TO IDC-VRF-RUTV.
           PERFORM VAL-RUT.
       FIN-VAL-FRM-VRF-IAVA.
           EXIT.
       
       VAL-FRM-CAI-IAVA SECTION.
       INI-VAL-FRM-CAI-IAVA.
       FIN-VAL-FRM-CAI-IAVA.
           EXIT.
       
       VAL-FRM-IIC-IAVA SECTION.
       INI-VAL-FRM-IIC-IAVA.
       FIN-VAL-FRM-IIC-IAVA.
           EXIT.
       
       VAL-CUF-COD-UFAD SECTION.
       INI-VAL-CUF-COD-UFAD.
       FIN-VAL-CUF-COD-UFAD.
           EXIT.
       
       VAL-CUF-FEC-FCUF SECTION.
       INI-VAL-CUF-FEC-FCUF.
      *VAL-FEC Validar Fecha
           MOVE CUF-FEC-FCUF IN CUF-FLD TO FEC-FECH.
           MOVE FEC-FORM-FEC TO FEC-FORM.
           MOVE FEC-FBLK-S  TO FEC-FBLK.
           MOVE FEC-CHOY-LE TO FEC-CHOY.
           MOVE FEC-VHBL-S TO FEC-VHBL.
      *<<<< INI_CUF_FEC_FCUF
           IF CUF-FEC-FCUF IN CUF-FLD NOT > ZEROES
               PERFORM GET-FHOY
               MOVE HOY-DHOY TO CUF-NUM-DCUF IN CUF-FLD
               MOVE HOY-MHOY TO CUF-NUM-MCUF IN CUF-FLD
               MOVE HOY-SHOY TO CUF-NUM-SCUF IN CUF-FLD
               MOVE HOY-AHOY TO CUF-NUM-ACUF IN CUF-FLD
               MOVE CUF-FEC-FCUF IN CUF-FLD TO FEC-FECH.
      *AAA Valida fecha cambio ubicacion fisica sea igual a fecha de
      *    hoy 12-02-1991
           MOVE FEC-CHOY-E TO FEC-CHOY.
      *>>>>
           MOVE FEC-VAL-FEC TO FEC-CMND.
           PERFORM PRO-FEC.
           IF FEC-STAT-OKS
               MOVE FEC-FECH TO CUF-FEC-FCUF IN CUF-FLD
           ELSE
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE FEC-MENS     TO FRM-MENS
               GO TO FIN-VAL-CUF-FEC-FCUF.
      *<<<< CUF_FEC_FCUF
           IF CUF-NUM-ICUF IN CUF-FLD > 1
               GO TO FCUF-VAR-CMBS.
      *    leer OPC y comparar las fechas
           MOVE CUF-CAI-IOPC IN CUF-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE CUF-IIC-IOPC IN CUF-FLD TO OPC-IIC-IOPC IN OPC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-OPC.
           IF NOT FIO-STAT-OKS
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'OPE    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CUF-FEC-FCUF.
           MOVE CUF-NUM-DCUF IN CUF-FLD  TO WSS-NUM-DCOM IN WSS-VARI.
           MOVE CUF-NUM-MCUF IN CUF-FLD  TO WSS-NUM-MCOM IN WSS-VARI.
           MOVE CUF-NUM-SCUF IN CUF-FLD  TO WSS-NUM-SCOM IN WSS-VARI.
           MOVE CUF-NUM-ACUF IN CUF-FLD  TO WSS-NUM-ACOM IN WSS-VARI.
           IF WSS-FEC-FCOM IN WSS-VARI < OPC-FEC-FCOL IN OPC
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'FECFCUFFCOL<' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
           GO TO FIN-VAL-CUF-FEC-FCUF.
       FCUF-VAR-CMBS.
      *    restar 1 al NRO-CAMBIO-ACTUAL y comparar las fechas
           SUBTRACT 1 FROM   CUF-NUM-ICUF IN CUF-FLD
                      GIVING CUF-NUM-ICUF IN CUF.
           MOVE CUF-CAI-IOPC IN CUF-FLD TO CUF-CAI-IOPC IN CUF.
           MOVE CUF-IIC-IOPC IN CUF-FLD TO CUF-IIC-IOPC IN CUF.
           MOVE CUF-NUM-IDLC IN CUF-FLD TO CUF-NUM-IDLC IN CUF.
      *     MOVE CUF-NUM-ICUF IN CUF-FLD TO CUF-NUM-ICUF IN CUF.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-CUF.
      *     ADD 1 TO CUF-NUM-ICUF IN CUF-FLD.
           IF FIO-STAT-OKS
               MOVE CUF-NUM-DCUF IN CUF-FLD  TO WSS-NUM-DCOM IN WSS-VARI
               MOVE CUF-NUM-MCUF IN CUF-FLD  TO WSS-NUM-MCOM IN WSS-VARI
               MOVE CUF-NUM-SCUF IN CUF-FLD  TO WSS-NUM-SCOM IN WSS-VARI
               MOVE CUF-NUM-ACUF IN CUF-FLD  TO WSS-NUM-ACOM IN WSS-VARI
               IF WSS-FEC-FCOM IN WSS-VARI < CUF-FEC-FCUF IN CUF
                   MOVE 'COL' TO MSG-COD-SIST
                   MOVE 'FCUFFCUFANT<' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   GO TO FIN-VAL-CUF-FEC-FCUF
               ELSE
                   GO TO FIN-VAL-CUF-FEC-FCUF.
      *>>>>
       FIN-VAL-CUF-FEC-FCUF.
           EXIT.
       
       VAL-FRM-GLS-UFAD SECTION.
       INI-VAL-FRM-GLS-UFAD.
       FIN-VAL-FRM-GLS-UFAD.
           EXIT.
       
       VAL-CUF-FEC-FING SECTION.
       INI-VAL-CUF-FEC-FING.
      *VAL-FEC Validar Fecha
           MOVE CUF-FEC-FING IN CUF-FLD TO FEC-FECH.
           MOVE FEC-FORM-FEC TO FEC-FORM.
           MOVE FEC-FBLK-S  TO FEC-FBLK.
           MOVE FEC-CHOY-LE TO FEC-CHOY.
           MOVE FEC-VHBL-S TO FEC-VHBL.
      *<<<< INI_CUF_FEC_FING
           IF CUF-FEC-FING IN CUF-FLD NOT > ZEROES
                MOVE CUF-FEC-FCUF IN CUF-FLD TO CUF-FEC-FING IN CUF-FLD
                                                FEC-FECH.
      *AAA Valida fecha cambio contable sea igual a fecha de hoy
      *    12-02-1991
           MOVE FEC-CHOY-E TO FEC-CHOY.
      *>>>>
           MOVE FEC-VAL-FEC TO FEC-CMND.
           PERFORM PRO-FEC.
           IF FEC-STAT-OKS
               MOVE FEC-FECH TO CUF-FEC-FING IN CUF-FLD
           ELSE
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE FEC-MENS     TO FRM-MENS
               GO TO FIN-VAL-CUF-FEC-FING.
      *<<<< CUF_FEC_FING

      *     MOVE CUF-FEC-FCUF IN CUF-FLD TO WSS-FEC-FEC1 IN WSS-VARI.
           MOVE CUF-NUM-SCUF IN CUF-FLD TO WSS-NUM-SEC1 IN WSS-VARI.
           MOVE CUF-NUM-ACUF IN CUF-FLD TO WSS-NUM-AEC1 IN WSS-VARI.
           MOVE CUF-NUM-MCUF IN CUF-FLD TO WSS-NUM-MEC1 IN WSS-VARI.
           MOVE CUF-NUM-DCUF IN CUF-FLD TO WSS-NUM-DEC1 IN WSS-VARI.
           
      *     MOVE CUF-FEC-FING IN CUF-FLD TO WSS-FEC-FEC2 IN WSS-VARI.
           MOVE CUF-NUM-SING IN CUF-FLD TO WSS-NUM-SEC2 IN WSS-VARI.
           MOVE CUF-NUM-AING IN CUF-FLD TO WSS-NUM-AEC2 IN WSS-VARI.
           MOVE CUF-NUM-MING IN CUF-FLD TO WSS-NUM-MEC2 IN WSS-VARI.
           MOVE CUF-NUM-DING IN CUF-FLD TO WSS-NUM-DEC2 IN WSS-VARI.

           IF WSS-FEC-FEC2 IN WSS-VARI < WSS-FEC-FEC1 IN WSS-VARI
      *GET-MSG Busca mensaje en tabla
      *        fecha nueva ubicacion < fecha ubicacion anterior
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'FECFINGFCUF<' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CUF-FEC-FING.
      *>>>>
       FIN-VAL-CUF-FEC-FING.
           EXIT.
       
       VAL-CUF-COD-UBFD SECTION.
       INI-VAL-CUF-COD-UBFD.
           IF CUF-COD-UBFD IN CUF-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-CUF-COD-UBFD.
      *<<<< CUF_COD_UBFD
           MOVE CUF-COD-UBFD IN CUF-FLD TO WSS-UBFD IN WSS-VARI.
           IF WSS-GLS-UFD9 IN WSS-VARI > SPACES
      *       AND  WSS-TAB-UBFD IN WSS-VARI = 'OFI'
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'COD    INV' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CUF-COD-UBFD.
      * AAA Elimina validacion de cambio de ubicacion fisica a oficina
      *     original 12-02-1991.
      *     IF WSS-GLS-UFD3 IN WSS-VARI = OPC-COD-OFOC IN CUF-FLD
      *         MOVE 'COL' TO MSG-COD-SIST
      *         MOVE 'CODUBFDOFOC=' TO MSG-COD-MENS
      *         PERFORM GET-MSG
      *         MOVE MSG-GLS-DESC TO FRM-MENS
      *         MOVE FRM-SUAR-MAL TO FRM-SUAR
      *         GO TO FIN-VAL-CUF-COD-UBFD.
           IF CUF-COD-UBFD IN CUF-FLD = CUF-COD-UFAD IN CUF-FLD
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'CODUFADUBFD=' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CUF-COD-UBFD.

       OFI-VAL-CUF-COD-UBFD.
           IF WSS-TAB-UBFD NOT = 'OFI'
               GO TO BCO-VAL-CUF-COD-UBFD.
           IF WSS-COD-UBFD IN WSS-UBFD NOT > SPACES
               PERFORM ERR-GLS
               GO TO FIN-VAL-CUF-COD-UBFD.
           MOVE WSS-COD-UBFD IN WSS-UBFD TO OFI-COD-OFIC IN OFI.
           PERFORM VAL-OFI.
           IF MSG-COD-MENS > SPACES
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
           IF MSG-COD-MENS = 'COD    NEX'
               MOVE SPACES TO FRM-GLS-UBFD IN CUF-FLD
           ELSE
               MOVE OFI-GLS-DESC IN OFI TO FRM-GLS-UBFD IN CUF-FLD.
           GO TO FIN-VAL-CUF-COD-UBFD.
       BCO-VAL-CUF-COD-UBFD.
           IF WSS-TAB-UBFD NOT = 'BCO'
               GO TO IVB-VAL-CUF-COD-UBFD.
           IF WSS-COD-UBFD IN WSS-UBFD NOT > SPACES
               PERFORM ERR-GLS
               GO TO FIN-VAL-CUF-COD-UBFD.
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'BCO'                    TO TAB-COD-TTAB IN TAB.
           MOVE WSS-COD-UBFD IN WSS-UBFD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT = 'COD    NEX' AND
              WSS-COD-UBFD IN WSS-UBFD > SPACES
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-UBFD IN CUF-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-UBFD IN CUF-FLD.
           GO TO FIN-VAL-CUF-COD-UBFD.
       IVB-VAL-CUF-COD-UBFD.
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'IVB' TO TAB-COD-TTAB IN TAB.
           MOVE WSS-TAB-UBFD IN WSS-UBFD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT = 'COD    NEX' AND
              CUF-COD-UBFD IN CUF-FLD > SPACES
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-UBFD IN CUF-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-UBFD IN CUF-FLD.
           IF FRM-SUAR = FRM-SUAR-MAL
               IF MSG-COD-MENS = 'COD    NEX'
                   MOVE 'COL' TO MSG-COD-SIST
                   MOVE 'CODUBFDIVB<>' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   GO TO FIN-VAL-CUF-COD-UBFD
               ELSE
                   GO TO FIN-VAL-CUF-COD-UBFD.
           IF WSS-COD-UBFD IN WSS-UBFD NOT > SPACES
               PERFORM ERR-GLS                    
               GO TO FIN-VAL-CUF-COD-UBFD.
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE WSS-TAB-UBFD IN WSS-UBFD TO TAB-COD-TTAB IN TAB.
           MOVE WSS-COD-UBFD IN WSS-UBFD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-TAB.
           IF MSG-COD-MENS NOT = 'COD    NEX' AND
              WSS-COD-UBFD IN WSS-UBFD > SPACES
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-UBFD IN CUF-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-UBFD IN CUF-FLD.
      *>>>>
       FIN-VAL-CUF-COD-UBFD.
           EXIT.
       
       VAL-CUF-COD-RCUF SECTION.
       INI-VAL-CUF-COD-RCUF.
       FIN-VAL-CUF-COD-RCUF.
           EXIT.
       
       VAL-FRM-GLS-UBFD SECTION.
       INI-VAL-FRM-GLS-UBFD.
       FIN-VAL-FRM-GLS-UBFD.
           EXIT.
       
       VAL-FRM-GLS-RCUF SECTION.
       INI-VAL-FRM-GLS-RCUF.
       FIN-VAL-FRM-GLS-RCUF.
           EXIT.
       
       VAL-DLC-COD-UBFD SECTION.
       INI-VAL-DLC-COD-UBFD.
       FIN-VAL-DLC-COD-UBFD.
           EXIT.
       
       VAL-CON-KEY-CUF SECTION.
       INI-VAL-CON-KEY-CUF.
      *<<<< INI_VAL_CON_KEY_CUF
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8
               GO TO FIN-VAL-CON-KEY-CUF.
      *>>>>
           IF     CUF-CAI-IOPC IN CUF-FLD NOT > SPACES 
              AND CUF-IIC-IOPC IN CUF-FLD NOT > ZEROES 
              AND CUF-NUM-IDLC IN CUF-FLD NOT > ZEROES 
              AND CUF-NUM-ICUF IN CUF-FLD NOT > ZEROES 
              AND FRM-GLS-NOMC IN CUF-FLD NOT > SPACES 
              AND FRM-IND-ICLI IN CUF-FLD NOT > SPACES 
              AND FRM-GLS-ICLI IN CUF-FLD NOT > SPACES 
              AND FRM-NUM-ICLI IN CUF-FLD NOT > ZEROES 
              AND FRM-VRF-ICLI IN CUF-FLD NOT > SPACES 
              AND FRM-CAI-ICLI IN CUF-FLD NOT > SPACES 
              AND FRM-IIC-ICLI IN CUF-FLD NOT > SPACES 
              AND FRM-GLS-NOMA IN CUF-FLD NOT > SPACES 
              AND FRM-IND-IAVA IN CUF-FLD NOT > SPACES 
              AND FRM-GLS-IAVA IN CUF-FLD NOT > SPACES 
              AND FRM-NUM-IAVA IN CUF-FLD NOT > ZEROES 
              AND FRM-VRF-IAVA IN CUF-FLD NOT > SPACES 
              AND FRM-CAI-IAVA IN CUF-FLD NOT > SPACES 
              AND FRM-IIC-IAVA IN CUF-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CUF.
      *<<<< FIN_VAL_CON_KEY_CUF
           IF NOT ( ( CUF-IIC-IOPC IN CUF-FLD > ZEROES   AND
                      CUF-NUM-IDLC IN CUF-FLD > ZEROES   AND
                      CUF-NUM-ICUF IN CUF-FLD > ZEROES ) OR

                    ( CUF-IIC-IOPC IN CUF-FLD     > ZEROES   AND
                      CUF-NUM-IDLC IN CUF-FLD     > ZEROES   AND
                      CUF-NUM-ICUF IN CUF-FLD NOT > ZEROES ) OR

                    ( FRM-NUM-ICLI IN CUF-FLD > ZEROES   AND 
                      FRM-NUM-IAVA IN CUF-FLD > ZEROES ) OR

                    ( FRM-NUM-ICLI IN CUF-FLD > ZEROES ) OR

                    ( FRM-NUM-IAVA IN CUF-FLD > ZEROES ) OR

                    ( FRM-GLS-NOMC IN CUF-FLD > SPACES   AND
                      FRM-GLS-NOMA IN CUF-FLD > SPACES ) OR

                    ( FRM-GLS-NOMC IN CUF-FLD > SPACES ) OR

                    ( FRM-GLS-NOMA IN CUF-FLD > SPACES ) )
               MOVE 'GNS'          TO MSG-COD-SIST
               MOVE 'VALCONKEY' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC   TO FRM-MENS
               MOVE FRM-SUAR-MAL   TO FRM-SUAR.
      *>>>>
       FIN-VAL-CON-KEY-CUF.
           EXIT.
       
       VAL-CON-IKY-CUF SECTION.
       INI-VAL-CON-IKY-CUF.
           IF     CUF-CAI-IOPC IN CUF-FLD NOT > SPACES 
              AND CUF-IIC-IOPC IN CUF-FLD NOT > ZEROES 
              AND CUF-NUM-IDLC IN CUF-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CUF.
      *<<<< FIN_VAL_CON_IKY_CUF
           IF CUF-IIC-IOPC  IN CUF-FLD NOT > ZEROES
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'IICIOPCBLK' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
           ELSE
               IF CUF-NUM-IDLC IN CUF-FLD NOT > ZEROES
                   MOVE 'GNS'          TO MSG-COD-SIST
                   MOVE 'VALCONKEY' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC   TO FRM-MENS
                   MOVE FRM-SUAR-MAL   TO FRM-SUAR.
      *>>>>
       FIN-VAL-CON-IKY-CUF.
           EXIT.
       
       VAL-CON-FLD-CUF SECTION.
       INI-VAL-CON-FLD-CUF.
      *<<<< VAL_CON_FLD_CUF
           IF CUF-FEC-FCUF IN CUF-FLD     > ZEROES AND
              CUF-FEC-FING IN CUF-FLD     > ZEROES
               MOVE CUF-FEC-FCUF IN CUF-FLD TO FEC-FEC1
               MOVE CUF-FEC-FING IN CUF-FLD TO FEC-FEC2
               MOVE FEC-FORM-FEC TO FEC-FORM
               MOVE FEC-CMP-FEC  TO FEC-CMND
               PERFORM PRO-FEC
               IF FEC-STAT-GRT
                   MOVE 'COL' TO MSG-COD-SIST
                   MOVE 'FECFCUFFING>' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   GO TO FIN-VAL-CON-FLD-CUF.

           IF CUF-COD-RCUF IN CUF-FLD > SPACES AND
              WSS-TAB-UBFD = 'OFI'
               PERFORM GNS-ERR-QUE
               MOVE EIBTRMID     TO QUE-TERM
               PERFORM GNS-BUS-OPID
               MOVE OPI-OPID     TO QUE-TYPE
               MOVE 1            TO QUE-NITM
               MOVE QUE-GET      TO QUE-CMND
               MOVE +150         TO QUE-LITM
               PERFORM GNS-PRO-QUE
               IF QUE-STAT NOT = QUE-STAT-OKS
                   MOVE 'COL' TO MSG-COD-SIST
                   MOVE 'NOEXISTQUEUE' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   GO TO FIN-VAL-CON-FLD-CUF
               ELSE
                   MOVE QUE-ITEM TO WSS-ITEM IN WSS-VARI
                   IF WSS-OFIC IN WSS-VARI NOT = WSS-GLS-UFD3
                                                  IN WSS-VARI
                       MOVE 'COL' TO MSG-COD-SIST
                       MOVE 'OFINUBINERES' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE FRM-SUAR-MAL TO FRM-SUAR
                       GO TO FIN-VAL-CON-FLD-CUF.
      *>>>>
       FIN-VAL-CON-FLD-CUF.
           EXIT.
       
       VAL-CON-CUF SECTION.
       INI-VAL-CON-CUF.
       FIN-VAL-CON-CUF.
           EXIT.
       
       VAL-NUM-KEY-CUF SECTION.
       INI-VAL-NUM-KEY-CUF.
           PERFORM VAL-NUM-IKY-CUF.
           PERFORM VAL-NUM-MKY-CUF.
           PERFORM VAL-NUM-AKY-CUF.
       FIN-VAL-NUM-KEY-CUF.
           EXIT.
       
       VAL-NUM-IKY-CUF SECTION.
       INI-VAL-NUM-IKY-CUF.
       VAL-NUM-CUF-IIC-IOPC.
           MOVE CUF-IIC-IOPC-ALF IN CUF-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           CALL 'GNSPVNUM' USING NUM-VARI.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CUF-IIC-IOPC IN CUF-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CUF-IIC-IOPC-LEN IN CUF-FLD
                GO TO FIN-VAL-NUM-IKY-CUF.
       VAL-NUM-CUF-NUM-IDLC.
           MOVE CUF-NUM-IDLC-ALF IN CUF-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           CALL 'GNSPVNUM' USING NUM-VARI.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CUF-NUM-IDLC IN CUF-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CUF-NUM-IDLC-LEN IN CUF-FLD
                GO TO FIN-VAL-NUM-IKY-CUF.
       FIN-VAL-NUM-IKY-CUF.
           EXIT.
       
       VAL-NUM-MKY-CUF SECTION.
       INI-VAL-NUM-MKY-CUF.
       FIN-VAL-NUM-MKY-CUF.
           EXIT.
       
       VAL-NUM-AKY-CUF SECTION.
       INI-VAL-NUM-AKY-CUF.
       VAL-NUM-CUF-NUM-ICUF.
           MOVE CUF-NUM-ICUF-ALF IN CUF-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 2 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           CALL 'GNSPVNUM' USING NUM-VARI.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CUF-NUM-ICUF IN CUF-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CUF-NUM-ICUF-LEN IN CUF-FLD
                GO TO FIN-VAL-NUM-AKY-CUF.
       VAL-NUM-FRM-NUM-ICLI.
           MOVE FRM-NUM-ICLI-ALF IN CUF-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           CALL 'GNSPVNUM' USING NUM-VARI.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO FRM-NUM-ICLI IN CUF-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO FRM-NUM-ICLI-LEN IN CUF-FLD
                GO TO FIN-VAL-NUM-AKY-CUF.
       VAL-NUM-FRM-NUM-IAVA.
           MOVE FRM-NUM-IAVA-ALF IN CUF-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           CALL 'GNSPVNUM' USING NUM-VARI.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO FRM-NUM-IAVA IN CUF-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO FRM-NUM-IAVA-LEN IN CUF-FLD
                GO TO FIN-VAL-NUM-AKY-CUF.
       FIN-VAL-NUM-AKY-CUF.
           EXIT.
       
       VAL-NUM-FLD-CUF SECTION.
       INI-VAL-NUM-FLD-CUF.
       FIN-VAL-NUM-FLD-CUF.
           EXIT.
       
       EDT-KEY-CUF SECTION.
       INI-EDT-KEY-CUF.
           PERFORM EDT-IKY-CUF.
           PERFORM EDT-MKY-CUF.
           PERFORM EDT-AKY-CUF.
       FIN-EDT-KEY-CUF.
           EXIT.
       
       EDT-IKY-CUF SECTION.
       INI-EDT-IKY-CUF.
           IF CUF-IIC-IOPC-LEN IN CUF-FLD = -9
                MOVE -1 TO CUF-IIC-IOPC-LEN IN CUF-FLD
                GO TO FIN-EDT-IKY-CUF.
           MOVE CUF-IIC-IOPC IN CUF-FLD TO CUF-IIC-IOPC-EDT IN CUF-FLD.
           IF CUF-NUM-IDLC-LEN IN CUF-FLD = -9
                MOVE -1 TO CUF-NUM-IDLC-LEN IN CUF-FLD
                GO TO FIN-EDT-IKY-CUF.
           MOVE CUF-NUM-IDLC IN CUF-FLD TO CUF-NUM-IDLC-EDT IN CUF-FLD.
       FIN-EDT-IKY-CUF.
           EXIT.
       
       EDT-MKY-CUF SECTION.
       INI-EDT-MKY-CUF.
       FIN-EDT-MKY-CUF.
           EXIT.
       
       EDT-AKY-CUF SECTION.
       INI-EDT-AKY-CUF.
           IF CUF-NUM-ICUF-LEN IN CUF-FLD = -9
                MOVE -1 TO CUF-NUM-ICUF-LEN IN CUF-FLD
                GO TO FIN-EDT-AKY-CUF.
           MOVE CUF-NUM-ICUF IN CUF-FLD TO CUF-NUM-ICUF-EDT IN CUF-FLD.
           IF FRM-NUM-ICLI-LEN IN CUF-FLD = -9
                MOVE -1 TO FRM-NUM-ICLI-LEN IN CUF-FLD
                GO TO FIN-EDT-AKY-CUF.
           MOVE FRM-NUM-ICLI IN CUF-FLD TO FRM-NUM-ICLI-EDT IN CUF-FLD.
           IF FRM-NUM-IAVA-LEN IN CUF-FLD = -9
                MOVE -1 TO FRM-NUM-IAVA-LEN IN CUF-FLD
                GO TO FIN-EDT-AKY-CUF.
           MOVE FRM-NUM-IAVA IN CUF-FLD TO FRM-NUM-IAVA-EDT IN CUF-FLD.
       FIN-EDT-AKY-CUF.
           EXIT.
       
       EDT-FLD-CUF SECTION.
       INI-EDT-FLD-CUF.
       FIN-EDT-FLD-CUF.
           EXIT.
