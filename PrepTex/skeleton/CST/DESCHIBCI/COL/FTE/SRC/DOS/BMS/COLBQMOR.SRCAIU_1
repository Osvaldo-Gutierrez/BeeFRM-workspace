      *
      * COLBQMOR
      * Version 3.0.1 Nov-2005
      * 09-Sep-14 11:16 AM
      *
       SET-TAG-COLBQMOR SECTION.
       INI-SET-TAG-COLBQMOR.
           MOVE
           '<IDNMOD NOM=$COLBQMOR$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$09-Sep-14 11:16 AM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBQMOR.
           EXIT.
           
       PUT-ALL-MOR SECTION.
       INI-PUT-ALL-MOR.
           PERFORM PUT-MOR-MOR.
       FIN-PUT-ALL-MOR.
           EXIT.
       
       PUT-MOR-MOR SECTION.
       INI-PUT-MOR-MOR.
           IF PGM-STAT-MOR-NUL
               GO TO FIN-PUT-MOR-MOR.
           IF NOT PGM-STAT-MOR-OKS
               PERFORM CLR-MOR-MOR
               GO TO FIN-PUT-MOR-MOR.
           MOVE MOR-COD-OFI IN MOR TO MOR-COD-OFI IN MOR-FLD.
      *BUS-OFI busca oficina en tablas
           IF MOR-COD-OFI IN MOR-FLD > SPACES
               MOVE MOR-COD-OFI IN MOR-FLD TO OFI-COD-OFIC IN OFI
               PERFORM BUS-OFI
               MOVE OFI-GLS-DESC IN OFI TO FRM-GLS-OFI IN MOR-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-OFI IN MOR-FLD.
       FIN-PUT-MOR-MOR.
           EXIT.
       
       KEY-ALL-MOR SECTION.
       INI-KEY-ALL-MOR.
           PERFORM KEY-MOR-MOR.
       FIN-KEY-ALL-MOR.
           EXIT.
       
       KEY-MOR-MOR SECTION.
       INI-KEY-MOR-MOR.
           MOVE MOR-COD-OFI IN MOR-FLD TO MOR-COD-OFI IN MOR.
       FIN-KEY-MOR-MOR.
           EXIT.
       
       SCR-CLRA-MOR SECTION.
       INI-SCR-CLRA-MOR.
           PERFORM CLR-KEY-MOR.
           PERFORM CLR-FLD-MOR.
       FIN-SCR-CLRA-MOR.
           EXIT.
       
       CLR-KEY-MOR SECTION.
       INI-CLR-KEY-MOR.
           MOVE ZEROES TO MOR-COD-OFI IN MOR-FLD.
           MOVE SPACES TO FRM-COD-EJE IN MOR-FLD.
       FIN-CLR-KEY-MOR.
           EXIT.
       
       CLR-FLD-MOR SECTION.
       INI-CLR-FLD-MOR.
           MOVE SPACES TO FRM-INF-DES IN MOR-FLD.
           MOVE SPACES TO FRM-INF-HAS IN MOR-FLD.
           MOVE SPACES TO FRM-GLS-OFI IN MOR-FLD.
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(1).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(2).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(3).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(4).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(5).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(6).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(7).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(8).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(9).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(10).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(11).
           MOVE SPACES TO FRM-LIN-DET IN MOR-FLD(12).
           PERFORM CLR-MOR-MOR.
      *<<<
           IF (SCR-CAR-FEC NOT = 'CARGA')
               MOVE 'CARGA' TO SCR-CAR-FEC
               MOVE HIGH-VALUE TO MOR-KEY-IMOR
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM COL-FIO-MOR
               IF NOT FIO-STAT-OKS
                  MOVE 'PROBLEMAS LA LEER COLVMOR CON HIGH VALUE' TO
                  FRM-MENS
               ELSE
                  MOVE MOR-NOM-CLI IN MOR TO SCR-FEC-INF.

           MOVE SCR-INF-DES TO FRM-INF-DES
           MOVE SCR-INF-HAS TO FRM-INF-HAS.
      *>>>
       FIN-CLR-FLD-MOR.
           EXIT.
       
       CLR-MOR-MOR SECTION.
       INI-CLR-MOR-MOR.
       FIN-CLR-MOR-MOR.
           EXIT.
       
       FST-KEY-MOR SECTION.
       INI-FST-KEY-MOR.
           MOVE -1 TO MOR-COD-OFI-LEN IN MOR-FLD.
       FIN-FST-KEY-MOR.
           EXIT.
       
       FST-FLD-MOR SECTION.
       INI-FST-FLD-MOR.
      *<<<<
           GO TO FIN-FST-FLD-MOR.
      *>>>>
           MOVE -1 TO FRM-INF-DES-LEN IN MOR-FLD.
       FIN-FST-FLD-MOR.
           EXIT.
       
       PRO-KEY-MOR SECTION.
       INI-PRO-KEY-MOR.
           MOVE FRM-CPIM-AEY TO FRM-COD-EJE-ATR IN MOR-FLD.
           PERFORM PRO-IKY-MOR.
           PERFORM PRO-AKY-MOR.
           PERFORM PRO-MKY-MOR.
       FIN-PRO-KEY-MOR.
           EXIT.
       
       PRO-IKY-MOR SECTION.
       INI-PRO-IKY-MOR.
           MOVE FRM-CPIM-AEY TO MOR-COD-OFI-ATR IN MOR-FLD.
      *<<<<
           MOVE FRM-CPIM-AEY TO FRM-COD-EJE-ATR IN MOR-FLD.             
      *>>>>
       FIN-PRO-IKY-MOR.
           EXIT.
       
       PRO-MKY-MOR SECTION.
       INI-PRO-MKY-MOR.
       FIN-PRO-MKY-MOR.
           EXIT.
       
       PRO-AKY-MOR SECTION.
       INI-PRO-AKY-MOR.
       FIN-PRO-AKY-MOR.
           EXIT.
       
       UNP-KEY-MOR SECTION.
       INI-UNP-KEY-MOR.
           MOVE FRM-CPIM-UBY TO FRM-COD-EJE-ATR IN MOR-FLD.
           PERFORM UNP-IKY-MOR.
           PERFORM UNP-AKY-MOR.
           PERFORM UNP-MKY-MOR.
       FIN-UNP-KEY-MOR.
           EXIT.
       
       UNP-IKY-MOR SECTION.
       INI-UNP-IKY-MOR.
           MOVE FRM-CPIM-UBY TO MOR-COD-OFI-ATR.
           MOVE FRM-CPIM-UBY TO FRM-COD-EJE-ATR.
       FIN-UNP-IKY-MOR.
           EXIT.
       
       UNP-MKY-MOR SECTION.
       INI-UNP-MKY-MOR.
       FIN-UNP-MKY-MOR.
           EXIT.
       
       UNP-AKY-MOR SECTION.
       INI-UNP-AKY-MOR.
       FIN-UNP-AKY-MOR.
           EXIT.
       
       UNP-FLD-MOR SECTION.
       INI-UNP-FLD-MOR.
      *<<<< 
           GO TO FIN-UNP-FLD-MOR.
      *>>>>
           MOVE FRM-CPIM-UBY TO FRM-INF-DES-ATR IN MOR-FLD.
           MOVE FRM-CPIM-UBY TO FRM-INF-HAS-ATR IN MOR-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-OFI-ATR IN MOR-FLD.
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(1).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(2).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(3).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(4).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(5).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(6).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(7).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(8).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(9).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(10).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(11).
           MOVE FRM-CPIM-UBY TO FRM-LIN-DET-ATR IN MOR-FLD(12).
           PERFORM UNP-MOR-MOR.
       FIN-UNP-FLD-MOR.
           EXIT.
       
       UNP-MOR-MOR SECTION.
       INI-UNP-MOR-MOR.
       FIN-UNP-MOR-MOR.
           EXIT.
       
       VAL-KEY-MOR SECTION.
       INI-VAL-KEY-MOR.
           PERFORM VAL-IKY-MOR.
           PERFORM VAL-MKY-MOR.
           PERFORM VAL-AKY-MOR.
       FIN-VAL-KEY-MOR.
           EXIT.
       
       VAL-IKY-MOR SECTION.
       INI-VAL-IKY-MOR.
           PERFORM VAL-MOR-COD-OFI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO MOR-COD-OFI-LEN IN MOR-FLD
                GO TO FIN-VAL-IKY-MOR.
           PERFORM VAL-FRM-COD-EJE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-EJE-LEN IN MOR-FLD
                GO TO FIN-VAL-IKY-MOR.
       FIN-VAL-IKY-MOR.
           EXIT.
       
       VAL-MKY-MOR SECTION.
       INI-VAL-MKY-MOR.
       FIN-VAL-MKY-MOR.
           EXIT.
       
       VAL-AKY-MOR SECTION.
       INI-VAL-AKY-MOR.
       FIN-VAL-AKY-MOR.
           EXIT.
       
       VAL-MOR-COD-OFI SECTION.
       INI-VAL-MOR-COD-OFI.
      *<<< INI_MOR_COD_OFI
           IF MOR-COD-OFI IN MOR-FLD = ZEROES                           
                PERFORM ERR-GLS                                         
                GO TO FIN-VAL-MOR-COD-OFI.                              
      *>>>
      *VAL-OFI Valida codigo de Oficina en TABLAS
           MOVE MOR-COD-OFI IN MOR-FLD TO OFI-COD-OFIC IN OFI.
           PERFORM VAL-OFI.
           IF MSG-COD-MENS NOT = SPACES
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
           IF MSG-COD-MENS = 'COD    NEX'
               MOVE SPACES TO FRM-GLS-OFI IN MOR-FLD
           ELSE
               MOVE OFI-GLS-DESC IN OFI TO FRM-GLS-OFI IN MOR-FLD.
       FIN-VAL-MOR-COD-OFI.
           EXIT.
       
       VAL-FRM-COD-EJE SECTION.
       INI-VAL-FRM-COD-EJE.
           IF FRM-COD-EJE IN MOR-FLD NOT > SPACES
              MOVE SPACES TO FRM-COD-EJE IN MOR-FLD.
       FIN-VAL-FRM-COD-EJE.
           EXIT.
       
       VAL-CON-KEY-MOR SECTION.
       INI-VAL-CON-KEY-MOR.
           IF     MOR-COD-OFI IN MOR-FLD NOT > ZEROES 
              AND FRM-COD-EJE IN MOR-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-MOR.
       FIN-VAL-CON-KEY-MOR.
           EXIT.
       
       VAL-CON-IKY-MOR SECTION.
       INI-VAL-CON-IKY-MOR.
           IF     MOR-COD-OFI IN MOR-FLD NOT > ZEROES 
              AND FRM-COD-EJE IN MOR-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-MOR.
       FIN-VAL-CON-IKY-MOR.
           EXIT.
       
       VAL-NUM-KEY-MOR SECTION.
       INI-VAL-NUM-KEY-MOR.
           PERFORM VAL-NUM-IKY-MOR.
           PERFORM VAL-NUM-MKY-MOR.
           PERFORM VAL-NUM-AKY-MOR.
       FIN-VAL-NUM-KEY-MOR.
           EXIT.
       
       VAL-NUM-IKY-MOR SECTION.
       INI-VAL-NUM-IKY-MOR.
       VAL-NUM-MOR-COD-OFI.
           MOVE MOR-COD-OFI-ALF IN MOR-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO MOR-COD-OFI IN MOR-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO MOR-COD-OFI-LEN IN MOR-FLD
                GO TO FIN-VAL-NUM-IKY-MOR.
       FIN-VAL-NUM-IKY-MOR.
           EXIT.
       
       VAL-NUM-MKY-MOR SECTION.
       INI-VAL-NUM-MKY-MOR.
       FIN-VAL-NUM-MKY-MOR.
           EXIT.
       
       VAL-NUM-AKY-MOR SECTION.
       INI-VAL-NUM-AKY-MOR.
       FIN-VAL-NUM-AKY-MOR.
           EXIT.
       
       VAL-NUM-FLD-MOR SECTION.
       INI-VAL-NUM-FLD-MOR.
       FIN-VAL-NUM-FLD-MOR.
           EXIT.
       
       EDT-KEY-MOR SECTION.
       INI-EDT-KEY-MOR.
           PERFORM EDT-IKY-MOR.
           PERFORM EDT-MKY-MOR.
           PERFORM EDT-AKY-MOR.
       FIN-EDT-KEY-MOR.
           EXIT.
       
       EDT-IKY-MOR SECTION.
       INI-EDT-IKY-MOR.
           IF MOR-COD-OFI-LEN IN MOR-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO MOR-COD-OFI-LEN IN MOR-FLD
                GO TO FIN-EDT-IKY-MOR.
           MOVE MOR-COD-OFI IN MOR-FLD TO MOR-COD-OFI-EDT IN MOR-EDT.
           MOVE MOR-COD-OFI-EDT IN MOR-EDT TO 
                MOR-COD-OFI-ALF IN MOR-FLD.
       FIN-EDT-IKY-MOR.
           EXIT.
       
       EDT-MKY-MOR SECTION.
       INI-EDT-MKY-MOR.
       FIN-EDT-MKY-MOR.
           EXIT.
       
       EDT-AKY-MOR SECTION.
       INI-EDT-AKY-MOR.
       FIN-EDT-AKY-MOR.
           EXIT.
       
       EDT-FLD-MOR SECTION.
       INI-EDT-FLD-MOR.
       FIN-EDT-FLD-MOR.
           EXIT.
