      *
      * COLBUCYA
      * Version 3.0.1 Nov-2005
      * 10-Jul-19 04:41 PM
      *
       SET-TAG-COLBUCYA SECTION.
       INI-SET-TAG-COLBUCYA.
           MOVE
           '<IDNMOD NOM=$COLBUCYA$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$10-Jul-19 04:41 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUCYA.
           EXIT.
           
       RET-ALL-CYA SECTION.
       INI-RET-ALL-CYA.
           PERFORM KEY-ALL-CYA.
           PERFORM RET-OPC-CYA.
           PERFORM RET-CYA-CYA.
           PERFORM RET-DBC-CYA.
           PERFORM RET-RDC-CYA.
       FIN-RET-ALL-CYA.
           EXIT.
       
       RET-OPC-CYA SECTION.
       INI-RET-OPC-CYA.
           MOVE OPC-MSC-TVOC IN CYA-FLD TO OPC-MSC-TVOC IN OPC.
           MOVE OPC-COD-TOOC IN CYA-FLD TO OPC-COD-TOOC IN OPC.
           MOVE OPC-COD-OFOC IN CYA-FLD TO OPC-COD-OFOC IN OPC.
           MOVE OPC-COD-VCOC IN CYA-FLD TO OPC-COD-VCOC IN OPC.
       FIN-RET-OPC-CYA.
           EXIT.
       
       RET-CYA-CYA SECTION.
       INI-RET-CYA-CYA.
           MOVE CYA-IND-TCYA IN CYA-FLD TO CYA-IND-TCYA IN CYA.
           MOVE CYA-CAI-ICYA IN CYA-FLD TO CYA-CAI-ICYA IN CYA.
           MOVE CYA-IIC-ICYA IN CYA-FLD TO CYA-IIC-ICYA IN CYA.
           MOVE CYA-SGV-LIQU IN CYA-FLD TO CYA-SGV-LIQU IN CYA.
      *<<<< FIN_RET_CYA_CYA
           MOVE SPACES TO CYA-IND-MAUT IN CYA.
           IF CYA-IND-TCYA IN CYA > SPACES
               MOVE 'TAB' TO MSC-COD-SIST
               MOVE 'TCYA' TO MSC-COD-TMSC IN MSC
               MOVE CYA-IND-TCYA IN CYA TO MSC-COD-CMSC IN MSC
               PERFORM BUS-MSC
               MOVE MSC-COD-DAT1(2) TO CYA-IND-MAUT IN CYA.
      *>>>>
       FIN-RET-CYA-CYA.
           EXIT.
       
       RET-DBC-CYA SECTION.
       INI-RET-DBC-CYA.
       FIN-RET-DBC-CYA.
           EXIT.
       
       RET-RDC-CYA SECTION.
       INI-RET-RDC-CYA.
           MOVE RDC-CAI-ICLI IN CYA-FLD TO RDC-CAI-ICLI IN RDC.
           MOVE RDC-IIC-ICLI IN CYA-FLD TO RDC-IIC-ICLI IN RDC.
       FIN-RET-RDC-CYA.
           EXIT.
       
       KEY-ALL-CYA SECTION.
       INI-KEY-ALL-CYA.
           PERFORM KEY-OPC-CYA.
           PERFORM KEY-CYA-CYA.
           PERFORM KEY-DBC-CYA.
           PERFORM KEY-RDC-CYA.
       FIN-KEY-ALL-CYA.
           EXIT.
       
       KEY-OPC-CYA SECTION.
       INI-KEY-OPC-CYA.
       FIN-KEY-OPC-CYA.
           EXIT.
       
       KEY-CYA-CYA SECTION.
       INI-KEY-CYA-CYA.
           MOVE CYA-CAI-IOPC IN CYA-FLD TO CYA-CAI-IOPC IN CYA.
           MOVE CYA-IIC-IOPC IN CYA-FLD TO CYA-IIC-IOPC IN CYA.
           MOVE CYA-NUM-ICAN IN CYA-FLD TO CYA-NUM-ICAN IN CYA.
           MOVE CYA-NUM-ICYA IN CYA-FLD TO CYA-NUM-ICYA IN CYA.
       FIN-KEY-CYA-CYA.
           EXIT.
       
       KEY-DBC-CYA SECTION.
       INI-KEY-DBC-CYA.
           MOVE DBC-GLS-NOMC IN CYA-FLD TO DBC-GLS-NOMC IN DBC.
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN CYA-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN CYA-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN CYA-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN CYA-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
       FIN-KEY-DBC-CYA.
           EXIT.
       
       KEY-RDC-CYA SECTION.
       INI-KEY-RDC-CYA.
       FIN-KEY-RDC-CYA.
           EXIT.
       
       SCR-CLRA-CYA SECTION.
       INI-SCR-CLRA-CYA.
           PERFORM CLR-KEY-CYA.
           PERFORM CLR-FLD-CYA.
       FIN-SCR-CLRA-CYA.
           EXIT.
       
       CLR-KEY-CYA SECTION.
       INI-CLR-KEY-CYA.
           MOVE SPACES TO CYA-CAI-IOPC IN CYA-FLD.
           MOVE ZEROES TO CYA-IIC-IOPC IN CYA-FLD.
           MOVE ZEROES TO CYA-NUM-ICAN IN CYA-FLD.
           MOVE ZEROES TO CYA-NUM-ICYA IN CYA-FLD.
           MOVE SPACES TO DBC-GLS-NOMC IN CYA-FLD.
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN CYA-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN CYA-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN CYA-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN CYA-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
       FIN-CLR-KEY-CYA.
           EXIT.
       
       CLR-FLD-CYA SECTION.
       INI-CLR-FLD-CYA.
           MOVE SPACES TO FRM-GLS-TVOC IN CYA-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN CYA-FLD.
           MOVE SPACES TO FRM-GLS-OFOC IN CYA-FLD.
           MOVE SPACES TO FRM-GLS-TM01 IN CYA-FLD.
           MOVE SPACES TO FRM-GLS-TCYA IN CYA-FLD.
           MOVE SPACES TO FRM-GLS-TM04 IN CYA-FLD.
           MOVE SPACES TO FRM-GLS-TM02 IN CYA-FLD.
           MOVE ZEROES TO FRM-VAL-MONX IN CYA-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN CYA-FLD.
           MOVE SPACES TO FRM-GLS-TM03 IN CYA-FLD.
           MOVE ZEROES TO FRM-VAL-VPES IN CYA-FLD.
           PERFORM CLR-OPC-CYA.
           PERFORM CLR-CYA-CYA.
           PERFORM CLR-DBC-CYA.
           PERFORM CLR-RDC-CYA.
       FIN-CLR-FLD-CYA.
           EXIT.
       
       CLR-OPC-CYA SECTION.
       INI-CLR-OPC-CYA.
           MOVE SPACES TO OPC-MSC-TVOC IN CYA-FLD.
           MOVE SPACES TO OPC-COD-TOOC IN CYA-FLD.
           MOVE SPACES TO OPC-COD-OFOC IN CYA-FLD.
           MOVE SPACES TO OPC-COD-VCOC IN CYA-FLD.
       FIN-CLR-OPC-CYA.
           EXIT.
       
       CLR-CYA-CYA SECTION.
       INI-CLR-CYA-CYA.
           MOVE SPACES TO CYA-IND-TCYA IN CYA-FLD.
           MOVE SPACES TO CYA-CAI-ICYA IN CYA-FLD.
           MOVE SPACES TO CYA-IIC-ICYA IN CYA-FLD.
           MOVE ZEROES TO CYA-SGV-LIQU IN CYA-FLD.
       FIN-CLR-CYA-CYA.
           EXIT.
       
       CLR-DBC-CYA SECTION.
       INI-CLR-DBC-CYA.
       FIN-CLR-DBC-CYA.
           EXIT.
       
       CLR-RDC-CYA SECTION.
       INI-CLR-RDC-CYA.
           MOVE SPACES TO RDC-CAI-ICLI IN CYA-FLD.
           MOVE SPACES TO RDC-IIC-ICLI IN CYA-FLD.
       FIN-CLR-RDC-CYA.
           EXIT.
       
       FST-KEY-CYA SECTION.
       INI-FST-KEY-CYA.
           MOVE -1 TO CYA-CAI-IOPC-LEN IN CYA-FLD.
       FIN-FST-KEY-CYA.
           EXIT.
       
       FST-FLD-CYA SECTION.
       INI-FST-FLD-CYA.
           MOVE -1 TO OPC-MSC-TVOC-LEN IN CYA-FLD.
       FIN-FST-FLD-CYA.
           EXIT.
       
       PRO-KEY-CYA SECTION.
       INI-PRO-KEY-CYA.
           PERFORM PRO-MKY-CYA.
       FIN-PRO-KEY-CYA.
           EXIT.
       
       PRO-MKY-CYA SECTION.
       INI-PRO-MKY-CYA.
       FIN-PRO-MKY-CYA.
           EXIT.
      * IDC-INI --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
           MOVE FRM-CPIM-AEY TO DBC-NUM-ICLI-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO DBC-VRF-ICLI-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO DBC-IND-ICLI-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO DBC-GLS-ICLI-ATR IN CYA-FLD.
      * IDC-FIN --> BMCHIBCI005.SRCAIU PRO ( KEY Y FLD )
       
       PRO-FLD-CYA SECTION.
       INI-PRO-FLD-CYA.
           MOVE FRM-CPIM-AEY TO FRM-GLS-TVOC-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-TOOC-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-OFOC-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-TM01-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-TCYA-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-TM04-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-TM02-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VAL-MONX-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-VCOC-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-TM03-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VAL-VPES-ATR IN CYA-FLD.
           PERFORM PRO-OPC-CYA.
           PERFORM PRO-CYA-CYA.
           PERFORM PRO-DBC-CYA.
           PERFORM PRO-RDC-CYA.
       FIN-PRO-FLD-CYA.
           EXIT.
       
       PRO-OPC-CYA SECTION.
       INI-PRO-OPC-CYA.
           MOVE FRM-CPIM-AEY TO OPC-MSC-TVOC-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO OPC-COD-TOOC-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO OPC-COD-OFOC-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO OPC-COD-VCOC-ATR IN CYA-FLD.
       FIN-PRO-OPC-CYA.
           EXIT.
       
       PRO-CYA-CYA SECTION.
       INI-PRO-CYA-CYA.
           MOVE FRM-CPIM-AEY TO CYA-IND-TCYA-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO CYA-CAI-ICYA-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO CYA-IIC-ICYA-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO CYA-SGV-LIQU-ATR IN CYA-FLD.
       FIN-PRO-CYA-CYA.
           EXIT.
       
       PRO-DBC-CYA SECTION.
       INI-PRO-DBC-CYA.
       FIN-PRO-DBC-CYA.
           EXIT.
       
       PRO-RDC-CYA SECTION.
       INI-PRO-RDC-CYA.
           MOVE FRM-CPIM-AEY TO RDC-CAI-ICLI-ATR IN CYA-FLD.
           MOVE FRM-CPIM-AEY TO RDC-IIC-ICLI-ATR IN CYA-FLD.
       FIN-PRO-RDC-CYA.
           EXIT.
       
       UNP-KEY-CYA SECTION.
       INI-UNP-KEY-CYA.
           PERFORM UNP-IKY-CYA.
           PERFORM UNP-AKY-CYA.
           PERFORM UNP-MKY-CYA.
       FIN-UNP-KEY-CYA.
           EXIT.
       
       UNP-IKY-CYA SECTION.
       INI-UNP-IKY-CYA.
           MOVE FRM-CPIM-UBY TO CYA-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO CYA-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO CYA-NUM-ICAN-ATR.
       FIN-UNP-IKY-CYA.
           EXIT.
       
       UNP-MKY-CYA SECTION.
       INI-UNP-MKY-CYA.
       FIN-UNP-MKY-CYA.
           EXIT.
       
       UNP-AKY-CYA SECTION.
       INI-UNP-AKY-CYA.
           MOVE FRM-CPIM-UBY TO CYA-NUM-ICYA-ATR.
           MOVE FRM-CPIM-UBY TO DBC-GLS-NOMC-ATR.
      * IDC-INI --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO DBC-NUM-ICLI-ATR IN CYA-FLD.
           MOVE FRM-CPIM-UBY TO DBC-VRF-ICLI-ATR IN CYA-FLD.
           MOVE FRM-CPIM-UBY TO DBC-IND-ICLI-ATR IN CYA-FLD.
           MOVE FRM-CPIM-UBY TO DBC-GLS-ICLI-ATR IN CYA-FLD.
      * IDC-FIN --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
       FIN-UNP-AKY-CYA.
           EXIT.
       
       UNP-FLD-CYA SECTION.
       INI-UNP-FLD-CYA.
           MOVE FRM-CPIM-UBY TO FRM-GLS-TVOC-ATR IN CYA-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-TOOC-ATR IN CYA-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-OFOC-ATR IN CYA-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-TM01-ATR IN CYA-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-TCYA-ATR IN CYA-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-TM04-ATR IN CYA-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-TM02-ATR IN CYA-FLD.
           MOVE FRM-CPIM-UBY TO FRM-VAL-MONX-ATR IN CYA-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-VCOC-ATR IN CYA-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-TM03-ATR IN CYA-FLD.
           MOVE FRM-CPIM-UBY TO FRM-VAL-VPES-ATR IN CYA-FLD.
           PERFORM UNP-OPC-CYA.
           PERFORM UNP-CYA-CYA.
           PERFORM UNP-DBC-CYA.
           PERFORM UNP-RDC-CYA.
       FIN-UNP-FLD-CYA.
           EXIT.
       
       UNP-OPC-CYA SECTION.
       INI-UNP-OPC-CYA.
           MOVE FRM-CPIM-UBY TO OPC-MSC-TVOC-ATR IN CYA-FLD.
           MOVE FRM-CPIM-UBY TO OPC-COD-TOOC-ATR IN CYA-FLD.
           MOVE FRM-CPIM-UBY TO OPC-COD-OFOC-ATR IN CYA-FLD.
           MOVE FRM-CPIM-UBY TO OPC-COD-VCOC-ATR IN CYA-FLD.
       FIN-UNP-OPC-CYA.
           EXIT.
       
       UNP-CYA-CYA SECTION.
       INI-UNP-CYA-CYA.
           MOVE FRM-CPIM-UBY TO CYA-IND-TCYA-ATR IN CYA-FLD.
           MOVE FRM-CPIM-UBY TO CYA-CAI-ICYA-ATR IN CYA-FLD.
           MOVE FRM-CPIM-UBY TO CYA-IIC-ICYA-ATR IN CYA-FLD.
           MOVE FRM-CPIM-UBY TO CYA-SGV-LIQU-ATR IN CYA-FLD.
       FIN-UNP-CYA-CYA.
           EXIT.
       
       UNP-DBC-CYA SECTION.
       INI-UNP-DBC-CYA.
       FIN-UNP-DBC-CYA.
           EXIT.
       
       UNP-RDC-CYA SECTION.
       INI-UNP-RDC-CYA.
           MOVE FRM-CPIM-UBY TO RDC-CAI-ICLI-ATR IN CYA-FLD.
           MOVE FRM-CPIM-UBY TO RDC-IIC-ICLI-ATR IN CYA-FLD.
       FIN-UNP-RDC-CYA.
           EXIT.
       
       VAL-MKY-CYA SECTION.
       INI-VAL-MKY-CYA.
       FIN-VAL-MKY-CYA.
           EXIT.
       
       VAL-FLD-CYA SECTION.
       INI-VAL-FLD-CYA.
           PERFORM VAL-OPC-MSC-TVOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-MSC-TVOC-LEN IN CYA-FLD
                GO TO FIN-VAL-FLD-CYA.
           PERFORM VAL-OPC-COD-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-TOOC-LEN IN CYA-FLD
                GO TO FIN-VAL-FLD-CYA.
           PERFORM VAL-OPC-COD-OFOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-OFOC-LEN IN CYA-FLD
                GO TO FIN-VAL-FLD-CYA.
           PERFORM VAL-FRM-GLS-TVOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TVOC-LEN IN CYA-FLD
                GO TO FIN-VAL-FLD-CYA.
           PERFORM VAL-FRM-GLS-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TOOC-LEN IN CYA-FLD
                GO TO FIN-VAL-FLD-CYA.
           PERFORM VAL-FRM-GLS-OFOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-OFOC-LEN IN CYA-FLD
                GO TO FIN-VAL-FLD-CYA.
           PERFORM VAL-RDC-CAI-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RDC-CAI-ICLI-LEN IN CYA-FLD
                GO TO FIN-VAL-FLD-CYA.
           PERFORM VAL-RDC-IIC-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RDC-IIC-ICLI-LEN IN CYA-FLD
                GO TO FIN-VAL-FLD-CYA.
           PERFORM VAL-FRM-GLS-TM01.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TM01-LEN IN CYA-FLD
                GO TO FIN-VAL-FLD-CYA.
           PERFORM VAL-CYA-IND-TCYA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CYA-IND-TCYA-LEN IN CYA-FLD
                GO TO FIN-VAL-FLD-CYA.
           PERFORM VAL-FRM-GLS-TCYA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TCYA-LEN IN CYA-FLD
                GO TO FIN-VAL-FLD-CYA.
           PERFORM VAL-FRM-GLS-TM04.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TM04-LEN IN CYA-FLD
                GO TO FIN-VAL-FLD-CYA.
           PERFORM VAL-CYA-CAI-ICYA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CYA-CAI-ICYA-LEN IN CYA-FLD
                GO TO FIN-VAL-FLD-CYA.
           PERFORM VAL-CYA-IIC-ICYA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CYA-IIC-ICYA-LEN IN CYA-FLD
                GO TO FIN-VAL-FLD-CYA.
           PERFORM VAL-OPC-COD-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-VCOC-LEN IN CYA-FLD
                GO TO FIN-VAL-FLD-CYA.
           PERFORM VAL-FRM-GLS-TM02.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TM02-LEN IN CYA-FLD
                GO TO FIN-VAL-FLD-CYA.
           PERFORM VAL-FRM-VAL-MONX.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-MONX-LEN IN CYA-FLD
                GO TO FIN-VAL-FLD-CYA.
           PERFORM VAL-FRM-GLS-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-VCOC-LEN IN CYA-FLD
                GO TO FIN-VAL-FLD-CYA.
           PERFORM VAL-FRM-GLS-TM03.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TM03-LEN IN CYA-FLD
                GO TO FIN-VAL-FLD-CYA.
           PERFORM VAL-FRM-VAL-VPES.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-VAL-VPES-LEN IN CYA-FLD
                GO TO FIN-VAL-FLD-CYA.
           PERFORM VAL-CYA-SGV-LIQU.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CYA-SGV-LIQU-LEN IN CYA-FLD
                GO TO FIN-VAL-FLD-CYA.
       FIN-VAL-FLD-CYA.
           EXIT.
       
       VAL-OPC-MSC-TVOC SECTION.
       INI-VAL-OPC-MSC-TVOC.
           IF OPC-MSC-TVOC IN CYA-FLD NOT > SPACES
              MOVE SPACES TO OPC-MSC-TVOC IN CYA-FLD.
      *VAL-IND Valida miscelaneo de tabla
           MOVE 'TAB' TO MSC-COD-SIST.
           MOVE 'TVAL' TO MSC-COD-TMSC IN MSC.
           MOVE OPC-MSC-TVOC IN CYA-FLD TO MSC-COD-CMSC IN MSC.
           PERFORM VAL-IND.
           IF MSG-COD-MENS = 'COD    NEX' OR
              OPC-MSC-TVOC IN CYA-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-TVOC IN CYA-FLD
           ELSE
               MOVE MSC-GLS-DESC IN MSC TO
                    FRM-GLS-TVOC IN CYA-FLD.
       FIN-VAL-OPC-MSC-TVOC.
           EXIT.
       
       VAL-OPC-COD-TOOC SECTION.
       INI-VAL-OPC-COD-TOOC.
           IF OPC-COD-TOOC IN CYA-FLD NOT > SPACES
              MOVE SPACES TO OPC-COD-TOOC IN CYA-FLD.
      *VAL-COD Valida codigo de tabla
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'TIO ' TO TAB-COD-TTAB IN TAB.
           MOVE OPC-COD-TOOC IN CYA-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              OPC-COD-TOOC IN CYA-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-TOOC IN CYA-FLD
           ELSE
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-TOOC IN CYA-FLD.
       FIN-VAL-OPC-COD-TOOC.
           EXIT.
       
       VAL-OPC-COD-OFOC SECTION.
       INI-VAL-OPC-COD-OFOC.
           IF OPC-COD-OFOC IN CYA-FLD NOT > SPACES
              MOVE SPACES TO OPC-COD-OFOC IN CYA-FLD.
      *VAL-OFI Valida codigo de Oficina en TABLAS
           MOVE OPC-COD-OFOC IN CYA-FLD TO OFI-COD-OFIC IN OFI.
           PERFORM VAL-OFI.
           IF MSG-COD-MENS NOT = SPACES
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
           IF MSG-COD-MENS = 'COD    NEX'
               MOVE SPACES TO FRM-GLS-OFOC IN CYA-FLD
           ELSE
               MOVE OFI-GLS-DESC IN OFI TO FRM-GLS-OFOC IN CYA-FLD.
       FIN-VAL-OPC-COD-OFOC.
           EXIT.
       
       VAL-FRM-GLS-TVOC SECTION.
       INI-VAL-FRM-GLS-TVOC.
           IF FRM-GLS-TVOC IN CYA-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-TVOC IN CYA-FLD.
       FIN-VAL-FRM-GLS-TVOC.
           EXIT.
       
       VAL-FRM-GLS-TOOC SECTION.
       INI-VAL-FRM-GLS-TOOC.
           IF FRM-GLS-TOOC IN CYA-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-TOOC IN CYA-FLD.
       FIN-VAL-FRM-GLS-TOOC.
           EXIT.
       
       VAL-FRM-GLS-OFOC SECTION.
       INI-VAL-FRM-GLS-OFOC.
           IF FRM-GLS-OFOC IN CYA-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-OFOC IN CYA-FLD.
       FIN-VAL-FRM-GLS-OFOC.
           EXIT.
       
       VAL-RDC-CAI-ICLI SECTION.
       INI-VAL-RDC-CAI-ICLI.
           IF RDC-CAI-ICLI IN CYA-FLD NOT > SPACES
              MOVE SPACES TO RDC-CAI-ICLI IN CYA-FLD.
       FIN-VAL-RDC-CAI-ICLI.
           EXIT.
       
       VAL-RDC-IIC-ICLI SECTION.
       INI-VAL-RDC-IIC-ICLI.
           IF RDC-IIC-ICLI IN CYA-FLD NOT > SPACES
              MOVE SPACES TO RDC-IIC-ICLI IN CYA-FLD.
       FIN-VAL-RDC-IIC-ICLI.
           EXIT.
       
       VAL-FRM-GLS-TM01 SECTION.
       INI-VAL-FRM-GLS-TM01.
           IF FRM-GLS-TM01 IN CYA-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-TM01 IN CYA-FLD.
       FIN-VAL-FRM-GLS-TM01.
           EXIT.
       
       VAL-CYA-IND-TCYA SECTION.
       INI-VAL-CYA-IND-TCYA.
           IF CYA-IND-TCYA IN CYA-FLD NOT > SPACES
              MOVE SPACES TO CYA-IND-TCYA IN CYA-FLD.
      *VAL-IND Valida miscelaneo de tabla
           MOVE 'TAB' TO MSC-COD-SIST.
           MOVE 'TCYA' TO MSC-COD-TMSC IN MSC.
           MOVE CYA-IND-TCYA IN CYA-FLD TO MSC-COD-CMSC IN MSC.
           PERFORM VAL-IND.
           IF MSG-COD-MENS = 'COD    NEX' OR
              CYA-IND-TCYA IN CYA-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-TCYA IN CYA-FLD
           ELSE
               MOVE MSC-GLS-DESC IN MSC TO
                    FRM-GLS-TCYA IN CYA-FLD.
       FIN-VAL-CYA-IND-TCYA.
           EXIT.
       
       VAL-FRM-GLS-TCYA SECTION.
       INI-VAL-FRM-GLS-TCYA.
           IF FRM-GLS-TCYA IN CYA-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-TCYA IN CYA-FLD.
       FIN-VAL-FRM-GLS-TCYA.
           EXIT.
       
       VAL-FRM-GLS-TM04 SECTION.
       INI-VAL-FRM-GLS-TM04.
           IF FRM-GLS-TM04 IN CYA-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-TM04 IN CYA-FLD.
       FIN-VAL-FRM-GLS-TM04.
           EXIT.
       
       VAL-CYA-CAI-ICYA SECTION.
       INI-VAL-CYA-CAI-ICYA.
           IF CYA-CAI-ICYA IN CYA-FLD NOT > SPACES
              MOVE SPACES TO CYA-CAI-ICYA IN CYA-FLD.
      *<<<< CYA_CAI_ICYA
           IF CYA-CAI-ICYA IN CYA-FLD NOT > SPACES
               MOVE SPACES TO CYA-CAI-ICYA IN CYA-FLD.
           MOVE MSC-COD-DAT1(3) TO WSS-GLS-DAT1.
      *ISP WSS-GLS-DAT1 SERA USADO EN VALIDACION DE CONSISTENCIA
      *    REG MSC VALIDADO AUTOMATICAMENTE EN CYA-IND-TCYA, SE PUSO
      *    ESTE CODIGO AQUI POR QUE NO SE PUEDE PONER DESPUES DE 
      *    VALIDAR CYA-IND-TCYA
      *>>>>
       FIN-VAL-CYA-CAI-ICYA.
           EXIT.
       
       VAL-CYA-IIC-ICYA SECTION.
       INI-VAL-CYA-IIC-ICYA.
           IF CYA-IIC-ICYA IN CYA-FLD NOT > SPACES
              MOVE SPACES TO CYA-IIC-ICYA IN CYA-FLD.
       FIN-VAL-CYA-IIC-ICYA.
           EXIT.
       
       VAL-OPC-COD-VCOC SECTION.
       INI-VAL-OPC-COD-VCOC.
           IF OPC-COD-VCOC IN CYA-FLD NOT > SPACES
              MOVE SPACES TO OPC-COD-VCOC IN CYA-FLD.
      *VAL-COD Valida codigo de tabla
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'VLR ' TO TAB-COD-TTAB IN TAB.
           MOVE OPC-COD-VCOC IN CYA-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              OPC-COD-VCOC IN CYA-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-VCOC IN CYA-FLD
           ELSE
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-VCOC IN CYA-FLD.
       FIN-VAL-OPC-COD-VCOC.
           EXIT.
       
       VAL-FRM-GLS-TM02 SECTION.
       INI-VAL-FRM-GLS-TM02.
           IF FRM-GLS-TM02 IN CYA-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-TM02 IN CYA-FLD.
       FIN-VAL-FRM-GLS-TM02.
           EXIT.
       
       VAL-FRM-VAL-MONX SECTION.
       INI-VAL-FRM-VAL-MONX.
      *<<<< FRM_VAL_MONX
           MOVE 'TVAL' TO MSC-CIC-TMSC IN MSC.
           MOVE '0'    TO MSC-CIC-CMSC IN MSC.
           MOVE 'TAB' TO FIO-SIST.
           MOVE 'MSC-CIC-TABL' TO FIO-AKEY.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-MSC.
           MOVE MSC-COD-CMSC IN MSC TO WSS-COD-TVOC IN WSS-VARI.
           IF WSS-COD-TVOC IN WSS-VARI NOT = OPC-MSC-TVOC IN CYA-FLD
               MOVE FRM-VAL-MONX IN CYA-FLD TO CYA-SGV-LIQU IN CYA-FLD
               IF CYA-SGV-LIQU IN CYA-FLD > OPC-VAL-CRED IN OPC
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   MOVE 'COL' TO MSG-COD-SIST
                   MOVE 'SGVLIQUIOPC>' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC IN MSG TO FRM-MENS
                   GO TO FIN-VAL-FRM-VAL-MONX.
           IF CYA-NUM-ICAN IN CYA-FLD > ZEROES                          
               IF CYA-SGV-LIQU IN CYA-FLD < ZEROES                      
                   MOVE 'A'   TO TAB-CIC-CTAB IN TAB                    
               ELSE                                                     
                   MOVE 'C'   TO TAB-CIC-CTAB IN TAB                    
           ELSE                                                         
               IF CYA-SGV-LIQU IN CYA-FLD < ZEROES                      
                   MOVE 'C'   TO TAB-CIC-CTAB IN TAB                    
               ELSE                                                     
                   MOVE 'A'   TO TAB-CIC-CTAB IN TAB.                   
           MOVE 'TAB' TO TAB-COD-SIST                                   
           MOVE 'MOV' TO TAB-CIC-TTAB IN TAB                            
           MOVE 'TAB-CIC-TABL' TO FIO-AKEY                              
           PERFORM BUS-TAB                                              
           MOVE TAB-GLS-ABRV IN TAB TO FRM-GLS-TM01 IN CYA-FLD          
           MOVE TAB-GLS-ABRV IN TAB TO FRM-GLS-TM02 IN CYA-FLD          
           MOVE TAB-GLS-ABRV IN TAB TO FRM-GLS-TM03 IN CYA-FLD          
           MOVE TAB-GLS-ABRV IN TAB TO FRM-GLS-TM04 IN CYA-FLD.         
      *>>>>
       FIN-VAL-FRM-VAL-MONX.
           EXIT.
       
       VAL-FRM-GLS-VCOC SECTION.
       INI-VAL-FRM-GLS-VCOC.
           IF FRM-GLS-VCOC IN CYA-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-VCOC IN CYA-FLD.
       FIN-VAL-FRM-GLS-VCOC.
           EXIT.
       
       VAL-FRM-GLS-TM03 SECTION.
       INI-VAL-FRM-GLS-TM03.
           IF FRM-GLS-TM03 IN CYA-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-TM03 IN CYA-FLD.
       FIN-VAL-FRM-GLS-TM03.
           EXIT.
       
       VAL-FRM-VAL-VPES SECTION.
       INI-VAL-FRM-VAL-VPES.
      *<<<< FRM_VAL_VPES
           IF CYA-NUM-ICAN IN CYA-FLD NOT = ZEROES AND
              FRM-VAL-VPES IN CYA-FLD = ZEROES
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE 'COL' TO MSG-COD-SIST
                MOVE 'VALVPESOBL' TO MSG-COD-MENS
                PERFORM GET-MSG
                MOVE MSG-GLS-DESC IN MSG TO FRM-MENS
                GO TO FIN-VAL-FRM-VAL-VPES.
           MOVE 'TVAL' TO MSC-CIC-TMSC IN MSC.
           MOVE '0'    TO MSC-CIC-CMSC IN MSC.
           MOVE 'TAB' TO FIO-SIST.
           MOVE 'MSC-CIC-TABL' TO FIO-AKEY.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-MSC.
           MOVE MSC-COD-CMSC IN MSC TO WSS-COD-TVOC IN WSS-VARI.
           IF WSS-COD-TVOC IN WSS-VARI = OPC-MSC-TVOC IN CYA-FLD
               MOVE FRM-VAL-VPES IN CYA-FLD TO CYA-SGV-LIQU IN CYA-FLD.
           IF CYA-NUM-ICAN IN CYA-FLD > ZEROES                          
               IF CYA-SGV-LIQU IN CYA-FLD < ZEROES                      
                   MOVE 'A'   TO TAB-CIC-CTAB IN TAB                    
               ELSE                                                     
                   MOVE 'C'   TO TAB-CIC-CTAB IN TAB                    
           ELSE                                                         
               IF CYA-SGV-LIQU IN CYA-FLD < ZEROES                      
                   MOVE 'C'   TO TAB-CIC-CTAB IN TAB                    
               ELSE                                                     
                   MOVE 'A'   TO TAB-CIC-CTAB IN TAB.                   
           MOVE 'TAB' TO TAB-COD-SIST                                   
           MOVE 'MOV' TO TAB-CIC-TTAB IN TAB                            
           MOVE 'TAB-CIC-TABL' TO FIO-AKEY                              
           PERFORM BUS-TAB                                              
           MOVE TAB-GLS-ABRV IN TAB TO FRM-GLS-TM01 IN CYA-FLD          
           MOVE TAB-GLS-ABRV IN TAB TO FRM-GLS-TM02 IN CYA-FLD          
           MOVE TAB-GLS-ABRV IN TAB TO FRM-GLS-TM03 IN CYA-FLD          
           MOVE TAB-GLS-ABRV IN TAB TO FRM-GLS-TM04 IN CYA-FLD.         

      *         IF CYA-SGV-LIQU IN CYA-FLD > OPC-VAL-CRED IN OPC
      *             MOVE FRM-SUAR-MAL TO FRM-SUAR
      *             MOVE 'COL' TO MSG-COD-SIST
      *             MOVE 'SGVLIQUIOPC>' TO MSG-COD-MENS
      *             PERFORM GET-MSG
      *             MOVE MSG-GLS-DESC IN MSG TO FRM-MENS
      *             GO TO FIN-VAL-FRM-VAL-VPES.
      *>>>>
       FIN-VAL-FRM-VAL-VPES.
           EXIT.
       
       VAL-CYA-SGV-LIQU SECTION.
       INI-VAL-CYA-SGV-LIQU.
       FIN-VAL-CYA-SGV-LIQU.
           EXIT.
       
       VAL-CON-FLD-CYA SECTION.
       INI-VAL-CON-FLD-CYA.
      *<<<<  VAL_CON_FLD_CYA
           IF CYA-CAI-ICYA IN CYA-FLD NOT > SPACES
               MOVE SPACES TO CYA-CAI-ICYA IN CYA-FLD.
           IF WSS-GLS-DAT1 NOT > SPACES
               MOVE SPACES TO CYA-CAI-ICYA IN CYA-FLD
               MOVE ZEROES TO CYA-IIC-ICYA IN CYA-FLD
               GO TO FIN-VAL-CON-FLD-CYA.

           IF CYA-IIC-ICYA IN CYA-FLD NOT > ZEROES
               IF CYA-NUM-ICAN IN CYA-FLD = ZEROES
                   MOVE 'IICICYACAOBL' TO MSG-COD-MENS
               ELSE
                   MOVE 'IICICYACCOBL' TO MSG-COD-MENS.

           IF CYA-IIC-ICYA IN CYA-FLD NOT > ZEROES
               MOVE 'COL'          TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CON-FLD-CYA.
           IF DBC-IND-ICLI IN CYA-FLD = 'M'
               GO TO NOT-VAL-CYA.
           MOVE RDC-CAI-ICLI IN CYA-FLD TO RCO-CAI-ICLI IN RCO.
           MOVE RDC-IIC-ICLI IN CYA-FLD TO RCO-IIC-ICLI IN RCO.
           MOVE CYA-CAI-ICYA IN CYA-FLD TO RCO-CAI-IOPE IN RCO.
           MOVE CYA-IIC-ICYA IN CYA-FLD TO RCO-IIC-IOPE IN RCO.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-RCO.
           IF FIO-STAT-OKS
               IF WSS-GLS-DAT1 = 'O'
                   MOVE 'COL'        TO MSG-COD-SIST
                   MOVE 'CICICYASICLI' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   GO TO FIN-VAL-CON-FLD-CYA
               ELSE
                   GO TO CER-VAL-CYA-CAI-ICYA.
           MOVE 'RCO-KEY-IOPE' TO FIO-AKEY.
           MOVE CYA-CAI-ICYA IN CYA-FLD TO RCO-CAI-IOPE IN RCO.
           MOVE CYA-IIC-ICYA IN CYA-FLD TO RCO-IIC-IOPE IN RCO.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-RCO.
           IF NOT FIO-STAT-OKS
               MOVE 'COL'        TO MSG-COD-SIST
               MOVE 'CICICYANEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CON-FLD-CYA
           ELSE
               IF WSS-GLS-DAT1 = 'M'
                   MOVE 'COL'        TO MSG-COD-SIST
                   MOVE 'CICICYANICLI' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   GO TO FIN-VAL-CON-FLD-CYA.
       CER-VAL-CYA-CAI-ICYA.
           MOVE RCO-CIC-IOPE IN RCO TO OPE-CIC-IOPE IN OPE.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-OPE.
           IF OPE-FEC-FTOP IN OPE > ZEROES
               IF OPE-FEC-FTOP IN OPE > OPE-FEC-FROP IN OPE
                   MOVE 'COL'          TO MSG-COD-SIST
                   MOVE 'FECFTOPICYA>' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MEN1
                   MOVE FRM-SUAR-MAL TO FRM-SUAR.
       NOT-VAL-CYA.              
      *>>>>
       FIN-VAL-CON-FLD-CYA.
           EXIT.
       
       VAL-CON-CYA SECTION.
       INI-VAL-CON-CYA.
       FIN-VAL-CON-CYA.
           EXIT.
       
       VAL-NUM-KEY-CYA SECTION.
       INI-VAL-NUM-KEY-CYA.
           PERFORM VAL-NUM-IKY-CYA.
           PERFORM VAL-NUM-MKY-CYA.
           PERFORM VAL-NUM-AKY-CYA.
       FIN-VAL-NUM-KEY-CYA.
           EXIT.
       
       VAL-NUM-IKY-CYA SECTION.
       INI-VAL-NUM-IKY-CYA.
       VAL-NUM-CYA-IIC-IOPC.
           MOVE CYA-IIC-IOPC-ALF IN CYA-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO CYA-IIC-IOPC IN CYA-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO CYA-IIC-IOPC-LEN IN CYA-FLD
                GO TO FIN-VAL-NUM-IKY-CYA.
       VAL-NUM-CYA-NUM-ICAN.
           MOVE CYA-NUM-ICAN-ALF IN CYA-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO CYA-NUM-ICAN IN CYA-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO CYA-NUM-ICAN-LEN IN CYA-FLD
                GO TO FIN-VAL-NUM-IKY-CYA.
       FIN-VAL-NUM-IKY-CYA.
           EXIT.
       
       VAL-NUM-MKY-CYA SECTION.
       INI-VAL-NUM-MKY-CYA.
       FIN-VAL-NUM-MKY-CYA.
           EXIT.
       
       VAL-NUM-AKY-CYA SECTION.
       INI-VAL-NUM-AKY-CYA.
       VAL-NUM-CYA-NUM-ICYA.
           MOVE CYA-NUM-ICYA-ALF IN CYA-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 1 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO CYA-NUM-ICYA IN CYA-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO CYA-NUM-ICYA-LEN IN CYA-FLD
                GO TO FIN-VAL-NUM-AKY-CYA.
      * IDC-INI --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
       VAL-NUM-DBC-NUM-ICLI.
           MOVE DBC-NUM-ICLI-ALF IN CYA-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO DBC-NUM-ICLI IN CYA-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO DBC-NUM-ICLI-LEN IN CYA-FLD
                GO TO FIN-VAL-NUM-AKY-CYA.
      * IDC-FIN --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
       FIN-VAL-NUM-AKY-CYA.
           EXIT.
       
       VAL-NUM-FLD-CYA SECTION.
       INI-VAL-NUM-FLD-CYA.
       VAL-NUM-FRM-VAL-MONX.
           MOVE FRM-VAL-MONX-ALF IN CYA-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9V TO FRM-VAL-MONX IN CYA-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-VAL-MONX-LEN IN CYA-FLD
                GO TO FIN-VAL-NUM-FLD-CYA.
       VAL-NUM-FRM-VAL-VPES.
           MOVE FRM-VAL-VPES-ALF IN CYA-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO FRM-VAL-VPES IN CYA-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-VAL-VPES-LEN IN CYA-FLD
                GO TO FIN-VAL-NUM-FLD-CYA.
       VAL-NUM-CYA-SGV-LIQU.
           MOVE CYA-SGV-LIQU-ALF IN CYA-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9V TO CYA-SGV-LIQU IN CYA-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO CYA-SGV-LIQU-LEN IN CYA-FLD
                GO TO FIN-VAL-NUM-FLD-CYA.
       FIN-VAL-NUM-FLD-CYA.
           EXIT.
       
       EDT-KEY-CYA SECTION.
       INI-EDT-KEY-CYA.
           PERFORM EDT-IKY-CYA.
           PERFORM EDT-MKY-CYA.
           PERFORM EDT-AKY-CYA.
       FIN-EDT-KEY-CYA.
           EXIT.
       
       EDT-IKY-CYA SECTION.
       INI-EDT-IKY-CYA.
           IF CYA-IIC-IOPC-LEN IN CYA-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CYA-IIC-IOPC-LEN IN CYA-FLD
                GO TO FIN-EDT-IKY-CYA.
           MOVE CYA-IIC-IOPC IN CYA-FLD TO CYA-IIC-IOPC-EDT IN CYA-EDT.
           MOVE CYA-IIC-IOPC-EDT IN CYA-EDT TO 
                CYA-IIC-IOPC-ALF IN CYA-FLD.
           IF CYA-NUM-ICAN-LEN IN CYA-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CYA-NUM-ICAN-LEN IN CYA-FLD
                GO TO FIN-EDT-IKY-CYA.
           MOVE CYA-NUM-ICAN IN CYA-FLD TO CYA-NUM-ICAN-EDT IN CYA-EDT.
           MOVE CYA-NUM-ICAN-EDT IN CYA-EDT TO 
                CYA-NUM-ICAN-ALF IN CYA-FLD.
       FIN-EDT-IKY-CYA.
           EXIT.
       
       EDT-MKY-CYA SECTION.
       INI-EDT-MKY-CYA.
       FIN-EDT-MKY-CYA.
           EXIT.
       
       EDT-AKY-CYA SECTION.
       INI-EDT-AKY-CYA.
           IF CYA-NUM-ICYA-LEN IN CYA-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CYA-NUM-ICYA-LEN IN CYA-FLD
                GO TO FIN-EDT-AKY-CYA.
           MOVE CYA-NUM-ICYA IN CYA-FLD TO CYA-NUM-ICYA-EDT IN CYA-EDT.
           MOVE CYA-NUM-ICYA-EDT IN CYA-EDT TO 
                CYA-NUM-ICYA-ALF IN CYA-FLD.
      * IDC-INI --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF DBC-NUM-ICLI-LEN IN CYA-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DBC-NUM-ICLI-LEN IN CYA-FLD
                GO TO FIN-EDT-AKY-CYA.
           MOVE DBC-NUM-ICLI IN CYA-FLD TO
                DBC-NUM-ICLI-EDT IN CYA-EDT.
