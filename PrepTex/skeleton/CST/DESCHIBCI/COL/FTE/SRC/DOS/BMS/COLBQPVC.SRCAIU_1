           EXIT.
       
       VAL-KEY-PVC SECTION.
       INI-VAL-KEY-PVC.
           PERFORM VAL-IKY-PVC.
           PERFORM VAL-MKY-PVC.
           PERFORM VAL-AKY-PVC.
       FIN-VAL-KEY-PVC.
           EXIT.
       
       VAL-IKY-PVC SECTION.
       INI-VAL-IKY-PVC.
           PERFORM VAL-PVC-KEY-IPVC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PVC-KEY-IPVC-LEN IN PVC-FLD
                GO TO FIN-VAL-IKY-PVC.
       FIN-VAL-IKY-PVC.
           EXIT.
       
       VAL-MKY-PVC SECTION.
       INI-VAL-MKY-PVC.
       FIN-VAL-MKY-PVC.
           EXIT.
       
       VAL-AKY-PVC SECTION.
       INI-VAL-AKY-PVC.
       FIN-VAL-AKY-PVC.
           EXIT.
       
       VAL-PVC-KEY-IPVC SECTION.
       INI-VAL-PVC-KEY-IPVC.
           IF PVC-KEY-IPVC IN PVC-FLD NOT > SPACES
              MOVE SPACES TO PVC-KEY-IPVC IN PVC-FLD.
       FIN-VAL-PVC-KEY-IPVC.
           EXIT.
       
       VAL-CON-KEY-PVC SECTION.
       INI-VAL-CON-KEY-PVC.
           IF     PVC-KEY-IPVC IN PVC-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-PVC.
       FIN-VAL-CON-KEY-PVC.
           EXIT.
       
       VAL-CON-IKY-PVC SECTION.
       INI-VAL-CON-IKY-PVC.
           IF     PVC-KEY-IPVC IN PVC-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-PVC.
       FIN-VAL-CON-IKY-PVC.
           EXIT.
       
