           03 WSS-GLS-TIT2.
              05 WSS-GLS-TIT2-A VALUE
                 '          CREDITOS DE CONSUMO CASTIGADOS AL '
                                                  PIC X(44).
              05 WSS-GLS-TIT2-B VALUE SPACES      PIC X(10).

           03 WSS-GLS-TIT3.
              05 WSS-GLS-TIT3-A VALUE
                 ' OPERACIONES EGRESADAS A CARTERA VENCIDA AL '
                                                  PIC X(44).
              05 WSS-GLS-TIT3-B VALUE SPACES      PIC X(10).

           03 WSS-GLS-TIT4.
              05 WSS-GLS-TIT4-A VALUE
                 '   CREDITOS DE CONSUMO CASTIGADOS EN EL MES '
                                                  PIC X(44).
              05 WSS-GLS-TIT4-B VALUE SPACES      PIC X(10).

           03 WSS-FEC-FSAL1.
              05 WSS-NUM-DSAL1                    PIC 9(02).
              05 FILLER          VALUE '/'        PIC X(01).
              05 WSS-NUM-MSAL1                    PIC 9(02).
              05 FILLER          VALUE '/'        PIC X(01).
              05 WSS-NUM-SSAL1                    PIC 9(02).
              05 WSS-NUM-ASAL1                    PIC 9(02).

           03 WSS-FEC-FSAL2.
              05 WSS-NUM-MSAL2                    PIC 9(02).
              05 FILLER          VALUE '/'        PIC X(01).
              05 WSS-NUM-SSAL2                    PIC 9(02).
              05 WSS-NUM-ASAL2                    PIC 9(02).
              05 FILLER   VALUE SPACES            PIC X(03).
