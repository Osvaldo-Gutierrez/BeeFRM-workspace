       VAL-NUM-FLD-CCT SECTION.
       INI-VAL-NUM-FLD-CCT.
       VAL-NUM-DLC-VAL-SDLC.
           MOVE DLC-VAL-SDLC-ALF IN CCT-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           CALL 'GNSPVNUM' USING NUM-VARI.
           IF NUM-STAT-OKS
               MOVE NUM-NN9V TO DLC-VAL-SDLC IN CCT-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO DLC-VAL-SDLC-LEN IN CCT-FLD
                GO TO FIN-VAL-NUM-FLD-CCT.
       VAL-NUM-VEN-VAL-SCAP.
           MOVE VEN-VAL-SCAP-ALF IN CCT-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           CALL 'GNSPVNUM' USING NUM-VARI.
           IF NUM-STAT-OKS
               MOVE NUM-NN9V TO VEN-VAL-SCAP IN CCT-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO VEN-VAL-SCAP-LEN IN CCT-FLD
                GO TO FIN-VAL-NUM-FLD-CCT.
       VAL-NUM-VEN-VAL-SFIN.
           MOVE VEN-VAL-SFIN-ALF IN CCT-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           CALL 'GNSPVNUM' USING NUM-VARI.
           IF NUM-STAT-OKS
               MOVE NUM-NN9V TO VEN-VAL-SFIN IN CCT-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO VEN-VAL-SFIN-LEN IN CCT-FLD
                GO TO FIN-VAL-NUM-FLD-CCT.
       FIN-VAL-NUM-FLD-CCT.
           EXIT.
       
       EDT-KEY-CCT SECTION.
       INI-EDT-KEY-CCT.
           PERFORM EDT-IKY-CCT.
           PERFORM EDT-MKY-CCT.
           PERFORM EDT-AKY-CCT.
       FIN-EDT-KEY-CCT.
           EXIT.
       
       EDT-IKY-CCT SECTION.
       INI-EDT-IKY-CCT.
           IF CCT-IIC-IOPC-LEN IN CCT-FLD = -9
                MOVE -1 TO CCT-IIC-IOPC-LEN IN CCT-FLD
                GO TO FIN-EDT-IKY-CCT.
           MOVE CCT-IIC-IOPC IN CCT-FLD TO CCT-IIC-IOPC-EDT IN CCT-FLD.
           IF VEN-NUM-IVCT-LEN IN CCT-FLD = -9
                MOVE -1 TO VEN-NUM-IVCT-LEN IN CCT-FLD
                GO TO FIN-EDT-IKY-CCT.
           MOVE VEN-NUM-IVCT IN CCT-FLD TO VEN-NUM-IVCT-EDT IN CCT-FLD.
           IF CCT-NUM-IDLC-LEN IN CCT-FLD = -9
                MOVE -1 TO CCT-NUM-IDLC-LEN IN CCT-FLD
                GO TO FIN-EDT-IKY-CCT.
           MOVE CCT-NUM-IDLC IN CCT-FLD TO CCT-NUM-IDLC-EDT IN CCT-FLD.
           IF VEN-NUM-IVEN-LEN IN CCT-FLD = -9
                MOVE -1 TO VEN-NUM-IVEN-LEN IN CCT-FLD
                GO TO FIN-EDT-IKY-CCT.
           MOVE VEN-NUM-IVEN IN CCT-FLD TO VEN-NUM-IVEN-EDT IN CCT-FLD.
       FIN-EDT-IKY-CCT.
           EXIT.
       
       EDT-MKY-CCT SECTION.
       INI-EDT-MKY-CCT.
       FIN-EDT-MKY-CCT.
           EXIT.
       
       EDT-AKY-CCT SECTION.
       INI-EDT-AKY-CCT.
           IF CCT-NUM-ICCT-LEN IN CCT-FLD = -9
                MOVE -1 TO CCT-NUM-ICCT-LEN IN CCT-FLD
                GO TO FIN-EDT-AKY-CCT.
           MOVE CCT-NUM-ICCT IN CCT-FLD TO CCT-NUM-ICCT-EDT IN CCT-FLD.
           IF FRM-NUM-ICLI-LEN IN CCT-FLD = -9
                MOVE -1 TO FRM-NUM-ICLI-LEN IN CCT-FLD
                GO TO FIN-EDT-AKY-CCT.
           MOVE FRM-NUM-ICLI IN CCT-FLD TO FRM-NUM-ICLI-EDT IN CCT-FLD.
           IF FRM-NUM-IAVA-LEN IN CCT-FLD = -9
                MOVE -1 TO FRM-NUM-IAVA-LEN IN CCT-FLD
                GO TO FIN-EDT-AKY-CCT.
           MOVE FRM-NUM-IAVA IN CCT-FLD TO FRM-NUM-IAVA-EDT IN CCT-FLD.
       FIN-EDT-AKY-CCT.
           EXIT.
       
       EDT-FLD-CCT SECTION.
       INI-EDT-FLD-CCT.
           IF DLC-VAL-SDLC-LEN IN CCT-FLD = -9
                MOVE -1 TO DLC-VAL-SDLC-LEN IN CCT-FLD
                GO TO FIN-EDT-FLD-CCT.
           MOVE DLC-VAL-SDLC IN CCT-FLD TO DLC-VAL-SDLC-EDT IN CCT-FLD.
           IF VEN-VAL-SCAP-LEN IN CCT-FLD = -9
                MOVE -1 TO VEN-VAL-SCAP-LEN IN CCT-FLD
                GO TO FIN-EDT-FLD-CCT.
           MOVE VEN-VAL-SCAP IN CCT-FLD TO VEN-VAL-SCAP-EDT IN CCT-FLD.
           IF VEN-VAL-SFIN-LEN IN CCT-FLD = -9
                MOVE -1 TO VEN-VAL-SFIN-LEN IN CCT-FLD
                GO TO FIN-EDT-FLD-CCT.
           MOVE VEN-VAL-SFIN IN CCT-FLD TO VEN-VAL-SFIN-EDT IN CCT-FLD.
       FIN-EDT-FLD-CCT.
           EXIT.
