       MAIN SECTION.
      *------------
       INI-MAIN.
       COPY GNSBGEDB.
           MOVE 'COLPLNAC' TO FIO-PROG.
           PERFORM GNS-BUS-IDD.

           MOVE FIO-INP      TO FIO-CMND.
           MOVE 'GNS'        TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
      *<<<< INI
           DISPLAY 'INICIO CAPTURA DE PARAMETROS'.
           DISPLAY '----------------------------'.

      *Captura Fecha de Proceso
           DISPLAY 'FECHA DE INICIO ( DDMMAAAA ) : '.
           ACCEPT  WSS-FEC-PRO IN WSS-VARI.
           DISPLAY WSS-FEC-PRO IN WSS-VARI.

           MOVE WSS-NUM-SPRO IN WSS-VARI TO WSS-NUM-SNAC IN WSS-NAC.
           MOVE WSS-NUM-APRO IN WSS-VARI TO WSS-NUM-ANAC IN WSS-NAC.
           MOVE WSS-NUM-MPRO IN WSS-VARI TO WSS-NUM-MNAC IN WSS-NAC.
           MOVE WSS-NUM-DPRO IN WSS-VARI TO WSS-NUM-DNAC IN WSS-NAC.
      *>>>>INI
           SORT GNSSRT
       COPY COLRSNAC.
           INPUT  PROCEDURE IS INP-SRT
           OUTPUT PROCEDURE IS OUT-SRT.
       FIN-MAIN.
           IF PGM-STAT-SRT-MAL OR SORT-RETURN > 0
               PERFORM GNS-PRO-ABT
           ELSE
               PERFORM GNS-PRO-END.

       INP-SRT SECTION.
      *---------------
       INI-INP-SRT.
           MOVE FIO-INP TO FIO-CMND.
           PERFORM COL-FIO-NAC.
       FST-INP-SRT.
      *<<<< INI_FST_INP
           GO TO INI-FST-INP2.
      *>>>> INI_FST_INP
           MOVE FIO-GET-FST TO FIO-CMND.
           PERFORM COL-FIO-NAC.
      *<<<< FIN_FST_INP
       INI-FST-INP2.
           MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM COL-FIO-NAC.
      *>>>> FIN_FST_INP
       LUP-INP-SRT.
           IF NOT FIO-STAT-OKS
              GO TO FIN-INP-SRT.
       MOV-INP-SRT.
           DISPLAY 'NAC-CIC-IOPC : ' NAC-CIC-IOPC IN NAC.
           DISPLAY 'NAC-SGV-LIQU : ' NAC-SGV-LIQU IN NAC.
           DISPLAY ' '.
       COPY COLRMNAC.
           RELEASE SRT.
       NXT-INP-SRT.
           MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM COL-FIO-NAC.
           GO TO LUP-INP-SRT.
       FIN-INP-SRT.
           MOVE FIO-CLO TO FIO-CMND.
           PERFORM COL-FIO-NAC.

       OUT-SRT SECTION.
      *---------------
       INI-OUT-SRT.
           IF PGM-STAT-SRT-MAL
               GO TO EXT-OUT-SRT.
           OPEN OUTPUT RPTNAC.
      *     MOVE 'COLPLNAC' TO RPT-NPGM.
           CALL 'GNSPLHDR' USING RPT-VARI.

      ***** CRW-COMIENZO
      *    INITIATE RPT-NAC.
           MOVE NAC-COD-COOC IN SRT TO RPT-CTRL-COD-COOC
           MOVE NAC-COD-OFIC IN SRT TO RPT-CTRL-COD-OFIC
      ***** CRW-FINAL

           RETURN GNSSRT AT END
                GO TO FIN-OUT-SRT.
       LUP-OUT-SRT.
      *<<<< LUP_OUT
      *>>>> LUP_OUT
       GEN-OUT-SRT.

      ***** CRW-COMIENZO
      *    GENERATE DL-NAC.

           PERFORM DEC-PH-NAC.
           MOVE RPT-DUSU                       TO RPT-HEAD-DUSU
           MOVE WSS-IND-CPAG IN WSS-NAC        TO RPT-HEAD-IND-CPAG
           MOVE WSS-COD-OFI IN WSS-NAC         TO RPT-HEAD-COD-OFI
           MOVE RPT-FDMA                       TO RPT-HEAD-FDMA
           MOVE RPT-DSIS                       TO RPT-HEAD-DSIS
           MOVE NAC-COD-OFIC IN SRT            TO RPT-HEAD-COD-OFIC
           MOVE OFI-GLS-DESC IN OFI            TO RPT-HEAD-GLS-DESC
           MOVE WSS-NUM-DNAC IN WSS-NAC        TO RPT-HEAD-NUM-DNAC
           MOVE WSS-NUM-MNAC IN WSS-NAC        TO RPT-HEAD-NUM-MNAC
           MOVE WSS-NUM-SNAC IN WSS-NAC        TO RPT-HEAD-NUM-SNAC
           MOVE WSS-NUM-ANAC IN WSS-NAC        TO RPT-HEAD-NUM-ANAC
           MOVE NAC-COD-COOC IN SRT            TO RPT-DETA-COD-COOC
           MOVE NAC-CAI-IOPC IN SRT            TO RPT-DETA-CAI-IOPC
           MOVE WSS-RAYA-IOPC IN WSS-NAC       TO RPT-DETA-RAYA-IOPC
           MOVE NAC-IIC-IOPC IN SRT            TO RPT-DETA-IIC-IOPC
           MOVE NAC-VAL-CAPI IN SRT            TO RPT-DETA-VAL-CAPI
           MOVE NAC-GLS-MONE IN SRT            TO RPT-DETA-GLS-MONE
           MOVE NAC-SGV-LIQU IN SRT            TO RPT-DETA-SGV-LIQU
           MOVE NAC-COD-ABON IN SRT            TO RPT-DETA-COD-ABON
           MOVE NAC-COD-CCTE IN SRT            TO RPT-DETA-COD-CCTE
           IF RPT-VARI-LINEA > 59 OR RPT-VARI-PAGE-COUNTER = 0
              PERFORM TITULOS
           END-IF.
           IF NAC-COD-OFIC IN SRT NOT = RPT-CTRL-COD-OFIC
              MOVE NAC-COD-COOC IN SRT TO RPT-CTRL-COD-COOC
              MOVE RPT-SGV-LIQU-COOC TO RPT-CTRL-SGV-LIQU IN CF-NAC-10
              WRITE RPTNAC-REG FROM  CF-NAC-09
              WRITE RPTNAC-REG FROM  CF-NAC-10
              ADD 2 TO RPT-VARI-LINEA
              MOVE 0 TO RPT-SGV-LIQU-COOC

              MOVE NAC-COD-OFIC IN SRT TO RPT-CTRL-COD-OFIC
              MOVE RPT-SGV-LIQU-OFIC TO RPT-CTRL-SGV-LIQU IN CF-NAC-12
              WRITE RPTNAC-REG FROM  CF-NAC-11
              WRITE RPTNAC-REG FROM  CF-NAC-12
              ADD 2 TO RPT-VARI-LINEA
              MOVE 0 TO RPT-SGV-LIQU-OFIC
              PERFORM TITULOS
           ELSE
              IF NAC-COD-COOC IN SRT NOT = RPT-CTRL-COD-COOC
                 MOVE NAC-COD-COOC IN SRT TO RPT-CTRL-COD-COOC
                 MOVE RPT-SGV-LIQU-COOC TO RPT-CTRL-SGV-LIQU
                                           IN CF-NAC-10
                 WRITE RPTNAC-REG FROM  CF-NAC-09
                 WRITE RPTNAC-REG FROM  CF-NAC-10
                 ADD 2 TO RPT-VARI-LINEA
                 MOVE 0 TO RPT-SGV-LIQU-COOC
              END-IF
           END-IF.
           ADD  NAC-SGV-LIQU IN SRT         TO RPT-SGV-LIQU-OFIC
           ADD  NAC-SGV-LIQU IN SRT         TO RPT-SGV-LIQU-COOC
           WRITE RPTNAC-REG FROM  DL-NAC-08.
           ADD 1 TO RPT-VARI-LINEA.
           GO TO NXT-OUT-SRT.

       TITULOS.
      *********
           ADD 1 TO RPT-VARI-PAGE-COUNTER
           MOVE RPT-VARI-PAGE-COUNTER TO RPT-HEAD-PAGE-COUNTER
           MOVE SPACES TO RPTNAC-REG
           WRITE RPTNAC-REG AFTER PAGE
           WRITE RPTNAC-REG FROM  PH-NAC-01
           WRITE RPTNAC-REG FROM  PH-NAC-02
           WRITE RPTNAC-REG FROM  PH-NAC-03
           WRITE RPTNAC-REG FROM  PH-NAC-04
           WRITE RPTNAC-REG FROM  PH-NAC-05
           WRITE RPTNAC-REG FROM  PH-NAC-06
           WRITE RPTNAC-REG FROM  PH-NAC-07
           MOVE 9 TO RPT-VARI-LINEA.

      ***** CRW-FINAL

       NXT-OUT-SRT.
           RETURN GNSSRT AT END
                GO TO FIN-OUT-SRT.
           GO TO LUP-OUT-SRT.
       FIN-OUT-SRT.

      ***** CRW-COMIENZO
      *    TERMINATE RPT-NAC.

           MOVE NAC-COD-COOC IN SRT TO RPT-CTRL-COD-COOC
           MOVE RPT-SGV-LIQU-COOC TO RPT-CTRL-SGV-LIQU IN CF-NAC-10
           WRITE RPTNAC-REG FROM  CF-NAC-09
           WRITE RPTNAC-REG FROM  CF-NAC-10
           MOVE NAC-COD-OFIC IN SRT TO RPT-CTRL-COD-OFIC
           MOVE RPT-SGV-LIQU-OFIC TO RPT-CTRL-SGV-LIQU IN CF-NAC-12
           WRITE RPTNAC-REG FROM  CF-NAC-11
           WRITE RPTNAC-REG FROM  CF-NAC-12

      ***** CRW-FINAL

           CLOSE RPTNAC.
       EXT-OUT-SRT.
           EXIT.
