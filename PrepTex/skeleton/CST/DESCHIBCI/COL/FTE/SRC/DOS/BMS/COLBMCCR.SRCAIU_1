                GO TO FIN-VAL-AKY-CCR.
           PERFORM VAL-FRM-IND-IAVA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-IND-IAVA-LEN IN CCR-FLD
                GO TO FIN-VAL-AKY-CCR.
           PERFORM VAL-FRM-GLS-IAVA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-IAVA-LEN IN CCR-FLD
                GO TO FIN-VAL-AKY-CCR.
           PERFORM VAL-FRM-NUM-IAVA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-NUM-IAVA-LEN IN CCR-FLD
                GO TO FIN-VAL-AKY-CCR.
           PERFORM VAL-FRM-VRF-IAVA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-VRF-IAVA-LEN IN CCR-FLD
                GO TO FIN-VAL-AKY-CCR.
       FIN-VAL-AKY-CCR.
           EXIT.
       
       VAL-FLD-CCR SECTION.
       INI-VAL-FLD-CCR.
           PERFORM VAL-OPC-MSC-TVOC.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPC-MSC-TVOC-LEN IN CCR-FLD
                GO TO FIN-VAL-FLD-CCR.
           PERFORM VAL-OPC-COD-TOOC.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPC-COD-TOOC-LEN IN CCR-FLD
                GO TO FIN-VAL-FLD-CCR.
           PERFORM VAL-OPC-COD-OFOC.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPC-COD-OFOC-LEN IN CCR-FLD
                GO TO FIN-VAL-FLD-CCR.
           PERFORM VAL-FRM-GLS-TVOC.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-TVOC-LEN IN CCR-FLD
                GO TO FIN-VAL-FLD-CCR.
           PERFORM VAL-FRM-GLS-TOOC.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-TOOC-LEN IN CCR-FLD
                GO TO FIN-VAL-FLD-CCR.
           PERFORM VAL-FRM-GLS-OFOC.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-OFOC-LEN IN CCR-FLD
                GO TO FIN-VAL-FLD-CCR.
           PERFORM VAL-CCR-IND-SCRA.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO CCR-IND-SCRA-LEN IN CCR-FLD
                GO TO FIN-VAL-FLD-CCR.
           PERFORM VAL-CCR-FEC-FCCR.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO CCR-FEC-FCCR-LEN IN CCR-FLD
                GO TO FIN-VAL-FLD-CCR.
           PERFORM VAL-FRM-GLS-SCRA.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-SCRA-LEN IN CCR-FLD
                GO TO FIN-VAL-FLD-CCR.
           PERFORM VAL-CCR-FEC-FING.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO CCR-FEC-FING-LEN IN CCR-FLD
                GO TO FIN-VAL-FLD-CCR.
           PERFORM VAL-CCR-IND-SCRT.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO CCR-IND-SCRT-LEN IN CCR-FLD
                GO TO FIN-VAL-FLD-CCR.
           PERFORM VAL-CCR-COD-RCCR.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO CCR-COD-RCCR-LEN IN CCR-FLD
                GO TO FIN-VAL-FLD-CCR.
           PERFORM VAL-FRM-GLS-SCRT.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-SCRT-LEN IN CCR-FLD
                GO TO FIN-VAL-FLD-CCR.
           PERFORM VAL-FRM-GLS-RCCR.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-RCCR-LEN IN CCR-FLD
                GO TO FIN-VAL-FLD-CCR.
           PERFORM VAL-DLC-IND-SCRT.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DLC-IND-SCRT-LEN IN CCR-FLD
                GO TO FIN-VAL-FLD-CCR.
       FIN-VAL-FLD-CCR.
           EXIT.
       
       VAL-OPC-MSC-TVOC SECTION.
       INI-VAL-OPC-MSC-TVOC.
       FIN-VAL-OPC-MSC-TVOC.
           EXIT.
       
       VAL-OPC-COD-TOOC SECTION.
       INI-VAL-OPC-COD-TOOC.
       FIN-VAL-OPC-COD-TOOC.
           EXIT.
       
       VAL-OPC-COD-OFOC SECTION.
       INI-VAL-OPC-COD-OFOC.
       FIN-VAL-OPC-COD-OFOC.
           EXIT.
       
       VAL-CCR-CAI-IOPC SECTION.
       INI-VAL-CCR-CAI-IOPC.
           IF CCR-CAI-IOPC IN CCR-FLD NOT > SPACES
              MOVE SPACES TO CCR-CAI-IOPC IN CCR-FLD.
       FIN-VAL-CCR-CAI-IOPC.
           EXIT.
       
       VAL-CCR-IIC-IOPC SECTION.
       INI-VAL-CCR-IIC-IOPC.
       FIN-VAL-CCR-IIC-IOPC.
           EXIT.
       
       VAL-FRM-GLS-TVOC SECTION.
       INI-VAL-FRM-GLS-TVOC.
       FIN-VAL-FRM-GLS-TVOC.
           EXIT.
       
       VAL-FRM-GLS-TOOC SECTION.
       INI-VAL-FRM-GLS-TOOC.
       FIN-VAL-FRM-GLS-TOOC.
           EXIT.
       
       VAL-FRM-GLS-OFOC SECTION.
       INI-VAL-FRM-GLS-OFOC.
       FIN-VAL-FRM-GLS-OFOC.
           EXIT.
       
       VAL-CCR-NUM-IDLC SECTION.
       INI-VAL-CCR-NUM-IDLC.
       FIN-VAL-CCR-NUM-IDLC.
           EXIT.
       
       VAL-CCR-NUM-ICCR SECTION.
       INI-VAL-CCR-NUM-ICCR.
       FIN-VAL-CCR-NUM-ICCR.
           EXIT.
       
       VAL-FRM-GLS-NOMC SECTION.
       INI-VAL-FRM-GLS-NOMC.
           IF FRM-GLS-NOMC IN CCR-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-NOMC IN CCR-FLD.
       FIN-VAL-FRM-GLS-NOMC.
           EXIT.
       
       VAL-FRM-IND-ICLI SECTION.
       INI-VAL-FRM-IND-ICLI.
           IF FRM-IND-ICLI IN CCR-FLD NOT > SPACES
              MOVE SPACES TO FRM-IND-ICLI IN CCR-FLD.
      *VAL-IDC Valida identificador
           MOVE FRM-IND-ICLI IN CCR-FLD TO IDC-IND-RUTV.
           PERFORM VAL-IDC.
           MOVE IDC-IND-RUTV TO FRM-IND-ICLI IN CCR-FLD.
       FIN-VAL-FRM-IND-ICLI.
           EXIT.
       
       VAL-FRM-GLS-ICLI SECTION.
       INI-VAL-FRM-GLS-ICLI.
           IF FRM-GLS-ICLI IN CCR-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-ICLI IN CCR-FLD.
      *VAL-GLS para IDC valida glosa ( extension ) de identificador
           MOVE FRM-GLS-ICLI IN CCR-FLD TO IDC-GLS-RUTV.
           PERFORM VAL-GLS.
           MOVE IDC-GLS-RUTV TO FRM-GLS-ICLI IN CCR-FLD.
       FIN-VAL-FRM-GLS-ICLI.
           EXIT.
       
       VAL-FRM-NUM-ICLI SECTION.
       INI-VAL-FRM-NUM-ICLI.
       FIN-VAL-FRM-NUM-ICLI.
           EXIT.
       
       VAL-FRM-VRF-ICLI SECTION.
       INI-VAL-FRM-VRF-ICLI.
           IF FRM-VRF-ICLI IN CCR-FLD NOT > SPACES
              MOVE SPACES TO FRM-VRF-ICLI IN CCR-FLD.
      *VAL-RUT Valida Rut para campo IDC
           MOVE IDC-ZERO-S TO IDC-ZERO.
           MOVE FRM-NUM-ICLI IN CCR-FLD TO IDC-NUM-RUTV.
           MOVE FRM-VRF-ICLI IN CCR-FLD TO IDC-VRF-RUTV.
           PERFORM VAL-RUT.
       FIN-VAL-FRM-VRF-ICLI.
           EXIT.
       
       VAL-FRM-CAI-ICLI SECTION.
       INI-VAL-FRM-CAI-ICLI.
       FIN-VAL-FRM-CAI-ICLI.
           EXIT.
       
       VAL-FRM-IIC-ICLI SECTION.
       INI-VAL-FRM-IIC-ICLI.
       FIN-VAL-FRM-IIC-ICLI.
           EXIT.
       
       VAL-FRM-GLS-NOMA SECTION.
       INI-VAL-FRM-GLS-NOMA.
           IF FRM-GLS-NOMA IN CCR-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-NOMA IN CCR-FLD.
       FIN-VAL-FRM-GLS-NOMA.
           EXIT.
       
       VAL-FRM-IND-IAVA SECTION.
       INI-VAL-FRM-IND-IAVA.
           IF FRM-IND-IAVA IN CCR-FLD NOT > SPACES
              MOVE SPACES TO FRM-IND-IAVA IN CCR-FLD.
      *VAL-IDC Valida identificador
           MOVE FRM-IND-IAVA IN CCR-FLD TO IDC-IND-RUTV.
           PERFORM VAL-IDC.
           MOVE IDC-IND-RUTV TO FRM-IND-IAVA IN CCR-FLD.
       FIN-VAL-FRM-IND-IAVA.
           EXIT.
       
       VAL-FRM-GLS-IAVA SECTION.
       INI-VAL-FRM-GLS-IAVA.
           IF FRM-GLS-IAVA IN CCR-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-IAVA IN CCR-FLD.
      *VAL-GLS para IDC valida glosa ( extension ) de identificador
           MOVE FRM-GLS-IAVA IN CCR-FLD TO IDC-GLS-RUTV.
           PERFORM VAL-GLS.
           MOVE IDC-GLS-RUTV TO FRM-GLS-IAVA IN CCR-FLD.
       FIN-VAL-FRM-GLS-IAVA.
           EXIT.
       
       VAL-FRM-NUM-IAVA SECTION.
       INI-VAL-FRM-NUM-IAVA.
       FIN-VAL-FRM-NUM-IAVA.
           EXIT.
       
       VAL-FRM-VRF-IAVA SECTION.
       INI-VAL-FRM-VRF-IAVA.
           IF FRM-VRF-IAVA IN CCR-FLD NOT > SPACES
              MOVE SPACES TO FRM-VRF-IAVA IN CCR-FLD.
      *VAL-RUT Valida Rut para campo IDC
           MOVE IDC-ZERO-S TO IDC-ZERO.
           MOVE FRM-NUM-IAVA IN CCR-FLD TO IDC-NUM-RUTV.
           MOVE FRM-VRF-IAVA IN CCR-FLD TO IDC-VRF-RUTV.
           PERFORM VAL-RUT.
       FIN-VAL-FRM-VRF-IAVA.
           EXIT.
       
       VAL-FRM-CAI-IAVA SECTION.
       INI-VAL-FRM-CAI-IAVA.
       FIN-VAL-FRM-CAI-IAVA.
           EXIT.
       
       VAL-FRM-IIC-IAVA SECTION.
       INI-VAL-FRM-IIC-IAVA.
       FIN-VAL-FRM-IIC-IAVA.
           EXIT.
       
       VAL-CCR-IND-SCRA SECTION.
       INI-VAL-CCR-IND-SCRA.
       FIN-VAL-CCR-IND-SCRA.
           EXIT.
       
       VAL-CCR-FEC-FCCR SECTION.
       INI-VAL-CCR-FEC-FCCR.
      *VAL-FEC Validar Fecha
           MOVE CCR-FEC-FCCR IN CCR-FLD TO FEC-FECH.
           MOVE FEC-FORM-FEC TO FEC-FORM.
           MOVE FEC-FBLK-S  TO FEC-FBLK.
           MOVE FEC-CHOY-LE TO FEC-CHOY.
           MOVE FEC-VHBL-S TO FEC-VHBL.
           MOVE FEC-VAL-FEC TO FEC-CMND.
           PERFORM PRO-FEC.
           IF FEC-STAT-OKS
               MOVE FEC-FECH TO CCR-FEC-FCCR IN CCR-FLD
           ELSE
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE FEC-MENS     TO FRM-MENS
               GO TO FIN-VAL-CCR-FEC-FCCR.
      *<<<< CCR_FEC_FCCR
           IF CCR-FEC-FCCR IN CCR-FLD NOT > ZEROES
               PERFORM GET-FHOY
               MOVE HOY-DHOY TO CCR-NUM-DCCR IN CCR-FLD
               MOVE HOY-MHOY TO CCR-NUM-MCCR IN CCR-FLD
               MOVE HOY-SHOY TO CCR-NUM-SCCR IN CCR-FLD
               MOVE HOY-AHOY TO CCR-NUM-ACCR IN CCR-FLD.
           IF CCR-NUM-ICCR IN CCR-FLD > 1
               GO TO FCCR-VAR-CMBS.
      *    leer OPC y comparar las fechas
           MOVE CCR-CAI-IOPC IN CCR-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE CCR-IIC-IOPC IN CCR-FLD TO OPC-IIC-IOPC IN OPC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-OPC.
           IF NOT FIO-STAT-OKS
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'OPE    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CCR-FEC-FCCR.
           MOVE CCR-NUM-DCCR IN CCR-FLD  TO WSS-NUM-DCOM IN WSS-VARI.
           MOVE CCR-NUM-MCCR IN CCR-FLD  TO WSS-NUM-MCOM IN WSS-VARI.
           MOVE CCR-NUM-SCCR IN CCR-FLD  TO WSS-NUM-SCOM IN WSS-VARI.
           MOVE CCR-NUM-ACCR IN CCR-FLD  TO WSS-NUM-ACOM IN WSS-VARI.
           IF WSS-FEC-FCOM IN WSS-VARI < OPC-FEC-FCOL IN OPC
      *        ccr-fec-fccr < opc-fec-fcol
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'FECFCCRFCOL<' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
           GO TO FIN-VAL-CCR-FEC-FCCR.
       FCCR-VAR-CMBS.
      *    restar 1 al NRO-CAMBIO-ACTUAL y comparar las fechas
           SUBTRACT 1 FROM   CCR-NUM-ICCR IN CCR-FLD
                      GIVING CCR-NUM-ICCR IN CCR.
           MOVE CCR-CAI-IOPC IN CCR-FLD TO CCR-CAI-IOPC IN CCR.
           MOVE CCR-IIC-IOPC IN CCR-FLD TO CCR-IIC-IOPC IN CCR.
           MOVE CCR-NUM-IDLC IN CCR-FLD TO CCR-NUM-IDLC IN CCR.
      *     MOVE CCR-NUM-ICCR IN CCR-FLD TO CCR-NUM-ICCR IN CCR.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-CCR.
      *     ADD 1 TO CCR-NUM-ICCR IN CCR-FLD.
           IF FIO-STAT-OKS
               MOVE CCR-NUM-DCCR IN CCR-FLD  TO WSS-NUM-DCOM IN WSS-VARI
               MOVE CCR-NUM-MCCR IN CCR-FLD  TO WSS-NUM-MCOM IN WSS-VARI
               MOVE CCR-NUM-SCCR IN CCR-FLD  TO WSS-NUM-SCOM IN WSS-VARI
               MOVE CCR-NUM-ACCR IN CCR-FLD  TO WSS-NUM-ACOM IN WSS-VARI
               IF WSS-FEC-FCOM IN WSS-VARI < CCR-FEC-FCCR IN CCR
      *            ccr-fec-fccr < ccr-fec-fccr anterior
                   MOVE 'COL' TO MSG-COD-SIST
                   MOVE 'FCCRFCCRANT<' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   GO TO FIN-VAL-CCR-FEC-FCCR
               ELSE
                   GO TO FIN-VAL-CCR-FEC-FCCR.
      *>>>>
       FIN-VAL-CCR-FEC-FCCR.
           EXIT.
       
       VAL-FRM-GLS-SCRA SECTION.
       INI-VAL-FRM-GLS-SCRA.
       FIN-VAL-FRM-GLS-SCRA.
           EXIT.
       
       VAL-CCR-FEC-FING SECTION.
       INI-VAL-CCR-FEC-FING.
      *VAL-FEC Validar Fecha
           MOVE CCR-FEC-FING IN CCR-FLD TO FEC-FECH.
           MOVE FEC-FORM-FEC TO FEC-FORM.
           MOVE FEC-FBLK-S  TO FEC-FBLK.
           MOVE FEC-CHOY-LE TO FEC-CHOY.
           MOVE FEC-VHBL-S TO FEC-VHBL.
           MOVE FEC-VAL-FEC TO FEC-CMND.
           PERFORM PRO-FEC.
           IF FEC-STAT-OKS
               MOVE FEC-FECH TO CCR-FEC-FING IN CCR-FLD
           ELSE
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE FEC-MENS     TO FRM-MENS
               GO TO FIN-VAL-CCR-FEC-FING.
      *<<<< CCR_FEC_FING
           IF CCR-FEC-FING IN CCR-FLD NOT > ZEROES
               MOVE CCR-FEC-FCCR IN CCR-FLD TO CCR-FEC-FING IN CCR-FLD
               GO TO FIN-VAL-CCR-FEC-FING.

      *     MOVE CCR-FEC-FCCR IN CCR-FLD TO WSS-FEC-FEC1 IN WSS-VARI.
           MOVE CCR-NUM-SCCR IN CCR-FLD TO WSS-NUM-SEC1 IN WSS-VARI.
           MOVE CCR-NUM-ACCR IN CCR-FLD TO WSS-NUM-AEC1 IN WSS-VARI.
           MOVE CCR-NUM-MCCR IN CCR-FLD TO WSS-NUM-MEC1 IN WSS-VARI.
           MOVE CCR-NUM-DCCR IN CCR-FLD TO WSS-NUM-DEC1 IN WSS-VARI.
           
      *     MOVE CCR-FEC-FING IN CCR-FLD TO WSS-FEC-FEC2 IN WSS-VARI.
           MOVE CCR-NUM-SING IN CCR-FLD TO WSS-NUM-SEC2 IN WSS-VARI.
           MOVE CCR-NUM-AING IN CCR-FLD TO WSS-NUM-AEC2 IN WSS-VARI.
           MOVE CCR-NUM-MING IN CCR-FLD TO WSS-NUM-MEC2 IN WSS-VARI.
           MOVE CCR-NUM-DING IN CCR-FLD TO WSS-NUM-DEC2 IN WSS-VARI.

           IF WSS-FEC-FEC2 IN WSS-VARI < WSS-FEC-FEC1 IN WSS-VARI
      *GET-MSG Busca mensaje en tabla
      *        ccr-fec-fing < ccr-fec-fccr      
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'FECFINGFCCR<' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CCR-FEC-FING.
      *>>>>
       FIN-VAL-CCR-FEC-FING.
           EXIT.
       
       VAL-CCR-IND-SCRT SECTION.
       INI-VAL-CCR-IND-SCRT.
           IF CCR-IND-SCRT IN CCR-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-CCR-IND-SCRT.
      *<<<< CCR_IND_SCRT
           IF CCR-IND-SCRT IN CCR-FLD = CCR-IND-SCRA IN CCR-FLD
      *        nueva situacion cartera debe ser distinta a la anterior
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'INDSCRTSCRA=' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CCR-IND-SCRT.
           IF CCR-IND-SCRT IN CCR-FLD = 'VIG' AND
              CCR-IND-SCRA IN CCR-FLD NOT > SPACES
      *        nueva situacion cartera debe ser distinta a la anterior
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'INDSCRTSCRA=' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CCR-IND-SCRT.
      *>>>>
      *VAL-IND Valida miscelaneo de tabla
           MOVE 'COL' TO MSC-COD-SIST.
           MOVE 'SCRT' TO MSC-COD-TMSC IN MSC.
           MOVE CCR-IND-SCRT IN CCR-FLD TO MSC-COD-CMSC IN MSC.
           PERFORM VAL-IND.
           IF MSG-COD-MENS = 'COD    NEX' OR
              CCR-IND-SCRT IN CCR-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-SCRT IN CCR-FLD
           ELSE
               MOVE MSC-GLS-DESC IN MSC TO
                    FRM-GLS-SCRT IN CCR-FLD.
       FIN-VAL-CCR-IND-SCRT.
           EXIT.
       
       VAL-CCR-COD-RCCR SECTION.
       INI-VAL-CCR-COD-RCCR.
       FIN-VAL-CCR-COD-RCCR.
           EXIT.
       
       VAL-FRM-GLS-SCRT SECTION.
       INI-VAL-FRM-GLS-SCRT.
       FIN-VAL-FRM-GLS-SCRT.
           EXIT.
       
       VAL-FRM-GLS-RCCR SECTION.
       INI-VAL-FRM-GLS-RCCR.
       FIN-VAL-FRM-GLS-RCCR.
           EXIT.
       
       VAL-DLC-IND-SCRT SECTION.
       INI-VAL-DLC-IND-SCRT.
       FIN-VAL-DLC-IND-SCRT.
           EXIT.
       
       VAL-CON-KEY-CCR SECTION.
       INI-VAL-CON-KEY-CCR.
      *<<<< INI_VAL_CON_KEY_CCR
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8
               GO TO FIN-VAL-CON-KEY-CCR.
      *>>>>
           IF     CCR-CAI-IOPC IN CCR-FLD NOT > SPACES 
              AND CCR-IIC-IOPC IN CCR-FLD NOT > ZEROES 
              AND CCR-NUM-IDLC IN CCR-FLD NOT > ZEROES 
              AND CCR-NUM-ICCR IN CCR-FLD NOT > ZEROES 
              AND FRM-GLS-NOMC IN CCR-FLD NOT > SPACES 
              AND FRM-IND-ICLI IN CCR-FLD NOT > SPACES 
              AND FRM-GLS-ICLI IN CCR-FLD NOT > SPACES 
              AND FRM-NUM-ICLI IN CCR-FLD NOT > ZEROES 
              AND FRM-VRF-ICLI IN CCR-FLD NOT > SPACES 
              AND FRM-CAI-ICLI IN CCR-FLD NOT > SPACES 
              AND FRM-IIC-ICLI IN CCR-FLD NOT > SPACES 
              AND FRM-GLS-NOMA IN CCR-FLD NOT > SPACES 
              AND FRM-IND-IAVA IN CCR-FLD NOT > SPACES 
              AND FRM-GLS-IAVA IN CCR-FLD NOT > SPACES 
              AND FRM-NUM-IAVA IN CCR-FLD NOT > ZEROES 
              AND FRM-VRF-IAVA IN CCR-FLD NOT > SPACES 
              AND FRM-CAI-IAVA IN CCR-FLD NOT > SPACES 
              AND FRM-IIC-IAVA IN CCR-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CCR.
      *<<<< FIN_VAL_CON_KEY_CCR
           IF NOT ( ( CCR-IIC-IOPC IN CCR-FLD > ZEROES   AND
                      CCR-NUM-IDLC IN CCR-FLD > ZEROES   AND
                      CCR-NUM-ICCR IN CCR-FLD > ZEROES ) OR

                    ( CCR-IIC-IOPC IN CCR-FLD     > ZEROES   AND
                      CCR-NUM-IDLC IN CCR-FLD     > ZEROES   AND
                      CCR-NUM-ICCR IN CCR-FLD NOT > ZEROES ) OR

                    ( FRM-NUM-ICLI IN CCR-FLD > ZEROES   AND 
                      FRM-NUM-IAVA IN CCR-FLD > ZEROES ) OR

                    ( FRM-NUM-ICLI IN CCR-FLD > ZEROES ) OR

                    ( FRM-NUM-IAVA IN CCR-FLD > ZEROES ) OR

                    ( FRM-GLS-NOMC IN CCR-FLD > SPACES   AND
                      FRM-GLS-NOMA IN CCR-FLD > SPACES ) OR

                    ( FRM-GLS-NOMC IN CCR-FLD > SPACES ) OR

                    ( FRM-GLS-NOMA IN CCR-FLD > SPACES ) )
               MOVE 'GNS'          TO MSG-COD-SIST
               MOVE 'VALCONKEY' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC   TO FRM-MENS
               MOVE FRM-SUAR-MAL   TO FRM-SUAR.
      *>>>>
       FIN-VAL-CON-KEY-CCR.
           EXIT.
       
       VAL-CON-IKY-CCR SECTION.
       INI-VAL-CON-IKY-CCR.
           IF     CCR-CAI-IOPC IN CCR-FLD NOT > SPACES 
              AND CCR-IIC-IOPC IN CCR-FLD NOT > ZEROES 
              AND CCR-NUM-IDLC IN CCR-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CCR.
      *<<<< FIN_VAL_CON_IKY_CCR
           IF CCR-IIC-IOPC  IN CCR-FLD NOT > ZEROES
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'IICIOPCBLK' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
           ELSE
               IF CCR-NUM-IDLC IN CCR-FLD NOT > ZEROES
                   MOVE 'GNS'          TO MSG-COD-SIST
                   MOVE 'VALCONKEY' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC   TO FRM-MENS
                   MOVE FRM-SUAR-MAL   TO FRM-SUAR.
      *>>>>
       FIN-VAL-CON-IKY-CCR.
           EXIT.
       
       VAL-CON-FLD-CCR SECTION.
       INI-VAL-CON-FLD-CCR.
      *<<<< VAL_CON_FLD_CCR
           IF CCR-FEC-FCCR IN CCR-FLD     > ZEROES AND
              CCR-FEC-FING IN CCR-FLD     > ZEROES
               MOVE CCR-FEC-FCCR IN CCR-FLD TO FEC-FEC1
               MOVE CCR-FEC-FING IN CCR-FLD TO FEC-FEC2
               MOVE FEC-FORM-FEC TO FEC-FORM
               MOVE FEC-CMP-FEC  TO FEC-CMND
               PERFORM PRO-FEC
               IF FEC-STAT-GRT
                   MOVE 'COL' TO MSG-COD-SIST
                   MOVE 'FECFCCRFING>' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   GO TO FIN-VAL-CON-FLD-CCR.
      *>>>>
       FIN-VAL-CON-FLD-CCR.
           EXIT.
       
       VAL-CON-CCR SECTION.
       INI-VAL-CON-CCR.
       FIN-VAL-CON-CCR.
           EXIT.
       
       VAL-NUM-KEY-CCR SECTION.
       INI-VAL-NUM-KEY-CCR.
           PERFORM VAL-NUM-IKY-CCR.
           PERFORM VAL-NUM-MKY-CCR.
           PERFORM VAL-NUM-AKY-CCR.
       FIN-VAL-NUM-KEY-CCR.
           EXIT.
       
       VAL-NUM-IKY-CCR SECTION.
       INI-VAL-NUM-IKY-CCR.
       VAL-NUM-CCR-IIC-IOPC.
           MOVE CCR-IIC-IOPC-ALF IN CCR-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           CALL 'GNSPVNUM' USING NUM-VARI.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CCR-IIC-IOPC IN CCR-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CCR-IIC-IOPC-LEN IN CCR-FLD
                GO TO FIN-VAL-NUM-IKY-CCR.
       VAL-NUM-CCR-NUM-IDLC.
           MOVE CCR-NUM-IDLC-ALF IN CCR-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           CALL 'GNSPVNUM' USING NUM-VARI.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CCR-NUM-IDLC IN CCR-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CCR-NUM-IDLC-LEN IN CCR-FLD
                GO TO FIN-VAL-NUM-IKY-CCR.
       FIN-VAL-NUM-IKY-CCR.
           EXIT.
       
       VAL-NUM-MKY-CCR SECTION.
       INI-VAL-NUM-MKY-CCR.
       FIN-VAL-NUM-MKY-CCR.
           EXIT.
       
       VAL-NUM-AKY-CCR SECTION.
       INI-VAL-NUM-AKY-CCR.
       VAL-NUM-CCR-NUM-ICCR.
           MOVE CCR-NUM-ICCR-ALF IN CCR-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 2 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           CALL 'GNSPVNUM' USING NUM-VARI.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CCR-NUM-ICCR IN CCR-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CCR-NUM-ICCR-LEN IN CCR-FLD
                GO TO FIN-VAL-NUM-AKY-CCR.
       VAL-NUM-FRM-NUM-ICLI.
           MOVE FRM-NUM-ICLI-ALF IN CCR-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           CALL 'GNSPVNUM' USING NUM-VARI.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO FRM-NUM-ICLI IN CCR-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO FRM-NUM-ICLI-LEN IN CCR-FLD
                GO TO FIN-VAL-NUM-AKY-CCR.
       VAL-NUM-FRM-NUM-IAVA.
           MOVE FRM-NUM-IAVA-ALF IN CCR-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           CALL 'GNSPVNUM' USING NUM-VARI.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO FRM-NUM-IAVA IN CCR-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO FRM-NUM-IAVA-LEN IN CCR-FLD
                GO TO FIN-VAL-NUM-AKY-CCR.
       FIN-VAL-NUM-AKY-CCR.
           EXIT.
       
       VAL-NUM-FLD-CCR SECTION.
       INI-VAL-NUM-FLD-CCR.
       FIN-VAL-NUM-FLD-CCR.
           EXIT.
       
       EDT-KEY-CCR SECTION.
       INI-EDT-KEY-CCR.
           PERFORM EDT-IKY-CCR.
           PERFORM EDT-MKY-CCR.
           PERFORM EDT-AKY-CCR.
       FIN-EDT-KEY-CCR.
           EXIT.
       
       EDT-IKY-CCR SECTION.
       INI-EDT-IKY-CCR.
           IF CCR-IIC-IOPC-LEN IN CCR-FLD = -9
                MOVE -1 TO CCR-IIC-IOPC-LEN IN CCR-FLD
                GO TO FIN-EDT-IKY-CCR.
           MOVE CCR-IIC-IOPC IN CCR-FLD TO CCR-IIC-IOPC-EDT IN CCR-FLD.
           IF CCR-NUM-IDLC-LEN IN CCR-FLD = -9
                MOVE -1 TO CCR-NUM-IDLC-LEN IN CCR-FLD
                GO TO FIN-EDT-IKY-CCR.
           MOVE CCR-NUM-IDLC IN CCR-FLD TO CCR-NUM-IDLC-EDT IN CCR-FLD.
       FIN-EDT-IKY-CCR.
           EXIT.
       
       EDT-MKY-CCR SECTION.
       INI-EDT-MKY-CCR.
       FIN-EDT-MKY-CCR.
           EXIT.
       
       EDT-AKY-CCR SECTION.
       INI-EDT-AKY-CCR.
           IF CCR-NUM-ICCR-LEN IN CCR-FLD = -9
                MOVE -1 TO CCR-NUM-ICCR-LEN IN CCR-FLD
                GO TO FIN-EDT-AKY-CCR.
           MOVE CCR-NUM-ICCR IN CCR-FLD TO CCR-NUM-ICCR-EDT IN CCR-FLD.
           IF FRM-NUM-ICLI-LEN IN CCR-FLD = -9
                MOVE -1 TO FRM-NUM-ICLI-LEN IN CCR-FLD
                GO TO FIN-EDT-AKY-CCR.
           MOVE FRM-NUM-ICLI IN CCR-FLD TO FRM-NUM-ICLI-EDT IN CCR-FLD.
           IF FRM-NUM-IAVA-LEN IN CCR-FLD = -9
                MOVE -1 TO FRM-NUM-IAVA-LEN IN CCR-FLD
                GO TO FIN-EDT-AKY-CCR.
           MOVE FRM-NUM-IAVA IN CCR-FLD TO FRM-NUM-IAVA-EDT IN CCR-FLD.
       FIN-EDT-AKY-CCR.
           EXIT.
       
       EDT-FLD-CCR SECTION.
       INI-EDT-FLD-CCR.
       FIN-EDT-FLD-CCR.
           EXIT.
