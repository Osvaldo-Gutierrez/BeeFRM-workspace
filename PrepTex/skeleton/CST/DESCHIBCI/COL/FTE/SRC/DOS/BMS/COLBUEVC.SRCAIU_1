       SET-TAG-COLBUEVC SECTION.
       INI-SET-TAG-COLBUEVC.
           MOVE
           '<IDNMOD NOM=$COLBUEVC$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$11-Jul-19 05:24 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUEVC.
           EXIT.
           
       RET-ALL-EVC SECTION.
       INI-RET-ALL-EVC.
           PERFORM KEY-ALL-EVC.
           PERFORM RET-OPC-EVC.
           PERFORM RET-EVC-EVC.
           PERFORM RET-PVC-EVC.
       FIN-RET-ALL-EVC.
           EXIT.
       
       RET-OPC-EVC SECTION.
       INI-RET-OPC-EVC.
           MOVE OPC-MSC-TVOC IN EVC-FLD TO OPC-MSC-TVOC IN OPC.
           MOVE OPC-COD-TOOC IN EVC-FLD TO OPC-COD-TOOC IN OPC.
           MOVE OPC-COD-OFOC IN EVC-FLD TO OPC-COD-OFOC IN OPC.
       FIN-RET-OPC-EVC.
           EXIT.
       
       RET-EVC-EVC SECTION.
       INI-RET-EVC-EVC.
           MOVE EVC-NUM-IDLC IN EVC-FLD TO EVC-NUM-IDLC IN EVC.
           MOVE EVC-NUM-VIGU IN EVC-FLD TO EVC-NUM-VIGU IN EVC.
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE EVC-NUM-DPVC IN EVC-FLD TO EVC-NUM-DPVC IN EVC.
           MOVE EVC-NUM-MPVC IN EVC-FLD TO EVC-NUM-MPVC IN EVC.
           MOVE EVC-NUM-SPVC IN EVC-FLD TO EVC-NUM-SPVC IN EVC.
           MOVE EVC-NUM-APVC IN EVC-FLD TO EVC-NUM-APVC IN EVC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE EVC-NUM-DPVE IN EVC-FLD TO EVC-NUM-DPVE IN EVC.
           MOVE EVC-MSC-TCVE IN EVC-FLD TO EVC-MSC-TCVE IN EVC.
           MOVE EVC-NUM-DESD IN EVC-FLD TO EVC-NUM-DESD IN EVC.
           MOVE EVC-VAL-VENC IN EVC-FLD TO EVC-VAL-VENC IN EVC.
           MOVE EVC-NUM-SEVE IN EVC-FLD TO EVC-NUM-SEVE IN EVC.
           MOVE EVC-IND-UTSV IN EVC-FLD TO EVC-IND-UTSV IN EVC.
           MOVE EVC-SGV-FVAR IN EVC-FLD TO EVC-SGV-FVAR IN EVC.
           MOVE EVC-IND-CAUT IN EVC-FLD TO EVC-IND-CAUT IN EVC.
           MOVE EVC-IND-FVAR IN EVC-FLD TO EVC-IND-FVAR IN EVC.
       FIN-RET-EVC-EVC.
           EXIT.
       
       RET-PVC-EVC SECTION.
       INI-RET-PVC-EVC.
       FIN-RET-PVC-EVC.
           EXIT.
       
       KEY-ALL-EVC SECTION.
       INI-KEY-ALL-EVC.
           PERFORM KEY-OPC-EVC.
           PERFORM KEY-EVC-EVC.
           PERFORM KEY-PVC-EVC.
       FIN-KEY-ALL-EVC.
           EXIT.
       
       KEY-OPC-EVC SECTION.
       INI-KEY-OPC-EVC.
       FIN-KEY-OPC-EVC.
           EXIT.
       
       KEY-EVC-EVC SECTION.
       INI-KEY-EVC-EVC.
           MOVE EVC-CAI-IOPC IN EVC-FLD TO EVC-CAI-IOPC IN EVC.
           MOVE EVC-IIC-IOPC IN EVC-FLD TO EVC-IIC-IOPC IN EVC.
           MOVE EVC-NUM-IEVC IN EVC-FLD TO EVC-NUM-IEVC IN EVC.
       FIN-KEY-EVC-EVC.
           EXIT.
       
       KEY-PVC-EVC SECTION.
       INI-KEY-PVC-EVC.
           MOVE PVC-CIC-IPVC IN EVC-FLD TO PVC-CIC-IPVC IN PVC.
       FIN-KEY-PVC-EVC.
           EXIT.
       
       SCR-CLRA-EVC SECTION.
       INI-SCR-CLRA-EVC.
           PERFORM CLR-KEY-EVC.
           PERFORM CLR-FLD-EVC.
       FIN-SCR-CLRA-EVC.
           EXIT.
       
       CLR-KEY-EVC SECTION.
       INI-CLR-KEY-EVC.
           MOVE SPACES TO EVC-CAI-IOPC IN EVC-FLD.
           MOVE ZEROES TO EVC-IIC-IOPC IN EVC-FLD.
           MOVE ZEROES TO EVC-NUM-IEVC IN EVC-FLD.
           MOVE SPACES TO PVC-CIC-IPVC IN EVC-FLD.
       FIN-CLR-KEY-EVC.
           EXIT.
       
       CLR-FLD-EVC SECTION.
       INI-CLR-FLD-EVC.
           MOVE SPACES TO FRM-GLS-TVOC IN EVC-FLD.
           MOVE SPACES TO FRM-GLS-TOOC IN EVC-FLD.
           MOVE SPACES TO FRM-GLS-OFOC IN EVC-FLD.
           MOVE ZEROES TO FRM-NUM-DURV IN EVC-FLD.
           MOVE SPACES TO FRM-IND-UTDV IN EVC-FLD.
           MOVE SPACES TO FRM-GLS-UTDV IN EVC-FLD.
           MOVE ZEROES TO FRM-NUM-SPVE IN EVC-FLD.
           MOVE SPACES TO FRM-IND-UTPV IN EVC-FLD.
           MOVE SPACES TO FRM-GLS-UTPV IN EVC-FLD.
           MOVE SPACES TO FRM-GLS-TCVE IN EVC-FLD.
           MOVE SPACES TO FRM-GLS-VCOC IN EVC-FLD.
           MOVE SPACES TO FRM-GLS-UTSV IN EVC-FLD.
           PERFORM CLR-OPC-EVC.
           PERFORM CLR-EVC-EVC.
           PERFORM CLR-PVC-EVC.
       FIN-CLR-FLD-EVC.
           EXIT.
       
       CLR-OPC-EVC SECTION.
       INI-CLR-OPC-EVC.
           MOVE SPACES TO OPC-MSC-TVOC IN EVC-FLD.
           MOVE SPACES TO OPC-COD-TOOC IN EVC-FLD.
           MOVE SPACES TO OPC-COD-OFOC IN EVC-FLD.
       FIN-CLR-OPC-EVC.
           EXIT.
       
       CLR-EVC-EVC SECTION.
       INI-CLR-EVC-EVC.
           MOVE ZEROES TO EVC-NUM-IDLC IN EVC-FLD.
           MOVE ZEROES TO EVC-NUM-VIGU IN EVC-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO EVC-FEC-FPVC IN EVC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO EVC-NUM-DPVE IN EVC-FLD.
           MOVE SPACES TO EVC-MSC-TCVE IN EVC-FLD.
           MOVE ZEROES TO EVC-NUM-DESD IN EVC-FLD.
           MOVE ZEROES TO EVC-VAL-VENC IN EVC-FLD.
           MOVE ZEROES TO EVC-NUM-SEVE IN EVC-FLD.
           MOVE SPACES TO EVC-IND-UTSV IN EVC-FLD.
           MOVE ZEROES TO EVC-SGV-FVAR IN EVC-FLD.
           MOVE SPACES TO EVC-IND-CAUT IN EVC-FLD.
           MOVE SPACES TO EVC-IND-FVAR IN EVC-FLD.
      *<<<< FIN_CLR_EVC_EVC
           IF SCR-IND-ENG IN SCR-VEC-ENG(SCR-IPRO) = 'S'
               MOVE SCR-NUM-IDOC IN SCR-VARI TO EVC-NUM-IDLC IN EVC-FLD.
      *PTC ini
           IF SCR-OFRM-SRV
               MOVE SCR-NUM-IDOC IN SCR-VARI TO EVC-NUM-IDLC IN EVC-FLD.
      *PTC fin
      *>>>>
       FIN-CLR-EVC-EVC.
           EXIT.
       
       CLR-PVC-EVC SECTION.
       INI-CLR-PVC-EVC.
       FIN-CLR-PVC-EVC.
           EXIT.
       
       FST-KEY-EVC SECTION.
       INI-FST-KEY-EVC.
           MOVE -1 TO EVC-CAI-IOPC-LEN IN EVC-FLD.
       FIN-FST-KEY-EVC.
           EXIT.
       
       FST-FLD-EVC SECTION.
       INI-FST-FLD-EVC.
           MOVE -1 TO FRM-NUM-DURV-LEN IN EVC-FLD.
       FIN-FST-FLD-EVC.
           EXIT.
       
       PRO-KEY-EVC SECTION.
       INI-PRO-KEY-EVC.
           PERFORM PRO-MKY-EVC.
       FIN-PRO-KEY-EVC.
           EXIT.
       
       PRO-MKY-EVC SECTION.
       INI-PRO-MKY-EVC.
       FIN-PRO-MKY-EVC.
           EXIT.
       
       PRO-FLD-EVC SECTION.
       INI-PRO-FLD-EVC.
           MOVE FRM-CPIM-AEY TO FRM-NUM-DURV-ATR IN EVC-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-UTDV-ATR IN EVC-FLD.
           MOVE FRM-CPIM-AEY TO FRM-NUM-SPVE-ATR IN EVC-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-UTPV-ATR IN EVC-FLD.
           PERFORM PRO-OPC-EVC.
           PERFORM PRO-EVC-EVC.
           PERFORM PRO-PVC-EVC.
       FIN-PRO-FLD-EVC.
           EXIT.
       
       PRO-OPC-EVC SECTION.
       INI-PRO-OPC-EVC.
       FIN-PRO-OPC-EVC.
           EXIT.
       
       PRO-EVC-EVC SECTION.
       INI-PRO-EVC-EVC.
           MOVE FRM-CPIM-AEY TO EVC-NUM-IDLC-ATR IN EVC-FLD.
           MOVE FRM-CPIM-AEY TO EVC-NUM-VIGU-ATR IN EVC-FLD.
           MOVE FRM-CPIM-AEY TO EVC-FEC-FPVC-ATR IN EVC-FLD.
           MOVE FRM-CPIM-AEY TO EVC-NUM-DPVE-ATR IN EVC-FLD.
           MOVE FRM-CPIM-AEY TO EVC-MSC-TCVE-ATR IN EVC-FLD.
           MOVE FRM-CPIM-AEY TO EVC-NUM-DESD-ATR IN EVC-FLD.
           MOVE FRM-CPIM-AEY TO EVC-VAL-VENC-ATR IN EVC-FLD.
           MOVE FRM-CPIM-AEY TO EVC-NUM-SEVE-ATR IN EVC-FLD.
           MOVE FRM-CPIM-AEY TO EVC-IND-UTSV-ATR IN EVC-FLD.
           MOVE FRM-CPIM-AEY TO EVC-SGV-FVAR-ATR IN EVC-FLD.
           MOVE FRM-CPIM-AEY TO EVC-IND-CAUT-ATR IN EVC-FLD.
           MOVE FRM-CPIM-AEY TO EVC-IND-FVAR-ATR IN EVC-FLD.
       FIN-PRO-EVC-EVC.
           EXIT.
       
       PRO-PVC-EVC SECTION.
       INI-PRO-PVC-EVC.
       FIN-PRO-PVC-EVC.
           EXIT.
       
       UNP-KEY-EVC SECTION.
       INI-UNP-KEY-EVC.
           PERFORM UNP-IKY-EVC.
           PERFORM UNP-AKY-EVC.
           PERFORM UNP-MKY-EVC.
       FIN-UNP-KEY-EVC.
           EXIT.
       
       UNP-IKY-EVC SECTION.
       INI-UNP-IKY-EVC.
           MOVE FRM-CPIM-UBY TO EVC-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO EVC-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO PVC-CIC-IPVC-ATR.
       FIN-UNP-IKY-EVC.
           EXIT.
       
       UNP-MKY-EVC SECTION.
       INI-UNP-MKY-EVC.
       FIN-UNP-MKY-EVC.
           EXIT.
       
       UNP-AKY-EVC SECTION.
       INI-UNP-AKY-EVC.
           MOVE FRM-CPIM-UBY TO EVC-NUM-IEVC-ATR.
       FIN-UNP-AKY-EVC.
           EXIT.
       
       UNP-FLD-EVC SECTION.
       INI-UNP-FLD-EVC.
           MOVE FRM-CPIM-UBY TO FRM-NUM-DURV-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IND-UTDV-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-NUM-SPVE-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IND-UTPV-ATR IN EVC-FLD.
           PERFORM UNP-OPC-EVC.
           PERFORM UNP-EVC-EVC.
           PERFORM UNP-PVC-EVC.
       FIN-UNP-FLD-EVC.
           EXIT.
       
       UNP-OPC-EVC SECTION.
       INI-UNP-OPC-EVC.
       FIN-UNP-OPC-EVC.
           EXIT.
       
       UNP-EVC-EVC SECTION.
       INI-UNP-EVC-EVC.
           MOVE FRM-CPIM-UBY TO EVC-NUM-IDLC-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO EVC-NUM-VIGU-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO EVC-FEC-FPVC-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO EVC-NUM-DPVE-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO EVC-MSC-TCVE-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO EVC-NUM-DESD-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO EVC-VAL-VENC-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO EVC-NUM-SEVE-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO EVC-IND-UTSV-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO EVC-SGV-FVAR-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO EVC-IND-CAUT-ATR IN EVC-FLD.
           MOVE FRM-CPIM-UBY TO EVC-IND-FVAR-ATR IN EVC-FLD.
       FIN-UNP-EVC-EVC.
           EXIT.
       
       UNP-PVC-EVC SECTION.
       INI-UNP-PVC-EVC.
       FIN-UNP-PVC-EVC.
           EXIT.
       
       VAL-MKY-EVC SECTION.
       INI-VAL-MKY-EVC.
       FIN-VAL-MKY-EVC.
           EXIT.
       
       VAL-FLD-EVC SECTION.
       INI-VAL-FLD-EVC.
           PERFORM VAL-OPC-MSC-TVOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-MSC-TVOC-LEN IN EVC-FLD
                GO TO FIN-VAL-FLD-EVC.
           PERFORM VAL-OPC-COD-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-TOOC-LEN IN EVC-FLD
                GO TO FIN-VAL-FLD-EVC.
           PERFORM VAL-OPC-COD-OFOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPC-COD-OFOC-LEN IN EVC-FLD
                GO TO FIN-VAL-FLD-EVC.
           PERFORM VAL-FRM-GLS-TVOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TVOC-LEN IN EVC-FLD
                GO TO FIN-VAL-FLD-EVC.
           PERFORM VAL-FRM-GLS-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TOOC-LEN IN EVC-FLD
                GO TO FIN-VAL-FLD-EVC.
           PERFORM VAL-FRM-GLS-OFOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-OFOC-LEN IN EVC-FLD
                GO TO FIN-VAL-FLD-EVC.
           PERFORM VAL-FRM-NUM-DURV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-NUM-DURV-LEN IN EVC-FLD
                GO TO FIN-VAL-FLD-EVC.
           PERFORM VAL-FRM-IND-UTDV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IND-UTDV-LEN IN EVC-FLD
                GO TO FIN-VAL-FLD-EVC.
           PERFORM VAL-FRM-GLS-UTDV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-UTDV-LEN IN EVC-FLD
                GO TO FIN-VAL-FLD-EVC.
           PERFORM VAL-EVC-NUM-IDLC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EVC-NUM-IDLC-LEN IN EVC-FLD
                GO TO FIN-VAL-FLD-EVC.
           PERFORM VAL-EVC-NUM-VIGU.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EVC-NUM-VIGU-LEN IN EVC-FLD
                GO TO FIN-VAL-FLD-EVC.
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-EVC-FEC-FPVC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                EVC-FEC-FPVC-LEN IN EVC-FLD
                GO TO FIN-VAL-FLD-EVC.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-EVC-NUM-DPVE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EVC-NUM-DPVE-LEN IN EVC-FLD
                GO TO FIN-VAL-FLD-EVC.
           PERFORM VAL-FRM-NUM-SPVE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-NUM-SPVE-LEN IN EVC-FLD
                GO TO FIN-VAL-FLD-EVC.
           PERFORM VAL-FRM-IND-UTPV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IND-UTPV-LEN IN EVC-FLD
                GO TO FIN-VAL-FLD-EVC.
           PERFORM VAL-FRM-GLS-UTPV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-UTPV-LEN IN EVC-FLD
                GO TO FIN-VAL-FLD-EVC.
           PERFORM VAL-EVC-MSC-TCVE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EVC-MSC-TCVE-LEN IN EVC-FLD
                GO TO FIN-VAL-FLD-EVC.
           PERFORM VAL-FRM-GLS-TCVE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TCVE-LEN IN EVC-FLD
                GO TO FIN-VAL-FLD-EVC.
           PERFORM VAL-EVC-NUM-DESD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EVC-NUM-DESD-LEN IN EVC-FLD
                GO TO FIN-VAL-FLD-EVC.
           PERFORM VAL-EVC-VAL-VENC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EVC-VAL-VENC-LEN IN EVC-FLD
                GO TO FIN-VAL-FLD-EVC.
           PERFORM VAL-EVC-NUM-SEVE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EVC-NUM-SEVE-LEN IN EVC-FLD
                GO TO FIN-VAL-FLD-EVC.
           PERFORM VAL-FRM-GLS-VCOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-VCOC-LEN IN EVC-FLD
                GO TO FIN-VAL-FLD-EVC.
           PERFORM VAL-EVC-IND-UTSV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EVC-IND-UTSV-LEN IN EVC-FLD
                GO TO FIN-VAL-FLD-EVC.
           PERFORM VAL-FRM-GLS-UTSV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-UTSV-LEN IN EVC-FLD
                GO TO FIN-VAL-FLD-EVC.
           PERFORM VAL-EVC-SGV-FVAR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO EVC-SGV-FVAR-LEN IN EVC-FLD
