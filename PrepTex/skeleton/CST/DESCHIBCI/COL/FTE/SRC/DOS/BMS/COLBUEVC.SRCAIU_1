               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CON-FLD-EVC.

           IF EVC-IND-FVAR IN EVC-FLD NOT > SPACES
               GO TO FIN-VAL-CON-FLD-EVC.
           MOVE 'COL'                   TO MSC-COD-SIST.
           MOVE 'FVAR'                  TO MSC-COD-TMSC IN MSC.
           MOVE EVC-IND-FVAR IN EVC-FLD TO MSC-COD-CMSC IN MSC.
           PERFORM BUS-MSC.
           IF MSC-CIC-CMSC IN MSC = 'M'
               GO TO CIC-CMSC-M.
           IF MSC-CIC-CMSC IN MSC = 'P'
               GO TO CIC-CMSC-P.
           GO TO FIN-VAL-CON-FLD-EVC.
       CIC-CMSC-P.
           IF NOT ( ( PVC-SGV-FPMI IN PVC = ZEROES OR
              PVC-SGV-FPMI IN PVC NOT > EVC-SGV-FVAR IN EVC-FLD ) AND
              ( PVC-SGV-FPMA IN PVC = ZEROES OR
              PVC-SGV-FPMA IN PVC NOT < EVC-SGV-FVAR IN EVC-FLD ) )
               MOVE 'COL'          TO MSG-COD-SIST
               MOVE 'SGVFVARNORNG' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN1
               MOVE PVC-SGV-FPMI IN PVC  TO WSS-SGV-VAL1-EDT
                                                 IN WSS-VARI
               MOVE '/'                  TO WSS-GLS-GUI1
                                                 IN WSS-VARI
               MOVE PVC-SGV-FPMA IN PVC  TO WSS-SGV-VAL2-EDT
                                                 IN WSS-VARI
               MOVE WSS-MEN1 IN WSS-VARI TO FRM-MEN2
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
           GO TO FIN-VAL-CON-FLD-EVC.
       CIC-CMSC-M.
           IF NOT ( ( PVC-SGV-FMMI IN PVC = ZEROES OR
              PVC-SGV-FMMI IN PVC NOT > EVC-SGV-FVAR IN EVC-FLD ) AND
              ( PVC-SGV-FMMA IN PVC = ZEROES OR
              PVC-SGV-FMMA IN PVC NOT < EVC-SGV-FVAR IN EVC-FLD ) )
               MOVE 'COL'          TO MSG-COD-SIST
               MOVE 'SGVFVARNORNG' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN1
               MOVE PVC-SGV-FMMI IN PVC  TO WSS-SGV-VAL1-EDT
                                                 IN WSS-VARI
               MOVE '/'                  TO WSS-GLS-GUI1
                                                 IN WSS-VARI
               MOVE PVC-SGV-FMMA IN PVC  TO WSS-SGV-VAL2-EDT
                                                 IN WSS-VARI
               MOVE WSS-MEN1 IN WSS-VARI TO FRM-MEN2
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
           GO TO FIN-VAL-CON-FLD-EVC.
      *
      *>>>>
       FIN-VAL-CON-FLD-EVC.
           EXIT.
       
       VAL-CON-EVC SECTION.
       INI-VAL-CON-EVC.
       FIN-VAL-CON-EVC.
           EXIT.
       
       VAL-NUM-KEY-EVC SECTION.
       INI-VAL-NUM-KEY-EVC.
           PERFORM VAL-NUM-IKY-EVC.
           PERFORM VAL-NUM-MKY-EVC.
           PERFORM VAL-NUM-AKY-EVC.
       FIN-VAL-NUM-KEY-EVC.
           EXIT.
       
       VAL-NUM-IKY-EVC SECTION.
       INI-VAL-NUM-IKY-EVC.
       VAL-NUM-EVC-IIC-IOPC.
           MOVE EVC-IIC-IOPC-ALF IN EVC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO EVC-IIC-IOPC IN EVC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO EVC-IIC-IOPC-LEN IN EVC-FLD
                GO TO FIN-VAL-NUM-IKY-EVC.
       FIN-VAL-NUM-IKY-EVC.
           EXIT.
       
       VAL-NUM-MKY-EVC SECTION.
       INI-VAL-NUM-MKY-EVC.
       FIN-VAL-NUM-MKY-EVC.
           EXIT.
       
       VAL-NUM-AKY-EVC SECTION.
       INI-VAL-NUM-AKY-EVC.
       VAL-NUM-EVC-NUM-IEVC.
           MOVE EVC-NUM-IEVC-ALF IN EVC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO EVC-NUM-IEVC IN EVC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO EVC-NUM-IEVC-LEN IN EVC-FLD
                GO TO FIN-VAL-NUM-AKY-EVC.
       FIN-VAL-NUM-AKY-EVC.
           EXIT.
       
       VAL-NUM-FLD-EVC SECTION.
       INI-VAL-NUM-FLD-EVC.
       VAL-NUM-FRM-NUM-DURV.
           MOVE FRM-NUM-DURV-ALF IN EVC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO FRM-NUM-DURV IN EVC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-NUM-DURV-LEN IN EVC-FLD
                GO TO FIN-VAL-NUM-FLD-EVC.
       VAL-NUM-EVC-NUM-IDLC.
           MOVE EVC-NUM-IDLC-ALF IN EVC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO EVC-NUM-IDLC IN EVC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO EVC-NUM-IDLC-LEN IN EVC-FLD
                GO TO FIN-VAL-NUM-FLD-EVC.
       VAL-NUM-EVC-NUM-VIGU.
           MOVE EVC-NUM-VIGU-ALF IN EVC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO EVC-NUM-VIGU IN EVC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO EVC-NUM-VIGU-LEN IN EVC-FLD
                GO TO FIN-VAL-NUM-FLD-EVC.
       VAL-NUM-EVC-NUM-DPVE.
           MOVE EVC-NUM-DPVE-ALF IN EVC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 2 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO EVC-NUM-DPVE IN EVC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO EVC-NUM-DPVE-LEN IN EVC-FLD
                GO TO FIN-VAL-NUM-FLD-EVC.
       VAL-NUM-FRM-NUM-SPVE.
           MOVE FRM-NUM-SPVE-ALF IN EVC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 5 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO FRM-NUM-SPVE IN EVC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-NUM-SPVE-LEN IN EVC-FLD
                GO TO FIN-VAL-NUM-FLD-EVC.
       VAL-NUM-EVC-NUM-DESD.
           MOVE EVC-NUM-DESD-ALF IN EVC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 2 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO EVC-NUM-DESD IN EVC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO EVC-NUM-DESD-LEN IN EVC-FLD
                GO TO FIN-VAL-NUM-FLD-EVC.
       VAL-NUM-EVC-VAL-VENC.
           MOVE EVC-VAL-VENC-ALF IN EVC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9V TO EVC-VAL-VENC IN EVC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO EVC-VAL-VENC-LEN IN EVC-FLD
                GO TO FIN-VAL-NUM-FLD-EVC.
       VAL-NUM-EVC-NUM-SEVE.
           MOVE EVC-NUM-SEVE-ALF IN EVC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO EVC-NUM-SEVE IN EVC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO EVC-NUM-SEVE-LEN IN EVC-FLD
                GO TO FIN-VAL-NUM-FLD-EVC.
       VAL-NUM-EVC-SGV-FVAR.
           MOVE EVC-SGV-FVAR-ALF IN EVC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NS9V TO EVC-SGV-FVAR IN EVC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO EVC-SGV-FVAR-LEN IN EVC-FLD
                GO TO FIN-VAL-NUM-FLD-EVC.
       FIN-VAL-NUM-FLD-EVC.
           EXIT.
       
       EDT-KEY-EVC SECTION.
       INI-EDT-KEY-EVC.
           PERFORM EDT-IKY-EVC.
           PERFORM EDT-MKY-EVC.
           PERFORM EDT-AKY-EVC.
       FIN-EDT-KEY-EVC.
           EXIT.
       
       EDT-IKY-EVC SECTION.
       INI-EDT-IKY-EVC.
           IF EVC-IIC-IOPC-LEN IN EVC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EVC-IIC-IOPC-LEN IN EVC-FLD
                GO TO FIN-EDT-IKY-EVC.
           MOVE EVC-IIC-IOPC IN EVC-FLD TO EVC-IIC-IOPC-EDT IN EVC-EDT.
           MOVE EVC-IIC-IOPC-EDT IN EVC-EDT TO 
                EVC-IIC-IOPC-ALF IN EVC-FLD.
       FIN-EDT-IKY-EVC.
           EXIT.
       
       EDT-MKY-EVC SECTION.
       INI-EDT-MKY-EVC.
       FIN-EDT-MKY-EVC.
           EXIT.
       
       EDT-AKY-EVC SECTION.
       INI-EDT-AKY-EVC.
           IF EVC-NUM-IEVC-LEN IN EVC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EVC-NUM-IEVC-LEN IN EVC-FLD
                GO TO FIN-EDT-AKY-EVC.
           MOVE EVC-NUM-IEVC IN EVC-FLD TO EVC-NUM-IEVC-EDT IN EVC-EDT.
           MOVE EVC-NUM-IEVC-EDT IN EVC-EDT TO 
                EVC-NUM-IEVC-ALF IN EVC-FLD.
       FIN-EDT-AKY-EVC.
           EXIT.
       
       EDT-FLD-EVC SECTION.
       INI-EDT-FLD-EVC.
           IF FRM-NUM-DURV-LEN IN EVC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-DURV-LEN IN EVC-FLD
                GO TO FIN-EDT-FLD-EVC.
           MOVE FRM-NUM-DURV IN EVC-FLD TO FRM-NUM-DURV-EDT IN EVC-EDT.
           MOVE FRM-NUM-DURV-EDT IN EVC-EDT TO 
                FRM-NUM-DURV-ALF IN EVC-FLD.
           IF EVC-NUM-IDLC-LEN IN EVC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EVC-NUM-IDLC-LEN IN EVC-FLD
                GO TO FIN-EDT-FLD-EVC.
           MOVE EVC-NUM-IDLC IN EVC-FLD TO EVC-NUM-IDLC-EDT IN EVC-EDT.
           MOVE EVC-NUM-IDLC-EDT IN EVC-EDT TO 
                EVC-NUM-IDLC-ALF IN EVC-FLD.
           IF EVC-NUM-VIGU-LEN IN EVC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EVC-NUM-VIGU-LEN IN EVC-FLD
                GO TO FIN-EDT-FLD-EVC.
           MOVE EVC-NUM-VIGU IN EVC-FLD TO EVC-NUM-VIGU-EDT IN EVC-EDT.
           MOVE EVC-NUM-VIGU-EDT IN EVC-EDT TO 
                EVC-NUM-VIGU-ALF IN EVC-FLD.
           IF EVC-NUM-DPVE-LEN IN EVC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EVC-NUM-DPVE-LEN IN EVC-FLD
                GO TO FIN-EDT-FLD-EVC.
           MOVE EVC-NUM-DPVE IN EVC-FLD TO EVC-NUM-DPVE-EDT IN EVC-EDT.
           MOVE EVC-NUM-DPVE-EDT IN EVC-EDT TO 
                EVC-NUM-DPVE-ALF IN EVC-FLD.
           IF FRM-NUM-SPVE-LEN IN EVC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-SPVE-LEN IN EVC-FLD
                GO TO FIN-EDT-FLD-EVC.
           MOVE FRM-NUM-SPVE IN EVC-FLD TO FRM-NUM-SPVE-EDT IN EVC-EDT.
           MOVE FRM-NUM-SPVE-EDT IN EVC-EDT TO 
                FRM-NUM-SPVE-ALF IN EVC-FLD.
           IF EVC-NUM-DESD-LEN IN EVC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EVC-NUM-DESD-LEN IN EVC-FLD
                GO TO FIN-EDT-FLD-EVC.
           MOVE EVC-NUM-DESD IN EVC-FLD TO EVC-NUM-DESD-EDT IN EVC-EDT.
           MOVE EVC-NUM-DESD-EDT IN EVC-EDT TO 
                EVC-NUM-DESD-ALF IN EVC-FLD.
           IF EVC-VAL-VENC-LEN IN EVC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EVC-VAL-VENC-LEN IN EVC-FLD
                GO TO FIN-EDT-FLD-EVC.
           MOVE EVC-VAL-VENC IN EVC-FLD TO EVC-VAL-VENC-EDT IN EVC-EDT.
           MOVE EVC-VAL-VENC-EDT IN EVC-EDT TO 
                EVC-VAL-VENC-ALF IN EVC-FLD.
           IF EVC-NUM-SEVE-LEN IN EVC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EVC-NUM-SEVE-LEN IN EVC-FLD
                GO TO FIN-EDT-FLD-EVC.
           MOVE EVC-NUM-SEVE IN EVC-FLD TO EVC-NUM-SEVE-EDT IN EVC-EDT.
           MOVE EVC-NUM-SEVE-EDT IN EVC-EDT TO 
                EVC-NUM-SEVE-ALF IN EVC-FLD.
           IF EVC-SGV-FVAR-LEN IN EVC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO EVC-SGV-FVAR-LEN IN EVC-FLD
                GO TO FIN-EDT-FLD-EVC.
           MOVE EVC-SGV-FVAR IN EVC-FLD TO EVC-SGV-FVAR-EDT IN EVC-EDT.
           MOVE EVC-SGV-FVAR-EDT IN EVC-EDT TO 
                EVC-SGV-FVAR-ALF IN EVC-FLD.
       FIN-EDT-FLD-EVC.
           EXIT.
      *<<<< EOF
      * ANL-FEC-FPVC SECTION.
      * INI-ANL-FEC-FPVC.                 
      *     MOVE EVC-FEC-FPVC IN EVC-FLD TO FEC-FECH.
      *     MOVE FEC-FORM-FEC TO FEC-FORM.
      *     MOVE FEC-FBLK-S  TO FEC-FBLK.
      *     MOVE FEC-CHOY-N TO FEC-CHOY.
      *     IF OPC-IND-AFER IN OPC = 'S'
      *         MOVE FEC-VHBL-S TO FEC-VHBL
      *     ELSE
      *         MOVE FEC-VHBL-N TO FEC-VHBL.
      *     MOVE FEC-VAL-FEC TO FEC-CMND.
      *     PERFORM PRO-FEC.
      *     IF FEC-STAT-OKS
      *         MOVE FEC-FECH TO EVC-FEC-FPVC IN EVC-FLD
      *     ELSE
      *         MOVE FRM-SUAR-MAL TO FRM-SUAR
      *         MOVE FEC-MENS     TO FRM-MENS
      *         GO TO FIN-ANL-FEC-FPVC.
      *     IF OPC-IND-AFER IN OPC = 'S' AND
      *        FEC-STAT-FER
      *         MOVE FEC-SIG-HBL TO FEC-CMND
      *         PERFORM CAL-FEC
      *         MOVE FRM-SUAR-MAL TO FRM-SUAR
      *         MOVE 'COL' TO MSG-COD-SIST
      *         MOVE 'FECFEVCPXHBL' TO MSG-COD-MENS
      *         PERFORM GET-MSG
      *         STRING MSG-GLS-DESC
      *                FEC-FECH
      *                MSG-GLS-DATA DELIMITED BY SIZE
      *         INTO FRM-MENS.
      * FIN-ANL-FEC-FPVC.                 
      *     EXIT.
      *ALD/18-JUL-1988
       CMP-FEC-NFEC SECTION.
       INI-CMP-FEC-NFEC.
           MOVE WSS-FEC-NFE1 IN WSS-VARI TO FEC-FEC1.
           MOVE WSS-FEC-NFE2 IN WSS-VARI TO FEC-FEC2.
           MOVE FEC-FORM-FEC TO FEC-FORM.
           MOVE FEC-CMP-FEC TO FEC-CMND.
           PERFORM PRO-FEC.
           IF FEC-STAT-LES
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-CMP-FEC-NFEC.

           MOVE WSS-FEC-NFE1 IN WSS-VARI TO FEC-FEC1.
           MOVE WSS-FEC-NFE3 IN WSS-VARI TO FEC-FEC2.
           MOVE FEC-FORM-FEC TO FEC-FORM.
           MOVE FEC-CMP-FEC TO FEC-CMND.
           PERFORM PRO-FEC.
           IF FEC-STAT-GRT
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
       FIN-CMP-FEC-NFEC.
           EXIT.

       SUM-FEC-NFEC SECTION.
       INI-SUM-FEC-NFEC.
           MOVE 'TAB' TO MSC-COD-SIST.
           MOVE 'IPER' TO MSC-COD-TMSC IN MSC.
           PERFORM BUS-MSC.
           MOVE FEC-FORM-FEC    TO FEC-FORM.
           PERFORM SUM-FEC-FCOL.
      *    PERFORM PRO-FEC.
           PERFORM CAL-FEC.
       FIN-SUM-FEC-NFEC.
           EXIT.

       SUM-FEC-FCOL SECTION.
       INI-SUM-FEC-FCOL.
           MOVE OPC-NUM-DCOL IN OPC     TO FEC-ITM1.
           MOVE OPC-NUM-MCOL IN OPC     TO FEC-ITM2.
           MOVE OPC-NUM-SCOL IN OPC     TO FEC-ITM3.
           MOVE OPC-NUM-ACOL IN OPC     TO FEC-ITM4.
           IF MSC-CIC-CMSC = 'D'
               MOVE FEC-SUM-DIA              TO FEC-CMND
               MOVE WSS-NUM-NFEC IN WSS-VARI TO FEC-NDIA
           ELSE
               IF MSC-CIC-CMSC = 'M'
                   MOVE FEC-SUM-MES              TO FEC-CMND
                   MOVE WSS-NUM-NFEC IN WSS-VARI TO FEC-NMES
               ELSE
                   IF MSC-CIC-CMSC = 'A'
                       MOVE FEC-SUM-ANO             TO FEC-CMND
                       MOVE WSS-NUM-NFEC IN WSS-VARI TO FEC-NANO.
       FIN-SUM-FEC-FCOL.
           EXIT.
      *
       SEL-PER-FPVC SECTION.
       INI-SEL-PER-FPVC.
           IF MSC-CIC-CMSC = 'D'
               MOVE FEC-SUM-DIA             TO FEC-CMND
               MOVE FRM-NUM-SPVE IN EVC-FLD TO FEC-NDIA
           ELSE
           IF MSC-CIC-CMSC = 'M'
               MOVE FEC-SUM-MES             TO FEC-CMND
               MOVE FRM-NUM-SPVE IN EVC-FLD TO FEC-NMES
           ELSE
           IF MSC-CIC-CMSC = 'A'
               MOVE FEC-SUM-ANO             TO FEC-CMND
               MOVE FRM-NUM-SPVE IN EVC-FLD TO FEC-NANO.
       FIN-SEL-PER-FPVC.
           EXIT.

       SEL-PER-FUVC SECTION.
       INI-SEL-PER-FUVC.
           IF MSC-CIC-CMSC = 'D'
               MOVE FEC-SUM-DIA             TO FEC-CMND
               MOVE FRM-NUM-DURV IN EVC-FLD TO FEC-NDIA
           ELSE
           IF MSC-CIC-CMSC = 'M'
               MOVE FEC-SUM-MES             TO FEC-CMND
               MOVE FRM-NUM-DURV IN EVC-FLD TO FEC-NMES
           ELSE
           IF MSC-CIC-CMSC = 'A'
               MOVE FEC-SUM-ANO             TO FEC-CMND
               MOVE FRM-NUM-DURV IN EVC-FLD TO FEC-NANO.
       FIN-SEL-PER-FUVC.
           EXIT.

       SEL-PER-DFEC SECTION.
       INI-SEL-PER-DFEC.
           IF MSC-CIC-CMSC = 'D'
               MOVE FEC-DIF-DIA TO FEC-CMND
      *        PERFORM PRO-FEC
               PERFORM CAL-FEC
               MOVE FEC-NDIA TO WSS-NUM-DVDO
           ELSE
           IF MSC-CIC-CMSC = 'M'
               MOVE FEC-DIF-MES TO FEC-CMND
      *        PERFORM PRO-FEC
               PERFORM CAL-FEC
               MOVE FEC-NMES TO WSS-NUM-DVDO
           ELSE
           IF MSC-CIC-CMSC = 'A'
               MOVE FEC-DIF-ANO TO FEC-CMND
      *        PERFORM PRO-FEC
               PERFORM CAL-FEC
               MOVE FEC-NANO TO WSS-NUM-DVDO.
       FIN-SEL-PER-DFEC.
           EXIT.

       GET-PER-FEC SECTION.
       INI-GET-PER-FEC.
           MOVE FEC-RET-JUL             TO FEC-CMND.
           MOVE FEC-FORM-FEC            TO FEC-FORM.
           MOVE EVC-FEC-FPVC IN EVC-FLD TO FEC-FECH.
      *    PERFORM PRO-FEC.
           PERFORM CAL-FEC.
           MOVE FEC-FJUL            TO WSS-NUM-JUL.
           MOVE FEC-RET-JUL         TO FEC-CMND.
           MOVE FEC-FORM-FEC        TO FEC-FORM.
           MOVE OPC-NUM-DCOL IN OPC TO FEC-ITM1.
           MOVE OPC-NUM-MCOL IN OPC TO FEC-ITM2.
           MOVE OPC-NUM-SCOL IN OPC TO FEC-ITM3.
           MOVE OPC-NUM-ACOL IN OPC TO FEC-ITM4.
      *    PERFORM PRO-FEC.
           PERFORM CAL-FEC.
           SUBTRACT FEC-FJUL FROM WSS-NUM-JUL
                           GIVING FRM-NUM-SPVE IN EVC-FLD
           IF FRM-NUM-SPVE IN EVC-FLD NOT > ZEROES
