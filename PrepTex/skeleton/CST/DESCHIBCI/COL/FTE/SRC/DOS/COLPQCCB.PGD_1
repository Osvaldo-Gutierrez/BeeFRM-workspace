       INI-BRW-SCR-ACT.
       FIN-BRW-SCR-ACT.
           EXIT.

       SCR-PGM SECTION.
       INI-SCR-PGM.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-RST
                PERFORM GET-KEY
                IF FRM-STAT-ABT
                    MOVE SPACES TO FRM-MENS
                    PERFORM SCR-MSSG
                    PERFORM DESSCRTRL
                    GO TO FIN-SCR-PGM
                ELSE
                    MOVE SPACES TO FRM-MENS
                    PERFORM FND-KEY
                    IF FIO-STAT = FIO-STAT-NEX
                        IF FRM-MENS NOT > SPACES
                            MOVE 'REGNEX    ' TO MSG-COD-MENS
                            MOVE 'GNS' TO MSG-COD-SIST
                            PERFORM GET-MSG
                            MOVE MSG-GLS-DESC TO FRM-MENS
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-PGM
                        ELSE
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-PGM
                    ELSE
                        MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
                        PERFORM SCR-TRL
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-PGM.
       TRL-SCR-PGM.
           PERFORM DESSCRTRL.
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-PGM.
           EXIT.

       BRW-SCR-PGM SECTION.
       INI-BRW-SCR-PGM.
       FIN-BRW-SCR-PGM.
           EXIT.

       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               PERFORM SCR-CLRA-CCB
               PERFORM EDT-KEY-CCB
               PERFORM EDT-FLD-CCB
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-CCB
               PERFORM EDT-KEY-CCB
           ELSE
               PERFORM CLR-FLD-CCB
               PERFORM EDT-FLD-CCB.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

       SCR-KEY SECTION.
       INI-SCR-KEY.
           PERFORM SCR-RST.
           PERFORM GET-KEY.
           IF FRM-STAT-ABT
                   MOVE SPACES TO FRM-MENS
                   PERFORM SCR-MSSG
                   GO TO FIN-SCR-KEY.
       FND-SCR-KEY.
           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
           IF FIO-STAT-OKS
               PERFORM PUT-SCR
               MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
               GO TO FIN-SCR-KEY.
      *JSS ELSE
      *        PERFORM CLR-FLD-CCB.
       ERR-SCR-KEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR.
           IF FRM-MENS NOT > SPACES
               IF FIO-STAT-EOF
                   MOVE 'REGEOF    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
               IF FIO-STAT = FIO-STAT-NEX
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
           MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-SCR-KEY.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO CCB-FLD.
           MOVE LOW-VALUES TO CCB-EDT.
       FIN-SCR-RST.
           EXIT.

       GET-KEY SECTION.
       INI-GET-KEY.
           PERFORM SCR-GKEY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-KEY.
           EXIT.

       GET-IKY SECTION.
       INI-GET-IKY.
           PERFORM SCR-GIKY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-IKY.
           EXIT.

       FND-KEY SECTION.
       INI-FND-KEY.
      *<<<< INI_FND_KEY
      *>>>>
           PERFORM KEY-ALL-CCB.
      *<<<< FND_KEY
      *AFRI-INI/JUL2008 (MAE)
      *    VALIDA QUE LA OPERACION TENGA SALDO
           IF SCR-CCPP = 'ING'                                          
              PERFORM VAL-OPC-ZERO
              IF CCB-IND-ZERO IN CCB-VARI = 'S'
                  MOVE 'OPC ZEROES' TO CCB-COD-MENS IN CCB-VARI
                  MOVE 'COL'            TO MSG-COD-SIST
                  MOVE CCB-COD-MENS IN CCB-VARI TO MSG-COD-MENS
                  PERFORM GET-MSG
                  MOVE MSG-GLS-DESC     TO FRM-MENS
                  MOVE '00' TO FIO-STAT                                 
                  MOVE SCR-STAT-CON TO SCR-STAT                         
                  GO TO FIN-FND-KEY.

      *AFRI-FIN/JUL2008 (MAE)
           IF SCR-CCPP = 'ING' OR 'ELI'
               PERFORM BUS-COR-CCB
               PERFORM CLR-CCB
               GO TO CON-FND-KEY.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8
               PERFORM LEE-NXT-PRV
               IF FIO-STAT-OKS
                   PERFORM SCR-CLRA-CCB
                   GO TO CON-FND-KEY
               ELSE
                   MOVE SPACES TO PGM-STAT
                   IF FRM-FFLD = FRM-FFLD-PF7
                       MOVE 'COL' TO MSG-COD-SIST
                       MOVE 'CCB    NOPRV' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE '00' TO FIO-STAT
                       MOVE SCR-STAT-CON TO SCR-STAT
                       GO TO FIN-FND-KEY
                   ELSE
                       MOVE 'COL' TO MSG-COD-SIST
                       MOVE 'CCB    NONXT' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE '00' TO FIO-STAT
                       MOVE SCR-STAT-CON TO SCR-STAT
                       GO TO FIN-FND-KEY.
           IF CCB-IIC-IOPC IN CCB NOT > ZEROES
               PERFORM ANL-KEY-CCB
               IF NOT FIO-STAT-OKS
                   GO TO NXT-FND-KEY.
           IF CCB-NUM-ICCB IN CCB NOT > ZEROES 
               PERFORM BUS-COR-CCB
               PERFORM CLR-CCB.
       CON-FND-KEY.
      *>>>>
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-CCB.
           MOVE FIO-STAT TO PGM-STAT-CCB.
      *<<<< FIN_FND_KEY
           IF SCR-CCPP = 'ACT' AND NO-ASIGNE-CORRELATIVO AND 
              NOT FIO-STAT-OKS
               PERFORM BUS-COR-CCB
               PERFORM CLR-CCB
               MOVE '23' TO FIO-STAT.

      *AFRI-INI/JUL2008 (MAE)
      *    VALIDA QUE LA OPERACION TENGA SALDO
           IF FIO-STAT-OKS  AND  SCR-CCPP = 'MOD'                       
               PERFORM VAL-OPC-MOD
               IF (CCB-IND-ERRO IN CCB-VARI = 'S' OR
                   CCB-IND-MOD  IN CCB-VARI = 'S')
                    MOVE 'COL'            TO MSG-COD-SIST
                    MOVE CCB-COD-MENS IN CCB-VARI TO MSG-COD-MENS
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC     TO FRM-MENS
                    MOVE '00' TO FIO-STAT                               
                    MOVE SCR-STAT-CON TO SCR-STAT                       
                    GO TO FIN-FND-KEY.

      *AFRI-FIN/JUL2008 (MAE)
           IF FIO-STAT-OKS AND (SCR-CCPP = 'MOD' OR SCR-CCPP = 'ELI' OR 
              (SCR-CCPP = 'ACT' AND SCR-ACTU = SCR-ACTU-MOD))
                PERFORM BUS-SIG-CCB
                IF FIO-STAT-OKS
                    MOVE 'COL' TO MSG-COD-SIST
                    MOVE 'NUMICCBINV' TO MSG-COD-MENS
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    MOVE '00' TO FIO-STAT
                    MOVE SCR-STAT-CON TO SCR-STAT
                    GO TO FIN-FND-KEY
                ELSE
                    MOVE '00' TO FIO-STAT.

           IF FIO-STAT-OKS
               MOVE ADR-CCB-REQA TO SCR-CCB-REQA.
           IF  BUS-POR-KEYS = BUS-POR-CLIE
                IF FIO-STAT-OKS
                    PERFORM BUS-CCB-SCBA
                    MOVE '00' TO FIO-STAT
                    GO TO NXT-FND-KEY
                ELSE
                    PERFORM BUS-CCB-SCBA
                    MOVE '23' TO FIO-STAT
                    GO TO NXT-FND-KEY.
           IF FIO-STAT-OKS
               PERFORM BUS-CCB-SCBA
               PERFORM ANL-CIC-IOPC
               GO TO NXT-FND-KEY.
           IF ( SCR-CCPP = 'ING' OR SCR-CCPP = 'ACT' )
               PERFORM BUS-CCB-SCBA
               PERFORM ANL-CIC-IOPC
               IF FIO-STAT-OKS
                   PERFORM PUT-OPC-CCB
                   PERFORM PUT-DLC-CCB
                   MOVE '23' TO FIO-STAT
               ELSE
                   MOVE SCR-STAT-CON TO SCR-STAT
                   MOVE '00' TO FIO-STAT.
       NXT-FND-KEY.
           IF PGM-STAT-OPC-OKS AND 
              OPC-MSC-STAT IN OPC NOT = 'A'
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'OPC    NOACA' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE SCR-STAT-CON TO SCR-STAT
               IF  SCR-CCPP = 'ING'
                   MOVE '23' TO FIO-STAT
               ELSE
                   MOVE '00' TO FIO-STAT.
           
           GO TO EXT-FND-KEY.
      *>>>>
       FIN-FND-KEY.
           IF FIO-STAT = FIO-STAT-NEX
               MOVE 'S' TO SCR-IERR.
       EXT-FND-KEY.
           EXIT.
 
       PUT-SCR SECTION.
       INI-PUT-SCR.
           PERFORM PUT-ALL-CCB.
       FIN-PUT-SCR.
           EXIT.

       STP-EXIT SECTION.
       INI-STP-EXIT.
           MOVE SCR-PROG TO PGM-PROG.
           MOVE SCR-NTRN TO PGM-NTRN.

           MOVE SCR-PROV TO SCR-PROG.
           MOVE SCR-NTRV TO SCR-NTRN.

           MOVE PGM-PROG TO SCR-PROV
           MOVE PGM-NTRN TO SCR-NTRV.
       FIN-STP-EXIT.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO SYS-NTRN.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
      *PTC-INI
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE CCB-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO CCB-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GKEY SECTION.
       INI-SCR-GKEY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
      *<<<< PFS_GET_CON OR PFS_GET_ACC OR PFS_SCR_GKEY
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 OR FRM-FFLD = FRM-FFLD-PF7 )
      *>>>>
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-CCB
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO CCB-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GKEY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-CCB.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-CCB
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-CCB.
           PERFORM VAL-IKY-CCB.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-MKY-CCB.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-AKY-CCB.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-CON-KEY-CCB.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GKEY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-CCB.
       ERR-SCR-GKEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-CCB.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GKEY.
           PERFORM SCR-MSSG.

       SCR-GIKY SECTION.
       INI-SCR-GIKY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-CCB
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO CCB-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GIKY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-CCB.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-IKY-CCB
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-CCB.
           PERFORM VAL-IKY-CCB.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GIKY.
           PERFORM VAL-CON-IKY-CCB.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GIKY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-CCB.
       ERR-SCR-GIKY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-IKY-CCB.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GIKY.
           PERFORM SCR-MSSG.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
      *<<<< PFS_GET_CON OR PFS_GET_ACC OR PFS_SCR_GKEY
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 OR FRM-FFLD = FRM-FFLD-PF7 )
      *>>>>
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO CCB-GLS-MEN2
               MOVE SCR-PRMT     TO CCB-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO CCB-FLD.
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO CCB-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
      *<<<< INI_HBL_IKY      
           GO TO SAL-HBL-IKY.
      *>>>>
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE CCB-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO CCB-FLD
      * FLB  7-NOV-1994
               PERFORM CLR-KEY-CCB

               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO CCB-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO CCB-GLS-MENS.
           PERFORM PRO-MKY-CCB.
           PERFORM PRO-AKY-CCB.
           PERFORM UNP-IKY-CCB.
           PERFORM FST-KEY-CCB.
      *<<<< FIN_HBL_IKY
       SAL-HBL-IKY.
       
           IF SCR-DESDE-CMN                                             
               MOVE CCB-GLS-MENS TO FRM-MENS                            
               MOVE LOW-VALUES TO CCB-FLD                               
               MOVE FRM-MENS TO CCB-GLS-MENS.                           
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.                           
           PERFORM PRO-MKY-CCB.                                         
           PERFORM PRO-AKY-CCB.                                         
           PERFORM UNP-IKY-CCB.                                         
           PERFORM FST-KEY-CCB.                                         
           MOVE '1' TO FRM-CPIM-AEY.
      *>>>>
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
      *<<<< INI_HBL_KEY
           GO TO SAL-HBL-KEY.
      *>>>>
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE CCB-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO CCB-FLD
               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO CCB-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO CCB-GLS-MENS.
           PERFORM UNP-KEY-CCB.
           PERFORM FST-KEY-CCB.
      *<<<< FIN_HBL_KEY
       SAL-HBL-KEY.
       
           IF SCR-DESDE-CMN                                             
               MOVE CCB-GLS-MENS TO FRM-MENS                            
               MOVE LOW-VALUES TO CCB-FLD                               
               MOVE FRM-MENS TO CCB-GLS-MENS.                           
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.                           
           PERFORM UNP-KEY-CCB.                                         
           PERFORM FST-KEY-CCB.                                         
           MOVE '1' TO FRM-CPIM-AEY.
      *>>>>
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
           PERFORM PRO-IKY-CCB.
           PERFORM PRO-AKY-CCB.
           PERFORM UNP-MKY-CCB.
           PERFORM UNP-FLD-CCB.
           PERFORM FST-FLD-CCB.
      *<<<< FIN_HBL_FLD
      *BUS-USR busca usuario en tablas
           IF SCR-USER > SPACES
               MOVE SCR-USER TO USR-COD-USER IN USR
                                CCB-COD-RCCB IN CCB-FLD
               PERFORM BUS-USR
               MOVE USR-GLS-DESC IN USR TO FRM-GLS-RCCB IN CCB-FLD.
      *>>>>
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO CCB-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-CCB.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
      *PTC-INI
           IF SCR-STAT NOT = SCR-STPP
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
      *<<<< FIN_SCR_TRL
      *DAD
      *BUS-USR busca usuario en tablas
           IF SCR-STAT NOT = SCR-STAT-CON
               IF SCR-USER > SPACES
                   MOVE SCR-USER TO CCB-COD-RCCB IN CCB-FLD
                   MOVE SCR-USER TO CCB-COD-RCCB IN CCB
                   MOVE SCR-USER TO USR-COD-USER IN USR
                   PERFORM BUS-USR
                   MOVE USR-GLS-DESC IN USR TO FRM-GLS-RCCB IN CCB-FLD.
      *
      *MVG-INI 28-MAY-2019 IGUALACION COL
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
      *MVG-FIN 28-MAY-2019 IGUALACION COL
      *>>>>
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-CCB
               PERFORM EDT-FLD-CCB
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-CCB
               PERFORM EDT-FLD-CCB
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-CCB.
           PERFORM EDT-FLD-CCB.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

      * PGM-EBR-ALL SECTION.
      * INI-PGM-EBR-ALL.
      *     MOVE 'GNS' TO FIO-SIST.
      *     MOVE FIO-EBR-ALL TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
      * FIN-PGM-EBR-ALL.
      *     EXIT.

       COPY COLBQCCB.
       COPY GNSBVNUM.
       COPY COLBFCCB.
       COPY GNSBFMSG.
       COPY GNSBGSYS.
       COPY GNSBGDTC.
      *PTC-INI
      *PTC-SUB
      * ISP  10-MAY-2001 13:46:35 
      * COPY GNSBGFRM.
       COPY GNSBG3CF.
      *PTC-FIN
       COPY GNSBGMSG.
       COPY GNSBEKEY.
       COPY GNSBIABT.
       COPY GNSBBTAB.
       COPY GNSBFTAB.
       COPY GNSBBMSC.
       COPY GNSBVIND.
       COPY GNSBVMSC.
       COPY GNSBFMSC.
       COPY GNSBVIDC.
       COPY GNSBPVRF.
       COPY GNSBEGLS.
       COPY GNSBGFEC.
       COPY GNSBGHOY.
       COPY TABBBOFI.
       COPY TABBFOFI.
       COPY TABBBUSR.
       COPY TABBFUSR.
      *PTC-INI
       COPY GNSBGQUE.
       COPY GNSBGTSK.
      *PTC-FIN

      *<<<< EOF
       RST-STAT SECTION.
       INI-RST-STAT.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
       FIN-RST-STAT.
           EXIT.
           
       BUS-CCB-SCBA SECTION.
       INI-BUS-CCB-SCBA.
           IF CCB-NUM-ICCB IN CCB = 1
               GO TO FIN-BUS-CCB-SCBA.

           PERFORM PUT-ALL-CCB.
           SUBTRACT 1 FROM CCB-NUM-ICCB IN CCB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-CCB.
           IF FIO-STAT-OKS
               MOVE CCB-IND-SCBZ IN CCB TO CCB-IND-SCBA IN CCB-FLD
           ELSE
               MOVE SPACES TO CCB-IND-SCBA IN CCB-FLD.
      *BUS-MSC busca glosa de miscelaneo en tabla
           IF CCB-IND-SCBA IN CCB-FLD > SPACES
               MOVE 'COL' TO MSC-COD-SIST
               MOVE 'SCBZ' TO MSC-COD-TMSC IN MSC
               MOVE CCB-IND-SCBA IN CCB-FLD TO MSC-COD-CMSC IN MSC
               PERFORM BUS-MSC
               MOVE MSC-GLS-DESC IN MSC TO
                    FRM-GLS-SCBA IN CCB-FLD.
           PERFORM RET-ALL-CCB.
       FIN-BUS-CCB-SCBA.
           EXIT.

       CLR-CCB SECTION.
       INI-CLR-CCB.
           MOVE SPACES TO CCB-IND-SCBA IN CCB.
           MOVE SPACES TO CCB-COD-ATRN IN CCB.
           MOVE SPACES TO CCB-FEC-FCCB IN CCB.
           MOVE SPACES TO CCB-FEC-FING IN CCB.
           MOVE SPACES TO CCB-IND-SCBZ IN CCB.
           MOVE SPACES TO CCB-COD-RCCB IN CCB.
           MOVE ZEROES TO CCB-SGV-GAST IN CCB.
       FIN-CLR-CCB.
           EXIT.

       MOD-DLC SECTION.
       INI-MOD-DLC.
           MOVE CCB-CAI-IOPC IN CCB TO DLC-CAI-IOPC IN DLC.             
           MOVE CCB-IIC-IOPC IN CCB TO DLC-IIC-IOPC IN DLC.             
           MOVE CCB-NUM-IDLC IN CCB TO DLC-NUM-IDLC IN DLC.             
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.                            
           PERFORM COL-FIO-DLC.                                         
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-MOD-DLC.                                       
           IF SCR-CCPP = 'ELI'                                          
               MOVE CCB-IND-SCBA IN CCB-FLD TO DLC-IND-SCBZ IN DLC      
           ELSE                                                         
               MOVE CCB-IND-SCBZ IN CCB-FLD TO DLC-IND-SCBZ IN DLC.     
           MOVE FIO-MOD TO FIO-CMND.                                    
           PERFORM COL-FIO-DLC.                                         

       FIN-MOD-DLC.
           EXIT.
       BCK-OUT SECTION.
       INI-BCK-OUT.
           MOVE FIO-BCK-OUT TO FIO-CMND.
           PERFORM COL-FIO-DLC.
           IF FIO-STAT-OKS
      *        Para informar que el backout se realizo con exito
               MOVE '49' TO FIO-STAT.
       FIN-BCK-OUT.
           EXIT.

       LEE-NXT-PRV SECTION.
       INI-LEE-NXT-PRV.
           IF SCR-CCB-REQA > SPACES
               PERFORM LEE-NXT-PRV-CCB
           ELSE
               PERFORM LEE-NXT-PRV-CIC.
       FIN-LEE-NXT-PRV.
           EXIT.

       LEE-NXT-PRV-CCB SECTION.
       INI-LEE-NXT-PRV-CCB.
           MOVE SCR-CCB-REQA TO ADR-CCB-REQA.
           MOVE FIO-KDEF-S TO FIO-KDEF.
           IF FRM-FFLD = FRM-FFLD-PF7
               MOVE FIO-GET-PRV TO FIO-CMND
           ELSE
               MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM COL-FIO-CCB.
           IF FIO-STAT-OKS
               MOVE '00' TO PGM-STAT-CCB
               MOVE ADR-CCB-REQA TO SCR-CCB-REQA.
       FIN-LEE-NXT-PRV-CCB.
           EXIT.

       LEE-NXT-PRV-CIC SECTION.
       INI-LEE-NXT-PRV-CIC.
           IF CCB-IIC-IOPC IN CCB > ZEROES AND
              CCB-NUM-IDLC IN CCB > ZEROES AND
              CCB-NUM-ICCB IN CCB > ZEROES
               MOVE FIO-GET-KEY    TO FIO-CMND
               PERFORM COL-FIO-CCB
               IF FIO-STAT-OKS
                   IF FRM-FFLD = FRM-FFLD-PF7
                       MOVE FIO-GET-PRV TO FIO-CMND
                   ELSE
                       MOVE FIO-GET-NXT TO FIO-CMND
               ELSE
                   IF FRM-FFLD = FRM-FFLD-PF7
                       MOVE FIO-GET-LEQ TO FIO-CMND
                   ELSE
                       MOVE FIO-GET-NLS TO FIO-CMND
           ELSE
               IF FRM-FFLD = FRM-FFLD-PF7
                   MOVE FIO-GET-LEQ TO FIO-CMND
               ELSE
                   MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM COL-FIO-CCB.
           IF FIO-STAT-OKS
               MOVE '00' TO PGM-STAT-CCB
               MOVE ADR-CCB-REQA TO SCR-CCB-REQA.
       FIN-LEE-NXT-PRV-CIC.
           EXIT.

       BUS-SIG-CCB SECTION.
       INI-BUS-SIG-CCB.
           PERFORM PUT-ALL-CCB.
           ADD 1 TO CCB-NUM-ICCB IN CCB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-CCB.
           PERFORM RET-ALL-CCB.
       FIN-BUS-SIG-CCB.
           EXIT.

