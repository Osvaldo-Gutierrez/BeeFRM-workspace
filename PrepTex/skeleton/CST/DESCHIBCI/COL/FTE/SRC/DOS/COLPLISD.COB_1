       MAIN SECTION.
      *------------
       INI-MAIN.
      *<<<< MAX_FILE
      *>>>>
      *    MOVE SYS-MXFL TO SYS-CMND.
      *    PERFORM GNS-PRO-SYS.
       COPY GNSBGEDB.
      *    MOVE 'COL' TO RPT-SIST.
           MOVE 'COLPLISD' TO FIO-PROG.
           PERFORM GNS-BUS-IDD.

           MOVE FIO-INP      TO FIO-CMND.
           MOVE 'GNS'        TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
      *<<<< INI
      *>>>>
           SORT GNSSRT
       COPY COLRSISD.
      *<<<< RS
      *>>>>
           INPUT  PROCEDURE IS INP-SRT
           OUTPUT PROCEDURE IS OUT-SRT.
       FIN-MAIN.
      *<<<< FIN
      *>>>>
           IF PGM-STAT-SRT-MAL OR SORT-RETURN > 0
               PERFORM GNS-PRO-ABT
           ELSE
               PERFORM GNS-PRO-END.

       INP-SRT SECTION.
      *---------------
       INI-INP-SRT.
      *<<<< INI_INP
           ACCEPT WSS-VAL-MPAR IN WSS-VARI.
           DISPLAY 'MONTO PARAMETRO: ' WSS-VAL-MPAR IN WSS-VARI.
           MOVE WSS-VAL-MPAR IN WSS-VARI TO WSS-VAL-MPAR IN WSS-ISD.

           DISPLAY ' '.
           ACCEPT WSS-IND-DISP IN WSS-VARI.
           DISPLAY 'INDICADOR DE DISPLAY (S/N): '
                                 WSS-IND-DISP IN WSS-VARI.
           DISPLAY ' '.
           DISPLAY 'VALIDACION DE PARAMETROS.'.
           DISPLAY '-------------------------'.
           DISPLAY ' '.

           DISPLAY 'VALIDA MONTO DE PARAMETRO:'.
           IF WSS-VAL-MPAR IN WSS-VARI NOT NUMERIC
               DISPLAY 'ERROR PARAMETRO NUMERICO: '
                              WSS-VAL-MPAR IN WSS-VARI
               MOVE PGM-SMAL TO PGM-STAT-SRT
               GO TO FIN-INP-SRT.

           DISPLAY ' '.
           DISPLAY 'VALIDA INDICADOR DE DISPLAY'.
           IF WSS-IND-DISP IN WSS-VARI NOT = 'S' AND
                                       NOT = 'N'
               DISPLAY 'ASUME IND. DE DISPLA = N'
               MOVE 'N' TO WSS-IND-DISP IN WSS-VARI.
      *>>>>
           MOVE FIO-INP TO FIO-CMND.
           PERFORM COL-FIO-ADC.
       FST-INP-SRT.
      *<<<< INI_FST_INP
      *>>>>
           MOVE FIO-GET-FST TO FIO-CMND.
           PERFORM COL-FIO-ADC.
      *<<<< FIN_FST_INP
      *>>>>
       LUP-INP-SRT.
           IF NOT FIO-STAT-OKS
              GO TO FIN-INP-SRT.
      *<<<< LUP_INP
           IF WSS-IND-DISP IN WSS-VARI = 'S'
               DISPLAY ' '
               DISPLAY 'REG. ADC LEIDO: ' ADC.

      * Como las deudas en el archivo D01 y en el parametro son en
      * miles de pesos, se debe calcular la deuda tambien en miles
      * de pesos, pero en el reporte debe ir en pesos
           COMPUTE SRT-VAL-DEUT IN SRT = ADC-VAL-DEDI IN ADC +
                                         ADC-VAL-DEIN IN ADC +
                                         ADC-VAL-DECO IN ADC +
                                         ADC-VAL-DEFI IN ADC.

           IF SRT-VAL-DEUT IN SRT < WSS-VAL-MPAR IN WSS-VARI OR
              ADC-VAL-DEDI IN ADC NOT > ZEROES
               GO TO NXT-INP-SRT.

           IF WSS-IND-DISP IN WSS-VARI = 'S'
               DISPLAY 'DEUDA TOTAL         : '
                                    SRT-VAL-DEUT IN SRT
               DISPLAY 'DEUDA DIRECTA       : '
                                    ADC-VAL-DEDI IN ADC
               DISPLAY 'DEUDA INDIRECTA     : '
                                    ADC-VAL-DEIN IN ADC
               DISPLAY 'DEUDA COMPLEMENTARIA: '
                                    ADC-VAL-DECO IN ADC
               DISPLAY 'DEUDA SOLIDARIA     : '
                                    ADC-VAL-DEFI IN ADC
               DISPLAY 'REG. ADC GRABADO.'.

           ADD 1 TO WSS-NUM-DEUD IN WSS-ISD.
      *>>>>
       MOV-INP-SRT.
       COPY COLRMISD.
      *<<<< RM
      *>>>>
           RELEASE SRT.
       NXT-INP-SRT.
      *<<<< INI_NXT_INP
      *>>>>
           MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM COL-FIO-ADC.
      *<<<< FIN_NXT_INP
      *>>>>
           GO TO LUP-INP-SRT.
       FIN-INP-SRT.
      *<<<< FIN_INP
      *>>>>
           MOVE FIO-CLO TO FIO-CMND.
           PERFORM COL-FIO-ADC.

