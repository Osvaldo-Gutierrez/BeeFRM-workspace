      *
      * COLBUINT
      * Version 3.0.1 Nov-2005
      * 10-Dec-09 02:08 PM
      *
       SET-TAG-COLBUINT SECTION.
       INI-SET-TAG-COLBUINT.
           MOVE
           '<IDNMOD NOM=$COLBUINT$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$10-Dec-09 02:08 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-COLBUINT.
           EXIT.
           
       RET-ALL-INT SECTION.
       INI-RET-ALL-INT.
           PERFORM KEY-ALL-INT.
           PERFORM RET-INT-INT.
       FIN-RET-ALL-INT.
           EXIT.
       
       RET-INT-INT SECTION.
       INI-RET-INT-INT.
           MOVE INT-CIC-TINT IN INT-FLD TO INT-CIC-TINT IN INT.
           MOVE INT-EXT-CTAB IN INT-FLD TO INT-EXT-CTAB IN INT.
           MOVE INT-GLS-DESC IN INT-FLD TO INT-GLS-DESC IN INT.
           MOVE INT-GLS-ABRV IN INT-FLD TO INT-GLS-ABRV IN INT.
           MOVE INT-GLS-DCOR IN INT-FLD TO INT-GLS-DCOR IN INT.
           MOVE INT-IND-VIGE IN INT-FLD TO INT-IND-VIGE IN INT.
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE INT-NUM-DTRN IN INT-FLD TO INT-NUM-DTRN IN INT.
           MOVE INT-NUM-MTRN IN INT-FLD TO INT-NUM-MTRN IN INT.
           MOVE INT-NUM-STRN IN INT-FLD TO INT-NUM-STRN IN INT.
           MOVE INT-NUM-ATRN IN INT-FLD TO INT-NUM-ATRN IN INT.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
       FIN-RET-INT-INT.
           EXIT.
       
       KEY-ALL-INT SECTION.
       INI-KEY-ALL-INT.
           PERFORM KEY-INT-INT.
       FIN-KEY-ALL-INT.
           EXIT.
       
       KEY-INT-INT SECTION.
       INI-KEY-INT-INT.
           MOVE INT-MSC-BCTI IN INT-FLD TO INT-MSC-BCTI IN INT.
           MOVE INT-MSC-PBTI IN INT-FLD TO INT-MSC-PBTI IN INT.
           MOVE INT-MSC-BPBT IN INT-FLD TO INT-MSC-BPBT IN INT.
           MOVE INT-MSC-SPBT IN INT-FLD TO INT-MSC-SPBT IN INT.
           MOVE INT-COD-CVTI IN INT-FLD TO INT-COD-CVTI IN INT.
           MOVE INT-MSC-TFPR IN INT-FLD TO INT-MSC-TFPR IN INT.
           MOVE INT-NUM-PREP IN INT-FLD TO INT-NUM-PREP IN INT.
           MOVE INT-IND-UTPR IN INT-FLD TO INT-IND-UTPR IN INT.
       FIN-KEY-INT-INT.
           EXIT.
       
       SCR-CLRA-INT SECTION.
       INI-SCR-CLRA-INT.
           PERFORM CLR-KEY-INT.
           PERFORM CLR-FLD-INT.
       FIN-SCR-CLRA-INT.
           EXIT.
       
       CLR-KEY-INT SECTION.
       INI-CLR-KEY-INT.
           MOVE SPACES TO INT-MSC-BCTI IN INT-FLD.
           MOVE SPACES TO INT-MSC-PBTI IN INT-FLD.
           MOVE SPACES TO INT-MSC-BPBT IN INT-FLD.
           MOVE SPACES TO INT-MSC-SPBT IN INT-FLD.
           MOVE SPACES TO INT-COD-CVTI IN INT-FLD.
           MOVE SPACES TO INT-MSC-TFPR IN INT-FLD.
           MOVE ZEROES TO INT-NUM-PREP IN INT-FLD.
           MOVE SPACES TO INT-IND-UTPR IN INT-FLD.
       FIN-CLR-KEY-INT.
           EXIT.
       
       CLR-FLD-INT SECTION.
       INI-CLR-FLD-INT.
           MOVE SPACES TO FRM-GLS-BCTI IN INT-FLD.
           MOVE SPACES TO FRM-GLS-PBTI IN INT-FLD.
           MOVE SPACES TO FRM-GLS-BPBT IN INT-FLD.
           MOVE SPACES TO FRM-GLS-SPBT IN INT-FLD.
           MOVE SPACES TO FRM-GLS-CVTI IN INT-FLD.
           MOVE SPACES TO FRM-GLS-TFPR IN INT-FLD.
           MOVE SPACES TO FRM-GLS-UTPR IN INT-FLD.
           PERFORM CLR-INT-INT.
       FIN-CLR-FLD-INT.
           EXIT.
       
       CLR-INT-INT SECTION.
       INI-CLR-INT-INT.
           MOVE SPACES TO INT-CIC-TINT IN INT-FLD.
           MOVE SPACES TO INT-EXT-CTAB IN INT-FLD.
           MOVE SPACES TO INT-GLS-DESC IN INT-FLD.
           MOVE SPACES TO INT-GLS-ABRV IN INT-FLD.
           MOVE SPACES TO INT-GLS-DCOR IN INT-FLD.
           MOVE SPACES TO INT-IND-VIGE IN INT-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO INT-FEC-FTRN IN INT-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
       FIN-CLR-INT-INT.
           EXIT.
       
       FST-KEY-INT SECTION.
       INI-FST-KEY-INT.
           MOVE -1 TO INT-MSC-BCTI-LEN IN INT-FLD.
       FIN-FST-KEY-INT.
           EXIT.
       
       FST-FLD-INT SECTION.
       INI-FST-FLD-INT.
           MOVE -1 TO INT-CIC-TINT-LEN IN INT-FLD.
       FIN-FST-FLD-INT.
           EXIT.
       
       PRO-KEY-INT SECTION.
       INI-PRO-KEY-INT.
           PERFORM PRO-MKY-INT.
       FIN-PRO-KEY-INT.
           EXIT.
       
       PRO-MKY-INT SECTION.
       INI-PRO-MKY-INT.
       FIN-PRO-MKY-INT.
           EXIT.
       
       PRO-FLD-INT SECTION.
       INI-PRO-FLD-INT.
           PERFORM PRO-INT-INT.
       FIN-PRO-FLD-INT.
           EXIT.
       
       PRO-INT-INT SECTION.
       INI-PRO-INT-INT.
           MOVE FRM-CPIM-AEY TO INT-CIC-TINT-ATR IN INT-FLD.
           MOVE FRM-CPIM-AEY TO INT-EXT-CTAB-ATR IN INT-FLD.
           MOVE FRM-CPIM-AEY TO INT-GLS-DESC-ATR IN INT-FLD.
           MOVE FRM-CPIM-AEY TO INT-GLS-ABRV-ATR IN INT-FLD.
           MOVE FRM-CPIM-AEY TO INT-GLS-DCOR-ATR IN INT-FLD.
           MOVE FRM-CPIM-AEY TO INT-IND-VIGE-ATR IN INT-FLD.
       FIN-PRO-INT-INT.
           EXIT.
       
       UNP-KEY-INT SECTION.
       INI-UNP-KEY-INT.
           PERFORM UNP-IKY-INT.
           PERFORM UNP-AKY-INT.
           PERFORM UNP-MKY-INT.
       FIN-UNP-KEY-INT.
           EXIT.
       
       UNP-IKY-INT SECTION.
       INI-UNP-IKY-INT.
           MOVE FRM-CPIM-UBY TO INT-MSC-BCTI-ATR.
           MOVE FRM-CPIM-UBY TO INT-MSC-PBTI-ATR.
           MOVE FRM-CPIM-UBY TO INT-MSC-BPBT-ATR.
           MOVE FRM-CPIM-UBY TO INT-MSC-SPBT-ATR.
           MOVE FRM-CPIM-UBY TO INT-COD-CVTI-ATR.
           MOVE FRM-CPIM-UBY TO INT-MSC-TFPR-ATR.
           MOVE FRM-CPIM-UBY TO INT-NUM-PREP-ATR.
           MOVE FRM-CPIM-UBY TO INT-IND-UTPR-ATR.
       FIN-UNP-IKY-INT.
           EXIT.
       
       UNP-MKY-INT SECTION.
       INI-UNP-MKY-INT.
       FIN-UNP-MKY-INT.
           EXIT.
       
       UNP-AKY-INT SECTION.
       INI-UNP-AKY-INT.
       FIN-UNP-AKY-INT.
           EXIT.
       
       UNP-FLD-INT SECTION.
       INI-UNP-FLD-INT.
           PERFORM UNP-INT-INT.
       FIN-UNP-FLD-INT.
           EXIT.
       
       UNP-INT-INT SECTION.
       INI-UNP-INT-INT.
           MOVE FRM-CPIM-UBY TO INT-CIC-TINT-ATR IN INT-FLD.
           MOVE FRM-CPIM-UBY TO INT-EXT-CTAB-ATR IN INT-FLD.
           MOVE FRM-CPIM-UBY TO INT-GLS-DESC-ATR IN INT-FLD.
           MOVE FRM-CPIM-UBY TO INT-GLS-ABRV-ATR IN INT-FLD.
           MOVE FRM-CPIM-UBY TO INT-GLS-DCOR-ATR IN INT-FLD.
           MOVE FRM-CPIM-UBY TO INT-IND-VIGE-ATR IN INT-FLD.
       FIN-UNP-INT-INT.
           EXIT.
       
       VAL-MKY-INT SECTION.
       INI-VAL-MKY-INT.
       FIN-VAL-MKY-INT.
           EXIT.
       
       VAL-FLD-INT SECTION.
       INI-VAL-FLD-INT.
           PERFORM VAL-FRM-GLS-BCTI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-BCTI-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
           PERFORM VAL-FRM-GLS-PBTI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-PBTI-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
           PERFORM VAL-FRM-GLS-BPBT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-BPBT-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
           PERFORM VAL-FRM-GLS-SPBT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-SPBT-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
           PERFORM VAL-FRM-GLS-CVTI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-CVTI-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
           PERFORM VAL-FRM-GLS-TFPR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TFPR-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
           PERFORM VAL-FRM-GLS-UTPR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-UTPR-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
           PERFORM VAL-INT-CIC-TINT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO INT-CIC-TINT-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
           PERFORM VAL-INT-EXT-CTAB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO INT-EXT-CTAB-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
           PERFORM VAL-INT-GLS-DESC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO INT-GLS-DESC-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
           PERFORM VAL-INT-GLS-ABRV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO INT-GLS-ABRV-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
           PERFORM VAL-INT-GLS-DCOR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO INT-GLS-DCOR-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
           PERFORM VAL-INT-IND-VIGE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO INT-IND-VIGE-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-INT-FEC-FTRN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                INT-FEC-FTRN-LEN IN INT-FLD
                GO TO FIN-VAL-FLD-INT.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
       FIN-VAL-FLD-INT.
           EXIT.
       
       VAL-FRM-GLS-BCTI SECTION.
       INI-VAL-FRM-GLS-BCTI.
       FIN-VAL-FRM-GLS-BCTI.
           EXIT.
       
       VAL-FRM-GLS-PBTI SECTION.
       INI-VAL-FRM-GLS-PBTI.
       FIN-VAL-FRM-GLS-PBTI.
           EXIT.
       
       VAL-FRM-GLS-BPBT SECTION.
       INI-VAL-FRM-GLS-BPBT.
       FIN-VAL-FRM-GLS-BPBT.
           EXIT.
       
       VAL-FRM-GLS-SPBT SECTION.
       INI-VAL-FRM-GLS-SPBT.
       FIN-VAL-FRM-GLS-SPBT.
           EXIT.
       
       VAL-FRM-GLS-CVTI SECTION.
       INI-VAL-FRM-GLS-CVTI.
       FIN-VAL-FRM-GLS-CVTI.
           EXIT.
       
       VAL-FRM-GLS-TFPR SECTION.
       INI-VAL-FRM-GLS-TFPR.
       FIN-VAL-FRM-GLS-TFPR.
           EXIT.
       
       VAL-FRM-GLS-UTPR SECTION.
       INI-VAL-FRM-GLS-UTPR.
       FIN-VAL-FRM-GLS-UTPR.
           EXIT.
       
       VAL-INT-CIC-TINT SECTION.
       INI-VAL-INT-CIC-TINT.
           IF INT-CIC-TINT IN INT-FLD NOT > SPACES
              MOVE SPACES TO INT-CIC-TINT IN INT-FLD.
      *<<<<
           IF INT-CIC-TINT IN INT-FLD NOT > SPACES
               MOVE INT-MSC-BCTI IN INT-FLD TO WSS-CIC-BCTI
               MOVE INT-MSC-PBTI IN INT-FLD TO WSS-CIC-PBTI
               MOVE INT-MSC-BPBT IN INT-FLD TO WSS-CIC-BPBT
               MOVE INT-MSC-SPBT IN INT-FLD TO WSS-CIC-SPBT
               MOVE INT-COD-CVTI IN INT-FLD TO WSS-CIC-CVTI
               MOVE INT-MSC-TFPR IN INT-FLD TO WSS-CIC-TFPR
               MOVE INT-NUM-PREP IN INT-FLD TO WSS-CIC-PREP
               MOVE INT-IND-UTPR IN INT-FLD TO WSS-CIC-UTPR
               MOVE WSS-CIC-TINT TO INT-CIC-TINT IN INT-FLD.
      *>>>>
       FIN-VAL-INT-CIC-TINT.
           EXIT.
       
       VAL-INT-EXT-CTAB SECTION.
       INI-VAL-INT-EXT-CTAB.
           IF INT-EXT-CTAB IN INT-FLD NOT > SPACES
              MOVE SPACES TO INT-EXT-CTAB IN INT-FLD.
       FIN-VAL-INT-EXT-CTAB.
           EXIT.
       
       VAL-INT-GLS-DESC SECTION.
       INI-VAL-INT-GLS-DESC.
           IF INT-GLS-DESC IN INT-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-INT-GLS-DESC.
       FIN-VAL-INT-GLS-DESC.
           EXIT.
       
       VAL-INT-GLS-ABRV SECTION.
       INI-VAL-INT-GLS-ABRV.
           IF INT-GLS-ABRV IN INT-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-INT-GLS-ABRV.
       FIN-VAL-INT-GLS-ABRV.
           EXIT.
       
       VAL-INT-GLS-DCOR SECTION.
       INI-VAL-INT-GLS-DCOR.
           IF INT-GLS-DCOR IN INT-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-INT-GLS-DCOR.
       FIN-VAL-INT-GLS-DCOR.
           EXIT.
       
       VAL-INT-IND-VIGE SECTION.
       INI-VAL-INT-IND-VIGE.
           IF INT-IND-VIGE IN INT-FLD NOT > SPACES
              MOVE SPACES TO INT-IND-VIGE IN INT-FLD.
      *VAL-IND Valida miscelaneo de tabla
           MOVE 'GNS' TO MSC-COD-SIST.
           MOVE 'VIGE' TO MSC-COD-TMSC IN MSC.
           MOVE INT-IND-VIGE IN INT-FLD TO MSC-COD-CMSC IN MSC.
           PERFORM VAL-IND.
       FIN-VAL-INT-IND-VIGE.
           EXIT.
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-INT-FEC-FTRN SECTION.
       INI-VAL-INT-FEC-FTRN.
           IF INT-FEC-FTRN IN INT-FLD  NOT > ZEROES
              MOVE ZEROES TO INT-FEC-FTRN IN INT-FLD.
       FIN-VAL-INT-FEC-FTRN.
           EXIT.
      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-CON-FLD-INT SECTION.
       INI-VAL-CON-FLD-INT.
       FIN-VAL-CON-FLD-INT.
           EXIT.
       
       VAL-CON-INT SECTION.
       INI-VAL-CON-INT.
       FIN-VAL-CON-INT.
           EXIT.
       
       VAL-NUM-KEY-INT SECTION.
       INI-VAL-NUM-KEY-INT.
           PERFORM VAL-NUM-IKY-INT.
           PERFORM VAL-NUM-MKY-INT.
           PERFORM VAL-NUM-AKY-INT.
       FIN-VAL-NUM-KEY-INT.
           EXIT.
       
       VAL-NUM-IKY-INT SECTION.
       INI-VAL-NUM-IKY-INT.
       VAL-NUM-INT-NUM-PREP.
           MOVE INT-NUM-PREP-ALF IN INT-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO INT-NUM-PREP IN INT-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO INT-NUM-PREP-LEN IN INT-FLD
                GO TO FIN-VAL-NUM-IKY-INT.
       FIN-VAL-NUM-IKY-INT.
           EXIT.
       
       VAL-NUM-MKY-INT SECTION.
       INI-VAL-NUM-MKY-INT.
       FIN-VAL-NUM-MKY-INT.
           EXIT.
       
       VAL-NUM-AKY-INT SECTION.
       INI-VAL-NUM-AKY-INT.
       FIN-VAL-NUM-AKY-INT.
           EXIT.
       
       VAL-NUM-FLD-INT SECTION.
       INI-VAL-NUM-FLD-INT.
       FIN-VAL-NUM-FLD-INT.
           EXIT.
       
       EDT-KEY-INT SECTION.
       INI-EDT-KEY-INT.
           PERFORM EDT-IKY-INT.
           PERFORM EDT-MKY-INT.
           PERFORM EDT-AKY-INT.
       FIN-EDT-KEY-INT.
           EXIT.
       
       EDT-IKY-INT SECTION.
       INI-EDT-IKY-INT.
           IF INT-NUM-PREP-LEN IN INT-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO INT-NUM-PREP-LEN IN INT-FLD
                GO TO FIN-EDT-IKY-INT.
           MOVE INT-NUM-PREP IN INT-FLD TO INT-NUM-PREP-EDT IN INT-EDT.
           MOVE INT-NUM-PREP-EDT IN INT-EDT TO 
                INT-NUM-PREP-ALF IN INT-FLD.
       FIN-EDT-IKY-INT.
           EXIT.
       
       EDT-MKY-INT SECTION.
       INI-EDT-MKY-INT.
       FIN-EDT-MKY-INT.
           EXIT.
       
       EDT-AKY-INT SECTION.
       INI-EDT-AKY-INT.
       FIN-EDT-AKY-INT.
           EXIT.
       
       EDT-FLD-INT SECTION.
       INI-EDT-FLD-INT.
       FIN-EDT-FLD-INT.
           EXIT.
