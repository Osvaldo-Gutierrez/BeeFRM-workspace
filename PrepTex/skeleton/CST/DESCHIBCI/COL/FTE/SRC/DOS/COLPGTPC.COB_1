       COPY COLBFACU.
       COPY COLBFTPC.
       COPY COLBFOPC.
       COPY COLBFDLC.
       COPY COLBFCYA.
       COPY SGCBFDBC.
       COPY SGCBFDCP.
       COPY SGCBFDCE.
       COPY DEUBFCRI.
      *
       COPY GNSBGDTC.
       COPY GNSBGIDD.
       COPY GNSBTABT.
       COPY GNSBBSYS.
       COPY GNSBGEND.
       COPY GNSBGABT.
      *
       COPY GNSBFMSG.
       COPY GNSBGHOY.
       COPY GNSBBMSC.
       COPY GNSBBMSG.
       COPY GNSBBTAB.
       COPY GNSBFTAB.
       COPY GNSBFMSC.
       COPY GNSBGCPT.
