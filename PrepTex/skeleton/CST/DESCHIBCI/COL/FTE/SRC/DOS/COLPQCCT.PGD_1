       COPY COLBGCCT.
       COPY SGCBFDBC.
       COPY COLBFDLC.
       COPY COLBFOPC.
       COPY COLBFRDC.
       COPY COLBGVEN.
       COPY GNSBPFEC.
       COPY COLBFEVC.
       COPY COLBFICG.
       COPY COLBFTOC.
       COPY COLBFRDT.
       COPY SGCBGCLI.
       COPY GNSBGCPT.
       COPY GNSBGPES.
