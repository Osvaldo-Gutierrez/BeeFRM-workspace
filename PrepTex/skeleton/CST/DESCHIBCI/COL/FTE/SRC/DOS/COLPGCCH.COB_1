              WRITE RPTCCH-REG FROM  PH-CCH-02
              WRITE RPTCCH-REG FROM  PH-CCH-03 AFTER 2
              WRITE RPTCCH-REG FROM  PH-CCH-04
              WRITE RPTCCH-REG FROM  PH-CCH-05
              WRITE RPTCCH-REG FROM  PH-CCH-06
              WRITE RPTCCH-REG FROM  PH-CCH-07
              MOVE 10 TO RPT-VARI-LINEA
           END-IF.
           ADD  SRT-NUM-NUNO IN SRT            TO RPT-TOT-NUM-REG
           ADD  VEN-VAL-SFIN IN SRT            TO RPT-TOT-VAL-SFIN
           WRITE RPTCCH-REG FROM  DL-CCH-08.
           ADD 1 TO RPT-VARI-LINEA.

