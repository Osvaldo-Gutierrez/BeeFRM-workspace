           MOVE OPC-VAL-SCRE IN SRT            TO RPT-DETA-VAL-SCRE
           MOVE VEN-FEC-FVEN IN SRT            TO RPT-CTRL-FEC-FVEN
           MOVE VEN-VAL-SCAP IN SRT            TO RPT-CTRL-VAL-SCAP
           MOVE OPC-VAL-SEGD IN SRT            TO RPT-CTRL-VAL-SEGD
           MOVE OPC-VAL-SEGP IN SRT            TO RPT-CTRL-VAL-SEGP

           IF RPT-VARI-LINEA > 60 OR RPT-VARI-PAGE-COUNTER = 0 OR
              OPC-COD-OFOC IN SRT NOT = RPT-CTRL-COD-OFIC      OR
              OPC-COD-COOC IN SRT NOT = RPT-CTRL-COD-COOC
              IF OPC-COD-OFOC IN SRT NOT = RPT-CTRL-COD-OFIC   OR
                 OPC-COD-COOC IN SRT NOT = RPT-CTRL-COD-COOC
                 MOVE OPC-COD-OFOC IN SRT TO RPT-CTRL-COD-OFIC
                 MOVE OPC-COD-COOC IN SRT TO RPT-CTRL-COD-COOC
                 MOVE RPT-TOT-NUM-REG TO RPT-CTRL-NUM-NUNO
                 MOVE RPT-TOT-VAL-SFIN TO RPT-CTRL-VAL-SFIN
                 MOVE SPACES TO RPT-CTRL-RESTO
                 WRITE RPTCCH-REG FROM  CF-CCH-09
                 WRITE RPTCCH-REG FROM  CF-CCH-10
                 WRITE RPTCCH-REG FROM  CF-CCH-11
                 ADD 3 TO RPT-VARI-LINEA
                 MOVE 0 TO RPT-TOT-NUM-REG
                           RPT-TOT-VAL-SFIN
              END-IF
              ADD 1 TO RPT-VARI-PAGE-COUNTER
              MOVE RPT-VARI-PAGE-COUNTER TO RPT-HEAD-PAGE-COUNTER
              MOVE SPACES TO RPTCCH-REG
              WRITE RPTCCH-REG AFTER PAGE
