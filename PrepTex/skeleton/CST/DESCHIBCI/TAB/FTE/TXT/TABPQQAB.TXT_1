*% LARGO_TOTAL_COMMAREA = 3000
*% PGM_PTC = TRUE
*% IF FIN_SCR_VARI
      *<<<<
           03 SCR-NUM-PANT               PIC 9(0010).                   
           03 SCR-QAB-FLD                PIC X(1200).                   
      *>>>>
*% END
*% IF WSS
      *<<<< WSS                                                         
       COPY TABWXQAB.                                                   
                                                                        
       COPY GNSWGCPT.                                                   
       COPY GNSWGSTR.                                                   
       COPY SGCBRDBC.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.        
       COPY TABBRWFC.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-WFC-REQA==.        
      *>>>>                                                             
*% END
*% IF INI_FIO
      *<<<< INI_FIO                                                     
           IF SCR-OFRM-SRV                                              
                MOVE TSK-TERM-ALF TO QAB-COD-TERM                       
                MOVE TSK-TERM-ALF TO QAB-COD-TERM-MAS                   
           ELSE                                                         
                MOVE EIBTRMID     TO QAB-COD-TERM                       
                MOVE EIBTRMID     TO QAB-COD-TERM-MAS.                  
           MOVE +3859 TO SCR-TCMA.                                      
           PERFORM GNS-ERR-QUE.                                         
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )      
               MOVE SCR-QAB-FLD TO QAB-FLD                              
               GO TO FIN-INI.                                           
                                                                        
           IF ( FRM-FFLD = FRM-FFLD-PF6 )                               
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM        
               ADD 1              TO QUE-NITM                           
               MOVE QAB-NOM-TSQAB TO QUE-COLA                           
               MOVE QAB-LEN-TSQAB TO QUE-LITM                           
               MOVE QUE-GET       TO QUE-CMND                           
               PERFORM GNS-PRO-QUE                                      
               IF QUE-STAT NOT = QUE-STAT-OKS                           
                  MOVE SCR-QAB-FLD TO QAB-FLD                           
                  GO TO FIN-INI.                                        
           PERFORM GET-FHOY.                                            
      *>>>>                                                             
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC                                                 
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6                   
               PERFORM VAL-NUM-KEY-QAB                                  
               PERFORM VAL-NUM-FLD-QAB                                  
               PERFORM SCRL-TAB-QAB                                     
               PERFORM SCR-TRL.                                         
      *>>>>                                                             
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY                                                 
           IF FRM-IND-NEXT IN QAB-FLD = 'S'                             
               MOVE FRM-CAI-CONVN IN QAB-FLD TO CAB-CAI-CONV IN CAB     
               MOVE FRM-IIC-CONVN IN QAB-FLD TO CAB-IIC-CONV IN CAB     
               MOVE FRM-IND-TIPON IN QAB-FLD TO CAB-IND-TIPO IN CAB     
               MOVE FRM-NUM-ICABN IN QAB-FLD TO CAB-NUM-ICAB IN CAB     
           ELSE                                                         
               MOVE CAB-CAI-CONV  IN QAB-FLD TO CAB-CAI-CONV IN CAB     
               MOVE CAB-IIC-CONV  IN QAB-FLD TO CAB-IIC-CONV IN CAB     
               MOVE CAB-IND-TIPO  IN QAB-FLD TO CAB-IND-TIPO IN CAB     
               MOVE ZEROES                   TO CAB-NUM-ICAB IN CAB.    
                                                                        
           MOVE ZEROES TO WSS-J IN WSS-VARI.                            
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-LEE-CAB.                                                     
           PERFORM TAB-FIO-CAB.                                         
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF                          
               GO TO CON-FND-KEY.                                       
           IF FRM-IND-NEXT IN QAB-FLD = 'S'                             
              IF NOT (CAB-CAI-CONV IN CAB = FRM-CAI-CONVN IN QAB-FLD AND
                      CAB-IIC-CONV IN CAB = FRM-IIC-CONVN IN QAB-FLD AND
                      (CAB-IND-TIPO IN CAB = FRM-IND-TIPON IN QAB-FLD OR
                       FRM-IND-TIPON IN QAB-FLD NOT > SPACES))          
                 GO TO CON-FND-KEY                                      
              ELSE                                                      
                 NEXT SENTENCE                                          
           ELSE                                                         
              IF NOT (CAB-CAI-CONV IN CAB = CAB-CAI-CONV  IN QAB-FLD AND
                      CAB-IIC-CONV IN CAB = CAB-IIC-CONV  IN QAB-FLD AND
                      (CAB-IND-TIPO IN CAB = CAB-IND-TIPO  IN QAB-FLD OR
                       CAB-IND-TIPO  IN QAB-FLD NOT > SPACES))          
                 GO TO CON-FND-KEY.                                     
                                                                        
           PERFORM CRG-VEC-QAB.                                         
                                                                        
           IF (SCR-OFRM-SRV AND WSS-J > WSS-NITMMAX)                    
              GO TO CON-FND-KEY.                                        
                                                                        
           IF ((NOT SCR-OFRM-SRV) AND WSS-J > WSS-JMAX)                 
                 GO TO CON-FND-KEY.                                     
       NXT-LEE-MET.                                                     
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-LEE-CAB.                                           
      *>>>>                                                             
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY                                                 
       CON-FND-KEY.                                                     
           PERFORM PUT-TAB.                                             
           IF MSG-COD-MENS > SPACES                                     
              GO TO MSG-FND-KEY.                                        
                                                                        
           MOVE CAB-CAI-CONV IN QAB-FLD TO WFC-CAI-CONV IN WFC.         
           MOVE CAB-IIC-CONV IN QAB-FLD TO WFC-IIC-CONV IN WFC.         
           MOVE 99999999                TO WFC-FEC-FCNV IN WFC.         
           MOVE 999999                  TO WFC-HRA-CONV IN WFC.         
           MOVE FIO-GET-LEQ  TO FIO-CMND.                               
           PERFORM TAB-FIO-WFC.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    WFC-CAI-CONV IN WFC = CAB-CAI-CONV IN QAB-FLD AND   
                    WFC-IIC-CONV IN WFC = CAB-IIC-CONV IN QAB-FLD )     
              MOVE 'TAB' TO MSG-COD-SIST                                
              MOVE 'WFC    NEX' TO MSG-COD-MENS                         
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              MOVE '23' TO FIO-STAT PGM-STAT-WFC                        
              GO TO FIN-FND-KEY.                                        
                                                                        
           MOVE FIO-STAT TO PGM-STAT-WFC.                               
           MOVE WFC-NUM-DCNV IN WFC TO WFC-NUM-DCNV IN QAB-FLD.         
           MOVE WFC-NUM-MCNV IN WFC TO WFC-NUM-MCNV IN QAB-FLD.         
           MOVE WFC-NUM-SCNV IN WFC TO WFC-NUM-SCNV IN QAB-FLD.         
           MOVE WFC-NUM-ACNV IN WFC TO WFC-NUM-ACNV IN QAB-FLD.         
           MOVE WFC-HRA-CONV IN WFC TO WFC-HRA-CONV IN QAB-FLD.         
                                                                        
           MOVE WFC-CAI-EMPR IN WFC TO DBC-CAI-ICLI IN DBC.             
           MOVE WFC-IIC-EMPR IN WFC TO DBC-IIC-ICLI IN DBC.             
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM SGC-FIO-DBC.                                         
           MOVE FIO-STAT TO PGM-STAT-DBC.                               
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.                        
           PERFORM CPT-BLKS.                                            
           MOVE CPT-STRN            TO DBC-GLS-NOMC IN QAB-FLD.         
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN QAB-FLD.         
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN QAB-FLD.         
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN QAB-FLD.         
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN QAB-FLD.         
                                                                        
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           GO TO FIN-FND-KEY.                                           
      *----------------------------------------------------------       
       MSG-FND-KEY.                                                     
           MOVE 'TAB' TO MSG-COD-SIST.                                  
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MENS.                               
           MOVE '00' TO FIO-STAT.                                       
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           GO TO FIN-FND-KEY.                                           
      *>>>>                                                             
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON                                                 
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )           
      *>>>>
*% END
*% IF EOF
      *<<<< EOF                                                         
       PGM-EBR-ALL SECTION.                                             
       INI-PGM-EBR-ALL.                                                 
           MOVE 'GNS' TO FIO-SIST.                                      
           MOVE FIO-EBR-ALL TO FIO-CMND.                                
           PERFORM GNS-FIO-TAB.                                         
       FIN-PGM-EBR-ALL.                                                 
           EXIT.                                                        

       COPY GNSBGVSM.                                                   
       COPY SGCBFDBC.                                                   
       COPY TABBFWFC.                                                   
                                                                        
       COPY GNSBBTAB.                                                   
       COPY GNSBVTAB.                                                   
       COPY GNSBVCOD.                                                   
       COPY GNSBPFEC.                                                   
                                                                        
       PUT-TAB SECTION.                                                 
       INI-PUT-TAB.                                                     
           MOVE QAB-NOM-TSQAB     TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE QAB-NOM-TSQAB-MAS TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE 1 TO SCR-NUM-PANT.                                      
           MOVE LOW-VALUES TO WSS-CAI-CONV-IDX-MTI.                     
           MOVE LOW-VALUES TO WSS-CAI-CONV-IDX-FRM.                     
      *                                                                 
           MOVE 1 TO FRM-IFLD.                                          
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.                        
                                                                        
           MOVE 1 TO WSS-F IN WSS-VARI.                                 
       LUP-PUT-TAB.                                                     
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI                  
              GO TO FIN-PUT-TAB.                                        
           IF WSS-CAI-CONV-VEC(WSS-F) > SPACES                          
              PERFORM PUT-TAB-QAB                                       
              PERFORM PUT-QAB-QUE                                       
              PERFORM SET-FRM-IFLD                                      
           ELSE                                                         
           IF FRM-IFLD = 1 AND WSS-F = 1                                
              MOVE 'TAB' TO MSG-COD-SIST                                
              MOVE 'QAB    NEX' TO MSG-COD-MENS                         
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              MOVE '00' TO FIO-STAT                                     
      *       MOVE 'S' TO SCR-IERR                                      
              GO TO FIN-PUT-TAB                                         
           ELSE                                                         
              MOVE '00' TO FIO-STAT                                     
              GO TO FIN-PUT-TAB.                                        
                                                                        
           ADD 1 TO WSS-F IN WSS-VARI.                                  
           GO TO LUP-PUT-TAB.                                           
       FIN-PUT-TAB.                                                     
           MOVE 1                  TO QUE-NITM.                         
           MOVE QAB-LEN-TSQAB-MAS  TO QUE-LITM.                         
           MOVE QAB-NOM-TSQAB-MAS  TO QUE-COLA.                         
                                                                        
           IF WSS-NITM > WSS-NITMMAX                                    
              MOVE 'S'           TO QAB-MAS-IND-HMAS                    
           ELSE                                                         
              MOVE 'N'           TO QAB-MAS-IND-HMAS.                   
                                                                        
           MOVE WSS-NITM         TO WSS-NUM-ICUR.                       
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.                   
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.                   
           MOVE WSS-NUM-ICUR-ALF TO QAB-MAS-NUM-TOCC.                   
           MOVE QAB-MAS-ITEM     TO QUE-ITEM.                           
           MOVE QUE-PUT          TO QUE-CMND.                           
           PERFORM GNS-PRO-QUE.                                         
           IF WSS-NITM > WSS-IMAX                                       
               MOVE 'TAB' TO MSG-COD-SIST                               
               MOVE 'QABTABDMAS' TO MSG-COD-MENS                        
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN2                            
               MOVE SPACES       TO MSG-COD-MENS.                       
       EXT-PUT-TAB.                                                     
           EXIT.                                                        
                                                                        
       SET-FRM-IFLD SECTION.                                            
       INI-SET-FRM-IFLD.                                                
           IF SCR-OFRM-SRV                                              
                MOVE 1 TO FRM-IFLD                                      
           ELSE                                                         
                ADD 1 TO FRM-IFLD.                                      
       FIN-SET-FRM-IFLD.                                                
           EXIT.                                                        
      *--------------------------------------------                     
       SCRL-TAB-QAB SECTION.                                            
       INI-SCRL-TAB-QAB.                                                
           MOVE SPACES TO MSG-COD-MENS.                                 
                                                                        
           IF FRM-FFLD = FRM-FFLD-PF6                                   
               PERFORM PUT-TAB-ASC                                      
           ELSE                                                         
               PERFORM PUT-TAB-DSC.                                     
           IF MSG-COD-MENS > SPACES                                     
               MOVE 'TAB' TO MSG-COD-SIST                               
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN2.                           
       FIN-SCRL-TAB-QAB.                                                
           EXIT.                                                        
                                                                        
      *--------------------------------------------                     
       PUT-TAB-QAB SECTION.                                             
       INI-PUT-TAB-QAB.                                                 
           PERFORM CLR-WSS.                                             
           MOVE WSS-CAI-CONV-VEC(WSS-F) TO WSS-CAI-CONV-MTI IN WSS-VARI.
           MOVE WSS-IIC-CONV-VEC(WSS-F) TO WSS-IIC-CONV-MTI IN WSS-VARI.
           MOVE WSS-IND-TIPO-VEC(WSS-F) TO WSS-IND-TIPO-MTI IN WSS-VARI.
           MOVE WSS-GLS-TIPO-VEC(WSS-F) TO WSS-GLS-TIPO-MTI IN WSS-VARI.
           MOVE WSS-NUM-ICAB-VEC(WSS-F) TO WSS-NUM-ICAB-MTI IN WSS-VARI.
           MOVE WSS-IND-SMWI-VEC(WSS-F) TO WSS-IND-SMWI-MTI IN WSS-VARI.
           MOVE WSS-GLS-SMWI-VEC(WSS-F) TO WSS-GLS-SMWI-MTI IN WSS-VARI.
                                                                        
           MOVE WSS-CAI-CONV-MTI IN WSS-VARI TO                         
                WSS-CAI-CONV-FRM IN WSS-VARI.                           
           MOVE WSS-IIC-CONV-MTI IN WSS-VARI TO                         
                WSS-IIC-CONV-FRM IN WSS-VARI.                           
           MOVE WSS-IND-TIPO-MTI IN WSS-VARI TO                         
                WSS-IND-TIPO-FRM IN WSS-VARI.                           
           MOVE WSS-GLS-TIPO-MTI IN WSS-VARI TO                         
                WSS-GLS-TIPO-FRM IN WSS-VARI.                           
           MOVE WSS-NUM-ICAB-MTI IN WSS-VARI TO                         
                WSS-NUM-ICAB-FRM IN WSS-VARI.                           
           MOVE WSS-IND-SMWI-MTI IN WSS-VARI TO                         
                WSS-IND-SMWI-FRM IN WSS-VARI.                           
           MOVE WSS-GLS-SMWI-MTI IN WSS-VARI TO                         
                WSS-GLS-SMWI-FRM IN WSS-VARI.                           
                                                                        
           IF FRM-IFLD > WSS-IMAX                                       
              GO TO FIN-PUT-TAB-QAB.                                    
                                                                        
           MOVE WSS-CAI-CONV-MTI IN WSS-VARI TO                         
                FRM-CAI-CONV IN QAB-FLD(FRM-IFLD).                      
           MOVE WSS-IIC-CONV-MTI IN WSS-VARI TO                         
                FRM-IIC-CONV IN QAB-FLD(FRM-IFLD).                      
           MOVE WSS-IND-TIPO-MTI IN WSS-VARI TO                         
                FRM-IND-TIPO IN QAB-FLD(FRM-IFLD).                      
           MOVE WSS-GLS-TIPO-MTI IN WSS-VARI TO                         
                FRM-GLS-TIPO IN QAB-FLD(FRM-IFLD).                      
           MOVE WSS-NUM-ICAB-MTI IN WSS-VARI TO                         
                FRM-NUM-ICAB IN QAB-FLD(FRM-IFLD).                      
           MOVE WSS-IND-SMWI-MTI IN WSS-VARI TO                         
                FRM-IND-SMWI IN QAB-FLD(FRM-IFLD).                      
           MOVE WSS-GLS-SMWI-MTI IN WSS-VARI TO                         
                FRM-GLS-SMWI IN QAB-FLD(FRM-IFLD).                      
       FIN-PUT-TAB-QAB.                                                 
           EXIT.                                                        
      *----------------------------------------------------             
       PUT-QAB-QUE SECTION.                                             
       INI-PUT-QAB-QUE.                                                 
           PERFORM PRO-FLD-QUE.                                         
           ADD 1 TO WSS-NITM.                                           
           MOVE QAB-NOM-TSQAB    TO QUE-COLA.                           
           MOVE QAB-LEN-TSQAB    TO QUE-LITM.                           
           MOVE WSS-NITM         TO QUE-NITM.                           
           MOVE SPACES           TO QUE-ITEM.                           
           IF SCR-OFRM-SRV                                              
              MOVE WSS-CAI-CONV-IDX-MTI TO QUE-ITEM                     
           ELSE                                                         
              MOVE WSS-CAI-CONV-IDX-FRM TO QUE-ITEM.                    
           MOVE QUE-PUT  TO QUE-CMND.                                   
           PERFORM GNS-PRO-QUE.                                         
       FIN-PUT-QAB-QUE.                                                 
           EXIT.                                                        
      *----------------------------------------------------             
                                                                        
       PUT-TAB-ASC SECTION.                                             
       INI-PUT-TAB-ASC.                                                 
           MOVE SPACES TO MSG-COD-MENS.                                 
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.           
           MOVE 0 TO FRM-IFLD.                                          
       LUP-PUT-TAB-ASC.                                                 
           ADD 1              TO QUE-NITM.                              
           MOVE QAB-NOM-TSQAB TO QUE-COLA.                              
           MOVE QAB-LEN-TSQAB TO QUE-LITM.                              
           MOVE QUE-GET       TO QUE-CMND.                              
           PERFORM GNS-PRO-QUE.                                         
           IF QUE-STAT NOT = QUE-STAT-OKS                               
               IF FRM-IFLD = 0                                          
                   MOVE 'QABTABNONXT' TO MSG-COD-MENS                   
                   GO TO FIN-PUT-TAB-ASC                                
               ELSE                                                     
                   GO TO FIN-PUT-TAB-ASC.                               
           ADD 1 TO FRM-IFLD.                                           
           MOVE QUE-ITEM TO FRM-CAI-CONV-IDX(FRM-IFLD).                 
           IF FRM-IFLD < WSS-IMAX                                       
               GO TO LUP-PUT-TAB-ASC.                                   
       FIN-PUT-TAB-ASC.                                                 
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX                      
               ADD 1 FRM-IFLD GIVING WSS-I                              
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1            
                                 UNTIL WSS-I > WSS-IMAX.                
           IF FRM-IFLD > 0                                              
               ADD 1 TO SCR-NUM-PANT.                                   
           IF FRM-IFLD = WSS-IMAX                                       
               ADD 1              TO QUE-NITM                           
               MOVE QAB-NOM-TSQAB TO QUE-COLA                           
               MOVE QAB-LEN-TSQAB TO QUE-LITM                           
               MOVE QUE-GET       TO QUE-CMND                           
               PERFORM GNS-PRO-QUE                                      
               IF QUE-STAT = QUE-STAT-OKS                               
                   MOVE 'TAB'        TO MSG-COD-SIST                    
                   MOVE 'QABTABDMAS' TO MSG-COD-MENS.                   
       EXT-PUT-TAB-ASC.                                                 
           EXIT.                                                        
                                                                        
       PUT-TAB-DSC SECTION.                                             
       INI-PUT-TAB-DSC.                                                 
           MOVE SPACES TO MSG-COD-MENS.                                 
           IF SCR-NUM-PANT = 1                                          
               MOVE 'QABTABNOPRV' TO MSG-COD-MENS                       
               GO TO EXT-PUT-TAB-DSC.                                   
                                                                        
           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.                
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.               
           ADD 1 TO QUE-NITM.                                           
           ADD 1 WSS-IMAX GIVING FRM-IFLD.                              
       LUP-PUT-TAB-DSC.                                                 
           SUBTRACT  1 FROM QUE-NITM.                                   
           MOVE QAB-NOM-TSQAB TO QUE-COLA                               
           MOVE QAB-LEN-TSQAB TO QUE-LITM                               
           MOVE QUE-GET       TO QUE-CMND.                              
           PERFORM GNS-PRO-QUE.                                         
           IF QUE-STAT NOT = QUE-STAT-OKS                               
               IF FRM-IFLD > WSS-IMAX                                   
                   MOVE 'QABTABNOPRV' TO MSG-COD-MENS                   
                   GO TO FIN-PUT-TAB-DSC                                
               ELSE                                                     
                   GO TO FIN-PUT-TAB-DSC.                               
           SUBTRACT 1 FROM FRM-IFLD.                                    
           MOVE QUE-ITEM TO FRM-CAI-CONV-IDX(FRM-IFLD).                 
           IF FRM-IFLD > 1                                              
               GO TO LUP-PUT-TAB-DSC.                                   
       FIN-PUT-TAB-DSC.                                                 
                                                                        
           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1                  
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I                    
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1           
                                 UNTIL WSS-I < 1.                       
                                                                        
           IF FRM-IFLD NOT > WSS-IMAX                                   
               SUBTRACT 1 FROM SCR-NUM-PANT                             
               MOVE 'TAB'        TO MSG-COD-SIST                        
               MOVE 'QABTABDMAS' TO MSG-COD-MENS.                       
       EXT-PUT-TAB-DSC.                                                 
           EXIT.                                                        
                                                                        
                                                                        
       COPY GNSBGCPT.                                                   
       COPY GNSBGSTR.                                                   
                                                                        
       CLR-FRM SECTION.                                                 
       INI-CLR-FRM.                                                     
           MOVE SPACES TO FRM-CAI-CONV IN QAB-FLD(WSS-I).               
           MOVE SPACES TO FRM-IIC-CONV IN QAB-FLD(WSS-I).               
           MOVE SPACES TO FRM-IND-TIPO IN QAB-FLD(WSS-I).               
           MOVE SPACES TO FRM-GLS-TIPO IN QAB-FLD(WSS-I).               
           MOVE ZEROES TO FRM-NUM-ICAB IN QAB-FLD(WSS-I).               
           MOVE SPACES TO FRM-IND-SMWI IN QAB-FLD(WSS-I).               
           MOVE SPACES TO FRM-GLS-SMWI IN QAB-FLD(WSS-I).               
       FIN-CLR-FRM.                                                     
           EXIT.                                                        
                                                                        
       CLR-WSS SECTION.                                                 
       INI-CLR-WSS.                                                     
           MOVE SPACES TO WSS-CAI-CONV-MTI IN WSS-VARI.                 
           MOVE SPACES TO WSS-IIC-CONV-MTI IN WSS-VARI.                 
           MOVE SPACES TO WSS-IND-TIPO-MTI IN WSS-VARI.                 
           MOVE SPACES TO WSS-GLS-TIPO-MTI IN WSS-VARI.                 
           MOVE ZEROES TO WSS-NUM-ICAB-MTI IN WSS-VARI.                 
           MOVE SPACES TO WSS-IND-SMWI-MTI IN WSS-VARI.                 
           MOVE SPACES TO WSS-GLS-SMWI-MTI IN WSS-VARI.                 
           MOVE SPACES TO WSS-CAI-CONV-FRM IN WSS-VARI.                 
           MOVE SPACES TO WSS-IIC-CONV-FRM IN WSS-VARI.                 
           MOVE SPACES TO WSS-IND-TIPO-FRM IN WSS-VARI.                 
           MOVE SPACES TO WSS-GLS-TIPO-FRM IN WSS-VARI.                 
           MOVE ZEROES TO WSS-NUM-ICAB-FRM IN WSS-VARI.                 
           MOVE SPACES TO WSS-IND-SMWI-FRM IN WSS-VARI.                 
           MOVE SPACES TO WSS-GLS-SMWI-FRM IN WSS-VARI.                 
           IF SCR-OFRM-SRV                                              
               PERFORM CLR-FRM-MTX.                                     
       FIN-CLR-WSS.                                                     
           EXIT.                                                        
                                                                        
       CLR-FRM-MTX SECTION.                                             
       INI-CLR-FRM-MTX.                                                 
           MOVE SPACES TO FRM-CAI-CONV IN QAB-FLD(1).                   
           MOVE SPACES TO FRM-IIC-CONV IN QAB-FLD(1).                   
           MOVE SPACES TO FRM-IND-TIPO IN QAB-FLD(1).                   
           MOVE SPACES TO FRM-GLS-TIPO IN QAB-FLD(1).                   
           MOVE ZEROES TO FRM-NUM-ICAB IN QAB-FLD(1).                   
           MOVE SPACES TO FRM-IND-SMWI IN QAB-FLD(1).                   
           MOVE SPACES TO FRM-GLS-SMWI IN QAB-FLD(1).                   
       FIN-CLR-FRM-MTX.                                                 
           EXIT.                                                        
                                                                        
       CRG-VEC-QAB SECTION.                                             
       INI-CRG-VEC-QAB.                                                 
           ADD 1 TO WSS-J.                                              
           IF WSS-J IN WSS-VARI > WSS-JMAX IN WSS-VARI                  
               GO TO FIN-CRG-VEC-QAB.                                   
                                                                        
           PERFORM CLR-QAB-VEC.                                         
                                                                        
           MOVE CAB-CAI-CONV IN CAB TO WSS-CAI-CONV-VEC(WSS-J).         
           MOVE CAB-IIC-CONV IN CAB TO WSS-IIC-CONV-VEC(WSS-J).         
           MOVE CAB-IND-TIPO IN CAB TO WSS-IND-TIPO-VEC(WSS-J).         
           IF CAB-IND-TIPO IN CAB = 'A'                                 
              MOVE 'ALTA        '   TO WSS-GLS-TIPO-VEC(WSS-J)          
           ELSE                                                         
           IF CAB-IND-TIPO IN CAB = 'B'                                 
              MOVE 'BAJA        '   TO WSS-GLS-TIPO-VEC(WSS-J)          
           ELSE                                                         
              MOVE 'DESCONOCIDO '   TO WSS-GLS-TIPO-VEC(WSS-J).         
           MOVE CAB-NUM-ICAB IN CAB TO WSS-NUM-ICAB-VEC(WSS-J).         
           MOVE CAB-IND-SMWI IN CAB TO WSS-IND-SMWI-VEC(WSS-J).         
           IF CAB-IND-SMWI IN CAB > SPACES                              
               MOVE 'TAB'               TO TAB-COD-SIST                 
               MOVE 'SWI'               TO TAB-COD-TTAB IN TAB          
               MOVE CAB-IND-SMWI IN CAB TO TAB-COD-CTAB IN TAB          
               PERFORM BUS-TAB                                          
               MOVE TAB-GLS-DCOR IN TAB TO WSS-GLS-SMWI-VEC(WSS-J)      
           ELSE                                                         
               MOVE SPACES              TO WSS-GLS-SMWI-VEC(WSS-J).     
       FIN-CRG-VEC-QAB.                                                 
           EXIT.                                                        
                                                                        
       CLR-QAB-VEC SECTION.                                             
       INI-CLR-QAB-VEC.                                                 
           MOVE SPACES TO WSS-CAI-CONV-VEC(WSS-J).                      
           MOVE SPACES TO WSS-IIC-CONV-VEC(WSS-J).                      
           MOVE SPACES TO WSS-IND-TIPO-VEC(WSS-J).                      
           MOVE SPACES TO WSS-GLS-TIPO-VEC(WSS-J).                      
           MOVE ZEROES TO WSS-NUM-ICAB-VEC(WSS-J).                      
           MOVE SPACES TO WSS-IND-SMWI-VEC(WSS-J).                      
           MOVE SPACES TO WSS-GLS-SMWI-VEC(WSS-J).                      
       FIN-CLR-QAB-VEC.                                                 
           EXIT.                                                        
                                                                        
       PRO-FLD-QUE SECTION.                                             
       INI-PRO-FLD-QUE.                                                 
           MOVE 'P' TO WSS-CAI-CONV-MTI-ATR WSS-CAI-CONV-FRM-ATR.       
           MOVE 'P' TO WSS-IIC-CONV-MTI-ATR WSS-IIC-CONV-FRM-ATR.       
           MOVE 'P' TO WSS-IND-TIPO-MTI-ATR WSS-IND-TIPO-FRM-ATR.       
           MOVE 'P' TO WSS-GLS-TIPO-MTI-ATR WSS-GLS-TIPO-FRM-ATR.       
           MOVE 'P' TO WSS-NUM-ICAB-MTI-ATR WSS-NUM-ICAB-FRM-ATR.       
           MOVE 'P' TO WSS-IND-SMWI-MTI-ATR WSS-IND-SMWI-FRM-ATR.       
           MOVE 'P' TO WSS-GLS-SMWI-MTI-ATR WSS-GLS-SMWI-FRM-ATR.       
       FIN-PRO-FLD-QUE.                                                 
           EXIT.                                                        
      *>>>> EOF                                                  
*% END       
