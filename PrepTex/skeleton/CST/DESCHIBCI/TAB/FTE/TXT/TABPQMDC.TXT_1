*% PGM_PTC = TRUE
*% LARGO_TOTAL_COMMAREA = 3000
*% IF FIN_SCR_VARI
      *<<<<
           03 SCR-NUM-PANT               PIC 9(0010).                   
           03 SCR-MDC-FLD                PIC X(1200).                   
      *>>>>
*% END
*% IF WSS
      *<<<< WSS                                                         
       COPY GNSWGCPT.                                                   
       COPY TABWXMDC.                                                   
       COPY TABBRMET.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MET-REQA==.        
       COPY SGCBRDBC.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.        
      *>>>>                                                             
*% END
*% IF INI_FIO
      *<<<< INI_FIO                                                     
           IF SCR-OFRM-SRV                                              
                MOVE TSK-TERM-ALF TO MDC-COD-TERM                       
                MOVE TSK-TERM-ALF TO MDC-COD-TERM-MAS                   
           ELSE                                                         
                MOVE EIBTRMID     TO MDC-COD-TERM                       
                MOVE EIBTRMID     TO MDC-COD-TERM-MAS.                  
           MOVE +3859 TO SCR-TCMA.                                      
           PERFORM GNS-ERR-QUE.                                         
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )      
               MOVE SCR-MDC-FLD TO MDC-FLD                              
               GO TO FIN-INI.                                           
                                                                        
           IF ( FRM-FFLD = FRM-FFLD-PF6 )                               
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM        
               ADD 1              TO QUE-NITM                           
               MOVE MDC-NOM-TSMDC TO QUE-COLA                           
               MOVE MDC-LEN-TSMDC TO QUE-LITM                           
               MOVE QUE-GET       TO QUE-CMND                           
               PERFORM GNS-PRO-QUE                                      
               IF QUE-STAT NOT = QUE-STAT-OKS                           
                  MOVE SCR-MDC-FLD TO MDC-FLD                           
                  GO TO FIN-INI.                                        
           PERFORM GET-FHOY.                                            
      *>>>>                                                             
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC                                                 
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6                   
               PERFORM VAL-NUM-KEY-MDC                                  
               PERFORM VAL-NUM-FLD-MDC                                  
               PERFORM SCRL-TAB-MDC                                     
               PERFORM SCR-TRL.                                         
      *>>>>                                                             
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY                                                 
           IF FRM-IND-NEXT IN MDC-FLD = 'S'                             
               MOVE MET-CAI-CONV IN MDC-FLD TO MET-CAI-CONV IN MET      
               MOVE MET-IIC-CONV IN MDC-FLD TO MET-IIC-CONV IN MET      
               MOVE MET-COD-TMET IN MDC-FLD TO MET-COD-TMET IN MET      
           ELSE                                                         
               MOVE WFC-CAI-CONV IN MDC-FLD TO MET-CAI-CONV IN MET      
               MOVE WFC-IIC-CONV IN MDC-FLD TO MET-IIC-CONV IN MET      
               MOVE SPACES                  TO MET-COD-TMET IN MET.     
                                                                        
           MOVE ZEROES TO WSS-J IN WSS-VARI.                            
           MOVE ZEROES TO MET-NUM-IMET IN MET.                          
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-LEE-MET.                                                     
           PERFORM TAB-FIO-MET.                                         
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF                          
              GO TO CON-FND-KEY.                                        
           IF NOT (MET-CAI-CONV IN MET = WFC-CAI-CONV IN MDC-FLD AND    
                   MET-IIC-CONV IN MET = WFC-IIC-CONV IN MDC-FLD)       
              GO TO CON-FND-KEY.                                        
                                                                        
           PERFORM CRG-VEC-MDC.                                         
                                                                        
           IF (SCR-OFRM-SRV AND WSS-J > WSS-NITMMAX)                    
              GO TO CON-FND-KEY.                                        
                                                                        
           IF ((NOT SCR-OFRM-SRV) AND WSS-J > WSS-JMAX)                 
                 GO TO CON-FND-KEY.                                     
       NXT-LEE-MET.                                                     
           MOVE FIO-GET-NXT TO FIO-CMND.
           GO TO LUP-LEE-MET.           
                                      
      *>>>>                                                             
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY                                                 
       CON-FND-KEY.                                                     
           PERFORM PUT-TAB.                                             
           IF MSG-COD-MENS > SPACES                                     
              GO TO MSG-FND-KEY.                                        
                                                                        
           MOVE WFC-CAI-CONV IN MDC-FLD TO WFC-CAI-CONV IN WFC.         
           MOVE WFC-IIC-CONV IN MDC-FLD TO WFC-IIC-CONV IN WFC.         
           MOVE '99999999'              TO WFC-FEC-FCNV IN WFC.         
           MOVE '999999'                TO WFC-HRA-CONV IN WFC.         
           MOVE FIO-GET-LEQ  TO FIO-CMND.                               
           PERFORM TAB-FIO-WFC.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    WFC-CAI-CONV IN WFC = WFC-CAI-CONV IN MDC-FLD AND   
                    WFC-IIC-CONV IN WFC = WFC-IIC-CONV IN MDC-FLD )     
              MOVE 'TAB' TO MSG-COD-SIST                                
              MOVE 'MDC    NEX' TO MSG-COD-MENS                         
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              MOVE '23' TO FIO-STAT PGM-STAT-WFC                        
              GO TO FIN-FND-KEY.                                        
                                                                        
           MOVE FIO-STAT TO PGM-STAT-WFC.                               
           MOVE WFC-NUM-DCNV IN WFC TO WFC-NUM-DCNV IN MDC-FLD.         
           MOVE WFC-NUM-MCNV IN WFC TO WFC-NUM-MCNV IN MDC-FLD.         
           MOVE WFC-NUM-SCNV IN WFC TO WFC-NUM-SCNV IN MDC-FLD.         
           MOVE WFC-NUM-ACNV IN WFC TO WFC-NUM-ACNV IN MDC-FLD.         
           MOVE WFC-HRA-CONV IN WFC TO WFC-HRA-CONV IN MDC-FLD.         
           MOVE WFC-MSC-STAT IN WFC TO WFC-MSC-STAT IN MDC-FLD.         
           MOVE WFC-COD-ATRN IN WFC TO WFC-COD-ATRN IN MDC-FLD.         
           IF WFC-MSC-STAT IN MDC-FLD > SPACES                          
               MOVE 'GNS' TO TAB-COD-SIST                               
               MOVE 'STT ' TO TAB-COD-TTAB IN TAB                       
               MOVE WFC-MSC-STAT IN MDC-FLD TO TAB-COD-CTAB IN TAB      
               PERFORM BUS-TAB                                          
               MOVE TAB-GLS-DCOR IN TAB TO FRM-GLS-STAT IN MDC-FLD      
           ELSE                                                         
               MOVE SPACES              TO FRM-GLS-STAT IN MDC-FLD.     
                                                                        
           MOVE WFC-CAI-EMPR IN WFC TO DBC-CAI-ICLI IN DBC.             
           MOVE WFC-IIC-EMPR IN WFC TO DBC-IIC-ICLI IN DBC.             
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM SGC-FIO-DBC.                                         
           MOVE FIO-STAT TO PGM-STAT-DBC.                               
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.                        
           PERFORM CPT-BLKS.                                            
           MOVE CPT-STRN            TO DBC-GLS-NOMC IN MDC-FLD.         
                                                                        
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN MDC-FLD.         
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN MDC-FLD.         
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN MDC-FLD.         
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN MDC-FLD.         
                                                                        
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           GO TO FIN-FND-KEY.                                           
      *----------------------------------------------------------       
       MSG-FND-KEY.                                                     
           MOVE 'TAB' TO MSG-COD-SIST.                                  
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MENS.                               
           MOVE '00' TO FIO-STAT.                                       
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           GO TO FIN-FND-KEY.                                           
      *>>>>                                                             
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON                                                 
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )           
      *>>>>                                                             
*% END
*% IF EOF
      *<<<< EOF                                                         
       PGM-EBR-ALL SECTION.                                             
       INI-PGM-EBR-ALL.                                                 
           MOVE 'GNS' TO FIO-SIST.                                      
           MOVE FIO-EBR-ALL TO FIO-CMND.                                
           PERFORM GNS-FIO-TAB.                                         
       FIN-PGM-EBR-ALL.                                                 
           EXIT.                                                        

       COPY GNSBGVSM.                                                   
                                                                        
       COPY GNSBBTAB.                                                   
       COPY GNSBVTAB.                                                   
       COPY GNSBVCOD.                                                   
       COPY GNSBPFEC.                                                   
                                                                        
       PUT-TAB SECTION.                                                 
       INI-PUT-TAB.                                                     
           MOVE MDC-NOM-TSMDC     TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE MDC-NOM-TSMDC-MAS TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE 1          TO SCR-NUM-PANT.                             
           MOVE LOW-VALUES TO WSS-COD-TMET-IDX-MTI.                     
           MOVE LOW-VALUES TO WSS-COD-TMET-IDX-FRM.                     
      *                                                                 
           MOVE 1 TO FRM-IFLD.                                          
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.                        
                                                                        
           MOVE 1 TO WSS-F IN WSS-VARI.                                 
       LUP-PUT-TAB.                                                     
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI                  
              GO TO FIN-PUT-TAB.                                        
           IF WSS-COD-TMET-VEC(WSS-F) > SPACES                          
              PERFORM PUT-TAB-MDC                                       
              PERFORM PUT-MDC-QUE                                       
              PERFORM SET-FRM-IFLD                                      
           ELSE                                                         
           IF FRM-IFLD = 1 AND WSS-F = 1                                
              MOVE 'TAB' TO MSG-COD-SIST                                
              MOVE 'MDC    NEX' TO MSG-COD-MENS                         
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              MOVE '00' TO FIO-STAT                                     
      *       MOVE 'S' TO SCR-IERR                                      
              GO TO FIN-PUT-TAB                                         
           ELSE                                                         
              MOVE '00' TO FIO-STAT                                     
              GO TO FIN-PUT-TAB.                                        
                                                                        
           ADD 1 TO WSS-F IN WSS-VARI.                                  
           GO TO LUP-PUT-TAB.                                           
       FIN-PUT-TAB.                                                     
           MOVE 1                  TO QUE-NITM.                         
           MOVE MDC-LEN-TSMDC-MAS  TO QUE-LITM.                         
           MOVE MDC-NOM-TSMDC-MAS  TO QUE-COLA.                         
                                                                        
           IF WSS-NITM > WSS-NITMMAX                                    
              MOVE 'S'           TO MDC-MAS-IND-HMAS                    
           ELSE                                                         
              MOVE 'N'           TO MDC-MAS-IND-HMAS.                   
                                                                        
           MOVE WSS-NITM         TO WSS-NUM-ICUR.                       
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.                   
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.                   
           MOVE WSS-NUM-ICUR-ALF TO MDC-MAS-NUM-TOCC.                   
           MOVE MDC-MAS-ITEM     TO QUE-ITEM.                           
           MOVE QUE-PUT          TO QUE-CMND.                           
           PERFORM GNS-PRO-QUE.                                         
           IF WSS-NITM > WSS-IMAX                                       
               MOVE 'TAB' TO MSG-COD-SIST                               
               MOVE 'MDCTABDMAS' TO MSG-COD-MENS                        
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN2                            
               MOVE SPACES       TO MSG-COD-MENS.                       
       EXT-PUT-TAB.                                                     
           EXIT.                                                        
                                                                        
       SET-FRM-IFLD SECTION.                                            
       INI-SET-FRM-IFLD.                                                
           IF SCR-OFRM-SRV                                              
                MOVE 1 TO FRM-IFLD                                      
           ELSE                                                         
                ADD 1 TO FRM-IFLD.                                      
       FIN-SET-FRM-IFLD.                                                
           EXIT.                                                        
      *--------------------------------------------                     
       SCRL-TAB-MDC SECTION.                                            
       INI-SCRL-TAB-MDC.                                                
           MOVE SPACES TO MSG-COD-MENS.                                 
                                                                        
           IF FRM-FFLD = FRM-FFLD-PF6                                   
               PERFORM PUT-TAB-ASC                                      
           ELSE                                                         
               PERFORM PUT-TAB-DSC.                                     
           IF MSG-COD-MENS > SPACES                                     
               MOVE 'TAB' TO MSG-COD-SIST                               
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN2.                           
       FIN-SCRL-TAB-MDC.                                                
           EXIT.                                                        
                                                                        
      *--------------------------------------------                     
       PUT-TAB-MDC SECTION.                                                 
       INI-PUT-TAB-MDC.
           PERFORM CLR-WSS.                                             
           MOVE WSS-COD-TMET-VEC(WSS-F) TO WSS-COD-TMET-MTI IN WSS-VARI.
           MOVE WSS-GLS-TMET-VEC(WSS-F) TO WSS-GLS-TMET-MTI IN WSS-VARI.
           MOVE WSS-NUM-METR-VEC(WSS-F) TO WSS-NUM-METR-MTI IN WSS-VARI.
           MOVE WSS-COD-ATRN-VEC(WSS-F) TO WSS-COD-ATRN-MTI IN WSS-VARI.
                                                                        
           MOVE WSS-COD-TMET-MTI IN WSS-VARI TO WSS-COD-TMET-FRM.       
           MOVE WSS-GLS-TMET-MTI IN WSS-VARI TO WSS-GLS-TMET-FRM.       
           MOVE WSS-NUM-METR-MTI IN WSS-VARI TO WSS-NUM-METR-FRM.       
           MOVE WSS-COD-ATRN-MTI IN WSS-VARI TO WSS-COD-ATRN-FRM.       
                                                                        
           IF FRM-IFLD > WSS-IMAX                                       
              GO TO FIN-PUT-TAB-MDC.                                    
                                                                        
           MOVE WSS-COD-TMET-MTI IN WSS-VARI TO                         
                FRM-COD-TMET IN MDC-FLD(FRM-IFLD).                      
           MOVE WSS-GLS-TMET-MTI IN WSS-VARI TO                         
                FRM-GLS-TMET IN MDC-FLD(FRM-IFLD).                      
           MOVE WSS-NUM-METR-MTI IN WSS-VARI TO                         
                FRM-NUM-METR IN MDC-FLD(FRM-IFLD).                      
           MOVE WSS-COD-ATRN-MTI IN WSS-VARI TO                         
                FRM-COD-ATRN IN MDC-FLD(FRM-IFLD).                      
       FIN-PUT-TAB-MDC.                                                 
           EXIT.                                                        
      *----------------------------------------------------             
       PUT-MDC-QUE SECTION.                                             
       INI-PUT-MDC-QUE.                                                 
           PERFORM PRO-FLD-QUE.                                         
           ADD 1 TO WSS-NITM.                                           
           MOVE MDC-NOM-TSMDC    TO QUE-COLA.                           
           MOVE MDC-LEN-TSMDC    TO QUE-LITM.                           
           MOVE WSS-NITM         TO QUE-NITM.                           
           MOVE SPACES           TO QUE-ITEM.                           
           IF SCR-OFRM-SRV                                              
              MOVE WSS-COD-TMET-IDX-MTI  TO QUE-ITEM                    
           ELSE                                                         
              MOVE WSS-COD-TMET-IDX-FRM  TO QUE-ITEM.                   
           MOVE QUE-PUT  TO QUE-CMND.                                   
           PERFORM GNS-PRO-QUE.                                         
       FIN-PUT-MDC-QUE.                                                 
           EXIT.                                                        
      *----------------------------------------------------             
                                                                        
       PUT-TAB-ASC SECTION.                                             
       INI-PUT-TAB-ASC.                                                 
           MOVE SPACES TO MSG-COD-MENS.                                 
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.           
           MOVE 0 TO FRM-IFLD.                                          
       LUP-PUT-TAB-ASC.                                                 
           ADD 1              TO QUE-NITM.                              
           MOVE MDC-NOM-TSMDC TO QUE-COLA.                              
           MOVE MDC-LEN-TSMDC TO QUE-LITM.                              
           MOVE QUE-GET       TO QUE-CMND.                              
           PERFORM GNS-PRO-QUE.                                         
           IF QUE-STAT NOT = QUE-STAT-OKS                               
               IF FRM-IFLD = 0                                          
                   MOVE 'MDCTABNONXT' TO MSG-COD-MENS                   
                   GO TO FIN-PUT-TAB-ASC                                
               ELSE                                                     
                   GO TO FIN-PUT-TAB-ASC.                               
           ADD 1 TO FRM-IFLD.                                           
           MOVE QUE-ITEM TO FRM-COD-TMET-IDX(FRM-IFLD).                 
           IF FRM-IFLD < WSS-IMAX                                       
               GO TO LUP-PUT-TAB-ASC.                                   
       FIN-PUT-TAB-ASC.                                                 
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX                      
               ADD 1 FRM-IFLD GIVING WSS-I                              
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1            
                                 UNTIL WSS-I > WSS-IMAX.                
           IF FRM-IFLD > 0                                              
               ADD 1 TO SCR-NUM-PANT.                                   
           IF FRM-IFLD = WSS-IMAX                                       
               ADD 1              TO QUE-NITM                           
               MOVE MDC-NOM-TSMDC TO QUE-COLA                           
               MOVE MDC-LEN-TSMDC TO QUE-LITM                           
               MOVE QUE-GET       TO QUE-CMND                           
               PERFORM GNS-PRO-QUE                                      
               IF QUE-STAT = QUE-STAT-OKS                               
                   MOVE 'TAB'        TO MSG-COD-SIST                    
                   MOVE 'MDCTABDMAS' TO MSG-COD-MENS.                   
       EXT-PUT-TAB-ASC.                                                 
           EXIT.                                                        
                                                                        
       PUT-TAB-DSC SECTION.                                             
       INI-PUT-TAB-DSC.                                                 
           MOVE SPACES TO MSG-COD-MENS.                                 
           IF SCR-NUM-PANT = 1                                          
               MOVE 'MDCTABNOPRV' TO MSG-COD-MENS                       
               GO TO EXT-PUT-TAB-DSC.                                   
                                                                        
           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.                
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.               
           ADD 1 TO QUE-NITM.                                           
           ADD 1 WSS-IMAX GIVING FRM-IFLD.                              
       LUP-PUT-TAB-DSC.                                                 
           SUBTRACT  1 FROM QUE-NITM.                                   
           MOVE MDC-NOM-TSMDC TO QUE-COLA                               
           MOVE MDC-LEN-TSMDC TO QUE-LITM                               
           MOVE QUE-GET       TO QUE-CMND.                              
           PERFORM GNS-PRO-QUE.                                         
           IF QUE-STAT NOT = QUE-STAT-OKS                               
               IF FRM-IFLD > WSS-IMAX                                   
                   MOVE 'MDCTABNOPRV' TO MSG-COD-MENS                   
                   GO TO FIN-PUT-TAB-DSC                                
               ELSE                                                     
                   GO TO FIN-PUT-TAB-DSC.                               
           SUBTRACT 1 FROM FRM-IFLD.                                    
           MOVE QUE-ITEM TO FRM-COD-TMET-IDX(FRM-IFLD).                 
           IF FRM-IFLD > 1                                              
               GO TO LUP-PUT-TAB-DSC.                                   
       FIN-PUT-TAB-DSC.                                                 
                                                                        
           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1                  
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I                    
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1           
                                 UNTIL WSS-I < 1.                       
                                                                        
           IF FRM-IFLD NOT > WSS-IMAX                                   
               SUBTRACT 1 FROM SCR-NUM-PANT                             
               MOVE 'TAB'        TO MSG-COD-SIST                        
               MOVE 'MDCTABDMAS' TO MSG-COD-MENS.                       
       EXT-PUT-TAB-DSC.                                                 
           EXIT.                                                        
                                                                        
       CLR-FRM SECTION.                                                 
       INI-CLR-FRM.                                                     
           MOVE SPACES TO FRM-COD-TMET IN MDC-FLD(WSS-I).               
           MOVE SPACES TO FRM-GLS-TMET IN MDC-FLD(WSS-I).               
           MOVE ZEROES TO FRM-NUM-METR IN MDC-FLD(WSS-I).               
           MOVE SPACES TO FRM-COD-ATRN IN MDC-FLD(WSS-I).               
       FIN-CLR-FRM.                                                     
           EXIT.                                                        
                                                                        
       CLR-WSS SECTION.                                                 
       INI-CLR-WSS.                                                     
           MOVE SPACES TO WSS-COD-TMET-MTI IN WSS-VARI.                 
           MOVE SPACES TO WSS-GLS-TMET-MTI IN WSS-VARI.                 
           MOVE ZEROES TO WSS-NUM-METR-MTI IN WSS-VARI.                 
           MOVE SPACES TO WSS-COD-ATRN-MTI IN WSS-VARI.                 
                                                                        
           MOVE SPACES TO WSS-COD-TMET-FRM IN WSS-VARI.                 
           MOVE SPACES TO WSS-GLS-TMET-FRM IN WSS-VARI.                 
           MOVE ZEROES TO WSS-NUM-METR-FRM IN WSS-VARI.                 
           MOVE SPACES TO WSS-COD-ATRN-FRM IN WSS-VARI.                 
           IF SCR-OFRM-SRV                                              
               PERFORM CLR-FRM-MTX.                                     
       FIN-CLR-WSS.                                                     
           EXIT.                                                        
                                                                        
       CLR-FRM-MTX SECTION.                                             
       INI-CLR-FRM-MTX.                                                 
           MOVE SPACES TO FRM-COD-TMET IN MDC-FLD(1).                   
           MOVE SPACES TO FRM-GLS-TMET IN MDC-FLD(1).                   
           MOVE ZEROES TO FRM-NUM-METR IN MDC-FLD(1).                   
           MOVE SPACES TO FRM-COD-ATRN IN MDC-FLD(1).                   
       FIN-CLR-FRM-MTX.                                                 
           EXIT.                                                        
                                                                        
       CRG-VEC-MDC SECTION.                                             
       INI-CRG-VEC-MDC.                                                 
           ADD 1 TO WSS-J.                                              
           IF WSS-J IN WSS-VARI > WSS-JMAX IN WSS-VARI                  
               GO TO FIN-CRG-VEC-MDC.                                   
                                                                        
           PERFORM CLR-MDC-VEC.                                         
                                                                        
           MOVE MET-COD-TMET IN MET TO WSS-COD-TMET-VEC(WSS-J).         
           MOVE MET-VAL-CFMN IN MET TO WSS-NUM-METR-VEC(WSS-J).         
           MOVE MET-COD-ATRN IN MET TO WSS-COD-ATRN-VEC(WSS-J).         
                                                                        
           MOVE 'TAB'               TO TAB-COD-SIST.                    
           MOVE 'TDM'               TO TAB-COD-TTAB IN TAB.             
           MOVE MET-COD-TMET IN MET TO TAB-COD-CTAB IN TAB.             
           PERFORM BUS-TAB.                                             
           IF MSG-COD-MENS IN MSG NOT > SPACES                          
              MOVE TAB-GLS-DESC TO WSS-GLS-TMET-VEC(WSS-J)              
           ELSE                                                         
              MOVE SPACES       TO WSS-GLS-TMET-VEC(WSS-J).             
       FIN-CRG-VEC-MDC.                                                 
           EXIT.                                                        
                                                                        
       CLR-MDC-VEC SECTION.                                             
       INI-CLR-MDC-VEC.                                                 
           MOVE SPACES TO WSS-COD-TMET-VEC(WSS-J).                      
           MOVE SPACES TO WSS-GLS-TMET-VEC(WSS-J).                      
           MOVE ZEROES TO WSS-NUM-METR-VEC(WSS-J).                      
           MOVE SPACES TO WSS-COD-ATRN-VEC(WSS-J).                      
       FIN-CLR-MDC-VEC.                                                 
           EXIT.                                                        
                                                                        
       PRO-FLD-QUE SECTION.                                             
       INI-PRO-FLD-QUE.                                                 
           MOVE 'P' TO WSS-COD-TMET-MTI-ATR.                            
           MOVE 'P' TO WSS-GLS-TMET-MTI-ATR.                            
           MOVE 'P' TO WSS-NUM-METR-MTI-ATR.                            
           MOVE 'P' TO WSS-COD-ATRN-MTI-ATR.                            
                                                                        
           MOVE 'P' TO WSS-COD-TMET-FRM-ATR.                            
           MOVE 'P' TO WSS-GLS-TMET-FRM-ATR.                            
           MOVE 'P' TO WSS-NUM-METR-FRM-ATR.                            
           MOVE 'P' TO WSS-COD-ATRN-FRM-ATR.                            
       FIN-PRO-FLD-QUE.                                                 
           EXIT.                                                        
                                                                        
       COPY TABBFMET.                                                   
       COPY SGCBFDBC.                                                   
       COPY GNSBGCPT.                                                   
      *>>>> EOF                                                         
*% END
