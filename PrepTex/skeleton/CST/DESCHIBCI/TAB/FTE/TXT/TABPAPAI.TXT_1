*%! Codigo adicional
*% NO_TIENE_TIMESTAMP = TRUE
*% HDR_CST = FALSE
*% GNSTCA_ORIG = 3500
*% GNSTCA = 3500
*% IF WSS_DTC
      *<<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSC-REQA==.
      *>>>>
*% END
*% IF WSS
      *<<<< WSS
       COPY GNSBRMSC.
      *>>>>
*% END
*% IF SCR_GKEY_VAL_IKY
      *<<<< SCR_GKEY_VAL_IKY
           PERFORM VAL-CON-KEY-PAI.                                     
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR               
               PERFORM FST-KEY-PAI                                      
               GO TO ERR-SCR-GKEY.                                      
      *>>>> 
*% END
*% IF SCR_GIKY_VAL_IKY
      *<<<< SCR_GIKY_VAL_IKY
           PERFORM VAL-CON-IKY-PAI.                                     
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR               
               PERFORM FST-KEY-PAI                                      
               GO TO ERR-SCR-GIKY.                                      
      *>>>> 
*% END
*% IF INI_HBL_IKY
      *<<<< INI_HBL_IKY
          MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
      *>>>> 
*% END
*% IF INI_HBL_KEY  AND  PGM_PQ
      *<<<< INI_HBL_KEY
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.                           
      *>>>> 
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-INST = SCR-INST-HKY
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'PAI' TO TAB-COD-TTAB IN TAB
               PERFORM ERR-TAB.
      *>>>>
*% END
*% IF CMN_ING
      *<<<< CMN_ING
          MOVE ZEROES TO PAI-HRA-HRTR IN PAI.
      *>>>>
*% END
*% IF CMN_ING OR CMN_MOD
      *<<<< CMN_ING OR CMN_MOD
           PERFORM GET-FHOY.
           MOVE HOY-STP-ITRN TO PAI-STP-ITRN IN PAI.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBVIND.
       COPY GNSBVMSC.
       COPY GNSBETAB.
       COPY GNSBBMSC.
       COPY GNSBFMSC.
      *>>>>
*% END
