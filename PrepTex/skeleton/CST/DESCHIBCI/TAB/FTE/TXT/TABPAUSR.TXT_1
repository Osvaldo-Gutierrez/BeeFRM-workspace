*%! Codigo adicional
*% PGM_PTC = TRUE
*% IF SETTAG
*% VSNPQ = "06.06.06"
*% FNCPQ = "EJPOOL"
*% VSNPU = "06.06.06"
*% FNCPU = "EJPOOL"
*% END
*% IF WSS
      *<<<< WSS
*% IF PGM_PU
      *INI-AFRI/ABRIL-2005
       COPY GNSWGLOG.
       COPY GNSBRERR.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-ERR-REQA==.
      *FIN-AFRI/ABRIL-2005
*% END
      *PVG INI 18-JUL-2005
       COPY TABWXFAD.
      *PVG FIN 18-JUL-2005
       COPY GNSMXW01.
      *>>>>
*% END
*% IF PGM_POS_RCK
      *<<<< PGM_POS_RCK 
      * PTC-INI-AUT
           PERFORM GNS-ERR-QUE.
           PERFORM GET-TSK-TERM.
           MOVE TSK-TERM-ALF TO PTC-TT-TAB-TERM.
           MOVE TSK-TERM-ALF TO PTC-IT-TAB-TERM.
           MOVE TSK-TERM-ALF TO PTC-OT-TAB-TERM.
           MOVE TSK-TERM-ALF TO PTC-CT-TAB-TERM.
      *PVG INI 18-JUL-2005
           MOVE TSK-TERM-ALF TO FAD-COD-TERM-USR.
      *PVG FIN 18-JUL-2005
      *>>>> PGM_POS_RCK 
*% END
*% IF INI_FIO AND PGM_VMS
      *<<<< INI_FIO

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'TAB'        TO FIO-SIST.
           PERFORM TAB-FIO-OFI.
           MOVE FIO-INP TO FIO-CMND.
           PERFORM TAB-FIO-OFI.
           MOVE SCR-OTRN TO USR-COD-OTRN IN USR.
      *    Autor Transaccion
           MOVE SCR-USER TO USR-COD-ATRN IN USR.
      *>>>>
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-INST = SCR-INST-HKY
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'USR' TO TAB-COD-TTAB IN TAB
               PERFORM ERR-TAB.
*% IF PGM_PQ
           MOVE PTC-IT-TAB-COLA TO QUE-COLA.
           MOVE PTC-IT-TAB-LENG TO QUE-LITM.
           MOVE +1              TO QUE-NITM.
           MOVE QUE-GET         TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT = QUE-STAT-OKS
              MOVE QUE-ITEM     TO IT-001
              MOVE IT-001-02-APT-COD-SIST TO SCR-SIST
              MOVE QUE-DEL TO QUE-CMND
              PERFORM GNS-PRO-QUE.
           IF SCR-SISG NOT > SPACES
              MOVE SCR-SIST TO SCR-SISG.

           MOVE SPACES          TO QUE-ITEM.
           MOVE PTC-TT-TAB-COLA TO QUE-COLA.
           MOVE PTC-TT-TAB-LENG TO QUE-LITM.
           MOVE +1              TO QUE-NITM.
           MOVE QUE-GET         TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT = QUE-STAT-OKS
              MOVE QUE-ITEM TO PTC-TT-TAB-CMND
              MOVE QUE-DEL  TO QUE-CMND
              PERFORM GNS-PRO-QUE.
*% END
      *>>>>
*% END
*% IF FND_KEY
      *<<<< FND_KEY
           IF USR-COD-TTAB IN USR NOT > SPACES
               IF USR-COD-USER IN USR > SPACES
                   MOVE 'USR-COD-USER' TO FIO-AKEY
                   MOVE FIO-GET-KEY    TO FIO-CMND
                   PERFORM TAB-FIO-USR
                   IF NOT FIO-STAT-OKS
                       MOVE FIO-STAT-NEX TO FIO-STAT
                       GO TO FIN-FND-KEY
                   ELSE
                       MOVE FIO-STAT TO PGM-STAT-USR
                       GO TO FIN-FND-KEY
               ELSE
               IF USR-NUM-IUSR IN USR > ZEROES
                   MOVE 'USR-IDC-IUSR'  TO FIO-AKEY
                   MOVE FIO-GET-KEY-ALT TO FIO-CMND
                   PERFORM TAB-FIO-USR
                   IF NOT FIO-STAT-OKS
                       MOVE FIO-STAT-NEX TO FIO-STAT
                       GO TO FIN-FND-KEY
                   ELSE
                       MOVE FIO-STAT TO PGM-STAT-USR
                       GO TO FIN-FND-KEY
               ELSE
                   MOVE 'USR-GLS-ABRV'  TO FIO-AKEY
                   MOVE FIO-GET-KEY-ALT TO FIO-CMND
                   PERFORM TAB-FIO-USR
                   IF NOT FIO-STAT-OKS
                       MOVE FIO-STAT-NEX TO FIO-STAT
                       GO TO FIN-FND-KEY
                   ELSE
                       MOVE FIO-STAT TO PGM-STAT-USR
                       GO TO FIN-FND-KEY.
           IF SCR-OFRM-SRV
              IF NOT PTC-TT-TAB-CMND = 'CUN'
                 MOVE FIO-GET-NLS TO FIO-CMND
           ELSE
      *>>>>
*% END
*% IF FIN_PUT_SCR
      *<<<< FIN_PUT_SCR
           IF SCR-CMND = 'CON' AND
              SCR-OFRM-SRV
              MOVE PTC-OT-TAB-COLA TO QUE-COLA
              MOVE QUE-DEL TO QUE-CMND
              PERFORM GNS-PRO-QUE

              MOVE PTC-CT-TAB-COLA TO QUE-COLA
              MOVE QUE-DEL TO QUE-CMND
              PERFORM GNS-PRO-QUE
      *PVG INI 18-JUL-2005 (FILTRO ADICIONAL)
              PERFORM LEE-FAD-USR
      *PVG FIN 18-JUL-2005
              MOVE ZEROES TO QUE-NITM
              MOVE ZEROES TO FRM-IFLD
              IF PTC-TT-TAB-CMND = 'CUN'
                 MOVE FIO-GET-KEY TO FIO-CMND
              ELSE
                 MOVE SPACES TO OT-001
      *ISP-INI 26-11-2004
      *           MOVE FIO-GET-NLS TO FIO-CMND.
                  MOVE FIO-GET-NLS TO FIO-CMND
                  PERFORM TAB-FIO-USR
                  IF FIO-STAT-OKS
                      MOVE FIO-GET-NXT TO FIO-CMND.
      *ISP-FIN 26-11-2004
       CIC-PTC-TAB.
           IF SCR-CMND = 'CON' AND
              SCR-OFRM-SRV
              PERFORM TAB-FIO-USR
              MOVE 'N' TO WSS-VRF-TAB
              PERFORM VRF-TAB
              IF WSS-VRF-TAB NOT = 'N'
                 IF WSS-VRF-TAB  = 'V'
                    MOVE FIO-GET-NXT TO FIO-CMND
                    GO TO CIC-PTC-TAB
                 ELSE
                    ADD 1 TO WSS-001-TOT-OCC
                    IF WSS-001-TOT-OCC NOT > IT-001-TOT-OCC
                       MOVE FIO-STAT TO PGM-STAT-USR
                       ADD 1 TO FRM-IFLD

                       MOVE USR-COD-TTAB IN USR TO
                            OT-001-02-TAB-COD-TTAB IN OT-001(FRM-IFLD)
                       MOVE USR-COD-USER IN USR TO
                            OT-001-02-TAB-COD-CTAB IN OT-001(FRM-IFLD)
                       MOVE SCR-SIST            TO
                            OT-001-03-APT-COD-SIST IN OT-001(FRM-IFLD)
                       MOVE USR-CIC-USER IN USR TO
                            OT-001-03-APT-CIC-CTAB IN OT-001(FRM-IFLD)
                       MOVE USR-EXT-CTAB IN USR TO
                            OT-001-03-APT-EXT-CTAB IN OT-001(FRM-IFLD)
                       MOVE USR-GLS-DESC IN USR TO
                            OT-001-03-APT-GLS-DESC IN OT-001(FRM-IFLD)
                       MOVE USR-GLS-ABRV IN USR TO
                            OT-001-03-APT-GLS-ABRV IN OT-001(FRM-IFLD)
                       MOVE USR-GLS-DCOR IN USR TO
                            OT-001-03-APT-GLS-DCOR IN OT-001(FRM-IFLD)
                       MOVE USR-IND-VIGE IN USR TO
                            OT-001-03-APT-IND-VIGE IN OT-001(FRM-IFLD)

                       MOVE USR-NUM-DTRN IN USR TO
                            USR-NUM-DTRN IN USR-FLD
                       MOVE USR-NUM-MTRN IN USR TO
                            USR-NUM-MTRN IN USR-FLD
                       MOVE USR-NUM-STRN IN USR TO
                            USR-NUM-STRN IN USR-FLD
                       MOVE USR-NUM-ATRN IN USR TO
                            USR-NUM-ATRN IN USR-FLD
                       MOVE USR-FEC-FTRN IN USR-FLD TO
                            OT-001-03-APT-FEC-FTRN IN OT-001(FRM-IFLD)

                       MOVE SPACES TO
                            OT-001-03-APT-GLS-DATA IN OT-001(FRM-IFLD)
                       PERFORM PUT-QUE-TAB
                       IF NOT PTC-TT-TAB-CMND = 'CUN'
                          MOVE FIO-GET-NXT TO FIO-CMND
                          GO TO CIC-PTC-TAB
                       ELSE
                          MOVE 15 TO FRM-IFLD
                          PERFORM PUT-QUE-TAB
                    ELSE
                       IF FRM-IFLD > ZEROES
                          MOVE 15 TO FRM-IFLD
                          PERFORM PUT-QUE-TAB
                       ELSE
                          NEXT SENTENCE
              ELSE
                 IF FRM-IFLD > ZEROES
                    MOVE 15 TO FRM-IFLD
                    PERFORM PUT-QUE-TAB.
      *>>>> FIN_PUT_SCR
*% END
*% IF FIN_CMN_ELI
      *<<<< FIN_CMN_ELI
      *INI-AFRI/ABRIL-2005
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ELI.
           PERFORM GET-FHOY.
           MOVE 'TABPUUSR'          TO ELO-COD-ATRN IN ELO-VARI.
           MOVE 'TAB'               TO ELO-COD-SIST IN ELO-VARI.
           MOVE 'USR'               TO ELO-COD-ENTI IN ELO-VARI.
           MOVE 'USR'               TO ELO-COD-CMPO IN ELO-VARI.
           MOVE HOY-FHOY            TO ELO-FEC-FEJE IN ELO-VARI.
           MOVE HOY-HHOY            TO ELO-HRA-HRTR IN ELO-VARI.
           MOVE USR-COD-TABL IN USR TO ELO-KEY-IELO IN ELO-VARI.
           MOVE USR-COD-USER IN USR TO ELO-COD-MENS IN ELO-VARI.
           PERFORM CMN-ELI-LOG.
      *FIN-AFRI/ABRIL-2005
      *>>>> FIN_CMN_ELI
*% END
*% IF CMN_ING OR CMN_MOD
      *<<<< CMN_ING OR CMN_MOD
           PERFORM GET-FHOY.
           MOVE HOY-STP-ITRN TO USR-STP-ITRN IN USR.
      *>>>>
*% END
*% IF FIN_HBL_FLD
      *<<<< FIN_HBL_FLD
      *LRH ini 18-NOV-1994
           IF (SCR-CMND = 'ING' OR 'MOD' OR 'ACT') AND
              USR-COD-TTAB IN USR-FLD NOT = 'EJC'
               MOVE FRM-CPIM-AEY TO USR-COD-BNCA-ATR IN USR-FLD
                                    USR-COD-REGL-ATR IN USR-FLD
                                    USR-COD-PLAT-ATR IN USR-FLD.
      *LRH fin 18-NOV-1994
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBETAB.
*% IF PGM_PU
      *INI-AFRI/ABRIL-2005
       COPY GNSBGLOG.
       COPY GNSBFERR.
      *FIN-AFRI/ABRIL-2005
*% END
*% IF PGM_PQ
       PUT-QUE-TAB SECTION.
       INI-PUT-QUE-TAB.
           IF FRM-IFLD = 15
              MOVE PTC-OT-TAB-COLA TO QUE-COLA
              MOVE OT-001          TO QUE-ITEM
              MOVE PTC-OT-TAB-LENG TO QUE-LITM
              MOVE QUE-PUT  TO QUE-CMND
              PERFORM GNS-PRO-QUE
              MOVE SPACES TO OT-001
              MOVE ZEROES TO FRM-IFLD.
       FIN-PUT-QUE-TAB.
           EXIT.

       VER-TAB SECTION.
       INI-VER-TAB.
      *     IF FIO-CMND = FIO-GET-NLS
      *         DISPLAY 'REGISTRO RECUPERADO NLS USR:' USR.
       FIN-VER-TAB.
           EXIT.

       VRF-TAB SECTION.
       INI-VRF-TAB.
           IF FIO-STAT-OKS
              IF IT-001-02-APT-IND-VIGE > SPACES AND
                 IT-001-02-APT-IND-VIGE NOT = USR-IND-VIGE IN USR
                 MOVE 'V' TO WSS-VRF-TAB
              ELSE
                 IF PTC-TT-TAB-CMND = 'CUN'
                    IF IT-001-01-TAB-COD-CTAB = USR-COD-USER IN USR
                       MOVE 'S' TO WSS-VRF-TAB
                    ELSE
                       NEXT SENTENCE
                 ELSE
                    MOVE 'S' TO WSS-VRF-TAB.
      *PVG INI 18-JUL-2005
           IF NOT FIO-STAT-OKS
              GO TO FIN-VRF-TAB.

           IF WSS-VRF-TAB = 'V'
              GO TO FIN-VRF-TAB.

           IF FAD-USR-EXT-CTAB > SPACES AND
              USR-EXT-CTAB IN USR  NOT = FAD-USR-EXT-CTAB
              IF FAD-USR-EXT-CTAB = 'AUT  '
                 PERFORM VRF-USR-AUT
              ELSE
              IF FAD-USR-EXT-CTAB = 'APR  '
                 PERFORM VRF-USR-APR
              ELSE
              IF FAD-USR-EXT-CTAB = 'BCI  '
                 PERFORM VRF-EJC-BCI
              ELSE
              IF FAD-USR-EXT-CTAB = 'BCS  '
                 PERFORM VRF-EJC-BCS
              ELSE
                 MOVE 'V' TO WSS-VRF-TAB
                 GO TO FIN-VRF-TAB.

           IF FAD-USR-GLS-DESC > SPACES AND
              USR-GLS-DESC IN USR NOT = FAD-USR-GLS-DESC
              MOVE 'V' TO WSS-VRF-TAB
              GO TO FIN-VRF-TAB.

           IF FAD-USR-GLS-DCOR > SPACES AND
              USR-GLS-DCOR IN USR NOT = FAD-USR-GLS-DCOR
              MOVE 'V' TO WSS-VRF-TAB
              GO TO FIN-VRF-TAB.

           IF FAD-USR-IND-VIGE > SPACES AND
              USR-IND-VIGE IN USR NOT = FAD-USR-IND-VIGE
              MOVE 'V' TO WSS-VRF-TAB
              GO TO FIN-VRF-TAB.

           IF FAD-USR-COD-UBIC > SPACES AND
              USR-COD-UBIC IN USR NOT = FAD-USR-COD-UBIC
              MOVE 'V' TO WSS-VRF-TAB
              GO TO FIN-VRF-TAB.

           IF FAD-USR-COD-CUSR > SPACES AND
              USR-COD-CUSR IN USR NOT = FAD-USR-COD-CUSR
              MOVE 'V' TO WSS-VRF-TAB
              GO TO FIN-VRF-TAB.

           IF FAD-USR-COD-OFUS > SPACES AND
              USR-COD-OFUS IN USR NOT = FAD-USR-COD-OFUS
              MOVE 'V' TO WSS-VRF-TAB
              GO TO FIN-VRF-TAB.

           IF FAD-USR-COD-AGES > SPACES AND
              USR-COD-AGES IN USR NOT = FAD-USR-COD-AGES
              MOVE 'V' TO WSS-VRF-TAB
              GO TO FIN-VRF-TAB.

           IF FAD-USR-COD-BNCA > SPACES AND
              USR-COD-BNCA IN USR NOT = FAD-USR-COD-BNCA
              MOVE 'V' TO WSS-VRF-TAB
              GO TO FIN-VRF-TAB.

           IF FAD-USR-COD-REGL > SPACES AND
              USR-COD-REGL IN USR NOT = FAD-USR-COD-REGL
              MOVE 'V' TO WSS-VRF-TAB
              GO TO FIN-VRF-TAB.

           IF FAD-USR-COD-PLAT > SPACES AND
              USR-COD-PLAT IN USR NOT = FAD-USR-COD-PLAT
              MOVE 'V' TO WSS-VRF-TAB
              GO TO FIN-VRF-TAB.

           IF FAD-USR-COD-CNEG > SPACES AND
              USR-COD-CNEG IN USR NOT = FAD-USR-COD-CNEG
              MOVE 'V' TO WSS-VRF-TAB
              GO TO FIN-VRF-TAB.

           IF FAD-USR-COD-PTDT > SPACES AND
              USR-COD-PTDT IN USR NOT = FAD-USR-COD-PTDT
              MOVE 'V' TO WSS-VRF-TAB
              GO TO FIN-VRF-TAB.
      *PVG FIN 18-JUL-2005
       FIN-VRF-TAB.
           EXIT.

      *PVG INI 18-JUL-2005
       LEE-FAD-USR SECTION.
       INI-LEE-FAD-USR.
           MOVE FAD-NOM-TSFAD-USR TO QUE-COLA.
           MOVE FAD-LEN-TSFAD-USR TO QUE-LITM.
           MOVE +1                TO QUE-NITM.
           MOVE QUE-GET           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT = QUE-STAT-OKS
              MOVE QUE-ITEM          TO FAD-ITEM-USR
              MOVE FAD-NOM-TSFAD-USR TO QUE-COLA
              MOVE QUE-DEL           TO QUE-CMND
              PERFORM GNS-PRO-QUE
           ELSE
              MOVE SPACES            TO FAD-ITEM-USR.
       FIN-LEE-FAD-USR.
           EXIT.

       VRF-USR-APR SECTION.
       INI-VRF-USR-APR.
           IF FAD-IND-TAPR = 'N'
              PERFORM LEE-COD-AGES.
           MOVE ZEROES TO FAD-I.
           MOVE 'V' TO WSS-VRF-TAB.
       LUP-VRF-USR-APR.
           ADD 1 TO FAD-I.
           IF FAD-I > FAD-IMAX
              GO TO FIN-VRF-USR-APR.
           IF FAD-COD-AGES(FAD-I) NOT > SPACES
              GO TO FIN-VRF-USR-APR.
           IF USR-COD-AGES IN USR NOT = FAD-COD-AGES(FAD-I)
              GO TO LUP-VRF-USR-APR.
           MOVE 'S' TO WSS-VRF-TAB.
       FIN-VRF-USR-APR.
           EXIT.

       LEE-COD-AGES SECTION.
       INI-LEE-COD-AGES.
           MOVE 'S'         TO FAD-IND-TAPR.
           MOVE ZEROES      TO FAD-I.

           MOVE 'TAB'       TO TAB-COD-SIST FIO-SIST.
           MOVE 'ADG'       TO TAB-COD-TTAB IN TAB.
           MOVE SPACES      TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-NLS TO FIO-CMND.
       LUP-LEE-COD-AGES.
           PERFORM GNS-FIO-TAB.
           IF NOT (FIO-STAT-OKS AND TAB-COD-TTAB IN TAB = 'ADG')
              GO TO FIN-LEE-COD-AGES.
           IF NOT (TAB-IND-VIGE IN TAB    = 'S'   AND
                   TAB-COD-DAT3 IN TAB(1) = 'CNV' AND
                   TAB-COD-DAT3 IN TAB(2) = 'CPR' )
              MOVE FIO-GET-NXT TO FIO-CMND
              GO TO LUP-LEE-COD-AGES.
           ADD 1 TO FAD-I.
           IF FAD-I > FAD-IMAX
              GO TO FIN-LEE-COD-AGES.
           MOVE TAB-COD-CTAB IN TAB TO FAD-GLS-AGES(FAD-I).
           MOVE FIO-GET-NXT TO FIO-CMND.
           GO TO LUP-LEE-COD-AGES.
       FIN-LEE-COD-AGES.
           EXIT.

       VRF-USR-AUT SECTION.
       INI-VRF-USR-AUT.
           IF FAD-IND-TAUT = 'N'
              PERFORM LEE-COD-CUSR.
           MOVE ZEROES TO FAD-I.
           MOVE 'V' TO WSS-VRF-TAB.
       LUP-VRF-USR-AUT.
           ADD 1 TO FAD-I.
           IF FAD-I > FAD-IMAX
              GO TO FIN-VRF-USR-AUT.
           IF FAD-COD-CUSR(FAD-I) NOT > SPACES
              GO TO FIN-VRF-USR-AUT.
           IF USR-COD-CUSR IN USR NOT = FAD-COD-CUSR(FAD-I)
              GO TO LUP-VRF-USR-AUT.
           MOVE 'S' TO WSS-VRF-TAB.
       FIN-VRF-USR-AUT.
           EXIT.

       LEE-COD-CUSR SECTION.
       INI-LEE-COD-CUSR.
           MOVE 'S'         TO FAD-IND-TAUT.
           MOVE ZEROES      TO FAD-I.

           MOVE 'TAB'       TO TAB-COD-SIST FIO-SIST.
           MOVE 'CUS'       TO TAB-COD-TTAB IN TAB.
           MOVE SPACES      TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-NLS TO FIO-CMND.
       LUP-LEE-COD-CUSR.
           PERFORM GNS-FIO-TAB.
           IF NOT (FIO-STAT-OKS AND TAB-COD-TTAB IN TAB = 'CUS')
              GO TO FIN-LEE-COD-CUSR.
           IF NOT (TAB-IND-VIGE IN TAB    = 'S'   AND
                   TAB-COD-DAT3 IN TAB(1) = 'CNV' AND
                   TAB-COD-DAT3 IN TAB(2) = 'CPR' )
              MOVE FIO-GET-NXT TO FIO-CMND
              GO TO LUP-LEE-COD-CUSR.
           ADD 1 TO FAD-I.
           IF FAD-I > FAD-IMAX
              GO TO FIN-LEE-COD-CUSR.
           MOVE TAB-COD-CTAB IN TAB TO FAD-GLS-CUSR(FAD-I).
           MOVE FIO-GET-NXT TO FIO-CMND.
           GO TO LUP-LEE-COD-CUSR.
       FIN-LEE-COD-CUSR.
           EXIT.

       VRF-EJC-BCI SECTION.
       INI-VRF-EJC-BCI.
           IF USR-COD-TTAB IN USR NOT = 'EJC'
              MOVE 'V' TO WSS-VRF-TAB
              GO TO FIN-VRF-EJC-BCI.

           MOVE USR-COD-OFUS IN USR TO OFI-COD-OFIC IN OFI.
           PERFORM VAL-OFI.
           IF MSG-COD-MENS > SPACES
              MOVE 'V' TO WSS-VRF-TAB
              GO TO FIN-VRF-EJC-BCI.
           IF OFI-COD-MODL IN OFI = '8'
              MOVE 'V' TO WSS-VRF-TAB
              GO TO FIN-VRF-EJC-BCI.
           MOVE 'S' TO WSS-VRF-TAB.
       FIN-VRF-EJC-BCI.
           EXIT.

       VRF-EJC-BCS SECTION.
       INI-VRF-EJC-BCS.
           IF USR-COD-TTAB IN USR NOT = 'EJC'
              MOVE 'V' TO WSS-VRF-TAB
              GO TO FIN-VRF-EJC-BCS.

           MOVE USR-COD-OFUS IN USR TO OFI-COD-OFIC IN OFI.
           PERFORM VAL-OFI.
           IF MSG-COD-MENS > SPACES
              MOVE 'V' TO WSS-VRF-TAB
              GO TO FIN-VRF-EJC-BCS.
           IF OFI-COD-MODL IN OFI NOT = '8'
              MOVE 'V' TO WSS-VRF-TAB
              GO TO FIN-VRF-EJC-BCS.
           MOVE 'S' TO WSS-VRF-TAB.
       FIN-VRF-EJC-BCS.
           EXIT.
      *PVG FIN 18-JUL-2005
*% END
      *>>>>
*% END
