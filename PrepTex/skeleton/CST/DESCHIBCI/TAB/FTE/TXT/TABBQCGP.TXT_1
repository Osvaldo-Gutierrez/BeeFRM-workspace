*% IF FIN_PUT_PLN_CGP
      *<<<< FIN_PUT_PLN_CGP                                             
           MOVE 'TAB'               TO TAB-COD-SIST FIO-SIST.           
           MOVE 'DPN'               TO TAB-COD-TTAB IN TAB.             
           MOVE PLN-CIC-IPRD IN PLN TO TAB-COD-CTAB IN TAB.             
           PERFORM BUS-TAB.                                             
           IF MSG-COD-MENS > SPACES                                     
              MOVE 'DESCRIPCION INEXISTENTE: CODIGO DPN='               
                                       TO FRM-GLS-GRP1 IN CGP-FLD       
              MOVE PLN-CIC-IPRD IN PLN TO FRM-GLS-GRP2 IN CGP-FLD       
           ELSE                                                         
              MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-GRP1 IN CGP-FLD       
              MOVE TAB-GLS-DATA IN TAB TO FRM-GLS-GRP2 IN CGP-FLD.      
                                                                        
           MOVE 'GNS'               TO TAB-COD-SIST FIO-SIST.           
           MOVE 'STT'               TO TAB-COD-TTAB IN TAB.             
           MOVE PLN-MSC-STAT IN PLN TO TAB-COD-CTAB IN TAB.             
           PERFORM BUS-TAB.                                             
           MOVE TAB-GLS-DCOR IN TAB TO FRM-GLS-STAT IN CGP-FLD.         
      *>>>>                                                             
*% END
*% IF INI_PLN_FEC_FINI
      *<<<< INI_PLN_FEC_FINI                                            
           MOVE FEC-VHBL-N TO FEC-VHBL.                                 
           MOVE FEC-CHOY-N TO FEC-CHOY.                                 
           IF PLN-FEC-FINI IN CGP-FLD NOT > ZEROES                      
              MOVE HOY-DHOY IN HOY-VARI TO PLN-NUM-DINI IN CGP-FLD      
              MOVE HOY-MHOY IN HOY-VARI TO PLN-NUM-MINI IN CGP-FLD      
              MOVE HOY-SHOY IN HOY-VARI TO PLN-NUM-SINI IN CGP-FLD      
              MOVE HOY-AHOY IN HOY-VARI TO PLN-NUM-AINI IN CGP-FLD      
              GO TO FIN-VAL-PLN-FEC-FINI.                               
      *>>>>                                                             
*% END
*% IF PLN_HRA_HINI
      *<<<< PLN_HRA_HINI                                                
           IF PLN-HRA-HINI IN CGP-FLD NOT > ZEROES                      
              MOVE HOY-HHOY IN HOY-VARI TO PLN-HRA-HINI IN CGP-FLD      
              GO TO FIN-VAL-PLN-HRA-HINI.                               
                                                                        
           MOVE PLN-HRA-HINI IN CGP-FLD TO PLN-HRA-HINI IN PLN.         
           IF PLN-NUM-HHIN IN PLN < ZEROES OR PLN-NUM-HHIN IN PLN > 23  
               MOVE 'TAB'         TO MSG-COD-SIST                       
               MOVE 'NUMHHTRNOVA' TO MSG-COD-MENS                       
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE FRM-SUAR-MAL TO FRM-SUAR                            
               GO TO FIN-VAL-PLN-HRA-HINI.                              
           IF PLN-NUM-MMIN IN PLN < ZEROES OR PLN-NUM-MMIN IN PLN > 59  
               MOVE 'TAB'         TO MSG-COD-SIST                       
               MOVE 'NUMMMTRNOVA' TO MSG-COD-MENS                       
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE FRM-SUAR-MAL TO FRM-SUAR                            
               GO TO FIN-VAL-PLN-HRA-HINI.                              
           IF PLN-NUM-SSIN IN PLN < ZEROES OR PLN-NUM-SSIN IN PLN > 59  
               MOVE 'TAB'         TO MSG-COD-SIST                       
               MOVE 'NUMSSTRNOVA' TO MSG-COD-MENS                       
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE FRM-SUAR-MAL TO FRM-SUAR                            
               GO TO FIN-VAL-PLN-HRA-HINI.                              
      *>>>>                                                             
*% END
*% IF FRM_IND_TCON
      *<<<< FRM_IND_TCON                                                
           IF FRM-IND-TCON IN CGP-FLD NOT > SPACES AND                  
              FRM-NUM-IREG IN CGP-FLD > ZEROES                          
                PERFORM ERR-GLS                                         
                GO TO FIN-VAL-FRM-IND-TCON.                             
      *>>>>                                                             
*% END
*% IF FRM_NUM_IREG
      *<<<< FRM_NUM_IREG                                                
           IF FRM-NUM-IREG IN CGP-FLD NOT > ZEROES                      
              MOVE ZEROES TO FRM-NUM-IREG IN CGP-FLD.                   
      *>>>>                                                             
*% END
*% IF EDT_IKY_CGP
      *<<<< EDT_IKY_CGP                                                 
           GO TO EXT-EDT-IKY-CGP.                                       
      *>>>>                                                             
*% END
*% IF FIN_EDT_IKY_CGP
      *<<<< FIN_EDT_IKY_CGP                                             
       EXT-EDT-IKY-CGP.                                                 
           IF PLN-HRA-HINI-LEN IN CGP-FLD = FRM-ERR-NUME                
                MOVE FRM-IND-ERRO TO PLN-HRA-HINI-LEN IN CGP-FLD        
                GO TO FIN-EDT-IKY-CGP.                                  
           MOVE PLN-HRA-HINI IN CGP-FLD TO WSS-HRA-HINI-EDT IN WSS-VARI.
           MOVE WSS-HRA-HINI-EDT IN WSS-VARI TO                         
                PLN-HRA-HINI-ALF IN CGP-FLD.                            
           IF FRM-NUM-IREG-LEN IN CGP-FLD = FRM-ERR-NUME                
                MOVE FRM-IND-ERRO TO FRM-NUM-IREG-LEN IN CGP-FLD        
                GO TO FIN-EDT-IKY-CGP.                                  
           MOVE FRM-NUM-IREG IN CGP-FLD TO FRM-NUM-IREG-EDT IN CGP-EDT. 
           MOVE FRM-NUM-IREG-EDT IN CGP-EDT TO                          
                FRM-NUM-IREG-ALF IN CGP-FLD.                            
      *>>>>                                                             
*% END
