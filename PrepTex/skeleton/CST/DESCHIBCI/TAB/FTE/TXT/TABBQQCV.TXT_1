*% IF FIN_FST_KEY_QCV      
      *<<<< FIN_FST_KEY_QCV                                             
           MOVE -1 TO FRM-CAI-CONV-LEN IN QCV-FLD.                      
      *>>>>                                                             
*% END
*% IF DBC_GLS_ICLI
      *<<<< DBC_GLS_ICLI                                                
           IF DBC-NUM-ICLI IN QCV-FLD > ZEROES                          
              MOVE DBC-NUM-ICLI IN QCV-FLD TO DBC-NUM-ICLI IN DBC       
              MOVE DBC-IND-ICLI IN QCV-FLD TO DBC-IND-ICLI IN DBC       
              MOVE DBC-GLS-ICLI IN QCV-FLD TO DBC-GLS-ICLI IN DBC       
              MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY                          
              MOVE FIO-GET-KEY TO FIO-CMND                              
              PERFORM SGC-FIO-DBC                                       
              IF NOT FIO-STAT-OKS                                       
                  MOVE 'SGC'        TO MSG-COD-SIST                     
                  MOVE 'CLI    NEX' TO MSG-COD-MENS                     
                  PERFORM GET-MSG                                       
                  MOVE MSG-GLS-DESC TO FRM-MENS                         
                  MOVE FRM-SUAR-MAL TO FRM-SUAR                         
              ELSE                                                      
                  MOVE DBC-CIC-ICLI IN DBC TO WSS-CIC-EMPR.             
      *>>>>                                                             
*% END
*% IF INI_FRM_FEC_PINI
      *<<<< INI_FRM_FEC_PINI                                            
           IF FRM-FEC-PINI IN QCV-FLD NOT > ZEROES                      
               MOVE '01011950' TO FEC-FECH.                             
           MOVE FEC-CHOY-N  TO FEC-CHOY.                                
           MOVE FEC-VHBL-N TO FEC-VHBL.                                 
      *>>>>                                                             
*% END
*% IF INI_FRM_FEC_PFIN
      *<<<< INI_FRM_FEC_PFIN                                            
           MOVE FEC-CHOY-N TO FEC-CHOY.                                 
           MOVE FEC-VHBL-N TO FEC-VHBL.                                 
      *>>>>                                                             
*% END
*% IF FRM_COD_ESTA
      *<<<< FRM_COD_ESTA                                                
           IF FRM-COD-ESTA IN QCV-FLD NOT > SPACES                      
              MOVE SPACES TO FRM-COD-ESTA IN QCV-FLD                    
              GO TO FIN-VAL-FRM-COD-ESTA.                               
                                                                        
      *PVG INI 01-AGO-2005                                              
      *    MOVE 'GNS'                    TO TAB-COD-SIST.               
      *    MOVE 'STT'                    TO TAB-COD-TTAB IN TAB.        
           MOVE 'TAB'                    TO TAB-COD-SIST.               
           MOVE 'SWI'                    TO TAB-COD-TTAB IN TAB.        
      *PVG FIN 01-AGO-2005                                              
           MOVE FRM-COD-ESTA IN QCV-FLD  TO TAB-COD-CTAB IN TAB.        
           PERFORM VAL-TAB.                                             
           IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'               
              MOVE 'GNS'        TO MSG-COD-SIST                         
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC IN MSG TO FRM-MEN1                      
              MOVE FRM-SUAR-MAL TO FRM-SUAR.                            
      *>>>>                                                             
*% END
*% IF FRM_COD_ATRN
      *<<<< FRM_COD_ATRN                                                
           IF FRM-COD-ATRN IN QCV-FLD NOT > SPACES                      
              MOVE SPACES TO FRM-COD-ATRN IN QCV-FLD                    
              GO TO FIN-VAL-FRM-COD-ATRN.                               
      *>>>>                                                             
*% END
*% IF FRM_COD_OFCV
      *<<<< FRM_COD_OFCV                                                
           IF FRM-COD-OFCV IN QCV-FLD NOT > SPACES                      
              MOVE SPACES TO FRM-COD-OFCV IN QCV-FLD                    
              GO TO FIN-VAL-FRM-COD-OFCV.                               
                                                                        
           MOVE FRM-COD-OFCV IN QCV-FLD TO OFI-COD-OFIC IN OFI.         
           PERFORM VAL-OFI.                                             
           IF MSG-COD-MENS NOT = SPACES                                 
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE FRM-SUAR-MAL TO FRM-SUAR.                           
      *>>>>                                                             
*% END
*% IF INI_FRM_FEC_FIAP
      *<<<< INI_FRM_FEC_FIAP                                            
           MOVE FEC-CHOY-N  TO FEC-CHOY.                                
           MOVE FEC-VHBL-N TO FEC-VHBL.                                 
      *>>>>                                                             
*% END
*% IF INI_FRM_FEC_FTAP
      *<<<< INI_FRM_FEC_FTAP                                            
           MOVE FEC-CHOY-N TO FEC-CHOY.                                 
           MOVE FEC-VHBL-N TO FEC-VHBL.                                 
      *>>>>                                                             
*% END
*% IF FRM_COD_EBCS
      *<<<< FRM_COD_EBCS                                                
           IF FRM-COD-EBCS IN QCV-FLD NOT > SPACES                      
              MOVE SPACES TO FRM-COD-EBCS IN QCV-FLD                    
              GO TO FIN-VAL-FRM-COD-EBCS.                               
                                                                        
           MOVE 'EJC '                  TO USR-COD-TTAB IN USR.         
           MOVE FRM-COD-EBCS IN QCV-FLD TO USR-COD-USER IN USR.         
           PERFORM VAL-TDU.                                             
           IF MSG-COD-MENS > SPACES                                     
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE FRM-SUAR-MAL TO FRM-SUAR.                           
      *>>>>                                                             
*% END
*% IF FRM_COD_SBCS
      *<<<< FRM_COD_SBCS                                                
           IF FRM-COD-SBCS IN QCV-FLD NOT > SPACES                      
              MOVE SPACES TO FRM-COD-SBCS IN QCV-FLD                    
              GO TO FIN-VAL-FRM-COD-SBCS.                               
                                                                        
           MOVE FRM-COD-SBCS IN QCV-FLD TO OFI-COD-OFIC IN OFI.         
           PERFORM VAL-OFI.                                             
           IF MSG-COD-MENS NOT = SPACES                                 
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE FRM-SUAR-MAL TO FRM-SUAR.                           
      *>>>>                                                             
*% END
*% IF FRM_COD_EBCI
      *<<<< FRM_COD_EBCI                                                
           IF FRM-COD-EBCI IN QCV-FLD NOT > SPACES                      
              MOVE SPACES TO FRM-COD-EBCI IN QCV-FLD                    
              GO TO FIN-VAL-FRM-COD-EBCI.                               
                                                                        
           MOVE 'EJC '                  TO USR-COD-TTAB IN USR.         
           MOVE FRM-COD-EBCI IN QCV-FLD TO USR-COD-USER IN USR.         
           PERFORM VAL-TDU.                                             
           IF MSG-COD-MENS > SPACES                                     
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE FRM-SUAR-MAL TO FRM-SUAR.                           
      *>>>>                                                             
*% END
*% IF FRM_COD_SBCI
      *<<<< FRM_COD_SBCI                                                
           IF FRM-COD-SBCI IN QCV-FLD NOT > SPACES                      
              MOVE SPACES TO FRM-COD-SBCI IN QCV-FLD                    
              GO TO FIN-VAL-FRM-COD-SBCI.                               
                                                                        
           MOVE FRM-COD-SBCI IN QCV-FLD TO OFI-COD-OFIC IN OFI.         
           PERFORM VAL-OFI.                                             
           IF MSG-COD-MENS NOT = SPACES                                 
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE FRM-SUAR-MAL TO FRM-SUAR.                           
      *>>>>                                                             
*% END
*% IF FRM_COD_ACTC
      *<<<< FRM_COD_ACTC                                                
           IF FRM-COD-ACTC IN QCV-FLD NOT > SPACES                      
              MOVE SPACES TO FRM-COD-ACTC IN QCV-FLD                    
              GO TO FIN-VAL-FRM-COD-ACTC.                               
                                                                        
           MOVE 'TAB' TO TAB-COD-SIST                                   
           MOVE 'ACT' TO TAB-COD-TTAB IN TAB.                           
           MOVE FRM-COD-ACTC IN QCV-FLD TO TAB-COD-CTAB IN TAB.         
           PERFORM VAL-COD.                                             
      *>>>>                                                             
*% END
*% IF FRM_COD_PTRF
      *<<<< FRM_COD_PTRF                                                
           IF FRM-COD-PTRF IN QCV-FLD NOT > SPACES                      
              MOVE SPACES TO FRM-COD-PTRF IN QCV-FLD                    
              GO TO FIN-VAL-FRM-COD-PTRF.                               
                                                                        
           MOVE 'TAB' TO TAB-COD-SIST.                                  
           MOVE 'PTR' TO TAB-COD-TTAB IN TAB.                           
           MOVE FRM-COD-PTRF IN QCV-FLD TO TAB-COD-CTAB IN TAB.         
           PERFORM VAL-COD.                                             
      *>>>>                                                             
*% END
