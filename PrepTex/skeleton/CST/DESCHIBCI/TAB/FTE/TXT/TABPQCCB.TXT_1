*% PGM_PTC = TRUE
*% LARGO_TOTAL_COMMAREA = 3000
*% IF FIN_SCR_VARI
      *<<<<
           03 SCR-NUM-PANT               PIC 9(0002).                   
           03 SCR-CCB-FLD                PIC X(0250).                   
      *>>>>
*% END
*% IF WSS
      *<<<< WSS                                                         
       COPY TABWXCCB.                                                   
      *>>>>                                                             
*% END
*% IF INI_FIO
      *<<<< INI_FIO                                                     
           IF SCR-OFRM-SRV                                              
                MOVE TSK-TERM-ALF TO CCB-COD-TERM                       
                MOVE TSK-TERM-ALF TO CCB-COD-TERM-MAS                   
           ELSE                                                         
                MOVE EIBTRMID     TO CCB-COD-TERM                       
                MOVE EIBTRMID     TO CCB-COD-TERM-MAS.                  
                                                                        
           MOVE +3859 TO SCR-TCMA.                                      
           PERFORM GNS-ERR-QUE.                                         
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )      
               MOVE SCR-CCB-FLD TO CCB-FLD                              
               GO TO FIN-INI.                                           
                                                                        
           IF ( FRM-FFLD = FRM-FFLD-PF6 )                               
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM        
               ADD 1              TO QUE-NITM                           
               MOVE CCB-NOM-TSCCB TO QUE-COLA                           
               MOVE CCB-LEN-TSCCB TO QUE-LITM                           
               MOVE QUE-GET       TO QUE-CMND                           
               PERFORM GNS-PRO-QUE                                      
               IF QUE-STAT NOT = QUE-STAT-OKS                           
                  MOVE SCR-CCB-FLD TO CCB-FLD                           
                  GO TO FIN-INI.                                        
      *>>>>                                                             
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC                                                 
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6                   
               PERFORM VAL-NUM-KEY-CCB                                  
               PERFORM VAL-NUM-FLD-CCB                                  
               PERFORM SCRL-TAB-CCB                                     
               PERFORM SCR-TRL.                                         
      *>>>>                                                             
*% END
*% IF FND_KEY
      *<<<< FND_KEY                                                     
           GO TO CON-FND-KEY.                                           
      *>>>>                                                             
*% END
*% IF FIN_FND_KEY
      *<<<< FND_KEY                                                     
       CON-FND-KEY.                                                     
           PERFORM LEE-ALL-RCC.                                         
                                                                        
           PERFORM PUT-TAB.                                             
                                                                        
           MOVE '00' TO FIO-STAT.                                       
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           GO TO FIN-FND-KEY.                                           
       MSG-FND-KEY.                                                     
           MOVE 'TAB'        TO MSG-COD-SIST.                           
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MENS.                               
           MOVE '00'         TO FIO-STAT.                               
           MOVE SCR-STAT-CON TO SCR-STAT.                               
      *>>>>                                                             
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON                                                 
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )           
      *>>>>                                                             
*% END
*% IF EOF
      *<<<<
       PGM-EBR-ALL SECTION.                                             
       INI-PGM-EBR-ALL.                                                 
           MOVE 'GNS' TO FIO-SIST.                                      
           MOVE FIO-EBR-ALL TO FIO-CMND.                                
           PERFORM GNS-FIO-TAB.                                         
       FIN-PGM-EBR-ALL.                                                 
           EXIT.                                                        

       COPY GNSBBTAB.                                                   
       COPY GNSBVTAB.                                                   
       COPY GNSBVCOD.                                                   
                                                                        
       LEE-ALL-RCC SECTION.                                             
       INI-LEE-ALL-RCC.                                                 
           MOVE SPACES      TO MSG-COD-MENS.                            
           MOVE SPACES      TO RCC-KEY-IREG IN RCC.                     
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-LEE-ALL-RCC.                                                 
           PERFORM TAB-FIO-RCC.                                         
           IF NOT FIO-STAT-OKS                                          
              GO TO EXT-LEE-ALL-RCC.                                    
           IF RCC-COD-BANC IN CCB-FLD > SPACES AND                      
              RCC-COD-BANC IN RCC NOT = RCC-COD-BANC IN CCB-FLD         
              GO TO NXT-LEE-ALL-RCC.                                    
                                                                        
           IF RCC-MSC-STAT IN RCC NOT = 'A'                             
              GO TO NXT-LEE-ALL-RCC.                                    
                                                                        
           IF RCC-COD-BANC IN RCC NOT > SPACES                          
              MOVE 'SIN' TO RCC-COD-BANC IN RCC.                        
           PERFORM CRG-VEC-CCB.                                         
           IF RCC-COD-BANC IN RCC = 'SIN'                               
              MOVE SPACES TO RCC-COD-BANC IN RCC.                       
                                                                        
       NXT-LEE-ALL-RCC.                                                 
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-LEE-ALL-RCC.                                       
       EXT-LEE-ALL-RCC.                                                 
           IF FIO-STAT-OKS                                              
              MOVE FIO-END-BRW TO FIO-CMND                              
              PERFORM TAB-FIO-RCC.                                      
       FIN-LEE-ALL-RCC.                                                 
           EXIT.                                                        
                                                                        
       CRG-VEC-CCB SECTION.                                             
       INI-CRG-VEC-CCB.                                                 
           MOVE 1 TO WSS-F IN WSS-VARI.                                 
       LUP-CRG-VEC-CCB.                                                 
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI                  
              GO TO FIN-CRG-VEC-CCB.                                    
           IF WSS-COD-BANC-V(WSS-F) NOT > SPACES                        
              GO TO CON-CRG-VEC-CCB.                                    
           IF WSS-COD-BANC-V(WSS-F) = RCC-COD-BANC IN RCC               
              GO TO CON-CRG-VEC-CCB-2.                                  
           ADD 1 TO WSS-F.                                              
           GO TO LUP-CRG-VEC-CCB.                                       
       CON-CRG-VEC-CCB.                                                 
           PERFORM CLR-CCB-VEC.                                         
                                                                        
           MOVE RCC-COD-BANC IN RCC TO WSS-COD-BANC-V(WSS-F).           
                                                                        
       CON-CRG-VEC-CCB-2.                                               
           ADD 1 TO WSS-NUM-TBAN-V(WSS-F).                              
           ADD 1 TO WSS-NUM-TOTA IN WSS-VARI.                           
           IF RCC-COD-CPTO IN RCC = 'INT'                               
              ADD 1 TO WSS-NUM-CINT-V(WSS-F)                            
              ADD 1 TO WSS-NUM-TINT IN WSS-VARI                         
           ELSE                                                         
           IF RCC-COD-CPTO IN RCC = 'SGS'                               
              ADD 1 TO WSS-NUM-CSEG-V(WSS-F)                            
              ADD 1 TO WSS-NUM-TSEG IN WSS-VARI                         
           ELSE                                                         
           IF RCC-COD-CPTO IN RCC = 'COM'                               
              ADD 1 TO WSS-NUM-CCOM-V(WSS-F)                            
              ADD 1 TO WSS-NUM-TCOM IN WSS-VARI                         
           ELSE                                                         
           IF RCC-COD-CPTO IN RCC = 'GAS'                               
              ADD 1 TO WSS-NUM-CGAS-V(WSS-F)                            
              ADD 1 TO WSS-NUM-TGAS IN WSS-VARI                         
           ELSE                                                         
           IF RCC-COD-CPTO IN RCC = 'IMP'                               
              ADD 1 TO WSS-NUM-CIMP-V(WSS-F)                            
              ADD 1 TO WSS-NUM-TIMP IN WSS-VARI                         
           ELSE                                                         
              ADD 1 TO WSS-NUM-COTR-V(WSS-F)                            
              ADD 1 TO WSS-NUM-TOTR IN WSS-VARI.                        
       FIN-CRG-VEC-CCB.                                                 
           EXIT.                                                        
                                                                        
       CLR-CCB-VEC SECTION.                                             
       INI-CLR-CCB-VEC.                                                 
           MOVE SPACES TO WSS-COD-BANC-V(WSS-F).                        
           MOVE ZEROES TO WSS-NUM-CINT-V(WSS-F).                        
           MOVE ZEROES TO WSS-NUM-CSEG-V(WSS-F).                        
           MOVE ZEROES TO WSS-NUM-CCOM-V(WSS-F).                        
           MOVE ZEROES TO WSS-NUM-CGAS-V(WSS-F).                        
           MOVE ZEROES TO WSS-NUM-CIMP-V(WSS-F).                        
           MOVE ZEROES TO WSS-NUM-COTR-V(WSS-F).                        
           MOVE ZEROES TO WSS-NUM-TBAN-V(WSS-F).                        
       FIN-CLR-CCB-VEC.                                                 
           EXIT.                                                        
                                                                        
                                                                        
       PUT-TAB SECTION.                                                 
       INI-PUT-TAB.                                                     
           MOVE CCB-NOM-TSCCB TO QUE-COLA.                              
           MOVE QUE-DEL       TO QUE-CMND.                              
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE CCB-NOM-TSCCB-MAS TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE 1          TO SCR-NUM-PANT.                             
           MOVE LOW-VALUES TO WSS-COD-BANC-IDX.                         
      *                                                                 
           MOVE 1       TO FRM-IFLD.                                    
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.                        
                                                                        
           MOVE 1 TO WSS-F IN WSS-VARI.                                 
       LUP-PUT-TAB.                                                     
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI                  
              GO TO FIN-PUT-TAB.                                        
           IF WSS-COD-BANC-V(WSS-F) > SPACES                            
              PERFORM PUT-TAB-CCB                                       
              PERFORM PUT-CCB-QUE                                       
              PERFORM SET-FRM-IFLD                                      
           ELSE                                                         
           IF FRM-IFLD = 1 AND WSS-F = 1                                
              GO TO MSG-PUT-TAB                                         
           ELSE                                                         
              MOVE '00' TO FIO-STAT                                     
              GO TO FIN-PUT-TAB.                                        
                                                                        
           ADD 1 TO WSS-F IN WSS-VARI.                                  
           GO TO LUP-PUT-TAB.                                           
       MSG-PUT-TAB.                                                     
           MOVE 'TAB'        TO MSG-COD-SIST.                           
           MOVE 'CRC    NEX' TO MSG-COD-MENS.                           
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MENS.                               
           MOVE '00'         TO FIO-STAT.                               
      *    MOVE 'S'          TO SCR-IERR.                               
       FIN-PUT-TAB.                                                     
           MOVE 1                  TO QUE-NITM.                         
           MOVE CCB-LEN-TSCCB-MAS  TO QUE-LITM.                         
           MOVE CCB-NOM-TSCCB-MAS  TO QUE-COLA.                         
                                                                        
           IF WSS-NITM > WSS-NITMMAX                                    
              MOVE 'S'           TO CCB-MAS-IND-HMAS                    
           ELSE                                                         
              MOVE 'N'           TO CCB-MAS-IND-HMAS.                   
                                                                        
           MOVE WSS-NITM         TO WSS-NUM-ICUR.                       
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.                   
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.                   
           MOVE WSS-NUM-ICUR-ALF TO CCB-MAS-NUM-TOCC.                   
           MOVE CCB-MAS-ITEM     TO QUE-ITEM.                           
           MOVE QUE-PUT          TO QUE-CMND.                           
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           IF WSS-NITM > WSS-IMAX                                       
               MOVE 'TAB'        TO MSG-COD-SIST                        
               MOVE 'CCB   DMAS' TO MSG-COD-MENS                        
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN2.                           
                                                                        
      *TOTALES                                                          
           MOVE WSS-NUM-TINT IN WSS-VARI TO FRM-NUM-TINT IN CCB-FLD.    
           MOVE WSS-NUM-TSEG IN WSS-VARI TO FRM-NUM-TSEG IN CCB-FLD.    
           MOVE WSS-NUM-TCOM IN WSS-VARI TO FRM-NUM-TCOM IN CCB-FLD.    
           MOVE WSS-NUM-TGAS IN WSS-VARI TO FRM-NUM-TGAS IN CCB-FLD.    
           MOVE WSS-NUM-TIMP IN WSS-VARI TO FRM-NUM-TIMP IN CCB-FLD.    
           MOVE WSS-NUM-TOTR IN WSS-VARI TO FRM-NUM-TOTR IN CCB-FLD.    
           MOVE WSS-NUM-TOTA IN WSS-VARI TO FRM-NUM-TOTA IN CCB-FLD.    
       EXT-PUT-TAB.                                                     
           EXIT.                                                        
                                                                        
       SET-FRM-IFLD SECTION.                                            
       INI-SET-FRM-IFLD.                                                
           IF SCR-OFRM-SRV                                              
                MOVE 1 TO FRM-IFLD                                      
           ELSE                                                         
                ADD 1 TO FRM-IFLD.                                      
       FIN-SET-FRM-IFLD.                                                
           EXIT.                                                        
      *--------------------------------------------                     
       SCRL-TAB-CCB SECTION.                                            
       INI-SCRL-TAB-CCB.                                                
           MOVE SPACES TO MSG-COD-MENS.                                 
                                                                        
           IF FRM-FFLD = FRM-FFLD-PF6                                   
               PERFORM PUT-TAB-ASC                                      
           ELSE                                                         
               PERFORM PUT-TAB-DSC.                                     
           IF MSG-COD-MENS > SPACES                                     
               MOVE 'TAB' TO MSG-COD-SIST                               
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN2.                           
       FIN-SCRL-TAB-CCB.                                                
           EXIT.                                                        
                                                                        
       PUT-TAB-CCB SECTION.                                             
       INI-PUT-TAB-CCB.                                                 
           PERFORM CLR-WSS.                                             
           MOVE WSS-COD-BANC-V(WSS-F) TO WSS-COD-BANC IN WSS-VARI.      
           MOVE WSS-NUM-CINT-V(WSS-F) TO WSS-NUM-CINT IN WSS-VARI.      
           MOVE WSS-NUM-CSEG-V(WSS-F) TO WSS-NUM-CSEG IN WSS-VARI.      
           MOVE WSS-NUM-CCOM-V(WSS-F) TO WSS-NUM-CCOM IN WSS-VARI.      
           MOVE WSS-NUM-CGAS-V(WSS-F) TO WSS-NUM-CGAS IN WSS-VARI.      
           MOVE WSS-NUM-CIMP-V(WSS-F) TO WSS-NUM-CIMP IN WSS-VARI.      
           MOVE WSS-NUM-COTR-V(WSS-F) TO WSS-NUM-COTR IN WSS-VARI.      
           MOVE WSS-NUM-TBAN-V(WSS-F) TO WSS-NUM-TBAN IN WSS-VARI.      
           IF WSS-COD-BANC IN WSS-VARI = 'SIN'                          
              MOVE 'SIN BANCA'              TO WSS-GLS-NBAN IN WSS-VARI 
           ELSE                                                         
              MOVE 'TAB'                    TO TAB-COD-SIST             
              MOVE 'BAN'                    TO TAB-COD-TTAB IN TAB      
              MOVE WSS-COD-BANC IN WSS-VARI TO TAB-COD-CTAB IN TAB      
              PERFORM BUS-TAB                                           
              MOVE TAB-GLS-DESC             TO WSS-GLS-NBAN IN WSS-VARI.
                                                                        
           IF FRM-IFLD > WSS-IMAX                                       
              GO TO FIN-PUT-TAB-CCB.                                    
                                                                        
           MOVE WSS-COD-BANC IN WSS-VARI TO                             
                FRM-COD-BANC IN CCB-FLD(FRM-IFLD).                      
           MOVE WSS-NUM-CINT IN WSS-VARI TO                             
                FRM-NUM-CINT IN CCB-FLD(FRM-IFLD).                      
           MOVE WSS-NUM-CSEG IN WSS-VARI TO                             
                FRM-NUM-CSEG IN CCB-FLD(FRM-IFLD).                      
           MOVE WSS-NUM-CCOM IN WSS-VARI TO                             
                FRM-NUM-CCOM IN CCB-FLD(FRM-IFLD).                      
           MOVE WSS-NUM-CGAS IN WSS-VARI TO                             
                FRM-NUM-CGAS IN CCB-FLD(FRM-IFLD).                      
           MOVE WSS-NUM-CIMP IN WSS-VARI TO                             
                FRM-NUM-CIMP IN CCB-FLD(FRM-IFLD).                      
           MOVE WSS-NUM-COTR IN WSS-VARI TO                             
                FRM-NUM-COTR IN CCB-FLD(FRM-IFLD).                      
           MOVE WSS-NUM-TBAN IN WSS-VARI TO                             
                FRM-NUM-TBAN IN CCB-FLD(FRM-IFLD).                      
                                                                        
           MOVE WSS-GLS-NBAN IN WSS-VARI TO                             
                FRM-GLS-NBAN IN CCB-FLD(FRM-IFLD).                      
       FIN-PUT-TAB-CCB.                                                 
           EXIT.                                                        
      *----------------------------------------------------             
       PUT-CCB-QUE SECTION.                                             
       INI-PUT-CCB-QUE.                                                 
           ADD 1                  TO WSS-NITM.                          
           MOVE CCB-NOM-TSCCB     TO QUE-COLA.                          
           MOVE CCB-LEN-TSCCB     TO QUE-LITM.                          
           MOVE WSS-NITM          TO QUE-NITM.                          
           MOVE SPACES            TO QUE-ITEM.                          
           MOVE WSS-COD-BANC-IDX  TO QUE-ITEM                           
           MOVE QUE-PUT           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
       FIN-PUT-CCB-QUE.                                                 
           EXIT.                                                        
      *----------------------------------------------------             
                                                                        
       PUT-TAB-ASC SECTION.                                             
       INI-PUT-TAB-ASC.                                                 
           MOVE SPACES TO MSG-COD-MENS.                                 
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.           
           MOVE 0 TO FRM-IFLD.                                          
       LUP-PUT-TAB-ASC.                                                 
           ADD 1              TO QUE-NITM.                              
           MOVE CCB-NOM-TSCCB TO QUE-COLA.                              
           MOVE CCB-LEN-TSCCB TO QUE-LITM.                              
           MOVE QUE-GET       TO QUE-CMND.                              
           PERFORM GNS-PRO-QUE.                                         
           IF QUE-STAT NOT = QUE-STAT-OKS                               
               IF FRM-IFLD = 0                                          
                   MOVE 'CCB   NONXT' TO MSG-COD-MENS                   
                   GO TO FIN-PUT-TAB-ASC                                
               ELSE                                                     
                   GO TO FIN-PUT-TAB-ASC.                               
           ADD 1 TO FRM-IFLD.                                           
           MOVE QUE-ITEM TO FRM-COD-BANC-IDX(FRM-IFLD).                 
           IF FRM-IFLD < WSS-IMAX                                       
               GO TO LUP-PUT-TAB-ASC.                                   
       FIN-PUT-TAB-ASC.                                                 
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX                      
               ADD 1 FRM-IFLD GIVING WSS-I                              
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1            
                                 UNTIL WSS-I > WSS-IMAX.                
           IF FRM-IFLD > 0                                              
               ADD 1 TO SCR-NUM-PANT.                                   
           IF FRM-IFLD = WSS-IMAX                                       
               ADD 1              TO QUE-NITM                           
               MOVE CCB-NOM-TSCCB TO QUE-COLA                           
               MOVE CCB-LEN-TSCCB TO QUE-LITM                           
               MOVE QUE-GET       TO QUE-CMND                           
               PERFORM GNS-PRO-QUE                                      
               IF QUE-STAT = QUE-STAT-OKS                               
                   MOVE 'TAB'        TO MSG-COD-SIST                    
                   MOVE 'CCB   DMAS' TO MSG-COD-MENS.                   
       EXT-PUT-TAB-ASC.                                                 
           EXIT.                                                        
                                                                        
       PUT-TAB-DSC SECTION.                                             
       INI-PUT-TAB-DSC.                                                 
           MOVE SPACES TO MSG-COD-MENS.                                 
           IF SCR-NUM-PANT = 1                                          
               MOVE 'CCB   NOPRV' TO MSG-COD-MENS                       
               GO TO EXT-PUT-TAB-DSC.                                   
                                                                        
           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.                
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.               
           ADD 1 TO QUE-NITM.                                           
           ADD 1 WSS-IMAX GIVING FRM-IFLD.                              
       LUP-PUT-TAB-DSC.                                                 
           SUBTRACT  1 FROM QUE-NITM.                                   
           MOVE CCB-NOM-TSCCB TO QUE-COLA                               
           MOVE CCB-LEN-TSCCB TO QUE-LITM                               
           MOVE QUE-GET       TO QUE-CMND.                              
           PERFORM GNS-PRO-QUE.                                         
           IF QUE-STAT NOT = QUE-STAT-OKS                               
               IF FRM-IFLD > WSS-IMAX                                   
                   MOVE 'CCB   NOPRV' TO MSG-COD-MENS                   
                   GO TO FIN-PUT-TAB-DSC                                
               ELSE                                                     
                   GO TO FIN-PUT-TAB-DSC.                               
           SUBTRACT 1 FROM FRM-IFLD.                                    
           MOVE QUE-ITEM TO FRM-COD-BANC-IDX(FRM-IFLD).                 
           IF FRM-IFLD > 1                                              
               GO TO LUP-PUT-TAB-DSC.                                   
       FIN-PUT-TAB-DSC.                                                 
                                                                        
           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1                  
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I                    
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1           
                                 UNTIL WSS-I < 1.                       
                                                                        
           IF FRM-IFLD NOT > WSS-IMAX                                   
               SUBTRACT 1 FROM SCR-NUM-PANT                             
               MOVE 'TAB'        TO MSG-COD-SIST                        
               MOVE 'CCB   DMAS' TO MSG-COD-MENS.                       
       EXT-PUT-TAB-DSC.                                                 
           EXIT.                                                        
                                                                        
      *------------------------------------------------------           
       CLR-FRM SECTION.                                                 
       INI-CLR-FRM.                                                     
           MOVE SPACES TO FRM-COD-BANC     IN CCB-FLD(WSS-I).           
           MOVE SPACES TO FRM-GLS-NBAN     IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-CINT     IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-CSEG     IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-CCOM     IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-CGAS     IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-CIMP     IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-COTR     IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-TBAN     IN CCB-FLD(WSS-I).           
                                                                        
           MOVE 'P'    TO FRM-NUM-CGAS-ATR IN CCB-FLD(WSS-I).           
           MOVE 'P'    TO FRM-NUM-CSEG-ATR IN CCB-FLD(WSS-I).           
           MOVE 'P'    TO FRM-NUM-COTR-ATR IN CCB-FLD(WSS-I).           
           MOVE 'P'    TO FRM-NUM-CINT-ATR IN CCB-FLD(WSS-I).           
           MOVE 'P'    TO FRM-GLS-NBAN-ATR IN CCB-FLD(WSS-I).           
           MOVE 'P'    TO FRM-COD-BANC-ATR IN CCB-FLD(WSS-I).           
           MOVE 'P'    TO FRM-NUM-CIMP-ATR IN CCB-FLD(WSS-I).           
           MOVE 'P'    TO FRM-NUM-CCOM-ATR IN CCB-FLD(WSS-I).           
           MOVE 'P'    TO FRM-NUM-TBAN-ATR IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-CGAS-LEN IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-CCOM-LEN IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-COTR-LEN IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-CSEG-LEN IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-CINT-LEN IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-GLS-NBAN-LEN IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-COD-BANC-LEN IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-CIMP-LEN IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-TBAN-LEN IN CCB-FLD(WSS-I).           
       FIN-CLR-FRM.                                                     
           EXIT.                                                        
                                                                        
       CLR-WSS SECTION.                                                 
       INI-CLR-WSS.                                                     
           MOVE SPACES TO WSS-COD-BANC IN WSS-VARI.                     
           MOVE SPACES TO WSS-GLS-NBAN IN WSS-VARI.                     
           MOVE ZEROES TO WSS-NUM-CINT IN WSS-VARI.                     
           MOVE ZEROES TO WSS-NUM-CSEG IN WSS-VARI.                     
           MOVE ZEROES TO WSS-NUM-CCOM IN WSS-VARI.                     
           MOVE ZEROES TO WSS-NUM-CGAS IN WSS-VARI.                     
           MOVE ZEROES TO WSS-NUM-CIMP IN WSS-VARI.                     
           MOVE ZEROES TO WSS-NUM-COTR IN WSS-VARI.                     
           MOVE ZEROES TO WSS-NUM-TBAN IN WSS-VARI.                     
                                                                        
           MOVE 'P'    TO WSS-NUM-CGAS-ATR.                             
           MOVE 'P'    TO WSS-NUM-CSEG-ATR.                             
           MOVE 'P'    TO WSS-NUM-COTR-ATR.                             
