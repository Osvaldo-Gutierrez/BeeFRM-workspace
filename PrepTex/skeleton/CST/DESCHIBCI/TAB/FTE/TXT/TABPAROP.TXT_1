*%! Codigo adicional
*% IF WSS     
     *<<<< WSS
       COPY GNSWV3CR.                                                   
       COPY GNSWG3CF.                                                   
       COPY GNSWGTSK.                                                   
       COPY GNSWGQUE.                                                   
      *>>>>
*% END
*% IF WSS  AND  PGM_PQ 
      *<<<< WSS
       77  WSS-IDN-TABPQROP-SKL            PIC X(0010)                  
            VALUE 'V:SKL V07+'.                                         
       77  WSS-IDN-TABPQROP-CDA            PIC X(0040)                  
            VALUE 'V:TABPQROP {vsnpgm}'.                                
      *>>>>
*% END    
*% IF WSS  AND  PGM_PU 
      *<<<< WSS
       77  WSS-IDN-TABPUROP-SKL            PIC X(0010)                  
            VALUE 'V:SKL V06+'.                                         
       77  WSS-IDN-TABPUROP-CDA            PIC X(0040)                  
            VALUE 'V:TABPUROP {vsnpgm}'.                                
      *>>>>
*% END  
*% IF INI_IBM
      *<<<< INI_IBM
           PERFORM GNS-ERR-QUE.                                         
           PERFORM GET-TSK-TERM.                                        
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
           IF FIO-STAT = FIO-STAT-NEX                                   
               MOVE 'S' TO SCR-IERR.                                    
      *>>>>
*% END
*% IF INI_SCR_PUTA
      *<<<< INI_SCR_PUTA
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR               
               MOVE 'S' TO SCR-IERR.                                    
      *>>>>
*% END
*% IF ERR_SCR_GKEY
      *<<<< ERR_SCR_GKEY
      *PTC-INI                                                          
           MOVE 'S' TO SCR-IERR.                                        
      *PTC-FIN                                                          
      *>>>>
*% END
*% IF ERR_SCR_GIKY
      *<<<< ERR_SCR_GIKY
      *PTC-INI                                                          
           MOVE 'S' TO SCR-IERR.                                        
      *PTC-FIN                                                          
      *>>>>
*% END
*% IF INI_HBL_IKY
      *<<<< INI_HBL_IKY
      *PTC-INI                                                          
           IF NOT SCR-OFRM-SRV                                          
      *PTC-FIN                                                          
      *>>>>
*% END
*% IF INI_HBL_KEY
      *<<<< INI_HBL_KEY
      *PTC-INI                                                          
           IF NOT SCR-OFRM-SRV                                          
      *PTC-FIN                                                          
      *>>>>
*% END
*% IF INI_SCR_TRL  AND  PGM_PQ
      *<<<< INI_SCR_TRL
      *PTC-INI                                                          
           IF SCR-STAT NOT = SCR-STPP                                   
               MOVE 'S' TO SCR-IERR.                                    
      *PTC-FIN                                                          
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBG3CF.
       COPY GNSBEGLS.                                                   
       COPY GNSBGQUE.                                                   
       COPY GNSBGTSK.                                                   
       COPY GNSBGVSM.                                                   
      *>>>>
*% END
*% IF INI_CMN_ING
      *<<<< INI_CMN_ING
           MOVE ZEROES  TO ROP-SGV-VAL1 IN ROP.                         
           MOVE ZEROES  TO ROP-SGV-VAL2 IN ROP.                         
      *>>>>
*% END
*% IF ERR_SCR_GFLD
      *<<<< ERR_SCR_GFLD
      *PTC-INI                                                          
           MOVE 'S' TO SCR-IERR.                                        
      *PTC-FIN                                                          
      *>>>>
*% END
*% IF INI_SCR_TRL  AND  PGM_PU
      *<<<< INI_SCR_TRL
      *PTC-INI                                                          
           IF SCR-STAT NOT = SCR-STPP                                   
               MOVE FRM-SUAR-MAL TO FRM-SUAR.                           
      *PTC-FIN                                                          
      *>>>>
*% END
*% IF INI_PRO_CONF  AND  PGM_PU
      *<<<< INI_PRO_CONF
      *PTC-INI                                                          
           IF SCR-OFRM-SRV AND                                          
              SCR-CONF NOT > SPACES                                     
                MOVE SPACES TO FRM-MENS.                                
      *PTC-FIN                                                          
      *>>>>
*% END
