*%! Codigo adicional
*% NO_INI_REG = TRUE
*% NO_TIENE_TIMESTAMP = TRUE
*% HDR_CST = FALSE
*% IF WSS_DTC
      *<<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-USR-REQA==.
      *>>>>
*% END
*% IF WSS
      *<<<< WSS
       COPY GNSWGHOY.
       COPY TABBRUSR.
       01  WSS-STAT.
              05 PGM-STAT-RES           VALUE '23'      PIC X(02).
                 88 PGM-STAT-RES-OKS    VALUE '00'.
                 88 PGM-STAT-RES-NUL    VALUE '  '.
      *>>>>
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-SISG NOT > SPACES
               MOVE SCR-SIST TO SCR-SISG.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
           MOVE PGM-STAT-RES TO PGM-STAT-TAB.
      *>>>>
*% END
*% IF FND_KEY OR CMN_ING OR INI_CMN_MOD OR INI_CMN_ACC
      *<<<< FND_KEY OR CMN_ING OR INI_CMN_MOD OR INI_CMN_ACC
           MOVE SCR-SISG TO FIO-SIST.
      *>>>>
*% END
*% IF INI_CMN_ELI OR INI_CMN_PGM
      *<<<< INI_CMN_ELI OR INI_CMN_PGM
           MOVE SCR-SISG TO FIO-SIST.
      *>>>>
*% END
*% IF ABT_CMN_ACC
      *<<<< ABT_CMN_ACC
               MOVE 'RESPONSABLE NO FUE ASIGNADO' TO FRM-MENS
      *>>>>
*% END
*% IF CMN_ACC
      *<<<< CMN_ACC
           PERFORM GET-FHOY.
           MOVE HOY-STP-ITRN TO TAB-STP-ITRN IN TAB.
           MOVE USR-COD-USER IN RES-FLD TO TAB-COD-ATRN IN TAB.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM TAB-FIO-RES.
           IF FIO-STAT-OKS
               MOVE 'NUEVO RESPONSABLE HA SIDO ASIGNADO'  TO FRM-MENS
           ELSE
               MOVE 'ERROR : RESPONSABLE NO FUE ASIGNADO' TO FRM-MENS.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY TABBBUSR.
       COPY TABBVUSR.
       COPY TABBFUSR.
       COPY GNSBGHOY.
      *>>>>
*% END
