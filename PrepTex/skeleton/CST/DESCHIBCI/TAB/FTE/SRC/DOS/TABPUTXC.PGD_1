      *========================
       PROGRAM-ID.   TABPUTXC.
       AUTHOR.       CONSIST.
       DATE-WRITTEN. 10-Aug-10 05:49 PM.

      * NOMBRE PROGRAMA                   TABPUTXC
      * VERSION PGM                       3.1
      * VERSION SKL                       08.07.21
      * OPCION                            UPD
      * ARCHIVO BASE                      TABTXC
      * PANTALLA                          TABTXC
      * INP/OUT                           DATACOM
      * TAMANO COMMAREA                   250


       ENVIRONMENT DIVISION.
      *=====================
       CONFIGURATION SECTION.
      *----------------------
       SPECIAL-NAMES.
               DECIMAL-POINT IS COMMA.

       DATA DIVISION.
      *==============

       WORKING-STORAGE SECTION.
      *------------------------
      * COPY GNSWGSCR.
       COPY GNSWGCVT.
       77  WSS-TAG-TABPUTXC-INI PIC X(125) VALUE                        
           '<IDNPGM NOM=$TABPUTXC$ SKL=$08.07.21$ VSN=${vsnpu}$        
      -    'GEN=$10-Aug-10 05:49 PM$ FNC=${fncpu}$/>'.    
       COPY GNSWCSCR.
       COPY GNSWVSCR.
       COPY TABWITXC.
       COPY TABBRTXC.


       COPY GNSWVIDD.
       COPY GNSWGRQA.
       COPY GNSWGUIB.
       COPY GNSWGELS.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-TXC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-TAB-REQA==.
       COPY GNSBRTAB.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSG-REQA==.
       COPY GNSBRMSG.
       COPY GNSWCFIO.
       COPY GNSWVFIO.
       COPY GNSWGFRM.
       COPY GNSWVNUM.
       COPY GNSWCNUM.
       COPY GNSWGFEC.
       COPY GNSWGHOY.
       COPY GNSWGSYS.

      *<<<< WSS
       01  WSS-VARI.                                                    
           03 WSS-HVAL      VALUE 999999999 PIC 9(09).                  
           03 WSS-MTRA      VALUE 12        PIC 9(02).                  
           03 WSS-DATA.                                                 
              05 WSS-PLZO                   PIC X(04).                  
              05 WSS-VCOC                   PIC X(06).                  
              05 WSS-BASE                   PIC X(01).                  
              05 WSS-NTRA                   PIC X(01).                  
           03 WSS-FAUX.                                                 
              05 WSS-SAUX                   PIC 9(02).                  
              05 WSS-AAUX                   PIC 9(02).                  
              05 WSS-MAUX                   PIC 9(02).                  
              05 WSS-DAUX                   PIC 9(02).                  
           03 WSS-KAUX                      PIC X(27).                  
           03 WSS-IFIN                      PIC X(01).                  
           03 WSS-ITAS                      PIC 9(02).                  
           03 WSS-ANTI                      PIC X(01).                  
           03 WSS-I                         PIC 9(02).                  
       01  TXC-VARI.                                                    
           03  TXC-CMSG                     PIC X(12).                  
           03  TXC-MENS                     PIC X(80).                  
  *>>>>
       01  PGM-VARI.
           03 PGM-ICLR                  VALUE ' '       PIC X(01).
           03 PGM-LIMPIAR-TODO          VALUE ' '       PIC X(01).
           03 PGM-LIMPIAR-LLAVES        VALUE 'K'       PIC X(01).
           03 PGM-LIMPIAR-CAMPOS        VALUE 'F'       PIC X(01).
           03 PGM-NO-LIMPIAR-NADA       VALUE 'N'       PIC X(01).
           03 PGM-PROG                                  PIC X(08).
           03 PGM-NTRN                                  PIC X(04).
           03 PGM-STAT.
              05 PGM-STAT-TXC.
                 88 PGM-STAT-TXC-NUL    VALUE '  '.
                 07 PGM-STT1-TXC        VALUE '2' PIC X(01).
                    88 PGM-STAT-TXC-OKS VALUE '0'.
                 07 PGM-STT2-TXC        VALUE '3' PIC X(01).
           03 PGM-SRNF                  VALUE '23'      PIC X(02).
           03 PGM-SOKS                  VALUE '00'      PIC X(02).
           03 PGM-SNUL                  VALUE '  '      PIC X(02).
           03 PGM-SUAR                  VALUE SPACES    PIC X(15).

       LINKAGE SECTION.
      *----------------
       01  DFHCOMMAREA.
           02 FILLER                    PIC X(250).
       COPY GNSLGFIO.

       PROCEDURE DIVISION.
      *===================

       MAIN SECTION.
       INI-MAIN.
           PERFORM INI.
       LUP-MAIN.
           IF SCR-INST = SCR-INST-HCM
                 PERFORM SCR-COM.
           IF SCR-CCPP = 'MOD'
                 PERFORM SCR-MOD
           ELSE
           IF SCR-CCPP = 'ING'
                 PERFORM SCR-ING
           ELSE
           IF SCR-CCPP = 'ACC'
                 PERFORM SCR-ACC
           ELSE
           IF SCR-CCPP = 'ELI'
                 PERFORM SCR-ELI
           ELSE
           IF SCR-CCPP = 'ACT'
                 PERFORM SCR-ACT
           ELSE
           IF SCR-CCPP = 'PGM'
                 PERFORM SCR-PGM
           ELSE
                 MOVE SCR-INST-HCM TO SCR-INST.
           GO TO LUP-MAIN.
       FIN-MAIN.
           EXIT.

       INI SECTION.
       INI-INI.
       COPY GNSBGEIB.
       COPY GNSBGEDB.
           MOVE LOW-VALUES TO TXC-FLD.
           MOVE LOW-VALUES TO TXC-EDT.
           PERFORM GNS-PRO-RCK.
           PERFORM GNS-BUS-VCA.
           PERFORM GNS-HDL-SYS.
           PERFORM GNS-HDL-FRM.
           MOVE 'TABPUTXC' TO FIO-PROG.
           PERFORM GNS-BUS-IDD.
       FIN-INI.
           EXIT.

       COPY GNSBHSYS.
       COPY GNSBHFRM.
       COPY GNSBPRCK.
       COPY GNSBBVCA.
       COPY GNSBBIDD.

       SCR-COM SECTION.
       INI-SCR-COM.
           PERFORM STP-EXIT.
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-CLRA-TXC.
           PERFORM EDT-KEY-TXC.
           PERFORM EDT-FLD-TXC.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-HCM TO SCR-INST.
           MOVE SCR-CMNP     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
           PERFORM GNS-PRO-SYS.
       XCT-ERR.
           MOVE 'XCTERR    ' TO MSG-COD-MENS.
           MOVE 'GNS'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO SYS-MEN1.
           MOVE SYS-PROG     TO SYS-MEN2.
           MOVE SYS-ABOR     TO SYS-CMND.
           PERFORM GNS-PRO-SYS.
       COPY GNSBGGBK.
       FIN-SCR-COM.
           EXIT.

       SCR-MOD SECTION.
       INI-SCR-MOD.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
               PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
               PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8
               PERFORM XCT-PRGP.
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
               GO TO MSG-SCR-MOD.
       MSG-SCR-MOD.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'MODNOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-MOD
               IF FIO-STAT-OKS
                   IF FRM-MEN1 NOT > SPACES
                       MOVE 'MODYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MEN1
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-MOD.
           EXIT.

       SCR-ING SECTION.
       INI-SCR-ING.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-IKY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
       MSG-SCR-ING.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'INGNOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-ING
               IF FIO-STAT-OKS
                   IF FRM-MEN1 NOT > SPACES
                       MOVE 'INGYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MEN1
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ING.
           EXIT.

       SCR-ACC SECTION.
       INI-SCR-ACC.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8 OR
                         FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM XCT-PRGP.
           MOVE SPACES TO FRM-MENS.
           IF SCR-STAT NOT = SCR-STAT-CON
               PERFORM CMN-ACC.
           PERFORM SCR-MSSG.
       FIN-SCR-ACC.
           EXIT.

       SCR-ELI SECTION.
       INI-SCR-ELI.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8 OR
                         FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM XCT-PRGP.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'ELINOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-ELI
               IF FIO-STAT-OKS
                   IF FRM-MEN1 NOT > SPACES
                       MOVE 'ELIYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MEN1
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ELI.
           EXIT.

       SCR-ACT SECTION.
       INI-SCR-ACT.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF ( FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8  OR
                           FRM-FFLD-PF5 OR FRM-FFLD-PF6 ) AND
              SCR-ACTU = SCR-ACTU-MOD
               PERFORM XCT-PRGP.
           IF SCR-ACTU = SCR-ACTU-MOD
                  PERFORM MSG-SCR-MOD
           ELSE
                  PERFORM MSG-SCR-ING.
       FIN-SCR-ACT.
           EXIT.

       SCR-PGM SECTION.
       INI-SCR-PGM.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
       TRL-SCR-PGM.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8 OR
                         FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM XCT-PRGP.
           MOVE SPACES TO FRM-MENS.
           IF SCR-STAT NOT = SCR-STAT-CON
               PERFORM CMN-PGM.
           PERFORM SCR-MSSG.
       FIN-SCR-PGM.
           EXIT.

       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               PERFORM SCR-CLRA-TXC
               PERFORM EDT-KEY-TXC
               PERFORM EDT-FLD-TXC
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-TXC
               PERFORM EDT-KEY-TXC
           ELSE
               PERFORM CLR-FLD-TXC
               PERFORM EDT-FLD-TXC.
           PERFORM STP-EXIT.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

       XCT-PRGP SECTION.
       INI-XCT-PRGP.
           MOVE SCR-INST-BRW TO SCR-INST.
           PERFORM STP-EXIT.
           MOVE FRM-FFLD   TO SCR-FFLD.
      *
           MOVE SCR-PROG     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
           PERFORM GNS-PRO-SYS.
           PERFORM XCT-ERR.
       FIN-XCT-PRGP.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO TXC-FLD.
           MOVE LOW-VALUES TO TXC-EDT.
       FIN-SCR-RST.
           EXIT.

       GET-FLD SECTION.
       INI-GET-FLD.
           PERFORM SCR-GFLD.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-FLD.
           EXIT.

       CMN-MOD SECTION.
       INI-CMN-MOD.
      *<<<<<INI_CMN_MOD
           MOVE 'U' TO WSS-ANTI.                                        
           PERFORM PROCESO-ELI.                                         
           IF TXC-CMSG > SPACES                                         
               MOVE TXC-MENS TO FRM-MENS                                
               MOVE FIO-BCK-OUT TO FIO-CMND                             
               PERFORM TAB-FIO-TXC                                      
               GO TO FIN-CMN-MOD.                                       
           PERFORM PROCESO-ING.                                         
           IF TXC-CMSG > SPACES                                         
               MOVE TXC-MENS TO FRM-MENS                                
               MOVE FIO-BCK-OUT TO FIO-CMND                             
               PERFORM TAB-FIO-TXC                                      
           ELSE                                                         
               MOVE '00' TO FIO-STAT.                                   
           GO TO FIN-CMN-MOD.                                           
      *>>>>>
           PERFORM KEY-ALL-TXC.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM TAB-FIO-TXC.
           IF FIO-STAT-OKS AND
              TXC-STP-ITRN IN TXC NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM TAB-FIO-TXC
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM TAB-FIO-TXC
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-MOD.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-MOD.
           PERFORM RET-STT-TXC.
           PERFORM RET-ALL-TXC.
           PERFORM RET-HDR-TXC.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM TAB-FIO-TXC.
       FIN-CMN-MOD.
           EXIT.

       CMN-ING SECTION.
       INI-CMN-ING.
           MOVE SPACES TO TXC.
           PERFORM RET-STT-TXC.
           PERFORM RET-ALL-TXC.
           PERFORM RET-HDR-TXC.
      *<<<<< CMN_ING
           PERFORM PROCESO-ING.                                         
           IF TXC-CMSG > SPACES                                         
               MOVE TXC-MENS TO FRM-MENS                                
               MOVE FIO-BCK-OUT TO FIO-CMND                             
               PERFORM TAB-FIO-TXC                                      
           ELSE                                                         
               MOVE '00' TO FIO-STAT.                                   
           GO TO FIN-CMN-ING.                                           
      *>>>>>
           MOVE FIO-PUT TO FIO-CMND.
           PERFORM TAB-FIO-TXC.
       FIN-CMN-ING.
           EXIT.

       CMN-ACC SECTION.
       INI-CMN-ACC.
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'ACCION NO REALIZADA' TO FRM-MENS
               GO TO FIN-CMN-ACC.
      *<<<<< INI_CMN_ACC
           IF SCR-CMND NOT = 'VRF'                                      
               GO TO FIN-CMN-ACC.                                       
           PERFORM PROCESO-VRF.                                         
           IF TXC-CMSG > SPACES                                         
               MOVE TXC-MENS TO FRM-MENS                                
               MOVE FIO-BCK-OUT TO FIO-CMND                             
               PERFORM TAB-FIO-TXC                                      
           ELSE                                                         
               MOVE 'TAB'          TO MSG-COD-SIST                      
               MOVE 'TXC    VRFOK' TO MSG-COD-MENS                      
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS.                           
           GO TO FIN-CMN-ACC.                                           
      *>>>>>
           PERFORM KEY-ALL-TXC.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM TAB-FIO-TXC.
           IF FIO-STAT-OKS AND
              TXC-STP-ITRN IN TXC NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM TAB-FIO-TXC
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM TAB-FIO-TXC
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-ACC.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ACC.
           PERFORM RET-ALL-TXC.
       FIN-CMN-ACC.
           EXIT.

       CMN-ELI SECTION.
       INI-CMN-ELI.
      *<<<<< INI_CMN_ELI
           MOVE SPACES TO WSS-ANTI.                                     
           PERFORM PROCESO-ELI.                                         
           IF TXC-CMSG > SPACES                                         
               MOVE TXC-MENS TO FRM-MENS                                
               MOVE FIO-BCK-OUT TO FIO-CMND                             
               PERFORM TAB-FIO-TXC                                      
           ELSE                                                         
               MOVE '00' TO FIO-STAT.                                   
           GO TO FIN-CMN-ELI.                                           
      *>>>>>
           PERFORM KEY-ALL-TXC.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM TAB-FIO-TXC.
           IF FIO-STAT-OKS AND
              TXC-STP-ITRN IN TXC NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM TAB-FIO-TXC
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM TAB-FIO-TXC
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-ELI.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ELI.
           MOVE FIO-DEL TO FIO-CMND.
           PERFORM TAB-FIO-TXC.
       FIN-CMN-ELI.
           EXIT.

       CMN-PGM SECTION.
       INI-CMN-PGM.
           PERFORM RET-ALL-TXC.
       FIN-CMN-PGM.
           EXIT.

       STP-EXIT SECTION.
       INI-STP-EXIT.
           MOVE SCR-PROG TO PGM-PROG.
           MOVE SCR-NTRN TO PGM-NTRN.

           MOVE SCR-PROV TO SCR-PROG.
           MOVE SCR-NTRV TO SCR-NTRN.

           MOVE PGM-PROG TO SCR-PROV
           MOVE PGM-NTRN TO SCR-NTRV.
       FIN-STP-EXIT.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO SYS-NTRN.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE TXC-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO TXC-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GFLD SECTION.
       INI-SCR-GFLD.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-FLD-TXC
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO TXC-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GFLD.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-TXC.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-TXC
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           PERFORM VAL-NUM-FLD-TXC.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-TXC
               PERFORM EDT-FLD-TXC
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           PERFORM VAL-MKY-TXC.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GFLD.
           PERFORM VAL-FLD-TXC.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GFLD.
           PERFORM VAL-CON-FLD-TXC.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GFLD.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-FLD-TXC.
       ERR-SCR-GFLD.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-TXC.
           PERFORM EDT-FLD-TXC.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GFLD.
           MOVE SCR-STAT-MOD TO SCR-STAT.

       GET-ACC SECTION.
       INI-GET-ACC.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO TXC-GLS-MEN2
               MOVE SCR-PRMT     TO TXC-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO TXC-FLD.
           IF NOT ( ( FRM-FFLD = FRM-FFLD-PF12 OR FRM-FFLD-PF3 ) OR
                      SCR-CONF > SPACES )
               PERFORM VAL-NUM-KEY-TXC
               PERFORM VAL-NUM-FLD-TXC.
       FIN-GET-ACC.
           MOVE SCR-STAT-SON TO SCR-STAT.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO TXC-GLS-MEN2
               MOVE SCR-PRMT     TO TXC-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO TXC-FLD.
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO TXC-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
           IF SCR-DESDE-CMN
               MOVE TXC-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO TXC-FLD
      * FLB  7-NOV-1994
               PERFORM CLR-KEY-TXC

               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO TXC-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO TXC-GLS-MENS.
           PERFORM PRO-FLD-TXC.
           PERFORM PRO-MKY-TXC.
           PERFORM UNP-IKY-TXC.
           PERFORM FST-KEY-TXC.
      *<<<<  FIN_HBL_IKY
           IF SCR-CMND = 'ITR' OR 'CTX' OR 'VRF'                        
               MOVE FRM-CPIM-AEY TO TXC-FEC-FTXC-ATR IN TXC-FLD.        
      *>>>>
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
           IF SCR-DESDE-CMN
               MOVE TXC-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO TXC-FLD
               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO TXC-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO TXC-GLS-MENS.
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-FLD-TXC.
           MOVE '1' TO FRM-CPIM-AEY.
           PERFORM UNP-KEY-TXC.
           PERFORM FST-KEY-TXC.
      *<<<< FIN_HBL_KEY
           IF SCR-CMND = 'ITR' OR 'CTX' OR 'VRF'                        
               MOVE FRM-CPIM-AEY TO TXC-FEC-FTXC-ATR IN TXC-FLD.        
      *>>>>
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
           PERFORM UNP-MKY-TXC.
           PERFORM UNP-FLD-TXC.
           PERFORM FST-FLD-TXC.
      *<<<< FIN_HBL_FLD
           IF SCR-CMND = 'CTX'                                          
               PERFORM PROT-TRAMOS VARYING FRM-IFLD FROM 1 BY 1         
                                   UNTIL FRM-IFLD > WSS-MTRA.           
           IF SCR-CMND = 'CTX' OR FRM-IND-NTRA IN TXC-FLD = 'N'         
               MOVE 0  TO FRM-NUM-MSTR-LEN IN TXC-FLD(1)                
               MOVE -1 TO FRM-SGV-VTXC-LEN IN TXC-FLD(1).               
      *>>>>
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO TXC-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-TXC.
           PERFORM PRO-FLD-TXC.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-TXC
               PERFORM EDT-FLD-TXC
               PERFORM SCR-PUTA
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-TXC
               PERFORM EDT-FLD-TXC
               PERFORM SCR-PUTA
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-TXC.
           PERFORM EDT-FLD-TXC.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           IF SCR-INST = SCR-INST-GCF
                  PERFORM SCR-GCNF
                  GO TO FIN-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
           IF SCR-STAT = SCR-STAT-ABT OR SCR-STAT-CON
                  GO TO FIN-DESSCRTRL.
           IF SCR-FFLD-RET OR SCR-FFLD-PF10
                  MOVE SCR-VFLD-YES TO SCR-VFLD
                  PERFORM PRO-CONF.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

       PRO-CONF SECTION.
       INI-PRO-CONF.
           IF FRM-MENS NOT > SPACES
               IF SCR-CONF NOT > SPACES
                   GO TO FIN-PRO-CONF
               ELSE
                   MOVE SCR-CONF TO FRM-MENS
           ELSE
               IF FRM-MEN1 NOT > SPACES
                   MOVE SCR-CONF TO FRM-MEN1.
           PERFORM PRO-KEY-TXC.
           PERFORM PRO-FLD-TXC.
           PERFORM FST-MSG.
           PERFORM SCR-MSSG.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM EDT-KEY-TXC
               PERFORM EDT-FLD-TXC.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GCF TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-PRO-CONF.
           EXIT.

       SCR-GCNF SECTION.
       INI-SCR-GCNF.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3  OR
                               FRM-FFLD-PF4 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO TXC-GLS-MEN2
               MOVE SCR-CONF     TO TXC-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO TXC-FLD.
           MOVE FRM-FFLD TO SCR-FFLD.
           IF FRM-FFLD = FRM-FFLD-PF3
               MOVE SCR-VFLD-NOT TO SCR-VFLD
           ELSE
           IF FRM-FFLD = FRM-FFLD-RET OR FRM-FFLD-PF10 OR FRM-FFLD-PF4
               MOVE SCR-VFLD-YES TO SCR-VFLD
               PERFORM VAL-NUM-KEY-TXC
               PERFORM VAL-NUM-FLD-TXC
           ELSE
           IF FRM-FFLD = FRM-FFLD-PF12
               MOVE SCR-STPP TO SCR-STAT
               IF SCR-STAT = SCR-STAT-MOD
                   PERFORM VAL-NUM-KEY-TXC
                   PERFORM VAL-NUM-FLD-TXC
                   PERFORM SCR-TRL
               ELSE
                   MOVE SCR-VFLD-NOT TO SCR-VFLD.
       FIN-SCR-GCNF.
           EXIT.

      * PGM-EBR-ALL SECTION.
      * INI-PGM-EBR-ALL.
      *     MOVE 'GNS' TO FIO-SIST.
      *     MOVE FIO-EBR-ALL TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
      * FIN-PGM-EBR-ALL.
      *     EXIT.

       RET-STT-TXC SECTION.
       INI-RET-STT-TXC.
           IF SCR-CCPP = 'ING'
               MOVE 'I' TO TXC-MSC-TACC IN TXC
               MOVE 'I' TO TXC-MSC-STAT IN TXC
           ELSE
           IF SCR-CCPP = 'MOD'
               MOVE 'M' TO TXC-MSC-TACC IN TXC
               MOVE 'M' TO TXC-MSC-STAT IN TXC.
       FIN-RET-STT-TXC.
           EXIT.

       RET-HDR-TXC SECTION.
       INI-RET-HDR-TXC.
           PERFORM GET-FHOY.
      *    Fecha Transaccion
           MOVE HOY-FHOY TO TXC-FEC-FTRN IN TXC.
      *    Hora Transaccion
           MOVE HOY-HHOY TO TXC-HRA-HRTR IN TXC.
      *    Codigo Origen transaccion
      *<<<< NOT PGM_VMS
           MOVE SCR-OTRN TO TXC-COD-OTRN IN TXC.
      *    Autor Transaccion
           MOVE SCR-USER TO TXC-COD-ATRN IN TXC.
      *>>>>
       FIN-RET-HDR-TXC.
           EXIT.

       COPY TABBUTXC.
       COPY GNSBVNUM.
       COPY TABBFTXC.
       COPY GNSBFMSG.
       COPY GNSBGSYS.
       COPY GNSBGDTC.
       COPY GNSBGFRM.
       COPY GNSBGMSG.
       COPY GNSBEKEY.
       COPY GNSBIABT.
       COPY GNSBFTAB.
       COPY GNSBGFEC.
       COPY GNSBGHOY.

      *<<<< EOF                                                   
       PROT-TRAMOS SECTION.                                             
       INI-PROT-TRAMOS.                                                 
           MOVE FRM-CPIM-AEY TO FRM-NUM-MSTR-ATR IN TXC-FLD(FRM-IFLD).  
       FIN-PROT-TRAMOS.                                                 
           EXIT.                                                        
       COPY GNSBPFEC.                                                   
       COPY GNSBVCOD.                                                   
       COPY GNSBVTAB.                                                   
       COPY GNSBEGLS.                                                   
      *>>>>      
      *<<<< EOF_PU OR EOF_VAX                                           
       PROCESO-ING SECTION.                                             
       INI-PROCESO-ING.                                                 
           MOVE SPACES TO TXC-CMSG TXC-MENS.                            
           PERFORM CALC-ACUM.                                           
           IF TXC-CMSG > SPACES                                         
               GO TO FIN-PROCESO-ING.                                   
           PERFORM GRABA-TRAMOS-NUEVOS.                                 
           IF TXC-CMSG > SPACES                                         
               GO TO FIN-PROCESO-ING.                                   
           PERFORM GRABA-TRAMOS-ANTIG.                                  
       FIN-PROCESO-ING.                                                 
           IF TXC-CMSG     > SPACES AND                                 
              TXC-MENS NOT > SPACES                                     
               MOVE 'TAB'    TO MSG-COD-SIST                            
               MOVE TXC-CMSG TO MSG-COD-MENS                            
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO TXC-MENS.                           
       EXT-PROCESO-ING.                                                 
           EXIT.                                                        
                                                                        
       CALC-ACUM SECTION.                                               
       INI-CALC-ACUM.                                                   
           MOVE SPACES TO TXC-CMSG TXC-MENS.                            
           MOVE 0 TO FRM-IFLD.                                          
       LUP-CALC-ACUM.                                                   
           ADD 1 TO FRM-IFLD.                                           
           IF FRM-IFLD > WSS-MTRA                                       
               GO TO FIN-CALC-ACUM.                                     
           IF FRM-NUM-MSTR IN TXC-FLD(FRM-IFLD) NOT > ZEROES            
               GO TO FIN-CALC-ACUM.                                     
           PERFORM KEY-ALL-TXC.                                         
           MOVE FRM-NUM-DTXC IN TXC-FLD TO TXC-NUM-DTXC IN TXC.         
           MOVE FRM-NUM-MTXC IN TXC-FLD TO TXC-NUM-MTXC IN TXC.         
           MOVE FRM-NUM-STXC IN TXC-FLD TO TXC-NUM-STXC IN TXC.         
           MOVE FRM-NUM-ATXC IN TXC-FLD TO TXC-NUM-ATXC IN TXC.         
           SUBTRACT FRM-NUM-MSTR IN TXC-FLD(FRM-IFLD) FROM WSS-HVAL     
                                    GIVING TXC-NUM-CMST IN TXC.         
           MOVE FIO-GET-LEQ TO FIO-CMND.                                
           PERFORM TAB-FIO-TXC.                                         
           IF FIO-STAT-OKS AND                                          
              TXC-COD-PLZO IN TXC = TXC-COD-PLZO IN TXC-FLD AND         
              TXC-COD-VCOC IN TXC = TXC-COD-VCOC IN TXC-FLD AND         
              TXC-NUM-DTXC IN TXC = FRM-NUM-DTXC IN TXC-FLD AND         
              TXC-NUM-MTXC IN TXC = FRM-NUM-MTXC IN TXC-FLD AND         
              TXC-NUM-STXC IN TXC = FRM-NUM-STXC IN TXC-FLD AND         
              TXC-NUM-ATXC IN TXC = FRM-NUM-ATXC IN TXC-FLD             
               MOVE TXC-NUM-DTXC IN TXC     TO FEC-DEC1                 
               MOVE TXC-NUM-MTXC IN TXC     TO FEC-MEC1                 
               MOVE TXC-NUM-STXC IN TXC     TO FEC-SEC1                 
               MOVE TXC-NUM-ATXC IN TXC     TO FEC-AEC1                 
               MOVE TXC-NUM-DTXC IN TXC-FLD TO FEC-DEC2                 
               MOVE TXC-NUM-MTXC IN TXC-FLD TO FEC-MEC2                 
               MOVE TXC-NUM-STXC IN TXC-FLD TO FEC-SEC2                 
               MOVE TXC-NUM-ATXC IN TXC-FLD TO FEC-AEC2                 
               MOVE FEC-FORM-FEC TO FEC-FORM                            
               MOVE FEC-DIF-DIA  TO FEC-CMND                            
               PERFORM CAL-FEC                                          
               IF NOT FEC-STAT-OKS                                      
                   MOVE FEC-MENS       TO TXC-MENS                      
                   MOVE 'TXCDIFDIAERR' TO TXC-CMSG                      
                   GO TO FIN-CALC-ACUM                                  
               ELSE                                                     
                   COMPUTE FRM-SGV-ATXC IN TXC-FLD(FRM-IFLD) ROUNDED =  
                   TXC-SGV-ATXC IN TXC +                                
                   TXC-SGV-VTXC * FRM-NUM-BASE IN TXC-FLD *             
                   FEC-NDIA                                             
           ELSE                                                         
               MOVE 0 TO FRM-SGV-ATXC IN TXC-FLD(FRM-IFLD).             
           GO TO LUP-CALC-ACUM.                                         
       FIN-CALC-ACUM.                                                   
           EXIT.                                                        
                                                                        
       GRABA-TRAMOS-NUEVOS SECTION.                                     
       INI-GRABA-TRAMOS-NUEVOS.                                         
      *Grabacion de nuevos tramos ( 'U' )                               
           MOVE 0 TO FRM-IFLD.                                          
       LUP-GRABA-TRAMOS-NUEVOS.                                         
           ADD 1 TO FRM-IFLD.                                           
           IF FRM-IFLD > WSS-MTRA                                       
               GO TO FIN-GRABA-TRAMOS-NUEVOS.                           
      *Si existen desplegados tramos historicos estos no se consideran  
      *en este proceso                                                  
           IF FRM-MSC-ANTI IN TXC-FLD(FRM-IFLD) = 'H'                   
               GO TO LUP-GRABA-TRAMOS-NUEVOS.                           
           IF FRM-NUM-MSTR IN TXC-FLD(FRM-IFLD) NOT > ZEROES            
               GO TO FIN-GRABA-TRAMOS-NUEVOS.                           
           PERFORM KEY-ALL-TXC.                                         
           SUBTRACT FRM-NUM-MSTR IN TXC-FLD(FRM-IFLD) FROM WSS-HVAL     
                                    GIVING TXC-NUM-CMST IN TXC.         
           PERFORM GET-FHOY.                                            
           MOVE HOY-STP-ITRN            TO TXC-STP-ITRN IN TXC.         
           MOVE SCR-USER                TO TXC-COD-ATRN IN TXC.         
           MOVE SCR-OTRN                TO TXC-COD-OTRN IN TXC.         
           MOVE 'I'                     TO TXC-MSC-STAT IN TXC.         
           MOVE 'U'                     TO TXC-MSC-ANTI IN TXC.         
           MOVE FRM-COD-BASE IN TXC-FLD TO TXC-COD-BASE IN TXC.         
           MOVE FRM-NUM-MSTR IN TXC-FLD(FRM-IFLD)                       
                                        TO TXC-NUM-MSTR IN TXC.         
           MOVE FRM-SGV-VTXC IN TXC-FLD(FRM-IFLD)                       
                                        TO TXC-SGV-VTXC IN TXC.         
           MOVE FRM-SGV-ATXC IN TXC-FLD(FRM-IFLD)                       
                                        TO TXC-SGV-ATXC IN TXC.         
           MOVE SPACES                  TO TXC-GLS-DISP IN TXC.         
           MOVE FIO-PUT TO FIO-CMND.                                    
           PERFORM TAB-FIO-TXC.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE FIO-MENS       TO TXC-MENS                          
               MOVE 'TXCPUT ERROR' TO TXC-CMSG                          
               GO TO FIN-GRABA-TRAMOS-NUEVOS.                           
           GO TO LUP-GRABA-TRAMOS-NUEVOS.                               
       FIN-GRABA-TRAMOS-NUEVOS.                                         
           EXIT.                                                        
                                                                        
       GRABA-TRAMOS-ANTIG SECTION.                                      
       INI-GRABA-TRAMOS-ANTIG.                                          
      *Grabacion de tramos historicos ( 'H' )                           
           PERFORM KEY-ALL-TXC.                                         
           MOVE FRM-NUM-DTXC IN TXC-FLD TO TXC-NUM-DTXC IN TXC.         
           MOVE FRM-NUM-MTXC IN TXC-FLD TO TXC-NUM-MTXC IN TXC.         
           MOVE FRM-NUM-STXC IN TXC-FLD TO TXC-NUM-STXC IN TXC.         
           MOVE FRM-NUM-ATXC IN TXC-FLD TO TXC-NUM-ATXC IN TXC.         
           MOVE 0                       TO TXC-NUM-CMST IN TXC.         
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-GRABA-TRAMOS-ANTIG.                                          
           PERFORM TAB-FIO-TXC.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    TXC-COD-PLZO IN TXC = TXC-COD-PLZO IN TXC-FLD AND   
                    TXC-COD-VCOC IN TXC = TXC-COD-VCOC IN TXC-FLD AND   
                    TXC-NUM-DTXC IN TXC = FRM-NUM-DTXC IN TXC-FLD AND   
                    TXC-NUM-MTXC IN TXC = FRM-NUM-MTXC IN TXC-FLD AND   
                    TXC-NUM-STXC IN TXC = FRM-NUM-STXC IN TXC-FLD AND   
                    TXC-NUM-ATXC IN TXC = FRM-NUM-ATXC IN TXC-FLD )     
               GO TO FIN-GRABA-TRAMOS-ANTIG.                            
      *Guarda la llave completa para reposicionarse con GET-NLS y       
      *poder comparar que me posicione en el mismo registro             
           MOVE TXC-KEY-ITXC IN TXC TO WSS-KAUX.                        
           PERFORM BUS-TRAMO-TASA.                                      
           IF FRM-IFLD > WSS-MTRA                                       
               MOVE FIO-GET-NXT TO FIO-CMND                             
               GO TO LUP-GRABA-TRAMOS-ANTIG.                            
      *Asigna el puntero al tramo que le corresponde                    
           MOVE FRM-IFLD TO WSS-ITAS.                                   
                                                                        
      *Solo se modifica la fecha del registro original                  
      *la fecha original se encuentra en FRM-FEC-FTXC IN TXC-FLD        
           MOVE TXC-NUM-DTXC IN TXC-FLD TO TXC-NUM-DTXC IN TXC.         
           MOVE TXC-NUM-MTXC IN TXC-FLD TO TXC-NUM-MTXC IN TXC.         
           MOVE TXC-NUM-STXC IN TXC-FLD TO TXC-NUM-STXC IN TXC.         
           MOVE TXC-NUM-ATXC IN TXC-FLD TO TXC-NUM-ATXC IN TXC.         
           PERFORM GET-FHOY.                                            
           MOVE HOY-STP-ITRN            TO TXC-STP-ITRN IN TXC.         
           MOVE SCR-USER                TO TXC-COD-ATRN IN TXC.         
           MOVE SCR-OTRN                TO TXC-COD-OTRN IN TXC.         
           MOVE 'I'                     TO TXC-MSC-STAT IN TXC.         
           MOVE 'H'                     TO TXC-MSC-ANTI IN TXC.         
           MOVE FRM-COD-BASE IN TXC-FLD TO TXC-COD-BASE IN TXC.         
           MOVE FRM-NUM-DTXC IN TXC-FLD TO FEC-DEC1.                    
           MOVE FRM-NUM-MTXC IN TXC-FLD TO FEC-MEC1.                    
           MOVE FRM-NUM-STXC IN TXC-FLD TO FEC-SEC1.                    
           MOVE FRM-NUM-ATXC IN TXC-FLD TO FEC-AEC1.                    
           MOVE TXC-NUM-DTXC IN TXC-FLD TO FEC-DEC2.                    
           MOVE TXC-NUM-MTXC IN TXC-FLD TO FEC-MEC2.                    
           MOVE TXC-NUM-STXC IN TXC-FLD TO FEC-SEC2.                    
           MOVE TXC-NUM-ATXC IN TXC-FLD TO FEC-AEC2.                    
           MOVE FEC-FORM-FEC TO FEC-FORM.                               
           MOVE FEC-DIF-DIA  TO FEC-CMND.                               
           PERFORM CAL-FEC.                                             
           IF NOT FEC-STAT-OKS                                          
               MOVE FEC-MENS       TO TXC-MENS                          
               MOVE 'TXCDIFDIAERR' TO TXC-CMSG                          
               GO TO FIN-GRABA-TRAMOS-ANTIG.                            
      *Calcula acumulado con la tasa original del registro              
           COMPUTE TXC-SGV-ATXC IN TXC ROUNDED =                        
               TXC-SGV-ATXC IN TXC +                                    
               TXC-SGV-VTXC * FRM-NUM-BASE IN TXC-FLD *                 
               FEC-NDIA.                                                
      *Valor de la tasa debe asignarse despues de calcular acumulado    
      *con la tasa original del registro                                
           MOVE FRM-SGV-VTXC IN TXC-FLD(WSS-ITAS) TO                    
                TXC-SGV-VTXC IN TXC.                                    
           MOVE SPACES TO TXC-GLS-DISP IN TXC.                          
           MOVE FIO-PUT TO FIO-CMND.                                    
           PERFORM TAB-FIO-TXC.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE FIO-MENS       TO TXC-MENS                          
               MOVE 'TXCPUT ERROR' TO TXC-CMSG                          
               GO TO FIN-GRABA-TRAMOS-ANTIG.                            
      *Debo posicionarme nuevamente en el registro original             
           MOVE WSS-KAUX TO TXC-KEY-ITXC IN TXC.                        
           MOVE FIO-GET-NLS TO FIO-CMND.                                
           PERFORM TAB-FIO-TXC.                                         
           IF NOT ( FIO-STAT-OKS AND TXC-KEY-ITXC IN TXC = WSS-KAUX )   
               MOVE 'TXCGNLSERROR' TO TXC-CMSG                          
               GO TO FIN-GRABA-TRAMOS-ANTIG.                            
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-GRABA-TRAMOS-ANTIG.                                
       FIN-GRABA-TRAMOS-ANTIG.                                          
           EXIT.                                                        
                                                                        
       BUS-TRAMO-TASA SECTION.                                          
       INI-BUS-TRAMO-TASA.                                              
           MOVE 0 TO FRM-IFLD.                                          
       LUP-BUS-TRAMO-TASA.                                              
           ADD 1 TO FRM-IFLD.                                           
           IF FRM-IFLD > WSS-MTRA                                       
               GO TO FIN-BUS-TRAMO-TASA.                                
      *Si existen desplegados tramos historicos estos no se consideran  
      *en este proceso                                                  
           IF FRM-MSC-ANTI IN TXC-FLD(FRM-IFLD) = 'H'                   
               GO TO LUP-BUS-TRAMO-TASA.                                
      *                                                                 
           IF TXC-NUM-MSTR IN TXC = FRM-NUM-MSTR IN TXC-FLD(FRM-IFLD)   
               ADD 1 WSS-MTRA GIVING FRM-IFLD                           
               GO TO FIN-BUS-TRAMO-TASA.                                
           IF TXC-NUM-MSTR IN TXC < FRM-NUM-MSTR IN TXC-FLD(FRM-IFLD)   
               GO TO FIN-BUS-TRAMO-TASA.                                
           GO TO LUP-BUS-TRAMO-TASA.                                    
       FIN-BUS-TRAMO-TASA.                                              
           EXIT.                                                        
                                                                        
       PROCESO-VRF SECTION.                                             
       INI-PROCESO-VRF.                                                 
           MOVE SPACES TO TXC-CMSG TXC-MENS.                            
           PERFORM KEY-ALL-TXC.                                         
           MOVE 0 TO TXC-NUM-CMST IN TXC.                               
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-PROCESO-VRF.                                                 
           PERFORM TAB-FIO-TXC.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    TXC-COD-PLZO IN TXC = TXC-COD-PLZO IN TXC-FLD AND   
                    TXC-COD-VCOC IN TXC = TXC-COD-VCOC IN TXC-FLD AND   
                    TXC-NUM-DTXC IN TXC = TXC-NUM-DTXC IN TXC-FLD AND   
                    TXC-NUM-MTXC IN TXC = TXC-NUM-MTXC IN TXC-FLD AND   
                    TXC-NUM-STXC IN TXC = TXC-NUM-STXC IN TXC-FLD AND   
                    TXC-NUM-ATXC IN TXC = TXC-NUM-ATXC IN TXC-FLD )     
               GO TO FIN-PROCESO-VRF.                                   
      *Guarda la llave completa para reposicionarse con GET-NLS y       
      *poder comparar que me posicione en el mismo registro             
           MOVE TXC-KEY-ITXC IN TXC TO WSS-KAUX.                        
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.                            
           PERFORM TAB-FIO-TXC.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE 'TXC    NOGKU' TO TXC-CMSG                          
               GO TO FIN-PROCESO-VRF.                                   
           MOVE 'V' TO TXC-MSC-STAT IN TXC.                             
           MOVE FIO-MOD TO FIO-CMND.                                    
           PERFORM TAB-FIO-TXC.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE 'TXC    NOMOD' TO TXC-CMSG                          
               GO TO FIN-PROCESO-VRF.                                   
      *Para iniciar de nuevo el START en el registro actual y poder     
      *leer el siguiente                                                
           MOVE WSS-KAUX TO TXC-KEY-ITXC IN TXC.                        
           MOVE FIO-GET-NLS TO FIO-CMND.                                
           PERFORM TAB-FIO-TXC.                                         
           IF NOT ( FIO-STAT-OKS AND TXC-KEY-ITXC IN TXC = WSS-KAUX )   
               MOVE 'TXCGNLSERROR' TO TXC-CMSG                          
               GO TO FIN-PROCESO-VRF.                                   
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-PROCESO-VRF.                                       
       FIN-PROCESO-VRF.                                                 
           EXIT.                                                        
                                                                        
       PROCESO-ELI SECTION.                                             
       INI-PROCESO-ELI.                                                 
           PERFORM GET-FHOY.                                            
           MOVE SPACES TO TXC-CMSG TXC-MENS.                            
           PERFORM KEY-ALL-TXC.                                         
           MOVE 0 TO TXC-NUM-CMST IN TXC.                               
       LUP-PROCESO-ELI.                                                 
           MOVE FIO-GET-NLS TO FIO-CMND.                                
           PERFORM TAB-FIO-TXC.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    TXC-COD-PLZO IN TXC = TXC-COD-PLZO IN TXC-FLD AND   
                    TXC-COD-VCOC IN TXC = TXC-COD-VCOC IN TXC-FLD AND   
                    TXC-NUM-DTXC IN TXC = TXC-NUM-DTXC IN TXC-FLD AND   
                    TXC-NUM-MTXC IN TXC = TXC-NUM-MTXC IN TXC-FLD AND   
                    TXC-NUM-STXC IN TXC = TXC-NUM-STXC IN TXC-FLD AND   
                    TXC-NUM-ATXC IN TXC = TXC-NUM-ATXC IN TXC-FLD )     
               GO TO FIN-PROCESO-ELI.                                   
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.                            
           PERFORM TAB-FIO-TXC.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE 'TXC    NOGKU' TO TXC-CMSG                          
               GO TO FIN-PROCESO-ELI.                                   
           IF WSS-ANTI > SPACES AND                                     
              TXC-MSC-ANTI IN TXC NOT = WSS-ANTI                        
               MOVE FIO-ULK-REC TO FIO-CMND                             
               PERFORM TAB-FIO-TXC                                      
               GO TO LUP-PROCESO-ELI.                                   
      *Se hace una validacion adicional para no eliminar                
      *Verificados anteriores                                           
           IF ( TXC-MSC-STAT IN TXC = 'V' ) AND                         
              ( TXC-NUM-DTXC IN TXC NOT = HOY-DHOY OR                   
                TXC-NUM-MTXC IN TXC NOT = HOY-MHOY OR                   
                TXC-NUM-STXC IN TXC NOT = HOY-SHOY OR                   
                TXC-NUM-ATXC IN TXC NOT = HOY-AHOY )                    
               MOVE 'TXCSTATYAVRF' TO TXC-CMSG                          
               GO TO FIN-PROCESO-ELI.                                   
           MOVE FIO-DEL TO FIO-CMND.                                    
           PERFORM TAB-FIO-TXC.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE 'TXC    NODEL' TO TXC-CMSG                          
               GO TO FIN-PROCESO-ELI.                                   
           GO TO LUP-PROCESO-ELI.                                       
       FIN-PROCESO-ELI.                                                 
           EXIT.                                                        
      *>>>>                                                             
