       PROCEDURE DIVISION.
      *===================

       MAIN SECTION.
       INI-MAIN.
           PERFORM INI.
       LUP-MAIN.
           IF SCR-INST = SCR-INST-HCM
                 PERFORM SCR-COM.
           IF SCR-CCPP = 'ACC'
                 PERFORM SCR-ACC
           ELSE
                 MOVE SCR-INST-HCM TO SCR-INST.
           GO TO LUP-MAIN.
       FIN-MAIN.
           EXIT.

       INI SECTION.
       INI-INI.
       COPY GNSBGEIB.
       COPY GNSBGEDB.
           MOVE LOW-VALUES TO OCD-FLD.
           MOVE LOW-VALUES TO OCD-EDT.
           PERFORM GNS-PRO-RCK.
           PERFORM GNS-BUS-VCA.
           MOVE +3000 TO SCR-TCMA.
           PERFORM GNS-HDL-SYS.
           PERFORM GNS-HDL-FRM.
           MOVE 'TABPQOCD' TO FIO-PROG.
           PERFORM GNS-BUS-IDD.
      *PTC-INI
           PERFORM GNS-ERR-QUE.
           PERFORM GET-TSK-TERM.
      *PTC-FIN
      *<<<< INI_FIO                                                     
           IF SCR-OFRM-SRV                                              
                MOVE TSK-TERM-ALF TO OCD-COD-TERM                       
                MOVE TSK-TERM-ALF TO OCD-COD-TERM-MAS                   
           ELSE                                                         
                MOVE EIBTRMID     TO OCD-COD-TERM                       
                MOVE EIBTRMID     TO OCD-COD-TERM-MAS.                  
           MOVE +3859 TO SCR-TCMA.                                      
           PERFORM GNS-ERR-QUE.                                         
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )      
               MOVE SCR-OCD-FLD TO OCD-FLD                              
               GO TO FIN-INI.                                           
                                                                        
           IF ( FRM-FFLD = FRM-FFLD-PF6 )                               
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM        
               ADD 1              TO QUE-NITM                           
               MOVE OCD-NOM-TSOCD TO QUE-COLA                           
               MOVE OCD-LEN-TSOCD TO QUE-LITM                           
               MOVE QUE-GET       TO QUE-CMND                           
               PERFORM GNS-PRO-QUE                                      
               IF QUE-STAT NOT = QUE-STAT-OKS                           
                  MOVE SCR-OCD-FLD TO OCD-FLD                           
                  GO TO FIN-INI.                                        
           PERFORM GET-FHOY.                                            
      *>>>>                                                             
       FIN-INI.
           EXIT.

       COPY GNSBHSYS.
       COPY GNSBHFRM.
       COPY GNSBPRCK.
       GNS-BUS-VCA SECTION.
       INI-GNS-BUS-VCA.
           MOVE 0 TO FRM-IFLD.
       LUP-GNS-BUS-VCA.
           ADD 1 TO FRM-IFLD.
           IF FRM-IFLD NOT > EIBCALEN AND
              FRM-IFLD NOT > 3000
                MOVE DFH-CMMA(FRM-IFLD) TO PGM-CMMA-CHR(FRM-IFLD)
                GO TO LUP-GNS-BUS-VCA.
           MOVE PGM-CMMA TO SCR-VARI.
       FIN-GNS-BUS-VCA.
           EXIT.
       COPY GNSBBIDD.

       SCR-COM SECTION.
       INI-SCR-COM.
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-CLRA-OCD.
           PERFORM EDT-KEY-OCD.
           PERFORM EDT-FLD-OCD.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-HCM TO SCR-INST.
           MOVE SCR-CMNP     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       XCT-ERR.
           MOVE 'XCTERR    ' TO MSG-COD-MENS.
           MOVE 'GNS'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO SYS-MEN1.
           MOVE SYS-PROG     TO SYS-MEN2.
           MOVE SYS-ABOR     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       COPY GNSBGGBK.
       FIN-SCR-COM.
           EXIT.



       SCR-ACC SECTION.
       INI-SCR-ACC.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ACC
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-ACC.
           PERFORM DESSCRTRL.
      *<<<< PFS_SCR_ACC                                                 
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6                   
               PERFORM VAL-NUM-KEY-OCD                                  
               PERFORM VAL-NUM-FLD-OCD                                  
               PERFORM SCRL-TAB-OCD                                     
               PERFORM SCR-TRL.                                         
      *>>>>                                                             
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ACC.
           EXIT.

       BRW-SCR-ACC SECTION.
       INI-BRW-SCR-ACC.
       FIN-BRW-SCR-ACC.
           EXIT.




       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               PERFORM SCR-CLRA-OCD
               PERFORM EDT-KEY-OCD
               PERFORM EDT-FLD-OCD
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-OCD
               PERFORM EDT-KEY-OCD
           ELSE
               PERFORM CLR-FLD-OCD
               PERFORM EDT-FLD-OCD.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

       SCR-KEY SECTION.
       INI-SCR-KEY.
           PERFORM SCR-RST.
           PERFORM GET-KEY.
           IF FRM-STAT-ABT
                   MOVE SPACES TO FRM-MENS
                   PERFORM SCR-MSSG
                   GO TO FIN-SCR-KEY.
       FND-SCR-KEY.
           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
           IF FIO-STAT-OKS
               PERFORM PUT-SCR
               MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
               GO TO FIN-SCR-KEY.
      *JSS ELSE
      *        PERFORM CLR-FLD-OCD.
       ERR-SCR-KEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR.
           IF FRM-MENS NOT > SPACES
               IF FIO-STAT-EOF
                   MOVE 'REGEOF    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
               IF FIO-STAT = FIO-STAT-NEX
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
           MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-SCR-KEY.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO OCD-FLD.
           MOVE LOW-VALUES TO OCD-EDT.
       FIN-SCR-RST.
           EXIT.

       GET-KEY SECTION.
       INI-GET-KEY.
           PERFORM SCR-GKEY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-KEY.
           EXIT.

       GET-IKY SECTION.
       INI-GET-IKY.
           PERFORM SCR-GIKY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-IKY.
           EXIT.

       FND-KEY SECTION.
       INI-FND-KEY.
      *<<<< INI_FND_KEY                                                 
           IF FRM-COD-CPTO IN OCD-FLD = 'INT'                           
              PERFORM CRG-ROC                                           
           ELSE                                                         
           IF FRM-COD-CPTO IN OCD-FLD = 'SGS'                           
              PERFORM CRG-CXE-SGS                                       
           ELSE                                                         
              MOVE 'CPT    NEX' TO MSG-COD-MENS.                        
                                                                        
           IF MSG-COD-MENS > SPACES                                     
              GO TO MSG-FND-KEY                                         
           ELSE                                                         
              GO TO CON-FND-KEY.                                        
      *>>>>                                                             
           PERFORM KEY-ALL-OCD.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM TAB-FIO-ROC.
           IF FIO-STAT-OKS
               MOVE ROC-STP-ITRN IN ROC TO SCR-UMOD.
      *<<<< FIN_FND_KEY                                                 
       CON-FND-KEY.                                                     
           PERFORM PUT-TAB.                                             
           IF MSG-COD-MENS = 'OCD    NEX'                               
               GO TO MSG-FND-KEY.                                       
           IF FRM-COD-CPTO IN OCD-FLD = 'SGS'                           
              GO TO CON-FND-KEY-SGS.                                    
                                                                        
           MOVE FRM-CAI-IOPE IN OCD-FLD TO ITR-CAI-IOPC IN ITR.         
           MOVE FRM-IIC-IOPE IN OCD-FLD TO ITR-IIC-IOPC IN ITR.         
           MOVE ZEROES                  TO ITR-NUM-ICAN IN ITR.         
           MOVE SPACES                  TO ITR-CIC-IOPV IN ITR          
           MOVE ZEROES                  TO ITR-NUM-IVCT IN ITR          
           MOVE 'INT'                   TO ITR-IND-TTAS IN ITR.         
           MOVE ZEROES                  TO ITR-FEC-FINI IN ITR.         
           MOVE FIO-GET-NLS TO FIO-CMND.                                
           PERFORM COL-FIO-ITR.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    ITR-CAI-IOPC IN ITR = FRM-CAI-IOPE IN OCD-FLD AND   
                    ITR-IIC-IOPC IN ITR = FRM-IIC-IOPE IN OCD-FLD AND   
                    ITR-NUM-ICAN IN ITR = ZEROES AND                    
                    ITR-IND-TTAS IN ITR = 'INT')                        
                MOVE 'ITR    NEX' TO MSG-COD-MENS                       
                GO TO MSG-FND-KEY                                       
           ELSE                                                         
               IF ITR-COD-CVTI IN ITR > SPACES                          
                   MOVE ITR-SGV-SPRD IN ITR TO FRM-SGV-TFIN IN OCD-FLD  
               ELSE                                                     
                   MOVE ITR-SGV-TINT IN ITR TO FRM-SGV-TFIN IN OCD-FLD. 
                                                                        
           MULTIPLY WSS-SGV-VFIN-VEC(WSS-J) BY 1 GIVING                 
                    WSS-SGV-VINT ROUNDED.                               
           IF WSS-SGV-VINT > FRM-SGV-TFIN IN OCD-FLD                    
               MOVE 'TAB'      TO MSG-COD-SIST                          
               MOVE 'TAXP>TMC' TO MSG-COD-MENS                          
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-GLS-TFAJ IN OCD-FLD             
               MOVE SPACES TO MSG-COD-MENS.                             
                                                                        
           IF WSS-SGV-VINT < FRM-SGV-TFIN IN OCD-FLD                    
               MOVE 'TAB'          TO MSG-COD-SIST                      
               MOVE 'TAXP<CFONDO'  TO MSG-COD-MENS                      
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-GLS-TFAJ IN OCD-FLD             
               MOVE SPACES TO MSG-COD-MENS.                             
       CON-FND-KEY-SGS.                                                 
           MOVE '00' TO FIO-STAT.                                       
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           GO TO FIN-FND-KEY.                                           
      *----------------------------------------------------------       
       MSG-FND-KEY.                                                     
           MOVE 'TAB'        TO MSG-COD-SIST.                           
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MENS.                               
           MOVE '00'         TO FIO-STAT.                               
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           GO TO FIN-FND-KEY.                                           
      *>>>>                                                             
       FIN-FND-KEY.
           IF FIO-STAT = FIO-STAT-NEX
               MOVE 'S' TO SCR-IERR.
       EXT-FND-KEY.
           EXIT.
 
       PUT-SCR SECTION.
       INI-PUT-SCR.
           IF PGM-STAT-ROC-OKS
               MOVE ROC-STP-ITRN IN ROC TO SCR-UMOD.
           PERFORM PUT-ALL-OCD.
       FIN-PUT-SCR.
           EXIT.

       STP-EXIT SECTION.
       INI-STP-EXIT.
           MOVE SCR-PROG TO PGM-PROG.
           MOVE SCR-NTRN TO PGM-NTRN.

           MOVE SCR-PROV TO SCR-PROG.
           MOVE SCR-NTRV TO SCR-NTRN.

           MOVE PGM-PROG TO SCR-PROV
           MOVE PGM-NTRN TO SCR-NTRV.
       FIN-STP-EXIT.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO SYS-NTRN.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
      *PTC-INI
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE OCD-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO OCD-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GKEY SECTION.
       INI-SCR-GKEY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-OCD
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO OCD-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GKEY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-OCD.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-OCD
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-OCD.
           PERFORM VAL-IKY-OCD.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-MKY-OCD.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-AKY-OCD.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-CON-KEY-OCD.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GKEY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-OCD.
       ERR-SCR-GKEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-OCD.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GKEY.
           PERFORM SCR-MSSG.

       SCR-GIKY SECTION.
       INI-SCR-GIKY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-OCD
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO OCD-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GIKY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-OCD.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-IKY-OCD
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-OCD.
           PERFORM VAL-IKY-OCD.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GIKY.
           PERFORM VAL-CON-IKY-OCD.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GIKY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-OCD.
       ERR-SCR-GIKY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-IKY-OCD.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GIKY.
           PERFORM SCR-MSSG.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
      *<<<< PFS_GET_CON                                                 
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )           
      *>>>>                                                             
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO OCD-GLS-MEN2
               MOVE SCR-PRMT     TO OCD-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO OCD-FLD.
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO OCD-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE OCD-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO OCD-FLD
      * FLB  7-NOV-1994
               PERFORM CLR-KEY-OCD

               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO OCD-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO OCD-GLS-MENS.
           PERFORM PRO-MKY-OCD.
           PERFORM PRO-AKY-OCD.
           PERFORM UNP-IKY-OCD.
           PERFORM FST-KEY-OCD.
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE OCD-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO OCD-FLD
               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO OCD-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO OCD-GLS-MENS.
           PERFORM UNP-KEY-OCD.
           PERFORM FST-KEY-OCD.
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
           PERFORM PRO-IKY-OCD.
           PERFORM PRO-AKY-OCD.
           PERFORM UNP-MKY-OCD.
           PERFORM UNP-FLD-OCD.
           PERFORM FST-FLD-OCD.
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO OCD-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-OCD.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
      *PTC-INI
           IF SCR-STAT NOT = SCR-STPP
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-OCD
               PERFORM EDT-FLD-OCD
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-OCD
               PERFORM EDT-FLD-OCD
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-OCD.
           PERFORM EDT-FLD-OCD.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

      * PGM-EBR-ALL SECTION.
      * INI-PGM-EBR-ALL.
      *     MOVE 'GNS' TO FIO-SIST.
      *     MOVE FIO-EBR-ALL TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
      * FIN-PGM-EBR-ALL.
      *     EXIT.

       COPY TABBQOCD.
       COPY GNSBVNUM.
       COPY TABBFROC.
       COPY GNSBFMSG.
       COPY GNSBGSYS.
       COPY GNSBGDTC.
      *PTC-INI
      *PTC-SUB
      * ISP  10-MAY-2001 13:46:35 
      * COPY GNSBGFRM.
       COPY GNSBG3CF.
      *PTC-FIN
       COPY GNSBGMSG.
       COPY GNSBEKEY.
       COPY GNSBIABT.
       COPY GNSBFTAB.
       COPY GNSBEGLS.
       COPY GNSBGFEC.
       COPY GNSBGHOY.
      *PTC-INI
       COPY GNSBGQUE.
       COPY GNSBGTSK.
      *PTC-FIN

      *<<<<
       PGM-EBR-ALL SECTION.                                             
       INI-PGM-EBR-ALL.                                                 
           MOVE 'GNS' TO FIO-SIST.                                      
           MOVE FIO-EBR-ALL TO FIO-CMND.                                
           PERFORM GNS-FIO-TAB.                                         
       FIN-PGM-EBR-ALL.                                                 
           EXIT.                                                        
       COPY COLBFITR.                                                   
       COPY COLBFCXE.                                                   
       COPY TABBBUSR.                                                   
       COPY TABBFUSR.                                                   
       COPY TABBFCIN.                                                   
       COPY GNSBBMSC.                                                   
       COPY GNSBFMSC.                                                   
       COPY GNSBBTAB.                                                   
       COPY GNSBVTAB.                                                   
       COPY GNSBVCOD.                                                   
       COPY GNSBPFEC.                                                   
       COPY GNSBGCPT.                                                   
       COPY GNSBGSTR.                                                   
                                                                        
       PUT-TAB SECTION.                                                 
       INI-PUT-TAB.                                                     
           MOVE OCD-NOM-TSOCD     TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE OCD-NOM-TSOCD-MAS TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE 1 TO SCR-NUM-PANT.                                      
           MOVE LOW-VALUES TO WSS-NUM-IVCT-IDX-MTI.                     
           MOVE LOW-VALUES TO WSS-NUM-IVCT-IDX-FRM.                     
      *                                                                 
           MOVE 1 TO FRM-IFLD.                                          
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.                        
                                                                        
           MOVE 1 TO WSS-F IN WSS-VARI.                                 
       LUP-PUT-TAB.                                                     
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI                  
              GO TO FIN-PUT-TAB.                                        
           IF WSS-CAI-IOPE-F-VEC(WSS-F) > SPACES                        
              PERFORM PUT-TAB-OCD                                       
              PERFORM PUT-OCD-QUE                                       
              PERFORM SET-FRM-IFLD                                      
           ELSE                                                         
           IF FRM-IFLD = 1 AND WSS-F = 1                                
              MOVE 'TAB'        TO MSG-COD-SIST                         
              MOVE 'OCD    NEX' TO MSG-COD-MENS                         
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              MOVE '00' TO FIO-STAT                                     
      *       MOVE 'S' TO SCR-IERR                                      
              GO TO FIN-PUT-TAB                                         
           ELSE                                                         
              MOVE '00' TO FIO-STAT                                     
              GO TO FIN-PUT-TAB.                                        
                                                                        
           ADD 1 TO WSS-F IN WSS-VARI.                                  
           GO TO LUP-PUT-TAB.                                           
       FIN-PUT-TAB.                                                     
           MOVE 1                  TO QUE-NITM.                         
           MOVE OCD-LEN-TSOCD-MAS  TO QUE-LITM.                         
           MOVE OCD-NOM-TSOCD-MAS  TO QUE-COLA.                         
                                                                        
           IF WSS-NITM > WSS-NITMMAX                                    
              MOVE 'S'           TO OCD-MAS-IND-HMAS                    
           ELSE                                                         
              MOVE 'N'           TO OCD-MAS-IND-HMAS.                   
                                                                        
           MOVE WSS-NITM         TO WSS-NUM-ICUR.                       
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.                   
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.                   
           MOVE WSS-NUM-ICUR-ALF TO OCD-MAS-NUM-TOCC.                   
           MOVE OCD-MAS-ITEM     TO QUE-ITEM.                           
           MOVE QUE-PUT          TO QUE-CMND.                           
           PERFORM GNS-PRO-QUE.                                         
           IF WSS-NITM > WSS-IMAX                                       
               MOVE 'TAB' TO MSG-COD-SIST                               
               MOVE 'OCDTABDMAS' TO MSG-COD-MENS                        
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN2.                           
       EXT-PUT-TAB.                                                     
           EXIT.                                                        
                                                                        
       SET-FRM-IFLD SECTION.                                            
       INI-SET-FRM-IFLD.                                                
           IF SCR-OFRM-SRV                                              
                MOVE 1 TO FRM-IFLD                                      
           ELSE                                                         
                ADD 1 TO FRM-IFLD.                                      
       FIN-SET-FRM-IFLD.                                                
           EXIT.                                                        
      *--------------------------------------------                     
       SCRL-TAB-OCD SECTION.                                            
       INI-SCRL-TAB-OCD.                                                
           MOVE SPACES TO MSG-COD-MENS.                                 
                                                                        
           IF FRM-FFLD = FRM-FFLD-PF6                                   
               PERFORM PUT-TAB-ASC                                      
           ELSE                                                         
               PERFORM PUT-TAB-DSC.                                     
           IF MSG-COD-MENS > SPACES                                     
               MOVE 'TAB' TO MSG-COD-SIST                               
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN2.                           
       FIN-SCRL-TAB-OCD.                                                
           EXIT.                                                        
                                                                        
      *--------------------------------------------                     
       PUT-TAB-OCD SECTION.                                             
       INI-PUT-TAB-OCD.                                                 
           PERFORM CLR-WSS.                                             
           MOVE WSS-NUM-IVCT-VEC(WSS-F)   TO  WSS-NUM-IVCT-MTI.         
           MOVE WSS-COD-EVEN-VEC(WSS-F)   TO  WSS-COD-EVEN-MTI.         
           MOVE WSS-GLS-DESC-VEC(WSS-F)   TO  WSS-GLS-DESC-MTI.         
           MOVE WSS-FEC-FEVT-VEC(WSS-F)   TO  WSS-FEC-FEVT-MTI.         
           MOVE WSS-COD-IPLT-VEC(WSS-F)   TO  WSS-COD-IPLT-MTI.         
           MOVE WSS-COD-TSEG-VEC(WSS-F)   TO  WSS-COD-TSEG-MTI.         
           IF FRM-COD-CPTO IN OCD-FLD = 'SGS'                           
              MULTIPLY WSS-SGV-TAMO-SGS(WSS-F) BY 1                     
                GIVING WSS-SGV-TAMO-MTI ROUNDED                         
              MULTIPLY WSS-VAL-MNTO-SGS(WSS-F) BY 1                     
                GIVING WSS-VAL-MNTO-MTI ROUNDED                         
              MULTIPLY WSS-SGV-VFIN-SGS(WSS-F) BY 1                     
                GIVING WSS-SGV-VFIN-MTI ROUNDED                         
           ELSE                                                         
           MULTIPLY WSS-SGV-TAMO-VEC(WSS-F) BY 1                        
                GIVING WSS-SGV-TAMO-MTI ROUNDED                         
           MULTIPLY WSS-VAL-MNTO-VEC(WSS-F) BY 1                        
                GIVING WSS-VAL-MNTO-MTI ROUNDED                         
           MULTIPLY WSS-SGV-VFIN-VEC(WSS-F) BY 1                        
                                       GIVING WSS-SGV-VFIN-MTI ROUNDED. 
           MOVE WSS-CAI-IOPE-F-VEC(WSS-F) TO  WSS-CAI-IOPE-F-MTI.       
           MOVE WSS-IIC-IOPE-F-VEC(WSS-F) TO  WSS-IIC-IOPE-F-MTI.       
           MOVE WSS-EXT-IOPE-F-VEC(WSS-F) TO  WSS-EXT-IOPE-F-MTI.       
           MOVE WSS-COD-SIST-F-VEC(WSS-F) TO  WSS-COD-SIST-F-MTI.       
           MOVE WSS-COD-EVEN-F-VEC(WSS-F) TO  WSS-COD-EVEN-F-MTI.       
           MOVE WSS-NUM-EVEN-F-VEC(WSS-F) TO  WSS-NUM-EVEN-F-MTI.       
           MOVE WSS-CAI-IOPV-F-VEC(WSS-F) TO  WSS-CAI-IOPV-F-MTI.       
           MOVE WSS-IIC-IOPV-F-VEC(WSS-F) TO  WSS-IIC-IOPV-F-MTI.       
           MOVE WSS-NUM-IVCT-F-VEC(WSS-F) TO  WSS-NUM-IVCT-F-MTI.       
           MOVE WSS-COD-CCPT-F-VEC(WSS-F) TO  WSS-COD-CCPT-F-MTI.       
           MOVE WSS-COD-SCPT-F-VEC(WSS-F) TO  WSS-COD-SCPT-F-MTI.       
                                                                        
           MOVE WSS-NUM-IVCT-VEC(WSS-F)   TO  WSS-NUM-IVCT.             
           MOVE WSS-COD-EVEN-VEC(WSS-F)   TO  WSS-COD-EVEN.             
           MOVE WSS-GLS-DESC-VEC(WSS-F)   TO  WSS-GLS-DESC.             
           MOVE WSS-FEC-FEVT-VEC(WSS-F)   TO  WSS-FEC-FEVT.             
           MOVE WSS-COD-IPLT-VEC(WSS-F)   TO  WSS-COD-IPLT.             
           MOVE WSS-COD-TSEG-VEC(WSS-F)   TO  WSS-COD-TSEG.             
           MOVE WSS-SGV-TAMO-MTI          TO  WSS-SGV-TAMO.             
           MOVE WSS-VAL-MNTO-MTI          TO  WSS-VAL-MNTO.             
           MOVE WSS-SGV-VFIN-MTI          TO  WSS-SGV-VFIN.             
           MOVE WSS-CAI-IOPE-F-VEC(WSS-F) TO  WSS-CAI-IOPE-F.           
           MOVE WSS-IIC-IOPE-F-VEC(WSS-F) TO  WSS-IIC-IOPE-F.           
           MOVE WSS-EXT-IOPE-F-VEC(WSS-F) TO  WSS-EXT-IOPE-F.           
           MOVE WSS-COD-SIST-F-VEC(WSS-F) TO  WSS-COD-SIST-F.           
           MOVE WSS-COD-EVEN-F-VEC(WSS-F) TO  WSS-COD-EVEN-F.           
           MOVE WSS-NUM-EVEN-F-VEC(WSS-F) TO  WSS-NUM-EVEN-F.           
           MOVE WSS-CAI-IOPV-F-VEC(WSS-F) TO  WSS-CAI-IOPV-F.           
           MOVE WSS-IIC-IOPV-F-VEC(WSS-F) TO  WSS-IIC-IOPV-F.           
           MOVE WSS-NUM-IVCT-F-VEC(WSS-F) TO  WSS-NUM-IVCT-F.           
           MOVE WSS-COD-CCPT-F-VEC(WSS-F) TO  WSS-COD-CCPT-F.           
           MOVE WSS-COD-SCPT-F-VEC(WSS-F) TO  WSS-COD-SCPT-F.           
                                                                        
           IF FRM-IFLD > WSS-IMAX                                       
              GO TO FIN-PUT-TAB-OCD.                                    
                                                                        
           MOVE WSS-NUM-IVCT-MTI   TO                                   
                FRM-NUM-IVCT   IN OCD-FLD(FRM-IFLD).                    
           MOVE WSS-COD-EVEN-MTI   TO                                   
                FRM-COD-EVEN   IN OCD-FLD(FRM-IFLD).                    
           MOVE WSS-GLS-DESC-MTI   TO                                   
                FRM-GLS-DESC   IN OCD-FLD(FRM-IFLD).                    
           MOVE WSS-FEC-FEVT-MTI   TO                                   
                FRM-FEC-FEVT   IN OCD-FLD(FRM-IFLD).                    
           MOVE WSS-COD-IPLT-MTI   TO                                   
                FRM-COD-IPLT   IN OCD-FLD(FRM-IFLD).                    
           MOVE WSS-COD-TSEG-MTI   TO                                   
                FRM-COD-TSEG   IN OCD-FLD(FRM-IFLD).                    
           MOVE WSS-SGV-TAMO-MTI   TO                                   
                FRM-SGV-TAMO   IN OCD-FLD(FRM-IFLD).                    
           MOVE WSS-VAL-MNTO-MTI   TO                                   
                FRM-VAL-MNTO   IN OCD-FLD(FRM-IFLD).                    
           MOVE WSS-SGV-VFIN-MTI   TO                                   
                FRM-SGV-VFIN   IN OCD-FLD(FRM-IFLD).                    
           MOVE WSS-CAI-IOPE-F-MTI TO                                   
                FRM-CAI-IOPE-F IN OCD-FLD(FRM-IFLD).                    
           MOVE WSS-IIC-IOPE-F-MTI TO                                   
                FRM-IIC-IOPE-F IN OCD-FLD(FRM-IFLD).                    
           MOVE WSS-EXT-IOPE-F-MTI TO                                   
                FRM-EXT-IOPE-F IN OCD-FLD(FRM-IFLD).                    
           MOVE WSS-COD-SIST-F-MTI TO                                   
                FRM-COD-SIST-F IN OCD-FLD(FRM-IFLD).                    
           MOVE WSS-COD-EVEN-F-MTI TO                                   
                FRM-COD-EVEN-F IN OCD-FLD(FRM-IFLD).                    
           MOVE WSS-NUM-EVEN-F-MTI TO                                   
                FRM-NUM-EVEN-F IN OCD-FLD(FRM-IFLD).                    
           MOVE WSS-CAI-IOPV-F-MTI TO                                   
                FRM-CAI-IOPV-F IN OCD-FLD(FRM-IFLD).                    
           MOVE WSS-IIC-IOPV-F-MTI TO                                   
                FRM-IIC-IOPV-F IN OCD-FLD(FRM-IFLD).                    
           MOVE WSS-NUM-IVCT-F-MTI TO                                   
                FRM-NUM-IVCT-F IN OCD-FLD(FRM-IFLD).                    
           MOVE WSS-COD-CCPT-F-MTI TO                                   
                FRM-COD-CCPT-F IN OCD-FLD(FRM-IFLD).                    
           MOVE WSS-COD-SCPT-F-MTI TO                                   
                FRM-COD-SCPT-F IN OCD-FLD(FRM-IFLD).                    
       FIN-PUT-TAB-OCD.                                                 
           EXIT.                                                        
      *----------------------------------------------------             
       PUT-OCD-QUE SECTION.                                             
       INI-PUT-OCD-QUE.                                                 
           IF (SCR-OFRM-SRV AND WSS-NITM > WSS-NITMMAX)                 
              GO TO FIN-PUT-OCD-QUE.                                    
                                                                        
           IF ((NOT SCR-OFRM-SRV) AND WSS-NITM > WSS-JMAX)              
                 GO TO FIN-PUT-OCD-QUE.                                 
                                                                        
           PERFORM PRO-FLD-QUE.                                         
           ADD 1 TO WSS-NITM.                                           
           MOVE OCD-NOM-TSOCD    TO QUE-COLA.                           
           MOVE OCD-LEN-TSOCD    TO QUE-LITM.                           
           MOVE WSS-NITM         TO QUE-NITM.                           
           MOVE SPACES           TO QUE-ITEM.                           
           IF SCR-OFRM-SRV                                              
              PERFORM EDT-VAL-VECT                                      
              MOVE WSS-NUM-IVCT-IDX-MTI  TO QUE-ITEM                    
           ELSE                                                         
              MOVE WSS-NUM-IVCT-IDX-FRM  TO QUE-ITEM.                   
           MOVE QUE-PUT  TO QUE-CMND.                                   
           PERFORM GNS-PRO-QUE.                                         
       FIN-PUT-OCD-QUE.                                                 
           EXIT.                                                        
      *----------------------------------------------------             
                                                                        
       PUT-TAB-ASC SECTION.                                             
       INI-PUT-TAB-ASC.                                                 
           MOVE SPACES TO MSG-COD-MENS.                                 
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.           
           MOVE 0 TO FRM-IFLD.                                          
       LUP-PUT-TAB-ASC.                                                 
           ADD 1              TO QUE-NITM.                              
           MOVE OCD-NOM-TSOCD TO QUE-COLA.                              
           MOVE OCD-LEN-TSOCD TO QUE-LITM.                              
           MOVE QUE-GET       TO QUE-CMND.                              
           PERFORM GNS-PRO-QUE.                                         
           IF QUE-STAT NOT = QUE-STAT-OKS                               
               IF FRM-IFLD = 0                                          
                   MOVE 'OCDTABNONXT' TO MSG-COD-MENS                   
                   GO TO FIN-PUT-TAB-ASC                                
               ELSE                                                     
                   GO TO FIN-PUT-TAB-ASC.                               
           ADD 1 TO FRM-IFLD.                                           
           MOVE QUE-ITEM TO FRM-NUM-IVCT-IDX(FRM-IFLD).                 
           IF FRM-IFLD < WSS-IMAX                                       
               GO TO LUP-PUT-TAB-ASC.                                   
       FIN-PUT-TAB-ASC.                                                 
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX                      
               ADD 1 FRM-IFLD GIVING WSS-I                              
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1            
                                 UNTIL WSS-I > WSS-IMAX.                
           IF FRM-IFLD > 0                                              
               ADD 1 TO SCR-NUM-PANT.                                   
           IF FRM-IFLD = WSS-IMAX                                       
               ADD 1              TO QUE-NITM                           
               MOVE OCD-NOM-TSOCD TO QUE-COLA                           
               MOVE OCD-LEN-TSOCD TO QUE-LITM                           
               MOVE QUE-GET       TO QUE-CMND                           
               PERFORM GNS-PRO-QUE                                      
               IF QUE-STAT = QUE-STAT-OKS                               
                   MOVE 'DEU'        TO MSG-COD-SIST                    
                   MOVE 'OCDTABDMAS' TO MSG-COD-MENS.                   
       EXT-PUT-TAB-ASC.                                                 
           EXIT.                                                        
                                                                        
       PUT-TAB-DSC SECTION.                                             
       INI-PUT-TAB-DSC.                                                 
           MOVE SPACES TO MSG-COD-MENS.                                 
           IF SCR-NUM-PANT = 1                                          
               MOVE 'OCDTABNOPRV' TO MSG-COD-MENS                       
               GO TO EXT-PUT-TAB-DSC.                                   
                                                                        
           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.                
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.               
           ADD 1 TO QUE-NITM.                                           
           ADD 1 WSS-IMAX GIVING FRM-IFLD.                              
       LUP-PUT-TAB-DSC.                                                 
           SUBTRACT  1 FROM QUE-NITM.                                   
           MOVE OCD-NOM-TSOCD TO QUE-COLA                               
           MOVE OCD-LEN-TSOCD TO QUE-LITM                               
           MOVE QUE-GET       TO QUE-CMND.                              
           PERFORM GNS-PRO-QUE.                                         
           IF QUE-STAT NOT = QUE-STAT-OKS                               
               IF FRM-IFLD > WSS-IMAX                                   
                   MOVE 'OCDTABNOPRV' TO MSG-COD-MENS                   
                   GO TO FIN-PUT-TAB-DSC                                
               ELSE                                                     
                   GO TO FIN-PUT-TAB-DSC.                               
           SUBTRACT 1 FROM FRM-IFLD.                                    
           MOVE QUE-ITEM TO FRM-NUM-IVCT-IDX(FRM-IFLD).                 
           IF FRM-IFLD > 1                                              
               GO TO LUP-PUT-TAB-DSC.                                   
       FIN-PUT-TAB-DSC.                                                 
                                                                        
