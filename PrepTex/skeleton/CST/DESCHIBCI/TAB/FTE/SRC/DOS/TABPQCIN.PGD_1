           IF CIN-MSC-STAT IN CIN = 'V'                                 
              IF SCR-CMND = 'ANL'                                       
                 MOVE 'CIN    ACANO' TO MSG-COD-MENS                    
                 GO TO SIG-FND-KEY.                                     
                                                                        
           IF  SCR-CMND = 'VRF'                                         
              IF CIN-MSC-STAT IN CIN = 'I' OR 'M'                       
                 GO TO FIN-FND-KEY                                      
              ELSE                                                      
                 MOVE 'CMN      INV' TO MSG-COD-MENS                    
                 GO TO SIG-FND-KEY.                                     
                                                                        
           IF  SCR-CMND = 'ACA'                                         
              IF CIN-MSC-STAT IN CIN = 'V'                              
                 GO TO FIN-FND-KEY                                      
              ELSE                                                      
                 MOVE 'CMN      INV' TO MSG-COD-MENS                    
                 GO TO SIG-FND-KEY.                                     
                                                                        
           IF  SCR-CMND = 'ANL'                                         
              IF CIN-MSC-STAT IN CIN = 'A'                              
                 GO TO FIN-FND-KEY                                      
              ELSE                                                      
                 MOVE 'CMN      INV' TO MSG-COD-MENS                    
                 GO TO SIG-FND-KEY.                                     
                                                                        
           IF  SCR-CMND = 'ELI'                                         
              IF CIN-MSC-STAT IN CIN = 'I' OR 'V'                       
