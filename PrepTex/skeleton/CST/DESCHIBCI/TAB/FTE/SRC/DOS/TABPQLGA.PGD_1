       ENVIRONMENT DIVISION.
      *=====================
       CONFIGURATION SECTION.
      *----------------------
       SPECIAL-NAMES.
               DECIMAL-POINT IS COMMA.
       DATA DIVISION.
      *==============

       WORKING-STORAGE SECTION.
      *------------------------
      * COPY GNSWGSCR.
       COPY GNSWGCVT.
       77  WSS-TAG-TABPQLGA-INI PIC X(125) VALUE                        
           '<IDNPGM NOM=$TABPQLGA$ SKL=$07.08.21$ VSN=${vsnpq}$        
      -    'GEN=$26-Apr-10 04:11 PM$ FNC=${fncpq}$/>'.    

       COPY GNSWCSCR.
      *PTC-INI
      *PTC-SUB
      * COPY GNSWVSCR.
       COPY GNSWV3CR.
      *PTC-FIN
      *<<<<
           03 SCR-NUM-PANT               PIC 9(0010).                   
           03 SCR-LGA-FLD                PIC X(1200).                   
      *>>>>
       COPY TABWILGA.

       COPY GNSBRLGA.


       COPY GNSWVIDD.
       COPY GNSWGRQA.
       COPY GNSWGUIB.
       COPY GNSWGELS.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-LGA-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-TAB-REQA==.
       COPY GNSBRTAB.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSG-REQA==.
       COPY GNSBRMSG.
       COPY GNSWCFIO.
       COPY GNSWVFIO.
      *PTC-INI
      *PTC-SUB
      * ISP  10-MAY-2001 13:46:35 
      * COPY GNSWGFRM.
       COPY GNSWG3CF.
       COPY GNSWGTSK.
       COPY GNSWGQUE.
      *PTC-FIN
       COPY GNSWVNUM.
       COPY GNSWCNUM.
       COPY GNSWGFEC.
       COPY GNSWGHOY.
       COPY GNSWGSYS.

      *<<<< WSS                                                         
       COPY TABWXLGA.                                                   
      *>>>>                                                             
       01  PGM-VARI.
           03 PGM-ICLR                  VALUE ' '       PIC X(01).
           03 PGM-LIMPIAR-TODO          VALUE ' '       PIC X(01).
           03 PGM-LIMPIAR-LLAVES        VALUE 'K'       PIC X(01).
           03 PGM-LIMPIAR-CAMPOS        VALUE 'F'       PIC X(01).
           03 PGM-NO-LIMPIAR-NADA       VALUE 'N'       PIC X(01).
           03 PGM-PROG                                  PIC X(08).
           03 PGM-NTRN                                  PIC X(04).
           03 PGM-STAT.
              05 PGM-STAT-LGA.
                 88 PGM-STAT-LGA-NUL    VALUE '  '.
                 07 PGM-STT1-LGA        VALUE '2' PIC X(01).
                    88 PGM-STAT-LGA-OKS VALUE '0'.
                 07 PGM-STT2-LGA        VALUE '3' PIC X(01).
           03 PGM-SRNF                  VALUE '23'      PIC X(02).
           03 PGM-SOKS                  VALUE '00'      PIC X(02).
           03 PGM-SNUL                  VALUE '  '      PIC X(02).
           03 PGM-SUAR                  VALUE SPACES    PIC X(15).
           03 PGM-CMMA.
              05 PGM-CMMA-CHR OCCURS 3000 PIC X(01).

       LINKAGE SECTION.
      *----------------
       01  DFHCOMMAREA.
           02 DFH-CMMA OCCURS 3000 PIC X(01).
       COPY GNSLGFIO.

       PROCEDURE DIVISION.
      *===================

       MAIN SECTION.
       INI-MAIN.
           PERFORM INI.
       LUP-MAIN.
           IF SCR-INST = SCR-INST-HCM
                 PERFORM SCR-COM.
           IF SCR-CCPP = 'ACC'
                 PERFORM SCR-ACC
           ELSE
                 MOVE SCR-INST-HCM TO SCR-INST.
           GO TO LUP-MAIN.
       FIN-MAIN.
           EXIT.

       INI SECTION.
       INI-INI.
       COPY GNSBGEIB.
       COPY GNSBGEDB.
           MOVE LOW-VALUES TO LGA-FLD.
           MOVE LOW-VALUES TO LGA-EDT.
           PERFORM GNS-PRO-RCK.
           PERFORM GNS-BUS-VCA.
           MOVE +3000 TO SCR-TCMA.
           PERFORM GNS-HDL-SYS.
           PERFORM GNS-HDL-FRM.
           MOVE 'TABPQLGA' TO FIO-PROG.
           PERFORM GNS-BUS-IDD.
      *PTC-INI
           PERFORM GNS-ERR-QUE.
           PERFORM GET-TSK-TERM.
      *PTC-FIN
      *<<<< INI_FIO                                                     
           IF SCR-OFRM-SRV                                              
                MOVE TSK-TERM-ALF TO LGA-COD-TERM                       
                MOVE TSK-TERM-ALF TO LGA-COD-TERM-MAS                   
           ELSE                                                         
                MOVE EIBTRMID     TO LGA-COD-TERM                       
                MOVE EIBTRMID     TO LGA-COD-TERM-MAS.                  
           MOVE +3859 TO SCR-TCMA.                                      
           PERFORM GNS-ERR-QUE.                                         
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )      
               MOVE SCR-LGA-FLD TO LGA-FLD                              
               GO TO FIN-INI.                                           
                                                                        
           IF ( FRM-FFLD = FRM-FFLD-PF6 )                               
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM        
               ADD 1              TO QUE-NITM                           
               MOVE LGA-NOM-TSLGA TO QUE-COLA                           
               MOVE LGA-LEN-TSLGA TO QUE-LITM                           
               MOVE QUE-GET       TO QUE-CMND                           
               PERFORM GNS-PRO-QUE                                      
               IF QUE-STAT NOT = QUE-STAT-OKS                           
                  MOVE SCR-LGA-FLD TO LGA-FLD                           
                  GO TO FIN-INI.                                        
           PERFORM GET-FHOY.                                            
      *>>>>                                                             
       FIN-INI.
           EXIT.

       COPY GNSBHSYS.
       COPY GNSBHFRM.
       COPY GNSBPRCK.
       GNS-BUS-VCA SECTION.
       INI-GNS-BUS-VCA.
           MOVE 0 TO FRM-IFLD.
       LUP-GNS-BUS-VCA.
           ADD 1 TO FRM-IFLD.
           IF FRM-IFLD NOT > EIBCALEN AND
              FRM-IFLD NOT > 3000
                MOVE DFH-CMMA(FRM-IFLD) TO PGM-CMMA-CHR(FRM-IFLD)
                GO TO LUP-GNS-BUS-VCA.
           MOVE PGM-CMMA TO SCR-VARI.
       FIN-GNS-BUS-VCA.
           EXIT.
       COPY GNSBBIDD.

       SCR-COM SECTION.
       INI-SCR-COM.
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-CLRA-LGA.
           PERFORM EDT-KEY-LGA.
           PERFORM EDT-FLD-LGA.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-HCM TO SCR-INST.
           MOVE SCR-CMNP     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       XCT-ERR.
           MOVE 'XCTERR    ' TO MSG-COD-MENS.
           MOVE 'GNS'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO SYS-MEN1.
           MOVE SYS-PROG     TO SYS-MEN2.
           MOVE SYS-ABOR     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       COPY GNSBGGBK.
       FIN-SCR-COM.
           EXIT.



       SCR-ACC SECTION.
       INI-SCR-ACC.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ACC
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-ACC.
           PERFORM DESSCRTRL.
      *<<<< PFS_SCR_ACC                                                 
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6                   
               PERFORM VAL-NUM-KEY-LGA                                  
               PERFORM VAL-NUM-FLD-LGA                                  
               PERFORM SCRL-TAB-LGA                                     
               PERFORM SCR-TRL.                                         
      *>>>>                                                             
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ACC.
           EXIT.

       BRW-SCR-ACC SECTION.
       INI-BRW-SCR-ACC.
       FIN-BRW-SCR-ACC.
           EXIT.




       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               PERFORM SCR-CLRA-LGA
               PERFORM EDT-KEY-LGA
               PERFORM EDT-FLD-LGA
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-LGA
               PERFORM EDT-KEY-LGA
           ELSE
               PERFORM CLR-FLD-LGA
               PERFORM EDT-FLD-LGA.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

       SCR-KEY SECTION.
       INI-SCR-KEY.
           PERFORM SCR-RST.
           PERFORM GET-KEY.
           IF FRM-STAT-ABT
                   MOVE SPACES TO FRM-MENS
                   PERFORM SCR-MSSG
                   GO TO FIN-SCR-KEY.
       FND-SCR-KEY.
           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
           IF FIO-STAT-OKS
               PERFORM PUT-SCR
               MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
               GO TO FIN-SCR-KEY.
      *JSS ELSE
      *        PERFORM CLR-FLD-LGA.
       ERR-SCR-KEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR.
           IF FRM-MENS NOT > SPACES
               IF FIO-STAT-EOF
                   MOVE 'REGEOF    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
               IF FIO-STAT = FIO-STAT-NEX
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
           MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-SCR-KEY.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO LGA-FLD.
           MOVE LOW-VALUES TO LGA-EDT.
       FIN-SCR-RST.
           EXIT.

       GET-KEY SECTION.
       INI-GET-KEY.
           PERFORM SCR-GKEY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-KEY.
           EXIT.

       GET-IKY SECTION.
       INI-GET-IKY.
           PERFORM SCR-GIKY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-IKY.
           EXIT.

       FND-KEY SECTION.
       INI-FND-KEY.
      *<<<< INI_FND_KEY                                                 
           IF FRM-IND-NEXT IN LGA-FLD = 'S'                             
               MOVE FRM-COD-SIST IN LGA-FLD TO FIO-SIST                 
               MOVE LGA-COD-IENT IN LGA-FLD TO LGA-COD-IENT IN LGA      
               MOVE LGA-CAI-IOPE IN LGA-FLD TO LGA-CAI-IOPE IN LGA      
               MOVE LGA-IIC-IOPE IN LGA-FLD TO LGA-IIC-IOPE IN LGA      
               MOVE FRM-NUM-IOPE IN LGA-FLD TO LGA-NUM-IOPE IN LGA      
               MOVE FRM-CAI-AOPE IN LGA-FLD TO LGA-CAI-AOPE IN LGA      
               MOVE FRM-IIC-AOPE IN LGA-FLD TO LGA-IIC-AOPE IN LGA      
               MOVE FRM-IND-IACC IN LGA-FLD TO LGA-IND-IACC IN LGA      
               MOVE FRM-NUM-DACC IN LGA-FLD TO LGA-NUM-DACC IN LGA      
               MOVE FRM-NUM-MACC IN LGA-FLD TO LGA-NUM-MACC IN LGA      
               MOVE FRM-NUM-SACC IN LGA-FLD TO LGA-NUM-SACC IN LGA      
               MOVE FRM-NUM-AACC IN LGA-FLD TO LGA-NUM-AACC IN LGA      
               MOVE FRM-HRA-HACC IN LGA-FLD TO LGA-HRA-HACC IN LGA      
           ELSE                                                         
               MOVE FRM-COD-SIST IN LGA-FLD TO FIO-SIST                 
               MOVE LGA-COD-IENT IN LGA-FLD TO LGA-COD-IENT IN LGA      
               MOVE LGA-CAI-IOPE IN LGA-FLD TO LGA-CAI-IOPE IN LGA      
               MOVE LGA-IIC-IOPE IN LGA-FLD TO LGA-IIC-IOPE IN LGA      
               MOVE LGA-NUM-IOPE IN LGA-FLD TO LGA-NUM-IOPE IN LGA      
               MOVE LGA-CAI-AOPE IN LGA-FLD TO LGA-CAI-AOPE IN LGA      
               MOVE LGA-IIC-AOPE IN LGA-FLD TO LGA-IIC-AOPE IN LGA      
               MOVE LGA-IND-IACC IN LGA-FLD TO LGA-IND-IACC IN LGA.     
                                                                        
           MOVE ZEROES TO WSS-J IN WSS-VARI.                            
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-LEE-LGA.                                                     
           MOVE 'MC2' TO FIO-SIST.                                      
           PERFORM GNS-FIO-LGA.                                         
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF                          
               GO TO CON-FND-KEY.                                       
           IF NOT (LGA-COD-IENT IN LGA = LGA-COD-IENT IN LGA-FLD AND    
                   LGA-CAI-IOPE IN LGA = LGA-CAI-IOPE IN LGA-FLD AND    
                   LGA-IIC-IOPE IN LGA = LGA-IIC-IOPE IN LGA-FLD)       
              GO TO CON-FND-KEY.                                        
                                                                        
           IF FRM-COD-IADC IN LGA-FLD > SPACES                          
              IF LGA-COD-INFA IN LGA NOT = FRM-COD-IADC IN LGA-FLD      
                 GO TO NXT-LEE-LGA.                                     
                                                                        
           PERFORM CRG-VEC-LGA.                                         
                                                                        
           IF (SCR-OFRM-SRV AND WSS-J > WSS-NITMMAX)                    
              GO TO CON-FND-KEY.                                        
                                                                        
           IF ((NOT SCR-OFRM-SRV) AND WSS-J > WSS-JMAX)                 
                 GO TO CON-FND-KEY.                                     
       NXT-LEE-LGA.                                                     
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-LEE-LGA.                                           
      *>>>>                                                             
           PERFORM KEY-ALL-LGA.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-LGA.
           IF FIO-STAT-OKS
               MOVE LGA-STP-ITRN IN LGA TO SCR-UMOD.
           MOVE FIO-STAT TO PGM-STAT-LGA.
      *<<<< FIN_FND_KEY                                                 
       CON-FND-KEY.                                                     
           PERFORM PUT-TAB.                                             
                                                                        
           MOVE '00' TO FIO-STAT.                                       
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           GO TO FIN-FND-KEY.                                           
      *----------------------------------------------------------       
       MSG-FND-KEY.                                                     
           MOVE 'TAB' TO MSG-COD-SIST.                                  
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MENS.                               
           MOVE '00' TO FIO-STAT.                                       
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           GO TO FIN-FND-KEY.                                           
      *>>>>                                                             
       FIN-FND-KEY.
           IF FIO-STAT = FIO-STAT-NEX
               MOVE 'S' TO SCR-IERR.
       EXT-FND-KEY.
           EXIT.
 
       PUT-SCR SECTION.
       INI-PUT-SCR.
           IF PGM-STAT-LGA-OKS
               MOVE LGA-STP-ITRN IN LGA TO SCR-UMOD.
           PERFORM PUT-ALL-LGA.
       FIN-PUT-SCR.
           EXIT.

       STP-EXIT SECTION.
       INI-STP-EXIT.
           MOVE SCR-PROG TO PGM-PROG.
           MOVE SCR-NTRN TO PGM-NTRN.

           MOVE SCR-PROV TO SCR-PROG.
           MOVE SCR-NTRV TO SCR-NTRN.

           MOVE PGM-PROG TO SCR-PROV
           MOVE PGM-NTRN TO SCR-NTRV.
       FIN-STP-EXIT.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO SYS-NTRN.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
      *PTC-INI
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE LGA-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO LGA-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GKEY SECTION.
       INI-SCR-GKEY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-LGA
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO LGA-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GKEY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-LGA.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-LGA
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-LGA.
           PERFORM VAL-IKY-LGA.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-MKY-LGA.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-AKY-LGA.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-CON-KEY-LGA.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GKEY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-LGA.
       ERR-SCR-GKEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-LGA.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GKEY.
           PERFORM SCR-MSSG.

       SCR-GIKY SECTION.
       INI-SCR-GIKY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-LGA
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO LGA-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GIKY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-LGA.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-IKY-LGA
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-LGA.
           PERFORM VAL-IKY-LGA.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GIKY.
           PERFORM VAL-CON-IKY-LGA.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GIKY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-LGA.
       ERR-SCR-GIKY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-IKY-LGA.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GIKY.
           PERFORM SCR-MSSG.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
      *<<<< PFS_GET_CON                                                 
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )           
      *>>>>                                                             
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO LGA-GLS-MEN2
               MOVE SCR-PRMT     TO LGA-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO LGA-FLD.
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO LGA-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE LGA-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO LGA-FLD
      * FLB  7-NOV-1994
               PERFORM CLR-KEY-LGA

               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO LGA-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO LGA-GLS-MENS.
           PERFORM PRO-MKY-LGA.
           PERFORM PRO-AKY-LGA.
           PERFORM UNP-IKY-LGA.
           PERFORM FST-KEY-LGA.
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE LGA-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO LGA-FLD
               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO LGA-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO LGA-GLS-MENS.
           PERFORM UNP-KEY-LGA.
           PERFORM FST-KEY-LGA.
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
           PERFORM PRO-IKY-LGA.
           PERFORM PRO-AKY-LGA.
           PERFORM UNP-MKY-LGA.
           PERFORM UNP-FLD-LGA.
           PERFORM FST-FLD-LGA.
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO LGA-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-LGA.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
      *PTC-INI
           IF SCR-STAT NOT = SCR-STPP
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-LGA
               PERFORM EDT-FLD-LGA
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-LGA
               PERFORM EDT-FLD-LGA
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-LGA.
           PERFORM EDT-FLD-LGA.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

      * PGM-EBR-ALL SECTION.
      * INI-PGM-EBR-ALL.
      *     MOVE 'GNS' TO FIO-SIST.
      *     MOVE FIO-EBR-ALL TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
      * FIN-PGM-EBR-ALL.
      *     EXIT.

       COPY TABBQLGA.
       COPY GNSBVNUM.
       COPY GNSBFLGA.
       COPY GNSBFMSG.
       COPY GNSBGSYS.
       COPY GNSBGDTC.
      *PTC-INI
      *PTC-SUB
      * ISP  10-MAY-2001 13:46:35 
      * COPY GNSBGFRM.
       COPY GNSBG3CF.
      *PTC-FIN
       COPY GNSBGMSG.
       COPY GNSBEKEY.
       COPY GNSBIABT.
       COPY GNSBFTAB.
       COPY GNSBEGLS.
       COPY GNSBGFEC.
       COPY GNSBGHOY.
      *PTC-INI
       COPY GNSBGQUE.
       COPY GNSBGTSK.
      *PTC-FIN

      *<<<< EOF                                                         
       PGM-EBR-ALL SECTION.                                             
       INI-PGM-EBR-ALL.                                                 
           MOVE 'GNS' TO FIO-SIST.                                      
           MOVE FIO-EBR-ALL TO FIO-CMND.                                
           PERFORM GNS-FIO-TAB.                                         
       FIN-PGM-EBR-ALL.                                                 
           EXIT.                                                        

       COPY GNSBGVSM.                                                   
                                                                        
       COPY GNSBBTAB.                                                   
       COPY GNSBVTAB.                                                   
       COPY GNSBVCOD.                                                   
       COPY GNSBPFEC.                                                   
                                                                        
       PUT-TAB SECTION.                                                 
       INI-PUT-TAB.                                                     
           MOVE LGA-NOM-TSLGA     TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE LGA-NOM-TSLGA-MAS TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE 1 TO SCR-NUM-PANT.                                      
           MOVE LOW-VALUES TO WSS-IND-ACCR-IDX-MTI.                     
           MOVE LOW-VALUES TO WSS-IND-ACCR-IDX-FRM.                     
      *                                                                 
           MOVE 1 TO FRM-IFLD.                                          
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.                        
                                                                        
           MOVE 1 TO WSS-F IN WSS-VARI.                                 
       LUP-PUT-TAB.                                                     
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI                  
              GO TO FIN-PUT-TAB.                                        
           IF WSS-IND-ACCR-VEC(WSS-F) > SPACES                          
              PERFORM PUT-TAB-LGA                                       
              PERFORM PUT-LGA-QUE                                       
              PERFORM SET-FRM-IFLD                                      
           ELSE                                                         
           IF FRM-IFLD = 1 AND WSS-F = 1                                
              MOVE 'TAB' TO MSG-COD-SIST                                
              MOVE 'LGA    NEX' TO MSG-COD-MENS                         
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              MOVE '00' TO FIO-STAT                                     
      *       MOVE 'S' TO SCR-IERR                                      
              GO TO FIN-PUT-TAB                                         
           ELSE                                                         
              MOVE '00' TO FIO-STAT                                     
              GO TO FIN-PUT-TAB.                                        
                                                                        
           ADD 1 TO WSS-F IN WSS-VARI.                                  
           GO TO LUP-PUT-TAB.                                           
       FIN-PUT-TAB.                                                     
           MOVE 1                  TO QUE-NITM.                         
           MOVE LGA-LEN-TSLGA-MAS  TO QUE-LITM.                         
           MOVE LGA-NOM-TSLGA-MAS  TO QUE-COLA.                         
                                                                        
           IF WSS-NITM > WSS-NITMMAX                                    
              MOVE 'S'           TO LGA-MAS-IND-HMAS                    
           ELSE                                                         
              MOVE 'N'           TO LGA-MAS-IND-HMAS.                   
                                                                        
           MOVE WSS-NITM         TO WSS-NUM-ICUR.                       
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.                   
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.                   
           MOVE WSS-NUM-ICUR-ALF TO LGA-MAS-NUM-TOCC.                   
           MOVE LGA-MAS-ITEM     TO QUE-ITEM.                           
           MOVE QUE-PUT          TO QUE-CMND.                           
           PERFORM GNS-PRO-QUE.                                         
           IF WSS-NITM > WSS-IMAX                                       
               MOVE 'TAB' TO MSG-COD-SIST                               
               MOVE 'LGATABDMAS' TO MSG-COD-MENS                        
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN2.                           
       EXT-PUT-TAB.                                                     
           EXIT.                                                        
                                                                        
       SET-FRM-IFLD SECTION.                                            
       INI-SET-FRM-IFLD.                                                
           IF SCR-OFRM-SRV                                              
                MOVE 1 TO FRM-IFLD                                      
           ELSE                                                         
                ADD 1 TO FRM-IFLD.                                      
       FIN-SET-FRM-IFLD.                                                
           EXIT.                                                        
      *--------------------------------------------                     
       SCRL-TAB-LGA SECTION.                                            
       INI-SCRL-TAB-LGA.                                                
           MOVE SPACES TO MSG-COD-MENS.                                 
                                                                       
           IF FRM-FFLD = FRM-FFLD-PF6                                   
               PERFORM PUT-TAB-ASC                                      
           ELSE                                                         
               PERFORM PUT-TAB-DSC.                                     
           IF MSG-COD-MENS > SPACES                                     
               MOVE 'TAB' TO MSG-COD-SIST                               
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN2.                           
       FIN-SCRL-TAB-LGA.                                                
           EXIT.                                                        
                                                                        
      *--------------------------------------------                     
       PUT-TAB-LGA SECTION.                                             
       INI-PUT-TAB-LGA.                                                 
           PERFORM CLR-WSS.                                             
           MOVE WSS-IND-ACCR-VEC(WSS-F) TO WSS-IND-ACCR-MTI IN WSS-VARI.
           MOVE WSS-NUM-DCCR-VEC(WSS-F) TO WSS-NUM-DCCR-MTI IN WSS-VARI.
           MOVE WSS-NUM-MCCR-VEC(WSS-F) TO WSS-NUM-MCCR-MTI IN WSS-VARI.
           MOVE WSS-NUM-SCCR-VEC(WSS-F) TO WSS-NUM-SCCR-MTI IN WSS-VARI.
           MOVE WSS-NUM-ACCR-VEC(WSS-F) TO WSS-NUM-ACCR-MTI IN WSS-VARI.
           MOVE WSS-HRA-ACCR-VEC(WSS-F) TO WSS-HRA-ACCR-MTI IN WSS-VARI.
           MOVE WSS-COD-OTRN-VEC(WSS-F) TO WSS-COD-OTRN-MTI IN WSS-VARI.
           MOVE WSS-COD-ATRN-VEC(WSS-F) TO WSS-COD-ATRN-MTI IN WSS-VARI.
           MOVE WSS-COD-INFA-VEC(WSS-F) TO WSS-COD-INFA-MTI IN WSS-VARI.
                                                                        
           MOVE WSS-IND-ACCR-MTI IN WSS-VARI TO WSS-IND-ACCR-FRM.       
           MOVE WSS-NUM-DCCR-MTI IN WSS-VARI TO WSS-NUM-DCCR-FRM.       
           MOVE WSS-NUM-MCCR-MTI IN WSS-VARI TO WSS-NUM-MCCR-FRM.       
           MOVE WSS-NUM-SCCR-MTI IN WSS-VARI TO WSS-NUM-SCCR-FRM.       
           MOVE WSS-NUM-ACCR-MTI IN WSS-VARI TO WSS-NUM-ACCR-FRM.       
           MOVE WSS-HRA-ACCR-MTI IN WSS-VARI TO WSS-HRA-ACCR-FRM.       
           MOVE WSS-COD-OTRN-MTI IN WSS-VARI TO WSS-COD-OTRN-FRM.       
           MOVE WSS-COD-ATRN-MTI IN WSS-VARI TO WSS-COD-ATRN-FRM.       
           MOVE WSS-COD-INFA-MTI IN WSS-VARI TO WSS-COD-INFA-FRM.       
                                                                        
           IF FRM-IFLD > WSS-IMAX                                       
              GO TO FIN-PUT-TAB-LGA.                                    
                                                                        
           MOVE WSS-IND-ACCR-MTI IN WSS-VARI TO                         
                FRM-IND-ACCR IN LGA-FLD(FRM-IFLD).                      
           MOVE WSS-NUM-DCCR-MTI IN WSS-VARI TO                         
                FRM-NUM-DCCR IN LGA-FLD(FRM-IFLD).                      
           MOVE WSS-NUM-MCCR-MTI IN WSS-VARI TO                         
                FRM-NUM-MCCR IN LGA-FLD(FRM-IFLD).                      
           MOVE WSS-NUM-SCCR-MTI IN WSS-VARI TO                         
                FRM-NUM-SCCR IN LGA-FLD(FRM-IFLD).                      
           MOVE WSS-NUM-ACCR-MTI IN WSS-VARI TO                         
                FRM-NUM-ACCR IN LGA-FLD(FRM-IFLD).                      
           MOVE WSS-HRA-ACCR-MTI IN WSS-VARI TO                         
                FRM-HRA-ACCR IN LGA-FLD(FRM-IFLD).                      
           MOVE WSS-COD-OTRN-MTI IN WSS-VARI TO                         
                FRM-COD-OTRN IN LGA-FLD(FRM-IFLD).                      
           MOVE WSS-COD-ATRN-MTI IN WSS-VARI TO                         
                FRM-COD-ATRN IN LGA-FLD(FRM-IFLD).                      
           MOVE WSS-COD-INFA-MTI IN WSS-VARI TO                         
                FRM-COD-INFA IN LGA-FLD(FRM-IFLD).                      
       FIN-PUT-TAB-LGA.                                                 
           EXIT.                                                        
      *----------------------------------------------------             
       PUT-LGA-QUE SECTION.                                             
       INI-PUT-LGA-QUE.                                                 
           PERFORM PRO-FLD-QUE.                                         
           ADD 1 TO WSS-NITM.                                           
           MOVE LGA-NOM-TSLGA    TO QUE-COLA.                           
           MOVE LGA-LEN-TSLGA    TO QUE-LITM.                           
           MOVE WSS-NITM         TO QUE-NITM.                           
           MOVE SPACES           TO QUE-ITEM.                           
           IF SCR-OFRM-SRV                                              
              MOVE WSS-IND-ACCR-IDX-MTI  TO QUE-ITEM                    
           ELSE                                                         
              MOVE WSS-IND-ACCR-IDX-FRM  TO QUE-ITEM.                   
           MOVE QUE-PUT  TO QUE-CMND.                                   
           PERFORM GNS-PRO-QUE.                                         
       FIN-PUT-LGA-QUE.                                                 
           EXIT.                                                        
      *----------------------------------------------------             
                                                                        
       PUT-TAB-ASC SECTION.                                             
       INI-PUT-TAB-ASC.                                                 
           MOVE SPACES TO MSG-COD-MENS.                                 
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.           
           MOVE 0 TO FRM-IFLD.                                          
       LUP-PUT-TAB-ASC.                                                 
           ADD 1              TO QUE-NITM.                              
           MOVE LGA-NOM-TSLGA TO QUE-COLA.                              
           MOVE LGA-LEN-TSLGA TO QUE-LITM.                              
           MOVE QUE-GET       TO QUE-CMND.                              
           PERFORM GNS-PRO-QUE.                                         
           IF QUE-STAT NOT = QUE-STAT-OKS                               
               IF FRM-IFLD = 0                                          
                   MOVE 'LGATABNONXT' TO MSG-COD-MENS                   
                   GO TO FIN-PUT-TAB-ASC                                
               ELSE                                                     
                   GO TO FIN-PUT-TAB-ASC.                               
           ADD 1 TO FRM-IFLD.                                           
           MOVE QUE-ITEM TO FRM-IND-ACCR-IDX(FRM-IFLD).                 
           IF FRM-IFLD < WSS-IMAX                                       
               GO TO LUP-PUT-TAB-ASC.                                   
       FIN-PUT-TAB-ASC.                                                 
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX                      
               ADD 1 FRM-IFLD GIVING WSS-I                              
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1            
                                 UNTIL WSS-I > WSS-IMAX.                
           IF FRM-IFLD > 0                                              
               ADD 1 TO SCR-NUM-PANT.                                   
           IF FRM-IFLD = WSS-IMAX                                       
               ADD 1              TO QUE-NITM                           
               MOVE LGA-NOM-TSLGA TO QUE-COLA                           
               MOVE LGA-LEN-TSLGA TO QUE-LITM                           
               MOVE QUE-GET       TO QUE-CMND                           
               PERFORM GNS-PRO-QUE                                      
               IF QUE-STAT = QUE-STAT-OKS                               
                   MOVE 'TAB'        TO MSG-COD-SIST                    
                   MOVE 'LGATABDMAS' TO MSG-COD-MENS.                   
       EXT-PUT-TAB-ASC.                                                 
           EXIT.                                                        
                                                                        
       PUT-TAB-DSC SECTION.                                             
       INI-PUT-TAB-DSC.                                                 
           MOVE SPACES TO MSG-COD-MENS.                                 
           IF SCR-NUM-PANT = 1                                          
               MOVE 'LGATABNOPRV' TO MSG-COD-MENS                       
               GO TO EXT-PUT-TAB-DSC.                                   
                                                                        
           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.                
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.               
           ADD 1 TO QUE-NITM.                                           
           ADD 1 WSS-IMAX GIVING FRM-IFLD.                              
       LUP-PUT-TAB-DSC.                                                 
           SUBTRACT  1 FROM QUE-NITM.                                   
           MOVE LGA-NOM-TSLGA TO QUE-COLA                               
           MOVE LGA-LEN-TSLGA TO QUE-LITM                               
           MOVE QUE-GET       TO QUE-CMND.                              
           PERFORM GNS-PRO-QUE.                                         
           IF QUE-STAT NOT = QUE-STAT-OKS                               
               IF FRM-IFLD > WSS-IMAX                                   
                   MOVE 'LGATABNOPRV' TO MSG-COD-MENS                   
                   GO TO FIN-PUT-TAB-DSC                                
               ELSE                                                     
                   GO TO FIN-PUT-TAB-DSC.                               
           SUBTRACT 1 FROM FRM-IFLD.                                    
           MOVE QUE-ITEM TO FRM-IND-ACCR-IDX(FRM-IFLD).                 
           IF FRM-IFLD > 1                                              
               GO TO LUP-PUT-TAB-DSC.                                   
       FIN-PUT-TAB-DSC.                                                 
                                                                        
           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1                  
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I                    
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1           
                                 UNTIL WSS-I < 1.                       
                                                                        
           IF FRM-IFLD NOT > WSS-IMAX                                   
               SUBTRACT 1 FROM SCR-NUM-PANT                             
               MOVE 'TAB'        TO MSG-COD-SIST                        
               MOVE 'LGATABDMAS' TO MSG-COD-MENS.                       
       EXT-PUT-TAB-DSC.                                                 
           EXIT.                                                        
                                                                        
                                                                        
       CLR-FRM SECTION.                                                 
       INI-CLR-FRM.                                                     
           MOVE SPACES TO FRM-IND-ACCR IN LGA-FLD(WSS-I).               
           MOVE ZEROES TO FRM-NUM-DCCR IN LGA-FLD(WSS-I).               
           MOVE ZEROES TO FRM-NUM-MCCR IN LGA-FLD(WSS-I).               
           MOVE ZEROES TO FRM-NUM-SCCR IN LGA-FLD(WSS-I).               
           MOVE ZEROES TO FRM-NUM-ACCR IN LGA-FLD(WSS-I).               
           MOVE ZEROES TO FRM-HRA-ACCR IN LGA-FLD(WSS-I).               
           MOVE SPACES TO FRM-COD-OTRN IN LGA-FLD(WSS-I).               
           MOVE SPACES TO FRM-COD-ATRN IN LGA-FLD(WSS-I).               
           MOVE SPACES TO FRM-COD-INFA IN LGA-FLD(WSS-I).               
       FIN-CLR-FRM.                                                     
           EXIT.                                                        
                                                                        
       CLR-WSS SECTION.                                                 
       INI-CLR-WSS.                                                     
           MOVE SPACES TO WSS-IND-ACCR-MTI IN WSS-VARI.                 
           MOVE ZEROES TO WSS-FEC-ACCR-MTI IN WSS-VARI.                 
           MOVE ZEROES TO WSS-HRA-ACCR-MTI IN WSS-VARI.                 
           MOVE SPACES TO WSS-COD-OTRN-MTI IN WSS-VARI.                 
           MOVE SPACES TO WSS-COD-ATRN-MTI IN WSS-VARI.                 
           MOVE SPACES TO WSS-COD-INFA-MTI IN WSS-VARI.                 
                                                                        
           MOVE SPACES TO WSS-IND-ACCR-FRM IN WSS-VARI.                 
           MOVE ZEROES TO WSS-FEC-ACCR-FRM IN WSS-VARI.                 
           MOVE ZEROES TO WSS-HRA-ACCR-FRM IN WSS-VARI.                 
           MOVE SPACES TO WSS-COD-OTRN-FRM IN WSS-VARI.                 
           MOVE SPACES TO WSS-COD-ATRN-FRM IN WSS-VARI.                 
           MOVE SPACES TO WSS-COD-INFA-FRM IN WSS-VARI.                 
                                                                        
           IF SCR-OFRM-SRV                                              
               PERFORM CLR-FRM-MTX.                                     
       FIN-CLR-WSS.                                                     
           EXIT.                                                        
                                                                        
       CLR-FRM-MTX SECTION.                                             
       INI-CLR-FRM-MTX.                                                 
           MOVE SPACES TO FRM-IND-ACCR IN LGA-FLD(1).                   
           MOVE ZEROES TO FRM-NUM-DCCR IN LGA-FLD(1).                   
           MOVE ZEROES TO FRM-NUM-MCCR IN LGA-FLD(1).                   
           MOVE ZEROES TO FRM-NUM-SCCR IN LGA-FLD(1).                   
           MOVE ZEROES TO FRM-NUM-ACCR IN LGA-FLD(1).                   
           MOVE ZEROES TO FRM-HRA-ACCR IN LGA-FLD(1).                   
           MOVE SPACES TO FRM-COD-OTRN IN LGA-FLD(1).                   
           MOVE SPACES TO FRM-COD-ATRN IN LGA-FLD(1).                   
           MOVE SPACES TO FRM-COD-INFA IN LGA-FLD(1).                   
       FIN-CLR-FRM-MTX.                                                 
           EXIT.                                                        
                                                                        
       CRG-VEC-LGA SECTION.                                             
       INI-CRG-VEC-LGA.                                                 
           ADD 1 TO WSS-J.                                              
           IF WSS-J IN WSS-VARI > WSS-JMAX IN WSS-VARI                  
               GO TO FIN-CRG-VEC-LGA.                                   
                                                                        
           PERFORM CLR-LGA-VEC.                                         
                                                                        
           MOVE LGA-IND-IACC IN LGA TO WSS-IND-ACCR-VEC(WSS-J).         
           MOVE LGA-NUM-DACC IN LGA TO WSS-NUM-DCCR-VEC(WSS-J).         
           MOVE LGA-NUM-MACC IN LGA TO WSS-NUM-MCCR-VEC(WSS-J).         
           MOVE LGA-NUM-SACC IN LGA TO WSS-NUM-SCCR-VEC(WSS-J).         
           MOVE LGA-NUM-AACC IN LGA TO WSS-NUM-ACCR-VEC(WSS-J).         
           MOVE LGA-HRA-HACC IN LGA TO WSS-HRA-ACCR-VEC(WSS-J).         
           MOVE LGA-COD-OTRN IN LGA TO WSS-COD-OTRN-VEC(WSS-J).         
           MOVE LGA-COD-ATRN IN LGA TO WSS-COD-ATRN-VEC(WSS-J).         
           MOVE LGA-COD-INFA IN LGA TO WSS-COD-INFA-VEC(WSS-J).         
       FIN-CRG-VEC-LGA.                                                 
           EXIT.                                                        
                                                                        
       CLR-LGA-VEC SECTION.                                             
       INI-CLR-LGA-VEC.                                                 
           MOVE SPACES TO WSS-IND-ACCR-VEC(WSS-J).                      
           MOVE ZEROES TO WSS-FEC-ACCR-VEC(WSS-J).                      
           MOVE ZEROES TO WSS-HRA-ACCR-VEC(WSS-J).                      
           MOVE SPACES TO WSS-COD-OTRN-VEC(WSS-J).                      
           MOVE SPACES TO WSS-COD-ATRN-VEC(WSS-J).                      
           MOVE SPACES TO WSS-COD-INFA-VEC(WSS-J).                      
       FIN-CLR-LGA-VEC.                                                 
           EXIT.                                                        
                                                                        
       PRO-FLD-QUE SECTION.                                             
       INI-PRO-FLD-QUE.                                                 
           MOVE 'P' TO WSS-IND-ACCR-MTI-ATR.                            
           MOVE 'P' TO WSS-FEC-ACCR-MTI-ATR.                            
           MOVE 'P' TO WSS-HRA-ACCR-MTI-ATR.                            
