       RET-ALL-RES SECTION.
       INI-RET-ALL-RES.
           PERFORM KEY-ALL-RES.
           PERFORM RET-RES-RES.
           PERFORM RET-USR-RES.
       FIN-RET-ALL-RES.
           EXIT.
       RET-RES-RES SECTION.
       INI-RET-RES-RES.
           MOVE RES-GLS-DESC IN RES-FLD TO RES-GLS-DESC IN RES.
           MOVE RES-COD-ATRN IN RES-FLD TO RES-COD-ATRN IN RES.
           MOVE RES-NUM-DTRN IN RES-FLD TO RES-NUM-DTRN IN RES.
           MOVE RES-NUM-MTRN IN RES-FLD TO RES-NUM-MTRN IN RES.
           MOVE RES-NUM-STRN IN RES-FLD TO RES-NUM-STRN IN RES.
           MOVE RES-NUM-ATRN IN RES-FLD TO RES-NUM-ATRN IN RES.
       FIN-RET-RES-RES.
           EXIT.
       RET-USR-RES SECTION.
       INI-RET-USR-RES.
       FIN-RET-USR-RES.
           EXIT.
       KEY-ALL-RES SECTION.
       INI-KEY-ALL-RES.
           PERFORM KEY-RES-RES.
           PERFORM KEY-USR-RES.
       FIN-KEY-ALL-RES.
           EXIT.
       KEY-RES-RES SECTION.
       INI-KEY-RES-RES.
           MOVE RES-COD-TABL IN RES-FLD TO RES-COD-TABL IN RES.
       FIN-KEY-RES-RES.
           EXIT.
       KEY-USR-RES SECTION.
       INI-KEY-USR-RES.
           MOVE USR-COD-USER IN RES-FLD TO USR-COD-USER IN USR.
       FIN-KEY-USR-RES.
           EXIT.
       SCR-CLRA-RES SECTION.
       INI-SCR-CLRA-RES.
           PERFORM CLR-KEY-RES.
           PERFORM CLR-FLD-RES.
       FIN-SCR-CLRA-RES.
           EXIT.
       CLR-KEY-RES SECTION.
       INI-CLR-KEY-RES.
           MOVE SPACES TO RES-COD-TABL IN RES-FLD.
           MOVE SPACES TO USR-COD-USER IN RES-FLD.
       FIN-CLR-KEY-RES.
           EXIT.
       CLR-FLD-RES SECTION.
       INI-CLR-FLD-RES.
           MOVE SPACES TO FRM-GLS-USER IN RES-FLD.
           MOVE SPACES TO FRM-GLS-DUSR IN RES-FLD.
           PERFORM CLR-RES-RES.
           PERFORM CLR-USR-RES.
       FIN-CLR-FLD-RES.
           EXIT.
       CLR-RES-RES SECTION.
       INI-CLR-RES-RES.
           MOVE SPACES TO RES-GLS-DESC IN RES-FLD.
           MOVE SPACES TO RES-COD-ATRN IN RES-FLD.
           MOVE SPACES TO RES-FEC-FTRN IN RES-FLD.
       FIN-CLR-RES-RES.
           EXIT.
       CLR-USR-RES SECTION.
       INI-CLR-USR-RES.
       FIN-CLR-USR-RES.
           EXIT.
       FST-KEY-RES SECTION.
       INI-FST-KEY-RES.
           MOVE -1 TO RES-COD-TABL-LEN IN RES-FLD.
       FIN-FST-KEY-RES.
           EXIT.
       FST-FLD-RES SECTION.
       INI-FST-FLD-RES.
       FIN-FST-FLD-RES.
           EXIT.
       PRO-KEY-RES SECTION.
       INI-PRO-KEY-RES.
           PERFORM PRO-MKY-RES.
       FIN-PRO-KEY-RES.
           EXIT.
       PRO-MKY-RES SECTION.
       INI-PRO-MKY-RES.
       FIN-PRO-MKY-RES.
           EXIT.
       PRO-FLD-RES SECTION.
       INI-PRO-FLD-RES.
           PERFORM PRO-RES-RES.
           PERFORM PRO-USR-RES.
       FIN-PRO-FLD-RES.
           EXIT.
       PRO-RES-RES SECTION.
       INI-PRO-RES-RES.
       FIN-PRO-RES-RES.
           EXIT.
       PRO-USR-RES SECTION.
       INI-PRO-USR-RES.
       FIN-PRO-USR-RES.
           EXIT.
       UNP-KEY-RES SECTION.
       INI-UNP-KEY-RES.
           PERFORM UNP-IKY-RES.
           PERFORM UNP-AKY-RES.
           PERFORM UNP-MKY-RES.
       FIN-UNP-KEY-RES.
           EXIT.
       UNP-IKY-RES SECTION.
       INI-UNP-IKY-RES.
           MOVE FRM-CPIM-UBY TO RES-COD-TABL-ATR.
           MOVE FRM-CPIM-UBY TO USR-COD-USER-ATR.
       FIN-UNP-IKY-RES.
           EXIT.
       UNP-MKY-RES SECTION.
       INI-UNP-MKY-RES.
       FIN-UNP-MKY-RES.
           EXIT.
       UNP-AKY-RES SECTION.
       INI-UNP-AKY-RES.
       FIN-UNP-AKY-RES.
           EXIT.
       UNP-FLD-RES SECTION.
       INI-UNP-FLD-RES.
           PERFORM UNP-RES-RES.
           PERFORM UNP-USR-RES.
       FIN-UNP-FLD-RES.
           EXIT.
       UNP-RES-RES SECTION.
       INI-UNP-RES-RES.
       FIN-UNP-RES-RES.
           EXIT.
       UNP-USR-RES SECTION.
       INI-UNP-USR-RES.
       FIN-UNP-USR-RES.
           EXIT.
       VAL-MKY-RES SECTION.
       INI-VAL-MKY-RES.
       FIN-VAL-MKY-RES.
           EXIT.
       VAL-FLD-RES SECTION.
       INI-VAL-FLD-RES.
           PERFORM VAL-RES-GLS-DESC.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RES-GLS-DESC-LEN IN RES-FLD
                GO TO FIN-VAL-FLD-RES.
           PERFORM VAL-FRM-GLS-USER.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-USER-LEN IN RES-FLD
                GO TO FIN-VAL-FLD-RES.
           PERFORM VAL-RES-COD-ATRN.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RES-COD-ATRN-LEN IN RES-FLD
                GO TO FIN-VAL-FLD-RES.
           PERFORM VAL-RES-FEC-FTRN.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RES-FEC-FTRN-LEN IN RES-FLD
                GO TO FIN-VAL-FLD-RES.
           PERFORM VAL-FRM-GLS-DUSR.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-DUSR-LEN IN RES-FLD
                GO TO FIN-VAL-FLD-RES.
       FIN-VAL-FLD-RES.
           EXIT.
       VAL-RES-GLS-DESC SECTION.
       INI-VAL-RES-GLS-DESC.
       FIN-VAL-RES-GLS-DESC.
           EXIT.
