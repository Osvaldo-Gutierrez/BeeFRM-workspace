           MOVE USR-GLS-IUSR IN USR-FLD TO IDC-GLS-RUTV.
           PERFORM VAL-GLS.
           MOVE IDC-GLS-RUTV TO USR-GLS-IUSR IN USR-FLD.
      *<<<< USR_GLS_IUSR
      *PGV IF SCR-INST = SCR-INST-GFL OR
           IF USR-IND-IUSR IN USR-FLD     > SPACES OR
              USR-NUM-IUSR IN USR-FLD NOT = ZEROES OR
              USR-VRF-IUSR IN USR-FLD     > SPACES OR
              USR-GLS-IUSR IN USR-FLD     > SPACES
               MOVE USR-GLS-IUSR IN USR-FLD TO IDC-GLS-RUTV
               PERFORM VAL-GLS.
      *>>>>
       FIN-VAL-USR-GLS-IUSR.
           EXIT.

       VAL-USR-GLS-ABRV SECTION.
       INI-VAL-USR-GLS-ABRV.
           IF USR-GLS-ABRV IN USR-FLD NOT > SPACES
              MOVE SPACES TO USR-GLS-ABRV IN USR-FLD.
      *<<<< USR_GLS_ABRV
           IF SCR-INST = SCR-INST-GFL AND
              USR-GLS-ABRV IN USR-FLD NOT > SPACES
               PERFORM ERR-GLS.
      *>>>>
       FIN-VAL-USR-GLS-ABRV.
           EXIT.

       VAL-CON-KEY-USR SECTION.
       INI-VAL-CON-KEY-USR.
           IF     USR-COD-TTAB IN USR-FLD NOT > SPACES
              AND USR-COD-USER IN USR-FLD NOT > SPACES
              AND USR-NUM-IUSR IN USR-FLD NOT > ZEROES
              AND USR-VRF-IUSR IN USR-FLD NOT > SPACES
              AND USR-IND-IUSR IN USR-FLD NOT > SPACES
              AND USR-GLS-IUSR IN USR-FLD NOT > SPACES
              AND USR-GLS-ABRV IN USR-FLD NOT > SPACES
                PERFORM ERR-KEY
                PERFORM FST-KEY-USR.
      *<<<< FIN_VAL_CON_KEY_USR
           IF USR-COD-TTAB IN USR-FLD     > SPACES AND
              USR-COD-USER IN USR-FLD NOT > SPACES
               MOVE 'GNS'          TO MSG-COD-SIST
               MOVE 'VALCONKEY' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC   TO FRM-MENS
               MOVE FRM-SUAR-MAL   TO FRM-SUAR.
      *>>>>
       FIN-VAL-CON-KEY-USR.
           EXIT.

       VAL-CON-IKY-USR SECTION.
       INI-VAL-CON-IKY-USR.
           IF     USR-COD-TTAB IN USR-FLD NOT > SPACES
              AND USR-COD-USER IN USR-FLD NOT > SPACES
                PERFORM ERR-KEY
                PERFORM FST-KEY-USR.
       FIN-VAL-CON-IKY-USR.
           EXIT.

       VAL-NUM-KEY-USR SECTION.
       INI-VAL-NUM-KEY-USR.
           PERFORM VAL-NUM-IKY-USR.
           PERFORM VAL-NUM-MKY-USR.
           PERFORM VAL-NUM-AKY-USR.
       FIN-VAL-NUM-KEY-USR.
           EXIT.

       VAL-NUM-IKY-USR SECTION.
       INI-VAL-NUM-IKY-USR.
       FIN-VAL-NUM-IKY-USR.
           EXIT.

       VAL-NUM-MKY-USR SECTION.
       INI-VAL-NUM-MKY-USR.
       VAL-NUM-USR-NUM-IUSR.
           MOVE USR-NUM-IUSR-ALF IN USR-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO USR-NUM-IUSR IN USR-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO USR-NUM-IUSR-LEN IN USR-FLD
                GO TO FIN-VAL-NUM-MKY-USR.
       FIN-VAL-NUM-MKY-USR.
           EXIT.

       VAL-NUM-AKY-USR SECTION.
       INI-VAL-NUM-AKY-USR.
       FIN-VAL-NUM-AKY-USR.
           EXIT.

       VAL-NUM-FLD-USR SECTION.
       INI-VAL-NUM-FLD-USR.
       FIN-VAL-NUM-FLD-USR.
           EXIT.

       EDT-KEY-USR SECTION.
       INI-EDT-KEY-USR.
           PERFORM EDT-IKY-USR.
           PERFORM EDT-MKY-USR.
           PERFORM EDT-AKY-USR.
       FIN-EDT-KEY-USR.
           EXIT.

       EDT-IKY-USR SECTION.
       INI-EDT-IKY-USR.
       FIN-EDT-IKY-USR.
           EXIT.

       EDT-MKY-USR SECTION.
       INI-EDT-MKY-USR.
           IF USR-NUM-IUSR-LEN IN USR-FLD = -9
                MOVE -1 TO USR-NUM-IUSR-LEN IN USR-FLD
                GO TO FIN-EDT-MKY-USR.
           MOVE USR-NUM-IUSR IN USR-FLD TO USR-NUM-IUSR-EDT IN USR-EDT.
           MOVE USR-NUM-IUSR-EDT IN USR-EDT TO
                USR-NUM-IUSR-ALF IN USR-FLD.
       FIN-EDT-MKY-USR.
           EXIT.

       EDT-AKY-USR SECTION.
       INI-EDT-AKY-USR.
       FIN-EDT-AKY-USR.
           EXIT.

       EDT-FLD-USR SECTION.
       INI-EDT-FLD-USR.
       FIN-EDT-FLD-USR.
           EXIT.
