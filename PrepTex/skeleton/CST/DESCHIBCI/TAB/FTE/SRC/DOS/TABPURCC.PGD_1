       PROCEDURE DIVISION.
      *===================

       MAIN SECTION.
       INI-MAIN.
           PERFORM INI.
       LUP-MAIN.
           IF SCR-INST = SCR-INST-HCM
                 PERFORM SCR-COM.
           IF SCR-CCPP = 'MOD'
                 PERFORM SCR-MOD
           ELSE
           IF SCR-CCPP = 'ING'
                 PERFORM SCR-ING
           ELSE
           IF SCR-CCPP = 'ACC'
                 PERFORM SCR-ACC
           ELSE
           IF SCR-CCPP = 'ELI'
                 PERFORM SCR-ELI
           ELSE
           IF SCR-CCPP = 'ACT'
                 PERFORM SCR-ACT
           ELSE
           IF SCR-CCPP = 'PGM'
                 PERFORM SCR-PGM
           ELSE
                 MOVE SCR-INST-HCM TO SCR-INST.
           GO TO LUP-MAIN.
       FIN-MAIN.
           EXIT.

       INI SECTION.
       INI-INI.
       COPY GNSBGEIB.
       COPY GNSBGEDB.
           MOVE LOW-VALUES TO RCC-FLD.
           MOVE LOW-VALUES TO RCC-EDT.
           PERFORM GNS-PRO-RCK.
           PERFORM GNS-BUS-VCA.
           PERFORM GNS-HDL-SYS.
           PERFORM GNS-HDL-FRM.
           MOVE 'TABPURCC' TO FIO-PROG.
           PERFORM GNS-BUS-IDD.
      *PTC-INI
           PERFORM GNS-ERR-QUE.
           PERFORM GET-TSK-TERM.
      *PTC-FIN
      *<<<< INI_FIO                                                     
           IF SCR-OFRM-SRV                                              
                MOVE TSK-TERM-ALF TO RCC-COD-TERM-CIC                   
           ELSE                                                         
                MOVE EIBTRMID     TO RCC-COD-TERM-CIC.                  
                                                                        
      * Procesamiento Multiple de Contextos (VRF-ACT-ANL)               
           MOVE 'N' TO WSS-IND-MULT.                                    
           MOVE 'TAB'          TO FIO-SIST.                             
           MOVE 'VSN'          TO TAB-COD-TTAB IN TAB.                  
           MOVE 'PROMULT  RCC' TO TAB-COD-CTAB IN TAB.                  
           MOVE FIO-GET-KEY    TO FIO-CMND.                             
           PERFORM GNS-FIO-TAB.                                         
           IF FIO-STAT-OKS AND TAB-MSC-STAT = 'S'                       
              MOVE 'S' TO WSS-IND-MULT.                                 
      *>>>>                                                             
       FIN-INI.
           EXIT.

       COPY GNSBHSYS.
       COPY GNSBHFRM.
       COPY GNSBPRCK.
       COPY GNSBBVCA.
       COPY GNSBBIDD.

       SCR-COM SECTION.
       INI-SCR-COM.
           PERFORM STP-EXIT.
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-CLRA-RCC.
           PERFORM EDT-KEY-RCC.
           PERFORM EDT-FLD-RCC.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-HCM TO SCR-INST.
           MOVE SCR-CMNP     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       XCT-ERR.
           MOVE 'XCTERR    ' TO MSG-COD-MENS.
           MOVE 'GNS'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO SYS-MEN1.
           MOVE SYS-PROG     TO SYS-MEN2.
           MOVE SYS-ABOR     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       COPY GNSBGGBK.
       FIN-SCR-COM.
           EXIT.

       SCR-MOD SECTION.
       INI-SCR-MOD.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
               PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
               PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8
               PERFORM XCT-PRGP.
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
               GO TO MSG-SCR-MOD.
       MSG-SCR-MOD.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'MODNOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-MOD
               IF FIO-STAT-OKS
                   IF FRM-MEN1 NOT > SPACES
                       MOVE 'MODYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MEN1
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-MOD.
           EXIT.

       SCR-ING SECTION.
       INI-SCR-ING.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-IKY
               MOVE SCR-INST-GKY TO SCR-INST
      *PTC-INI
               IF NOT SCR-OFRM-SRV
                   PERFORM SCR-DMSG
               ELSE
                   PERFORM SCR-PUTA
      *PTC-FIN
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
       MSG-SCR-ING.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'INGNOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-ING
               IF FIO-STAT-OKS
                   IF FRM-MEN1 NOT > SPACES
                       MOVE 'INGYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MEN1
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ING.
           EXIT.

       SCR-ACC SECTION.
       INI-SCR-ACC.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8 OR
                         FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM XCT-PRGP.
           MOVE SPACES TO FRM-MENS.
           IF SCR-STAT NOT = SCR-STAT-CON
               PERFORM CMN-ACC.
           PERFORM SCR-MSSG.
       FIN-SCR-ACC.
           EXIT.

       SCR-ELI SECTION.
       INI-SCR-ELI.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8 OR
                         FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM XCT-PRGP.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'ELINOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-ELI
               IF FIO-STAT-OKS
                   IF FRM-MEN1 NOT > SPACES
                       MOVE 'ELIYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MEN1
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ELI.
           EXIT.

       SCR-ACT SECTION.
       INI-SCR-ACT.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF ( FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8  OR
                           FRM-FFLD-PF5 OR FRM-FFLD-PF6 ) AND
              SCR-ACTU = SCR-ACTU-MOD
               PERFORM XCT-PRGP.
           IF SCR-ACTU = SCR-ACTU-MOD
                  PERFORM MSG-SCR-MOD
           ELSE
                  PERFORM MSG-SCR-ING.
       FIN-SCR-ACT.
           EXIT.

       SCR-PGM SECTION.
       INI-SCR-PGM.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
       TRL-SCR-PGM.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8 OR
                         FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM XCT-PRGP.
           MOVE SPACES TO FRM-MENS.
           IF SCR-STAT NOT = SCR-STAT-CON
               PERFORM CMN-PGM.
           PERFORM SCR-MSSG.
       FIN-SCR-PGM.
           EXIT.

       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               PERFORM SCR-CLRA-RCC
               PERFORM EDT-KEY-RCC
               PERFORM EDT-FLD-RCC
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-RCC
               PERFORM EDT-KEY-RCC
           ELSE
               PERFORM CLR-FLD-RCC
               PERFORM EDT-FLD-RCC.
           PERFORM STP-EXIT.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

       XCT-PRGP SECTION.
       INI-XCT-PRGP.
           MOVE SCR-INST-BRW TO SCR-INST.
           PERFORM STP-EXIT.
           MOVE FRM-FFLD   TO SCR-FFLD.
      *
           MOVE SCR-PROG     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
           PERFORM XCT-ERR.
       FIN-XCT-PRGP.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO RCC-FLD.
           MOVE LOW-VALUES TO RCC-EDT.
       FIN-SCR-RST.
           EXIT.

       GET-FLD SECTION.
       INI-GET-FLD.
           PERFORM SCR-GFLD.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-FLD.
           EXIT.

       CMN-MOD SECTION.
       INI-CMN-MOD.
      *<<<< INI_CMN_MOD                                                 
           IF RCC-IND-VIGE IN RCC-FLD = 'A'                             
               MOVE 'S' TO SCR-IERR                                     
               MOVE 'TAB' TO MSG-COD-SIST                               
               MOVE 'RCCMOD NOACA' TO MSG-COD-MENS                      
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               GO TO FIN-CMN-MOD.                                       
                                                                        
           IF RCC-IND-VIGE IN RCC-FLD = 'D'                             
               MOVE 'S' TO SCR-IERR                                     
               MOVE 'TAB' TO MSG-COD-SIST                               
               MOVE 'RCCMOD NOANL' TO MSG-COD-MENS                      
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               GO TO FIN-CMN-MOD.                                       
                                                                        
           MOVE 'I' TO RCC-IND-VIGE IN RCC-FLD.                         
      *>>>>                                                             
           PERFORM KEY-ALL-RCC.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM TAB-FIO-RCC.
           IF FIO-STAT-OKS AND
              RCC-STP-ITRN IN RCC NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM TAB-FIO-RCC
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM TAB-FIO-RCC
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-MOD.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-MOD.
           PERFORM RET-STT-RCC.
           PERFORM RET-ALL-RCC.
           PERFORM RET-HDR-RCC.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM TAB-FIO-RCC.
      *<<<<
           IF NOT FIO-STAT-OKS                                          
              GO TO FIN-CMN-MOD.                                        
                                                                        
           MOVE SPACES TO WSS-IND-VIGE.                                 
           PERFORM CHK-STAT-RCC.                                        
           MOVE 'I' TO WSS-IND-VIGE.                                    
           PERFORM MOD-STAT-RCC.                                        
           IF WSS-IND-MODR = 'N'                                        
               MOVE 'S' TO SCR-IERR                                     
               MOVE 'TAB' TO MSG-COD-SIST                               
               MOVE 'MODNOT    ' TO MSG-COD-MENS                        
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE WSS-BKP-IRCC TO FRM-MEN2                            
           ELSE                                                         
               MOVE 'MODYES    ' TO MSG-COD-MENS                        
               MOVE 'GNS' TO MSG-COD-SIST                               
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN1.                           
                                                                        
           MOVE RCC-CAI-IRCC IN RCC TO RCC-CAI-IRCC-TS.                 
           MOVE RCC-IIC-IRCC IN RCC TO RCC-IIC-IRCC-TS.                 
           MOVE RCC-NOM-TS-CICRCC   TO QUE-COLA.                        
           MOVE RCC-LEN-TS-CICRCC   TO QUE-LITM.                        
           MOVE RCC-ITEM-CICRCC     TO QUE-ITEM.                        
           MOVE QUE-PUT        TO QUE-CMND.                             
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE 'TAB' TO FIO-SIST.                                      
           MOVE 'DPL' TO TAB-COD-TTAB IN TAB.                           
           MOVE RCC-CIC-IRCC IN RCC TO TAB-COD-CTAB IN TAB.             
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM GNS-FIO-TAB.                                         
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-CMN-MOD.                                       
                                                                        
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.                            
           PERFORM GNS-FIO-TAB.                                         
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-CMN-MOD.                                       
                                                                        
           MOVE FRM-GLS-DES1 IN RCC-FLD TO TAB-GLS-DESC IN TAB.         
           MOVE FRM-GLS-DES2 IN RCC-FLD TO TAB-GLS-DATA IN TAB.         
           PERFORM GET-FHOY.                                            
           MOVE HOY-FHOY                TO TAB-FEC-FTRN IN TAB.         
           MOVE HOY-HHOY                TO TAB-HRA-HRTR IN TAB.         
           MOVE SCR-OTRN                TO TAB-COD-OTRN IN TAB.         
           MOVE SCR-USER                TO TAB-COD-ATRN IN TAB.         
           MOVE FIO-MOD TO FIO-CMND.                                    
           PERFORM GNS-FIO-TAB.                                         
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-CMN-MOD.                                       
      *>>>>
       FIN-CMN-MOD.
           EXIT.

       CMN-ING SECTION.
       INI-CMN-ING.
           MOVE SPACES TO RCC.
      *<<<< INI_CMN_ING                                                 
      *PVG INI 03-NOV-2004                                              
      *    IF RCC-NUM-IRCC IN RCC-FLD = 1                               
      *       GO TO CIC-CMN-ING.                                        
      *    PERFORM KEY-ALL-RCC.                                         
      *    MOVE 1 TO RCC-NUM-IRCC IN RCC.                               
      *    MOVE FIO-GET-KEY TO FIO-CMND.                                
      *    PERFORM TAB-FIO-RCC.                                         
      *    IF FIO-STAT-OKS                                              
      *       MOVE RCC-CAI-IRCC IN RCC TO RCC-CAI-IRCC IN RCC-FLD       
      *       MOVE RCC-IIC-IRCC IN RCC TO RCC-IIC-IRCC IN RCC-FLD       
      *    ELSE                                                         
      *       MOVE 'TAB'               TO MSG-COD-SIST                  
      *       MOVE 'RCCUNO   NEX'      TO MSG-COD-MENS                  
      *       PERFORM GET-MSG                                           
      *       MOVE MSG-GLS-DESC        TO FRM-MENS                      
      *       MOVE '23'                TO FIO-STAT                      
      *       GO TO FIN-CMN-ING.                                        
      *    GO TO CON-CMN-ING.                                           
      *PVG FIN 03-NOV-2004                                              
      *OBTIENE CIC PARA RCC (ENGANCHE A AGD)                            
       CIC-CMN-ING.                                                     
           PERFORM OBT-CIC-RCC.                                         
           IF CIC-STAT NOT = CIC-STAT-OKS                               
              GO TO FIN-CMN-ING.                                        
           MOVE CIC-CAI-ICIC IN CIC TO RCC-CAI-IRCC IN RCC.             
           MOVE CIC-IIC-ICIC IN CIC TO RCC-IIC-IRCC IN RCC.             
           MOVE CIC-CAI-ICIC IN CIC TO RCC-CAI-IRCC IN RCC-FLD.         
           MOVE CIC-IIC-ICIC IN CIC TO RCC-IIC-IRCC IN RCC-FLD.         
           MOVE 'TAB'                   TO FIO-SIST.                    
           MOVE 'DPL'                   TO TAB-COD-TTAB IN TAB          
           MOVE RCC-CIC-IRCC IN RCC     TO TAB-COD-CTAB IN TAB.         
           MOVE FRM-GLS-DES1 IN RCC-FLD TO TAB-GLS-DESC IN TAB.         
           MOVE FRM-GLS-DES2 IN RCC-FLD TO TAB-GLS-DATA IN TAB.         
           MOVE 'I'                     TO TAB-MSC-TACC IN TAB.         
           MOVE 'I'                     TO TAB-MSC-STAT IN TAB.         
           MOVE 'S'                     TO TAB-IND-VIGE IN TAB.         
           MOVE SPACES                  TO TAB-COD-OTRN IN TAB.         
           MOVE 'TABPURCC'              TO TAB-COD-ATRN IN TAB.         
           MOVE RCC-CIC-IRCC IN RCC     TO TAB-GLS-DCOR IN TAB.         
           MOVE RCC-CAI-IRCC IN RCC     TO TAB-GLS-ABRV IN TAB.         
           PERFORM GET-FHOY.                                            
           MOVE HOY-FHOY                TO TAB-FEC-FTRN IN TAB.         
           MOVE HOY-HHOY                TO TAB-HRA-HRTR IN TAB.         
           MOVE SCR-OTRN                TO TAB-COD-OTRN IN TAB.         
           MOVE SCR-USER                TO TAB-COD-ATRN IN TAB.         
                                                                        
           MOVE FIO-PUT TO FIO-CMND.                                    
           PERFORM GNS-FIO-TAB.                                         
           IF NOT FIO-STAT-OKS                                          
              MOVE 'TAB'               TO MSG-COD-SIST                  
              MOVE 'DPL    NOGRB'      TO MSG-COD-MENS                  
              PERFORM GET-MSG                                           
      *>>>>                                                             
           PERFORM RET-STT-RCC.
           PERFORM RET-ALL-RCC.
           PERFORM RET-HDR-RCC.
           MOVE FIO-PUT TO FIO-CMND.
           PERFORM TAB-FIO-RCC.
      *<<<< FIN_CMN_ING                                                 
           IF NOT (FIO-STAT-OKS AND SCR-OFRM-SRV)                       
                 GO TO FIN-CMN-ING.                                     
      *INFORMA CIC DE RCC (ENGANCHE AGD)                                
           MOVE RCC-CAI-IRCC IN RCC TO RCC-CAI-IRCC-TS.                 
           MOVE RCC-IIC-IRCC IN RCC TO RCC-IIC-IRCC-TS.                 
           MOVE RCC-NOM-TS-CICRCC   TO QUE-COLA.                        
           MOVE RCC-LEN-TS-CICRCC   TO QUE-LITM.                        
           MOVE RCC-ITEM-CICRCC     TO QUE-ITEM.                        
           MOVE QUE-PUT        TO QUE-CMND.                             
           PERFORM GNS-PRO-QUE.                                         
      *>>>>                                                             
       FIN-CMN-ING.
           EXIT.

       CMN-ACC SECTION.
       INI-CMN-ACC.
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'ACCION NO REALIZADA' TO FRM-MENS
               GO TO FIN-CMN-ACC.
      *<<<<
           PERFORM KEY-ALL-RCC.                                         
      *     MOVE FIO-GET-KEY-UPD TO FIO-CMND.                           
      *     PERFORM TAB-FIO-RCC.                                        
      *     IF FIO-STAT-OKS AND                                         
      *        RCC-STP-ITRN IN RCC NOT = SCR-UMOD                       
      *         MOVE FIO-ULK-REC TO FIO-CMND                            
      *         PERFORM TAB-FIO-RCC                                     
      *         MOVE FIO-BCK-OUT TO FIO-CMND                            
      *         PERFORM TAB-FIO-RCC                                     
      *         MOVE 'GNS' TO MSG-COD-SIST                              
      *         MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS                     
      *         PERFORM GET-MSG                                         
      *         MOVE MSG-GLS-DESC TO FRM-MENS                           
      *         GO TO FIN-CMN-ACC.                                      
      *     IF NOT FIO-STAT-OKS                                         
      *         GO TO FIN-CMN-ACC.                                      
                                                                        
           PERFORM RET-ALL-RCC.                                         
      *PVG INI 02-NOV-2004                                              
           IF SCR-CMND = 'VRF'                                          
              PERFORM VAL-CON-CPTO                                      
              IF MSG-COD-MENS > SPACES                                  
                 MOVE 'S'           TO SCR-IERR                         
                 MOVE 'TAB'         TO MSG-COD-SIST                     
                 PERFORM GET-MSG                                        
                 MOVE MSG-GLS-DESC  TO FRM-MENS                         
                 GO TO FIN-CMN-ACC.                                     
      *PVG FIN 02-NOV-2004                                              
                                                                        
           IF SCR-CMND = 'VRF'                                          
               PERFORM CHK-AGD-RCC                                      
               IF WSS-IND-CHKA = 'N'                                    
                   MOVE 'S' TO SCR-IERR                                 
                   MOVE 'TAB' TO MSG-COD-SIST                           
                   MOVE 'AGDTAGDDUPL' TO MSG-COD-MENS                   
                   PERFORM GET-MSG                                      
                   MOVE MSG-GLS-DESC TO FRM-MENS                        
               ELSE                                                     
                   MOVE 'I' TO WSS-IND-VIGE                             
                   PERFORM CHK-STAT-RCC                                 
                   IF WSS-IND-CHKR = 'N'                                
                      MOVE 'S' TO SCR-IERR                              
                      MOVE 'TAB' TO MSG-COD-SIST                        
                      MOVE 'RCCSTATNOING' TO MSG-COD-MENS               
                      PERFORM GET-MSG                                   
                      MOVE MSG-GLS-DESC   TO FRM-MEN1                   
                      MOVE WSS-BKP-IRCC TO FRM-MEN2                     
                   ELSE                                                 
                      MOVE 'V' TO WSS-IND-VIGE                          
                      PERFORM MOD-STAT-RCC                              
                      IF WSS-IND-MODR = 'N'                             
                         MOVE 'S' TO SCR-IERR                           
                         MOVE 'TAB' TO MSG-COD-SIST                     
                         MOVE 'VRFNOT    ' TO MSG-COD-MENS              
                         PERFORM GET-MSG                                
                         MOVE MSG-GLS-DESC TO FRM-MENS                  
                         MOVE WSS-BKP-IRCC TO FRM-MEN2                  
                      ELSE                                              
                         MOVE 'GNS'        TO MSG-COD-SIST              
                         MOVE 'VRFYES    ' TO MSG-COD-MENS              
                         PERFORM GET-MSG                                
                         MOVE MSG-GLS-DESC TO FRM-MEN1.                 
                                                                        
           IF SCR-CMND = 'ACA'                                          
               MOVE 'V' TO WSS-IND-VIGE                                 
               PERFORM CHK-STAT-RCC                                     
               IF WSS-IND-CHKR = 'N'                                    
                  MOVE 'S' TO SCR-IERR                                  
                  MOVE 'TAB' TO MSG-COD-SIST                            
                  MOVE 'RCCSTATNOVRF' TO MSG-COD-MENS                   
                  PERFORM GET-MSG                                       
                  MOVE MSG-GLS-DESC   TO FRM-MEN1                       
                  MOVE WSS-BKP-IRCC TO FRM-MEN2                         
               ELSE                                                     
                  MOVE 'A' TO WSS-IND-VIGE                              
                  PERFORM MOD-STAT-RCC                                  
                  IF WSS-IND-MODR = 'N'                                 
                     MOVE 'S' TO SCR-IERR                               
                     MOVE 'TAB' TO MSG-COD-SIST                         
                     MOVE 'ACANOT    ' TO MSG-COD-MENS                  
                     PERFORM GET-MSG                                    
                     MOVE MSG-GLS-DESC TO FRM-MEN1                      
                     MOVE WSS-BKP-IRCC TO FRM-MEN2                      
                  ELSE                                                  
                     MOVE 'GNS'        TO MSG-COD-SIST                  
                     MOVE 'ACAYES    ' TO MSG-COD-MENS                  
                     PERFORM GET-MSG                                    
                     MOVE MSG-GLS-DESC TO FRM-MEN1.                     
                                                                        
           IF SCR-CMND = 'ANL'                                          
               MOVE 'A' TO WSS-IND-VIGE                                 
               PERFORM CHK-STAT-RCC                                     
               IF WSS-IND-CHKR = 'N'                                    
                  MOVE 'S' TO SCR-IERR                                  
                  MOVE 'TAB' TO MSG-COD-SIST                            
                  MOVE 'RCCSTATNOACA' TO MSG-COD-MENS                   
                  PERFORM GET-MSG                                       
                  MOVE MSG-GLS-DESC   TO FRM-MEN1                       
                  MOVE WSS-BKP-IRCC TO FRM-MEN2                         
               ELSE                                                     
                  MOVE 'D' TO WSS-IND-VIGE                              
                  PERFORM MOD-STAT-RCC                                  
                  IF WSS-IND-MODR = 'N'                                 
                     MOVE 'S' TO SCR-IERR                               
                     MOVE 'TAB' TO MSG-COD-SIST                         
                     MOVE 'ANLNOT    ' TO MSG-COD-MENS                  
                     PERFORM GET-MSG                                    
                     MOVE MSG-GLS-DESC TO FRM-MEN1                      
                     MOVE WSS-BKP-IRCC TO FRM-MEN2                      
                  ELSE                                                  
                     MOVE 'GNS'        TO MSG-COD-SIST                  
                     MOVE 'ANLYES    ' TO MSG-COD-MENS                  
                     PERFORM GET-MSG                                    
                     MOVE MSG-GLS-DESC TO FRM-MEN1.                     
           GO TO FIN-CMN-ACC.   
      *>>>>
           PERFORM KEY-ALL-RCC.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM TAB-FIO-RCC.
           IF FIO-STAT-OKS AND
              RCC-STP-ITRN IN RCC NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM TAB-FIO-RCC
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM TAB-FIO-RCC
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-ACC.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ACC.
           PERFORM RET-ALL-RCC.
       FIN-CMN-ACC.
           EXIT.

       CMN-ELI SECTION.
       INI-CMN-ELI.
      *<<<< INI_CMN_ELI                                                 
           IF RCC-IND-VIGE IN RCC-FLD = 'A'                             
               MOVE 'S' TO SCR-IERR                                     
               MOVE 'TAB' TO MSG-COD-SIST                               
               MOVE 'RCCELI NOACA' TO MSG-COD-MENS                      
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               GO TO FIN-CMN-ELI.                                       
                                                                        
           IF RCC-IND-VIGE IN RCC-FLD = 'D'                             
               MOVE 'S' TO SCR-IERR                                     
               MOVE 'TAB' TO MSG-COD-SIST                               
               MOVE 'RCCELI NOANL' TO MSG-COD-MENS                      
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               GO TO FIN-CMN-ELI.                                       
      *>>>>                                                             
           PERFORM KEY-ALL-RCC.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM TAB-FIO-RCC.
           IF FIO-STAT-OKS AND
              RCC-STP-ITRN IN RCC NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM TAB-FIO-RCC
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM TAB-FIO-RCC
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-ELI.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ELI.
           MOVE FIO-DEL TO FIO-CMND.
           PERFORM TAB-FIO-RCC.
      *<<<<
           MOVE FIO-STAT TO PGM-STAT-RCC.                               
           PERFORM ELI-AGD.                                             
           MOVE PGM-STAT-RCC TO FIO-STAT.                               
      *>>>>
       FIN-CMN-ELI.
           EXIT.

       CMN-PGM SECTION.
       INI-CMN-PGM.
           PERFORM RET-ALL-RCC.
       FIN-CMN-PGM.
           EXIT.

       STP-EXIT SECTION.
       INI-STP-EXIT.
           MOVE SCR-PROG TO PGM-PROG.
           MOVE SCR-NTRN TO PGM-NTRN.

           MOVE SCR-PROV TO SCR-PROG.
           MOVE SCR-NTRV TO SCR-NTRN.

           MOVE PGM-PROG TO SCR-PROV
           MOVE PGM-NTRN TO SCR-NTRV.
       FIN-STP-EXIT.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO SYS-NTRN.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
      *PTC-INI
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE RCC-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO RCC-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GFLD SECTION.
       INI-SCR-GFLD.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-FLD-RCC
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO RCC-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GFLD.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-RCC.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-RCC
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           PERFORM VAL-NUM-FLD-RCC.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-RCC
               PERFORM EDT-FLD-RCC
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           PERFORM VAL-MKY-RCC.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               GO TO ERR-SCR-GFLD.
           PERFORM VAL-FLD-RCC.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GFLD.
           PERFORM VAL-CON-FLD-RCC.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GFLD.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-FLD-RCC.
       ERR-SCR-GFLD.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-RCC.
           PERFORM EDT-FLD-RCC.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GFLD.
           MOVE SCR-STAT-MOD TO SCR-STAT.

       GET-ACC SECTION.
       INI-GET-ACC.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO RCC-GLS-MEN2
               MOVE SCR-PRMT     TO RCC-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO RCC-FLD.
           IF NOT ( ( FRM-FFLD = FRM-FFLD-PF12 OR FRM-FFLD-PF3 ) OR
                      SCR-CONF > SPACES )
               PERFORM VAL-NUM-KEY-RCC
               PERFORM VAL-NUM-FLD-RCC.
       FIN-GET-ACC.
           MOVE SCR-STAT-SON TO SCR-STAT.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO RCC-GLS-MEN2
               MOVE SCR-PRMT     TO RCC-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO RCC-FLD.
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO RCC-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE RCC-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO RCC-FLD
      * FLB  7-NOV-1994
               PERFORM CLR-KEY-RCC

               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO RCC-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO RCC-GLS-MENS.
           PERFORM PRO-FLD-RCC.
           PERFORM PRO-MKY-RCC.
           PERFORM UNP-IKY-RCC.
           PERFORM FST-KEY-RCC.
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE RCC-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO RCC-FLD
               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO RCC-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO RCC-GLS-MENS.
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-FLD-RCC.
           MOVE '1' TO FRM-CPIM-AEY.
           PERFORM UNP-KEY-RCC.
           PERFORM FST-KEY-RCC.
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
           PERFORM UNP-MKY-RCC.
           PERFORM UNP-FLD-RCC.
           PERFORM FST-FLD-RCC.
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO RCC-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-RCC.
           PERFORM PRO-FLD-RCC.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
      * PTC-INI
           IF SCR-STAT NOT = SCR-STPP
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
      * PTC-FIN
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-RCC
               PERFORM EDT-FLD-RCC
               PERFORM SCR-PUTA
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-RCC
               PERFORM EDT-FLD-RCC
               PERFORM SCR-PUTA
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-RCC.
           PERFORM EDT-FLD-RCC.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           IF SCR-INST = SCR-INST-GCF
                  PERFORM SCR-GCNF
                  GO TO FIN-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
           IF SCR-STAT = SCR-STAT-ABT OR SCR-STAT-CON
                  GO TO FIN-DESSCRTRL.
           IF SCR-FFLD-RET OR SCR-FFLD-PF10
                  MOVE SCR-VFLD-YES TO SCR-VFLD
                  PERFORM PRO-CONF.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

       PRO-CONF SECTION.
       INI-PRO-CONF.
      *PTC-INI
           IF SCR-OFRM-SRV AND
              SCR-CONF NOT > SPACES
                MOVE SPACES TO FRM-MENS.
      *PTC-FIN
           IF FRM-MENS NOT > SPACES
               IF SCR-CONF NOT > SPACES
                   GO TO FIN-PRO-CONF
               ELSE
                   MOVE SCR-CONF TO FRM-MENS
           ELSE
               IF FRM-MEN1 NOT > SPACES
                   MOVE SCR-CONF TO FRM-MEN1.
           PERFORM PRO-KEY-RCC.
           PERFORM PRO-FLD-RCC.
           PERFORM FST-MSG.
           PERFORM SCR-MSSG.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM EDT-KEY-RCC
               PERFORM EDT-FLD-RCC.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GCF TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-PRO-CONF.
           EXIT.

       SCR-GCNF SECTION.
       INI-SCR-GCNF.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3  OR
                               FRM-FFLD-PF4 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO RCC-GLS-MEN2
               MOVE SCR-CONF     TO RCC-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO RCC-FLD.
           MOVE FRM-FFLD TO SCR-FFLD.
           IF FRM-FFLD = FRM-FFLD-PF3
               MOVE SCR-VFLD-NOT TO SCR-VFLD
           ELSE
           IF FRM-FFLD = FRM-FFLD-RET OR FRM-FFLD-PF10 OR FRM-FFLD-PF4
               MOVE SCR-VFLD-YES TO SCR-VFLD
               PERFORM VAL-NUM-KEY-RCC
               PERFORM VAL-NUM-FLD-RCC
           ELSE
           IF FRM-FFLD = FRM-FFLD-PF12
               MOVE SCR-STPP TO SCR-STAT
               IF SCR-STAT = SCR-STAT-MOD
                   PERFORM VAL-NUM-KEY-RCC
                   PERFORM VAL-NUM-FLD-RCC
                   PERFORM SCR-TRL
               ELSE
                   MOVE SCR-VFLD-NOT TO SCR-VFLD.
       FIN-SCR-GCNF.
           EXIT.

      * PGM-EBR-ALL SECTION.
