       SET-TAG-TABBQCAB SECTION.
       INI-SET-TAG-TABBQCAB.
           MOVE
           '<IDNMOD NOM=$TABBQCAB$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$23-Apr-10 11:24 AM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-TABBQCAB.
           EXIT.
           
       PUT-ALL-CAB SECTION.
       INI-PUT-ALL-CAB.
           PERFORM PUT-CAB-CAB.
           PERFORM PUT-WFC-CAB.
           PERFORM PUT-DBC-CAB.
       FIN-PUT-ALL-CAB.
           EXIT.
       
       PUT-CAB-CAB SECTION.
       INI-PUT-CAB-CAB.
           IF PGM-STAT-CAB-NUL
               GO TO FIN-PUT-CAB-CAB.
           IF NOT PGM-STAT-CAB-OKS
               PERFORM CLR-CAB-CAB
               GO TO FIN-PUT-CAB-CAB.
           MOVE CAB-CAI-CONV IN CAB TO CAB-CAI-CONV IN CAB-FLD.
           MOVE CAB-IIC-CONV IN CAB TO CAB-IIC-CONV IN CAB-FLD.
           MOVE CAB-IND-TIPO IN CAB TO CAB-IND-TIPO IN CAB-FLD.
           MOVE CAB-NUM-ICAB IN CAB TO CAB-NUM-ICAB IN CAB-FLD.
           MOVE CAB-IND-IWFL IN CAB TO CAB-IND-IWFL IN CAB-FLD.
           MOVE CAB-IND-SMWI IN CAB TO CAB-IND-SMWI IN CAB-FLD.
      *<<<<                                                             
           IF CAB-IND-SMWI IN CAB-FLD > SPACES                          
              MOVE 'TAB'  TO TAB-COD-SIST                               
              MOVE 'SWI'  TO TAB-COD-TTAB IN TAB                        
              MOVE CAB-IND-SMWI IN CAB-FLD TO TAB-COD-CTAB IN TAB       
              PERFORM BUS-TAB                                           
              IF MSG-COD-MENS NOT > SPACES                              
                 MOVE TAB-GLS-DCOR IN TAB TO FRM-GLS-SMWI IN CAB-FLD    
              ELSE                                                      
                 MOVE SPACES              TO FRM-GLS-SMWI IN CAB-FLD.   
                                                                        
           MOVE CAB-COD-METR IN CAB(01) TO FRM-COD-METR IN CAB-FLD(01). 
           MOVE CAB-NUM-METR IN CAB(01) TO FRM-NUM-METR IN CAB-FLD(01). 
           IF CAB-COD-METR IN CAB(01) > SPACES                          
              MOVE 'TAB'                   TO TAB-COD-SIST              
              MOVE 'TDM'                   TO TAB-COD-TTAB IN TAB       
              MOVE CAB-COD-METR IN CAB(01) TO TAB-COD-CTAB IN TAB       
              PERFORM BUS-TAB                                           
              IF MSG-COD-MENS NOT > SPACES                              
                 MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-TMET IN CAB-FLD(01)
              ELSE                                                      
                 MOVE SPACES            TO FRM-GLS-TMET IN CAB-FLD(01). 
                                                                        
           MOVE CAB-COD-METR IN CAB(02) TO FRM-COD-METR IN CAB-FLD(02). 
           MOVE CAB-NUM-METR IN CAB(02) TO FRM-NUM-METR IN CAB-FLD(02). 
           IF CAB-COD-METR IN CAB(02) > SPACES                          
              MOVE 'TAB'                   TO TAB-COD-SIST              
              MOVE 'TDM'                   TO TAB-COD-TTAB IN TAB       
              MOVE CAB-COD-METR IN CAB(02) TO TAB-COD-CTAB IN TAB       
              PERFORM BUS-TAB                                           
              IF MSG-COD-MENS NOT > SPACES                              
                 MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-TMET IN CAB-FLD(02)
              ELSE                                                      
                 MOVE SPACES            TO FRM-GLS-TMET IN CAB-FLD(02). 
                                                                        
           MOVE CAB-COD-METR IN CAB(03) TO FRM-COD-METR IN CAB-FLD(03). 
           MOVE CAB-NUM-METR IN CAB(03) TO FRM-NUM-METR IN CAB-FLD(03). 
           IF CAB-COD-METR IN CAB(03) > SPACES                          
              MOVE 'TAB'                   TO TAB-COD-SIST              
              MOVE 'TDM'                   TO TAB-COD-TTAB IN TAB       
              MOVE CAB-COD-METR IN CAB(03) TO TAB-COD-CTAB IN TAB       
              PERFORM BUS-TAB                                           
              IF MSG-COD-MENS NOT > SPACES                              
                 MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-TMET IN CAB-FLD(03)
              ELSE                                                      
                 MOVE SPACES            TO FRM-GLS-TMET IN CAB-FLD(03). 
                                                                        
           MOVE CAB-COD-METR IN CAB(04) TO FRM-COD-METR IN CAB-FLD(04). 
           MOVE CAB-NUM-METR IN CAB(04) TO FRM-NUM-METR IN CAB-FLD(04). 
           IF CAB-COD-METR IN CAB(04) > SPACES                          
              MOVE 'TAB'                   TO TAB-COD-SIST              
              MOVE 'TDM'                   TO TAB-COD-TTAB IN TAB       
              MOVE CAB-COD-METR IN CAB(04) TO TAB-COD-CTAB IN TAB       
              PERFORM BUS-TAB                                           
              IF MSG-COD-MENS NOT > SPACES                              
                 MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-TMET IN CAB-FLD(04)
              ELSE                                                      
                 MOVE SPACES            TO FRM-GLS-TMET IN CAB-FLD(04). 
                                                                        
           MOVE CAB-COD-METR IN CAB(05) TO FRM-COD-METR IN CAB-FLD(05). 
           MOVE CAB-NUM-METR IN CAB(05) TO FRM-NUM-METR IN CAB-FLD(05). 
           IF CAB-COD-METR IN CAB(05) > SPACES                          
              MOVE 'TAB'                   TO TAB-COD-SIST              
              MOVE 'TDM'                   TO TAB-COD-TTAB IN TAB       
              MOVE CAB-COD-METR IN CAB(05) TO TAB-COD-CTAB IN TAB       
              PERFORM BUS-TAB                                           
              IF MSG-COD-MENS NOT > SPACES                              
                 MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-TMET IN CAB-FLD(05)
              ELSE                                                      
                 MOVE SPACES            TO FRM-GLS-TMET IN CAB-FLD(05). 
                                                                        
           MOVE CAB-COD-METR IN CAB(06) TO FRM-COD-METR IN CAB-FLD(06). 
           MOVE CAB-NUM-METR IN CAB(06) TO FRM-NUM-METR IN CAB-FLD(06). 
           IF CAB-COD-METR IN CAB(06) > SPACES                          
              MOVE 'TAB'                   TO TAB-COD-SIST              
              MOVE 'TDM'                   TO TAB-COD-TTAB IN TAB       
              MOVE CAB-COD-METR IN CAB(06) TO TAB-COD-CTAB IN TAB       
              PERFORM BUS-TAB                                           
              IF MSG-COD-MENS NOT > SPACES                              
                 MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-TMET IN CAB-FLD(06)
              ELSE                                                      
                 MOVE SPACES            TO FRM-GLS-TMET IN CAB-FLD(06). 
                                                                        
           MOVE CAB-COD-METR IN CAB(07) TO FRM-COD-METR IN CAB-FLD(07). 
           MOVE CAB-NUM-METR IN CAB(07) TO FRM-NUM-METR IN CAB-FLD(07). 
           IF CAB-COD-METR IN CAB(07) > SPACES                          
              MOVE 'TAB'                   TO TAB-COD-SIST              
              MOVE 'TDM'                   TO TAB-COD-TTAB IN TAB       
              MOVE CAB-COD-METR IN CAB(07) TO TAB-COD-CTAB IN TAB       
              PERFORM BUS-TAB                                           
              IF MSG-COD-MENS NOT > SPACES                              
                 MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-TMET IN CAB-FLD(07)
              ELSE                                                      
                 MOVE SPACES            TO FRM-GLS-TMET IN CAB-FLD(07). 
                                                                        
           MOVE CAB-COD-METR IN CAB(08) TO FRM-COD-METR IN CAB-FLD(08). 
           MOVE CAB-NUM-METR IN CAB(08) TO FRM-NUM-METR IN CAB-FLD(08). 
           IF CAB-COD-METR IN CAB(08) > SPACES                          
              MOVE 'TAB'                   TO TAB-COD-SIST              
              MOVE 'TDM'                   TO TAB-COD-TTAB IN TAB       
              MOVE CAB-COD-METR IN CAB(08) TO TAB-COD-CTAB IN TAB       
              PERFORM BUS-TAB                                           
              IF MSG-COD-MENS NOT > SPACES                              
                 MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-TMET IN CAB-FLD(08)
              ELSE                                                      
                 MOVE SPACES            TO FRM-GLS-TMET IN CAB-FLD(08). 
                                                                        
           MOVE CAB-COD-METR IN CAB(09) TO FRM-COD-METR IN CAB-FLD(09). 
           MOVE CAB-NUM-METR IN CAB(09) TO FRM-NUM-METR IN CAB-FLD(09). 
           IF CAB-COD-METR IN CAB(09) > SPACES                          
              MOVE 'TAB'                   TO TAB-COD-SIST              
              MOVE 'TDM'                   TO TAB-COD-TTAB IN TAB       
              MOVE CAB-COD-METR IN CAB(09) TO TAB-COD-CTAB IN TAB       
              PERFORM BUS-TAB                                           
              IF MSG-COD-MENS NOT > SPACES                              
                 MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-TMET IN CAB-FLD(09)
              ELSE                                                      
                 MOVE SPACES            TO FRM-GLS-TMET IN CAB-FLD(09). 
                                                                        
           MOVE CAB-COD-METR IN CAB(10) TO FRM-COD-METR IN CAB-FLD(10). 
           MOVE CAB-NUM-METR IN CAB(10) TO FRM-NUM-METR IN CAB-FLD(10). 
           IF CAB-COD-METR IN CAB(10) > SPACES                          
              MOVE 'TAB'                   TO TAB-COD-SIST              
              MOVE 'TDM'                   TO TAB-COD-TTAB IN TAB       
              MOVE CAB-COD-METR IN CAB(10) TO TAB-COD-CTAB IN TAB       
              PERFORM BUS-TAB                                           
              IF MSG-COD-MENS NOT > SPACES                              
                 MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-TMET IN CAB-FLD(10)
              ELSE                                                      
                 MOVE SPACES            TO FRM-GLS-TMET IN CAB-FLD(10). 
      *>>>>                                                             
       FIN-PUT-CAB-CAB.
           EXIT.
       
       PUT-WFC-CAB SECTION.
       INI-PUT-WFC-CAB.
           IF PGM-STAT-WFC-NUL
               GO TO FIN-PUT-WFC-CAB.
           IF NOT PGM-STAT-WFC-OKS
               PERFORM CLR-WFC-CAB
               GO TO FIN-PUT-WFC-CAB.
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE WFC-NUM-DCNV IN WFC TO WFC-NUM-DCNV IN CAB-FLD.
           MOVE WFC-NUM-MCNV IN WFC TO WFC-NUM-MCNV IN CAB-FLD.
           MOVE WFC-NUM-SCNV IN WFC TO WFC-NUM-SCNV IN CAB-FLD.
           MOVE WFC-NUM-ACNV IN WFC TO WFC-NUM-ACNV IN CAB-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE WFC-HRA-CONV IN WFC TO WFC-HRA-CONV IN CAB-FLD.
       FIN-PUT-WFC-CAB.
           EXIT.
       
       PUT-DBC-CAB SECTION.
       INI-PUT-DBC-CAB.
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-CAB.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-CAB
               GO TO FIN-PUT-DBC-CAB.
      * IDC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN CAB-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN CAB-FLD.
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN CAB-FLD.
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN CAB-FLD.
      * IDC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-CAI-ICLI IN DBC TO DBC-CAI-ICLI IN CAB-FLD.
           MOVE DBC-IIC-ICLI IN DBC TO DBC-IIC-ICLI IN CAB-FLD.
           MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN CAB-FLD.
      *<<<<
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.                        
           PERFORM CPT-BLKS.                                            
           MOVE CPT-STRN            TO DBC-GLS-NOMC IN CAB-FLD.         
      *>>>>
       FIN-PUT-DBC-CAB.
           EXIT.
       
       KEY-ALL-CAB SECTION.
       INI-KEY-ALL-CAB.
           PERFORM KEY-CAB-CAB.
           PERFORM KEY-WFC-CAB.
           PERFORM KEY-DBC-CAB.
       FIN-KEY-ALL-CAB.
           EXIT.
       
       KEY-CAB-CAB SECTION.
       INI-KEY-CAB-CAB.
           MOVE CAB-CAI-CONV IN CAB-FLD TO CAB-CAI-CONV IN CAB.
           MOVE CAB-IIC-CONV IN CAB-FLD TO CAB-IIC-CONV IN CAB.
           MOVE CAB-IND-TIPO IN CAB-FLD TO CAB-IND-TIPO IN CAB.
           MOVE CAB-NUM-ICAB IN CAB-FLD TO CAB-NUM-ICAB IN CAB.
       FIN-KEY-CAB-CAB.
           EXIT.
       
       KEY-WFC-CAB SECTION.
       INI-KEY-WFC-CAB.
       FIN-KEY-WFC-CAB.
           EXIT.
       
       KEY-DBC-CAB SECTION.
       INI-KEY-DBC-CAB.
       FIN-KEY-DBC-CAB.
           EXIT.
       
       SCR-CLRA-CAB SECTION.
       INI-SCR-CLRA-CAB.
           PERFORM CLR-KEY-CAB.
           PERFORM CLR-FLD-CAB.
       FIN-SCR-CLRA-CAB.
           EXIT.
       
       CLR-KEY-CAB SECTION.
       INI-CLR-KEY-CAB.
           MOVE SPACES TO CAB-CAI-CONV IN CAB-FLD.
           MOVE SPACES TO CAB-IIC-CONV IN CAB-FLD.
           MOVE SPACES TO CAB-IND-TIPO IN CAB-FLD.
           MOVE ZEROES TO CAB-NUM-ICAB IN CAB-FLD.
       FIN-CLR-KEY-CAB.
           EXIT.
       
       CLR-FLD-CAB SECTION.
       INI-CLR-FLD-CAB.
           MOVE SPACES TO FRM-GLS-TIPO IN CAB-FLD.
           MOVE SPACES TO FRM-GLS-SMWI IN CAB-FLD.
           MOVE SPACES TO FRM-COD-METR IN CAB-FLD(1).
           MOVE SPACES TO FRM-COD-METR IN CAB-FLD(2).
           MOVE SPACES TO FRM-COD-METR IN CAB-FLD(3).
           MOVE SPACES TO FRM-COD-METR IN CAB-FLD(4).
           MOVE SPACES TO FRM-COD-METR IN CAB-FLD(5).
           MOVE SPACES TO FRM-COD-METR IN CAB-FLD(6).
           MOVE SPACES TO FRM-COD-METR IN CAB-FLD(7).
           MOVE SPACES TO FRM-COD-METR IN CAB-FLD(8).
           MOVE SPACES TO FRM-COD-METR IN CAB-FLD(9).
           MOVE SPACES TO FRM-COD-METR IN CAB-FLD(10).
           MOVE SPACES TO FRM-GLS-TMET IN CAB-FLD(1).
           MOVE SPACES TO FRM-GLS-TMET IN CAB-FLD(2).
           MOVE SPACES TO FRM-GLS-TMET IN CAB-FLD(3).
           MOVE SPACES TO FRM-GLS-TMET IN CAB-FLD(4).
           MOVE SPACES TO FRM-GLS-TMET IN CAB-FLD(5).
           MOVE SPACES TO FRM-GLS-TMET IN CAB-FLD(6).
           MOVE SPACES TO FRM-GLS-TMET IN CAB-FLD(7).
           MOVE SPACES TO FRM-GLS-TMET IN CAB-FLD(8).
           MOVE SPACES TO FRM-GLS-TMET IN CAB-FLD(9).
           MOVE SPACES TO FRM-GLS-TMET IN CAB-FLD(10).
           MOVE ZEROES TO FRM-NUM-METR IN CAB-FLD(1).
           MOVE ZEROES TO FRM-NUM-METR IN CAB-FLD(2).
           MOVE ZEROES TO FRM-NUM-METR IN CAB-FLD(3).
           MOVE ZEROES TO FRM-NUM-METR IN CAB-FLD(4).
           MOVE ZEROES TO FRM-NUM-METR IN CAB-FLD(5).
           MOVE ZEROES TO FRM-NUM-METR IN CAB-FLD(6).
           MOVE ZEROES TO FRM-NUM-METR IN CAB-FLD(7).
           MOVE ZEROES TO FRM-NUM-METR IN CAB-FLD(8).
           MOVE ZEROES TO FRM-NUM-METR IN CAB-FLD(9).
           MOVE ZEROES TO FRM-NUM-METR IN CAB-FLD(10).
           PERFORM CLR-CAB-CAB.
           PERFORM CLR-WFC-CAB.
           PERFORM CLR-DBC-CAB.
       FIN-CLR-FLD-CAB.
           EXIT.
       
       CLR-CAB-CAB SECTION.
       INI-CLR-CAB-CAB.
           MOVE SPACES TO CAB-IND-IWFL IN CAB-FLD.
           MOVE SPACES TO CAB-IND-SMWI IN CAB-FLD.
       FIN-CLR-CAB-CAB.
           EXIT.
       
       CLR-WFC-CAB SECTION.
       INI-CLR-WFC-CAB.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO WFC-FEC-FCNV IN CAB-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO WFC-HRA-CONV IN CAB-FLD.
       FIN-CLR-WFC-CAB.
           EXIT.
       
       CLR-DBC-CAB SECTION.
       INI-CLR-DBC-CAB.
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN CAB-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN CAB-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN CAB-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN CAB-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO DBC-CAI-ICLI IN CAB-FLD.
           MOVE SPACES TO DBC-IIC-ICLI IN CAB-FLD.
           MOVE SPACES TO DBC-GLS-NOMC IN CAB-FLD.
       FIN-CLR-DBC-CAB.
           EXIT.
       
       FST-KEY-CAB SECTION.
       INI-FST-KEY-CAB.
           MOVE -1 TO CAB-CAI-CONV-LEN IN CAB-FLD.
       FIN-FST-KEY-CAB.
           EXIT.
       
       FST-FLD-CAB SECTION.
       INI-FST-FLD-CAB.
      *<<<<
           IF SCR-CMND = 'AEW'                                          
              MOVE -1 TO CAB-IND-SMWI-LEN IN CAB-FLD                    
              GO TO FIN-FST-FLD-CAB.                                    
      *>>>>
           MOVE -1 TO CAB-IND-IWFL-LEN IN CAB-FLD.
       FIN-FST-FLD-CAB.
           EXIT.
       
       PRO-KEY-CAB SECTION.
       INI-PRO-KEY-CAB.
           PERFORM PRO-IKY-CAB.
           PERFORM PRO-AKY-CAB.
           PERFORM PRO-MKY-CAB.
       FIN-PRO-KEY-CAB.
           EXIT.
       
       PRO-IKY-CAB SECTION.
       INI-PRO-IKY-CAB.
           MOVE FRM-CPIM-AEY TO CAB-CAI-CONV-ATR IN CAB-FLD.
           MOVE FRM-CPIM-AEY TO CAB-IIC-CONV-ATR IN CAB-FLD.
           MOVE FRM-CPIM-AEY TO CAB-IND-TIPO-ATR IN CAB-FLD.
           MOVE FRM-CPIM-AEY TO CAB-NUM-ICAB-ATR IN CAB-FLD.
       FIN-PRO-IKY-CAB.
           EXIT.
       
       PRO-MKY-CAB SECTION.
       INI-PRO-MKY-CAB.
       FIN-PRO-MKY-CAB.
           EXIT.
       
       PRO-AKY-CAB SECTION.
       INI-PRO-AKY-CAB.
       FIN-PRO-AKY-CAB.
           EXIT.
       
       UNP-KEY-CAB SECTION.
       INI-UNP-KEY-CAB.
           PERFORM UNP-IKY-CAB.
           PERFORM UNP-AKY-CAB.
           PERFORM UNP-MKY-CAB.
       FIN-UNP-KEY-CAB.
           EXIT.
       
       UNP-IKY-CAB SECTION.
       INI-UNP-IKY-CAB.
           MOVE FRM-CPIM-UBY TO CAB-CAI-CONV-ATR.
           MOVE FRM-CPIM-UBY TO CAB-IIC-CONV-ATR.
           MOVE FRM-CPIM-UBY TO CAB-IND-TIPO-ATR.
           MOVE FRM-CPIM-UBY TO CAB-NUM-ICAB-ATR.
       FIN-UNP-IKY-CAB.
           EXIT.
       
       UNP-MKY-CAB SECTION.
       INI-UNP-MKY-CAB.
       FIN-UNP-MKY-CAB.
           EXIT.
       
       UNP-AKY-CAB SECTION.
       INI-UNP-AKY-CAB.
       FIN-UNP-AKY-CAB.
           EXIT.
       
       UNP-FLD-CAB SECTION.
       INI-UNP-FLD-CAB.
           MOVE FRM-CPIM-UBY TO FRM-COD-METR-ATR IN CAB-FLD(1).
           MOVE FRM-CPIM-UBY TO FRM-COD-METR-ATR IN CAB-FLD(2).
           MOVE FRM-CPIM-UBY TO FRM-COD-METR-ATR IN CAB-FLD(3).
           MOVE FRM-CPIM-UBY TO FRM-COD-METR-ATR IN CAB-FLD(4).
           MOVE FRM-CPIM-UBY TO FRM-COD-METR-ATR IN CAB-FLD(5).
           MOVE FRM-CPIM-UBY TO FRM-COD-METR-ATR IN CAB-FLD(6).
           MOVE FRM-CPIM-UBY TO FRM-COD-METR-ATR IN CAB-FLD(7).
           MOVE FRM-CPIM-UBY TO FRM-COD-METR-ATR IN CAB-FLD(8).
           MOVE FRM-CPIM-UBY TO FRM-COD-METR-ATR IN CAB-FLD(9).
           MOVE FRM-CPIM-UBY TO FRM-COD-METR-ATR IN CAB-FLD(10).
           MOVE FRM-CPIM-UBY TO FRM-NUM-METR-ATR IN CAB-FLD(1).
           MOVE FRM-CPIM-UBY TO FRM-NUM-METR-ATR IN CAB-FLD(2).
           MOVE FRM-CPIM-UBY TO FRM-NUM-METR-ATR IN CAB-FLD(3).
           MOVE FRM-CPIM-UBY TO FRM-NUM-METR-ATR IN CAB-FLD(4).
           MOVE FRM-CPIM-UBY TO FRM-NUM-METR-ATR IN CAB-FLD(5).
           MOVE FRM-CPIM-UBY TO FRM-NUM-METR-ATR IN CAB-FLD(6).
           MOVE FRM-CPIM-UBY TO FRM-NUM-METR-ATR IN CAB-FLD(7).
           MOVE FRM-CPIM-UBY TO FRM-NUM-METR-ATR IN CAB-FLD(8).
           MOVE FRM-CPIM-UBY TO FRM-NUM-METR-ATR IN CAB-FLD(9).
           MOVE FRM-CPIM-UBY TO FRM-NUM-METR-ATR IN CAB-FLD(10).
           PERFORM UNP-CAB-CAB.
           PERFORM UNP-WFC-CAB.
           PERFORM UNP-DBC-CAB.
       FIN-UNP-FLD-CAB.
           EXIT.
       
       UNP-CAB-CAB SECTION.
       INI-UNP-CAB-CAB.
           MOVE FRM-CPIM-UBY TO CAB-IND-IWFL-ATR IN CAB-FLD.
           MOVE FRM-CPIM-UBY TO CAB-IND-SMWI-ATR IN CAB-FLD.
       FIN-UNP-CAB-CAB.
           EXIT.
       
       UNP-WFC-CAB SECTION.
       INI-UNP-WFC-CAB.
       FIN-UNP-WFC-CAB.
           EXIT.
       
       UNP-DBC-CAB SECTION.
       INI-UNP-DBC-CAB.
       FIN-UNP-DBC-CAB.
           EXIT.
       
       VAL-KEY-CAB SECTION.
       INI-VAL-KEY-CAB.
           PERFORM VAL-IKY-CAB.
           PERFORM VAL-MKY-CAB.
           PERFORM VAL-AKY-CAB.
       FIN-VAL-KEY-CAB.
           EXIT.
       
       VAL-IKY-CAB SECTION.
       INI-VAL-IKY-CAB.
           PERFORM VAL-CAB-CAI-CONV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAB-CAI-CONV-LEN IN CAB-FLD
                GO TO FIN-VAL-IKY-CAB.
           PERFORM VAL-CAB-IIC-CONV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAB-IIC-CONV-LEN IN CAB-FLD
                GO TO FIN-VAL-IKY-CAB.
           PERFORM VAL-CAB-IND-TIPO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAB-IND-TIPO-LEN IN CAB-FLD
                GO TO FIN-VAL-IKY-CAB.
           PERFORM VAL-CAB-NUM-ICAB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAB-NUM-ICAB-LEN IN CAB-FLD
                GO TO FIN-VAL-IKY-CAB.
       FIN-VAL-IKY-CAB.
