      * Prueba Programa procesa Tasas Maximas Convencionales

       ENVIRONMENT DIVISION.
      *=====================
       CONFIGURATION SECTION.
      *----------------------
       SPECIAL-NAMES.
               DECIMAL-POINT IS COMMA.

       DATA DIVISION.
      *==============

       WORKING-STORAGE SECTION.
      *------------------------
       COPY TABWGTXC.
       01  PGM-VARI.
           03 PGM-PLZO                   PIC X(04).
           03 PGM-VCOC                   PIC X(06).
           03 PGM-FECH.
              05 PGM-DECH                PIC 9(02).
              05 PGM-MECH                PIC 9(02).
              05 PGM-SECH                PIC 9(02).
              05 PGM-AECH                PIC 9(02).
           03 PGM-MNTO                   PIC 9(09).
           03 PGM-MNTO-EDT               PIC  ---.---.---.--9,9999.

       PROCEDURE DIVISION.
      *===================

       MAIN SECTION.
       INI-MAIN.
       COPY GNSBGEDB.
       LUP-MAIN.
           DISPLAY 'INGRESE PLAZO (' TXC-PLZO '): '.
           ACCEPT PGM-PLZO.
           IF PGM-PLZO = 'F' OR 'f'
               GO TO FIN-MAIN.
           IF PGM-PLZO > SPACES
               MOVE PGM-PLZO TO TXC-PLZO.
           DISPLAY 'PLAZO INGRESADO (' TXC-PLZO '): '.

           DISPLAY 'INGRESE MONEDA (' TXC-VCOC '): '.
           ACCEPT PGM-VCOC.
           IF PGM-VCOC > SPACES
               MOVE PGM-VCOC TO TXC-VCOC.
           DISPLAY 'MONEDA INGRESADA (' TXC-VCOC '): '.

           DISPLAY 'INGRESE FECHA 1 (DDMMSSAA) (' TXC-FEC1 '): '.
           ACCEPT PGM-FECH.
           IF PGM-FECH > ZEROES
               MOVE PGM-FECH TO TXC-FEC1.
           DISPLAY 'FECHA 1 INGRESADA (' TXC-FEC1 '): '.

           DISPLAY 'INGRESE FECHA 2 (DDMMSSAA) (' TXC-FEC2 '): '.
           ACCEPT PGM-FECH.
           IF PGM-FECH > ZEROES
               MOVE PGM-FECH TO TXC-FEC2.
           DISPLAY 'FECHA 2 INGRESADA (' TXC-FEC2 '): '.

           DISPLAY 'INGRESE MONTO : '.
           ACCEPT PGM-MNTO.
           IF PGM-MNTO > ZEROES
               MOVE PGM-MNTO TO TXC-MNTO.
           MOVE TXC-MNTO TO PGM-MNTO-EDT.
           DISPLAY 'MONTO INGRESADO (' PGM-MNTO-EDT '): '.

           PERFORM TAB-BUS-TXC.

           IF TXC-CMSG > SPACES
               DISPLAY TXC-CMSG ' - ' TXC-MENS
           ELSE
               PERFORM DISPLAY-TABLA.
           GO TO LUP-MAIN.
       FIN-MAIN.
           PERFORM GNS-PRO-END.

       DISPLAY-TABLA SECTION.
       INI-DISPLAY-TABLA.
           DISPLAY '--------------------------------------------------'.
           DISPLAY 'VALOR DE TASA      : ' TXC-VTXC.
           DISPLAY 'VALOR DE ACUMULADO : ' TXC-ATXC.
           DISPLAY '--------------------------------------------------'.
       FIN-DISPLAY-TABLA.
           EXIT.

       COPY TABBBTXC.
       COPY GNSBGEND.
