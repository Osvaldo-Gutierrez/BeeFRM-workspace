       COPY GNSWG3CF.
       COPY GNSWGTSK.
       COPY GNSWGQUE.
      *PTC-FIN
       COPY GNSWVNUM.
       COPY GNSWCNUM.
       COPY GNSWGFEC.
       COPY GNSWGHOY.
       COPY GNSWGSYS.

      *<<<< WSS
       COPY TABWGCRO.                                                   
       COPY GNSWGCPT.                                                   
       COPY GNSWGPES.                                                   
       01  WSS-VARI.                                                    
           03  WSS-I                              PIC 9(02).            
           03  WSS-FEC-CNT                        PIC X(08).            
           03  WSS-COD-PESO                       PIC X(06).            
           03  WSS-FEC-FROP.                                            
               05  WSS-NUM-SROP                   PIC 9(02).            
               05  WSS-NUM-AROP                   PIC 9(02).            
