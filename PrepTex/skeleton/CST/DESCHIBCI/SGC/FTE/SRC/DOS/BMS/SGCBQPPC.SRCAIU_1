           MOVE PPC-COD-TCTC IN PPC TO PPC-COD-TCTC IN PPC-FLD.
           MOVE PPC-CAI-AVAL IN PPC TO PPC-CAI-AVAL IN PPC-FLD.
           MOVE PPC-IIC-AVAL IN PPC TO PPC-IIC-AVAL IN PPC-FLD.
      *<<<< FIN_PUT_PPC_PPC
           MOVE PPC-VRF-ICAB IN PPC TO FRM-IND-ICAB IN PPC-FLD.
           MOVE PPC-VRF-ICCA IN PPC TO FRM-IND-ICCA IN PPC-FLD.

           IF PPC-MSC-STAT IN PPC-FLD > SPACES
              MOVE 'GNS'                   TO TAB-COD-SIST
              MOVE 'STT'                   TO TAB-COD-TTAB IN TAB
              MOVE PPC-MSC-STAT IN PPC-FLD TO TAB-COD-CTAB IN TAB
              PERFORM BUS-TAB
              MOVE TAB-GLS-DCOR IN TAB TO FRM-GLS-STAT IN PPC-FLD
           ELSE
              MOVE SPACES              TO FRM-GLS-STAT IN PPC-FLD.
           
           IF PPC-COD-TOOC IN PPC-FLD = '*  '
              MOVE 'TODOS' TO FRM-GLS-TOOC IN PPC-FLD
           ELSE
           IF PPC-COD-TOOC IN PPC-FLD > SPACES
              MOVE 'TAB'                   TO TAB-COD-SIST
              MOVE 'TIO'                   TO TAB-COD-TTAB IN TAB
              MOVE PPC-COD-TOOC IN PPC-FLD TO TAB-COD-CTAB IN TAB
              PERFORM BUS-TAB
              MOVE TAB-GLS-DCOR IN TAB TO FRM-GLS-TOOC IN PPC-FLD.

           IF PPC-COD-AOOC IN PPC-FLD = '*  '
              MOVE 'TODOS' TO FRM-GLS-AOOC IN PPC-FLD
           ELSE
           IF PPC-COD-AOOC IN PPC-FLD > SPACES
              MOVE 'TAB'                    TO TAB-COD-SIST
              MOVE 'AUX'                    TO TAB-COD-TTAB IN TAB
              MOVE PPC-COD-TOOC IN PPC-FLD  TO WSS-COD-TOOC IN WSS-VARI
              MOVE PPC-COD-AOOC IN PPC-FLD  TO WSS-COD-AOOC IN WSS-VARI
              MOVE WSS-COD-CAUX IN WSS-VARI TO TAB-COD-CTAB IN TAB
              PERFORM BUS-TAB
              MOVE TAB-GLS-DCOR IN TAB TO FRM-GLS-AOOC IN PPC-FLD.

           IF PPC-COD-OFIC IN PPC-FLD > SPACES
              MOVE PPC-COD-OFIC IN PPC-FLD TO OFI-COD-OFIC IN OFI
              PERFORM BUS-OFI
              MOVE OFI-GLS-DESC IN OFI TO FRM-GLS-OFIC IN PPC-FLD
           ELSE
              MOVE SPACES              TO FRM-GLS-OFIC IN PPC-FLD.

           IF PPC-NUM-MES1 IN PPC-FLD > ZEROES
              IF PPC-NUM-MES1 IN PPC-FLD = 1
                 MOVE 'ENERO' TO FRM-GLS-MES1 IN PPC-FLD
              ELSE
              IF PPC-NUM-MES1 IN PPC-FLD = 2
                 MOVE 'FEBRERO' TO FRM-GLS-MES1 IN PPC-FLD
              ELSE
              IF PPC-NUM-MES1 IN PPC-FLD = 3
                 MOVE 'MARZO' TO FRM-GLS-MES1 IN PPC-FLD
              ELSE
              IF PPC-NUM-MES1 IN PPC-FLD = 4
                 MOVE 'ABRIL' TO FRM-GLS-MES1 IN PPC-FLD
              ELSE
              IF PPC-NUM-MES1 IN PPC-FLD = 5
                 MOVE 'MAYO' TO FRM-GLS-MES1 IN PPC-FLD
              ELSE
              IF PPC-NUM-MES1 IN PPC-FLD = 6
                 MOVE 'JUNIO' TO FRM-GLS-MES1 IN PPC-FLD
              ELSE
              IF PPC-NUM-MES1 IN PPC-FLD = 7
                 MOVE 'JULIO' TO FRM-GLS-MES1 IN PPC-FLD
              ELSE
              IF PPC-NUM-MES1 IN PPC-FLD = 8
                 MOVE 'AGOSTO' TO FRM-GLS-MES1 IN PPC-FLD
              ELSE
              IF PPC-NUM-MES1 IN PPC-FLD = 9
                 MOVE 'SEPTIEMBRE' TO FRM-GLS-MES1 IN PPC-FLD
              ELSE
              IF PPC-NUM-MES1 IN PPC-FLD = 10
                 MOVE 'OCTUBRE' TO FRM-GLS-MES1 IN PPC-FLD
              ELSE
              IF PPC-NUM-MES1 IN PPC-FLD = 11
                 MOVE 'NOVIEMBRE' TO FRM-GLS-MES1 IN PPC-FLD
              ELSE
              IF PPC-NUM-MES1 IN PPC-FLD = 12
                 MOVE 'DICIEMBRE' TO FRM-GLS-MES1 IN PPC-FLD
              ELSE
                 MOVE 'ERROR'     TO FRM-GLS-MES1 IN PPC-FLD
           ELSE
              MOVE SPACES TO FRM-GLS-MES1 IN PPC-FLD.

           IF PPC-NUM-MES2 IN PPC-FLD > ZEROES
              IF PPC-NUM-MES2 IN PPC-FLD = 1
                 MOVE 'ENERO' TO FRM-GLS-MES2 IN PPC-FLD
              ELSE
              IF PPC-NUM-MES2 IN PPC-FLD = 2
                 MOVE 'FEBRERO' TO FRM-GLS-MES2 IN PPC-FLD
              ELSE
              IF PPC-NUM-MES2 IN PPC-FLD = 3
                 MOVE 'MARZO' TO FRM-GLS-MES2 IN PPC-FLD
              ELSE
              IF PPC-NUM-MES2 IN PPC-FLD = 4
                 MOVE 'ABRIL' TO FRM-GLS-MES2 IN PPC-FLD
              ELSE
              IF PPC-NUM-MES2 IN PPC-FLD = 5
                 MOVE 'MAYO' TO FRM-GLS-MES2 IN PPC-FLD
              ELSE
              IF PPC-NUM-MES2 IN PPC-FLD = 6
                 MOVE 'JUNIO' TO FRM-GLS-MES2 IN PPC-FLD
              ELSE
              IF PPC-NUM-MES2 IN PPC-FLD = 7
                 MOVE 'JULIO' TO FRM-GLS-MES2 IN PPC-FLD
              ELSE
              IF PPC-NUM-MES2 IN PPC-FLD = 8
                 MOVE 'AGOSTO' TO FRM-GLS-MES2 IN PPC-FLD
              ELSE
              IF PPC-NUM-MES2 IN PPC-FLD = 9
                 MOVE 'SEPTIEMBRE' TO FRM-GLS-MES2 IN PPC-FLD
              ELSE
              IF PPC-NUM-MES2 IN PPC-FLD = 10
                 MOVE 'OCTUBRE' TO FRM-GLS-MES2 IN PPC-FLD
              ELSE
              IF PPC-NUM-MES2 IN PPC-FLD = 11
                 MOVE 'NOVIEMBRE' TO FRM-GLS-MES2 IN PPC-FLD
              ELSE
              IF PPC-NUM-MES2 IN PPC-FLD = 12
                 MOVE 'DICIEMBRE' TO FRM-GLS-MES2 IN PPC-FLD
              ELSE
                 MOVE 'ERROR'     TO FRM-GLS-MES2 IN PPC-FLD
           ELSE
              MOVE SPACES TO FRM-GLS-MES2 IN PPC-FLD.

           IF PPC-COD-TCTC IN PPC-FLD > SPACES
              MOVE 'COL'                   TO TAB-COD-SIST
              MOVE 'PAC'                   TO TAB-COD-TTAB IN TAB
              MOVE PPC-COD-TCTC IN PPC-FLD TO TAB-COD-CTAB IN TAB
              PERFORM BUS-TAB
              MOVE TAB-GLS-DCOR IN TAB TO FRM-GLS-TCTC IN PPC-FLD
           ELSE
              MOVE SPACES              TO FRM-GLS-TCTC IN PPC-FLD.
      *>>>>
       FIN-PUT-PPC-PPC.
           EXIT.
       
