       FIN-INI.
           EXIT.

       COPY GNSBHSYS.
       COPY GNSBHFRM.
       COPY GNSBPRCK.
       COPY GNSBBVCA.
       COPY GNSBBIDD.

       SCR-COM SECTION.
       INI-SCR-COM.
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-CLRA-QPA.
           PERFORM EDT-KEY-QPA.
           PERFORM EDT-FLD-QPA.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-HCM TO SCR-INST.
           MOVE SCR-CMNP     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       XCT-ERR.
           MOVE 'XCTERR    ' TO MSG-COD-MENS.
           MOVE 'GNS'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO SYS-MEN1.
           MOVE SYS-PROG     TO SYS-MEN2.
           MOVE SYS-ABOR     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       COPY GNSBGGBK.
       FIN-SCR-COM.
           EXIT.

       SCR-MOD SECTION.
       INI-SCR-MOD.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-MOD
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-MOD.
           PERFORM DESSCRTRL.
       MSG-SCR-MOD.
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-MOD.
           EXIT.

       BRW-SCR-MOD SECTION.
       INI-BRW-SCR-MOD.
       FIN-BRW-SCR-MOD.
           EXIT.

       SCR-ING SECTION.
       INI-SCR-ING.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-IKY
               MOVE SCR-INST-GKY TO SCR-INST
      *PTC-INI
               IF NOT SCR-OFRM-SRV
                   PERFORM SCR-DMSG
               ELSE
                   PERFORM SCR-PUTA
      *PTC-FIN
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-RST
                PERFORM GET-IKY
                IF NOT FRM-STAT-ABT
                        MOVE SPACES TO FRM-MENS
                        PERFORM FND-KEY
                        IF NOT FIO-STAT-OKS AND
                           FIO-STAT NOT = FIO-STAT-NEX
      *JSS                  PERFORM CLR-FLD-QPA
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
                            PERFORM SCR-TRL
                        ELSE
      *PTC-INI
                        MOVE 'S' TO SCR-IERR
      *PTC-FIN
                        MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR
                        IF FRM-MENS NOT > SPACES
                            MOVE 'REGEXI    ' TO MSG-COD-MENS
                            MOVE 'GNS' TO MSG-COD-SIST
                            PERFORM GET-MSG
                            MOVE MSG-GLS-DESC TO FRM-MENS
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-ING
                        ELSE
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-ING
                ELSE
                        MOVE SPACES TO FRM-MENS
                        PERFORM SCR-MSSG
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ING
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON.
           PERFORM DESSCRTRL.
       MSG-SCR-ING.
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ING.
           EXIT.

       SCR-ACC SECTION.
       INI-SCR-ACC.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ACC
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-ACC.
           PERFORM DESSCRTRL.
      *<<<< PFS_SCR_ACC                                                 
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6                   
               PERFORM VAL-NUM-KEY-QPA                                  
               PERFORM VAL-NUM-FLD-QPA                                  
               PERFORM SCRL-TAB-QPA                                     
               PERFORM SCR-TRL.                                         
      *>>>>                                                             
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ACC.
           EXIT.

       BRW-SCR-ACC SECTION.
       INI-BRW-SCR-ACC.
       FIN-BRW-SCR-ACC.
           EXIT.

       SCR-ELI SECTION.
       INI-SCR-ELI.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ELI
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-ELI.
           PERFORM DESSCRTRL.
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ELI.
           EXIT.

       BRW-SCR-ELI SECTION.
       INI-BRW-SCR-ELI.
       FIN-BRW-SCR-ELI.
           EXIT.

       SCR-ACT SECTION.
       INI-SCR-ACT.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-RST
                PERFORM GET-KEY
                IF NOT FRM-STAT-ABT
                        MOVE SPACES TO FRM-MENS
                        PERFORM FND-KEY
                        IF FIO-STAT-OKS
                            MOVE SCR-ACTU-MOD TO SCR-ACTU
                            PERFORM PUT-SCR
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
                            PERFORM SCR-TRL
                        ELSE
                        MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR
                        IF FIO-STAT-EOF
                            IF FRM-MENS NOT > SPACES
                                MOVE 'REGEOF    ' TO MSG-COD-MENS
                                MOVE 'GNS' TO MSG-COD-SIST
                                PERFORM GET-MSG
                                MOVE MSG-GLS-DESC TO FRM-MENS
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                            ELSE
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                        ELSE
                        IF FIO-STAT = FIO-STAT-NEX
                            IF FRM-MENS NOT > SPACES
                                MOVE 'REGNEX    ' TO MSG-COD-MENS
                                MOVE 'GNS' TO MSG-COD-SIST
                                PERFORM GET-MSG
                                MOVE MSG-GLS-DESC TO FRM-MENS
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                            ELSE
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                        ELSE
                            MOVE SCR-ACTU-ING TO SCR-ACTU
      *JSS                  PERFORM CLR-FLD-QPA
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
                            PERFORM SCR-TRL
                ELSE
                        MOVE SPACES TO FRM-MENS
                        PERFORM SCR-MSSG
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ACT
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-ACT.
           PERFORM DESSCRTRL.
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ACT.
           EXIT.

       BRW-SCR-ACT SECTION.
       INI-BRW-SCR-ACT.
       FIN-BRW-SCR-ACT.
           EXIT.

       SCR-PGM SECTION.
       INI-SCR-PGM.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-RST
                PERFORM GET-KEY
                IF FRM-STAT-ABT
                    MOVE SPACES TO FRM-MENS
                    PERFORM SCR-MSSG
                    PERFORM DESSCRTRL
                    GO TO FIN-SCR-PGM
                ELSE
                    MOVE SPACES TO FRM-MENS
                    PERFORM FND-KEY
                    IF FIO-STAT = FIO-STAT-NEX
                        IF FRM-MENS NOT > SPACES
                            MOVE 'REGNEX    ' TO MSG-COD-MENS
                            MOVE 'GNS' TO MSG-COD-SIST
                            PERFORM GET-MSG
                            MOVE MSG-GLS-DESC TO FRM-MENS
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-PGM
                        ELSE
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-PGM
                    ELSE
                        MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
                        PERFORM SCR-TRL
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-PGM.
       TRL-SCR-PGM.
           PERFORM DESSCRTRL.
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-PGM.
           EXIT.

       BRW-SCR-PGM SECTION.
       INI-BRW-SCR-PGM.
       FIN-BRW-SCR-PGM.
           EXIT.

       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               PERFORM SCR-CLRA-QPA
               PERFORM EDT-KEY-QPA
               PERFORM EDT-FLD-QPA
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-QPA
               PERFORM EDT-KEY-QPA
           ELSE
               PERFORM CLR-FLD-QPA
               PERFORM EDT-FLD-QPA.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

       SCR-KEY SECTION.
       INI-SCR-KEY.
           PERFORM SCR-RST.
           PERFORM GET-KEY.
           IF FRM-STAT-ABT
                   MOVE SPACES TO FRM-MENS
                   PERFORM SCR-MSSG
                   GO TO FIN-SCR-KEY.
       FND-SCR-KEY.
           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
           IF FIO-STAT-OKS
               PERFORM PUT-SCR
               MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
               GO TO FIN-SCR-KEY.
      *JSS ELSE
      *        PERFORM CLR-FLD-QPA.
       ERR-SCR-KEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR.
           IF FRM-MENS NOT > SPACES
               IF FIO-STAT-EOF
                   MOVE 'REGEOF    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
               IF FIO-STAT = FIO-STAT-NEX
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
           MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-SCR-KEY.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO QPA-FLD.
           MOVE LOW-VALUES TO QPA-EDT.
       FIN-SCR-RST.
           EXIT.

       GET-KEY SECTION.
       INI-GET-KEY.
           PERFORM SCR-GKEY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-KEY.
           EXIT.

       GET-IKY SECTION.
       INI-GET-IKY.
           PERFORM SCR-GIKY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-IKY.
           EXIT.

       FND-KEY SECTION.
       INI-FND-KEY.
      *<<<< INI_FND_KEY                                                 
           MOVE PAE-CAI-IFOL IN QPA-FLD TO PAE-CAI-IFOL IN PAE.         
           MOVE PAE-IIC-IFOL IN QPA-FLD TO PAE-IIC-IFOL IN PAE.         
      *INI-RFS                                                          
           IF FRM-IND-SGTE IN QPA-FLD = 'S' AND                         
              FRM-NUM-IDBN IN QPA-FLD > ZEROS                           
              MOVE FRM-NUM-IDBN IN QPA-FLD TO PAE-NUM-IPAE IN PAE       
           ELSE                                                         
              MOVE ZEROES TO PAE-NUM-IPAE IN PAE.                       
      *FIN-RFS                                                          
           MOVE ZEROES TO WSS-J IN WSS-VARI.                            
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-LEE-PAE.                                                     
           PERFORM SGC-FIO-PAE.                                         
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF                          
               GO TO CON-FND-KEY.                                       
                                                                        
           IF PAE-CAI-IFOL IN QPA-FLD     NOT = PAE-CAI-IFOL IN PAE OR  
              PAE-IIC-IFOL IN QPA-FLD     NOT = PAE-IIC-IFOL IN PAE     
              GO TO CON-FND-KEY.                                        
                                                                        
           PERFORM CRG-VEC-QPA.                                         
                                                                        
           IF (SCR-OFRM-SRV AND WSS-J > WSS-NITMMAX)                    
               GO TO CON-FND-KEY.                                       
                                                                        
           IF ((NOT SCR-OFRM-SRV) AND WSS-J > WSS-JMAX)                 
               GO TO CON-FND-KEY.                                       
                                                                        
       NXT-LEE-PAE.                                                     
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-LEE-PAE.                                           
      *>>>>                                                             
           PERFORM KEY-ALL-QPA.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-PAE.
           IF FIO-STAT-OKS
               MOVE PAE-STP-ITRN IN PAE TO SCR-UMOD.
           MOVE FIO-STAT TO PGM-STAT-PAE.
      *<<<< FIN_FND_KEY                                                 
       CON-FND-KEY.                                                     
           PERFORM PUT-TAB.                                             
                                                                        
           MOVE '00' TO FIO-STAT.                                       
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           GO TO FIN-FND-KEY.                                           
      *----------------------------------------------------------       
       MSG-FND-KEY.                                                     
           MOVE 'TAB' TO MSG-COD-SIST.                                  
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MENS.                               
           MOVE '00' TO FIO-STAT.                                       
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           GO TO FIN-FND-KEY.                                           
      *>>>>      
       FIN-FND-KEY.
           IF FIO-STAT = FIO-STAT-NEX
               MOVE 'S' TO SCR-IERR.
       EXT-FND-KEY.
           EXIT.
 
       PUT-SCR SECTION.
       INI-PUT-SCR.
           IF PGM-STAT-PAE-OKS
               MOVE PAE-STP-ITRN IN PAE TO SCR-UMOD.
           PERFORM PUT-ALL-QPA.
       FIN-PUT-SCR.
           EXIT.

       STP-EXIT SECTION.
       INI-STP-EXIT.
           MOVE SCR-PROG TO PGM-PROG.
           MOVE SCR-NTRN TO PGM-NTRN.

           MOVE SCR-PROV TO SCR-PROG.
           MOVE SCR-NTRV TO SCR-NTRN.

           MOVE PGM-PROG TO SCR-PROV
           MOVE PGM-NTRN TO SCR-NTRV.
       FIN-STP-EXIT.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO SYS-NTRN.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
      *PTC-INI
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE QPA-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO QPA-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GKEY SECTION.
       INI-SCR-GKEY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-QPA
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO QPA-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GKEY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-QPA.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-QPA
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-QPA.
           PERFORM VAL-IKY-QPA.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-MKY-QPA.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-AKY-QPA.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-CON-KEY-QPA.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GKEY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-QPA.
       ERR-SCR-GKEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-QPA.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GKEY.
           PERFORM SCR-MSSG.

       SCR-GIKY SECTION.
       INI-SCR-GIKY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-QPA
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO QPA-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GIKY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-QPA.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-IKY-QPA
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-QPA.
           PERFORM VAL-IKY-QPA.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GIKY.
           PERFORM VAL-CON-IKY-QPA.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GIKY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-QPA.
       ERR-SCR-GIKY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-IKY-QPA.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GIKY.
           PERFORM SCR-MSSG.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
      *<<<< PFS_GET_CON                                                 
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )           
      *>>>>                                                             
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO QPA-GLS-MEN2
               MOVE SCR-PRMT     TO QPA-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO QPA-FLD.
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO QPA-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE QPA-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO QPA-FLD
      * FLB  7-NOV-1994
               PERFORM CLR-KEY-QPA

               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO QPA-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO QPA-GLS-MENS.
           PERFORM PRO-MKY-QPA.
           PERFORM PRO-AKY-QPA.
           PERFORM UNP-IKY-QPA.
           PERFORM FST-KEY-QPA.
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE QPA-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO QPA-FLD
               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO QPA-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO QPA-GLS-MENS.
           PERFORM UNP-KEY-QPA.
           PERFORM FST-KEY-QPA.
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
           PERFORM PRO-IKY-QPA.
           PERFORM PRO-AKY-QPA.
           PERFORM UNP-MKY-QPA.
           PERFORM UNP-FLD-QPA.
           PERFORM FST-FLD-QPA.
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO QPA-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-QPA.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
      *PTC-INI
           IF SCR-STAT NOT = SCR-STPP
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-QPA
               PERFORM EDT-FLD-QPA
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-QPA
               PERFORM EDT-FLD-QPA
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-QPA.
           PERFORM EDT-FLD-QPA.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

      * PGM-EBR-ALL SECTION.
      * INI-PGM-EBR-ALL.
      *     MOVE 'GNS' TO FIO-SIST.
      *     MOVE FIO-EBR-ALL TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
      * FIN-PGM-EBR-ALL.
      *     EXIT.

       COPY SGCBQQPA.
       COPY GNSBVNUM.
       COPY SGCBFPAE.
       COPY GNSBFMSG.
       COPY GNSBGSYS.
       COPY GNSBGDTC.
      *PTC-INI
      *PTC-SUB
      * ISP  10-MAY-2001 13:46:35 
      * COPY GNSBGFRM.
       COPY GNSBG3CF.
      *PTC-FIN
       COPY GNSBGMSG.
       COPY GNSBEKEY.
       COPY GNSBIABT.
       COPY GNSBFTAB.
       COPY GNSBVIND.
       COPY GNSBVMSC.
       COPY GNSBFMSC.
       COPY GNSBVIDC.
       COPY GNSBPVRF.
      *PTC-INI
       COPY GNSBGQUE.
       COPY GNSBGTSK.
      *PTC-FIN

      *<<<< EOF                                                         
       COPY SGCBFPAE
       COPY GNSBBTAB.                                                   
       COPY GNSBVTAB.                                                   
       COPY GNSBVCOD.                                                   
      *COPY GNSBPFEC.                                                   
       COPY SGCBFDBC.                                                   
       COPY SGCBFRPR.                                                   
                                                                        
       PUT-TAB SECTION.                                                 
       INI-PUT-TAB.                                                     
           MOVE QPA-NOM-TSQPA     TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE QPA-NOM-TSQPA-MAS TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE 1 TO SCR-NUM-PANT.                                      
           MOVE LOW-VALUES TO WSS-GLS-NQPA-IDX-MTI.                     
           MOVE LOW-VALUES TO WSS-GLS-NQPA-IDX-FRM.                     
      *                                                                 
           MOVE 1 TO FRM-IFLD.                                          
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.                        
                                                                        
           MOVE 1 TO WSS-F IN WSS-VARI.                                 
       LUP-PUT-TAB.                                                     
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI                  
              GO TO FIN-PUT-TAB.                                        
           IF WSS-NUM-CORR-V(WSS-F) > SPACES                            
              PERFORM PUT-TAB-QPA                                       
              PERFORM PUT-QPA-QUE                                       
              PERFORM SET-FRM-IFLD                                      
           ELSE                                                         
           IF FRM-IFLD = 1 AND WSS-F = 1                                
              MOVE 'TAB' TO MSG-COD-SIST                                
              MOVE 'QPA    NEX' TO MSG-COD-MENS                         
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              MOVE '00' TO FIO-STAT                                     
      *       MOVE 'S' TO SCR-IERR                                      
              GO TO FIN-PUT-TAB                                         
           ELSE                                                         
              MOVE '00' TO FIO-STAT                                     
              GO TO FIN-PUT-TAB.                                        
                                                                        
           ADD 1 TO WSS-F IN WSS-VARI.                                  
           GO TO LUP-PUT-TAB.                                           
       FIN-PUT-TAB.                                                     
           MOVE 1                  TO QUE-NITM.                         
           MOVE QPA-LEN-TSQPA-MAS  TO QUE-LITM.                         
           MOVE QPA-NOM-TSQPA-MAS  TO QUE-COLA.                         
                                                                        
           IF WSS-NITM > WSS-NITMMAX                                    
              MOVE 'S'           TO QPA-MAS-IND-HMAS                    
           ELSE                                                         
              MOVE 'N'           TO QPA-MAS-IND-HMAS.                   
                                                                        
           MOVE WSS-NITM         TO WSS-NUM-ICUR.                       
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.                   
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.                   
           MOVE WSS-NUM-ICUR-ALF TO QPA-MAS-NUM-TOCC.                   
           MOVE QPA-MAS-ITEM     TO QUE-ITEM.                           
           MOVE QUE-PUT          TO QUE-CMND.                           
           PERFORM GNS-PRO-QUE.                                         
           IF WSS-NITM > WSS-IMAX                                       
               MOVE 'SGC' TO MSG-COD-SIST                               
               MOVE 'QPATABDMAS' TO MSG-COD-MENS                        
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN2.                           
       EXT-PUT-TAB.                                                     
           EXIT.                                                        
                                                                        
       SET-FRM-IFLD SECTION.                                            
       INI-SET-FRM-IFLD.                                                
           IF SCR-OFRM-SRV                                              
                MOVE 1 TO FRM-IFLD                                      
           ELSE                                                         
                ADD 1 TO FRM-IFLD.                                      
       FIN-SET-FRM-IFLD.                                                
           EXIT.                                                        
      *--------------------------------------------                     
       SCRL-TAB-QPA SECTION.                                            
       INI-SCRL-TAB-QPA.                                                
           MOVE SPACES TO MSG-COD-MENS.                                 
                                                                        
           IF FRM-FFLD = FRM-FFLD-PF6                                   
               PERFORM PUT-TAB-ASC                                      
           ELSE                                                         
               PERFORM PUT-TAB-DSC.                                     
           IF MSG-COD-MENS > SPACES                                     
               MOVE 'TAB' TO MSG-COD-SIST                               
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN2.                           
       FIN-SCRL-TAB-QPA.                                                
           EXIT.                                                        
                                                                        
      *--------------------------------------------                     
       PUT-TAB-QPA SECTION.                                             
       INI-PUT-TAB-QPA.                                                 
           PERFORM CLR-WSS.                                             
           MOVE WSS-NUM-CORR-V(WSS-F) TO WSS-NUM-CORR-QPA IN WSS-VARI.  
           MOVE WSS-GLS-APAT-V(WSS-F) TO WSS-GLS-APAT-QPA IN WSS-VARI.  
           MOVE WSS-GLS-NOMB-V(WSS-F) TO WSS-GLS-NOMB-QPA IN WSS-VARI.  
           MOVE WSS-DIA-FCTR-V(WSS-F) TO WSS-DIA-FCTR-QPA IN WSS-VARI.  
           MOVE WSS-MES-FCTR-V(WSS-F) TO WSS-MES-FCTR-QPA IN WSS-VARI.  
           MOVE WSS-ANS-FCTR-V(WSS-F) TO WSS-ANS-FCTR-QPA IN WSS-VARI.  
           MOVE WSS-COD-PTZC-V(WSS-F) TO WSS-COD-PTZC-QPA IN WSS-VARI.  
           MOVE WSS-GLS-PTZC-V(WSS-F) TO WSS-GLS-PTZC-QPA IN WSS-VARI.  
           MOVE WSS-COD-ORGN-V(WSS-F) TO WSS-COD-ORGN-QPA IN WSS-VARI.  
           MOVE WSS-GLS-ORGN-V(WSS-F) TO WSS-GLS-ORGN-QPA IN WSS-VARI.  
           MOVE WSS-COD-CRGO-V(WSS-F) TO WSS-COD-CRGO-QPA IN WSS-VARI.  
           MOVE WSS-GLS-CRGO-V(WSS-F) TO WSS-GLS-CRGO-QPA IN WSS-VARI.  
           MOVE WSS-MSC-STAT-V(WSS-F) TO WSS-MSC-STAT-QPA IN WSS-VARI.  
                                                                        
           MOVE WSS-NUM-CORR-QPA IN WSS-VARI TO WSS-NUM-CORR-FRM.       
           MOVE WSS-GLS-APAT-QPA IN WSS-VARI TO WSS-GLS-APAT-FRM.       
           MOVE WSS-GLS-NOMB-QPA IN WSS-VARI TO WSS-GLS-NOMB-FRM.       
           MOVE WSS-DIA-FCTR-QPA IN WSS-VARI TO WSS-DIA-FCTR-FRM.       
           MOVE WSS-MES-FCTR-QPA IN WSS-VARI TO WSS-MES-FCTR-FRM.       
           MOVE WSS-ANS-FCTR-QPA IN WSS-VARI TO WSS-ANS-FCTR-FRM.       
           MOVE WSS-COD-PTZC-QPA IN WSS-VARI TO WSS-COD-PTZC-FRM.       
           MOVE WSS-GLS-PTZC-QPA IN WSS-VARI TO WSS-GLS-PTZC-FRM.       
           MOVE WSS-COD-ORGN-QPA IN WSS-VARI TO WSS-COD-ORGN-FRM.       
           MOVE WSS-GLS-ORGN-QPA IN WSS-VARI TO WSS-GLS-ORGN-FRM.       
           MOVE WSS-COD-CRGO-QPA IN WSS-VARI TO WSS-COD-CRGO-FRM.       
           MOVE WSS-GLS-CRGO-QPA IN WSS-VARI TO WSS-GLS-CRGO-FRM.       
           MOVE WSS-MSC-STAT-QPA IN WSS-VARI TO WSS-MSC-STAT-FRM.
                                                                        
           IF FRM-IFLD > WSS-IMAX                                       
              GO TO FIN-PUT-TAB-QPA.                                    
                                                                        
           MOVE WSS-NUM-CORR-QPA IN WSS-VARI TO                         
               FRM-NUM-IPAE IN QPA-FLD(FRM-IFLD).                       
           MOVE WSS-GLS-APAT-QPA IN WSS-VARI TO                         
               FRM-GLS-APAT IN QPA-FLD(FRM-IFLD).                       
           MOVE WSS-GLS-NOMB-QPA IN WSS-VARI TO                         
               FRM-GLS-NOMB IN QPA-FLD(FRM-IFLD).                       
           MOVE WSS-DIA-FCTR-QPA IN WSS-VARI TO                         
               FRM-DIA-FCTR IN QPA-FLD(FRM-IFLD).                       
           MOVE WSS-MES-FCTR-QPA IN WSS-VARI TO                         
               FRM-MES-FCTR IN QPA-FLD(FRM-IFLD).                       
           MOVE WSS-ANS-FCTR-QPA IN WSS-VARI TO                         
               FRM-ANS-FCTR IN QPA-FLD(FRM-IFLD).                       
           MOVE WSS-COD-PTZC-QPA IN WSS-VARI TO                         
               FRM-COD-PTZC IN QPA-FLD(FRM-IFLD).                       
           MOVE WSS-GLS-PTZC-QPA IN WSS-VARI TO                         
               FRM-GLS-PTZC IN QPA-FLD(FRM-IFLD).                       
           MOVE WSS-COD-ORGN-QPA IN WSS-VARI TO                         
               FRM-COD-ORGN IN QPA-FLD(FRM-IFLD).                       
           MOVE WSS-GLS-ORGN-QPA IN WSS-VARI TO                         
               FRM-GLS-ORGN IN QPA-FLD(FRM-IFLD).                       
           MOVE WSS-COD-CRGO-QPA IN WSS-VARI TO                         
               FRM-COD-CRGO IN QPA-FLD(FRM-IFLD).                       
           MOVE WSS-GLS-CRGO-QPA IN WSS-VARI TO                         
               FRM-GLS-CRGO IN QPA-FLD(FRM-IFLD).                       
           MOVE WSS-MSC-STAT-QPA IN WSS-VARI TO                         
               FRM-MSC-STAT IN QPA-FLD(FRM-IFLD).                       
       FIN-PUT-TAB-QPA.                                                 
           EXIT.                                                        
      *----------------------------------------------------             
       PUT-QPA-QUE SECTION.                                             
       INI-PUT-QPA-QUE.                                                 
           PERFORM PRO-FLD-QUE.                                         
           ADD 1 TO WSS-NITM.                                           
           MOVE QPA-NOM-TSQPA    TO QUE-COLA.                           
           MOVE QPA-LEN-TSQPA    TO QUE-LITM.                           
           MOVE WSS-NITM         TO QUE-NITM.                           
           MOVE SPACES           TO QUE-ITEM.                           
           IF SCR-OFRM-SRV                                              
              MOVE WSS-GLS-NQPA-IDX-MTI  TO QUE-ITEM                    
           ELSE                                                         
              MOVE WSS-GLS-NQPA-IDX-FRM  TO QUE-ITEM.                   
           MOVE QUE-PUT  TO QUE-CMND.                                   
           PERFORM GNS-PRO-QUE.                                         
       FIN-PUT-QPA-QUE.                                                 
           EXIT.                                                        
      *----------------------------------------------------             
                                                                        
       PUT-TAB-ASC SECTION.                                             
       INI-PUT-TAB-ASC.                                                 
           MOVE SPACES TO MSG-COD-MENS.                                 
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.           
           MOVE 0 TO FRM-IFLD.                                          
       LUP-PUT-TAB-ASC.                                                 
           ADD 1              TO QUE-NITM.                              
           MOVE QPA-NOM-TSQPA TO QUE-COLA.                              
           MOVE QPA-LEN-TSQPA TO QUE-LITM.                              
           MOVE QUE-GET       TO QUE-CMND.                              
           PERFORM GNS-PRO-QUE.                                         
           IF QUE-STAT NOT = QUE-STAT-OKS                               
               IF FRM-IFLD = 0                                          
                   MOVE 'QPATABNONXT' TO MSG-COD-MENS                   
                   GO TO FIN-PUT-TAB-ASC                                
               ELSE                                                     
                   GO TO FIN-PUT-TAB-ASC.                               
           ADD 1 TO FRM-IFLD.                                           
           MOVE QUE-ITEM TO FRM-NUM-IPAE-IDX(FRM-IFLD).                 
           IF FRM-IFLD < WSS-IMAX                                       
               GO TO LUP-PUT-TAB-ASC.                                   
       FIN-PUT-TAB-ASC.                                                 
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX                      
               ADD 1 FRM-IFLD GIVING WSS-I                              
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1            
                                 UNTIL WSS-I > WSS-IMAX.                
           IF FRM-IFLD > 0                                              
               ADD 1 TO SCR-NUM-PANT.                                   
           IF FRM-IFLD = WSS-IMAX                                       
               ADD 1              TO QUE-NITM                           
               MOVE QPA-NOM-TSQPA TO QUE-COLA                           
               MOVE QPA-LEN-TSQPA TO QUE-LITM                           
               MOVE QUE-GET       TO QUE-CMND                           
               PERFORM GNS-PRO-QUE                                      
               IF QUE-STAT = QUE-STAT-OKS                               
                   MOVE 'SGC'        TO MSG-COD-SIST                    
                   MOVE 'QPATABDMAS' TO MSG-COD-MENS.                   
       EXT-PUT-TAB-ASC.                                                 
           EXIT.                                                        
                                                                        
       PUT-TAB-DSC SECTION.                                             
       INI-PUT-TAB-DSC.                                                 
           MOVE SPACES TO MSG-COD-MENS.                                 
           IF SCR-NUM-PANT = 1                                          
               MOVE 'QPATABNOPRV' TO MSG-COD-MENS                       
               GO TO EXT-PUT-TAB-DSC.                                   
                                                                        
           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.                
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.               
           ADD 1 TO QUE-NITM.                                           
           ADD 1 WSS-IMAX GIVING FRM-IFLD.                              
       LUP-PUT-TAB-DSC.                                                 
           SUBTRACT  1 FROM QUE-NITM.                                   
           MOVE QPA-NOM-TSQPA TO QUE-COLA                               
           MOVE QPA-LEN-TSQPA TO QUE-LITM                               
           MOVE QUE-GET       TO QUE-CMND.                              
           PERFORM GNS-PRO-QUE.                                         
           IF QUE-STAT NOT = QUE-STAT-OKS                               
               IF FRM-IFLD > WSS-IMAX                                   
                   MOVE 'QPATABNOPRV' TO MSG-COD-MENS                   
                   GO TO FIN-PUT-TAB-DSC                                
               ELSE                                                     
                   GO TO FIN-PUT-TAB-DSC.                               
           SUBTRACT 1 FROM FRM-IFLD.                                    
           MOVE QUE-ITEM TO FRM-NUM-IPAE-IDX(FRM-IFLD).                 
           IF FRM-IFLD > 1                                              
               GO TO LUP-PUT-TAB-DSC.                                   
       FIN-PUT-TAB-DSC.                                                 
                                                                        
           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1                  
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I                    
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1           
                                 UNTIL WSS-I < 1.                       
                                                                        
           IF FRM-IFLD NOT > WSS-IMAX                                   
               SUBTRACT 1 FROM SCR-NUM-PANT                             
               MOVE 'SGC'        TO MSG-COD-SIST                        
               MOVE 'QPATABDMAS' TO MSG-COD-MENS.                       
       EXT-PUT-TAB-DSC.                                                 
           EXIT.                                                        
                                                                        
       COPY GNSBGCPT.                                                   
       COPY GNSBGSTR.                                                   
                                                                        
       CLR-FRM SECTION.                                                 
       INI-CLR-FRM.                                                     
           MOVE SPACES TO FRM-NUM-IPAE IN QPA-FLD(WSS-I).               
           MOVE SPACES TO FRM-GLS-APAT IN QPA-FLD(WSS-I).               
           MOVE SPACES TO FRM-GLS-NOMB IN QPA-FLD(WSS-I).               
           MOVE SPACES TO FRM-DIA-FCTR IN QPA-FLD(WSS-I).               
           MOVE SPACES TO FRM-MES-FCTR IN QPA-FLD(WSS-I).               
           MOVE SPACES TO FRM-ANS-FCTR IN QPA-FLD(WSS-I).               
           MOVE SPACES TO FRM-COD-PTZC IN QPA-FLD(WSS-I).               
           MOVE SPACES TO FRM-GLS-PTZC IN QPA-FLD(WSS-I).               
           MOVE SPACES TO FRM-COD-ORGN IN QPA-FLD(WSS-I).               
           MOVE SPACES TO FRM-GLS-ORGN IN QPA-FLD(WSS-I).               
           MOVE SPACES TO FRM-COD-CRGO IN QPA-FLD(WSS-I).               
           MOVE SPACES TO FRM-GLS-CRGO IN QPA-FLD(WSS-I).               
           MOVE SPACES TO FRM-MSC-STAT IN QPA-FLD(WSS-I).               
       FIN-CLR-FRM.                                                     
           EXIT.                                                        
                                                                        
       CLR-WSS SECTION.                                                 
       INI-CLR-WSS.                                                     
           MOVE SPACES TO WSS-NUM-CORR-QPA IN WSS-VARI.                 
           MOVE SPACES TO WSS-GLS-APAT-QPA IN WSS-VARI.                 
           MOVE SPACES TO WSS-GLS-NOMB-QPA IN WSS-VARI.                 
           MOVE SPACES TO WSS-DIA-FCTR-QPA IN WSS-VARI.                 
           MOVE SPACES TO WSS-MES-FCTR-QPA IN WSS-VARI.                 
           MOVE SPACES TO WSS-ANS-FCTR-QPA IN WSS-VARI.                 
           MOVE SPACES TO WSS-COD-PTZC-QPA IN WSS-VARI.                 
           MOVE SPACES TO WSS-GLS-PTZC-QPA IN WSS-VARI.                 
           MOVE SPACES TO WSS-COD-ORGN-QPA IN WSS-VARI.                 
           MOVE SPACES TO WSS-GLS-ORGN-QPA IN WSS-VARI.                 
           MOVE SPACES TO WSS-COD-CRGO-QPA IN WSS-VARI.                 
           MOVE SPACES TO WSS-GLS-CRGO-QPA IN WSS-VARI.                 
           MOVE SPACES TO WSS-MSC-STAT-QPA IN WSS-VARI.                 
           IF SCR-OFRM-SRV                                              
               PERFORM CLR-FRM-MTX.                                     
       FIN-CLR-WSS.                                                     
           EXIT.                                                        
                                                                        
       CLR-FRM-MTX SECTION.                                             
       INI-CLR-FRM-MTX.                                                 
           MOVE SPACES TO FRM-NUM-IPAE IN QPA-FLD(1).                   
           MOVE SPACES TO FRM-GLS-APAT IN QPA-FLD(1).                   
           MOVE SPACES TO FRM-GLS-NOMB IN QPA-FLD(1).                   
           MOVE SPACES TO FRM-DIA-FCTR IN QPA-FLD(1).                   
           MOVE SPACES TO FRM-MES-FCTR IN QPA-FLD(1).                   
           MOVE SPACES TO FRM-ANS-FCTR IN QPA-FLD(1).                   
           MOVE SPACES TO FRM-COD-PTZC IN QPA-FLD(1).                   
           MOVE SPACES TO FRM-GLS-PTZC IN QPA-FLD(1).                   
           MOVE SPACES TO FRM-COD-ORGN IN QPA-FLD(1).                   
           MOVE SPACES TO FRM-GLS-ORGN IN QPA-FLD(1).                   
           MOVE SPACES TO FRM-COD-CRGO IN QPA-FLD(1).                   
           MOVE SPACES TO FRM-GLS-CRGO IN QPA-FLD(1).                   
           MOVE SPACES TO FRM-MSC-STAT IN QPA-FLD(1).                   
       FIN-CLR-FRM-MTX.                                                 
           EXIT.                                                        
                                                                        
       CRG-VEC-QPA SECTION.                                             
       INI-CRG-VEC-QPA.                                                 
           ADD 1 TO WSS-J.                                              
           IF WSS-J IN WSS-VARI > WSS-JMAX IN WSS-VARI                  
               GO TO FIN-CRG-VEC-QPA.                                   
                                                                        
           PERFORM CLR-QPA-VEC.                                         
                                                                        
      *   correlativo                                                   
           MOVE PAE-NUM-IPAE IN PAE  TO                                 
                            WSS-NUM-CORR-V IN WSS-VARI(WSS-J).          
                                                                        
      *   apellido paterno persona alta exposicion                      
           MOVE PAE-GLS-APAT IN PAE  TO                                 
                            WSS-GLS-APAT-V IN WSS-VARI(WSS-J).          
                                                                        
      *   nombre persona alta exposicion                                
           MOVE PAE-GLS-NOMB IN PAE  TO                                 
                            WSS-GLS-NOMB-V IN WSS-VARI(WSS-J).          
                                                                        
      *   fecha contrato alta exposicion                                
           MOVE PAE-NUM-SCTR IN PAE  TO                                 
                            WSS-SIGL-FCTR IN WSS-VARI.                  
           MOVE PAE-NUM-ACTR IN PAE  TO                                 
                            WSS-ANNO-FCTR IN WSS-VARI.                  
           MOVE WSS-ANO-SIGL IN WSS-VARI TO WSS-ANS-FCTR-V(WSS-J).      
           MOVE '-'                  TO                                 
                            WSS-NUM-GUI1-V IN WSS-VARI(WSS-J).          
           MOVE PAE-NUM-MCTR IN PAE  TO                                 
                            WSS-MES-FCTR-V IN WSS-VARI(WSS-J).          
           MOVE '-'                  TO                                 
                            WSS-NUM-GUI2-V IN WSS-VARI(WSS-J).          
           MOVE PAE-NUM-DCTR IN PAE  TO                                 
                            WSS-DIA-FCTR-V IN WSS-VARI(WSS-J).          
                                                                        
      *   codigo parentezco con persona alta exposicion                 
           MOVE PAE-COD-PTZC IN PAE  TO                                 
                            WSS-COD-PTZC-V IN WSS-VARI(WSS-J).          
                                                                        
      *   busca glosa parentezco con persona alta exposicion            
           IF WSS-COD-PTZC-V IN WSS-VARI(WSS-J) = SPACES                
              MOVE SPACES TO                                            
                            WSS-GLS-PTZC-V IN WSS-VARI(WSS-J)           
              GO TO COD-ORGN.                                           
           MOVE 'SGC'                   TO TAB-COD-SIST.                
           MOVE 'PZC'                   TO TAB-COD-TTAB IN TAB.         
           MOVE PAE-COD-PTZC IN PAE TO TAB-COD-CTAB IN TAB.             
           PERFORM VAL-COD.                                             
           IF MSG-COD-MENS NOT > SPACES                                 
              MOVE TAB-GLS-DESC TO WSS-GLS-PTZC-V IN WSS-VARI(WSS-J)    
           ELSE                                                         
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              MOVE FRM-SUAR-MAL TO FRM-SUAR.                            
                                                                        
       COD-ORGN.                                                        
      *   codigo organismo alta exposicion                              
           MOVE PAE-COD-ORGN IN PAE  TO                                 
                             WSS-COD-ORGN-V IN WSS-VARI(WSS-J).         
      *  busca glosa parentezco con persona alta exposicion             
           IF WSS-COD-ORGN-V IN WSS-VARI(WSS-J) = SPACES                
              MOVE SPACES TO WSS-GLS-ORGN-V IN WSS-VARI(WSS-J)          
              GO TO COD-CRGO.                                           
           MOVE 'SGC'                   TO TAB-COD-SIST.                
           MOVE 'OAE'                   TO TAB-COD-TTAB IN TAB.         
           MOVE PAE-COD-ORGN IN PAE TO TAB-COD-CTAB IN TAB.             
           PERFORM VAL-COD.                                             
           IF MSG-COD-MENS NOT > SPACES                                 
              MOVE TAB-GLS-DESC TO WSS-GLS-ORGN-V IN WSS-VARI(WSS-J)    
           ELSE                                                         
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              MOVE FRM-SUAR-MAL TO FRM-SUAR.                            
                                                                        
       COD-CRGO.                                                        
      *   codigo cargo alta exposicion                                  
           MOVE PAE-COD-CRGO IN PAE  TO                                 
                             WSS-COD-CRGO-V IN WSS-VARI(WSS-J).         
      *   glosa cargo alta exposicion                                   
           MOVE PAE-GLS-CRGO IN PAE  TO                                 
                             WSS-GLS-CRGO-V IN WSS-VARI(WSS-J).         
      *  mueve el estado a la pantalla
           MOVE PAE-MSC-STAT IN PAE  TO                                 
                             WSS-MSC-STAT-V IN WSS-VARI(WSS-J).         
       FIN-CRG-VEC-QPA.                                                 
           EXIT.                                                        
                                                                        
       CLR-QPA-VEC SECTION.                                             
       INI-CLR-QPA-VEC.                                                 
           MOVE SPACES TO WSS-NUM-CORR-V(WSS-J).                        
           MOVE SPACES TO WSS-GLS-APAT-V(WSS-J).                        
           MOVE SPACES TO WSS-GLS-NOMB-V(WSS-J).                        
           MOVE SPACES TO WSS-DIA-FCTR-V(WSS-J).                        
           MOVE SPACES TO WSS-MES-FCTR-V(WSS-J).                        
           MOVE SPACES TO WSS-ANS-FCTR-V(WSS-J).                        
           MOVE SPACES TO WSS-COD-PTZC-V(WSS-J).                        
           MOVE SPACES TO WSS-GLS-PTZC-V(WSS-J).                        
           MOVE SPACES TO WSS-COD-ORGN-V(WSS-J).                        
           MOVE SPACES TO WSS-GLS-ORGN-V(WSS-J).                        
           MOVE SPACES TO WSS-COD-CRGO-V(WSS-J).                        
           MOVE SPACES TO WSS-GLS-CRGO-V(WSS-J).                        
           MOVE SPACES TO WSS-MSC-STAT-V(WSS-J).                        
       FIN-CLR-QPA-VEC.                                                 
           EXIT.                                                        
                                                                        
       PRO-FLD-QUE SECTION.                                             
       INI-PRO-FLD-QUE.                                                 
           MOVE 'P' TO WSS-NUM-CORR-ATR           .                     
           MOVE 'P' TO WSS-GLS-APAT-ATR.                                
           MOVE 'P' TO WSS-GLS-NOMB-ATR.                                
           MOVE 'P' TO WSS-DIA-FCTR-ATR.                                
           MOVE 'P' TO WSS-MES-FCTR-ATR.                                
           MOVE 'P' TO WSS-ANS-FCTR-ATR.                                
           MOVE 'P' TO WSS-COD-PTZC-ATR.                                
           MOVE 'P' TO WSS-GLS-PTZC-ATR.                                
           MOVE 'P' TO WSS-COD-ORGN-ATR.                                
           MOVE 'P' TO WSS-GLS-ORGN-ATR.                                
           MOVE 'P' TO WSS-COD-CRGO-ATR.                                
           MOVE 'P' TO WSS-GLS-CRGO-ATR.                                
           MOVE 'P' TO WSS-MSC-STAT-ATR.                                
       FIN-PRO-FLD-QUE.                                                 
           EXIT.                                                        
      *>>>> EOF                                                         
