                MOVE -1 TO DBC-COD-OFIC-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-FRM-GLS-LUGA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-LUGA-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-FRM-GLS-OFIC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-OFIC-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-DBC-COD-EJEC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DBC-COD-EJEC-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-DBC-COD-ACTI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DBC-COD-ACTI-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-FRM-GLS-EJEC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-EJEC-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-FRM-GLS-ACTI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-ACTI-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-DCP-COD-PROF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DCP-COD-PROF-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-DCP-FEC-FNAC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DCP-FEC-FNAC-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-FRM-GLS-PROF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-PROF-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-DCP-IND-SEXO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DCP-IND-SEXO-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
           PERFORM VAL-DCP-IND-ECIV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DCP-IND-ECIV-LEN IN CCC-FLD
                GO TO FIN-VAL-FLD-CCC.
