           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO EFE-FLD.
           MOVE LOW-VALUES TO EFE-EDT.


       FIN-SCR-RST.
           EXIT.

       GET-KEY SECTION.
       INI-GET-KEY.


           PERFORM SCR-GKEY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-KEY.
           EXIT.

       GET-IKY SECTION.
       INI-GET-IKY.


           PERFORM SCR-GIKY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-IKY.
           EXIT.

       FND-KEY SECTION.
       INI-FND-KEY.
      *<<<< INI_FND_KEY
           PERFORM KEY-ALL-EFE.
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM LEE-NXT-PRV-ANT
               IF MSG-COD-MENS = 'EFESI'
                   PERFORM SCR-CLRA-EFE
                   GO TO CON-FND-KEY
               ELSE
                   MOVE SPACES TO PGM-STAT
                   IF FRM-FFLD = FRM-FFLD-PF5
                       MOVE 'SGC' TO MSG-COD-SIST
                       MOVE 'EFE    NOPRV' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE '00' TO FIO-STAT
                       MOVE SCR-STAT-CON TO SCR-STAT
                       GO TO FIN-FND-KEY
                   ELSE
                       MOVE 'SGC' TO MSG-COD-SIST
                       MOVE 'EFE    NONXT' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE '00' TO FIO-STAT
                       MOVE SCR-STAT-CON TO SCR-STAT
                       GO TO FIN-FND-KEY.

           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8
               PERFORM LEE-NXT-PRV
               IF FIO-STAT-OKS
                   PERFORM SCR-CLRA-EFE
                   PERFORM BUS-EFE
                   IF NOT FIO-STAT-OKS
                       MOVE 'SGC'        TO MSG-COD-SIST
                       MOVE 'EFE    NEX' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MEN1
                       MOVE DBC-CAI-ICLI IN DBC TO
                                     EFE-CAI-ICLI IN EFE-FLD
                       MOVE DBC-IIC-ICLI IN DBC TO
                                     EFE-IIC-ICLI IN EFE-FLD
                       PERFORM LEE-REG-AEP
                       IF FIO-STAT-OKS
                           MOVE 'SGC'        TO MSG-COD-SIST
                           MOVE 'AEP    EXT' TO MSG-COD-MENS
                           PERFORM GET-MSG
                           MOVE MSG-GLS-DESC TO FRM-MEN2
                           MOVE '00' TO FIO-STAT
                           MOVE SCR-STAT-CON TO SCR-STAT
                           GO TO FIN-FND-KEY
                       ELSE
                           MOVE '00' TO FIO-STAT
                           MOVE SCR-STAT-CON TO SCR-STAT
                           GO TO FIN-FND-KEY
                   ELSE
                       GO TO CON-FND-KEY
               ELSE
                   MOVE SPACES TO PGM-STAT
                   IF FRM-FFLD = FRM-FFLD-PF7
                       MOVE 'SGC' TO MSG-COD-SIST
                       MOVE 'DBC    NOPRV' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE '00' TO FIO-STAT
                       MOVE SCR-STAT-CON TO SCR-STAT
                       GO TO FIN-FND-KEY
                   ELSE
                       MOVE 'SGC' TO MSG-COD-SIST
                       MOVE 'DBC    NONXT' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE '00' TO FIO-STAT
                       MOVE SCR-STAT-CON TO SCR-STAT
                       GO TO FIN-FND-KEY.

           IF DBC-NUM-ICLI IN EFE-FLD > ZEROES
      *IDC-CLI Busca Cliente por medio de IdC
               MOVE DBC-NUM-ICLI IN EFE-FLD TO DBC-NUM-ICLI IN DBC
               MOVE DBC-IND-ICLI IN EFE-FLD TO DBC-IND-ICLI IN DBC
               MOVE DBC-GLS-ICLI IN EFE-FLD TO DBC-GLS-ICLI IN DBC
               MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY
               MOVE 'IDC' TO SCR-KEY-IDBC
               MOVE FIO-GET-KEY-ALT TO FIO-CMND
               PERFORM GET-CLI

               MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
               PERFORM CPT-BLKS
               MOVE CPT-STRN TO DBC-GLS-NOMC IN EFE-FLD

      *        MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN EFE-FLD
               IF MSG-COD-MENS > SPACES
                   MOVE FIO-STAT-NEX TO FIO-STAT
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   GO TO FIN-FND-KEY
               ELSE
               MOVE '00' TO PGM-STAT-DBC
      *         IF DBC-IND-TICL IN DBC NOT = 'E'
      *             MOVE 'SGC'        TO MSG-COD-SIST
      *             MOVE 'CLI    PER' TO MSG-COD-MENS
      *             PERFORM GET-MSG
      *             MOVE FIO-STAT-NEX TO FIO-STAT
      *             MOVE MSG-GLS-DESC TO FRM-MENS
      *             PERFORM PUT-DBC-EFE
      *             GO TO FIN-FND-KEY
      *         ELSE
                   MOVE ADR-DBC-REQA TO SCR-DBC-REQA
                   MOVE DBC-CAI-ICLI IN DBC TO EFE-CAI-ICLI IN EFE-FLD
                   MOVE DBC-IIC-ICLI IN DBC TO EFE-IIC-ICLI IN EFE-FLD
           ELSE
      *NOM-CLI Busca Cliente por medio del Nombre

               MOVE DBC-GLS-NOMC IN EFE-FLD TO STR-GLS-GSTR
               PERFORM STR-UNS
               MOVE STR-GLS-FUNS IN STR-VARI(1) TO DBC-GLS-APAT IN DBC
               MOVE STR-GLS-FUNS IN STR-VARI(2) TO DBC-GLS-AMAT IN DBC
               MOVE STR-GLS-FUNS IN STR-VARI(3) TO DBC-GLS-NOMB IN DBC

               MOVE 'DBC-GLS-NOMC'  TO FIO-AKEY
               MOVE 'NOM' TO SCR-KEY-IDBC
               PERFORM BUS-CLI-EMP
               IF NOT FIO-STAT-OKS
                   MOVE FIO-STAT-NEX TO FIO-STAT
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   GO TO FIN-FND-KEY
               ELSE
                   MOVE ADR-DBC-REQA TO SCR-DBC-REQA
                   MOVE '00' TO PGM-STAT-DBC

                   MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
                   PERFORM CPT-BLKS
                   MOVE CPT-STRN TO DBC-GLS-NOMC IN EFE-FLD

                   MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN EFE-FLD
                   MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN EFE-FLD
                   MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN EFE-FLD
                   MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN EFE-FLD
                   MOVE DBC-CAI-ICLI IN DBC TO EFE-CAI-ICLI IN EFE-FLD
                   MOVE DBC-IIC-ICLI IN DBC TO EFE-IIC-ICLI IN EFE-FLD.
      *>>>>


           PERFORM KEY-ALL-EFE.
      *<<<< FND_KEY
           IF EFE-FEC-FEFE IN EFE-FLD NOT > ZEROES
               MOVE '99999999' TO EFE-FEC-FEFE IN EFE
               MOVE FIO-GET-LEQ TO FIO-CMND
               PERFORM SGC-FIO-EFE
               IF FIO-STAT-OKS AND
                  EFE-CAI-ICLI IN EFE = EFE-CAI-ICLI IN EFE-FLD AND
                  EFE-IIC-ICLI IN EFE = EFE-IIC-ICLI IN EFE-FLD
                   MOVE '00' TO PGM-STAT-EFE
                   MOVE ADR-EFE-REQA TO SCR-EFE-REQA
                   MOVE EFE-STP-ITRN IN EFE TO SCR-UMOD
      *RGD-INI 03-08-2009 "ORIGEN RENTA"
                   GO TO CON-FND-KEY
      *            GO TO FIN-FND-KEY
      *RGD-FIN 03-08-2009
               ELSE
                   MOVE 'SGC'        TO MSG-COD-SIST
                   MOVE 'EFE    NEX' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MEN1
                   PERFORM LEE-REG-AEP
                   IF FIO-STAT-OKS
      *GET-MSG Busca mensaje en tabla
                       MOVE 'SGC'        TO MSG-COD-SIST
                       MOVE 'AEP    EXT' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MEN2
                       MOVE FIO-STAT-NEX TO FIO-STAT
                       GO TO FIN-FND-KEY
                   ELSE
                       MOVE FIO-STAT-NEX TO FIO-STAT
                       GO TO FIN-FND-KEY.

           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-EFE.
           IF FIO-STAT-OKS
               MOVE '00' TO PGM-STAT-EFE
               MOVE EFE-STP-ITRN IN EFE TO SCR-UMOD
               MOVE ADR-EFE-REQA TO SCR-EFE-REQA
      *RGD-INI 03-08-2009 "ORIGEN RENTA"
               GO TO CON-FND-KEY.
      *        GO TO FIN-FND-KEY.
      *RGD-FIN 03-08-2009
           PERFORM KEY-ALL-EFE.
           MOVE FIO-GET-LEQ TO FIO-CMND.
           PERFORM SGC-FIO-EFE.
           IF FIO-STAT-OKS AND
              EFE-CAI-ICLI IN EFE = EFE-CAI-ICLI IN EFE-FLD AND
              EFE-IIC-ICLI IN EFE = EFE-IIC-ICLI IN EFE-FLD
               MOVE '00' TO PGM-STAT-EFE
               IF SCR-CCPP = 'ACT' OR 'ING'
                   PERFORM KEY-ALL-EFE
                   PERFORM PUT-ALL-EFE
                   MOVE 1 TO WSS-ILST
                   MOVE '23' TO FIO-STAT
      *RGD-INI 03-08-2009 "ORIGEN RENTA"
                   GO TO CON-FND-KEY
      *RGD-FIN 03-08-2009
               ELSE
                   MOVE 0 TO WSS-ILST
                   MOVE '00' TO FIO-STAT
                   MOVE EFE-STP-ITRN IN EFE TO SCR-UMOD
                   MOVE ADR-EFE-REQA TO SCR-EFE-REQA
      *RGD-INI 03-08-2009 "ORIGEN RENTA"
                   GO TO CON-FND-KEY
      *RGD-FIN 03-08-2009
           ELSE
               PERFORM LEE-REG-AEP
               IF FIO-STAT-OKS
                   MOVE 'SGC'        TO MSG-COD-SIST
                   MOVE 'AEP    EXT' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MEN2
                   PERFORM KEY-ALL-EFE
                   MOVE 0 TO WSS-ILST
                   MOVE '23' TO FIO-STAT
                   GO TO FIN-FND-KEY
               ELSE
                   PERFORM KEY-ALL-EFE
                   MOVE 0 TO WSS-ILST
                   MOVE '23' TO FIO-STAT.
           GO TO FIN-FND-KEY.
      *>>>>


           MOVE FIO-GET-KEY TO FIO-CMND.


           PERFORM SGC-FIO-EFE.
           IF FIO-STAT-OKS
               MOVE EFE-STP-ITRN IN EFE TO SCR-UMOD.
           MOVE FIO-STAT TO PGM-STAT-EFE.
      *<<<< FIN_FND_KEY
       CON-FND-KEY.
      *RGD-INI 03-08-2009 "ORIGEN RENTA"
           MOVE FIO-STAT TO PGM-STAT-EFE.
           MOVE EFE-CIC-ICLI IN EFE TO DCC-CIC-ICLI IN DCC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DCC.
           MOVE FIO-STAT TO PGM-STAT-DCC.
           MOVE SPACES TO SCR-COD-BDIR.
           IF FIO-STAT-OKS
              MOVE DCC-COD-BDIR IN DCC TO DCC-COD-BDIR IN EFE-FLD
              MOVE DCC-COD-BDIR IN DCC TO SCR-COD-BDIR.
           MOVE PGM-STAT-EFE TO FIO-STAT.
      *RGD-FIN 03-08-2009
           IF FIO-STAT-OKS
               MOVE EFE-STP-ITRN IN EFE TO SCR-UMOD
               MOVE ADR-EFE-REQA TO SCR-EFE-REQA.
      *>>>>


       FIN-FND-KEY.
           IF FIO-STAT = FIO-STAT-NEX
               MOVE 'S' TO SCR-IERR.
       EXT-FND-KEY.
           EXIT.
 
       PUT-SCR SECTION.
       INI-PUT-SCR.
           IF PGM-STAT-EFE-OKS
               MOVE EFE-STP-ITRN IN EFE TO SCR-UMOD.

      *<<<< PUT_SCR
      *>>>> PUT_SCR

           PERFORM PUT-ALL-EFE.


       FIN-PUT-SCR.
           EXIT.

       STP-EXIT SECTION.
       INI-STP-EXIT.


           MOVE SCR-PROG TO PGM-PROG.
           MOVE SCR-NTRN TO PGM-NTRN.

           MOVE SCR-PROV TO SCR-PROG.
           MOVE SCR-NTRV TO SCR-NTRN.

           MOVE PGM-PROG TO SCR-PROV
           MOVE PGM-NTRN TO SCR-NTRV.


       FIN-STP-EXIT.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO SYS-NTRN.


           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.


      *PTC-INI
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE EFE-FLD TO FRM-DFLD.


           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO EFE-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GKEY SECTION.
       INI-SCR-GKEY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.


           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
      *<<<< PFS_SCR_GKEY
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 OR FRM-FFLD = FRM-FFLD-PF7 )
      *>>>>


               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-EFE
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO EFE-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GKEY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-EFE.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-EFE
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-EFE.


           PERFORM VAL-IKY-EFE.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.


           PERFORM VAL-MKY-EFE.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.


           PERFORM VAL-AKY-EFE.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.


           PERFORM VAL-CON-KEY-EFE.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GKEY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-EFE.
       ERR-SCR-GKEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN


           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-EFE.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GKEY.


           PERFORM SCR-MSSG.

       SCR-GIKY SECTION.
       INI-SCR-GIKY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )


               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-EFE
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO EFE-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GIKY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-EFE.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-IKY-EFE
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-EFE.


           PERFORM VAL-IKY-EFE.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GIKY.


           PERFORM VAL-CON-IKY-EFE.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GIKY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-EFE.
       ERR-SCR-GIKY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN


           PERFORM SCR-MSSG.
           PERFORM EDT-IKY-EFE.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GIKY.


           PERFORM SCR-MSSG.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
      *<<<< PFS_GET_CON OR PFS_GET_ACC
           IF NOT
              ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD = FRM-FFLD-PF6 OR
                FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD = FRM-FFLD-PF8 )
      *>>>>


               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO EFE-GLS-MEN2
               MOVE SCR-PRMT     TO EFE-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO EFE-FLD.


       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO EFE-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.


      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE EFE-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO EFE-FLD
      * FLB  7-NOV-1994
               PERFORM CLR-KEY-EFE

               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO EFE-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO EFE-GLS-MENS.
           PERFORM PRO-MKY-EFE.
           PERFORM PRO-AKY-EFE.
           PERFORM UNP-IKY-EFE.
           PERFORM FST-KEY-EFE.


       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.


      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE EFE-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO EFE-FLD
               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO EFE-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO EFE-GLS-MENS.
           PERFORM UNP-KEY-EFE.
           PERFORM FST-KEY-EFE.


       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.


           PERFORM PRO-IKY-EFE.
           PERFORM PRO-AKY-EFE.
           PERFORM UNP-MKY-EFE.
           PERFORM UNP-FLD-EFE.
           PERFORM FST-FLD-EFE.


       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO EFE-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.


           PERFORM PRO-KEY-EFE.
           PERFORM FST-MSG.


       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.




      *PTC-INI
           IF SCR-STAT NOT = SCR-STPP
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
      *<<<< FIN_SCR_TRL
           IF ( SCR-CCPP = 'ACT' OR 'ING' ) AND ING-SOBRE-EL-ULTIMO
               MOVE 'SGC' TO MSG-COD-SIST
               MOVE 'ING    LSTRG' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS.
      *>>>>


           IF SCR-STAT = SCR-STAT-MOD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-EFE
               PERFORM EDT-FLD-EFE
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-EFE
               PERFORM EDT-FLD-EFE
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-EFE.
           PERFORM EDT-FLD-EFE.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

      * PGM-EBR-ALL SECTION.
      * INI-PGM-EBR-ALL.
      *     MOVE 'GNS' TO FIO-SIST.
      *     MOVE FIO-EBR-ALL TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
      * FIN-PGM-EBR-ALL.
      *     EXIT.

       COPY SGCBQEFE.
       COPY GNSBVNUM.
       COPY SGCBFEFE.
       COPY GNSBFMSG.
       COPY GNSBGSYS.
       COPY GNSBGDTC.
      *PTC-INI
      *PTC-SUB
      * ISP  10-MAY-2001 13:46:35 
      * COPY GNSBGFRM.
       COPY GNSBG3CF.
