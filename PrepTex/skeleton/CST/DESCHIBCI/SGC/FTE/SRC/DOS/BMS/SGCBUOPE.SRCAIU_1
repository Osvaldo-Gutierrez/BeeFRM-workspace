               MOVE 'SGC'          TO MSG-COD-SIST
               MOVE 'FECFTOPMOTTI' TO MSG-COD-MENS IN MSG
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO  FIN-VAL-CON-FLD-OPE.

           IF OPE-FEC-FROP IN OPE-FLD > ZEROS
               IF OPE-FEC-FTOP IN OPE-FLD NOT > ZEROS
                   MOVE 'SGC'          TO MSG-COD-SIST
                   MOVE 'FECFROPFTOP ' TO MSG-COD-MENS IN MSG
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
               ELSE
                   MOVE OPE-FEC-FTOP IN OPE-FLD TO FEC-FEC1
                   MOVE OPE-FEC-FROP IN OPE-FLD TO FEC-FEC2
                   MOVE FEC-FORM-FEC TO FEC-FORM
                   MOVE FEC-CMP-FEC  TO FEC-CMND
                   PERFORM PRO-FEC
                   IF FEC-STAT-GRT
                       MOVE 'SGC'          TO MSG-COD-SIST
                       MOVE 'FECFTOP>FROP' TO MSG-COD-MENS IN MSG
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>>
       FIN-VAL-CON-FLD-OPE.
           EXIT.
       
       VAL-CON-OPE SECTION.
       INI-VAL-CON-OPE.
       FIN-VAL-CON-OPE.
           EXIT.
       
       VAL-NUM-KEY-OPE SECTION.
       INI-VAL-NUM-KEY-OPE.
           PERFORM VAL-NUM-IKY-OPE.
           PERFORM VAL-NUM-MKY-OPE.
           PERFORM VAL-NUM-AKY-OPE.
       FIN-VAL-NUM-KEY-OPE.
           EXIT.
       
       VAL-NUM-IKY-OPE SECTION.
       INI-VAL-NUM-IKY-OPE.
       VAL-NUM-OPE-IIC-IOPE.
           MOVE OPE-IIC-IOPE-ALF IN OPE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO OPE-IIC-IOPE IN OPE-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO OPE-IIC-IOPE-LEN IN OPE-FLD
                GO TO FIN-VAL-NUM-IKY-OPE.
       FIN-VAL-NUM-IKY-OPE.
           EXIT.
       
       VAL-NUM-MKY-OPE SECTION.
       INI-VAL-NUM-MKY-OPE.
      * IDC-INI --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
       VAL-NUM-DBC-NUM-ICLI.
           MOVE DBC-NUM-ICLI-ALF IN OPE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO DBC-NUM-ICLI IN OPE-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO DBC-NUM-ICLI-LEN IN OPE-FLD
                GO TO FIN-VAL-NUM-MKY-OPE.
      * IDC-FIN --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
       FIN-VAL-NUM-MKY-OPE.
           EXIT.
       
       VAL-NUM-AKY-OPE SECTION.
       INI-VAL-NUM-AKY-OPE.
       FIN-VAL-NUM-AKY-OPE.
           EXIT.
       
       VAL-NUM-FLD-OPE SECTION.
       INI-VAL-NUM-FLD-OPE.
       VAL-NUM-RCO-NUM-DIRE.
           MOVE RCO-NUM-DIRE-ALF IN OPE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 2 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO RCO-NUM-DIRE IN OPE-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO RCO-NUM-DIRE-LEN IN OPE-FLD
                GO TO FIN-VAL-NUM-FLD-OPE.
       FIN-VAL-NUM-FLD-OPE.
           EXIT.
       
       EDT-KEY-OPE SECTION.
       INI-EDT-KEY-OPE.
           PERFORM EDT-IKY-OPE.
           PERFORM EDT-MKY-OPE.
           PERFORM EDT-AKY-OPE.
       FIN-EDT-KEY-OPE.
           EXIT.
       
       EDT-IKY-OPE SECTION.
       INI-EDT-IKY-OPE.
           IF OPE-IIC-IOPE-LEN IN OPE-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO OPE-IIC-IOPE-LEN IN OPE-FLD
                GO TO FIN-EDT-IKY-OPE.
           MOVE OPE-IIC-IOPE IN OPE-FLD TO OPE-IIC-IOPE-EDT IN OPE-EDT.
           MOVE OPE-IIC-IOPE-EDT IN OPE-EDT TO 
                OPE-IIC-IOPE-ALF IN OPE-FLD.
       FIN-EDT-IKY-OPE.
           EXIT.
       
       EDT-MKY-OPE SECTION.
       INI-EDT-MKY-OPE.
      * IDC-INI --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF DBC-NUM-ICLI-LEN IN OPE-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DBC-NUM-ICLI-LEN IN OPE-FLD
                GO TO FIN-EDT-MKY-OPE.
           MOVE DBC-NUM-ICLI IN OPE-FLD TO
                DBC-NUM-ICLI-EDT IN OPE-EDT.
           MOVE DBC-NUM-ICLI-EDT IN OPE-EDT TO
                DBC-NUM-ICLI-ALF IN OPE-FLD.
      * IDC-FIN --> BMCHIBCI011.SRCAIU EDICION NUMERICA
       FIN-EDT-MKY-OPE.
           EXIT.
       
       EDT-AKY-OPE SECTION.
       INI-EDT-AKY-OPE.
       FIN-EDT-AKY-OPE.
           EXIT.
       
       EDT-FLD-OPE SECTION.
       INI-EDT-FLD-OPE.
           IF RCO-NUM-DIRE-LEN IN OPE-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO RCO-NUM-DIRE-LEN IN OPE-FLD
                GO TO FIN-EDT-FLD-OPE.
           MOVE RCO-NUM-DIRE IN OPE-FLD TO RCO-NUM-DIRE-EDT IN OPE-EDT.
           MOVE RCO-NUM-DIRE-EDT IN OPE-EDT TO 
                RCO-NUM-DIRE-ALF IN OPE-FLD.
       FIN-EDT-FLD-OPE.
           EXIT.
