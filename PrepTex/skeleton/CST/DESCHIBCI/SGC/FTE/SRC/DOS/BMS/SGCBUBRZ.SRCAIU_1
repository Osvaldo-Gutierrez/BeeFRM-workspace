           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO BRZ-COD-RLUN-LEN IN BRZ-FLD
                GO TO FIN-VAL-FLD-BRZ.
           PERFORM VAL-BRZ-COD-RLVR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO BRZ-COD-RLVR-LEN IN BRZ-FLD
                GO TO FIN-VAL-FLD-BRZ.
           PERFORM VAL-BRZ-VAL-AFSC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO BRZ-VAL-AFSC-LEN IN BRZ-FLD
                GO TO FIN-VAL-FLD-BRZ.
           PERFORM VAL-BRZ-VAL-VCOM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO BRZ-VAL-VCOM-LEN IN BRZ-FLD
                GO TO FIN-VAL-FLD-BRZ.
           PERFORM VAL-BRZ-GLS-FOJA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO BRZ-GLS-FOJA-LEN IN BRZ-FLD
                GO TO FIN-VAL-FLD-BRZ.
           PERFORM VAL-BRZ-NUM-NINS.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO BRZ-NUM-NINS-LEN IN BRZ-FLD
                GO TO FIN-VAL-FLD-BRZ.
           PERFORM VAL-BRZ-NUM-AINS.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO BRZ-NUM-AINS-LEN IN BRZ-FLD
                GO TO FIN-VAL-FLD-BRZ.
