      *
      * SGCBUDBN
      * Version 3.0.1 Nov-2005
      * 26-Sep-11 06:45 PM
      *
       SET-TAG-SGCBUDBN SECTION.
       INI-SET-TAG-SGCBUDBN.
           MOVE
           '<IDNMOD NOM=$SGCBUDBN$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$26-Sep-11 06:45 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-SGCBUDBN.
           EXIT.
           
       RET-ALL-DBN SECTION.
       INI-RET-ALL-DBN.
           PERFORM KEY-ALL-DBN.
           PERFORM RET-DBN-DBN.
           PERFORM RET-DBC-DBN.
       FIN-RET-ALL-DBN.
           EXIT.
       
       RET-DBN-DBN SECTION.
       INI-RET-DBN-DBN.
           IF SCR-CCPP = 'MOD'
              MOVE DBN-COD-EEFR IN DBN TO DBN-COD-EEFR IN DBN-FLD
              MOVE DBN-NUM-DEFR IN DBN TO DBN-NUM-DEFR IN DBN-FLD
              MOVE DBN-NUM-MEFR IN DBN TO DBN-NUM-MEFR IN DBN-FLD
              MOVE DBN-NUM-SEFR IN DBN TO DBN-NUM-SEFR IN DBN-FLD
              MOVE DBN-NUM-AEFR IN DBN TO DBN-NUM-AEFR IN DBN-FLD.

           MOVE DBN-COD-TIPO IN DBN-FLD TO DBN-COD-TIPO IN DBN.
           MOVE DBN-COD-CBCO IN DBN-FLD TO DBN-COD-CBCO IN DBN.
           MOVE DBN-GLS-CCTE IN DBN-FLD TO DBN-GLS-CCTE IN DBN.
           MOVE DBN-COD-TDEU IN DBN-FLD TO DBN-COD-TDEU IN DBN.
           MOVE DBN-VAL-MDEU IN DBN-FLD TO DBN-VAL-MDEU IN DBN.
           MOVE DBN-COD-EEFR IN DBN-FLD TO DBN-COD-EEFR IN DBN.
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBN-NUM-DEFR IN DBN-FLD TO DBN-NUM-DEFR IN DBN.
           MOVE DBN-NUM-MEFR IN DBN-FLD TO DBN-NUM-MEFR IN DBN.
           MOVE DBN-NUM-SEFR IN DBN-FLD TO DBN-NUM-SEFR IN DBN.
           MOVE DBN-NUM-AEFR IN DBN-FLD TO DBN-NUM-AEFR IN DBN.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
       FIN-RET-DBN-DBN.
           EXIT.
       
       RET-DBC-DBN SECTION.
       INI-RET-DBC-DBN.
       FIN-RET-DBC-DBN.
           EXIT.
       
       KEY-ALL-DBN SECTION.
       INI-KEY-ALL-DBN.
           PERFORM KEY-DBN-DBN.
           PERFORM KEY-DBC-DBN.
       FIN-KEY-ALL-DBN.
           EXIT.
       
       KEY-DBN-DBN SECTION.
       INI-KEY-DBN-DBN.
           MOVE DBN-CAI-IFOL IN DBN-FLD TO DBN-CAI-IFOL IN DBN.
           MOVE DBN-IIC-IFOL IN DBN-FLD TO DBN-IIC-IFOL IN DBN.
           MOVE DBN-NUM-IDBN IN DBN-FLD TO DBN-NUM-IDBN IN DBN.
       FIN-KEY-DBN-DBN.
           EXIT.
       
       KEY-DBC-DBN SECTION.
       INI-KEY-DBC-DBN.
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DBC-NUM-ICLI IN DBN-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-VRF-ICLI IN DBN-FLD TO DBC-VRF-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN DBN-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN DBN-FLD TO DBC-GLS-ICLI IN DBC.
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
       FIN-KEY-DBC-DBN.
           EXIT.
       
       SCR-CLRA-DBN SECTION.
       INI-SCR-CLRA-DBN.
           PERFORM CLR-KEY-DBN.
           PERFORM CLR-FLD-DBN.
       FIN-SCR-CLRA-DBN.
           EXIT.
       
       CLR-KEY-DBN SECTION.
       INI-CLR-KEY-DBN.
           MOVE SPACES TO DBN-CAI-IFOL IN DBN-FLD.
           MOVE ZEROES TO DBN-IIC-IFOL IN DBN-FLD.
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN DBN-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN DBN-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN DBN-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN DBN-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBN-NUM-IDBN IN DBN-FLD.
       FIN-CLR-KEY-DBN.
           EXIT.
       
       CLR-FLD-DBN SECTION.
       INI-CLR-FLD-DBN.
           MOVE SPACES TO FRM-GLS-NOMC IN DBN-FLD.
           MOVE SPACES TO FRM-GLS-BOCC IN DBN-FLD.
           MOVE SPACES TO FRM-GLS-TDEU IN DBN-FLD.
           PERFORM CLR-DBN-DBN.
           PERFORM CLR-DBC-DBN.
       FIN-CLR-FLD-DBN.
           EXIT.
       
       CLR-DBN-DBN SECTION.
       INI-CLR-DBN-DBN.
           MOVE SPACES TO DBN-COD-TIPO IN DBN-FLD.
           MOVE SPACES TO DBN-COD-CBCO IN DBN-FLD.
           MOVE SPACES TO DBN-GLS-CCTE IN DBN-FLD.
           MOVE SPACES TO DBN-COD-TDEU IN DBN-FLD.
           MOVE ZEROES TO DBN-VAL-MDEU IN DBN-FLD.
           MOVE SPACES TO DBN-COD-EEFR IN DBN-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBN-FEC-FEFR IN DBN-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
       FIN-CLR-DBN-DBN.
           EXIT.
       
       CLR-DBC-DBN SECTION.
       INI-CLR-DBC-DBN.
       FIN-CLR-DBC-DBN.
           EXIT.
       
       FST-KEY-DBN SECTION.
       INI-FST-KEY-DBN.
           MOVE -1 TO DBN-CAI-IFOL-LEN IN DBN-FLD.
       FIN-FST-KEY-DBN.
           EXIT.
       
       FST-FLD-DBN SECTION.
       INI-FST-FLD-DBN.
           MOVE -1 TO DBN-COD-TIPO-LEN IN DBN-FLD.
       FIN-FST-FLD-DBN.
           EXIT.
       
       PRO-KEY-DBN SECTION.
       INI-PRO-KEY-DBN.
           PERFORM PRO-MKY-DBN.
       FIN-PRO-KEY-DBN.
           EXIT.
       
       PRO-MKY-DBN SECTION.
       INI-PRO-MKY-DBN.
       FIN-PRO-MKY-DBN.
           EXIT.
       
       PRO-FLD-DBN SECTION.
       INI-PRO-FLD-DBN.
           PERFORM PRO-DBN-DBN.
           PERFORM PRO-DBC-DBN.
       FIN-PRO-FLD-DBN.
           EXIT.
       
       PRO-DBN-DBN SECTION.
       INI-PRO-DBN-DBN.
           MOVE FRM-CPIM-AEY TO DBN-COD-TIPO-ATR IN DBN-FLD.
           MOVE FRM-CPIM-AEY TO DBN-COD-CBCO-ATR IN DBN-FLD.
           MOVE FRM-CPIM-AEY TO DBN-GLS-CCTE-ATR IN DBN-FLD.
           MOVE FRM-CPIM-AEY TO DBN-COD-TDEU-ATR IN DBN-FLD.
           MOVE FRM-CPIM-AEY TO DBN-VAL-MDEU-ATR IN DBN-FLD.
       FIN-PRO-DBN-DBN.
           EXIT.
       
       PRO-DBC-DBN SECTION.
       INI-PRO-DBC-DBN.
       FIN-PRO-DBC-DBN.
           EXIT.
       
       UNP-KEY-DBN SECTION.
       INI-UNP-KEY-DBN.
           PERFORM UNP-IKY-DBN.
           PERFORM UNP-AKY-DBN.
           PERFORM UNP-MKY-DBN.
       FIN-UNP-KEY-DBN.
           EXIT.
       
       UNP-IKY-DBN SECTION.
       INI-UNP-IKY-DBN.
           MOVE FRM-CPIM-UBY TO DBN-CAI-IFOL-ATR.
           MOVE FRM-CPIM-UBY TO DBN-IIC-IFOL-ATR.
      * IDC-INI --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO DBC-NUM-ICLI-ATR IN DBN-FLD.
           MOVE FRM-CPIM-UBY TO DBC-VRF-ICLI-ATR IN DBN-FLD.
           MOVE FRM-CPIM-UBY TO DBC-IND-ICLI-ATR IN DBN-FLD.
           MOVE FRM-CPIM-UBY TO DBC-GLS-ICLI-ATR IN DBN-FLD.
      * IDC-FIN --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
           MOVE FRM-CPIM-UBY TO DBN-NUM-IDBN-ATR.
       FIN-UNP-IKY-DBN.
           EXIT.
       
       UNP-MKY-DBN SECTION.
       INI-UNP-MKY-DBN.
       FIN-UNP-MKY-DBN.
           EXIT.
       
       UNP-AKY-DBN SECTION.
       INI-UNP-AKY-DBN.
       FIN-UNP-AKY-DBN.
           EXIT.
       
       UNP-FLD-DBN SECTION.
       INI-UNP-FLD-DBN.
           PERFORM UNP-DBN-DBN.
           PERFORM UNP-DBC-DBN.
       FIN-UNP-FLD-DBN.
           EXIT.
       
       UNP-DBN-DBN SECTION.
       INI-UNP-DBN-DBN.
           MOVE FRM-CPIM-UBY TO DBN-COD-TIPO-ATR IN DBN-FLD.
           MOVE FRM-CPIM-UBY TO DBN-COD-CBCO-ATR IN DBN-FLD.
           MOVE FRM-CPIM-UBY TO DBN-GLS-CCTE-ATR IN DBN-FLD.
           MOVE FRM-CPIM-UBY TO DBN-COD-TDEU-ATR IN DBN-FLD.
           MOVE FRM-CPIM-UBY TO DBN-VAL-MDEU-ATR IN DBN-FLD.
       FIN-UNP-DBN-DBN.
           EXIT.
       
       UNP-DBC-DBN SECTION.
       INI-UNP-DBC-DBN.
       FIN-UNP-DBC-DBN.
           EXIT.
       
       VAL-MKY-DBN SECTION.
       INI-VAL-MKY-DBN.
       FIN-VAL-MKY-DBN.
           EXIT.
       
       VAL-FLD-DBN SECTION.
       INI-VAL-FLD-DBN.
           PERFORM VAL-FRM-GLS-NOMC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-NOMC-LEN IN DBN-FLD
                GO TO FIN-VAL-FLD-DBN.
           PERFORM VAL-DBN-COD-TIPO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DBN-COD-TIPO-LEN IN DBN-FLD
                GO TO FIN-VAL-FLD-DBN.
           PERFORM VAL-DBN-COD-CBCO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DBN-COD-CBCO-LEN IN DBN-FLD
                GO TO FIN-VAL-FLD-DBN.
           PERFORM VAL-FRM-GLS-BOCC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-BOCC-LEN IN DBN-FLD
                GO TO FIN-VAL-FLD-DBN.
           PERFORM VAL-DBN-GLS-CCTE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DBN-GLS-CCTE-LEN IN DBN-FLD
                GO TO FIN-VAL-FLD-DBN.
           PERFORM VAL-DBN-COD-TDEU.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DBN-COD-TDEU-LEN IN DBN-FLD
                GO TO FIN-VAL-FLD-DBN.
           PERFORM VAL-FRM-GLS-TDEU.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TDEU-LEN IN DBN-FLD
                GO TO FIN-VAL-FLD-DBN.
           PERFORM VAL-DBN-VAL-MDEU.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DBN-VAL-MDEU-LEN IN DBN-FLD
                GO TO FIN-VAL-FLD-DBN.
           PERFORM VAL-DBN-COD-EEFR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DBN-COD-EEFR-LEN IN DBN-FLD
                GO TO FIN-VAL-FLD-DBN.
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DBN-FEC-FEFR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DBN-FEC-FEFR-LEN IN DBN-FLD
                GO TO FIN-VAL-FLD-DBN.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
       FIN-VAL-FLD-DBN.
           EXIT.
       
       VAL-FRM-GLS-NOMC SECTION.
       INI-VAL-FRM-GLS-NOMC.
       FIN-VAL-FRM-GLS-NOMC.
           EXIT.
       
       VAL-DBN-COD-TIPO SECTION.
       INI-VAL-DBN-COD-TIPO.
           IF DBN-COD-TIPO IN DBN-FLD NOT > SPACES
              MOVE SPACES TO DBN-COD-TIPO IN DBN-FLD.
      *<<<< DBN_COD_TIPO
           MOVE 'SGC'                   TO TAB-COD-SIST.                
           MOVE 'IFI'                   TO TAB-COD-TTAB IN TAB.         
           MOVE DBN-COD-TIPO IN DBN-FLD TO TAB-COD-CTAB IN TAB.         
           PERFORM VAL-COD.                                             
           MOVE TAB-CIC-CTAB TO WSS-CIC-TIPO IN WSS-VARI.               
      *>>>>
       FIN-VAL-DBN-COD-TIPO.
           EXIT.
       
       VAL-DBN-COD-CBCO SECTION.
       INI-VAL-DBN-COD-CBCO.
           IF DBN-COD-CBCO IN DBN-FLD NOT > SPACES
              MOVE SPACES TO DBN-COD-CBCO IN DBN-FLD.
      *<<<< DBN_COD_CBCO
           IF WSS-CIC-TIPO IN WSS-VARI = 'BCA'                      
               MOVE 'TAB'                   TO TAB-COD-SIST         
               MOVE 'BCO'                   TO TAB-COD-TTAB IN TAB  
               MOVE DBN-COD-CBCO IN DBN-FLD TO TAB-COD-CTAB IN TAB  
               PERFORM VAL-COD                                      
               IF MSG-COD-MENS NOT > SPACES                         
                  MOVE TAB-GLS-DESC TO FRM-GLS-BOCC IN DBN-FLD      
               ELSE                                                 
                  MOVE MSG-GLS-DESC TO FRM-MENS                     
                  MOVE FRM-SUAR-MAL TO FRM-SUAR                     
                  GO TO FIN-VAL-DBN-COD-CBCO.                       
           IF WSS-CIC-TIPO IN WSS-VARI = 'CCM'                      
               MOVE 'SGC'                   TO TAB-COD-SIST         
               MOVE 'CCM'                   TO TAB-COD-TTAB IN TAB  
               MOVE DBN-COD-CBCO IN DBN-FLD TO TAB-COD-CTAB IN TAB  
               PERFORM VAL-COD                                      
               IF MSG-COD-MENS NOT > SPACES                         
                  MOVE TAB-GLS-DESC TO FRM-GLS-BOCC IN DBN-FLD      
               ELSE                                                 
                  MOVE MSG-GLS-DESC TO FRM-MENS                     
                  MOVE FRM-SUAR-MAL TO FRM-SUAR
                  GO TO FIN-VAL-DBN-COD-CBCO.                 
      *>>>>
       FIN-VAL-DBN-COD-CBCO.
           EXIT.
       
       VAL-FRM-GLS-BOCC SECTION.
       INI-VAL-FRM-GLS-BOCC.
       FIN-VAL-FRM-GLS-BOCC.
           EXIT.
       
       VAL-DBN-GLS-CCTE SECTION.
       INI-VAL-DBN-GLS-CCTE.
           IF DBN-GLS-CCTE IN DBN-FLD NOT > SPACES
              MOVE SPACES TO DBN-GLS-CCTE IN DBN-FLD.
       FIN-VAL-DBN-GLS-CCTE.
           EXIT.
       
       VAL-DBN-COD-TDEU SECTION.
       INI-VAL-DBN-COD-TDEU.
           IF DBN-COD-TDEU IN DBN-FLD NOT > SPACES
              MOVE SPACES TO DBN-COD-TDEU IN DBN-FLD.
       FIN-VAL-DBN-COD-TDEU.
           EXIT.
       
       VAL-FRM-GLS-TDEU SECTION.
       INI-VAL-FRM-GLS-TDEU.
       FIN-VAL-FRM-GLS-TDEU.
           EXIT.
       
       VAL-DBN-VAL-MDEU SECTION.
       INI-VAL-DBN-VAL-MDEU.
       FIN-VAL-DBN-VAL-MDEU.
           EXIT.
       
       VAL-DBN-COD-EEFR SECTION.
       INI-VAL-DBN-COD-EEFR.
       FIN-VAL-DBN-COD-EEFR.
           EXIT.
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-DBN-FEC-FEFR SECTION.
       INI-VAL-DBN-FEC-FEFR.
           IF DBN-FEC-FEFR IN DBN-FLD  NOT > ZEROES
              MOVE ZEROES TO DBN-FEC-FEFR IN DBN-FLD.
       FIN-VAL-DBN-FEC-FEFR.
           EXIT.
      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-CON-FLD-DBN SECTION.
       INI-VAL-CON-FLD-DBN.
      *<<<< VAL-CON-FLD-DBN
      *>>>>
       FIN-VAL-CON-FLD-DBN.
           EXIT.
       
       VAL-CON-DBN SECTION.
       INI-VAL-CON-DBN.
       FIN-VAL-CON-DBN.
           EXIT.
       
       VAL-NUM-KEY-DBN SECTION.
       INI-VAL-NUM-KEY-DBN.
           PERFORM VAL-NUM-IKY-DBN.
           PERFORM VAL-NUM-MKY-DBN.
           PERFORM VAL-NUM-AKY-DBN.
       FIN-VAL-NUM-KEY-DBN.
           EXIT.
       
       VAL-NUM-IKY-DBN SECTION.
       INI-VAL-NUM-IKY-DBN.
       VAL-NUM-DBN-IIC-IFOL.
           MOVE DBN-IIC-IFOL-ALF IN DBN-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO DBN-IIC-IFOL IN DBN-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO DBN-IIC-IFOL-LEN IN DBN-FLD
                GO TO FIN-VAL-NUM-IKY-DBN.
      * IDC-INI --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
       VAL-NUM-DBC-NUM-ICLI.
           MOVE DBC-NUM-ICLI-ALF IN DBN-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO DBC-NUM-ICLI IN DBN-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO DBC-NUM-ICLI-LEN IN DBN-FLD
                GO TO FIN-VAL-NUM-IKY-DBN.
      * IDC-FIN --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
       VAL-NUM-DBN-NUM-IDBN.
           MOVE DBN-NUM-IDBN-ALF IN DBN-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO DBN-NUM-IDBN IN DBN-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO DBN-NUM-IDBN-LEN IN DBN-FLD
                GO TO FIN-VAL-NUM-IKY-DBN.
       FIN-VAL-NUM-IKY-DBN.
           EXIT.
       
       VAL-NUM-MKY-DBN SECTION.
       INI-VAL-NUM-MKY-DBN.
       FIN-VAL-NUM-MKY-DBN.
           EXIT.
       
       VAL-NUM-AKY-DBN SECTION.
       INI-VAL-NUM-AKY-DBN.
       FIN-VAL-NUM-AKY-DBN.
           EXIT.
       
       VAL-NUM-FLD-DBN SECTION.
       INI-VAL-NUM-FLD-DBN.
       VAL-NUM-DBN-VAL-MDEU.
           MOVE DBN-VAL-MDEU-ALF IN DBN-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9V TO DBN-VAL-MDEU IN DBN-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO DBN-VAL-MDEU-LEN IN DBN-FLD
                GO TO FIN-VAL-NUM-FLD-DBN.
       FIN-VAL-NUM-FLD-DBN.
           EXIT.
       
       EDT-KEY-DBN SECTION.
       INI-EDT-KEY-DBN.
           PERFORM EDT-IKY-DBN.
           PERFORM EDT-MKY-DBN.
           PERFORM EDT-AKY-DBN.
       FIN-EDT-KEY-DBN.
           EXIT.
       
       EDT-IKY-DBN SECTION.
       INI-EDT-IKY-DBN.
           IF DBN-IIC-IFOL-LEN IN DBN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DBN-IIC-IFOL-LEN IN DBN-FLD
                GO TO FIN-EDT-IKY-DBN.
           MOVE DBN-IIC-IFOL IN DBN-FLD TO DBN-IIC-IFOL-EDT IN DBN-EDT.
           MOVE DBN-IIC-IFOL-EDT IN DBN-EDT TO 
                DBN-IIC-IFOL-ALF IN DBN-FLD.
      * IDC-INI --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF DBC-NUM-ICLI-LEN IN DBN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DBC-NUM-ICLI-LEN IN DBN-FLD
                GO TO FIN-EDT-IKY-DBN.
           MOVE DBC-NUM-ICLI IN DBN-FLD TO
                DBC-NUM-ICLI-EDT IN DBN-EDT.
           MOVE DBC-NUM-ICLI-EDT IN DBN-EDT TO
                DBC-NUM-ICLI-ALF IN DBN-FLD.
      * IDC-FIN --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF DBN-NUM-IDBN-LEN IN DBN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DBN-NUM-IDBN-LEN IN DBN-FLD
                GO TO FIN-EDT-IKY-DBN.
           MOVE DBN-NUM-IDBN IN DBN-FLD TO DBN-NUM-IDBN-EDT IN DBN-EDT.
           MOVE DBN-NUM-IDBN-EDT IN DBN-EDT TO 
                DBN-NUM-IDBN-ALF IN DBN-FLD.
       FIN-EDT-IKY-DBN.
           EXIT.
       
       EDT-MKY-DBN SECTION.
       INI-EDT-MKY-DBN.
       FIN-EDT-MKY-DBN.
           EXIT.
       
       EDT-AKY-DBN SECTION.
       INI-EDT-AKY-DBN.
       FIN-EDT-AKY-DBN.
           EXIT.
       
       EDT-FLD-DBN SECTION.
       INI-EDT-FLD-DBN.
           IF DBN-VAL-MDEU-LEN IN DBN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DBN-VAL-MDEU-LEN IN DBN-FLD
                GO TO FIN-EDT-FLD-DBN.
           MOVE DBN-VAL-MDEU IN DBN-FLD TO DBN-VAL-MDEU-EDT IN DBN-EDT.
           MOVE DBN-VAL-MDEU-EDT IN DBN-EDT TO 
                DBN-VAL-MDEU-ALF IN DBN-FLD.
       FIN-EDT-FLD-DBN.
           EXIT.
