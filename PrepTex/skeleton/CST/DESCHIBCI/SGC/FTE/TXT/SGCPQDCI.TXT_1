*%! CMA_USR = TRUE
*% HDR_CST    = TRUE
*% NO_HDR_CST = FALSE
*% PGM_PTC    = TRUE
*% GNSTCA_ORIG = 3000
*% GNSTCA = 3000
*% IF WSS  
      *<<<< WSS
       COPY GNSWV3CR.                                                   
      *     FIN_SCR_VARI                       
           03 SCR-CIC-ICLI.     
              05  SCR-CAI-ICLI                         PIC X(04).       
              05  SCR-IIC-ICLI                         PIC X(08).       

       01 WSS-VARI.                                                     
          03 WSS-FEC-FINI.                                              
             05 WSS-NUM-SINI                           PIC 9(02).       
             05 WSS-NUM-AINI                           PIC 9(02).       
             05 WSS-NUM-MINI                           PIC 9(02).       
             05 WSS-NUM-DINI                           PIC 9(02).       
          03 WSS-FEC-FTER.                                              
             05 WSS-NUM-STER                           PIC 9(02).       
             05 WSS-NUM-ATER                           PIC 9(02).       
             05 WSS-NUM-MTER                           PIC 9(02).       
             05 WSS-NUM-DTER                           PIC 9(02).       
                                                                        
       COPY GNSWGCPT.                                                   
       COPY GNSWGSTR.                                                   
       COPY SGCBRDBC.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.        
       COPY SGCBRDCP.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DCP-REQA==.        
       COPY SGCBRDCE.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DCE-REQA==.        
      *>>>>
*% END
*% IF SYS_SCOD 
      *<<<< SYS_SCOD
           MOVE +3000 TO SCR-TCMA.                                       
      *>>>>
*% END
*% IF PGM_POS_EDB  AND  PGM_PU 
      *<<<< PGM_POS_EDB
           MOVE +3000 TO SCR-TCMA.                                       
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY                                                 
                                                                        
           GO TO FIN-FND-KEY.                                           
       SIG-FND-KEY.                                                     
           MOVE 'TAB'        TO MSG-COD-SIST.                           
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MENS.                               
           MOVE '00'         TO FIO-STAT.                               
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           GO TO FIN-FND-KEY.                                           
      *>>>>                                                             
*% END
*% IF FIN_HBL_FLD  
      *<<<< FIN_HBL_FLD
           IF SCR-CMND = 'ING'                                          
               MOVE 'N'      TO DCI-IND-AACC IN DCI-FLD                 
               PERFORM GET-FHOY                                         
               MOVE HOY-DHOY TO DCI-NUM-DTRN IN DCI-FLD                 
               MOVE HOY-MHOY TO DCI-NUM-MTRN IN DCI-FLD                 
               MOVE HOY-SHOY TO DCI-NUM-STRN IN DCI-FLD                 
               MOVE HOY-AHOY TO DCI-NUM-ATRN IN DCI-FLD.                
      *>>>>
*% END                                                             
*% IF EOF_PQ
      *<<<< EOF_PQ
       GNS-BUS-VCA SECTION.                                             
       INI-GNS-BUS-VCA.                                                 
           MOVE 0 TO FRM-IFLD.                                          
       LUP-GNS-BUS-VCA.                                                 
           ADD 1 TO FRM-IFLD.                                           
           IF FRM-IFLD NOT > EIBCALEN AND                               
              FRM-IFLD NOT > 3000                                       
                MOVE DFH-CMMA(FRM-IFLD) TO PGM-CMMA-CHR(FRM-IFLD)       
                GO TO LUP-GNS-BUS-VCA.                                  
           MOVE PGM-CMMA TO SCR-VARI.                                   
       FIN-GNS-BUS-VCA.                                                 
           EXIT.                                                        
      *>>>>                                                             
*% END
*% IF EOF_IBM   
      *<<<< EOF 
       COPY GNSBVTAB.                                                   
       COPY GNSBVCOD.                                                   
       COPY GNSBBTAB.                                                   
       COPY GNSBGCPT.                                                   
       COPY GNSBGSTR.                                                   
       COPY SGCBFDBC.                                                   
       COPY SGCBFDCP.                                                   
       COPY SGCBFDCE.                                                   
       COPY SGCBGCLI.                                                   
       COPY TABBVTDU.                                                   
       COPY TABBVOFI.                                                   
      *>>>>                                                             
*% END
