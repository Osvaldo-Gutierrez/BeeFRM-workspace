*% IF FIN_DBC_GLS_ICLI
      *<<<< DBC_GLS_ICLI                                                
           IF DBC-NUM-ICLI IN DAC-FLD > ZEROES AND                      
              DAC-IIC-IFOL IN DAC-FLD = ZEROES                          
              MOVE DBC-NUM-ICLI IN DAC-FLD TO DBC-NUM-ICLI IN DBC       
              MOVE DBC-IND-ICLI IN DAC-FLD TO DBC-IND-ICLI IN DBC       
              MOVE DBC-GLS-ICLI IN DAC-FLD TO DBC-GLS-ICLI IN DBC       
              MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY                          
              MOVE FIO-GET-KEY TO FIO-CMND                              
              PERFORM SGC-FIO-DBC                                       
              IF NOT FIO-STAT-OKS                                       
                  MOVE 'SGC'        TO MSG-COD-SIST                     
                  MOVE 'CLI    NEX' TO MSG-COD-MENS                     
                  PERFORM GET-MSG                                       
                  MOVE SPACES       TO FRM-GLS-NOMC IN DAC-FLD          
                  MOVE MSG-GLS-DESC TO FRM-MENS                         
                  MOVE FRM-SUAR-MAL TO FRM-SUAR                         
                  GO TO FIN-VAL-DBC-GLS-ICLI                            
              ELSE                                                      
                  PERFORM CPT-STR-NOMC                                  
                  PERFORM FND-RPR-LAST                                  
                  PERFORM CHK-RPR-LAST                                  
           ELSE                                                         
              IF  DAC-IIC-IFOL IN DAC-FLD > ZEROES                      
                  PERFORM FND-RPR-IFOL.                                 
      *>>>>                                                             
*% END
*% IF EOF
      *<<<< EOF                                                         
       FND-RPR-IFOL SECTION.                                            
       INI-FND-RPR-IFOL.                                                
           MOVE 'N'                        TO WSS-FND-INFO.             
           MOVE DAC-CAI-IFOL IN DAC-FLD    TO RPR-CAI-IFOL IN RPR.      
           MOVE DAC-IIC-IFOL IN DAC-FLD    TO RPR-IIC-IFOL IN RPR.      
           MOVE FIO-GET-KEY                TO FIO-CMND.                 
           PERFORM SGC-FIO-RPR.                                         
           IF NOT FIO-STAT-OKS                                          
              MOVE 'SGC'          TO MSG-COD-SIST                       
              MOVE 'FOLRPRNODISP' TO MSG-COD-MENS                       
              PERFORM GET-MSG                                           
              MOVE SPACES       TO DAC-CAI-IFOL IN DAC-FLD              
              MOVE ZEROES       TO DAC-IIC-IFOL IN DAC-FLD              
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              MOVE FRM-SUAR-MAL TO FRM-SUAR                             
              GO TO FIN-FND-RPR-IFOL                                    
           ELSE                                                         
            IF ( SCR-CMND = 'MOD' OR SCR-CMND = 'ELI' ) AND             
                 RPR-IND-ESTA IN RPR = 'A'                              
               MOVE 'SGC'         TO MSG-COD-SIST                       
               MOVE 'FOLRPRYAACA' TO MSG-COD-MENS                       
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE FRM-SUAR-MAL TO FRM-SUAR                            
               MOVE 'S'          TO WSS-FND-INFO IN WSS-VARI            
               GO TO FIN-FND-RPR-IFOL                                   
            ELSE                                                        
             IF SCR-CMND = 'MRA'  AND                                   
                RPR-IND-ESTA IN RPR NOT = 'A'                           
                MOVE 'SGC'         TO MSG-COD-SIST                      
                MOVE 'FOLRPRNOACA' TO MSG-COD-MENS                      
                PERFORM GET-MSG                                         
                MOVE MSG-GLS-DESC TO FRM-MENS                           
                MOVE FRM-SUAR-MAL TO FRM-SUAR                           
                MOVE 'S'          TO WSS-FND-INFO IN WSS-VARI           
                GO TO FIN-FND-RPR-IFOL                                  
             ELSE                                                       
                MOVE 'S'                 TO WSS-FND-INFO IN WSS-VARI    
                MOVE RPR-CAI-IFOL IN RPR TO DAC-CAI-IFOL IN DAC-FLD     
                MOVE RPR-IIC-IFOL IN RPR TO DAC-IIC-IFOL IN DAC-FLD     
                PERFORM FND-DBC-ICLI                                    
                GO TO FIN-FND-RPR-IFOL.                                 
       FIN-FND-RPR-IFOL.                                                
           EXIT.                                                        
                                                                        
       FND-DBC-ICLI SECTION.                                            
       INI-FND-DBC-ICLI.                                                
              MOVE RPR-CIC-ICLI IN RPR TO DBC-CIC-ICLI IN DBC           
              MOVE SPACES      TO FIO-AKEY                              
              MOVE FIO-GET-KEY TO FIO-CMND                              
              PERFORM SGC-FIO-DBC                                       
              IF NOT FIO-STAT-OKS                                       
                  MOVE 'SGC'        TO MSG-COD-SIST                     
                  MOVE 'CLI    NEX' TO MSG-COD-MENS                     
                  PERFORM GET-MSG                                       
                  MOVE SPACES       TO FRM-GLS-NOMC IN DAC-FLD          
                  MOVE MSG-GLS-DESC TO FRM-MENS                         
                  MOVE FRM-SUAR-MAL TO FRM-SUAR                         
                  GO TO FIN-FND-DBC-ICLI                                
              ELSE                                                      
                  PERFORM CPT-STR-NOMC.                                 
       FIN-FND-DBC-ICLI.                                                
           EXIT.                                                        
                                                                        
       FND-RPR-LAST SECTION.                                            
       INI-FND-RPR-LAST.                                                
           MOVE 'N'                        TO WSS-FND-INFO.             
           MOVE DBC-CIC-ICLI IN DBC        TO RPR-CIC-ICLI IN RPR.      
           MOVE '99999999'                 TO RPR-FEC-FRPR IN RPR.      
           MOVE FIO-GET-LEQ                TO FIO-CMND.                 
       LUP-FND-RPR-LAST.                                                
           MOVE 'RPR-AKY-IRPR'             TO FIO-AKEY.                 
           PERFORM SGC-FIO-RPR.                                         
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF                          
              GO TO FIN-FND-RPR-LAST.                                   
           IF RPR-CIC-ICLI IN RPR NOT = DBC-CIC-ICLI IN DBC             
              GO TO FIN-FND-RPR-LAST                                    
           ELSE                                                         
            IF ( SCR-CMND = 'MOD' OR SCR-CMND = 'ELI' ) AND             
               RPR-IND-ESTA IN RPR = 'A'                                
               MOVE 'SGC'         TO MSG-COD-SIST                       
               MOVE 'FOLRPRYAACA' TO MSG-COD-MENS                       
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE FRM-SUAR-MAL TO FRM-SUAR                            
               MOVE 'S'          TO WSS-FND-INFO IN WSS-VARI            
               GO TO FIN-FND-RPR-LAST                                   
            ELSE                                                        
             IF SCR-CMND = 'MRA'  AND                                   
                RPR-IND-ESTA IN RPR NOT = 'A'                           
                MOVE 'SGC'         TO MSG-COD-SIST                      
                MOVE 'FOLRPRNOACA' TO MSG-COD-MENS                      
                PERFORM GET-MSG                                         
                MOVE MSG-GLS-DESC TO FRM-MENS                           
                MOVE FRM-SUAR-MAL TO FRM-SUAR                           
                MOVE 'S'          TO WSS-FND-INFO IN WSS-VARI           
                GO TO FIN-FND-RPR-LAST                                  
             ELSE                                                       
                MOVE 'S'                 TO WSS-FND-INFO IN WSS-VARI    
                MOVE RPR-CAI-IFOL IN RPR TO DAC-CAI-IFOL IN DAC-FLD     
                MOVE RPR-IIC-IFOL IN RPR TO DAC-IIC-IFOL IN DAC-FLD     
                GO TO FIN-FND-RPR-LAST.                                 
       LEE-PRV-RPR.                                                     
           MOVE FIO-GET-PRV            TO FIO-CMND.                     
           GO TO LUP-FND-RPR-LAST.                                      
       FIN-FND-RPR-LAST.                                                
           EXIT.                                                        
                                                                        
       CHK-RPR-LAST SECTION.                                            
       INI-CHK-RPR-LAST.                                                
           IF WSS-FND-INFO = 'N'                                        
                  MOVE 'SGC'          TO MSG-COD-SIST                   
                  MOVE 'FOLRPRNODISP' TO MSG-COD-MENS                   
                  PERFORM GET-MSG                                       
                  MOVE SPACES       TO DAC-CAI-IFOL IN DAC-FLD          
                  MOVE ZEROES       TO DAC-IIC-IFOL IN DAC-FLD          
                  MOVE MSG-GLS-DESC TO FRM-MENS                         
                  MOVE FRM-SUAR-MAL TO FRM-SUAR                         
                  GO TO FIN-CHK-RPR-LAST.                               
       FIN-CHK-RPR-LAST.                                                
           EXIT.                                                        
                                                                        
       CPT-STR-NOMC SECTION.                                            
       INI-CPT-STR-NOMC.                                                
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.                        
           PERFORM CPT-BLKS.                                            
           MOVE CPT-STRN  TO FRM-GLS-NOMC IN DAC-FLD.                   
           MOVE DBC-CAI-ICLI IN DBC TO SCR-CAI-ICLI.                    
           MOVE DBC-IIC-ICLI IN DBC TO SCR-IIC-ICLI.                    
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN DAC-FLD.         
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN DAC-FLD.         
       FIN-CPT-STR-NOMC.                                                
           EXIT.                                                        
                                                                        
       CPT-MTZ-PROD SECTION.                                            
       INI-CPT-MTZ-PROD.                                                
           MOVE ZEROES             TO WSS-NUM-READ.                     
           MOVE ZEROES             TO WSS-NUM-PROD.                     
       LUP-CPT-MTZ-PROD.                                                
           ADD 1                   TO WSS-NUM-READ.                     
           IF WSS-NUM-READ > 8                                          
              GO TO FIN-CPT-MTZ-PROD.                                   
*% IF PGM_BQ
           IF WSS-COD-PRSO IN WSS-VARI(WSS-NUM-READ) > SPACES           
              ADD 1                TO WSS-NUM-PROD                      
              MOVE WSS-COD-PRSO IN WSS-VARI(WSS-NUM-READ) TO            
                   DAC-COD-PRSO IN DAC(WSS-NUM-PROD)                    
              MOVE WSS-VAL-PRSO IN WSS-VARI(WSS-NUM-READ) TO            
                   DAC-VAL-PRSO IN DAC(WSS-NUM-PROD).                   
           GO TO LUP-CPT-MTZ-PROD.                                      
*% ELSE
      *    IF WSS-COD-PRSO IN WSS-VARI(WSS-NUM-READ) > SPACES           
              ADD 1                TO WSS-NUM-PROD.
              MOVE WSS-COD-PRSO IN WSS-VARI(WSS-NUM-READ) TO            
                   DAC-COD-PRSO IN DAC(WSS-NUM-PROD).                    
              MOVE WSS-VAL-PRSO IN WSS-VARI(WSS-NUM-READ) TO            
                   DAC-VAL-PRSO IN DAC(WSS-NUM-PROD).                   
           GO TO LUP-CPT-MTZ-PROD.                                      
*% END
       FIN-CPT-MTZ-PROD.                                                
           EXIT.                                                        
                                                                        
       VAL-COD-PROD SECTION.                                            
       INI-VAL-COD-PROD.                                                
            MOVE 'N'                     TO WSS-DAT-ERRO IN WSS-VARI.   
            IF WSS-COD-PROD IN WSS-VARI > SPACES                        
               MOVE 'SGC'                TO TAB-COD-SIST                
               MOVE 'DOF'                TO TAB-COD-TTAB IN TAB         
               MOVE WSS-COD-PROD IN WSS-VARI TO TAB-COD-CTAB IN TAB     
               PERFORM VAL-COD                                          
               IF MSG-COD-MENS  > SPACES                                
                  MOVE MSG-GLS-DESC      TO FRM-MENS                    
                  MOVE FRM-SUAR-MAL      TO FRM-SUAR                    
                  GO TO FIN-VAL-COD-PROD.                               
            IF WSS-VAL-PROD IN WSS-VARI > ZEROES AND                    
               WSS-COD-PROD IN WSS-VARI = SPACES                        
                  MOVE 'S'               TO WSS-DAT-ERRO IN WSS-VARI.   
       FIN-VAL-COD-PROD.                                                
           EXIT.                                                        
                                                                        
       VAL-VAL-PROD SECTION.                                            
       INI-VAL-VAL-PROD.                                                
            MOVE 'N'                     TO WSS-DAT-ERRO IN WSS-VARI.   
            IF WSS-COD-PROD IN WSS-VARI > SPACES                        
               IF WSS-VAL-PROD IN WSS-VARI = ZEROES                     
                  MOVE 'S'               TO WSS-DAT-ERRO IN WSS-VARI    
                  GO TO FIN-VAL-VAL-PROD.                               
       FIN-VAL-VAL-PROD.                                                
           EXIT.                                                        
                                                                        
       VAL-IND-SINO SECTION.                                            
       INI-VAL-IND-SINO.                                                
               MOVE 'GNS'                    TO TAB-COD-SIST            
                                                FIO-SIST.               
               MOVE 'SON'                    TO TAB-COD-TTAB IN TAB.    
               MOVE WSS-IND-SINO IN WSS-VARI TO TAB-COD-CTAB IN TAB.    
               PERFORM VAL-COD.                                         
               GO TO FIN-VAL-IND-SINO.                                  
       FIN-VAL-IND-SINO.                                                
           EXIT.                                                        
      *>>>> EOF
*% END
