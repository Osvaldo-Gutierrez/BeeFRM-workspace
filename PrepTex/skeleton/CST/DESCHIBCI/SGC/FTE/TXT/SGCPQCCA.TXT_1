*%! Codigo adicional
*% NO_TIENE_TIMESTAMP = TRUE
*% NO_HDR_CST = TRUE
*% LARGO_TOTAL_COMMAREA = 3000
*% FMT_CHI = TRUE
*% GEN_BCI = TRUE
*% PGM_PTC = TRUE
*% IF CMT_ID
*% END
*% IF WSS
      *<<<< WSS
       COPY GNSWGCPT.
       COPY GNSWGSTR.
       COPY SGCBRDBC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
       COPY SGCWXCCA.
*% IF NOT PGM_MCP AND NOT PGM_PTC
       COPY GNSWGQUE.
*% END
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
       01 WSS-NUM-CCUR.
           03 WSS-NUM-ICUR-EDT  PIC  ZZ9.
           03 WSS-NUM-ICUR-ALF       PIC X(03).
           03 WSS-NUM-ICUR      REDEFINES
              WSS-NUM-ICUR-ALF       PIC 9(03).
      *>>>>
*% END
*% IF FIN_SCR_VARI
           03 SCR-NUM-PANT               PIC 9(0002).
           03 SCR-CCA-FLD                PIC X(1100).
           03 SCR-CIC-ICLI.
              05 SCR-CAI-ICLI            PIC X(04).
              05 SCR-IIC-ICLI            PIC X(08).
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-OFRM-SRV
                MOVE TSK-TERM-ALF TO CCA-COD-TERM
                MOVE TSK-TERM-ALF TO CCA-COD-TERM-MAS
           ELSE
                MOVE EIBTRMID     TO CCA-COD-TERM
                MOVE EIBTRMID     TO CCA-COD-TERM-MAS.
           MOVE +3859 TO SCR-TCMA.
           PERFORM GNS-ERR-QUE.
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )
               MOVE SCR-CCA-FLD TO CCA-FLD
               GO TO FIN-INI.

           IF ( FRM-FFLD = FRM-FFLD-PF6 )
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM
               ADD 1              TO QUE-NITM
               MOVE CCA-NOM-TSCCA TO QUE-COLA
               MOVE CCA-LEN-TSCCA TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT NOT = QUE-STAT-OKS
                  MOVE SCR-CCA-FLD TO CCA-FLD
                  GO TO FIN-INI.

           PERFORM GET-FHOY.

      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
           IF FRM-IND-SGTE IN CCA-FLD = 'S'
               MOVE FRM-COD-CPTS IN CCA-FLD TO CAC-COD-CPTO IN CAC
               MOVE FRM-CAI-CLIS IN CCA-FLD TO CAC-CAI-ICLI IN CAC
               MOVE FRM-IIC-CLIS IN CCA-FLD TO CAC-IIC-ICLI IN CAC
               MOVE FRM-NUM-DNIS IN CCA-FLD TO CAC-NUM-DINI IN CAC
               MOVE FRM-NUM-MNIS IN CCA-FLD TO CAC-NUM-MINI IN CAC
               MOVE FRM-NUM-SNIS IN CCA-FLD TO CAC-NUM-SINI IN CAC
               MOVE FRM-NUM-ANIS IN CCA-FLD TO CAC-NUM-AINI IN CAC
               MOVE FRM-HRA-INIS IN CCA-FLD TO CAC-HRA-HINI IN CAC
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM SGC-FIO-CAC
               GO TO CNT-LEE-CAC.

           MOVE CAC-COD-CPTO IN CCA-FLD TO CAC-COD-CPTO IN CAC.
           MOVE SCR-CAI-ICLI            TO CAC-CAI-ICLI IN CAC.
           MOVE SCR-IIC-ICLI            TO CAC-IIC-ICLI IN CAC.
           MOVE CAC-NUM-SINI IN CCA-FLD TO CAC-NUM-SINI IN CAC.
           MOVE CAC-NUM-AINI IN CCA-FLD TO CAC-NUM-AINI IN CAC.
           MOVE CAC-NUM-MINI IN CCA-FLD TO CAC-NUM-MINI IN CAC.
           MOVE CAC-NUM-DINI IN CCA-FLD TO CAC-NUM-DINI IN CAC.
           MOVE CAC-HRA-HINI IN CCA-FLD TO CAC-HRA-HINI IN CAC.

           IF CAC-FEC-FINI IN CCA-FLD > ZEROES
              MOVE CAC-NUM-DINI IN CCA-FLD TO WSS-NUM-DINI IN WSS-VARI
              MOVE CAC-NUM-MINI IN CCA-FLD TO WSS-NUM-MINI IN WSS-VARI
              MOVE CAC-NUM-SINI IN CCA-FLD TO WSS-NUM-SINI IN WSS-VARI
              MOVE CAC-NUM-AINI IN CCA-FLD TO WSS-NUM-AINI IN WSS-VARI
           ELSE
              MOVE ZEROES                  TO WSS-FEC-FINI IN WSS-VARI.

      *----------------------------------------------------------------*
           IF CAC-COD-CPTO IN CAC NOT > SPACES
              MOVE '999999999999' TO CAC-COD-CPTO IN CAC.
           IF CAC-CIC-ICLI IN CAC NOT > SPACES
              MOVE '999999999999' TO CAC-CIC-ICLI IN CAC.
           IF CAC-FEC-FINI IN CAC NOT > ZEROES
              MOVE 99999999       TO CAC-FEC-FINI IN CAC.
           IF CAC-HRA-HINI IN CAC NOT > ZEROES
              MOVE 999999         TO CAC-HRA-HINI IN CAC.
      *----------------------------------------------------------------*
       CNT-LEE-CAC.
           MOVE ZEROES TO WSS-J IN WSS-VARI.
           MOVE FIO-GET-LEQ TO FIO-CMND.
       LUP-LEE-CAC.
           IF CAC-COD-CPTO IN CCA-FLD NOT > SPACES
              MOVE 'CAC-CIC-ICLI' TO FIO-AKEY.
           PERFORM SGC-FIO-CAC.
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF
               GO TO CON-FND-KEY.

           IF CAC-COD-CPTO IN CCA-FLD > SPACES AND
              CAC-COD-CPTO IN CAC NOT = CAC-COD-CPTO IN CCA-FLD
              GO TO FIN-LEE-CAC.

           IF DBC-NUM-ICLI IN CCA-FLD > ZEROES AND
              SCR-CIC-ICLI NOT = CAC-CIC-ICLI IN CAC
              IF CAC-COD-CPTO IN CCA-FLD > SPACES
                 GO TO NXT-LEE-CAC
              ELSE
                 GO TO FIN-LEE-CAC.

           IF CAC-FEC-FINI IN CCA-FLD > ZEROES AND
              CAC-FEC-FINI IN CAC > WSS-FEC-FINI IN WSS-VARI
              GO TO NXT-LEE-CAC.

           IF CAC-IND-VIGE IN CCA-FLD > SPACES AND
              CAC-IND-VIGE IN CAC NOT = CAC-IND-VIGE IN CCA-FLD
              GO TO NXT-LEE-CAC.

           PERFORM CRG-VEC-CCA.

           IF (SCR-OFRM-SRV AND WSS-J > WSS-NITMMAX)
              GO TO CON-FND-KEY.

           IF ((NOT SCR-OFRM-SRV) AND WSS-J > WSS-JMAX)
                 GO TO CON-FND-KEY.

       NXT-LEE-CAC.
           MOVE FIO-GET-PRV TO FIO-CMND.
           GO TO LUP-LEE-CAC.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
       FIN-LEE-CAC.
           IF FIO-STAT-OKS
              MOVE FIO-END-BRW TO FIO-CMND
              PERFORM SGC-FIO-CAC.

       CON-FND-KEY.
           PERFORM PUT-TAB.

           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *----------------------------------------------------------
       MSG-FND-KEY.
           MOVE 'TAB' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM VAL-NUM-KEY-CCA
               PERFORM VAL-NUM-FLD-CCA
               PERFORM SCRL-TAB-CCA
               PERFORM SCR-TRL.
      *>>>>
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBEGLS.
       COPY GNSBBTAB.
       COPY GNSBVTAB.
       COPY GNSBVCOD.
       COPY GNSBPFEC.
       COPY SGCBFDBC.

       PUT-TAB SECTION.
       INI-PUT-TAB.
           MOVE CCA-NOM-TSCCA     TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE CCA-NOM-TSCCA-MAS TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE 1 TO SCR-NUM-PANT.
           MOVE LOW-VALUES TO WSS-GLS-NCCA-IDX-MTI.
           MOVE LOW-VALUES TO WSS-GLS-NCCA-IDX-FRM.
      *
           MOVE 1 TO FRM-IFLD.
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.

           MOVE 1 TO WSS-F IN WSS-VARI.
       LUP-PUT-TAB.
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI
              GO TO FIN-PUT-TAB.
           IF WSS-COD-CPTO-V(WSS-F) > SPACES
              PERFORM PUT-TAB-CCA
              PERFORM PUT-CCA-QUE
              PERFORM SET-FRM-IFLD
           ELSE
           IF FRM-IFLD = 1 AND WSS-F = 1
              MOVE 'TAB' TO MSG-COD-SIST
              MOVE 'CCA    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE '00' TO FIO-STAT
      *       MOVE 'S' TO SCR-IERR
              GO TO FIN-PUT-TAB
           ELSE
              MOVE '00' TO FIO-STAT
              GO TO FIN-PUT-TAB.

           ADD 1 TO WSS-F IN WSS-VARI.
           GO TO LUP-PUT-TAB.
       FIN-PUT-TAB.
           MOVE 1                  TO QUE-NITM.
           MOVE CCA-LEN-TSCCA-MAS  TO QUE-LITM.
           MOVE CCA-NOM-TSCCA-MAS  TO QUE-COLA.

           IF WSS-NITM > WSS-NITMMAX
              MOVE 'S'           TO CCA-MAS-IND-HMAS
           ELSE
              MOVE 'N'           TO CCA-MAS-IND-HMAS.

           MOVE WSS-NITM         TO WSS-NUM-ICUR.
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.
           MOVE WSS-NUM-ICUR-ALF TO CCA-MAS-NUM-TOCC.
           MOVE CCA-MAS-ITEM     TO QUE-ITEM.
           MOVE QUE-PUT          TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF WSS-NITM > WSS-IMAX
               MOVE 'TAB' TO MSG-COD-SIST
               MOVE 'CCATABDMAS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       EXT-PUT-TAB.
           EXIT.

       SET-FRM-IFLD SECTION.
       INI-SET-FRM-IFLD.
           IF SCR-OFRM-SRV
                MOVE 1 TO FRM-IFLD
           ELSE
                ADD 1 TO FRM-IFLD.
       FIN-SET-FRM-IFLD.
           EXIT.
      *--------------------------------------------
       SCRL-TAB-CCA SECTION.
       INI-SCRL-TAB-CCA.
           MOVE SPACES TO MSG-COD-MENS.

           IF FRM-FFLD = FRM-FFLD-PF6
               PERFORM PUT-TAB-ASC
           ELSE
               PERFORM PUT-TAB-DSC.
           IF MSG-COD-MENS > SPACES
               MOVE 'TAB' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-CCA.
           EXIT.

      *--------------------------------------------
       PUT-TAB-CCA SECTION.
       INI-PUT-TAB-CCA.
           PERFORM CLR-WSS.
           MOVE WSS-COD-CPTO-V(WSS-F) TO WSS-COD-CPTO-MTI.
           MOVE WSS-NUM-ICLI-V(WSS-F) TO WSS-NUM-ICLI-MTI.
           MOVE WSS-VRF-ICLI-V(WSS-F) TO WSS-VRF-ICLI-MTI.
           MOVE WSS-IND-ICLI-V(WSS-F) TO WSS-IND-ICLI-MTI.
           MOVE WSS-GLS-ICLI-V(WSS-F) TO WSS-GLS-ICLI-MTI.
           MOVE WSS-FEC-FINI-V(WSS-F) TO WSS-FEC-FINI-MTI.
           MOVE WSS-HRA-HINI-V(WSS-F) TO WSS-HRA-HINI-MTI.
           MOVE WSS-IND-VIGE-V(WSS-F) TO WSS-IND-VIGE-MTI.
           MOVE WSS-FEC-FTER-V(WSS-F) TO WSS-FEC-FTER-MTI.
           MOVE WSS-HRA-HTER-V(WSS-F) TO WSS-HRA-HTER-MTI.
           MOVE WSS-VAL-MNTO-V(WSS-F) TO WSS-VAL-MNTO-MTI.
           MOVE WSS-VAL-PORC-V(WSS-F) TO WSS-VAL-PORC-MTI.
           MOVE WSS-COD-COD1-V(WSS-F) TO WSS-COD-COD1-MTI.
           MOVE WSS-COD-COD2-V(WSS-F) TO WSS-COD-COD2-MTI.
           MOVE WSS-COD-CD12-V(WSS-F) TO WSS-COD-CD12-MTI.
           MOVE WSS-COD-COD3-V(WSS-F) TO WSS-COD-COD3-MTI.
           MOVE WSS-COD-COD4-V(WSS-F) TO WSS-COD-COD4-MTI.
           MOVE WSS-COD-CD34-V(WSS-F) TO WSS-COD-CD34-MTI.
           MOVE WSS-CAI-ICLI-V(WSS-F) TO WSS-CAI-ICLI-MTI.
           MOVE WSS-GLS-DES1-V(WSS-F) TO WSS-GLS-DES1-MTI.
           MOVE WSS-GLS-DES2-V(WSS-F) TO WSS-GLS-DES2-MTI.
           MOVE WSS-GLS-DESC-V(WSS-F) TO WSS-GLS-DESC-MTI.
           MOVE WSS-IIC-ICLI-V(WSS-F) TO WSS-IIC-ICLI-MTI.
           MOVE WSS-COD-MONE-V(WSS-F) TO WSS-COD-MONE-MTI.


           MOVE WSS-COD-CPTO-MTI      TO WSS-COD-CPTO-FRM.
           MOVE WSS-NUM-ICLI-MTI      TO WSS-NUM-ICLI-FRM.
           MOVE WSS-VRF-ICLI-MTI      TO WSS-VRF-ICLI-FRM.
           MOVE WSS-IND-ICLI-MTI      TO WSS-IND-ICLI-FRM.
           MOVE WSS-GLS-ICLI-MTI      TO WSS-GLS-ICLI-FRM.
           MOVE WSS-FEC-FINI-MTI      TO WSS-FEC-FINI-FRM.
           MOVE WSS-HRA-HINI-MTI      TO WSS-HRA-HINI-FRM.
           MOVE WSS-IND-VIGE-MTI      TO WSS-IND-VIGE-FRM.
           MOVE WSS-FEC-FTER-MTI      TO WSS-FEC-FTER-FRM.
           MOVE WSS-HRA-HTER-MTI      TO WSS-HRA-HTER-FRM.
           MOVE WSS-VAL-MNTO-MTI      TO WSS-VAL-MNTO-FRM.
           MOVE WSS-VAL-PORC-MTI      TO WSS-VAL-PORC-FRM.
           MOVE WSS-COD-COD1-MTI      TO WSS-COD-COD1-FRM.
           MOVE WSS-COD-COD2-MTI      TO WSS-COD-COD2-FRM.
           MOVE WSS-COD-CD12-MTI      TO WSS-COD-CD12-FRM.
           MOVE WSS-COD-COD3-MTI      TO WSS-COD-COD3-FRM.
           MOVE WSS-COD-COD4-MTI      TO WSS-COD-COD4-FRM.
           MOVE WSS-COD-CD34-MTI      TO WSS-COD-CD34-FRM.
           MOVE WSS-CAI-ICLI-MTI      TO WSS-CAI-ICLI-FRM.
           MOVE WSS-GLS-DES1-MTI      TO WSS-GLS-DES1-FRM.
           MOVE WSS-GLS-DES2-MTI      TO WSS-GLS-DES2-FRM.
           MOVE WSS-GLS-DESC-MTI      TO WSS-GLS-DESC-FRM.
           MOVE WSS-IIC-ICLI-MTI      TO WSS-IIC-ICLI-FRM.
           MOVE WSS-COD-MONE-MTI      TO WSS-COD-MONE-FRM.

           IF FRM-IFLD > WSS-IMAX
              GO TO FIN-PUT-TAB-CCA.

           MOVE WSS-COD-CPTO-MTI TO FRM-COD-CPTO IN CCA-FLD(FRM-IFLD).
           MOVE WSS-NUM-ICLI-MTI TO FRM-NUM-ICLI IN CCA-FLD(FRM-IFLD).
           MOVE WSS-VRF-ICLI-MTI TO FRM-VRF-ICLI IN CCA-FLD(FRM-IFLD).
           MOVE WSS-IND-ICLI-MTI TO FRM-IND-ICLI IN CCA-FLD(FRM-IFLD).
           MOVE WSS-GLS-ICLI-MTI TO FRM-GLS-ICLI IN CCA-FLD(FRM-IFLD).
           MOVE WSS-FEC-FINI-MTI TO FRM-FEC-FINI IN CCA-FLD(FRM-IFLD).
           MOVE WSS-HRA-HINI-MTI TO FRM-HRA-HINI IN CCA-FLD(FRM-IFLD).
           MOVE WSS-IND-VIGE-MTI TO FRM-IND-VIGE IN CCA-FLD(FRM-IFLD).
           MOVE WSS-FEC-FTER-MTI TO FRM-FEC-FTER IN CCA-FLD(FRM-IFLD).
           MOVE WSS-HRA-HTER-MTI TO FRM-HRA-HTER IN CCA-FLD(FRM-IFLD).
           MOVE WSS-VAL-MNTO-MTI TO FRM-VAL-MNTO IN CCA-FLD(FRM-IFLD).
           MOVE WSS-VAL-PORC-MTI TO FRM-VAL-PORC IN CCA-FLD(FRM-IFLD).
           MOVE WSS-COD-COD1-MTI TO FRM-COD-COD1 IN CCA-FLD(FRM-IFLD).
           MOVE WSS-COD-COD2-MTI TO FRM-COD-COD2 IN CCA-FLD(FRM-IFLD).
           MOVE WSS-COD-CD12-MTI TO FRM-COD-CD12 IN CCA-FLD(FRM-IFLD).
           MOVE WSS-COD-COD3-MTI TO FRM-COD-COD3 IN CCA-FLD(FRM-IFLD).
           MOVE WSS-COD-COD4-MTI TO FRM-COD-COD4 IN CCA-FLD(FRM-IFLD).
           MOVE WSS-COD-CD34-MTI TO FRM-COD-CD34 IN CCA-FLD(FRM-IFLD).
           MOVE WSS-CAI-ICLI-MTI TO FRM-CAI-ICLI IN CCA-FLD(FRM-IFLD).
           MOVE WSS-GLS-DES1-MTI TO FRM-GLS-DES1 IN CCA-FLD(FRM-IFLD).
           MOVE WSS-GLS-DES2-MTI TO FRM-GLS-DES2 IN CCA-FLD(FRM-IFLD).
           MOVE WSS-GLS-DESC-MTI TO FRM-GLS-DESC IN CCA-FLD(FRM-IFLD).
           MOVE WSS-IIC-ICLI-MTI TO FRM-IIC-ICLI IN CCA-FLD(FRM-IFLD).
           MOVE WSS-COD-MONE-MTI TO FRM-COD-MONE IN CCA-FLD(FRM-IFLD).

       FIN-PUT-TAB-CCA.
           EXIT.
      *----------------------------------------------------
       PUT-CCA-QUE SECTION.
       INI-PUT-CCA-QUE.
           PERFORM PRO-FLD-QUE.
           ADD 1 TO WSS-NITM.
           MOVE CCA-NOM-TSCCA    TO QUE-COLA.
           MOVE CCA-LEN-TSCCA    TO QUE-LITM.
           MOVE WSS-NITM         TO QUE-NITM.
           MOVE SPACES           TO QUE-ITEM.
           IF SCR-OFRM-SRV
              MOVE WSS-GLS-NCCA-IDX-MTI  TO QUE-ITEM
           ELSE
              MOVE WSS-GLS-NCCA-IDX-FRM  TO QUE-ITEM.
           MOVE QUE-PUT  TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
       FIN-PUT-CCA-QUE.
           EXIT.
      *----------------------------------------------------

       PUT-TAB-ASC SECTION.
       INI-PUT-TAB-ASC.
           MOVE SPACES TO MSG-COD-MENS.
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.
           MOVE 0 TO FRM-IFLD.
       LUP-PUT-TAB-ASC.
           ADD 1              TO QUE-NITM.
           MOVE CCA-NOM-TSCCA TO QUE-COLA.
           MOVE CCA-LEN-TSCCA TO QUE-LITM.
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD = 0
                   MOVE 'CCATABNONXT' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-ASC
               ELSE
                   GO TO FIN-PUT-TAB-ASC.
           ADD 1 TO FRM-IFLD.
           MOVE QUE-ITEM TO FRM-COD-CPTO-IDX(FRM-IFLD).
           IF FRM-IFLD < WSS-IMAX
               GO TO LUP-PUT-TAB-ASC.
       FIN-PUT-TAB-ASC.
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX
               ADD 1 FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1
                                 UNTIL WSS-I > WSS-IMAX.
           IF FRM-IFLD > 0
               ADD 1 TO SCR-NUM-PANT.
           IF FRM-IFLD = WSS-IMAX
               ADD 1              TO QUE-NITM
               MOVE CCA-NOM-TSCCA TO QUE-COLA
               MOVE CCA-LEN-TSCCA TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT = QUE-STAT-OKS
                   MOVE 'DEU'        TO MSG-COD-SIST
                   MOVE 'CCATABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-ASC.
           EXIT.

       PUT-TAB-DSC SECTION.
       INI-PUT-TAB-DSC.
           MOVE SPACES TO MSG-COD-MENS.
           IF SCR-NUM-PANT = 1
               MOVE 'CCATABNOPRV' TO MSG-COD-MENS
               GO TO EXT-PUT-TAB-DSC.

           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.
           ADD 1 TO QUE-NITM.
           ADD 1 WSS-IMAX GIVING FRM-IFLD.
       LUP-PUT-TAB-DSC.
           SUBTRACT  1 FROM QUE-NITM.
           MOVE CCA-NOM-TSCCA TO QUE-COLA
           MOVE CCA-LEN-TSCCA TO QUE-LITM
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD > WSS-IMAX
                   MOVE 'CCATABNOPRV' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-DSC
               ELSE
                   GO TO FIN-PUT-TAB-DSC.
           SUBTRACT 1 FROM FRM-IFLD.
           MOVE QUE-ITEM TO FRM-COD-CPTO-IDX(FRM-IFLD).
           IF FRM-IFLD > 1
               GO TO LUP-PUT-TAB-DSC.
       FIN-PUT-TAB-DSC.

           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1
                                 UNTIL WSS-I < 1.

           IF FRM-IFLD NOT > WSS-IMAX
               SUBTRACT 1 FROM SCR-NUM-PANT
               MOVE 'DEU'        TO MSG-COD-SIST
               MOVE 'CCATABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-DSC.
           EXIT.

       COPY GNSBGCPT.
       COPY GNSBGSTR.

       CLR-FRM SECTION.
       INI-CLR-FRM.
           MOVE SPACES TO FRM-COD-CPTO IN CCA-FLD(WSS-I).
           MOVE ZEROES TO FRM-NUM-ICLI IN CCA-FLD(WSS-I).
           MOVE SPACES TO FRM-VRF-ICLI IN CCA-FLD(WSS-I).
           MOVE SPACES TO FRM-IND-ICLI IN CCA-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-ICLI IN CCA-FLD(WSS-I).
           MOVE ZEROES TO FRM-FEC-FINI IN CCA-FLD(WSS-I).
           MOVE ZEROES TO FRM-HRA-HINI IN CCA-FLD(WSS-I).
           MOVE SPACES TO FRM-IND-VIGE IN CCA-FLD(WSS-I).
           MOVE ZEROES TO FRM-FEC-FTER IN CCA-FLD(WSS-I).
           MOVE ZEROES TO FRM-HRA-HTER IN CCA-FLD(WSS-I).
           MOVE ZEROES TO FRM-VAL-MNTO IN CCA-FLD(WSS-I).
           MOVE ZEROES TO FRM-VAL-PORC IN CCA-FLD(WSS-I).
