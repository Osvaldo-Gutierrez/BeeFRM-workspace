*%! Codigo adicional
*% NO_TIENE_TIMESTAMP = TRUE
*% NO_HDR_CST = TRUE
*% LARGO_TOTAL_COMMAREA = 3000
*% FMT_CHI = TRUE
*% GEN_BCI = TRUE
*% PGM_PTC = TRUE
*% IF WSS
      *<<<< WSS 
       COPY GNSWGCPT.
       COPY GNSWGSTR.
       COPY SGCBRDBC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
       COPY SGCBRRPR.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RPR-REQA==.
       COPY SGCWXQVE.
*% IF NOT PGM_MCP AND NOT PGM_PTC
       COPY GNSWGQUE.
*% END
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
       01 WSS-NUM-CCUR.
           03 WSS-NUM-ICUR-EDT  PIC  ZZ9.
           03 WSS-NUM-ICUR-ALF       PIC X(03).
           03 WSS-NUM-ICUR      REDEFINES
              WSS-NUM-ICUR-ALF       PIC 9(03).
      *>>>>
*% END
*% IF FIN_SCR_VARI
           03 SCR-NUM-PANT               PIC 9(0012).
           03 SCR-QVE-FLD                PIC X(1100).
           03 SCR-CIC-ICLI.
              05 SCR-CAI-ICLI            PIC X(04).
              05 SCR-IIC-ICLI            PIC X(08).
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-OFRM-SRV
                MOVE TSK-TERM-ALF TO QVE-COD-TERM
                MOVE TSK-TERM-ALF TO QVE-COD-TERM-MAS
           ELSE
                MOVE EIBTRMID     TO QVE-COD-TERM
                MOVE EIBTRMID     TO QVE-COD-TERM-MAS.
           MOVE +3859 TO SCR-TCMA.
           PERFORM GNS-ERR-QUE.
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )
               MOVE SCR-QVE-FLD TO QVE-FLD
               GO TO FIN-INI.

           IF ( FRM-FFLD = FRM-FFLD-PF6 )
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM
               ADD 1              TO QUE-NITM
               MOVE QVE-NOM-TSQVE TO QUE-COLA
               MOVE QVE-LEN-TSQVE TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT NOT = QUE-STAT-OKS
                  MOVE SCR-QVE-FLD TO QVE-FLD
                  GO TO FIN-INI.

      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
           MOVE VEH-CAI-IFOL IN QVE-FLD TO VEH-CAI-IFOL IN VEH.
           MOVE VEH-IIC-IFOL IN QVE-FLD TO VEH-IIC-IFOL IN VEH.
           MOVE ZEROES                  TO VEH-NUM-IVEH IN VEH.
           
           IF FRM-IND-ISGT IN QVE-FLD = 'S'  AND
              FRM-NUM-ISGT IN QVE-FLD > ZEROES
              MOVE FRM-NUM-ISGT IN QVE-FLD TO VEH-NUM-IVEH IN VEH
	   ELSE
	      MOVE ZEROES                  TO VEH-NUM-IVEH IN VEH.  	
  
           MOVE ZEROES TO WSS-J IN WSS-VARI.
           MOVE FIO-GET-NLS TO FIO-CMND.
       LUP-LEE-VEH.
           PERFORM SGC-FIO-VEH.
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF
               GO TO CON-FND-KEY.

           IF (VEH-CAI-IFOL IN QVE-FLD NOT = VEH-CAI-IFOL IN VEH OR
               VEH-IIC-IFOL IN QVE-FLD NOT = VEH-IIC-IFOL IN VEH)
               GO TO CON-FND-KEY.

           PERFORM CRG-VEC-QVE.

           IF (SCR-OFRM-SRV AND WSS-J > WSS-NITMMAX)
               GO TO CON-FND-KEY.

           IF ((NOT SCR-OFRM-SRV) AND WSS-J > WSS-JMAX)
               GO TO CON-FND-KEY.

       NXT-LEE-VEH.
           MOVE FIO-GET-NXT TO FIO-CMND.
           GO TO LUP-LEE-VEH.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
       CON-FND-KEY.
           PERFORM PUT-TAB.

           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *----------------------------------------------------------
       MSG-FND-KEY.
           MOVE 'TAB' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM VAL-NUM-KEY-QVE
               PERFORM VAL-NUM-FLD-QVE
               PERFORM SCRL-TAB-QVE
               PERFORM SCR-TRL.
      *>>>>
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBBTAB.
       COPY GNSBVTAB.
       COPY GNSBVCOD.
       COPY SGCBFDBC.
       COPY SGCBFRPR.

       PUT-TAB SECTION.
       INI-PUT-TAB.
           MOVE QVE-NOM-TSQVE     TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE QVE-NOM-TSQVE-MAS TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE 1 TO SCR-NUM-PANT.
           MOVE LOW-VALUES TO WSS-GLS-NQVE-IDX-MTI.
           MOVE LOW-VALUES TO WSS-GLS-NQVE-IDX-FRM.
      *
           MOVE 1 TO FRM-IFLD.
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.

           MOVE 1 TO WSS-F IN WSS-VARI.
       LUP-PUT-TAB.
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI
              GO TO FIN-PUT-TAB.
     ***   IF WSS-COD-MRCV-V(WSS-F) > SPACES
           IF WSS-NUM-IVEH-V(WSS-F) > ZEROES
	      PERFORM PUT-TAB-QVE
              PERFORM PUT-QVE-QUE
              PERFORM SET-FRM-IFLD
           ELSE
           IF FRM-IFLD = 1 AND WSS-F = 1
              MOVE 'TAB' TO MSG-COD-SIST
              MOVE 'QVE    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE '00' TO FIO-STAT
      *       MOVE 'S' TO SCR-IERR
              GO TO FIN-PUT-TAB
           ELSE
              MOVE '00' TO FIO-STAT
              GO TO FIN-PUT-TAB.

           ADD 1 TO WSS-F IN WSS-VARI.
           GO TO LUP-PUT-TAB.
       FIN-PUT-TAB.
           MOVE 1                  TO QUE-NITM.
           MOVE QVE-LEN-TSQVE-MAS  TO QUE-LITM.
           MOVE QVE-NOM-TSQVE-MAS  TO QUE-COLA.

           IF WSS-NITM > WSS-NITMMAX
              MOVE 'S'           TO QVE-MAS-IND-HMAS
           ELSE
              MOVE 'N'           TO QVE-MAS-IND-HMAS.

           MOVE WSS-NITM         TO WSS-NUM-ICUR.
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.
           MOVE WSS-NUM-ICUR-ALF TO QVE-MAS-NUM-TOCC.
           MOVE QVE-MAS-ITEM     TO QUE-ITEM.
           MOVE QUE-PUT          TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF WSS-NITM > WSS-IMAX
               MOVE 'TAB' TO MSG-COD-SIST
               MOVE 'QVETABDMAS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       EXT-PUT-TAB.
           EXIT.

       SET-FRM-IFLD SECTION.
       INI-SET-FRM-IFLD.
           IF SCR-OFRM-SRV
                MOVE 1 TO FRM-IFLD
           ELSE
                ADD 1 TO FRM-IFLD.
       FIN-SET-FRM-IFLD.
           EXIT.
      *--------------------------------------------
       SCRL-TAB-QVE SECTION.
       INI-SCRL-TAB-QVE.
           MOVE SPACES TO MSG-COD-MENS.

           IF FRM-FFLD = FRM-FFLD-PF6
               PERFORM PUT-TAB-ASC
           ELSE
               PERFORM PUT-TAB-DSC.
           IF MSG-COD-MENS > SPACES
               MOVE 'TAB' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-QVE.
           EXIT.

      *--------------------------------------------
       PUT-TAB-QVE SECTION.
       INI-PUT-TAB-QVE.

           PERFORM CLR-WSS.
           MOVE WSS-NUM-IVEH-V(WSS-F) TO WSS-NUM-IVEH-QVE IN WSS-VARI.
           MOVE WSS-GLS-MRCV-V(WSS-F) TO WSS-GLS-MRCV-QVE IN WSS-VARI.
           MOVE WSS-GLS-MODV-V(WSS-F) TO WSS-GLS-MODV-QVE IN WSS-VARI.
           MOVE WSS-NUM-AVEH-V(WSS-F) TO WSS-NUM-AVEH-QVE IN WSS-VARI.
           MOVE WSS-VAL-DPRM-V(WSS-F) TO WSS-VAL-DPRM-QVE IN WSS-VARI.
           MOVE WSS-VAL-SDEU-V(WSS-F) TO WSS-VAL-SDEU-QVE IN WSS-VARI.

           MOVE WSS-NUM-IVEH-QVE IN WSS-VARI TO WSS-NUM-IVEH-FRM.
           MOVE WSS-GLS-MRCV-QVE IN WSS-VARI TO WSS-GLS-MRCV-FRM.
           MOVE WSS-GLS-MODV-QVE IN WSS-VARI TO WSS-GLS-MODV-FRM.
           MOVE WSS-NUM-AVEH-QVE IN WSS-VARI TO WSS-NUM-AVEH-FRM
           MOVE WSS-VAL-DPRM-QVE IN WSS-VARI TO WSS-VAL-DPRM-FRM.
           MOVE WSS-VAL-SDEU-QVE IN WSS-VARI TO WSS-VAL-SDEU-FRM.
           IF FRM-IFLD > WSS-IMAX
              GO TO FIN-PUT-TAB-QVE.

           MOVE WSS-NUM-IVEH-QVE IN WSS-VARI TO
                FRM-NUM-IVEH IN QVE-FLD(FRM-IFLD).
           MOVE WSS-GLS-MRCV-QVE IN WSS-VARI TO
                FRM-GLS-MRCV IN QVE-FLD(FRM-IFLD).
           MOVE WSS-GLS-MODV-QVE IN WSS-VARI TO 
                FRM-GLS-MODV IN QVE-FLD(FRM-IFLD).
           MOVE WSS-NUM-AVEH-QVE IN WSS-VARI TO 
                FRM-NUM-AVEH IN QVE-FLD(FRM-IFLD).
           MOVE WSS-VAL-DPRM-QVE IN WSS-VARI TO 
                FRM-VAL-DPRM IN QVE-FLD(FRM-IFLD).
           MOVE WSS-VAL-SDEU-QVE IN WSS-VARI TO
                FRM-VAL-SDEU IN QVE-FLD(FRM-IFLD).

       FIN-PUT-TAB-QVE.
           EXIT.
      *----------------------------------------------------
       PUT-QVE-QUE SECTION.
       INI-PUT-QVE-QUE.
           PERFORM PRO-FLD-QUE.
           ADD 1 TO WSS-NITM.
           MOVE QVE-NOM-TSQVE    TO QUE-COLA.
           MOVE QVE-LEN-TSQVE    TO QUE-LITM.
           MOVE WSS-NITM         TO QUE-NITM.
           MOVE SPACES           TO QUE-ITEM.
           IF SCR-OFRM-SRV
              MOVE WSS-GLS-NQVE-IDX-MTI  TO QUE-ITEM
           ELSE
              MOVE WSS-GLS-NQVE-IDX-FRM  TO QUE-ITEM.
           MOVE QUE-PUT  TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
       FIN-PUT-QVE-QUE.
           EXIT.
      *----------------------------------------------------

       PUT-TAB-ASC SECTION.
       INI-PUT-TAB-ASC.
           MOVE SPACES TO MSG-COD-MENS.
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.
           MOVE 0 TO FRM-IFLD.
       LUP-PUT-TAB-ASC.
           ADD 1              TO QUE-NITM.
           MOVE QVE-NOM-TSQVE TO QUE-COLA.
           MOVE QVE-LEN-TSQVE TO QUE-LITM.
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD = 0
                   MOVE 'QVETABNONXT' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-ASC
               ELSE
                   GO TO FIN-PUT-TAB-ASC.
           ADD 1 TO FRM-IFLD.
           MOVE QUE-ITEM TO FRM-NUM-IVEH-IDX(FRM-IFLD).
           IF FRM-IFLD < WSS-IMAX
               GO TO LUP-PUT-TAB-ASC.
       FIN-PUT-TAB-ASC.
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX
               ADD 1 FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1
                                 UNTIL WSS-I > WSS-IMAX.
           IF FRM-IFLD > 0
               ADD 1 TO SCR-NUM-PANT.
           IF FRM-IFLD = WSS-IMAX
               ADD 1              TO QUE-NITM
               MOVE QVE-NOM-TSQVE TO QUE-COLA
               MOVE QVE-LEN-TSQVE TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT = QUE-STAT-OKS
                   MOVE 'SGC'        TO MSG-COD-SIST
                   MOVE 'QVETABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-ASC.
           EXIT.

       PUT-TAB-DSC SECTION.
       INI-PUT-TAB-DSC.
           MOVE SPACES TO MSG-COD-MENS.
           IF SCR-NUM-PANT = 1
               MOVE 'QVETABNOPRV' TO MSG-COD-MENS
               GO TO EXT-PUT-TAB-DSC.

           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.
           ADD 1 TO QUE-NITM.
           ADD 1 WSS-IMAX GIVING FRM-IFLD.
       LUP-PUT-TAB-DSC.
           SUBTRACT  1 FROM QUE-NITM.
           MOVE QVE-NOM-TSQVE TO QUE-COLA
           MOVE QVE-LEN-TSQVE TO QUE-LITM
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD > WSS-IMAX
                   MOVE 'QVETABNOPRV' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-DSC
               ELSE
                   GO TO FIN-PUT-TAB-DSC.
           SUBTRACT 1 FROM FRM-IFLD.
           MOVE QUE-ITEM TO FRM-NUM-IVEH-IDX(FRM-IFLD).
           IF FRM-IFLD > 1
               GO TO LUP-PUT-TAB-DSC.
       FIN-PUT-TAB-DSC.

           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1
                                 UNTIL WSS-I < 1.

           IF FRM-IFLD NOT > WSS-IMAX
               SUBTRACT 1 FROM SCR-NUM-PANT
               MOVE 'SGC'        TO MSG-COD-SIST
               MOVE 'QVETABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-DSC.
           EXIT.

       COPY GNSBGCPT.
       COPY GNSBGSTR.

       CLR-FRM SECTION.
       INI-CLR-FRM.
           MOVE ZEROES TO FRM-NUM-IVEH IN QVE-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-MRCV IN QVE-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-MODV IN QVE-FLD(WSS-I).
           MOVE ZEROES TO FRM-NUM-AVEH IN QVE-FLD(WSS-I).
           MOVE ZEROES TO FRM-VAL-DPRM IN QVE-FLD(WSS-I).
           MOVE ZEROES TO FRM-VAL-SDEU IN QVE-FLD(WSS-I).
       FIN-CLR-FRM.
           EXIT.

       CLR-WSS SECTION.
       INI-CLR-WSS.
           MOVE ZEROES TO WSS-NUM-IVEH-QVE IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-MRCV-QVE IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-MODV-QVE IN WSS-VARI.
           MOVE ZEROES TO WSS-NUM-AVEH-QVE IN WSS-VARI.
           MOVE ZEROES TO WSS-VAL-DPRM-QVE IN WSS-VARI.
           MOVE ZEROES TO WSS-VAL-SDEU-QVE IN WSS-VARI.
           IF SCR-OFRM-SRV
               PERFORM CLR-FRM-MTX.
       FIN-CLR-WSS.
           EXIT.

       CLR-FRM-MTX SECTION.
       INI-CLR-FRM-MTX.
           MOVE ZEROES TO FRM-NUM-IVEH IN QVE-FLD(1). 
           MOVE SPACES TO FRM-GLS-MRCV IN QVE-FLD(1). 
           MOVE SPACES TO FRM-GLS-MODV IN QVE-FLD(1). 
           MOVE ZEROES TO FRM-NUM-AVEH IN QVE-FLD(1).
           MOVE ZEROES TO FRM-VAL-DPRM IN QVE-FLD(1).
           MOVE ZEROES TO FRM-VAL-SDEU IN QVE-FLD(1).
       FIN-CLR-FRM-MTX.
           EXIT.

       CRG-VEC-QVE SECTION.
       INI-CRG-VEC-QVE.
           ADD 1 TO WSS-J.
           IF WSS-J IN WSS-VARI > WSS-JMAX IN WSS-VARI
               GO TO FIN-CRG-VEC-QVE.

           PERFORM CLR-QVE-VEC.
           
           MOVE VEH-NUM-IVEH IN VEH TO
                             WSS-NUM-IVEH-V IN WSS-VARI(WSS-J).
     ****  MOVE VEH-COD-MRCV IN VEH TO
     ****                    WSS-COD-MRCV-V IN WSS-VARI(WSS-J).
           MOVE 'BGL'                   TO TAB-COD-SIST.
           MOVE 'MRC'                   TO TAB-COD-TTAB IN TAB.
     ****  MOVE  WSS-COD-MRCV-V IN WSS-VARI(WSS-J)
           MOVE  VEH-COD-MRCV   IN VEH  TO TAB-COD-CTAB IN TAB.
           PERFORM BUS-TAB.
           IF MSG-COD-MENS IN MSG NOT > SPACES
              MOVE TAB-GLS-DESC IN TAB TO
                             WSS-GLS-MRCV-V IN WSS-VARI(WSS-J)
           ELSE
              MOVE SPACES TO WSS-GLS-MRCV-V IN WSS-VARI(WSS-J).

           MOVE VEH-COD-MRCV IN VEH     TO WSS-VEH-MARC IN WSS-VARI.
           MOVE VEH-COD-MODV IN VEH     TO WSS-VEH-MODE IN WSS-VARI.           
           MOVE 'BGL'                   TO TAB-COD-SIST.
           MOVE 'MOD'                   TO TAB-COD-TTAB IN TAB. 
           MOVE  WSS-MARCA-MODELO       TO TAB-COD-CTAB IN TAB.
           PERFORM BUS-TAB.
           IF MSG-COD-MENS IN MSG NOT > SPACES
              MOVE TAB-GLS-DESC IN TAB TO
                             WSS-GLS-MODV-V IN WSS-VARI(WSS-J)
           ELSE
              MOVE SPACES TO WSS-GLS-MODV-V IN WSS-VARI(WSS-J).


           MOVE VEH-NUM-AVEH IN VEH TO
                             WSS-NUM-AVEH-V IN WSS-VARI(WSS-J).
           IF SCR-OFRM-SRV
              MOVE VEH-VAL-DPRM IN VEH TO FRM-VAL-DPRM-EDT
              MOVE FRM-VAL-DPRM-EDT TO
                             WSS-VAL-DPRM-V IN WSS-VARI(WSS-J)
              MOVE VEH-VAL-SDEU IN VEH TO FRM-VAL-SDEU-EDT
              MOVE FRM-VAL-SDEU-EDT TO
                             WSS-VAL-SDEU-V IN WSS-VARI(WSS-J)  
	   ELSE
              MOVE VEH-VAL-DPRM IN VEH TO
                               WSS-VAL-DPRM-V IN WSS-VARI(WSS-J)
              MOVE VEH-VAL-SDEU IN VEH TO
                               WSS-VAL-SDEU-V IN WSS-VARI(WSS-J).           
 
       FIN-CRG-VEC-QVE.
           EXIT.

       CLR-QVE-VEC SECTION.
       INI-CLR-QVE-VEC.
           MOVE ZEROES TO WSS-NUM-IVEH-V(WSS-J). 
           MOVE SPACES TO WSS-GLS-MRCV-V(WSS-J). 
           MOVE SPACES TO WSS-GLS-MODV-V(WSS-J). 
           MOVE ZEROES TO WSS-NUM-AVEH-V(WSS-J).
           MOVE ZEROES TO WSS-VAL-DPRM-V(WSS-J).
           MOVE ZEROES TO WSS-VAL-SDEU-V(WSS-J).
       FIN-CLR-QVE-VEC.
           EXIT.

       PRO-FLD-QUE SECTION.
       INI-PRO-FLD-QUE.
           MOVE 'P' TO WSS-NUM-IVEH-ATR. 
           MOVE 'P' TO WSS-GLS-MRCV-ATR. 
           MOVE 'P' TO WSS-GLS-MODV-ATR. 
           MOVE 'P' TO WSS-NUM-AVEH-ATR.
           MOVE 'P' TO WSS-VAL-DPRM-ATR.
           MOVE 'P' TO WSS-VAL-SDEU-ATR. 
       FIN-PRO-FLD-QUE.
           EXIT.
      *>>>> EOF
*% END
