*%! Codigo adicional
*% IF INI_FST_FLD_QPA
    *<<<<< INI_FST_FLD_QPA
           MOVE -1 TO FRM-MES-FCTR-LEN IN QPA-FLD(1).                   
    *>>>>>
*% END
*% IF FIN_FST_KEY_QPA
      *<<<< FIN_FST_KEY_QPA                                             
           MOVE -1 TO DBC-NUM-ICLI-LEN IN QPA-FLD.                      
      *>>>>                                                             
*% END 
*% IF DBC_VRF_ICLI
      *<<<< DBC_VRF_ICLI                                                
           IF MSG-COD-MENS > SPACES                                     
              GO TO FIN-VAL-DBC-VRF-ICLI.                               
           IF DBC-NUM-ICLI IN QPA-FLD > ZEROES                          
               MOVE DBC-NUM-ICLI IN QPA-FLD TO DBC-NUM-ICLI IN DBC      
               MOVE DBC-IND-ICLI IN QPA-FLD TO DBC-IND-ICLI IN DBC      
               MOVE DBC-GLS-ICLI IN QPA-FLD TO DBC-GLS-ICLI IN DBC      
               MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY                         
               MOVE FIO-GET-KEY TO FIO-CMND                             
               PERFORM SGC-FIO-DBC                                      
               IF NOT FIO-STAT-OKS                                      
                   MOVE 'SGC'        TO MSG-COD-SIST                    
                   MOVE 'CLI    NEX' TO MSG-COD-MENS                    
                   PERFORM GET-MSG                                      
                   MOVE SPACES       TO FRM-GLS-NOMC IN QPA-FLD         
                   MOVE MSG-GLS-DESC TO FRM-MENS                        
                   MOVE FRM-SUAR-MAL TO FRM-SUAR                        
                   GO TO FIN-VAL-DBC-VRF-ICLI                           
               ELSE                                                     
                   MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN                 
                   PERFORM CPT-BLKS                                     
                   MOVE CPT-STRN            TO FRM-GLS-NOMC IN QPA-FLD  
                   MOVE DBC-CAI-ICLI IN DBC TO SCR-CAI-ICLI             
                   MOVE DBC-IIC-ICLI IN DBC TO SCR-IIC-ICLI             
                   PERFORM OBT-RPR-CLI                                  
                   IF MSG-COD-MENS > SPACES                             
                      GO TO FIN-VAL-DBC-VRF-ICLI.                       
      *>>>>                                                             
*% END 
*% IF PAE_CAI_IFOL
      *<<<< PAE_CAI_IFOL                                                
           IF NOT PAE-CAI-IFOL IN QPA-FLD > SPACES                      
               GO TO FIN-VAL-PAE-CAI-IFOL.                              
                                                                        
           MOVE PAE-CAI-IFOL IN QPA-FLD TO RPR-CAI-IFOL IN RPR.         
           MOVE PAE-IIC-IFOL IN QPA-FLD TO RPR-IIC-IFOL IN RPR.         
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM SGC-FIO-RPR.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE 'SGC'        TO MSG-COD-SIST                        
               MOVE 'RPR    NEX' TO MSG-COD-MENS                        
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE FRM-SUAR-MAL TO FRM-SUAR                            
               GO TO FIN-VAL-PAE-CAI-IFOL.                              
                                                                        
           MOVE RPR-CIC-ICLI IN RPR TO DBC-CAI-ICLI IN DBC.             
           MOVE RPR-IIC-ICLI IN RPR TO DBC-IIC-ICLI IN DBC.             
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM SGC-FIO-DBC.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE 'SGC'        TO MSG-COD-SIST                        
               MOVE 'CLI    NEX' TO MSG-COD-MENS                        
               PERFORM GET-MSG                                          
               MOVE SPACES       TO FRM-GLS-NOMC IN QPA-FLD             
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE FRM-SUAR-MAL TO FRM-SUAR                            
               GO TO FIN-VAL-PAE-CAI-IFOL                               
           ELSE                                                         
               MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN                     
               PERFORM CPT-BLKS                                         
               MOVE CPT-STRN            TO FRM-GLS-NOMC IN QPA-FLD      
               MOVE DBC-CAI-ICLI IN DBC TO SCR-CAI-ICLI                 
               MOVE DBC-IIC-ICLI IN DBC TO SCR-IIC-ICLI                 
               MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN QPA-FLD      
               MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN QPA-FLD      
               MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN QPA-FLD      
               MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN QPA-FLD.     
      *>>>>                                                             
*% END
*% IF EOF
   *<<<<<<EOF
       OBT-RPR-CLI SECTION.                                             
       INI-OBT-RPR-CLI.                                                 
           MOVE DBC-CIC-ICLI IN DBC TO RPR-CIC-ICLI IN RPR.             
           MOVE '99999999'          TO RPR-FEC-FRPR IN RPR.             
           MOVE 'RPR-AKY-IRPR' TO FIO-AKEY.                             
           MOVE FIO-GET-LEQ TO FIO-CMND.                                
           PERFORM SGC-FIO-RPR.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
               RPR-CIC-ICLI IN RPR = DBC-CIC-ICLI IN DBC)               
               MOVE 'SGC'        TO MSG-COD-SIST                        
               MOVE 'RPR    NEX' TO MSG-COD-MENS                        
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE FRM-SUAR-MAL TO FRM-SUAR                            
               GO TO FIN-OBT-RPR-CLI.                                   
                                                                        
           MOVE RPR-CAI-IFOL IN RPR TO PAE-CAI-IFOL IN QPA-FLD.         
           MOVE RPR-IIC-IFOL IN RPR TO PAE-IIC-IFOL IN QPA-FLD.         
       FIN-OBT-RPR-CLI.                                                 
           EXIT.                                                        
   *>>>>>>
*% END
