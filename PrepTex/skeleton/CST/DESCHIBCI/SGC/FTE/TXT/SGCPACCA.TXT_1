*%! Codigo adicional
*% NO_TIENE_TIMESTAMP = TRUE
*% NO_HDR_CST = TRUE
*% LARGO_TOTAL_COMMAREA = 3000
*% FMT_CHI = TRUE
*% GEN_BCI = TRUE
*% PGM_PTC = TRUE
*% IF WSS
      *<<<< WSS
       COPY GNSWGCPT.
       COPY GNSWGSTR.
       COPY SGCBRDBC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
       COPY SGCWXCCA.
*% IF NOT PGM_MCP AND NOT PGM_PTC
       COPY GNSWGQUE.
*% END
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
       01 WSS-NUM-CCUR.
           03 WSS-NUM-ICUR-EDT  PIC  ZZ9.
           03 WSS-NUM-ICUR-ALF       PIC X(03).
           03 WSS-NUM-ICUR      REDEFINES
              WSS-NUM-ICUR-ALF       PIC 9(03).
      *>>>>
*% END
*% IF FIN_SCR_VARI
           03 SCR-NUM-PANT               PIC 9(0005).
           03 SCR-CCA-FLD                PIC X(1100).
           03 SCR-CIC-ICLI.
              05 SCR-CAI-ICLI            PIC X(04).
              05 SCR-IIC-ICLI            PIC X(08).
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-OFRM-SRV
                MOVE TSK-TERM-ALF TO CCA-COD-TERM
                MOVE TSK-TERM-ALF TO CCA-COD-TERM-MAS
           ELSE
                MOVE EIBTRMID     TO CCA-COD-TERM
                MOVE EIBTRMID     TO CCA-COD-TERM-MAS.
           MOVE +3859 TO SCR-TCMA.
           PERFORM GNS-ERR-QUE.
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )
               MOVE SCR-CCA-FLD TO CCA-FLD
               GO TO FIN-INI.

           IF ( FRM-FFLD = FRM-FFLD-PF6 )
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM
               ADD 1              TO QUE-NITM
               MOVE CCA-NOM-TSCCA TO QUE-COLA
               MOVE CCA-LEN-TSCCA TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT NOT = QUE-STAT-OKS
                  MOVE SCR-CCA-FLD TO CCA-FLD
                  GO TO FIN-INI.

           PERFORM GET-FHOY.

      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
           MOVE CAC-COD-CPTO IN CCA-FLD TO CAC-COD-CPTO IN CAC.       
           MOVE SCR-CAI-ICLI            TO CAC-CAI-ICLI IN CAC.
           MOVE SCR-IIC-ICLI            TO CAC-IIC-ICLI IN CAC.
           MOVE CAC-NUM-SINI IN CCA-FLD TO CAC-NUM-SINI IN CAC.      
           MOVE CAC-NUM-AINI IN CCA-FLD TO CAC-NUM-AINI IN CAC.     
           MOVE CAC-NUM-MINI IN CCA-FLD TO CAC-NUM-MINI IN CAC.       
           MOVE CAC-NUM-DINI IN CCA-FLD TO CAC-NUM-DINI IN CAC.      

           MOVE ZEROES TO WSS-J IN WSS-VARI.
           MOVE FIO-GET-NLS TO FIO-CMND.
       LUP-LEE-CAC.
           PERFORM SGC-FIO-CAC.
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF
               GO TO CON-FND-KEY.

           IF CAC-COD-CPTO IN CCA-FLD > SPACES AND
              CAC-COD-CPTO IN CCA-FLD NOT = CAC-COD-CPTO IN CAC
               GO TO NXT-LEE-CAC.

           IF DBC-NUM-ICLI IN CCA-FLD > ZEROES AND
             (SCR-CAI-ICLI  NOT = CAC-CAI-ICLI IN CAC OR
              SCR-IIC-ICLI  NOT = CAC-IIC-ICLI IN CAC)
               GO TO NXT-LEE-CAC.

           MOVE CAC-NUM-DINI IN CCA-FLD TO WSS-NUM-DINI IN WSS-VARI.
           MOVE CAC-NUM-MINI IN CCA-FLD TO WSS-NUM-MINI IN WSS-VARI.
           MOVE CAC-NUM-SINI IN CCA-FLD TO WSS-NUM-SINI IN WSS-VARI.
           MOVE CAC-NUM-AINI IN CCA-FLD TO WSS-NUM-AINI IN WSS-VARI.

           IF WSS-FEC-FINI IN WSS-VARI > CAC-FEC-FINI IN CAC
               GO TO NXT-LEE-CAC.

           IF CAC-IND-VIGE IN CCA-FLD > SPACES AND
              CAC-IND-VIGE IN CCA-FLD NOT = CAC-IND-VIGE IN CAC
               GO TO NXT-LEE-CAC.

           PERFORM CRG-VEC-CCA.

           IF (SCR-OFRM-SRV AND WSS-J > WSS-NITMMAX)
              GO TO CON-FND-KEY.

           IF ((NOT SCR-OFRM-SRV) AND WSS-J > WSS-JMAX)
                 GO TO CON-FND-KEY.

       NXT-LEE-CAC.
           MOVE FIO-GET-NXT TO FIO-CMND.
           GO TO LUP-LEE-CAC.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
       CON-FND-KEY.
           PERFORM PUT-TAB.

           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *----------------------------------------------------------
       MSG-FND-KEY.
           MOVE 'TAB' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM VAL-NUM-KEY-CCA
               PERFORM VAL-NUM-FLD-CCA
               PERFORM SCRL-TAB-CCA
               PERFORM SCR-TRL.
      *>>>>
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBBTAB.
       COPY GNSBVTAB.
       COPY GNSBVCOD.
       COPY GNSBPFEC.
       COPY SGCBFDBC.

       PUT-TAB SECTION.
       INI-PUT-TAB.
           MOVE CCA-NOM-TSCCA     TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE CCA-NOM-TSCCA-MAS TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE 1 TO SCR-NUM-PANT.
           MOVE LOW-VALUES TO WSS-GLS-NCCA-IDX-MTI.
           MOVE LOW-VALUES TO WSS-GLS-NCCA-IDX-FRM.
      *
           MOVE 1 TO FRM-IFLD.
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.

           MOVE 1 TO WSS-F IN WSS-VARI.
       LUP-PUT-TAB.
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI
              GO TO FIN-PUT-TAB.
           IF WSS-COD-CPTO-V(WSS-F) > SPACES
              PERFORM PUT-TAB-CCA
              PERFORM PUT-CCA-QUE
              PERFORM SET-FRM-IFLD
           ELSE
           IF FRM-IFLD = 1 AND WSS-F = 1
              MOVE 'TAB' TO MSG-COD-SIST
              MOVE 'CCA    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE '00' TO FIO-STAT
      *       MOVE 'S' TO SCR-IERR
              GO TO FIN-PUT-TAB
           ELSE
              MOVE '00' TO FIO-STAT
              GO TO FIN-PUT-TAB.

           ADD 1 TO WSS-F IN WSS-VARI.
           GO TO LUP-PUT-TAB.
       FIN-PUT-TAB.
           MOVE 1                  TO QUE-NITM.
           MOVE CCA-LEN-TSCCA-MAS  TO QUE-LITM.
           MOVE CCA-NOM-TSCCA-MAS  TO QUE-COLA.

           IF WSS-NITM > WSS-NITMMAX
              MOVE 'S'           TO CCA-MAS-IND-HMAS
           ELSE
              MOVE 'N'           TO CCA-MAS-IND-HMAS.

           MOVE WSS-NITM         TO WSS-NUM-ICUR.
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.
           MOVE WSS-NUM-ICUR-ALF TO CCA-MAS-NUM-TOCC.
           MOVE CCA-MAS-ITEM     TO QUE-ITEM.
           MOVE QUE-PUT          TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF WSS-NITM > WSS-IMAX
               MOVE 'TAB' TO MSG-COD-SIST
               MOVE 'CCATABDMAS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       EXT-PUT-TAB.
           EXIT.

       SET-FRM-IFLD SECTION.
       INI-SET-FRM-IFLD.
           IF SCR-OFRM-SRV
                MOVE 1 TO FRM-IFLD
           ELSE
                ADD 1 TO FRM-IFLD.
       FIN-SET-FRM-IFLD.
           EXIT.
      *--------------------------------------------
       SCRL-TAB-CCA SECTION.
       INI-SCRL-TAB-CCA.
           MOVE SPACES TO MSG-COD-MENS.

           IF FRM-FFLD = FRM-FFLD-PF6
               PERFORM PUT-TAB-ASC
           ELSE
               PERFORM PUT-TAB-DSC.
           IF MSG-COD-MENS > SPACES
               MOVE 'TAB' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-CCA.
           EXIT.

      *--------------------------------------------
       PUT-TAB-CCA SECTION.
       INI-PUT-TAB-CCA.
           PERFORM CLR-WSS.
           MOVE WSS-COD-CPTO-V(WSS-F) TO WSS-COD-CPTO-CCA IN WSS-VARI.
           MOVE WSS-GLS-CONC-V(WSS-F) TO WSS-GLS-CONC-CCA IN WSS-VARI.
           MOVE WSS-FEC-FINI-V(WSS-F) TO WSS-FEC-FINI-CCA IN WSS-VARI.
           MOVE WSS-FEC-FTER-V(WSS-F) TO WSS-FEC-FTER-CCA IN WSS-VARI.
           MOVE WSS-GLS-VIGE-V(WSS-F) TO WSS-GLS-VIGE-CCA IN WSS-VARI.
           MOVE WSS-NUM-ICLI-V(WSS-F) TO WSS-NUM-ICLI-CCA IN WSS-VARI.
           MOVE WSS-VRF-ICLI-V(WSS-F) TO WSS-VRF-ICLI-CCA IN WSS-VARI.
           MOVE WSS-IND-ICLI-V(WSS-F) TO WSS-IND-ICLI-CCA IN WSS-VARI.
           MOVE WSS-GLS-ICLI-V(WSS-F) TO WSS-GLS-ICLI-CCA IN WSS-VARI.
           MOVE WSS-GLS-NOMB-V(WSS-F) TO WSS-GLS-NOMB-CCA IN WSS-VARI.

           MOVE WSS-COD-CPTO-CCA IN WSS-VARI TO WSS-COD-CPTO-FRM.
           MOVE WSS-GLS-CONC-CCA IN WSS-VARI TO WSS-GLS-CONC-FRM.
           MOVE WSS-FEC-FINI-CCA IN WSS-VARI TO WSS-FEC-FINI-FRM.
           MOVE WSS-FEC-FTER-CCA IN WSS-VARI TO WSS-FEC-FTER-FRM.
           MOVE WSS-GLS-VIGE-CCA IN WSS-VARI TO WSS-GLS-VIGE-FRM.
           MOVE WSS-NUM-ICLI-CCA IN WSS-VARI TO WSS-NUM-ICLI-FRM.
           MOVE WSS-VRF-ICLI-CCA IN WSS-VARI TO WSS-VRF-ICLI-FRM.
           MOVE WSS-IND-ICLI-CCA IN WSS-VARI TO WSS-IND-ICLI-FRM.
           MOVE WSS-GLS-ICLI-CCA IN WSS-VARI TO WSS-GLS-ICLI-FRM.
           MOVE WSS-GLS-NOMB-CCA IN WSS-VARI TO WSS-GLS-NOMB-FRM.

           IF FRM-IFLD > WSS-IMAX
              GO TO FIN-PUT-TAB-CCA.

           MOVE WSS-COD-CPTO-CCA IN WSS-VARI TO
               FRM-COD-CPTO IN CCA-FLD(FRM-IFLD).
           MOVE WSS-GLS-CONC-CCA IN WSS-VARI TO
               FRM-GLS-CONC IN CCA-FLD(FRM-IFLD).
           MOVE WSS-FEC-FINI-CCA IN WSS-VARI TO
               FRM-FEC-FINI IN CCA-FLD(FRM-IFLD).
           MOVE WSS-FEC-FTER-CCA IN WSS-VARI TO
               FRM-FEC-FTER IN CCA-FLD(FRM-IFLD).
           MOVE WSS-GLS-VIGE-CCA IN WSS-VARI TO
               FRM-GLS-VIGE IN CCA-FLD(FRM-IFLD).
           MOVE WSS-NUM-ICLI-CCA IN WSS-VARI TO
               FRM-NUM-ICLI IN CCA-FLD(FRM-IFLD).
           MOVE WSS-VRF-ICLI-CCA IN WSS-VARI TO
               FRM-VRF-ICLI IN CCA-FLD(FRM-IFLD).
           MOVE WSS-IND-ICLI-CCA IN WSS-VARI TO
               FRM-IND-ICLI IN CCA-FLD(FRM-IFLD).
           MOVE WSS-GLS-ICLI-CCA IN WSS-VARI TO
               FRM-GLS-ICLI IN CCA-FLD(FRM-IFLD).
           MOVE WSS-GLS-NOMB-CCA IN WSS-VARI TO
               FRM-GLS-NOMB IN CCA-FLD(FRM-IFLD).
       FIN-PUT-TAB-CCA.
           EXIT.
      *----------------------------------------------------
       PUT-CCA-QUE SECTION.
       INI-PUT-CCA-QUE.
           PERFORM PRO-FLD-QUE.
           ADD 1 TO WSS-NITM.
           MOVE CCA-NOM-TSCCA    TO QUE-COLA.
           MOVE CCA-LEN-TSCCA    TO QUE-LITM.
           MOVE WSS-NITM         TO QUE-NITM.
           MOVE SPACES           TO QUE-ITEM.
           IF SCR-OFRM-SRV
              MOVE WSS-GLS-NCCA-IDX-MTI  TO QUE-ITEM
           ELSE
              MOVE WSS-GLS-NCCA-IDX-FRM  TO QUE-ITEM.
           MOVE QUE-PUT  TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
       FIN-PUT-CCA-QUE.
           EXIT.
      *----------------------------------------------------

       PUT-TAB-ASC SECTION.
       INI-PUT-TAB-ASC.
           MOVE SPACES TO MSG-COD-MENS.
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.
           MOVE 0 TO FRM-IFLD.
       LUP-PUT-TAB-ASC.
           ADD 1              TO QUE-NITM.
           MOVE CCA-NOM-TSCCA TO QUE-COLA.
           MOVE CCA-LEN-TSCCA TO QUE-LITM.
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD = 0
                   MOVE 'CCATABNONXT' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-ASC
               ELSE
                   GO TO FIN-PUT-TAB-ASC.
           ADD 1 TO FRM-IFLD.
           MOVE QUE-ITEM TO FRM-COD-CPTO-IDX(FRM-IFLD).
           IF FRM-IFLD < WSS-IMAX
               GO TO LUP-PUT-TAB-ASC.
       FIN-PUT-TAB-ASC.
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX
               ADD 1 FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1
                                 UNTIL WSS-I > WSS-IMAX.
           IF FRM-IFLD > 0
               ADD 1 TO SCR-NUM-PANT.
           IF FRM-IFLD = WSS-IMAX
               ADD 1              TO QUE-NITM
               MOVE CCA-NOM-TSCCA TO QUE-COLA
               MOVE CCA-LEN-TSCCA TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT = QUE-STAT-OKS
                   MOVE 'DEU'        TO MSG-COD-SIST
                   MOVE 'CCATABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-ASC.
           EXIT.

       PUT-TAB-DSC SECTION.
       INI-PUT-TAB-DSC.
           MOVE SPACES TO MSG-COD-MENS.
           IF SCR-NUM-PANT = 1
               MOVE 'CCATABNOPRV' TO MSG-COD-MENS
               GO TO EXT-PUT-TAB-DSC.

           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.
           ADD 1 TO QUE-NITM.
           ADD 1 WSS-IMAX GIVING FRM-IFLD.
       LUP-PUT-TAB-DSC.
           SUBTRACT  1 FROM QUE-NITM.
           MOVE CCA-NOM-TSCCA TO QUE-COLA
           MOVE CCA-LEN-TSCCA TO QUE-LITM
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD > WSS-IMAX
                   MOVE 'CCATABNOPRV' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-DSC
               ELSE
                   GO TO FIN-PUT-TAB-DSC.
           SUBTRACT 1 FROM FRM-IFLD.
           MOVE QUE-ITEM TO FRM-COD-CPTO-IDX(FRM-IFLD).
           IF FRM-IFLD > 1
               GO TO LUP-PUT-TAB-DSC.
       FIN-PUT-TAB-DSC.

           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1
                                 UNTIL WSS-I < 1.

           IF FRM-IFLD NOT > WSS-IMAX
               SUBTRACT 1 FROM SCR-NUM-PANT
               MOVE 'DEU'        TO MSG-COD-SIST
               MOVE 'CCATABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-DSC.
           EXIT.

       COPY GNSBGCPT.
       COPY GNSBGSTR.

       CLR-FRM SECTION.
       INI-CLR-FRM.
           MOVE SPACES TO FRM-COD-CPTO IN CCA-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-CONC IN CCA-FLD(WSS-I).
           MOVE SPACES TO FRM-FEC-FINI IN CCA-FLD(WSS-I).
           MOVE SPACES TO FRM-FEC-FTER IN CCA-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-VIGE IN CCA-FLD(WSS-I).
           MOVE ZEROES TO FRM-NUM-ICLI IN CCA-FLD(WSS-I).
           MOVE SPACES TO FRM-VRF-ICLI IN CCA-FLD(WSS-I).
           MOVE SPACES TO FRM-IND-ICLI IN CCA-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-ICLI IN CCA-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-NOMB IN CCA-FLD(WSS-I).
       FIN-CLR-FRM.
           EXIT.

       CLR-WSS SECTION.
       INI-CLR-WSS.
           MOVE SPACES TO WSS-COD-CPTO-CCA IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-CONC-CCA IN WSS-VARI.
           MOVE SPACES TO WSS-FEC-FINI-CCA IN WSS-VARI.
           MOVE SPACES TO WSS-FEC-FTER-CCA IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-VIGE-CCA IN WSS-VARI.
           MOVE ZEROES TO WSS-NUM-ICLI-CCA IN WSS-VARI.
           MOVE SPACES TO WSS-VRF-ICLI-CCA IN WSS-VARI.
           MOVE SPACES TO WSS-IND-ICLI-CCA IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-ICLI-CCA IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-NOMB-CCA IN WSS-VARI.

           IF SCR-OFRM-SRV
               PERFORM CLR-FRM-MTX.
       FIN-CLR-WSS.
           EXIT.

       CLR-FRM-MTX SECTION.
       INI-CLR-FRM-MTX.
           MOVE SPACES TO FRM-COD-CPTO IN CCA-FLD(1).
           MOVE SPACES TO FRM-GLS-CONC IN CCA-FLD(1).
           MOVE SPACES TO FRM-FEC-FINI IN CCA-FLD(1).
           MOVE SPACES TO FRM-FEC-FTER IN CCA-FLD(1).
           MOVE SPACES TO FRM-GLS-VIGE IN CCA-FLD(1).
           MOVE ZEROES TO FRM-NUM-ICLI IN CCA-FLD(1).
           MOVE SPACES TO FRM-VRF-ICLI IN CCA-FLD(1).
           MOVE SPACES TO FRM-IND-ICLI IN CCA-FLD(1).
           MOVE SPACES TO FRM-GLS-ICLI IN CCA-FLD(1).
           MOVE SPACES TO FRM-GLS-NOMB IN CCA-FLD(1).
       FIN-CLR-FRM-MTX.
           EXIT.

       CRG-VEC-CCA SECTION.
       INI-CRG-VEC-CCA.
           ADD 1 TO WSS-J.
           IF WSS-J IN WSS-VARI > WSS-JMAX IN WSS-VARI
               GO TO FIN-CRG-VEC-CCA.

           PERFORM CLR-CCA-VEC.

           MOVE CAC-COD-CPTO IN CAC TO
                             WSS-COD-CPTO-V IN WSS-VARI(WSS-J).
           MOVE 'TAB'                   TO TAB-COD-SIST.
           MOVE 'PSC'                   TO TAB-COD-TTAB IN TAB.
           MOVE WSS-COD-CPTO-V IN WSS-VARI(WSS-J)
                                        TO TAB-COD-CTAB IN TAB.
           PERFORM BUS-TAB.
           IF MSG-COD-MENS IN MSG NOT > SPACES
              MOVE TAB-GLS-DESC IN TAB TO
                             WSS-GLS-CONC-V IN WSS-VARI(WSS-J)
           ELSE
              MOVE SPACES TO WSS-GLS-CONC-V IN WSS-VARI(WSS-J).

           MOVE CAC-NUM-SINI IN CAC TO WSS-NUM-STRA IN WSS-VARI.
           MOVE CAC-NUM-AINI IN CAC TO WSS-NUM-ATRA IN WSS-VARI.
           MOVE CAC-NUM-MINI IN CAC TO WSS-NUM-MTRA IN WSS-VARI.
           MOVE CAC-NUM-DINI IN CAC TO WSS-NUM-DTRA IN WSS-VARI.
           MOVE WSS-FEC-FTRA IN WSS-VARI TO
                             WSS-FEC-FINI-V IN WSS-VARI(WSS-J).

           MOVE CAC-NUM-STER IN CAC TO WSS-NUM-STRA IN WSS-VARI.
           MOVE CAC-NUM-ATER IN CAC TO WSS-NUM-ATRA IN WSS-VARI.
           MOVE CAC-NUM-MTER IN CAC TO WSS-NUM-MTRA IN WSS-VARI.
           MOVE CAC-NUM-DTER IN CAC TO WSS-NUM-DTRA IN WSS-VARI.
           MOVE WSS-FEC-FTRA IN WSS-VARI TO
                             WSS-FEC-FTER-V IN WSS-VARI(WSS-J).

           MOVE CAC-IND-VIGE IN CAC TO
                             WSS-GLS-VIGE-V IN WSS-VARI(WSS-J).

           MOVE CAC-CAI-ICLI IN CAC TO DBC-CAI-ICLI IN DBC.
           MOVE CAC-IIC-ICLI IN CAC TO DBC-IIC-ICLI IN DBC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           IF NOT FIO-STAT-OKS
               MOVE 'SGC'        TO MSG-COD-SIST
               MOVE 'CLI    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO WSS-GLS-NOMB-V IN WSS-VARI(WSS-J)
               MOVE ZEROES       TO WSS-NUM-ICLI-V IN WSS-VARI(WSS-J)
               MOVE SPACES       TO WSS-VRF-ICLI-V IN WSS-VARI(WSS-J)
               MOVE SPACES       TO WSS-IND-ICLI-V IN WSS-VARI(WSS-J)
               MOVE SPACES       TO WSS-GLS-ICLI-V IN WSS-VARI(WSS-J)
           ELSE
               MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
               PERFORM CPT-BLKS
               MOVE CPT-STRN     TO WSS-GLS-NOMB-V IN WSS-VARI(WSS-J)
               MOVE DBC-NUM-ICLI IN DBC  
                                 TO WSS-NUM-ICLI-V IN WSS-VARI(WSS-J)
               MOVE DBC-VRF-ICLI IN DBC
                                 TO WSS-VRF-ICLI-V IN WSS-VARI(WSS-J)
               MOVE DBC-IND-ICLI IN DBC 
                                 TO WSS-IND-ICLI-V IN WSS-VARI(WSS-J)
               MOVE DBC-GLS-ICLI IN DBC 
                                 TO WSS-GLS-ICLI-V IN WSS-VARI(WSS-J).
       FIN-CRG-VEC-CCA.
           EXIT.

       CLR-CCA-VEC SECTION.
       INI-CLR-CCA-VEC.
           MOVE SPACES TO WSS-COD-CPTO-V(WSS-J).
           MOVE SPACES TO WSS-GLS-CONC-V(WSS-J).
