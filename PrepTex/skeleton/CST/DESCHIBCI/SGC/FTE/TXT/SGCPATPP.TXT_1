*%! Codigo adicional
*% PGM_PTC = TRUE
*% GNSTCA = 3000
*% LARGO_TOTAL_COMMAREA = 3000
*% IF WSS
      *<<<< WSS
       01  WSS-VARI.                                                    
           03 WSS-FEC-CURR.                                             
              05 WSS-FEC-DCUR    VALUE ZEROS   PIC 99.                  
              05 WSS-FEC-MCUR    VALUE ZEROS   PIC 99.                  
              05 WSS-FEC-SCUR    VALUE ZEROS   PIC 99.                  
              05 WSS-FEC-ACUR    VALUE ZEROS   PIC 99.                  
           03 WSS-IND-CALN       VALUE 'N'     PIC X(01).               
           03 WSS-DOC-TCL        VALUE SPACES.                          
              05 WSS-FILLER                    PIC X(02).               
              05 WSS-DTCL                      PIC X(01).               
           03 WSS-TIP-TICL.                                             
              05 WSS-TAB-TIP VALUE 'TICL'  PIC X(04).                   
              05 WSS-TAB-TICL VALUE ' '        PIC X(01).               
      *>>>> WSS                                                            
*% END
*%  IF FIN_SCR_VARI
      *<<<< FIN_SCR_VARI                                                
        COPY GNSBRMSC.                                                  
        COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSC-REQA==.       
      *>>>> FIN_SCR_VARI
*% END
*% IF FIN_FND_KEY 
     *<<<<<FIN_FND_KEY
           IF SCR-CMND = 'ING'                                          
              PERFORM GET-FHOY                                          
              MOVE HOY-SHOY TO WSS-FEC-SCUR IN WSS-VARI                 
              MOVE HOY-AHOY TO WSS-FEC-ACUR IN WSS-VARI                 
              MOVE HOY-MHOY TO WSS-FEC-MCUR IN WSS-VARI                 
              MOVE HOY-DHOY TO WSS-FEC-DCUR IN WSS-VARI                 
              MOVE WSS-FEC-CURR IN WSS-VARI                             
                               TO TPP-FEC-FEFR IN TPP-FLD               
              MOVE SCR-USER    TO TPP-COD-EEFR IN TPP-FLD.              
                                                                        
           IF (SCR-CMND = 'MOD' OR  SCR-CMND = 'ELI' OR                 
                   SCR-CMND = 'CON') AND                                
                                   (TPP-FEC-FEFR IN TPP-FLD = ZEROS)    
                   PERFORM GET-FHOY                                     
                   MOVE HOY-SHOY TO WSS-FEC-SCUR IN WSS-VARI            
                   MOVE HOY-AHOY TO WSS-FEC-ACUR IN WSS-VARI            
                   MOVE HOY-MHOY TO WSS-FEC-MCUR IN WSS-VARI            
                   MOVE HOY-DHOY TO WSS-FEC-DCUR IN WSS-VARI            
                   MOVE WSS-FEC-CURR IN WSS-VARI                        
                                 TO TPP-FEC-FEFR IN TPP-FLD             
                   IF TPP-COD-EEFR IN TPP-FLD = SPACES                  
                      MOVE SCR-USER TO TPP-COD-EEFR IN TPP-FLD          
                   ELSE                                                 
                      NEXT SENTENCE                                     
              ELSE                                                      
              IF (SCR-CMND = 'MOD' OR  SCR-CMND = 'ELI' OR              
                   SCR-CMND = 'CON') AND                                
                                   (TPP-COD-EEFR IN TPP-FLD = SPACES)   
                  MOVE SCR-USER TO TPP-COD-EEFR IN TPP-FLD.             
                                                                        
           IF SCR-CMND = 'ING'                                          
              MOVE 'S'    TO TPP-IND-CRPR IN TPP-FLD                    
              IF TPP-COD-TCLI IN TPP-FLD = 'P'                          
                 MOVE 'S' TO TPP-IND-CDPX IN TPP-FLD                    
              ELSE                                                      
                 MOVE 'N' TO TPP-IND-CDPX IN TPP-FLD.                   
                                                                        
            IF SCR-CMND = 'ING'                                         
               IF TPP-COD-TCLI IN TPP-FLD = 'E'                         
                  MOVE 'S' TO TPP-IND-CDEX IN TPP-FLD                   
               ELSE                                                     
                  MOVE 'N' TO TPP-IND-CDEX IN TPP-FLD.                  
      *>>>>
*% END
*% IF EOF
      *<<<< EOF                                                   
       COPY GNSBBTAB.                                                   
       COPY GNSBVTAB.                                                   
       COPY GNSBVCOD.                                                   
       COPY GNSBFMSC.                                                   
       COPY GNSBVIND.                                                   
       COPY GNSBBMSC.                                                   
       COPY GNSBVMSC.                                                   
      *>>>> EOF     
*% END
