*%! Codigo adicional para archivo
*% IF INI_VAL_CON_KEY_RCO
      *<<<< INI_VAL_CON_KEY_RCO
           IF RCO-CAI-IOPE IN RCO-FLD > SPACES AND
              RCO-IIC-IOPE IN RCO-FLD NOT > ZEROES
               MOVE 'SGC' TO MSG-COD-SIST
               MOVE 'RCOIOPENOCON' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CON-KEY-RCO.
      * CONDICION 1 ( SOLO ICLI (NOMBRE O IDC) )
           IF NOT ((( DBC-NUM-ICLI IN RCO-FLD > ZEROES OR
                      DBC-GLS-NOMC IN RCO-FLD > SPACES ) AND
                      RCO-IIC-IOPE IN RCO-FLD NOT > ZEROES ) OR
      * CONDICION 2 ( SOLO IOPE )
                    ( RCO-IIC-IOPE IN RCO-FLD > ZEROES AND
                      DBC-NUM-ICLI IN RCO-FLD NOT > ZEROES AND
                      DBC-GLS-NOMC IN RCO-FLD NOT > SPACES ) OR
      * CONDICION 3 ( ICLI (NOMBRE O IDC) + IOPE )
                   (( DBC-NUM-ICLI IN RCO-FLD > ZEROES OR
                      DBC-GLS-NOMC IN RCO-FLD > SPACES ) AND
                      RCO-IIC-IOPE IN RCO-FLD > ZEROES ))
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'VALCONKEY' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CON-KEY-RCO.
      *>>>>
*% END
*% IF INI_VAL_CON_IKY_RCO
      *<<<< INI_VAL_CON_IKY_RCO
           IF RCO-CAI-IOPE IN RCO-FLD > SPACES AND
              RCO-IIC-IOPE IN RCO-FLD NOT > ZEROES
               MOVE 'SGC' TO MSG-COD-SIST
               MOVE 'RCOIOPENOCON' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CON-IKY-RCO.
      * CONDICION 3 ( ICLI (NOMBRE O IDC) + IOPE )
           IF NOT  (( DBC-NUM-ICLI IN RCO-FLD > ZEROES OR
                      DBC-GLS-NOMC IN RCO-FLD > SPACES ) AND
                      RCO-IIC-IOPE IN RCO-FLD > ZEROES )
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'VALCONIKY' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CON-IKY-RCO.
      *>>>>
*% END
*% IF FIN_PUT_DBC_RCO
      *<<<< FIN_PUT_DBC_RCO
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.
           PERFORM CPT-BLKS.
           MOVE CPT-STRN TO DBC-GLS-NOMC IN RCO-FLD.
      *>>>>
*% END
*% IF OPE_MSC_TVOC
      *<<<< OPE_MSC_TVOC
           GO TO FIN-VAL-OPE-MSC-TVOC.
      *>>>>
*% END
*% IF OPE_COD_TOOC
      *<<<< OPE_COD_TOOC
           GO TO FIN-VAL-OPE-COD-TOOC.
      *>>>>
*% END
*% IF OPE_COD_OFOC
      *<<<< OPE_COD_OFOC
           GO TO FIN-VAL-OPE-COD-OFOC.
      *>>>>
*% END
*% IF NOT PGM_PER
*% IF DBC_IND_ICLI
      *<<<< DBC_IND_ICLI
           MOVE DBC-IND-ICLI IN RCO-FLD TO IDC-IND-RUTV.
           PERFORM VAL-IDC.
           MOVE IDC-IND-RUTV TO DBC-IND-ICLI IN RCO-FLD.
      *>>>>
*% END
*% IF DBC_GLS_ICLI
      *<<<< DBC_GLS_ICLI
           MOVE DBC-GLS-ICLI IN RCO-FLD TO IDC-GLS-RUTV.
           PERFORM VAL-GLS.
           MOVE IDC-GLS-RUTV TO DBC-GLS-ICLI IN RCO-FLD.
      *>>>>
*% END
*% IF DBC_VRF_ICLI
      *<<<< DBC_VRF_ICLI
           MOVE IDC-ZERO-S TO IDC-ZERO.
           MOVE DBC-NUM-ICLI IN RCO-FLD TO IDC-NUM-RUTV.
           MOVE DBC-VRF-ICLI IN RCO-FLD TO IDC-VRF-RUTV.
           PERFORM VAL-RUT.
      *>>>>
*% END
*% END
*% IF RCO_NUM_DIRE
      *<<<< RCO_NUM_DIRE
           IF RCO-NUM-DIRE IN RCO-FLD NOT > ZEROES
               MOVE RCO-CAI-ICLI IN RCO-FLD TO DBC-CAI-ICLI IN DBC
               MOVE RCO-IIC-ICLI IN RCO-FLD TO DBC-IIC-ICLI IN DBC
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM SGC-FIO-DBC
               IF FIO-STAT-OKS
                   MOVE DBC-NUM-DCOR IN DBC TO RCO-NUM-DIRE IN RCO-FLD.
           MOVE RCO-CAI-ICLI IN RCO-FLD TO RCD-CAI-ICLI IN RCD.
           MOVE RCO-IIC-ICLI IN RCO-FLD TO RCD-IIC-ICLI IN RCD.
           MOVE RCO-NUM-DIRE IN RCO-FLD TO RCD-NUM-DIRE IN RCD.
           MOVE FIO-GET-KEY  TO FIO-CMND.
           PERFORM SGC-FIO-RCD.
           IF NOT FIO-STAT-OKS
               MOVE 'SGC'          TO MSG-COD-SIST
               MOVE 'NUMDIRENEX  ' TO MSG-COD-MENS IN MSG
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>>
*% END
*% IF RCO_VAL_PRCO
      *<<<< RCO_VAL_PRCO
           IF RCO-VAL-PRCO IN RCO-FLD NOT > ZEROES
               MOVE 100 TO RCO-VAL-PRCO IN RCO-FLD.
      *DAD
           IF RCO-VAL-PRCO IN RCO-FLD > 100
               MOVE 'SGC'         TO MSG-COD-SIST
               MOVE 'VALPRCO100>' TO MSG-COD-MENS IN MSG
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC  TO FRM-MENS
               MOVE FRM-SUAR-MAL  TO FRM-SUAR.
      *>>>>
*% END
*% IF FIN_RET_OPE_RCO
      *<<<< FIN_RET_OPE_RCO
           MOVE OPE-COD-COPR IN RCO-FLD TO RCO-COD-COPR IN RCO.
      *>>>>
*% END
*% IF PGM_BU
*% IF INI_VAL_FLD_RCO
      *<<<<
           PERFORM VAL-OPE-MSC-TVOP.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPE-MSC-TVOP-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-OPE-COD-TOPR.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPE-COD-TOPR-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-OPE-COD-OFOP.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPE-COD-OFOP-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-FRM-GLS-TVOP.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-TVOP-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-FRM-GLS-TOPR.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-TOPR-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-FRM-GLS-OFOP.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-OFOP-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-RCO-NUM-DIRE.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RCO-NUM-DIRE-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-RCO-COD-STAT.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RCO-COD-STAT-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-OPE-IND-MCLI.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPE-IND-MCLI-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-FRM-GLS-STAT.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-STAT-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-FRM-GLS-MCLI.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-MCLI-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-RCO-COD-RCOP.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RCO-COD-RCOP-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-RCO-VAL-PRCO.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RCO-VAL-PRCO-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-FRM-GLS-RCOP.
          IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-RCOP-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-OPE-COD-COPR.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPE-COD-COPR-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           GO TO FIN-VAL-FLD-RCO.
      *>>>>
*% END
*% END
