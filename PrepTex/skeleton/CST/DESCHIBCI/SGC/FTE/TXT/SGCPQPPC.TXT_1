*%! Codigo adicional
*% NO_TIENE_TIMESTAMP = TRUE
*% NO_HDR_CST = TRUE
*% LARGO_TOTAL_COMMAREA = 3000
*% FMT_CHI = TRUE
*% GEN_BCI = TRUE
*% PGM_PTC = TRUE
*% IF FIN_SCR_VARI
      *<<<< FIN_SCR_VARI
      *>>>>
*% END
*% IF WSS
      *<<<< WSS
       COPY SGCBRDBC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
       COPY TABBROFI.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OFI-REQA==.
       COPY GNSWGCPT.

       01  WSS-VARI.
           03 WSS-COD-CAUX.
              05 WSS-COD-TOOC                           PIC X(03).
              05 WSS-COD-AOOC                           PIC X(03).
      *>>>>
*% END
*% IF INI_FIO
      *<<<< INI_FIO
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
           GO TO CON-FND-KEY.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
       CON-FND-KEY.
      *--------------------------------------------------------------*
           PERFORM LEE-COL-PPC.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
      *--------------------------------------------------------------*
       LEE-COL-PPC SECTION.
       INI-LEE-COL-PPC.
           PERFORM KEY-ALL-PPC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-PPC.
           IF FIO-STAT-OKS
              GO TO CNT-LEE-COL-PPC.

           PERFORM KEY-PPC-PPC.
           MOVE '*  '               TO PPC-COD-AOOC IN PPC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-PPC.
           IF FIO-STAT-OKS
              GO TO CNT-LEE-COL-PPC.

           PERFORM KEY-PPC-PPC.
           MOVE '*     '          TO PPC-COD-COOC IN PPC
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-PPC.
           IF FIO-STAT-OKS
              GO TO CNT-LEE-COL-PPC.

           MOVE '23' TO FIO-STAT
           GO TO EXT-LEE-COL-PPC.

       CNT-LEE-COL-PPC.
           MOVE PPC-COD-VCOC IN PPC TO PPC-COD-VCOC IN PPC-FLD.
           MOVE PPC-COD-TOOC IN PPC TO PPC-COD-TOOC IN PPC-FLD.
           MOVE PPC-COD-AOOC IN PPC TO PPC-COD-AOOC IN PPC-FLD.

       EXT-LEE-COL-PPC.
           MOVE FIO-STAT TO PGM-STAT-PPC.
       FIN-LEE-COL-PPC.
           EXIT.
      *--------------------------------------------------------------*

       COPY SGCBFDBC.
       COPY TABBFOFI.
       COPY TABBBOFI.
       COPY TABBVOFI.
       COPY GNSBVCOD.
       COPY GNSBVTAB.
       COPY GNSBBTAB.
       COPY GNSBEGLS.
       COPY GNSBGCPT.
      *>>>>
*% END
