           IF NOT ( FIO-STAT-OKS AND
                    ARG-KEY-ILDC IN ARG = LDC-KEY-ILDC IN LDC )
               MOVE 'ARG    NEX' TO CCU-COD-MENS
                                    MENS-LDC(LDC-NUM-ILDC IN LDC)
