       COPY GNSWGSYS.

      *<<<< WSS
       COPY GNSBRMNU.

       COPY GNSWGCPT.

       COPY GARBRGAP.
       COPY GARBRGDD.
       COPY GARBRGDO.
       COPY GARBRGDP.
       COPY GARBRGES.
       COPY GARBRGIN.
       COPY GARBRGLE.
       COPY GARBRGSE.
       COPY GARBRGSI.
       COPY GARBRGTP.
       COPY GARBRGUB.
       COPY GARBRGVT.
