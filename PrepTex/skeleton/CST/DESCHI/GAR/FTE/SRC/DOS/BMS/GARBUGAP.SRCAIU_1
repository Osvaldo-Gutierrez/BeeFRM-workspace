       VAL-FLD-GAP SECTION.
       INI-VAL-FLD-GAP.
           PERFORM VAL-FRM-GLS-TIP-REA-NRE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-TIP-REA-NRE-LEN IN GAP-FLD
                GO TO FIN-VAL-FLD-GAP.
           PERFORM VAL-GDG-TIP-OPE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-TIP-OPE-LEN IN GAP-FLD
                GO TO FIN-VAL-FLD-GAP.
           PERFORM VAL-GDG-STP-OPE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-STP-OPE-LEN IN GAP-FLD
                GO TO FIN-VAL-FLD-GAP.
           PERFORM VAL-FRM-GLS-TIP-OPE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-TIP-OPE-LEN IN GAP-FLD
                GO TO FIN-VAL-FLD-GAP.
           PERFORM VAL-FRM-GLS-STP-OPE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-STP-OPE-LEN IN GAP-FLD
                GO TO FIN-VAL-FLD-GAP.
           PERFORM VAL-GDG-COD-OFI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-COD-OFI-LEN IN GAP-FLD
                GO TO FIN-VAL-FLD-GAP.
           PERFORM VAL-FRM-GLS-OFI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-OFI-LEN IN GAP-FLD
                GO TO FIN-VAL-FLD-GAP.
           PERFORM VAL-GDG-VCB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-VCB-LEN IN GAP-FLD
                GO TO FIN-VAL-FLD-GAP.
           PERFORM VAL-FRM-GLS-VCB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-VCB-LEN IN GAP-FLD
                GO TO FIN-VAL-FLD-GAP.
           PERFORM VAL-GDG-DES-1.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-DES-1-LEN IN GAP-FLD
                GO TO FIN-VAL-FLD-GAP.
           PERFORM VAL-GDG-DES-2.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-DES-2-LEN IN GAP-FLD
                GO TO FIN-VAL-FLD-GAP.
           PERFORM VAL-DBC-IND-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DBC-IND-ICLI-LEN IN GAP-FLD
                GO TO FIN-VAL-FLD-GAP.
           PERFORM VAL-DBC-GLS-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DBC-GLS-ICLI-LEN IN GAP-FLD
                GO TO FIN-VAL-FLD-GAP.
           PERFORM VAL-DBC-NUM-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DBC-NUM-ICLI-LEN IN GAP-FLD
                GO TO FIN-VAL-FLD-GAP.
           PERFORM VAL-DBC-VRF-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DBC-VRF-ICLI-LEN IN GAP-FLD
                GO TO FIN-VAL-FLD-GAP.
           PERFORM VAL-GAP-CAI-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GAP-CAI-ICLI-LEN IN GAP-FLD
                GO TO FIN-VAL-FLD-GAP.
           PERFORM VAL-GAP-IIC-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GAP-IIC-ICLI-LEN IN GAP-FLD
                GO TO FIN-VAL-FLD-GAP.
           PERFORM VAL-DBC-GLS-NOMC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DBC-GLS-NOMC-LEN IN GAP-FLD
                GO TO FIN-VAL-FLD-GAP.
           PERFORM VAL-GAP-FOJ-ICR-ACR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GAP-FOJ-ICR-ACR-LEN IN GAP-FLD
                GO TO FIN-VAL-FLD-GAP.
           PERFORM VAL-GAP-NUM-ICR-ACR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GAP-NUM-ICR-ACR-LEN IN GAP-FLD
                GO TO FIN-VAL-FLD-GAP.
           PERFORM VAL-GAP-ANO-ICR-ACR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GAP-ANO-ICR-ACR-LEN IN GAP-FLD
                GO TO FIN-VAL-FLD-GAP.
           PERFORM VAL-GAP-VAL-ACR-PRF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GAP-VAL-ACR-PRF-LEN IN GAP-FLD
                GO TO FIN-VAL-FLD-GAP.
       FIN-VAL-FLD-GAP.
           EXIT.
       
