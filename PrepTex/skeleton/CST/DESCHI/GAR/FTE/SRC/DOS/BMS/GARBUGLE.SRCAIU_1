           MOVE GLE-COD-REG IN GLE-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              GLE-COD-REG IN GLE-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-REG IN GLE-FLD
           ELSE
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-REG IN GLE-FLD.
           GO TO FIN-VAL-GLE-COD-REG.
      *>>>>
       FIN-VAL-GLE-COD-REG.
           EXIT.
       
       VAL-FRM-GLS-REG SECTION.
       INI-VAL-FRM-GLS-REG.
       FIN-VAL-FRM-GLS-REG.
           EXIT.
       
       VAL-GLE-LOC-REG SECTION.
       INI-VAL-GLE-LOC-REG.
           IF GLE-LOC-REG IN GLE-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-GLE-LOC-REG.
      *<<<< GLE_LOC_REG
      *VAL-COD Valida codigo de tabla
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'LOC' TO TAB-COD-TTAB IN TAB.
           MOVE GLE-LOC-REG IN GLE-FLD TO WSS-LOC-REG.
           MOVE WSS-COD-LOC TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              GLE-LOC-REG IN GLE-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-LOC-REG IN GLE-FLD
               GO TO FIN-VAL-GLE-LOC-REG.
           IF TAB-COD-DAT2 IN TAB(7) NOT = WSS-COD-REG
               MOVE 'GAR'       TO MSG-COD-SIST
               MOVE 'REGIONNEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-GLE-LOC-REG.
           MOVE TAB-GLS-DESC IN TAB TO
                FRM-GLS-LOC-REG IN GLE-FLD.
      *>>>>
       FIN-VAL-GLE-LOC-REG.
           EXIT.
       
       VAL-FRM-GLS-LOC-REG SECTION.
       INI-VAL-FRM-GLS-LOC-REG.
       FIN-VAL-FRM-GLS-LOC-REG.
           EXIT.
       
       VAL-GLE-FOJ-ICR SECTION.
       INI-VAL-GLE-FOJ-ICR.
           IF GLE-FOJ-ICR IN GLE-FLD NOT > SPACES
              MOVE SPACES TO GLE-FOJ-ICR IN GLE-FLD.
       FIN-VAL-GLE-FOJ-ICR.
           EXIT.
       
       VAL-GLE-FOJ-PHB SECTION.
       INI-VAL-GLE-FOJ-PHB.
           IF GLE-FOJ-PHB IN GLE-FLD NOT > SPACES
              MOVE SPACES TO GLE-FOJ-PHB IN GLE-FLD.
       FIN-VAL-GLE-FOJ-PHB.
           EXIT.
       
       VAL-GLE-NUM-ICR SECTION.
       INI-VAL-GLE-NUM-ICR.
       FIN-VAL-GLE-NUM-ICR.
           EXIT.
       
       VAL-GLE-NUM-PHB SECTION.
       INI-VAL-GLE-NUM-PHB.
       FIN-VAL-GLE-NUM-PHB.
           EXIT.
       
       VAL-GLE-ANO-ICR SECTION.
       INI-VAL-GLE-ANO-ICR.
      *<<<< GLE_ANO_ICR

           PERFORM GET-FHOY.
           MOVE HOY-FHOY TO WSS-HOY-FHOY IN WSS-VARI.
           IF GLE-ANO-ICR IN GLE-FLD > WSS-SA-FHOY
               MOVE 'GAR' TO MSG-COD-SIST
               MOVE 'GLEANO>HOY' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-GLE-ANO-ICR.
      *
           IF GLE-ANO-ICR IN GLE-FLD > 0 AND < 1900
               MOVE 'GAR' TO MSG-COD-SIST
               MOVE 'GLEANO<1900' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-GLE-ANO-ICR.

      *DAD ini  05-JAN-1995
      *     IF ( GLE-ANO-ICR > ZEROES AND
      *          GLE-FEC-PFN > ZEROES )
      *          IF GLE-ANO-ICR < GLE-FA-PFN IN GLE-FLD
      *              MOVE 'GAR' TO MSG-COD-SIST
      *              MOVE 'GLEICR>=PFN' TO MSG-COD-MENS
      *              PERFORM GET-MSG
      *              MOVE MSG-GLS-DESC TO FRM-MENS
      *              MOVE FRM-SUAR-MAL TO FRM-SUAR
      *              GO TO FIN-VAL-GLE-ANO-ICR.
      *DAD fin  05-JAN-1995
      *>>>>
       FIN-VAL-GLE-ANO-ICR.
           EXIT.
       
       VAL-GLE-ANO-PHB SECTION.
       INI-VAL-GLE-ANO-PHB.
      *<<<< GLE_ANO_PHB

           IF GLE-ANO-PHB > 0 AND < 1900
               MOVE 'GAR' TO MSG-COD-SIST
               MOVE 'GLEANO<1900' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-GLE-ANO-PHB.

      *DAD ini  05-JAN-1995
      *     IF ( GLE-ANO-PHB > ZEROES AND
      *          GLE-ANO-ICR > ZEROES )
      *           IF GLE-ANO-PHB < GLE-ANO-ICR
      *              MOVE 'GAR' TO MSG-COD-SIST
      *              MOVE 'GLEPHB<ICR' TO MSG-COD-MENS
      *              PERFORM GET-MSG
      *              MOVE MSG-GLS-DESC TO FRM-MENS
      *              MOVE FRM-SUAR-MAL TO FRM-SUAR
      *              GO TO FIN-VAL-GLE-ANO-PHB.
      *DAD fin  05-JAN-1995
      *>>>>
       FIN-VAL-GLE-ANO-PHB.
           EXIT.
       
       VAL-CON-FLD-GLE SECTION.
       INI-VAL-CON-FLD-GLE.
       FIN-VAL-CON-FLD-GLE.
           EXIT.
       
       VAL-CON-GLE SECTION.
       INI-VAL-CON-GLE.
       FIN-VAL-CON-GLE.
           EXIT.
       
       VAL-NUM-KEY-GLE SECTION.
       INI-VAL-NUM-KEY-GLE.
           PERFORM VAL-NUM-IKY-GLE.
           PERFORM VAL-NUM-MKY-GLE.
           PERFORM VAL-NUM-AKY-GLE.
       FIN-VAL-NUM-KEY-GLE.
           EXIT.
       
       VAL-NUM-IKY-GLE SECTION.
       INI-VAL-NUM-IKY-GLE.
       VAL-NUM-GLE-COD-CRR.
           MOVE GLE-COD-CRR-ALF IN GLE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO GLE-COD-CRR IN GLE-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO GLE-COD-CRR-LEN IN GLE-FLD
                GO TO FIN-VAL-NUM-IKY-GLE.
       FIN-VAL-NUM-IKY-GLE.
           EXIT.
       
       VAL-NUM-MKY-GLE SECTION.
       INI-VAL-NUM-MKY-GLE.
       FIN-VAL-NUM-MKY-GLE.
           EXIT.
       
       VAL-NUM-AKY-GLE SECTION.
       INI-VAL-NUM-AKY-GLE.
       FIN-VAL-NUM-AKY-GLE.
           EXIT.
       
       VAL-NUM-FLD-GLE SECTION.
       INI-VAL-NUM-FLD-GLE.
       VAL-NUM-GLE-NUM-ICR.
           MOVE GLE-NUM-ICR-ALF IN GLE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 5 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO GLE-NUM-ICR IN GLE-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO GLE-NUM-ICR-LEN IN GLE-FLD
                GO TO FIN-VAL-NUM-FLD-GLE.
       VAL-NUM-GLE-NUM-PHB.
           MOVE GLE-NUM-PHB-ALF IN GLE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 5 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO GLE-NUM-PHB IN GLE-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO GLE-NUM-PHB-LEN IN GLE-FLD
                GO TO FIN-VAL-NUM-FLD-GLE.
       VAL-NUM-GLE-ANO-ICR.
           MOVE GLE-ANO-ICR-ALF IN GLE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 4 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO GLE-ANO-ICR IN GLE-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO GLE-ANO-ICR-LEN IN GLE-FLD
                GO TO FIN-VAL-NUM-FLD-GLE.
       VAL-NUM-GLE-ANO-PHB.
           MOVE GLE-ANO-PHB-ALF IN GLE-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 4 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO GLE-ANO-PHB IN GLE-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO GLE-ANO-PHB-LEN IN GLE-FLD
                GO TO FIN-VAL-NUM-FLD-GLE.
       FIN-VAL-NUM-FLD-GLE.
           EXIT.
       
       EDT-KEY-GLE SECTION.
       INI-EDT-KEY-GLE.
           PERFORM EDT-IKY-GLE.
           PERFORM EDT-MKY-GLE.
           PERFORM EDT-AKY-GLE.
       FIN-EDT-KEY-GLE.
           EXIT.
       
       EDT-IKY-GLE SECTION.
       INI-EDT-IKY-GLE.
           IF GLE-COD-CRR-LEN IN GLE-FLD = -9
                MOVE -1 TO GLE-COD-CRR-LEN IN GLE-FLD
                GO TO FIN-EDT-IKY-GLE.
           MOVE GLE-COD-CRR IN GLE-FLD TO GLE-COD-CRR-EDT IN GLE-EDT.
           MOVE GLE-COD-CRR-EDT IN GLE-EDT TO 
                GLE-COD-CRR-ALF IN GLE-FLD.
       FIN-EDT-IKY-GLE.
           EXIT.
       
       EDT-MKY-GLE SECTION.
       INI-EDT-MKY-GLE.
       FIN-EDT-MKY-GLE.
           EXIT.
       
       EDT-AKY-GLE SECTION.
       INI-EDT-AKY-GLE.
       FIN-EDT-AKY-GLE.
           EXIT.
       
       EDT-FLD-GLE SECTION.
       INI-EDT-FLD-GLE.
           IF GLE-NUM-ICR-LEN IN GLE-FLD = -9
                MOVE -1 TO GLE-NUM-ICR-LEN IN GLE-FLD
                GO TO FIN-EDT-FLD-GLE.
           MOVE GLE-NUM-ICR IN GLE-FLD TO GLE-NUM-ICR-EDT IN GLE-EDT.
           MOVE GLE-NUM-ICR-EDT IN GLE-EDT TO 
                GLE-NUM-ICR-ALF IN GLE-FLD.
           IF GLE-NUM-PHB-LEN IN GLE-FLD = -9
                MOVE -1 TO GLE-NUM-PHB-LEN IN GLE-FLD
                GO TO FIN-EDT-FLD-GLE.
           MOVE GLE-NUM-PHB IN GLE-FLD TO GLE-NUM-PHB-EDT IN GLE-EDT.
           MOVE GLE-NUM-PHB-EDT IN GLE-EDT TO 
                GLE-NUM-PHB-ALF IN GLE-FLD.
           IF GLE-ANO-ICR-LEN IN GLE-FLD = -9
                MOVE -1 TO GLE-ANO-ICR-LEN IN GLE-FLD
                GO TO FIN-EDT-FLD-GLE.
           MOVE GLE-ANO-ICR IN GLE-FLD TO GLE-ANO-ICR-EDT IN GLE-EDT.
           MOVE GLE-ANO-ICR-EDT IN GLE-EDT TO 
                GLE-ANO-ICR-ALF IN GLE-FLD.
           IF GLE-ANO-PHB-LEN IN GLE-FLD = -9
                MOVE -1 TO GLE-ANO-PHB-LEN IN GLE-FLD
                GO TO FIN-EDT-FLD-GLE.
           MOVE GLE-ANO-PHB IN GLE-FLD TO GLE-ANO-PHB-EDT IN GLE-EDT.
           MOVE GLE-ANO-PHB-EDT IN GLE-EDT TO 
                GLE-ANO-PHB-ALF IN GLE-FLD.
       FIN-EDT-FLD-GLE.
           EXIT.
