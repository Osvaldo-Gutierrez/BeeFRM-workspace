       INI-VAL-MKY-GDD.
       FIN-VAL-MKY-GDD.
           EXIT.
       
       VAL-FLD-GDD SECTION.
       INI-VAL-FLD-GDD.
           PERFORM VAL-FRM-GLS-TIP-REA-NRE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-TIP-REA-NRE-LEN IN GDD-FLD
                GO TO FIN-VAL-FLD-GDD.
           PERFORM VAL-FRM-NUM-QDEU.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-NUM-QDEU-LEN IN GDD-FLD
                GO TO FIN-VAL-FLD-GDD.
           PERFORM VAL-GDG-TIP-OPE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-TIP-OPE-LEN IN GDD-FLD
                GO TO FIN-VAL-FLD-GDD.
           PERFORM VAL-GDG-STP-OPE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-STP-OPE-LEN IN GDD-FLD
                GO TO FIN-VAL-FLD-GDD.
           PERFORM VAL-FRM-GLS-TIP-OPE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-TIP-OPE-LEN IN GDD-FLD
                GO TO FIN-VAL-FLD-GDD.
           PERFORM VAL-FRM-GLS-STP-OPE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-STP-OPE-LEN IN GDD-FLD
                GO TO FIN-VAL-FLD-GDD.
           PERFORM VAL-GDG-COD-OFI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-COD-OFI-LEN IN GDD-FLD
                GO TO FIN-VAL-FLD-GDD.
           PERFORM VAL-FRM-GLS-OFI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-OFI-LEN IN GDD-FLD
                GO TO FIN-VAL-FLD-GDD.
           PERFORM VAL-GDG-VCB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-VCB-LEN IN GDD-FLD
                GO TO FIN-VAL-FLD-GDD.
           PERFORM VAL-FRM-GLS-VCB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-VCB-LEN IN GDD-FLD
                GO TO FIN-VAL-FLD-GDD.
           PERFORM VAL-GDG-DES-1.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-DES-1-LEN IN GDD-FLD
                GO TO FIN-VAL-FLD-GDD.
           PERFORM VAL-GDG-DES-2.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-DES-2-LEN IN GDD-FLD
                GO TO FIN-VAL-FLD-GDD.
           PERFORM VAL-GDG-COD-EJE-ORI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-COD-EJE-ORI-LEN IN GDD-FLD
                GO TO FIN-VAL-FLD-GDD.
           PERFORM VAL-FRM-GLS-EJE-ORI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-EJE-ORI-LEN IN GDD-FLD
                GO TO FIN-VAL-FLD-GDD.
           PERFORM VAL-DBC-COD-EJEC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DBC-COD-EJEC-LEN IN GDD-FLD
                GO TO FIN-VAL-FLD-GDD.
           PERFORM VAL-FRM-GLS-EJEC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-EJEC-LEN IN GDD-FLD
                GO TO FIN-VAL-FLD-GDD.
           PERFORM VAL-GDD-VAL-LIM-DDR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDD-VAL-LIM-DDR-LEN IN GDD-FLD
                GO TO FIN-VAL-FLD-GDD.
           PERFORM VAL-GDD-PCT-LIM-DDR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDD-PCT-LIM-DDR-LEN IN GDD-FLD
                GO TO FIN-VAL-FLD-GDD.
       FIN-VAL-FLD-GDD.
           EXIT.
       
       VAL-FRM-GLS-TIP-REA-NRE SECTION.
