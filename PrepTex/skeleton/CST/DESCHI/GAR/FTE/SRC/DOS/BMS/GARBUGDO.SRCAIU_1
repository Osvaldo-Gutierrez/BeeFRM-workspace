           EXIT.
       
       CLR-FLD-GDO SECTION.
       INI-CLR-FLD-GDO.
           MOVE SPACES TO FRM-GLS-TIP-REA-NRE IN GDO-FLD.
           MOVE SPACES TO FRM-GLS-TIP-OPE IN GDO-FLD.
           MOVE SPACES TO FRM-GLS-STP-OPE IN GDO-FLD.
           MOVE SPACES TO FRM-GLS-OFI IN GDO-FLD.
           MOVE SPACES TO FRM-GLS-VCB IN GDO-FLD.
           MOVE SPACES TO FRM-GLS-TIP-DOC IN GDO-FLD.
           MOVE SPACES TO FRM-GLS-IIV-EMI IN GDO-FLD.
           PERFORM CLR-GDO-GDO.
           PERFORM CLR-GDG-GDO.
       FIN-CLR-FLD-GDO.
           EXIT.
       
       CLR-GDO-GDO SECTION.
       INI-CLR-GDO-GDO.
           MOVE SPACES TO GDO-COD-DOC IN GDO-FLD.
           MOVE SPACES TO GDO-TIP-DOC IN GDO-FLD.
           MOVE ZEROES TO GDO-FEC-CAP IN GDO-FLD.
           MOVE SPACES TO GDO-TIV-EMI IN GDO-FLD.
           MOVE SPACES TO GDO-CIV-EMI IN GDO-FLD.
           MOVE SPACES TO GDO-SIV-EMI IN GDO-FLD.
       FIN-CLR-GDO-GDO.
           EXIT.
       
       CLR-GDG-GDO SECTION.
       INI-CLR-GDG-GDO.
           MOVE SPACES TO GDG-TIP-OPE IN GDO-FLD.
           MOVE SPACES TO GDG-STP-OPE IN GDO-FLD.
           MOVE SPACES TO GDG-COD-OFI IN GDO-FLD.
           MOVE SPACES TO GDG-VCB IN GDO-FLD.
           MOVE SPACES TO GDG-DES-1 IN GDO-FLD.
           MOVE SPACES TO GDG-DES-2 IN GDO-FLD.
       FIN-CLR-GDG-GDO.
           EXIT.
       
       FST-KEY-GDO SECTION.
       INI-FST-KEY-GDO.
           MOVE -1 TO GDO-NUM-SIS-LEN IN GDO-FLD.
       FIN-FST-KEY-GDO.
           EXIT.
       
       FST-FLD-GDO SECTION.
       INI-FST-FLD-GDO.
           MOVE -1 TO GDO-COD-DOC-LEN IN GDO-FLD.
       FIN-FST-FLD-GDO.
           EXIT.
       
       PRO-KEY-GDO SECTION.
       INI-PRO-KEY-GDO.
           PERFORM PRO-MKY-GDO.
       FIN-PRO-KEY-GDO.
           EXIT.
       
       PRO-MKY-GDO SECTION.
       INI-PRO-MKY-GDO.
       FIN-PRO-MKY-GDO.
           EXIT.
       
       PRO-FLD-GDO SECTION.
       INI-PRO-FLD-GDO.
           PERFORM PRO-GDO-GDO.
           PERFORM PRO-GDG-GDO.
       FIN-PRO-FLD-GDO.
           EXIT.
       
       PRO-GDO-GDO SECTION.
       INI-PRO-GDO-GDO.
           MOVE FRM-CPIM-AEY TO GDO-COD-DOC-ATR IN GDO-FLD.
           MOVE FRM-CPIM-AEY TO GDO-TIP-DOC-ATR IN GDO-FLD.
           MOVE FRM-CPIM-AEY TO GDO-FEC-CAP-ATR IN GDO-FLD.
           MOVE FRM-CPIM-AEY TO GDO-TIV-EMI-ATR IN GDO-FLD.
           MOVE FRM-CPIM-AEY TO GDO-CIV-EMI-ATR IN GDO-FLD.
           MOVE FRM-CPIM-AEY TO GDO-SIV-EMI-ATR IN GDO-FLD.
       FIN-PRO-GDO-GDO.
           EXIT.
       
       PRO-GDG-GDO SECTION.
       INI-PRO-GDG-GDO.
       FIN-PRO-GDG-GDO.
           EXIT.
       
       UNP-KEY-GDO SECTION.
       INI-UNP-KEY-GDO.
           PERFORM UNP-IKY-GDO.
           PERFORM UNP-AKY-GDO.
           PERFORM UNP-MKY-GDO.
       FIN-UNP-KEY-GDO.
           EXIT.
       
       UNP-IKY-GDO SECTION.
       INI-UNP-IKY-GDO.
           MOVE FRM-CPIM-UBY TO GDO-NUM-SIS-ATR.
           MOVE FRM-CPIM-UBY TO GDO-COD-CRR-ATR.
       FIN-UNP-IKY-GDO.
           EXIT.
       
       UNP-MKY-GDO SECTION.
       INI-UNP-MKY-GDO.
       FIN-UNP-MKY-GDO.
           EXIT.
       
       UNP-AKY-GDO SECTION.
       INI-UNP-AKY-GDO.
       FIN-UNP-AKY-GDO.
           EXIT.
       
       UNP-FLD-GDO SECTION.
       INI-UNP-FLD-GDO.
           PERFORM UNP-GDO-GDO.
           PERFORM UNP-GDG-GDO.
       FIN-UNP-FLD-GDO.
           EXIT.
       
       UNP-GDO-GDO SECTION.
       INI-UNP-GDO-GDO.
           MOVE FRM-CPIM-UBY TO GDO-COD-DOC-ATR IN GDO-FLD.
           MOVE FRM-CPIM-UBY TO GDO-TIP-DOC-ATR IN GDO-FLD.
           MOVE FRM-CPIM-UBY TO GDO-FEC-CAP-ATR IN GDO-FLD.
           MOVE FRM-CPIM-UBY TO GDO-TIV-EMI-ATR IN GDO-FLD.
           MOVE FRM-CPIM-UBY TO GDO-CIV-EMI-ATR IN GDO-FLD.
           MOVE FRM-CPIM-UBY TO GDO-SIV-EMI-ATR IN GDO-FLD.
       FIN-UNP-GDO-GDO.
           EXIT.
       
       UNP-GDG-GDO SECTION.
       INI-UNP-GDG-GDO.
       FIN-UNP-GDG-GDO.
           EXIT.
       
       VAL-MKY-GDO SECTION.
       INI-VAL-MKY-GDO.
       FIN-VAL-MKY-GDO.
           EXIT.
       
       VAL-FLD-GDO SECTION.
       INI-VAL-FLD-GDO.
           PERFORM VAL-FRM-GLS-TIP-REA-NRE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-TIP-REA-NRE-LEN IN GDO-FLD
                GO TO FIN-VAL-FLD-GDO.
           PERFORM VAL-GDG-TIP-OPE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-TIP-OPE-LEN IN GDO-FLD
                GO TO FIN-VAL-FLD-GDO.
           PERFORM VAL-GDG-STP-OPE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-STP-OPE-LEN IN GDO-FLD
                GO TO FIN-VAL-FLD-GDO.
           PERFORM VAL-FRM-GLS-TIP-OPE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-TIP-OPE-LEN IN GDO-FLD
                GO TO FIN-VAL-FLD-GDO.
           PERFORM VAL-FRM-GLS-STP-OPE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-STP-OPE-LEN IN GDO-FLD
                GO TO FIN-VAL-FLD-GDO.
           PERFORM VAL-GDG-COD-OFI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-COD-OFI-LEN IN GDO-FLD
                GO TO FIN-VAL-FLD-GDO.
           PERFORM VAL-FRM-GLS-OFI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-OFI-LEN IN GDO-FLD
                GO TO FIN-VAL-FLD-GDO.
           PERFORM VAL-GDG-VCB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-VCB-LEN IN GDO-FLD
                GO TO FIN-VAL-FLD-GDO.
           PERFORM VAL-FRM-GLS-VCB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-VCB-LEN IN GDO-FLD
                GO TO FIN-VAL-FLD-GDO.
           PERFORM VAL-GDG-DES-1.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-DES-1-LEN IN GDO-FLD
                GO TO FIN-VAL-FLD-GDO.
           PERFORM VAL-GDG-DES-2.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-DES-2-LEN IN GDO-FLD
                GO TO FIN-VAL-FLD-GDO.
           PERFORM VAL-GDO-COD-DOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDO-COD-DOC-LEN IN GDO-FLD
                GO TO FIN-VAL-FLD-GDO.
           PERFORM VAL-GDO-TIP-DOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDO-TIP-DOC-LEN IN GDO-FLD
                GO TO FIN-VAL-FLD-GDO.
           PERFORM VAL-FRM-GLS-TIP-DOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-TIP-DOC-LEN IN GDO-FLD
                GO TO FIN-VAL-FLD-GDO.
           PERFORM VAL-GDO-FEC-CAP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDO-FEC-CAP-LEN IN GDO-FLD
                GO TO FIN-VAL-FLD-GDO.
           PERFORM VAL-GDO-TIV-EMI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDO-TIV-EMI-LEN IN GDO-FLD
                GO TO FIN-VAL-FLD-GDO.
           PERFORM VAL-GDO-CIV-EMI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDO-CIV-EMI-LEN IN GDO-FLD
                GO TO FIN-VAL-FLD-GDO.
           PERFORM VAL-GDO-SIV-EMI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDO-SIV-EMI-LEN IN GDO-FLD
                GO TO FIN-VAL-FLD-GDO.
           PERFORM VAL-FRM-GLS-IIV-EMI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-IIV-EMI-LEN IN GDO-FLD
                GO TO FIN-VAL-FLD-GDO.
       FIN-VAL-FLD-GDO.
           EXIT.
       
       VAL-FRM-GLS-TIP-REA-NRE SECTION.
       INI-VAL-FRM-GLS-TIP-REA-NRE.
       FIN-VAL-FRM-GLS-TIP-REA-NRE.
           EXIT.
       
       VAL-GDG-TIP-OPE SECTION.
       INI-VAL-GDG-TIP-OPE.
       FIN-VAL-GDG-TIP-OPE.
           EXIT.
       
       VAL-GDG-STP-OPE SECTION.
       INI-VAL-GDG-STP-OPE.
       FIN-VAL-GDG-STP-OPE.
           EXIT.
       
       VAL-FRM-GLS-TIP-OPE SECTION.
       INI-VAL-FRM-GLS-TIP-OPE.
       FIN-VAL-FRM-GLS-TIP-OPE.
           EXIT.
       
       VAL-FRM-GLS-STP-OPE SECTION.
       INI-VAL-FRM-GLS-STP-OPE.
       FIN-VAL-FRM-GLS-STP-OPE.
           EXIT.
       
       VAL-GDG-COD-OFI SECTION.
       INI-VAL-GDG-COD-OFI.
       FIN-VAL-GDG-COD-OFI.
           EXIT.
       
       VAL-FRM-GLS-OFI SECTION.
       INI-VAL-FRM-GLS-OFI.
       FIN-VAL-FRM-GLS-OFI.
           EXIT.
       
       VAL-GDG-VCB SECTION.
       INI-VAL-GDG-VCB.
       FIN-VAL-GDG-VCB.
           EXIT.
       
       VAL-FRM-GLS-VCB SECTION.
       INI-VAL-FRM-GLS-VCB.
       FIN-VAL-FRM-GLS-VCB.
           EXIT.
       
       VAL-GDG-DES-1 SECTION.
       INI-VAL-GDG-DES-1.
       FIN-VAL-GDG-DES-1.
           EXIT.
       
       VAL-GDG-DES-2 SECTION.
       INI-VAL-GDG-DES-2.
       FIN-VAL-GDG-DES-2.
           EXIT.
       
       VAL-GDO-COD-DOC SECTION.
       INI-VAL-GDO-COD-DOC.
           IF GDO-COD-DOC IN GDO-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-GDO-COD-DOC.
       FIN-VAL-GDO-COD-DOC.
           EXIT.
       
       VAL-GDO-TIP-DOC SECTION.
       INI-VAL-GDO-TIP-DOC.
           IF GDO-TIP-DOC IN GDO-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-GDO-TIP-DOC.
      *VAL-COD Valida codigo de tabla
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'TIO ' TO TAB-COD-TTAB IN TAB.
           MOVE GDO-TIP-DOC IN GDO-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              GDO-TIP-DOC IN GDO-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-TIP-DOC IN GDO-FLD
           ELSE
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-TIP-DOC IN GDO-FLD.
       FIN-VAL-GDO-TIP-DOC.
           EXIT.
       
       VAL-FRM-GLS-TIP-DOC SECTION.
       INI-VAL-FRM-GLS-TIP-DOC.
       FIN-VAL-FRM-GLS-TIP-DOC.
           EXIT.
       
       VAL-GDO-FEC-CAP SECTION.
       INI-VAL-GDO-FEC-CAP.
      *VAL-FEC Validar Fecha
           MOVE GDO-FEC-CAP IN GDO-FLD TO FEC-FECH.
           MOVE FEC-FORM-FEC TO FEC-FORM.
           MOVE FEC-FBLK-N  TO FEC-FBLK.
           MOVE FEC-CHOY-LE TO FEC-CHOY.
           MOVE FEC-VHBL-S TO FEC-VHBL.
      *<<<< INI_GDO_FEC_CAP

      *>>>>
           MOVE FEC-VAL-FEC TO FEC-CMND.
           PERFORM PRO-FEC.
           IF FEC-STAT-OKS
               MOVE FEC-FECH TO GDO-FEC-CAP IN GDO-FLD
           ELSE
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE FEC-MENS     TO FRM-MENS
               GO TO FIN-VAL-GDO-FEC-CAP.
      *<<<< GDO_FEC_CAP

           MOVE GDG-STP-OPE IN GDO-FLD TO WSS-STP-OPE.
           IF NOT DEPOSITO-A-PLAZO
               GO TO FIN-VAL-GDO-FEC-CAP.
           MOVE FEC-CHOY-N TO FEC-CHOY.
           MOVE GAR-FA-SOL-CNS IN CMA-VARI TO FEC-FANO IN FEC-VARI.
           MOVE GAR-FM-SOL-CNS IN CMA-VARI TO FEC-FMES IN FEC-VARI.
           MOVE GAR-FD-SOL-CNS IN CMA-VARI TO FEC-FDIA IN FEC-VARI.
           MOVE FEC-SVLD TO FEC-SEC1.
           MOVE FEC-AVLD TO FEC-AEC1.
           MOVE FEC-MVLD TO FEC-MEC1.
           MOVE FEC-DVLD TO FEC-DEC1.

           MOVE GDO-FA-CAP IN GDO-FLD TO FEC-FANO IN FEC-VARI.
           MOVE GDO-FM-CAP IN GDO-FLD TO FEC-FMES IN FEC-VARI.
           MOVE GDO-FD-CAP IN GDO-FLD TO FEC-FDIA IN FEC-VARI.
           MOVE FEC-SVLD TO FEC-SEC2.
           MOVE FEC-AVLD TO FEC-AEC2.
           MOVE FEC-MVLD TO FEC-MEC2.
           MOVE FEC-DVLD TO FEC-DEC2.

           MOVE FEC-FORM-FEC TO FEC-FORM.
           MOVE FEC-CMP-FEC TO FEC-CMND.
           PERFORM PRO-FEC.
           IF FEC-STAT-LES
               MOVE 'CAP>SOL' TO MSG-COD-MENS
               MOVE 'GAR'     TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-GDO-FEC-CAP.

      *>>>>
       FIN-VAL-GDO-FEC-CAP.
           EXIT.
       
       VAL-GDO-TIV-EMI SECTION.
       INI-VAL-GDO-TIV-EMI.
           IF GDO-TIV-EMI IN GDO-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-GDO-TIV-EMI.
       FIN-VAL-GDO-TIV-EMI.
           EXIT.
       
       VAL-GDO-CIV-EMI SECTION.
       INI-VAL-GDO-CIV-EMI.
           IF GDO-CIV-EMI IN GDO-FLD NOT > SPACES
              MOVE SPACES TO GDO-CIV-EMI IN GDO-FLD.
       FIN-VAL-GDO-CIV-EMI.
           EXIT.
       
       VAL-GDO-SIV-EMI SECTION.
       INI-VAL-GDO-SIV-EMI.
           IF GDO-SIV-EMI IN GDO-FLD NOT > SPACES
              MOVE SPACES TO GDO-SIV-EMI IN GDO-FLD.
      *<<<< GDO_SIV_EMI
      *VAL-COD Valida codigo de tabla
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE GDO-TIV-EMI IN GDO-FLD TO TAB-COD-TTAB IN TAB.
           MOVE SPACES TO TAB-COD-CTAB IN TAB.
           MOVE GDO-CIV-EMI IN GDO-FLD TO WSS-CIV-EMI
           MOVE GDO-SIV-EMI IN GDO-FLD TO WSS-SIV-EMI
           MOVE WSS-CIV-SIV TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = SPACES
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-IIV-EMI IN GDO-FLD
           ELSE
               MOVE 0 TO GDO-SIV-EMI IN GDO-FLD
               MOVE -1 TO GDO-TIV-EMI IN GDO-FLD
               MOVE SPACES TO FRM-GLS-IIV-EMI IN GDO-FLD
               GO TO FIN-VAL-GDO-SIV-EMI.

      *>>>>
       FIN-VAL-GDO-SIV-EMI.
           EXIT.
       
       VAL-FRM-GLS-IIV-EMI SECTION.
       INI-VAL-FRM-GLS-IIV-EMI.
       FIN-VAL-FRM-GLS-IIV-EMI.
           EXIT.
       
       VAL-CON-FLD-GDO SECTION.
       INI-VAL-CON-FLD-GDO.
       FIN-VAL-CON-FLD-GDO.
           EXIT.
       
       VAL-CON-GDO SECTION.
       INI-VAL-CON-GDO.
       FIN-VAL-CON-GDO.
           EXIT.
       
       VAL-NUM-KEY-GDO SECTION.
       INI-VAL-NUM-KEY-GDO.
           PERFORM VAL-NUM-IKY-GDO.
           PERFORM VAL-NUM-MKY-GDO.
           PERFORM VAL-NUM-AKY-GDO.
       FIN-VAL-NUM-KEY-GDO.
           EXIT.
       
       VAL-NUM-IKY-GDO SECTION.
       INI-VAL-NUM-IKY-GDO.
       VAL-NUM-GDO-COD-CRR.
           MOVE GDO-COD-CRR-ALF IN GDO-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO GDO-COD-CRR IN GDO-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO GDO-COD-CRR-LEN IN GDO-FLD
                GO TO FIN-VAL-NUM-IKY-GDO.
       FIN-VAL-NUM-IKY-GDO.
           EXIT.
       
       VAL-NUM-MKY-GDO SECTION.
       INI-VAL-NUM-MKY-GDO.
       FIN-VAL-NUM-MKY-GDO.
           EXIT.
       
       VAL-NUM-AKY-GDO SECTION.
       INI-VAL-NUM-AKY-GDO.
       FIN-VAL-NUM-AKY-GDO.
           EXIT.
       
       VAL-NUM-FLD-GDO SECTION.
       INI-VAL-NUM-FLD-GDO.
       FIN-VAL-NUM-FLD-GDO.
           EXIT.
       
       EDT-KEY-GDO SECTION.
       INI-EDT-KEY-GDO.
           PERFORM EDT-IKY-GDO.
           PERFORM EDT-MKY-GDO.
           PERFORM EDT-AKY-GDO.
       FIN-EDT-KEY-GDO.
           EXIT.
       
       EDT-IKY-GDO SECTION.
       INI-EDT-IKY-GDO.
           IF GDO-COD-CRR-LEN IN GDO-FLD = -9
                MOVE -1 TO GDO-COD-CRR-LEN IN GDO-FLD
                GO TO FIN-EDT-IKY-GDO.
           MOVE GDO-COD-CRR IN GDO-FLD TO GDO-COD-CRR-EDT IN GDO-EDT.
           MOVE GDO-COD-CRR-EDT IN GDO-EDT TO 
                GDO-COD-CRR-ALF IN GDO-FLD.
