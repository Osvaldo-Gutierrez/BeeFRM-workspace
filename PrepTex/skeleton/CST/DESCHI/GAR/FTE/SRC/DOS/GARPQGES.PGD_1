       COPY GARBFGDG.
       COPY GARBFTAG.

       COPY COLBFOPC.
       COPY COLBFDLC.

      *<<<< NOT PGM_PU
       COPY GARBGVCG.
       COPY GARBKVCG.
      *>>>>

       COPY GNSBVIND.
       COPY GNSBBMSC.
       COPY GNSBVMSC.
       COPY GNSBFMSC.

       COPY GNSBGPES.
      *>>>>
