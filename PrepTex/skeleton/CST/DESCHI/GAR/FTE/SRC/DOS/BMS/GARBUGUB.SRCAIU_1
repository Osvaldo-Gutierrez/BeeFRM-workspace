           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO GUB-COD-CRR IN GUB-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO GUB-COD-CRR-LEN IN GUB-FLD
                GO TO FIN-VAL-NUM-IKY-GUB.
       FIN-VAL-NUM-IKY-GUB.
           EXIT.
       
       VAL-NUM-MKY-GUB SECTION.
       INI-VAL-NUM-MKY-GUB.
       FIN-VAL-NUM-MKY-GUB.
           EXIT.
       
       VAL-NUM-AKY-GUB SECTION.
       INI-VAL-NUM-AKY-GUB.
       FIN-VAL-NUM-AKY-GUB.
           EXIT.
       
       VAL-NUM-FLD-GUB SECTION.
       INI-VAL-NUM-FLD-GUB.
       FIN-VAL-NUM-FLD-GUB.
           EXIT.
       
       EDT-KEY-GUB SECTION.
       INI-EDT-KEY-GUB.
           PERFORM EDT-IKY-GUB.
           PERFORM EDT-MKY-GUB.
           PERFORM EDT-AKY-GUB.
       FIN-EDT-KEY-GUB.
           EXIT.
       
       EDT-IKY-GUB SECTION.
       INI-EDT-IKY-GUB.
           IF GUB-COD-CRR-LEN IN GUB-FLD = -9
                MOVE -1 TO GUB-COD-CRR-LEN IN GUB-FLD
                GO TO FIN-EDT-IKY-GUB.
           MOVE GUB-COD-CRR IN GUB-FLD TO GUB-COD-CRR-EDT IN GUB-EDT.
           MOVE GUB-COD-CRR-EDT IN GUB-EDT TO 
                GUB-COD-CRR-ALF IN GUB-FLD.
       FIN-EDT-IKY-GUB.
           EXIT.
       
       EDT-MKY-GUB SECTION.
       INI-EDT-MKY-GUB.
       FIN-EDT-MKY-GUB.
           EXIT.
       
       EDT-AKY-GUB SECTION.
       INI-EDT-AKY-GUB.
       FIN-EDT-AKY-GUB.
           EXIT.
       
       EDT-FLD-GUB SECTION.
       INI-EDT-FLD-GUB.
       FIN-EDT-FLD-GUB.
           EXIT.
