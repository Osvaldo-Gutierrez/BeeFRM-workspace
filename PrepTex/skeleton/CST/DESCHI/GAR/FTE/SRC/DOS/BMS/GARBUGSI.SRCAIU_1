      * 19-Oct-93 12:57 PM
       RET-ALL-GSI SECTION.
       INI-RET-ALL-GSI.
           PERFORM KEY-ALL-GSI.
           PERFORM RET-GSI-GSI.
           PERFORM RET-GDG-GSI.
       FIN-RET-ALL-GSI.
           EXIT.
       
       RET-GSI-GSI SECTION.
       INI-RET-GSI-GSI.
           MOVE GSI-FD-SIN IN GSI-FLD TO GAR-FD-SIN IN GSI.
           MOVE GSI-FM-SIN IN GSI-FLD TO GAR-FM-SIN IN GSI.
           MOVE GSI-FA-SIN IN GSI-FLD TO GAR-FA-SIN IN GSI.
           MOVE GSI-PCT-SIN IN GSI-FLD TO GAR-PCT-SIN IN GSI.
           MOVE GSI-IND-CUB-SEG IN GSI-FLD TO GAR-IND-CUB-SEG IN GSI.
           MOVE GSI-VAL-PAG-SEG IN GSI-FLD TO GAR-VAL-PAG-SEG IN GSI.
           MOVE GSI-DST-IDM IN GSI-FLD TO GAR-DST-IDM IN GSI.
           MOVE GSI-IND-RTS IN GSI-FLD TO GAR-IND-RTS IN GSI.
       FIN-RET-GSI-GSI.
           EXIT.
       
       RET-GDG-GSI SECTION.
       INI-RET-GDG-GSI.
           MOVE GDG-TIP-OPE IN GSI-FLD TO GAR-TIP-OPE IN GDG.
           MOVE GDG-STP-OPE IN GSI-FLD TO GAR-STP-OPE IN GDG.
           MOVE GDG-COD-OFI IN GSI-FLD TO GAR-COD-OFI IN GDG.
           MOVE GDG-VCB IN GSI-FLD TO GAR-VCB IN GDG.
           MOVE GDG-DES-1 IN GSI-FLD TO GAR-DES-1 IN GDG.
           MOVE GDG-DES-2 IN GSI-FLD TO GAR-DES-2 IN GDG.
       FIN-RET-GDG-GSI.
           EXIT.
       
       KEY-ALL-GSI SECTION.
       INI-KEY-ALL-GSI.
           PERFORM KEY-GSI-GSI.
           PERFORM KEY-GDG-GSI.
       FIN-KEY-ALL-GSI.
           EXIT.
       
       KEY-GSI-GSI SECTION.
       INI-KEY-GSI-GSI.
           MOVE GSI-NUM-SIS IN GSI-FLD TO GAR-NUM-SIS IN GSI.
           MOVE GSI-COD-CRR IN GSI-FLD TO GAR-COD-CRR IN GSI.
       FIN-KEY-GSI-GSI.
           EXIT.
       
       KEY-GDG-GSI SECTION.
       INI-KEY-GDG-GSI.
       FIN-KEY-GDG-GSI.
           EXIT.
       
       SCR-CLRA-GSI SECTION.
       INI-SCR-CLRA-GSI.
           PERFORM CLR-KEY-GSI.
           PERFORM CLR-FLD-GSI.
       FIN-SCR-CLRA-GSI.
           EXIT.
       
       CLR-KEY-GSI SECTION.
       INI-CLR-KEY-GSI.
           MOVE SPACES TO GSI-NUM-SIS IN GSI-FLD.
           MOVE ZEROES TO GSI-COD-CRR IN GSI-FLD.
       FIN-CLR-KEY-GSI.
           EXIT.
       
       CLR-FLD-GSI SECTION.
       INI-CLR-FLD-GSI.
           MOVE SPACES TO FRM-GLS-TIP-REA-NRE IN GSI-FLD.
           MOVE SPACES TO FRM-GLS-TIP-OPE IN GSI-FLD.
           MOVE SPACES TO FRM-GLS-STP-OPE IN GSI-FLD.
           MOVE SPACES TO FRM-GLS-OFI IN GSI-FLD.
           MOVE SPACES TO FRM-GLS-VCB IN GSI-FLD.
           PERFORM CLR-GSI-GSI.
           PERFORM CLR-GDG-GSI.
       FIN-CLR-FLD-GSI.
           EXIT.
       
       CLR-GSI-GSI SECTION.
       INI-CLR-GSI-GSI.
           MOVE ZEROES TO GSI-FEC-SIN IN GSI-FLD.
           MOVE ZEROES TO GSI-PCT-SIN IN GSI-FLD.
           MOVE SPACES TO GSI-IND-CUB-SEG IN GSI-FLD.
           MOVE ZEROES TO GSI-VAL-PAG-SEG IN GSI-FLD.
           MOVE SPACES TO GSI-DST-IDM IN GSI-FLD.
           MOVE SPACES TO GSI-IND-RTS IN GSI-FLD.
       FIN-CLR-GSI-GSI.
           EXIT.
       
       CLR-GDG-GSI SECTION.
       INI-CLR-GDG-GSI.
           MOVE SPACES TO GDG-TIP-OPE IN GSI-FLD.
           MOVE SPACES TO GDG-STP-OPE IN GSI-FLD.
           MOVE SPACES TO GDG-COD-OFI IN GSI-FLD.
           MOVE SPACES TO GDG-VCB IN GSI-FLD.
           MOVE SPACES TO GDG-DES-1 IN GSI-FLD.
           MOVE SPACES TO GDG-DES-2 IN GSI-FLD.
       FIN-CLR-GDG-GSI.
           EXIT.
       
       FST-KEY-GSI SECTION.
       INI-FST-KEY-GSI.
           MOVE -1 TO GSI-NUM-SIS-LEN IN GSI-FLD.
       FIN-FST-KEY-GSI.
           EXIT.
       
       FST-FLD-GSI SECTION.
       INI-FST-FLD-GSI.
           MOVE -1 TO GSI-FEC-SIN-LEN IN GSI-FLD.
       FIN-FST-FLD-GSI.
           EXIT.
       
       PRO-KEY-GSI SECTION.
       INI-PRO-KEY-GSI.
           PERFORM PRO-MKY-GSI.
       FIN-PRO-KEY-GSI.
           EXIT.
       
       PRO-MKY-GSI SECTION.
       INI-PRO-MKY-GSI.
       FIN-PRO-MKY-GSI.
           EXIT.
       
       PRO-FLD-GSI SECTION.
       INI-PRO-FLD-GSI.
           PERFORM PRO-GSI-GSI.
           PERFORM PRO-GDG-GSI.
       FIN-PRO-FLD-GSI.
           EXIT.
       
       PRO-GSI-GSI SECTION.
       INI-PRO-GSI-GSI.
           MOVE FRM-CPIM-AEY TO GSI-FEC-SIN-ATR IN GSI-FLD.
           MOVE FRM-CPIM-AEY TO GSI-PCT-SIN-ATR IN GSI-FLD.
           MOVE FRM-CPIM-AEY TO GSI-IND-CUB-SEG-ATR IN GSI-FLD.
           MOVE FRM-CPIM-AEY TO GSI-VAL-PAG-SEG-ATR IN GSI-FLD.
           MOVE FRM-CPIM-AEY TO GSI-DST-IDM-ATR IN GSI-FLD.
           MOVE FRM-CPIM-AEY TO GSI-IND-RTS-ATR IN GSI-FLD.
       FIN-PRO-GSI-GSI.
           EXIT.
       
       PRO-GDG-GSI SECTION.
       INI-PRO-GDG-GSI.
       FIN-PRO-GDG-GSI.
           EXIT.
       
       UNP-KEY-GSI SECTION.
       INI-UNP-KEY-GSI.
           PERFORM UNP-IKY-GSI.
           PERFORM UNP-AKY-GSI.
           PERFORM UNP-MKY-GSI.
       FIN-UNP-KEY-GSI.
           EXIT.
       
       UNP-IKY-GSI SECTION.
       INI-UNP-IKY-GSI.
           MOVE FRM-CPIM-UBY TO GSI-NUM-SIS-ATR.
           MOVE FRM-CPIM-UBY TO GSI-COD-CRR-ATR.
       FIN-UNP-IKY-GSI.
           EXIT.
       
       UNP-MKY-GSI SECTION.
       INI-UNP-MKY-GSI.
       FIN-UNP-MKY-GSI.
           EXIT.
       
       UNP-AKY-GSI SECTION.
       INI-UNP-AKY-GSI.
       FIN-UNP-AKY-GSI.
           EXIT.
       
       UNP-FLD-GSI SECTION.
       INI-UNP-FLD-GSI.
           PERFORM UNP-GSI-GSI.
           PERFORM UNP-GDG-GSI.
       FIN-UNP-FLD-GSI.
           EXIT.
       
       UNP-GSI-GSI SECTION.
       INI-UNP-GSI-GSI.
           MOVE FRM-CPIM-UBY TO GSI-FEC-SIN-ATR IN GSI-FLD.
           MOVE FRM-CPIM-UBY TO GSI-PCT-SIN-ATR IN GSI-FLD.
           MOVE FRM-CPIM-UBY TO GSI-IND-CUB-SEG-ATR IN GSI-FLD.
           MOVE FRM-CPIM-UBY TO GSI-VAL-PAG-SEG-ATR IN GSI-FLD.
           MOVE FRM-CPIM-UBY TO GSI-DST-IDM-ATR IN GSI-FLD.
           MOVE FRM-CPIM-UBY TO GSI-IND-RTS-ATR IN GSI-FLD.
       FIN-UNP-GSI-GSI.
           EXIT.
       
       UNP-GDG-GSI SECTION.
       INI-UNP-GDG-GSI.
       FIN-UNP-GDG-GSI.
           EXIT.
       
       VAL-MKY-GSI SECTION.
       INI-VAL-MKY-GSI.
       FIN-VAL-MKY-GSI.
           EXIT.
       
       VAL-FLD-GSI SECTION.
       INI-VAL-FLD-GSI.
           PERFORM VAL-FRM-GLS-TIP-REA-NRE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-TIP-REA-NRE-LEN IN GSI-FLD
                GO TO FIN-VAL-FLD-GSI.
           PERFORM VAL-GDG-TIP-OPE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-TIP-OPE-LEN IN GSI-FLD
                GO TO FIN-VAL-FLD-GSI.
           PERFORM VAL-GDG-STP-OPE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-STP-OPE-LEN IN GSI-FLD
                GO TO FIN-VAL-FLD-GSI.
           PERFORM VAL-FRM-GLS-TIP-OPE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-TIP-OPE-LEN IN GSI-FLD
                GO TO FIN-VAL-FLD-GSI.
           PERFORM VAL-FRM-GLS-STP-OPE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-STP-OPE-LEN IN GSI-FLD
                GO TO FIN-VAL-FLD-GSI.
           PERFORM VAL-GDG-COD-OFI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-COD-OFI-LEN IN GSI-FLD
                GO TO FIN-VAL-FLD-GSI.
           PERFORM VAL-FRM-GLS-OFI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-OFI-LEN IN GSI-FLD
                GO TO FIN-VAL-FLD-GSI.
           PERFORM VAL-GDG-VCB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-VCB-LEN IN GSI-FLD
                GO TO FIN-VAL-FLD-GSI.
           PERFORM VAL-FRM-GLS-VCB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-VCB-LEN IN GSI-FLD
                GO TO FIN-VAL-FLD-GSI.
           PERFORM VAL-GDG-DES-1.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-DES-1-LEN IN GSI-FLD
                GO TO FIN-VAL-FLD-GSI.
           PERFORM VAL-GDG-DES-2.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GDG-DES-2-LEN IN GSI-FLD
                GO TO FIN-VAL-FLD-GSI.
           PERFORM VAL-GSI-FEC-SIN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GSI-FEC-SIN-LEN IN GSI-FLD
                GO TO FIN-VAL-FLD-GSI.
           PERFORM VAL-GSI-PCT-SIN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GSI-PCT-SIN-LEN IN GSI-FLD
                GO TO FIN-VAL-FLD-GSI.
           PERFORM VAL-GSI-IND-CUB-SEG.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GSI-IND-CUB-SEG-LEN IN GSI-FLD
                GO TO FIN-VAL-FLD-GSI.
           PERFORM VAL-GSI-VAL-PAG-SEG.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GSI-VAL-PAG-SEG-LEN IN GSI-FLD
                GO TO FIN-VAL-FLD-GSI.
           PERFORM VAL-GSI-DST-IDM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GSI-DST-IDM-LEN IN GSI-FLD
                GO TO FIN-VAL-FLD-GSI.
           PERFORM VAL-GSI-IND-RTS.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO GSI-IND-RTS-LEN IN GSI-FLD
                GO TO FIN-VAL-FLD-GSI.
       FIN-VAL-FLD-GSI.
           EXIT.
       
       VAL-FRM-GLS-TIP-REA-NRE SECTION.
       INI-VAL-FRM-GLS-TIP-REA-NRE.
       FIN-VAL-FRM-GLS-TIP-REA-NRE.
           EXIT.
       
       VAL-GDG-TIP-OPE SECTION.
       INI-VAL-GDG-TIP-OPE.
       FIN-VAL-GDG-TIP-OPE.
           EXIT.
       
       VAL-GDG-STP-OPE SECTION.
       INI-VAL-GDG-STP-OPE.
       FIN-VAL-GDG-STP-OPE.
           EXIT.
       
       VAL-FRM-GLS-TIP-OPE SECTION.
       INI-VAL-FRM-GLS-TIP-OPE.
       FIN-VAL-FRM-GLS-TIP-OPE.
           EXIT.
       
       VAL-FRM-GLS-STP-OPE SECTION.
       INI-VAL-FRM-GLS-STP-OPE.
       FIN-VAL-FRM-GLS-STP-OPE.
           EXIT.
       
       VAL-GDG-COD-OFI SECTION.
       INI-VAL-GDG-COD-OFI.
       FIN-VAL-GDG-COD-OFI.
           EXIT.
       
       VAL-FRM-GLS-OFI SECTION.
       INI-VAL-FRM-GLS-OFI.
       FIN-VAL-FRM-GLS-OFI.
           EXIT.
       
       VAL-GDG-VCB SECTION.
       INI-VAL-GDG-VCB.
       FIN-VAL-GDG-VCB.
           EXIT.
       
       VAL-FRM-GLS-VCB SECTION.
       INI-VAL-FRM-GLS-VCB.
       FIN-VAL-FRM-GLS-VCB.
           EXIT.
       
       VAL-GDG-DES-1 SECTION.
       INI-VAL-GDG-DES-1.
       FIN-VAL-GDG-DES-1.
           EXIT.
       
       VAL-GDG-DES-2 SECTION.
       INI-VAL-GDG-DES-2.
       FIN-VAL-GDG-DES-2.
           EXIT.
       
       VAL-GSI-FEC-SIN SECTION.
       INI-VAL-GSI-FEC-SIN.
      *VAL-FEC Validar Fecha
           MOVE GSI-FEC-SIN IN GSI-FLD TO FEC-FECH.
           MOVE FEC-FORM-FEC TO FEC-FORM.
           MOVE FEC-FBLK-N  TO FEC-FBLK.
           MOVE FEC-CHOY-LE TO FEC-CHOY.
           MOVE FEC-VHBL-S TO FEC-VHBL.
           MOVE FEC-VAL-FEC TO FEC-CMND.
           PERFORM PRO-FEC.
           IF FEC-STAT-OKS
               MOVE FEC-FECH TO GSI-FEC-SIN IN GSI-FLD
           ELSE
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE FEC-MENS     TO FRM-MENS
               GO TO FIN-VAL-GSI-FEC-SIN.
      *<<<< GSI_FEC_SIN

      *VALIDA CON FECHA CONSTITUCION GARANTIA.
           MOVE GSI-NUM-SIS IN GSI-FLD TO GAR-NUM-SIS IN GDG.
           MOVE GSI-COD-CRR IN GSI-FLD TO GAR-COD-CRR IN GDG.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GAR-FIO-GDG.

      *    MOVE '01011937' TO FEC-FEC1.
           MOVE GAR-FA-ESC-CNS IN GDG TO FEC-FANO IN FEC-VARI.
           MOVE GAR-FM-ESC-CNS IN GDG TO FEC-FMES IN FEC-VARI.
           MOVE GAR-FD-ESC-CNS IN GDG TO FEC-FDIA IN FEC-VARI.
           MOVE FEC-SVLD TO FEC-SEC1.
           MOVE FEC-AVLD TO FEC-AEC1.
           MOVE FEC-MVLD TO FEC-MEC1.
           MOVE FEC-DVLD TO FEC-DEC1.

           MOVE GSI-FA-SIN IN GSI-FLD TO FEC-FANO IN FEC-VARI.
           MOVE GSI-FM-SIN IN GSI-FLD TO FEC-FMES IN FEC-VARI.
           MOVE GSI-FD-SIN IN GSI-FLD TO FEC-FDIA IN FEC-VARI.
           MOVE FEC-SVLD TO FEC-SEC2.
           MOVE FEC-AVLD TO FEC-AEC2.
           MOVE FEC-MVLD TO FEC-MEC2.
           MOVE FEC-DVLD TO FEC-DEC2.

           MOVE FEC-FORM-FEC TO FEC-FORM.
           MOVE FEC-CMP-FEC TO FEC-CMND.
           PERFORM PRO-FEC.
           IF FEC-STAT-GRT
      *<<<< NOT PGM_ARG
               MOVE 'FEC<MIN' TO MSG-COD-MENS
      *>>>>
               GO TO ERR-VAL-GSI-FEC-SIN.

           GO TO FIN-VAL-GSI-FEC-SIN.
       ERR-VAL-GSI-FEC-SIN.
           MOVE 'GAR'     TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>>
       FIN-VAL-GSI-FEC-SIN.
           EXIT.
       
       VAL-GSI-PCT-SIN SECTION.
       INI-VAL-GSI-PCT-SIN.
           IF GSI-PCT-SIN IN GSI-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-GSI-PCT-SIN.
      *<<<< GSI_PCT_SIN
           IF NOT ( GSI-PCT-SIN IN GSI-FLD > ZEROES AND 
                    GSI-PCT-SIN IN GSI-FLD < 100,01 )
               MOVE 'GAR'     TO MSG-COD-SIST
               MOVE '% MALO' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>>
       FIN-VAL-GSI-PCT-SIN.
           EXIT.
       
       VAL-GSI-IND-CUB-SEG SECTION.
       INI-VAL-GSI-IND-CUB-SEG.
           IF GSI-IND-CUB-SEG IN GSI-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-GSI-IND-CUB-SEG.
      *<<<< GSI_IND_CUB_SEG
           IF NOT (GSI-IND-CUB-SEG = 'S' OR 'N')
               MOVE 'DEBE SER S o N ' TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>>
       FIN-VAL-GSI-IND-CUB-SEG.
           EXIT.
       
       VAL-GSI-VAL-PAG-SEG SECTION.
       INI-VAL-GSI-VAL-PAG-SEG.
           IF GSI-VAL-PAG-SEG IN GSI-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-GSI-VAL-PAG-SEG.
      *<<<< GSI_VAL_PAG_SEG
           IF GSI-IND-CUB-SEG IN GSI-FLD NOT = 'S'
               MOVE 0 TO GSI-VAL-PAG-SEG IN GSI-FLD
               GO TO FIN-VAL-GSI-VAL-PAG-SEG.

           IF GSI-VAL-PAG-SEG IN GSI-FLD = ZEROES
               MOVE 'GAR'     TO MSG-COD-SIST
               MOVE 'GSIVALPAG=0' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>>
       FIN-VAL-GSI-VAL-PAG-SEG.
           EXIT.
       
       VAL-GSI-DST-IDM SECTION.
       INI-VAL-GSI-DST-IDM.
           IF GSI-DST-IDM IN GSI-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-GSI-DST-IDM.
       FIN-VAL-GSI-DST-IDM.
           EXIT.
       
       VAL-GSI-IND-RTS SECTION.
       INI-VAL-GSI-IND-RTS.
           IF GSI-IND-RTS IN GSI-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-GSI-IND-RTS.
      *<<<< GSI_IND_RTS
           IF NOT (GSI-IND-RTS = 'S' OR 'N')
               MOVE 'DEBE SER S o N ' TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>>
       FIN-VAL-GSI-IND-RTS.
           EXIT.
       
       VAL-CON-FLD-GSI SECTION.
       INI-VAL-CON-FLD-GSI.
       FIN-VAL-CON-FLD-GSI.
           EXIT.
       
       VAL-CON-GSI SECTION.
       INI-VAL-CON-GSI.
       FIN-VAL-CON-GSI.
           EXIT.
       
       VAL-NUM-KEY-GSI SECTION.
       INI-VAL-NUM-KEY-GSI.
           PERFORM VAL-NUM-IKY-GSI.
           PERFORM VAL-NUM-MKY-GSI.
           PERFORM VAL-NUM-AKY-GSI.
       FIN-VAL-NUM-KEY-GSI.
           EXIT.
       
       VAL-NUM-IKY-GSI SECTION.
       INI-VAL-NUM-IKY-GSI.
       VAL-NUM-GSI-COD-CRR.
           MOVE GSI-COD-CRR-ALF IN GSI-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO GSI-COD-CRR IN GSI-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO GSI-COD-CRR-LEN IN GSI-FLD
                GO TO FIN-VAL-NUM-IKY-GSI.
       FIN-VAL-NUM-IKY-GSI.
           EXIT.
       
       VAL-NUM-MKY-GSI SECTION.
       INI-VAL-NUM-MKY-GSI.
       FIN-VAL-NUM-MKY-GSI.
           EXIT.
       
       VAL-NUM-AKY-GSI SECTION.
       INI-VAL-NUM-AKY-GSI.
       FIN-VAL-NUM-AKY-GSI.
           EXIT.
       
       VAL-NUM-FLD-GSI SECTION.
       INI-VAL-NUM-FLD-GSI.
       VAL-NUM-GSI-PCT-SIN.
           MOVE GSI-PCT-SIN-ALF IN GSI-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 2 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9V TO GSI-PCT-SIN IN GSI-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO GSI-PCT-SIN-LEN IN GSI-FLD
                GO TO FIN-VAL-NUM-FLD-GSI.
       VAL-NUM-GSI-VAL-PAG-SEG.
           MOVE GSI-VAL-PAG-SEG-ALF IN GSI-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9V TO GSI-VAL-PAG-SEG IN GSI-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO GSI-VAL-PAG-SEG-LEN IN GSI-FLD
                GO TO FIN-VAL-NUM-FLD-GSI.
       FIN-VAL-NUM-FLD-GSI.
           EXIT.
       
       EDT-KEY-GSI SECTION.
       INI-EDT-KEY-GSI.
           PERFORM EDT-IKY-GSI.
           PERFORM EDT-MKY-GSI.
           PERFORM EDT-AKY-GSI.
       FIN-EDT-KEY-GSI.
           EXIT.
       
       EDT-IKY-GSI SECTION.
       INI-EDT-IKY-GSI.
           IF GSI-COD-CRR-LEN IN GSI-FLD = -9
                MOVE -1 TO GSI-COD-CRR-LEN IN GSI-FLD
                GO TO FIN-EDT-IKY-GSI.
           MOVE GSI-COD-CRR IN GSI-FLD TO GSI-COD-CRR-EDT IN GSI-EDT.
           MOVE GSI-COD-CRR-EDT IN GSI-EDT TO 
                GSI-COD-CRR-ALF IN GSI-FLD.
       FIN-EDT-IKY-GSI.
           EXIT.
       
       EDT-MKY-GSI SECTION.
       INI-EDT-MKY-GSI.
       FIN-EDT-MKY-GSI.
           EXIT.
       
       EDT-AKY-GSI SECTION.
       INI-EDT-AKY-GSI.
       FIN-EDT-AKY-GSI.
           EXIT.
       
       EDT-FLD-GSI SECTION.
       INI-EDT-FLD-GSI.
           IF GSI-PCT-SIN-LEN IN GSI-FLD = -9
                MOVE -1 TO GSI-PCT-SIN-LEN IN GSI-FLD
                GO TO FIN-EDT-FLD-GSI.
           MOVE GSI-PCT-SIN IN GSI-FLD TO GSI-PCT-SIN-EDT IN GSI-EDT.
           MOVE GSI-PCT-SIN-EDT IN GSI-EDT TO 
                GSI-PCT-SIN-ALF IN GSI-FLD.
           IF GSI-VAL-PAG-SEG-LEN IN GSI-FLD = -9
                MOVE -1 TO GSI-VAL-PAG-SEG-LEN IN GSI-FLD
                GO TO FIN-EDT-FLD-GSI.
           MOVE GSI-VAL-PAG-SEG IN GSI-FLD TO 
                GSI-VAL-PAG-SEG-EDT IN GSI-EDT.
           MOVE GSI-VAL-PAG-SEG-EDT IN GSI-EDT TO 
                GSI-VAL-PAG-SEG-ALF IN GSI-FLD.
       FIN-EDT-FLD-GSI.
           EXIT.
