           EXIT.
       
       VAL-FRM-VAL-CMLC SECTION.
       INI-VAL-FRM-VAL-CMLC.
       FIN-VAL-FRM-VAL-CMLC.
           EXIT.
       
       VAL-FRM-VAL-CDLC SECTION.
       INI-VAL-FRM-VAL-CDLC.
       FIN-VAL-FRM-VAL-CDLC.
           EXIT.
       
       VAL-CON-KEY-CLC SECTION.
       INI-VAL-CON-KEY-CLC.
      *<<<< INI_VAL_CON_KEY_CLC
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6
               GO TO FIN-VAL-CON-KEY-CLC.
      *>>>>
           IF     DBC-GLS-NOMC IN CLC-FLD NOT > SPACES 
              AND DBC-NUM-ICLI IN CLC-FLD NOT > ZEROES 
              AND DBC-VRF-ICLI IN CLC-FLD NOT > SPACES 
              AND DBC-IND-ICLI IN CLC-FLD NOT > SPACES 
              AND DBC-GLS-ICLI IN CLC-FLD NOT > SPACES 
              AND CLF-CAI-ICLF IN CLC-FLD NOT > SPACES 
              AND CLF-IIC-ICLF IN CLC-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CLC.
      *<<<< FIN_VAL_CON_KEY_CLC
           IF NOT ( DBC-NUM-ICLI IN CLC-FLD > ZEROES OR
                    DBC-GLS-NOMC IN CLC-FLD > SPACES OR
                    CLF-IIC-ICLF IN CLC-FLD > ZEROES )
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'VALCONKEY' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CON-KEY-CLC.
      *>>>>
       FIN-VAL-CON-KEY-CLC.
           EXIT.
       
       VAL-CON-IKY-CLC SECTION.
       INI-VAL-CON-IKY-CLC.
           IF     CLF-CAI-ICLF IN CLC-FLD NOT > SPACES 
              AND CLF-IIC-ICLF IN CLC-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CLC.
       FIN-VAL-CON-IKY-CLC.
           EXIT.
       
       VAL-CON-FLD-CLC SECTION.
       INI-VAL-CON-FLD-CLC.
       FIN-VAL-CON-FLD-CLC.
           EXIT.
       
       VAL-CON-CLC SECTION.
       INI-VAL-CON-CLC.
       FIN-VAL-CON-CLC.
           EXIT.
       
       VAL-NUM-KEY-CLC SECTION.
       INI-VAL-NUM-KEY-CLC.
           PERFORM VAL-NUM-IKY-CLC.
           PERFORM VAL-NUM-MKY-CLC.
           PERFORM VAL-NUM-AKY-CLC.
       FIN-VAL-NUM-KEY-CLC.
           EXIT.
       
       VAL-NUM-IKY-CLC SECTION.
       INI-VAL-NUM-IKY-CLC.
       FIN-VAL-NUM-IKY-CLC.
           EXIT.
       
       VAL-NUM-MKY-CLC SECTION.
       INI-VAL-NUM-MKY-CLC.
       FIN-VAL-NUM-MKY-CLC.
           EXIT.
       
       VAL-NUM-AKY-CLC SECTION.
       INI-VAL-NUM-AKY-CLC.
       VAL-NUM-DBC-NUM-ICLI.
           MOVE DBC-NUM-ICLI-ALF IN CLC-FLD TO FRM-ANFL.
           MOVE 8 TO FRM-J.
           MOVE 8 TO FRM-K.
       LU1-VAL-NUM-DBC-NUM-ICLI-0.
           IF FRM-ANFL-CHR( FRM-J ) NOT > SPACES
               SUBTRACT 1 FROM FRM-J
               IF FRM-J > 0
                   GO TO LU1-VAL-NUM-DBC-NUM-ICLI-0.
       LU2-VAL-NUM-DBC-NUM-ICLI-0.
           IF ( FRM-J > 0 ) AND ( FRM-J NOT = 8 )
               MOVE FRM-ANFL-CHR( FRM-J ) TO FRM-ANFL-CHR( FRM-K )
               SUBTRACT 1 FROM FRM-J
               SUBTRACT 1 FROM FRM-K
               GO TO LU2-VAL-NUM-DBC-NUM-ICLI-0.
       LU3-VAL-NUM-DBC-NUM-ICLI-0.
           IF FRM-K > 0 AND FRM-K NOT = 8
               MOVE SPACES TO FRM-ANFL-CHR( FRM-K )
               SUBTRACT 1 FROM FRM-K
               GO TO LU3-VAL-NUM-DBC-NUM-ICLI-0.
           MOVE FRM-ANFL TO DBC-NUM-ICLI-ALF IN CLC-FLD.

           INSPECT DBC-NUM-ICLI-ALF IN CLC-FLD
                   REPLACING LEADING SPACES BY ZEROES.
           IF DBC-NUM-ICLI-ALF IN CLC-FLD IS NOT NUMERIC
               MOVE 'FORMATO NUMERICO INVALIDO' TO FRM-MENS
               MOVE -9 TO DBC-NUM-ICLI-LEN IN CLC-FLD
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
           IF FRM-SUAR = FRM-SUAR-MAL OR 
              FRM-SUAR = FRM-SUAR-CLR
                GO TO FIN-VAL-NUM-AKY-CLC.
       FIN-VAL-NUM-AKY-CLC.
           EXIT.
       
       VAL-NUM-FLD-CLC SECTION.
       INI-VAL-NUM-FLD-CLC.
       FIN-VAL-NUM-FLD-CLC.
           EXIT.
