       COPY GNSWGSYS.
       01  PGM-STAT.
           03 PGM-STAT-SRT            VALUE ' ' PIC X(01).
              88 PGM-STAT-SRT-OKS     VALUE ' '.
              88 PGM-STAT-SRT-MAL     VALUE 'M'.
           03 PGM-SOKS                VALUE ' ' PIC X(01).
           03 PGM-SMAL                VALUE 'M' PIC X(01).
      *<<<< WSS
       COPY TABBRUSR.
      *>>>>
      *<<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-USR-REQA==.
      *>>>>

       REPORT SECTION.
      *--------------
       COPY TABRFPZA.
           .
       COPY TABRTPZA.

       PROCEDURE DIVISION.
      *==================
       DECLARATIVES.
       DEC-PH-PZA SECTION.
           USE BEFORE REPORTING PH-PZA.
       INI-PH-PZA.
       FIN-PH-PZA.
           EXIT.
       END DECLARATIVES.

       MAIN SECTION.
      *------------
       INI-MAIN.
       COPY GNSBGEDB.
           MOVE 'TABPLPZA' TO FIO-PROG.
           PERFORM GNS-BUS-IDD.
       INI-INP-SRT.
           CALL 'GNSPLHDR' USING RPT-VARI.
           MOVE FIO-INP TO FIO-CMND.
           PERFORM TAB-FIO-PZA.
       FST-INP-SRT.
      *<<<< INI_FST_INP
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR DE APERTURA EN ARCHIVO PZA ' FIO-STAT
               GOBACK.
           GO TO INI-OUT-SRT.
      *>>>>
           MOVE FIO-GET-FST TO FIO-CMND.
           PERFORM TAB-FIO-PZA.
       INI-OUT-SRT.
      *<<<< INI_OUT
           MOVE LOW-VALUES TO PZA.
           MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM TAB-FIO-PZA.
           IF NOT FIO-STAT-OKS OR
              PZA-COD-TTAB IN PZA NOT = 'PZA' OR
              PZA-COD-PLZA IN PZA > SPACES
               MOVE 'TAB'        TO MSG-COD-SIST
               MOVE 'TAB    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               DISPLAY MSG-GLS-DESC
               GOBACK.
           MOVE PZA-GLS-DESC IN PZA TO WSS-GLS-DTAB.
           IF PZA-COD-ATRN IN PZA > SPACES
               MOVE FIO-INP TO FIO-CMND
               PERFORM TAB-FIO-USR
               MOVE PZA-COD-ATRN IN PZA TO USR-COD-USER IN USR
               PERFORM BUS-USR
               IF NOT FIO-STAT-OKS
                   MOVE '00'   TO FIO-STAT
                   MOVE SPACES TO USR-GLS-DESC IN USR
               ELSE
                   NEXT SENTENCE      
           ELSE
               MOVE SPACES TO USR-GLS-DESC.
           MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM TAB-FIO-PZA.
      *>>>>
           OPEN OUTPUT RPTPZA.
           INITIATE RPT-PZA.
       LUP-MAIN.
       LUP-INP-SRT.
           IF NOT FIO-STAT-OKS
                GO TO FIN-MAIN.
      *<<<< LUP_INP
           IF PZA-COD-TTAB > 'PZA'
               GO TO FIN-INP-SRT.
      *>>>>
       MOV-INP-SRT.
       COPY TABRMPZA.
       LUP-OUT-SRT.
      *<<<< LUP_OUT
           IF PZA-COD-CBCO > SPACES
               MOVE 'TAB'        TO TAB-COD-SIST
               MOVE 'BCO'        TO TAB-COD-TTAB
               MOVE PZA-COD-CBCO TO TAB-COD-CTAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC TO WSS-GLS-CBCO
           ELSE
               MOVE SPACES TO WSS-GLS-CBCO.
      *>>>>
           GENERATE DL-PZA.
       NXT-INP-SRT.
       NXT-OUT-SRT.
           MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM TAB-FIO-PZA.
           GO TO LUP-MAIN.
       FIN-MAIN.
       FIN-INP-SRT.
           MOVE FIO-CLO TO FIO-CMND.
           PERFORM TAB-FIO-PZA.
       FIN-OUT-SRT.
           TERMINATE RPT-PZA.
           CLOSE RPTPZA.
           IF PGM-STAT-SRT-MAL
               PERFORM GNS-PRO-ABT
           ELSE
               PERFORM GNS-PRO-END.

       COPY TABBFPZA.
       COPY GNSBGDTC.
       COPY GNSBGIDD.
       COPY GNSBTABT.
       COPY GNSBBSYS.
       COPY GNSBFTAB.
       COPY GNSBFMSG.
       COPY GNSBGEND.
       COPY GNSBGABT.

