      * 30-Dec-92 01:00 PM
       PUT-ALL-RES SECTION.
       INI-PUT-ALL-RES.
           PERFORM PUT-TAB-RES.
           PERFORM PUT-USR-RES.
      *<<<< FIN_PUT_ALL_RES
           IF TAB-COD-ATRN IN TAB > SPACES
               MOVE TAB-COD-ATRN IN TAB TO USR-COD-USER IN USR
               PERFORM BUS-USR
               MOVE USR-GLS-DESC IN USR TO FRM-GLS-USER IN RES-FLD.
      *>>>>
       FIN-PUT-ALL-RES.
           EXIT.
       
       PUT-TAB-RES SECTION.
       INI-PUT-TAB-RES.
           IF PGM-STAT-TAB-NUL
               GO TO FIN-PUT-TAB-RES.
           IF NOT PGM-STAT-TAB-OKS
               PERFORM CLR-TAB-RES
               GO TO FIN-PUT-TAB-RES.
           MOVE TAB-COD-TABL IN TAB TO TAB-COD-TABL IN RES-FLD.
           MOVE TAB-GLS-DESC IN TAB TO TAB-GLS-DESC IN RES-FLD.
           MOVE TAB-COD-ATRN IN TAB TO TAB-COD-ATRN IN RES-FLD.
           MOVE TAB-NUM-DTRN IN TAB TO TAB-NUM-DTRN IN RES-FLD.
           MOVE TAB-NUM-MTRN IN TAB TO TAB-NUM-MTRN IN RES-FLD.
           MOVE TAB-NUM-STRN IN TAB TO TAB-NUM-STRN IN RES-FLD.
           MOVE TAB-NUM-ATRN IN TAB TO TAB-NUM-ATRN IN RES-FLD.
       FIN-PUT-TAB-RES.
           EXIT.
       
       PUT-USR-RES SECTION.
       INI-PUT-USR-RES.
           IF PGM-STAT-USR-NUL
               GO TO FIN-PUT-USR-RES.
           IF NOT PGM-STAT-USR-OKS
               PERFORM CLR-USR-RES
               GO TO FIN-PUT-USR-RES.
           MOVE USR-COD-USER IN USR TO USR-COD-USER IN RES-FLD.
       FIN-PUT-USR-RES.
           EXIT.
       
       KEY-ALL-RES SECTION.
       INI-KEY-ALL-RES.
           PERFORM KEY-TAB-RES.
           PERFORM KEY-USR-RES.
       FIN-KEY-ALL-RES.
           EXIT.
       
       KEY-TAB-RES SECTION.
       INI-KEY-TAB-RES.
           MOVE TAB-COD-TABL IN RES-FLD TO TAB-COD-TABL IN TAB.
       FIN-KEY-TAB-RES.
           EXIT.
       
       KEY-USR-RES SECTION.
       INI-KEY-USR-RES.
           MOVE USR-COD-USER IN RES-FLD TO USR-COD-USER IN USR.
       FIN-KEY-USR-RES.
           EXIT.
       
       SCR-CLRA-RES SECTION.
       INI-SCR-CLRA-RES.
           PERFORM CLR-KEY-RES.
           PERFORM CLR-FLD-RES.
       FIN-SCR-CLRA-RES.
           EXIT.
       
       CLR-KEY-RES SECTION.
       INI-CLR-KEY-RES.
           MOVE SPACES TO TAB-COD-TABL IN RES-FLD.
           MOVE SPACES TO USR-COD-USER IN RES-FLD.
       FIN-CLR-KEY-RES.
           EXIT.
       
       CLR-FLD-RES SECTION.
       INI-CLR-FLD-RES.
           MOVE SPACES TO FRM-GLS-USER IN RES-FLD.
           MOVE SPACES TO FRM-GLS-DUSR IN RES-FLD.
           PERFORM CLR-TAB-RES.
           PERFORM CLR-USR-RES.
       FIN-CLR-FLD-RES.
           EXIT.
       
       CLR-TAB-RES SECTION.
       INI-CLR-TAB-RES.
           MOVE SPACES TO TAB-GLS-DESC IN RES-FLD.
           MOVE SPACES TO TAB-COD-ATRN IN RES-FLD.
           MOVE SPACES TO TAB-FEC-FTRN IN RES-FLD.
       FIN-CLR-TAB-RES.
           EXIT.
       
       CLR-USR-RES SECTION.
       INI-CLR-USR-RES.
       FIN-CLR-USR-RES.
           EXIT.
       
       FST-KEY-RES SECTION.
       INI-FST-KEY-RES.
           MOVE -1 TO TAB-COD-TABL-LEN IN RES-FLD.
       FIN-FST-KEY-RES.
           EXIT.
       
       FST-FLD-RES SECTION.
       INI-FST-FLD-RES.
       FIN-FST-FLD-RES.
           EXIT.
       
       PRO-KEY-RES SECTION.
       INI-PRO-KEY-RES.
           PERFORM PRO-IKY-RES.
           PERFORM PRO-AKY-RES.
           PERFORM PRO-MKY-RES.
       FIN-PRO-KEY-RES.
           EXIT.
       
       PRO-IKY-RES SECTION.
       INI-PRO-IKY-RES.
           MOVE FRM-CPIM-AEY TO TAB-COD-TABL-ATR IN RES-FLD.
           MOVE FRM-CPIM-AEY TO USR-COD-USER-ATR IN RES-FLD.
       FIN-PRO-IKY-RES.
           EXIT.
       
       PRO-MKY-RES SECTION.
       INI-PRO-MKY-RES.
       FIN-PRO-MKY-RES.
           EXIT.
       
       PRO-AKY-RES SECTION.
       INI-PRO-AKY-RES.
       FIN-PRO-AKY-RES.
           EXIT.
       
       UNP-KEY-RES SECTION.
       INI-UNP-KEY-RES.
           PERFORM UNP-IKY-RES.
           PERFORM UNP-AKY-RES.
           PERFORM UNP-MKY-RES.
       FIN-UNP-KEY-RES.
           EXIT.
       
       UNP-IKY-RES SECTION.
       INI-UNP-IKY-RES.
           MOVE FRM-CPIM-UBY TO TAB-COD-TABL-ATR.
           MOVE FRM-CPIM-UBY TO USR-COD-USER-ATR.
       FIN-UNP-IKY-RES.
           EXIT.
       
       UNP-MKY-RES SECTION.
       INI-UNP-MKY-RES.
       FIN-UNP-MKY-RES.
           EXIT.
       
       UNP-AKY-RES SECTION.
       INI-UNP-AKY-RES.
       FIN-UNP-AKY-RES.
           EXIT.
       
       UNP-FLD-RES SECTION.
       INI-UNP-FLD-RES.
           PERFORM UNP-TAB-RES.
           PERFORM UNP-USR-RES.
       FIN-UNP-FLD-RES.
           EXIT.
       
       UNP-TAB-RES SECTION.
       INI-UNP-TAB-RES.
       FIN-UNP-TAB-RES.
           EXIT.
       
       UNP-USR-RES SECTION.
       INI-UNP-USR-RES.
       FIN-UNP-USR-RES.
           EXIT.
       
       VAL-KEY-RES SECTION.
       INI-VAL-KEY-RES.
           PERFORM VAL-IKY-RES.
           PERFORM VAL-MKY-RES.
           PERFORM VAL-AKY-RES.
       FIN-VAL-KEY-RES.
           EXIT.
       
       VAL-IKY-RES SECTION.
       INI-VAL-IKY-RES.
           PERFORM VAL-TAB-COD-TABL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO TAB-COD-TABL-LEN IN RES-FLD
                GO TO FIN-VAL-IKY-RES.
           PERFORM VAL-USR-COD-USER.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO USR-COD-USER-LEN IN RES-FLD
                GO TO FIN-VAL-IKY-RES.
       FIN-VAL-IKY-RES.
           EXIT.
       
       VAL-MKY-RES SECTION.
       INI-VAL-MKY-RES.
       FIN-VAL-MKY-RES.
           EXIT.
       
       VAL-AKY-RES SECTION.
       INI-VAL-AKY-RES.
       FIN-VAL-AKY-RES.
           EXIT.
       
       VAL-TAB-COD-TABL SECTION.
       INI-VAL-TAB-COD-TABL.
           IF TAB-COD-TABL IN RES-FLD NOT > SPACES
              MOVE SPACES TO TAB-COD-TABL IN RES-FLD.
      *<<<< TAB_COD_TABL
           IF TAB-COD-TABL IN RES-FLD NOT > SPACES
               PERFORM ERR-KEY.
      *>>>>
       FIN-VAL-TAB-COD-TABL.
           EXIT.
       
       VAL-USR-COD-USER SECTION.
       INI-VAL-USR-COD-USER.
           IF USR-COD-USER IN RES-FLD NOT > SPACES
              MOVE SPACES TO USR-COD-USER IN RES-FLD.
      *<<<< USR_COD_USER
           IF SCR-CCPP = 'ACC' AND SCR-STPP = SCR-STAT-CON
               MOVE SPACES TO FRM-GLS-DUSR IN RES-FLD
               GO TO FIN-VAL-USR-COD-USER.
           MOVE USR-COD-USER IN RES-FLD TO USR-COD-USER IN USR.
           PERFORM VAL-USR.
           IF MSG-COD-MENS > SPACES
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
           IF MSG-COD-MENS = 'COD    NEX' AND
              USR-COD-USER IN RES-FLD > SPACES
               MOVE SPACES TO FRM-GLS-DUSR IN RES-FLD
           ELSE
               MOVE USR-GLS-DESC TO FRM-GLS-DUSR IN RES-FLD.
      *>>>>
       FIN-VAL-USR-COD-USER.
           EXIT.
       
       VAL-CON-KEY-RES SECTION.
       INI-VAL-CON-KEY-RES.
           IF     TAB-COD-TABL IN RES-FLD NOT > SPACES 
              AND USR-COD-USER IN RES-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-RES.
       FIN-VAL-CON-KEY-RES.
           EXIT.
       
       VAL-CON-IKY-RES SECTION.
       INI-VAL-CON-IKY-RES.
           IF     TAB-COD-TABL IN RES-FLD NOT > SPACES 
              AND USR-COD-USER IN RES-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-RES.
       FIN-VAL-CON-IKY-RES.
           EXIT.
       
       VAL-NUM-KEY-RES SECTION.
       INI-VAL-NUM-KEY-RES.
           PERFORM VAL-NUM-IKY-RES.
           PERFORM VAL-NUM-MKY-RES.
           PERFORM VAL-NUM-AKY-RES.
       FIN-VAL-NUM-KEY-RES.
           EXIT.
       
       VAL-NUM-IKY-RES SECTION.
       INI-VAL-NUM-IKY-RES.
       FIN-VAL-NUM-IKY-RES.
           EXIT.
       
       VAL-NUM-MKY-RES SECTION.
       INI-VAL-NUM-MKY-RES.
       FIN-VAL-NUM-MKY-RES.
           EXIT.
       
       VAL-NUM-AKY-RES SECTION.
       INI-VAL-NUM-AKY-RES.
       FIN-VAL-NUM-AKY-RES.
           EXIT.
       
       VAL-NUM-FLD-RES SECTION.
       INI-VAL-NUM-FLD-RES.
       FIN-VAL-NUM-FLD-RES.
           EXIT.
       
       EDT-KEY-RES SECTION.
       INI-EDT-KEY-RES.
           PERFORM EDT-IKY-RES.
           PERFORM EDT-MKY-RES.
           PERFORM EDT-AKY-RES.
       FIN-EDT-KEY-RES.
           EXIT.
       
       EDT-IKY-RES SECTION.
       INI-EDT-IKY-RES.
       FIN-EDT-IKY-RES.
           EXIT.
       
       EDT-MKY-RES SECTION.
       INI-EDT-MKY-RES.
       FIN-EDT-MKY-RES.
           EXIT.
       
       EDT-AKY-RES SECTION.
       INI-EDT-AKY-RES.
       FIN-EDT-AKY-RES.
           EXIT.
       
       EDT-FLD-RES SECTION.
       INI-EDT-FLD-RES.
       FIN-EDT-FLD-RES.
           EXIT.
