      *<<<< BCO_GLS_SWFT
      *>>>>
       FIN-VAL-BCO-GLS-SWFT.
           EXIT.
       
       VAL-BCO-GLS-SBCO SECTION.
       INI-VAL-BCO-GLS-SBCO.
           IF BCO-GLS-SBCO IN BCO-FLD NOT > SPACES
              MOVE SPACES TO BCO-GLS-SBCO IN BCO-FLD.
      *<<<< BCO_GLS_SBCO
      *>>>>
       FIN-VAL-BCO-GLS-SBCO.
           EXIT.
       
       VAL-CON-FLD-BCO SECTION.
       INI-VAL-CON-FLD-BCO.
       FIN-VAL-CON-FLD-BCO.
           EXIT.
       
       VAL-CON-BCO SECTION.
       INI-VAL-CON-BCO.
       FIN-VAL-CON-BCO.
           EXIT.
       
       VAL-NUM-KEY-BCO SECTION.
       INI-VAL-NUM-KEY-BCO.
           PERFORM VAL-NUM-IKY-BCO.
           PERFORM VAL-NUM-MKY-BCO.
           PERFORM VAL-NUM-AKY-BCO.
       FIN-VAL-NUM-KEY-BCO.
           EXIT.
       
       VAL-NUM-IKY-BCO SECTION.
       INI-VAL-NUM-IKY-BCO.
       FIN-VAL-NUM-IKY-BCO.
           EXIT.
       
       VAL-NUM-MKY-BCO SECTION.
       INI-VAL-NUM-MKY-BCO.
       FIN-VAL-NUM-MKY-BCO.
           EXIT.
       
       VAL-NUM-AKY-BCO SECTION.
       INI-VAL-NUM-AKY-BCO.
       FIN-VAL-NUM-AKY-BCO.
           EXIT.
       
       VAL-NUM-FLD-BCO SECTION.
       INI-VAL-NUM-FLD-BCO.
       VAL-NUM-BCO-NUM-IBCO.
           MOVE BCO-NUM-IBCO-ALF IN BCO-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           CALL 'GNSPVNUM' USING NUM-VARI.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO BCO-NUM-IBCO IN BCO-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO BCO-NUM-IBCO-LEN IN BCO-FLD
                GO TO FIN-VAL-NUM-FLD-BCO.
       FIN-VAL-NUM-FLD-BCO.
           EXIT.
       
       EDT-KEY-BCO SECTION.
       INI-EDT-KEY-BCO.
           PERFORM EDT-IKY-BCO.
           PERFORM EDT-MKY-BCO.
           PERFORM EDT-AKY-BCO.
       FIN-EDT-KEY-BCO.
           EXIT.
       
       EDT-IKY-BCO SECTION.
       INI-EDT-IKY-BCO.
       FIN-EDT-IKY-BCO.
           EXIT.
       
       EDT-MKY-BCO SECTION.
       INI-EDT-MKY-BCO.
       FIN-EDT-MKY-BCO.
           EXIT.
       
       EDT-AKY-BCO SECTION.
       INI-EDT-AKY-BCO.
       FIN-EDT-AKY-BCO.
           EXIT.
       
       EDT-FLD-BCO SECTION.
       INI-EDT-FLD-BCO.
           IF BCO-NUM-IBCO-LEN IN BCO-FLD = -9
                MOVE -1 TO BCO-NUM-IBCO-LEN IN BCO-FLD
                GO TO FIN-EDT-FLD-BCO.
           MOVE BCO-NUM-IBCO IN BCO-FLD TO BCO-NUM-IBCO-EDT IN BCO-FLD.
       FIN-EDT-FLD-BCO.
           EXIT.
