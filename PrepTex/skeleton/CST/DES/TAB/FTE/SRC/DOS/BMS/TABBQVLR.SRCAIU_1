      * 30-Dec-92 01:07 PM
       PUT-ALL-VLR SECTION.
       INI-PUT-ALL-VLR.
           PERFORM PUT-VLR-VLR.
       FIN-PUT-ALL-VLR.
           EXIT.
       
       PUT-VLR-VLR SECTION.
       INI-PUT-VLR-VLR.
           IF PGM-STAT-VLR-NUL
               GO TO FIN-PUT-VLR-VLR.
           IF NOT PGM-STAT-VLR-OKS
               PERFORM CLR-VLR-VLR
               GO TO FIN-PUT-VLR-VLR.
           MOVE VLR-MSC-TVAL IN VLR TO VLR-MSC-TVAL IN VLR-FLD.
           MOVE VLR-COD-CVAL IN VLR TO VLR-COD-CVAL IN VLR-FLD.
           MOVE VLR-COD-TCAM IN VLR TO VLR-COD-TCAM IN VLR-FLD.
           MOVE VLR-CIC-TVAL IN VLR TO VLR-CIC-TVAL IN VLR-FLD.
           MOVE VLR-CIC-CVAL IN VLR TO VLR-CIC-CVAL IN VLR-FLD.
           MOVE VLR-CIC-TCAM IN VLR TO VLR-CIC-TCAM IN VLR-FLD.
           MOVE VLR-EXT-CTAB IN VLR TO VLR-EXT-CTAB IN VLR-FLD.
           MOVE VLR-GLS-DESC IN VLR TO VLR-GLS-DESC IN VLR-FLD.
           MOVE VLR-GLS-ABRV IN VLR TO VLR-GLS-ABRV IN VLR-FLD.
           MOVE VLR-GLS-DCOR IN VLR TO VLR-GLS-DCOR IN VLR-FLD.
           MOVE VLR-IND-VIGE IN VLR TO VLR-IND-VIGE IN VLR-FLD.
           MOVE VLR-NUM-DTRN IN VLR TO VLR-NUM-DTRN IN VLR-FLD.
           MOVE VLR-NUM-MTRN IN VLR TO VLR-NUM-MTRN IN VLR-FLD.
           MOVE VLR-NUM-STRN IN VLR TO VLR-NUM-STRN IN VLR-FLD.
           MOVE VLR-NUM-ATRN IN VLR TO VLR-NUM-ATRN IN VLR-FLD.
           MOVE VLR-GLS-SBCC IN VLR TO VLR-GLS-SBCC IN VLR-FLD.
           MOVE VLR-GLS-SWFT IN VLR TO VLR-GLS-SWFT IN VLR-FLD.
       FIN-PUT-VLR-VLR.
           EXIT.
       
       KEY-ALL-VLR SECTION.
       INI-KEY-ALL-VLR.
           PERFORM KEY-VLR-VLR.
       FIN-KEY-ALL-VLR.
           EXIT.
       
       KEY-VLR-VLR SECTION.
       INI-KEY-VLR-VLR.
           MOVE VLR-MSC-TVAL IN VLR-FLD TO VLR-MSC-TVAL IN VLR.
           MOVE VLR-COD-CVAL IN VLR-FLD TO VLR-COD-CVAL IN VLR.
           MOVE VLR-COD-TCAM IN VLR-FLD TO VLR-COD-TCAM IN VLR.
       FIN-KEY-VLR-VLR.
           EXIT.
       
       SCR-CLRA-VLR SECTION.
       INI-SCR-CLRA-VLR.
           PERFORM CLR-KEY-VLR.
           PERFORM CLR-FLD-VLR.
       FIN-SCR-CLRA-VLR.
           EXIT.
       
       CLR-KEY-VLR SECTION.
       INI-CLR-KEY-VLR.
           MOVE SPACES TO VLR-MSC-TVAL IN VLR-FLD.
           MOVE SPACES TO VLR-COD-CVAL IN VLR-FLD.
           MOVE SPACES TO VLR-COD-TCAM IN VLR-FLD.
       FIN-CLR-KEY-VLR.
           EXIT.
       
       CLR-FLD-VLR SECTION.
       INI-CLR-FLD-VLR.
           PERFORM CLR-VLR-VLR.
       FIN-CLR-FLD-VLR.
           EXIT.
       
       CLR-VLR-VLR SECTION.
       INI-CLR-VLR-VLR.
           MOVE SPACES TO VLR-CIC-TVAL IN VLR-FLD.
           MOVE SPACES TO VLR-CIC-CVAL IN VLR-FLD.
           MOVE SPACES TO VLR-CIC-TCAM IN VLR-FLD.
           MOVE SPACES TO VLR-EXT-CTAB IN VLR-FLD.
           MOVE SPACES TO VLR-GLS-DESC IN VLR-FLD.
           MOVE SPACES TO VLR-GLS-ABRV IN VLR-FLD.
           MOVE SPACES TO VLR-GLS-DCOR IN VLR-FLD.
           MOVE SPACES TO VLR-IND-VIGE IN VLR-FLD.
           MOVE SPACES TO VLR-FEC-FTRN IN VLR-FLD.
           MOVE SPACES TO VLR-GLS-SBCC IN VLR-FLD.
           MOVE SPACES TO VLR-GLS-SWFT IN VLR-FLD.
       FIN-CLR-VLR-VLR.
           EXIT.
       
       FST-KEY-VLR SECTION.
       INI-FST-KEY-VLR.
           MOVE -1 TO VLR-MSC-TVAL-LEN IN VLR-FLD.
       FIN-FST-KEY-VLR.
           EXIT.
       
       FST-FLD-VLR SECTION.
       INI-FST-FLD-VLR.
           MOVE -1 TO VLR-CIC-TVAL-LEN IN VLR-FLD.
       FIN-FST-FLD-VLR.
           EXIT.
       
       PRO-KEY-VLR SECTION.
       INI-PRO-KEY-VLR.
           PERFORM PRO-IKY-VLR.
           PERFORM PRO-AKY-VLR.
           PERFORM PRO-MKY-VLR.
       FIN-PRO-KEY-VLR.
           EXIT.
       
       PRO-IKY-VLR SECTION.
       INI-PRO-IKY-VLR.
           MOVE FRM-CPIM-AEY TO VLR-MSC-TVAL-ATR IN VLR-FLD.
           MOVE FRM-CPIM-AEY TO VLR-COD-CVAL-ATR IN VLR-FLD.
           MOVE FRM-CPIM-AEY TO VLR-COD-TCAM-ATR IN VLR-FLD.
       FIN-PRO-IKY-VLR.
           EXIT.
       
       PRO-MKY-VLR SECTION.
       INI-PRO-MKY-VLR.
       FIN-PRO-MKY-VLR.
           EXIT.
       
       PRO-AKY-VLR SECTION.
       INI-PRO-AKY-VLR.
       FIN-PRO-AKY-VLR.
           EXIT.
       
       UNP-KEY-VLR SECTION.
       INI-UNP-KEY-VLR.
           PERFORM UNP-IKY-VLR.
           PERFORM UNP-AKY-VLR.
           PERFORM UNP-MKY-VLR.
       FIN-UNP-KEY-VLR.
           EXIT.
       
       UNP-IKY-VLR SECTION.
       INI-UNP-IKY-VLR.
           MOVE FRM-CPIM-UBY TO VLR-MSC-TVAL-ATR.
           MOVE FRM-CPIM-UBY TO VLR-COD-CVAL-ATR.
           MOVE FRM-CPIM-UBY TO VLR-COD-TCAM-ATR.
       FIN-UNP-IKY-VLR.
           EXIT.
       
       UNP-MKY-VLR SECTION.
       INI-UNP-MKY-VLR.
       FIN-UNP-MKY-VLR.
           EXIT.
       
       UNP-AKY-VLR SECTION.
       INI-UNP-AKY-VLR.
       FIN-UNP-AKY-VLR.
           EXIT.
       
       UNP-FLD-VLR SECTION.
       INI-UNP-FLD-VLR.
           PERFORM UNP-VLR-VLR.
       FIN-UNP-FLD-VLR.
           EXIT.
       
       UNP-VLR-VLR SECTION.
       INI-UNP-VLR-VLR.
           MOVE FRM-CPIM-UBY TO VLR-CIC-TVAL-ATR IN VLR-FLD.
           MOVE FRM-CPIM-UBY TO VLR-CIC-CVAL-ATR IN VLR-FLD.
           MOVE FRM-CPIM-UBY TO VLR-CIC-TCAM-ATR IN VLR-FLD.
           MOVE FRM-CPIM-UBY TO VLR-EXT-CTAB-ATR IN VLR-FLD.
           MOVE FRM-CPIM-UBY TO VLR-GLS-DESC-ATR IN VLR-FLD.
           MOVE FRM-CPIM-UBY TO VLR-GLS-ABRV-ATR IN VLR-FLD.
           MOVE FRM-CPIM-UBY TO VLR-GLS-DCOR-ATR IN VLR-FLD.
           MOVE FRM-CPIM-UBY TO VLR-IND-VIGE-ATR IN VLR-FLD.
           MOVE FRM-CPIM-UBY TO VLR-GLS-SBCC-ATR IN VLR-FLD.
           MOVE FRM-CPIM-UBY TO VLR-GLS-SWFT-ATR IN VLR-FLD.
       FIN-UNP-VLR-VLR.
           EXIT.
       
       VAL-KEY-VLR SECTION.
       INI-VAL-KEY-VLR.
           PERFORM VAL-IKY-VLR.
           PERFORM VAL-MKY-VLR.
           PERFORM VAL-AKY-VLR.
       FIN-VAL-KEY-VLR.
           EXIT.
       
       VAL-IKY-VLR SECTION.
       INI-VAL-IKY-VLR.
           PERFORM VAL-VLR-MSC-TVAL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO VLR-MSC-TVAL-LEN IN VLR-FLD
                GO TO FIN-VAL-IKY-VLR.
           PERFORM VAL-VLR-COD-CVAL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO VLR-COD-CVAL-LEN IN VLR-FLD
                GO TO FIN-VAL-IKY-VLR.
           PERFORM VAL-VLR-COD-TCAM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO VLR-COD-TCAM-LEN IN VLR-FLD
                GO TO FIN-VAL-IKY-VLR.
       FIN-VAL-IKY-VLR.
           EXIT.
       
       VAL-MKY-VLR SECTION.
       INI-VAL-MKY-VLR.
       FIN-VAL-MKY-VLR.
           EXIT.
       
       VAL-AKY-VLR SECTION.
       INI-VAL-AKY-VLR.
       FIN-VAL-AKY-VLR.
           EXIT.
       
       VAL-VLR-MSC-TVAL SECTION.
       INI-VAL-VLR-MSC-TVAL.
           IF VLR-MSC-TVAL IN VLR-FLD NOT > SPACES
              MOVE SPACES TO VLR-MSC-TVAL IN VLR-FLD.
      *<<<< VLR_MSC_TVAL
           IF VLR-MSC-TVAL IN VLR-FLD > SPACES
               MOVE 'TAB' TO MSC-COD-SIST
               MOVE 'TVAL' TO MSC-COD-TMSC IN MSC
               MOVE VLR-MSC-TVAL IN VLR-FLD TO MSC-COD-CMSC IN MSC
               PERFORM VAL-IND
           ELSE
               PERFORM ERR-KEY.
      *>>>>
       FIN-VAL-VLR-MSC-TVAL.
           EXIT.
       
       VAL-VLR-COD-CVAL SECTION.
       INI-VAL-VLR-COD-CVAL.
           IF VLR-COD-CVAL IN VLR-FLD NOT > SPACES
              MOVE SPACES TO VLR-COD-CVAL IN VLR-FLD.
      *<<<< VLR_COD_CVAL
           IF VLR-COD-CVAL IN VLR-FLD > SPACES
               IF VLR-COD-TCAM IN VLR-FLD >  SPACES
                   MOVE 'TAB' TO TAB-COD-SIST
                   MOVE 'VLR' TO TAB-COD-TTAB IN TAB
                   MOVE VLR-MSC-TVAL IN VLR-FLD TO VLR-MSC-TVAL IN VLR
                   MOVE VLR-COD-CVAL IN VLR-FLD TO VLR-COD-CVAL IN VLR
                   MOVE SPACES                  TO VLR-COD-TCAM IN VLR
                   MOVE VLR-COD-VCAM IN VLR     TO TAB-COD-CTAB IN TAB
                   PERFORM VAL-COD
               ELSE
                   MOVE SPACES TO VLR-COD-TCAM IN VLR-FLD
           ELSE
               IF VLR-COD-TCAM IN VLR-FLD > SPACES
      *GET-MSG Busca mensaje en tabla
                   MOVE 'GNS' TO MSG-COD-SIST
                   MOVE 'VALCONKEY' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>>
       FIN-VAL-VLR-COD-CVAL.
           EXIT.
       
       VAL-VLR-COD-TCAM SECTION.
       INI-VAL-VLR-COD-TCAM.
           IF VLR-COD-TCAM IN VLR-FLD NOT > SPACES
              MOVE SPACES TO VLR-COD-TCAM IN VLR-FLD.
      *<<<< VLR_COD_TCAM
      *>>>>
       FIN-VAL-VLR-COD-TCAM.
           EXIT.
       
       VAL-CON-KEY-VLR SECTION.
       INI-VAL-CON-KEY-VLR.
           IF     VLR-MSC-TVAL IN VLR-FLD NOT > SPACES 
              AND VLR-COD-CVAL IN VLR-FLD NOT > SPACES 
              AND VLR-COD-TCAM IN VLR-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-VLR.
       FIN-VAL-CON-KEY-VLR.
           EXIT.
       
       VAL-CON-IKY-VLR SECTION.
       INI-VAL-CON-IKY-VLR.
           IF     VLR-MSC-TVAL IN VLR-FLD NOT > SPACES 
              AND VLR-COD-CVAL IN VLR-FLD NOT > SPACES 
              AND VLR-COD-TCAM IN VLR-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-VLR.
       FIN-VAL-CON-IKY-VLR.
           EXIT.
       
       VAL-NUM-KEY-VLR SECTION.
       INI-VAL-NUM-KEY-VLR.
           PERFORM VAL-NUM-IKY-VLR.
           PERFORM VAL-NUM-MKY-VLR.
           PERFORM VAL-NUM-AKY-VLR.
       FIN-VAL-NUM-KEY-VLR.
           EXIT.
       
       VAL-NUM-IKY-VLR SECTION.
       INI-VAL-NUM-IKY-VLR.
       FIN-VAL-NUM-IKY-VLR.
           EXIT.
       
       VAL-NUM-MKY-VLR SECTION.
       INI-VAL-NUM-MKY-VLR.
       FIN-VAL-NUM-MKY-VLR.
           EXIT.
       
       VAL-NUM-AKY-VLR SECTION.
       INI-VAL-NUM-AKY-VLR.
       FIN-VAL-NUM-AKY-VLR.
           EXIT.
       
       VAL-NUM-FLD-VLR SECTION.
       INI-VAL-NUM-FLD-VLR.
       FIN-VAL-NUM-FLD-VLR.
           EXIT.
       
       EDT-KEY-VLR SECTION.
       INI-EDT-KEY-VLR.
           PERFORM EDT-IKY-VLR.
           PERFORM EDT-MKY-VLR.
           PERFORM EDT-AKY-VLR.
       FIN-EDT-KEY-VLR.
           EXIT.
       
       EDT-IKY-VLR SECTION.
       INI-EDT-IKY-VLR.
       FIN-EDT-IKY-VLR.
           EXIT.
       
       EDT-MKY-VLR SECTION.
       INI-EDT-MKY-VLR.
       FIN-EDT-MKY-VLR.
           EXIT.
       
       EDT-AKY-VLR SECTION.
       INI-EDT-AKY-VLR.
       FIN-EDT-AKY-VLR.
           EXIT.
       
       EDT-FLD-VLR SECTION.
       INI-EDT-FLD-VLR.
       FIN-EDT-FLD-VLR.
           EXIT.
