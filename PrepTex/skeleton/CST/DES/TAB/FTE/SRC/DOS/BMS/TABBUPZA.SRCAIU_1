       VAL-FLD-PZA SECTION.
       INI-VAL-FLD-PZA.
           PERFORM VAL-PZA-CIC-PLZA.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO PZA-CIC-PLZA-LEN IN PZA-FLD
                GO TO FIN-VAL-FLD-PZA.
           PERFORM VAL-PZA-EXT-CTAB.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO PZA-EXT-CTAB-LEN IN PZA-FLD
                GO TO FIN-VAL-FLD-PZA.
           PERFORM VAL-PZA-GLS-PLZA.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO PZA-GLS-PLZA-LEN IN PZA-FLD
                GO TO FIN-VAL-FLD-PZA.
           PERFORM VAL-PZA-GLS-JURI.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO PZA-GLS-JURI-LEN IN PZA-FLD
                GO TO FIN-VAL-FLD-PZA.
           PERFORM VAL-PZA-GLS-ABRV.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO PZA-GLS-ABRV-LEN IN PZA-FLD
                GO TO FIN-VAL-FLD-PZA.
           PERFORM VAL-PZA-GLS-DCOR.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO PZA-GLS-DCOR-LEN IN PZA-FLD
                GO TO FIN-VAL-FLD-PZA.
           PERFORM VAL-PZA-IND-VIGE.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO PZA-IND-VIGE-LEN IN PZA-FLD
                GO TO FIN-VAL-FLD-PZA.
           PERFORM VAL-PZA-FEC-FTRN.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO PZA-FEC-FTRN-LEN IN PZA-FLD
                GO TO FIN-VAL-FLD-PZA.
           PERFORM VAL-PZA-COD-BNCO.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO PZA-COD-BNCO-LEN IN PZA-FLD
                GO TO FIN-VAL-FLD-PZA.
           PERFORM VAL-PZA-GLS-SBCC.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO PZA-GLS-SBCC-LEN IN PZA-FLD
                GO TO FIN-VAL-FLD-PZA.
           PERFORM VAL-PZA-GLS-SBCO.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO PZA-GLS-SBCO-LEN IN PZA-FLD
                GO TO FIN-VAL-FLD-PZA.
           PERFORM VAL-FRM-GLS-BNCO.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-BNCO-LEN IN PZA-FLD
                GO TO FIN-VAL-FLD-PZA.
       FIN-VAL-FLD-PZA.
           EXIT.
       
