      * 30-Dec-92 01:05 PM
       PUT-ALL-TIO SECTION.
       INI-PUT-ALL-TIO.
           PERFORM PUT-TIO-TIO.
       FIN-PUT-ALL-TIO.
           EXIT.
       
       PUT-TIO-TIO SECTION.
       INI-PUT-TIO-TIO.
           IF PGM-STAT-TIO-NUL
               GO TO FIN-PUT-TIO-TIO.
           IF NOT PGM-STAT-TIO-OKS
               PERFORM CLR-TIO-TIO
               GO TO FIN-PUT-TIO-TIO.
           MOVE TIO-COD-TIOP IN TIO TO TIO-COD-TIOP IN TIO-FLD.
           MOVE TIO-CIC-TIOP IN TIO TO TIO-CIC-TIOP IN TIO-FLD.
           MOVE TIO-EXT-CTAB IN TIO TO TIO-EXT-CTAB IN TIO-FLD.
           MOVE TIO-GLS-DESC IN TIO TO TIO-GLS-DESC IN TIO-FLD.
           MOVE TIO-GLS-ABRV IN TIO TO TIO-GLS-ABRV IN TIO-FLD.
           MOVE TIO-GLS-DCOR IN TIO TO TIO-GLS-DCOR IN TIO-FLD.
           MOVE TIO-IND-VIGE IN TIO TO TIO-IND-VIGE IN TIO-FLD.
           MOVE TIO-NUM-DTRN IN TIO TO TIO-NUM-DTRN IN TIO-FLD.
           MOVE TIO-NUM-MTRN IN TIO TO TIO-NUM-MTRN IN TIO-FLD.
           MOVE TIO-NUM-STRN IN TIO TO TIO-NUM-STRN IN TIO-FLD.
           MOVE TIO-NUM-ATRN IN TIO TO TIO-NUM-ATRN IN TIO-FLD.
           MOVE TIO-GLS-SIST IN TIO TO TIO-GLS-SIST IN TIO-FLD.
           MOVE TIO-GLS-SBCC IN TIO TO TIO-GLS-SBCC IN TIO-FLD.
       FIN-PUT-TIO-TIO.
           EXIT.
       
       KEY-ALL-TIO SECTION.
       INI-KEY-ALL-TIO.
           PERFORM KEY-TIO-TIO.
       FIN-KEY-ALL-TIO.
           EXIT.
       
       KEY-TIO-TIO SECTION.
       INI-KEY-TIO-TIO.
           MOVE TIO-COD-TIOP IN TIO-FLD TO TIO-COD-TIOP IN TIO.
       FIN-KEY-TIO-TIO.
           EXIT.
       
       SCR-CLRA-TIO SECTION.
       INI-SCR-CLRA-TIO.
           PERFORM CLR-KEY-TIO.
           PERFORM CLR-FLD-TIO.
       FIN-SCR-CLRA-TIO.
           EXIT.
       
       CLR-KEY-TIO SECTION.
       INI-CLR-KEY-TIO.
           MOVE SPACES TO TIO-COD-TIOP IN TIO-FLD.
       FIN-CLR-KEY-TIO.
           EXIT.
       
       CLR-FLD-TIO SECTION.
       INI-CLR-FLD-TIO.
           PERFORM CLR-TIO-TIO.
       FIN-CLR-FLD-TIO.
           EXIT.
       
       CLR-TIO-TIO SECTION.
       INI-CLR-TIO-TIO.
           MOVE SPACES TO TIO-CIC-TIOP IN TIO-FLD.
           MOVE SPACES TO TIO-EXT-CTAB IN TIO-FLD.
           MOVE SPACES TO TIO-GLS-DESC IN TIO-FLD.
           MOVE SPACES TO TIO-GLS-ABRV IN TIO-FLD.
           MOVE SPACES TO TIO-GLS-DCOR IN TIO-FLD.
           MOVE SPACES TO TIO-IND-VIGE IN TIO-FLD.
           MOVE SPACES TO TIO-FEC-FTRN IN TIO-FLD.
           MOVE SPACES TO TIO-GLS-SIST IN TIO-FLD.
           MOVE SPACES TO TIO-GLS-SBCC IN TIO-FLD.
       FIN-CLR-TIO-TIO.
           EXIT.
       
       FST-KEY-TIO SECTION.
       INI-FST-KEY-TIO.
           MOVE -1 TO TIO-COD-TIOP-LEN IN TIO-FLD.
       FIN-FST-KEY-TIO.
           EXIT.
       
       FST-FLD-TIO SECTION.
       INI-FST-FLD-TIO.
           MOVE -1 TO TIO-CIC-TIOP-LEN IN TIO-FLD.
       FIN-FST-FLD-TIO.
           EXIT.
       
       PRO-KEY-TIO SECTION.
       INI-PRO-KEY-TIO.
           PERFORM PRO-IKY-TIO.
           PERFORM PRO-AKY-TIO.
           PERFORM PRO-MKY-TIO.
       FIN-PRO-KEY-TIO.
           EXIT.
       
       PRO-IKY-TIO SECTION.
       INI-PRO-IKY-TIO.
           MOVE FRM-CPIM-AEY TO TIO-COD-TIOP-ATR IN TIO-FLD.
       FIN-PRO-IKY-TIO.
           EXIT.
       
       PRO-MKY-TIO SECTION.
       INI-PRO-MKY-TIO.
       FIN-PRO-MKY-TIO.
           EXIT.
       
       PRO-AKY-TIO SECTION.
       INI-PRO-AKY-TIO.
       FIN-PRO-AKY-TIO.
           EXIT.
       
       UNP-KEY-TIO SECTION.
       INI-UNP-KEY-TIO.
           PERFORM UNP-IKY-TIO.
           PERFORM UNP-AKY-TIO.
           PERFORM UNP-MKY-TIO.
       FIN-UNP-KEY-TIO.
           EXIT.
       
       UNP-IKY-TIO SECTION.
       INI-UNP-IKY-TIO.
           MOVE FRM-CPIM-UBY TO TIO-COD-TIOP-ATR.
       FIN-UNP-IKY-TIO.
           EXIT.
       
       UNP-MKY-TIO SECTION.
       INI-UNP-MKY-TIO.
       FIN-UNP-MKY-TIO.
           EXIT.
       
       UNP-AKY-TIO SECTION.
       INI-UNP-AKY-TIO.
       FIN-UNP-AKY-TIO.
           EXIT.
       
       UNP-FLD-TIO SECTION.
       INI-UNP-FLD-TIO.
           PERFORM UNP-TIO-TIO.
       FIN-UNP-FLD-TIO.
           EXIT.
       
       UNP-TIO-TIO SECTION.
       INI-UNP-TIO-TIO.
           MOVE FRM-CPIM-UBY TO TIO-CIC-TIOP-ATR IN TIO-FLD.
           MOVE FRM-CPIM-UBY TO TIO-EXT-CTAB-ATR IN TIO-FLD.
           MOVE FRM-CPIM-UBY TO TIO-GLS-DESC-ATR IN TIO-FLD.
           MOVE FRM-CPIM-UBY TO TIO-GLS-ABRV-ATR IN TIO-FLD.
           MOVE FRM-CPIM-UBY TO TIO-GLS-DCOR-ATR IN TIO-FLD.
           MOVE FRM-CPIM-UBY TO TIO-IND-VIGE-ATR IN TIO-FLD.
           MOVE FRM-CPIM-UBY TO TIO-GLS-SIST-ATR IN TIO-FLD.
           MOVE FRM-CPIM-UBY TO TIO-GLS-SBCC-ATR IN TIO-FLD.
       FIN-UNP-TIO-TIO.
           EXIT.
       
       VAL-KEY-TIO SECTION.
       INI-VAL-KEY-TIO.
           PERFORM VAL-IKY-TIO.
           PERFORM VAL-MKY-TIO.
           PERFORM VAL-AKY-TIO.
       FIN-VAL-KEY-TIO.
           EXIT.
       
       VAL-IKY-TIO SECTION.
       INI-VAL-IKY-TIO.
           PERFORM VAL-TIO-COD-TIOP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO TIO-COD-TIOP-LEN IN TIO-FLD
                GO TO FIN-VAL-IKY-TIO.
       FIN-VAL-IKY-TIO.
           EXIT.
       
       VAL-MKY-TIO SECTION.
       INI-VAL-MKY-TIO.
       FIN-VAL-MKY-TIO.
           EXIT.
       
       VAL-AKY-TIO SECTION.
       INI-VAL-AKY-TIO.
       FIN-VAL-AKY-TIO.
           EXIT.
       
       VAL-TIO-COD-TIOP SECTION.
       INI-VAL-TIO-COD-TIOP.
           IF TIO-COD-TIOP IN TIO-FLD NOT > SPACES
              MOVE SPACES TO TIO-COD-TIOP IN TIO-FLD.
      *<<<< TIO_COD_TIOP
      *>>>>
       FIN-VAL-TIO-COD-TIOP.
           EXIT.
       
       VAL-CON-KEY-TIO SECTION.
       INI-VAL-CON-KEY-TIO.
           IF     TIO-COD-TIOP IN TIO-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-TIO.
       FIN-VAL-CON-KEY-TIO.
           EXIT.
       
       VAL-CON-IKY-TIO SECTION.
       INI-VAL-CON-IKY-TIO.
           IF     TIO-COD-TIOP IN TIO-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-TIO.
       FIN-VAL-CON-IKY-TIO.
           EXIT.
       
       VAL-NUM-KEY-TIO SECTION.
       INI-VAL-NUM-KEY-TIO.
           PERFORM VAL-NUM-IKY-TIO.
           PERFORM VAL-NUM-MKY-TIO.
           PERFORM VAL-NUM-AKY-TIO.
       FIN-VAL-NUM-KEY-TIO.
           EXIT.
       
       VAL-NUM-IKY-TIO SECTION.
       INI-VAL-NUM-IKY-TIO.
       FIN-VAL-NUM-IKY-TIO.
           EXIT.
       
       VAL-NUM-MKY-TIO SECTION.
       INI-VAL-NUM-MKY-TIO.
       FIN-VAL-NUM-MKY-TIO.
           EXIT.
       
       VAL-NUM-AKY-TIO SECTION.
       INI-VAL-NUM-AKY-TIO.
       FIN-VAL-NUM-AKY-TIO.
           EXIT.
       
       VAL-NUM-FLD-TIO SECTION.
       INI-VAL-NUM-FLD-TIO.
       FIN-VAL-NUM-FLD-TIO.
           EXIT.
       
       EDT-KEY-TIO SECTION.
       INI-EDT-KEY-TIO.
           PERFORM EDT-IKY-TIO.
           PERFORM EDT-MKY-TIO.
           PERFORM EDT-AKY-TIO.
       FIN-EDT-KEY-TIO.
           EXIT.
       
       EDT-IKY-TIO SECTION.
       INI-EDT-IKY-TIO.
       FIN-EDT-IKY-TIO.
           EXIT.
       
       EDT-MKY-TIO SECTION.
       INI-EDT-MKY-TIO.
       FIN-EDT-MKY-TIO.
           EXIT.
       
       EDT-AKY-TIO SECTION.
       INI-EDT-AKY-TIO.
       FIN-EDT-AKY-TIO.
           EXIT.
       
       EDT-FLD-TIO SECTION.
       INI-EDT-FLD-TIO.
       FIN-EDT-FLD-TIO.
           EXIT.
