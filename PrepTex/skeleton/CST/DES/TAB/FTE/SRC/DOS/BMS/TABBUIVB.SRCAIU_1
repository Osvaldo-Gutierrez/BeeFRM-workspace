           PERFORM UNP-IVB-IVB.
       FIN-UNP-FLD-IVB.
           EXIT.
       
       UNP-IVB-IVB SECTION.
       INI-UNP-IVB-IVB.
           MOVE FRM-CPIM-UBY TO IVB-CIC-TTAB-ATR IN IVB-FLD.
           MOVE FRM-CPIM-UBY TO IVB-CIC-CIVB-ATR IN IVB-FLD.
           MOVE FRM-CPIM-UBY TO IVB-CIC-SIVB-ATR IN IVB-FLD.
           MOVE FRM-CPIM-UBY TO IVB-EXT-CTAB-ATR IN IVB-FLD.
           MOVE FRM-CPIM-UBY TO IVB-GLS-DESC-ATR IN IVB-FLD.
           MOVE FRM-CPIM-UBY TO IVB-GLS-ABRV-ATR IN IVB-FLD.
           MOVE FRM-CPIM-UBY TO IVB-GLS-DCOR-ATR IN IVB-FLD.
           MOVE FRM-CPIM-UBY TO IVB-IND-VIGE-ATR IN IVB-FLD.
           MOVE FRM-CPIM-UBY TO IVB-IND-IIVB-ATR IN IVB-FLD.
           MOVE FRM-CPIM-UBY TO IVB-GLS-IIVB-ATR IN IVB-FLD.
           MOVE FRM-CPIM-UBY TO IVB-NUM-IIVB-ATR IN IVB-FLD.
           MOVE FRM-CPIM-UBY TO IVB-VRF-IIVB-ATR IN IVB-FLD.
           MOVE FRM-CPIM-UBY TO IVB-GLS-SIVB-ATR IN IVB-FLD.
           MOVE FRM-CPIM-UBY TO IVB-COD-DPTO-ATR IN IVB-FLD.
       FIN-UNP-IVB-IVB.
           EXIT.
       
       VAL-MKY-IVB SECTION.
       INI-VAL-MKY-IVB.
       FIN-VAL-MKY-IVB.
           EXIT.
       
       VAL-FLD-IVB SECTION.
       INI-VAL-FLD-IVB.
           PERFORM VAL-IVB-CIC-TTAB.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO IVB-CIC-TTAB-LEN IN IVB-FLD
                GO TO FIN-VAL-FLD-IVB.
           PERFORM VAL-IVB-CIC-CIVB.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO IVB-CIC-CIVB-LEN IN IVB-FLD
                GO TO FIN-VAL-FLD-IVB.
           PERFORM VAL-IVB-CIC-SIVB.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO IVB-CIC-SIVB-LEN IN IVB-FLD
                GO TO FIN-VAL-FLD-IVB.
           PERFORM VAL-IVB-EXT-CTAB.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO IVB-EXT-CTAB-LEN IN IVB-FLD
                GO TO FIN-VAL-FLD-IVB.
           PERFORM VAL-IVB-GLS-DESC.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO IVB-GLS-DESC-LEN IN IVB-FLD
                GO TO FIN-VAL-FLD-IVB.
           PERFORM VAL-IVB-GLS-ABRV.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO IVB-GLS-ABRV-LEN IN IVB-FLD
                GO TO FIN-VAL-FLD-IVB.
           PERFORM VAL-IVB-GLS-DCOR.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO IVB-GLS-DCOR-LEN IN IVB-FLD
                GO TO FIN-VAL-FLD-IVB.
           PERFORM VAL-IVB-IND-VIGE.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO IVB-IND-VIGE-LEN IN IVB-FLD
                GO TO FIN-VAL-FLD-IVB.
           PERFORM VAL-IVB-FEC-FTRN.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO IVB-FEC-FTRN-LEN IN IVB-FLD
                GO TO FIN-VAL-FLD-IVB.
           PERFORM VAL-IVB-IND-IIVB.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO IVB-IND-IIVB-LEN IN IVB-FLD
                GO TO FIN-VAL-FLD-IVB.
           PERFORM VAL-IVB-GLS-IIVB.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO IVB-GLS-IIVB-LEN IN IVB-FLD
                GO TO FIN-VAL-FLD-IVB.
           PERFORM VAL-IVB-NUM-IIVB.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO IVB-NUM-IIVB-LEN IN IVB-FLD
                GO TO FIN-VAL-FLD-IVB.
           PERFORM VAL-IVB-VRF-IIVB.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO IVB-VRF-IIVB-LEN IN IVB-FLD
                GO TO FIN-VAL-FLD-IVB.
           PERFORM VAL-IVB-GLS-SIVB.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO IVB-GLS-SIVB-LEN IN IVB-FLD
                GO TO FIN-VAL-FLD-IVB.
           PERFORM VAL-IVB-COD-DPTO.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO IVB-COD-DPTO-LEN IN IVB-FLD
                GO TO FIN-VAL-FLD-IVB.
           PERFORM VAL-FRM-GLS-DPTO.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-DPTO-LEN IN IVB-FLD
                GO TO FIN-VAL-FLD-IVB.
       FIN-VAL-FLD-IVB.
           EXIT.
       
       VAL-IVB-CIC-TTAB SECTION.
       INI-VAL-IVB-CIC-TTAB.
           IF IVB-CIC-TTAB IN IVB-FLD NOT > SPACES
              MOVE SPACES TO IVB-CIC-TTAB IN IVB-FLD.
      *<<<< IVB_CIC_TTAB
           IF IVB-CIC-TTAB IN IVB-FLD NOT > SPACES AND
              IVB-CIC-CIVB IN IVB-FLD NOT > SPACES AND
              IVB-CIC-SIVB IN IVB-FLD NOT > SPACES
               MOVE IVB-COD-TTAB IN IVB-FLD TO IVB-CIC-TTAB IN IVB-FLD
               MOVE IVB-COD-CIVB IN IVB-FLD TO IVB-CIC-CIVB IN IVB-FLD
               MOVE IVB-COD-SIVB IN IVB-FLD TO IVB-CIC-SIVB IN IVB-FLD.
      *>>>>
       FIN-VAL-IVB-CIC-TTAB.
           EXIT.
       
       VAL-IVB-CIC-CIVB SECTION.
       INI-VAL-IVB-CIC-CIVB.
           IF IVB-CIC-CIVB IN IVB-FLD NOT > SPACES
              MOVE SPACES TO IVB-CIC-CIVB IN IVB-FLD.
       FIN-VAL-IVB-CIC-CIVB.
           EXIT.
       
       VAL-IVB-CIC-SIVB SECTION.
       INI-VAL-IVB-CIC-SIVB.
           IF IVB-CIC-SIVB IN IVB-FLD NOT > SPACES
              MOVE SPACES TO IVB-CIC-SIVB IN IVB-FLD.
       FIN-VAL-IVB-CIC-SIVB.
           EXIT.
       
       VAL-IVB-EXT-CTAB SECTION.
       INI-VAL-IVB-EXT-CTAB.
           IF IVB-EXT-CTAB IN IVB-FLD NOT > SPACES
              MOVE SPACES TO IVB-EXT-CTAB IN IVB-FLD.
       FIN-VAL-IVB-EXT-CTAB.
           EXIT.
       
       VAL-IVB-GLS-DESC SECTION.
       INI-VAL-IVB-GLS-DESC.
           IF IVB-GLS-DESC IN IVB-FLD NOT > SPACES
              MOVE SPACES TO IVB-GLS-DESC IN IVB-FLD.
      *<<<< IVB_GLS_DESC
           IF IVB-GLS-DESC IN IVB-FLD NOT > SPACES
               PERFORM ERR-GLS.
      *>>>>
       FIN-VAL-IVB-GLS-DESC.
           EXIT.
       
       VAL-IVB-GLS-ABRV SECTION.
       INI-VAL-IVB-GLS-ABRV.
           IF IVB-GLS-ABRV IN IVB-FLD NOT > SPACES
              MOVE SPACES TO IVB-GLS-ABRV IN IVB-FLD.
      *<<<< IVB_GLS_ABRV
           IF IVB-GLS-ABRV IN IVB-FLD NOT > SPACES
               PERFORM ERR-GLS.
      *>>>>
       FIN-VAL-IVB-GLS-ABRV.
           EXIT.
       
       VAL-IVB-GLS-DCOR SECTION.
       INI-VAL-IVB-GLS-DCOR.
           IF IVB-GLS-DCOR IN IVB-FLD NOT > SPACES
              MOVE SPACES TO IVB-GLS-DCOR IN IVB-FLD.
      *<<<< IVB_GLS_DCOR
           IF IVB-GLS-DCOR IN IVB-FLD NOT > SPACES
               PERFORM ERR-GLS.
      *>>>>
       FIN-VAL-IVB-GLS-DCOR.
           EXIT.
       
       VAL-IVB-IND-VIGE SECTION.
       INI-VAL-IVB-IND-VIGE.
           IF IVB-IND-VIGE IN IVB-FLD NOT > SPACES
              MOVE SPACES TO IVB-IND-VIGE IN IVB-FLD.
      *<<<< IVB_IND_VIGE
           MOVE 'GNS' TO MSC-COD-SIST.
           MOVE 'VIGE' TO MSC-COD-TMSC IN MSC.
           MOVE IVB-IND-VIGE IN IVB-FLD TO MSC-COD-CMSC IN MSC.
           PERFORM VAL-IND.
      *>>>>
       FIN-VAL-IVB-IND-VIGE.
           EXIT.
       
       VAL-IVB-FEC-FTRN SECTION.
       INI-VAL-IVB-FEC-FTRN.
       FIN-VAL-IVB-FEC-FTRN.
           EXIT.
       
       VAL-IVB-IND-IIVB SECTION.
       INI-VAL-IVB-IND-IIVB.
           IF IVB-IND-IIVB IN IVB-FLD NOT > SPACES
              MOVE SPACES TO IVB-IND-IIVB IN IVB-FLD.
      *VAL-IDC Valida identificador
           MOVE IVB-IND-IIVB IN IVB-FLD TO IDC-IND-RUTV.
           PERFORM VAL-IDC.
           MOVE IDC-IND-RUTV TO IVB-IND-IIVB IN IVB-FLD.
      *<<<< IVB_IND_IIVB
           IF IVB-COD-CIVB IN IVB-FLD > SPACES
               MOVE IVB-IND-IIVB IN IVB-FLD TO IDC-IND-RUTV
               PERFORM VAL-IDC
               MOVE IDC-IND-RUTV TO IVB-IND-IIVB IN IVB-FLD
           ELSE
               MOVE SPACES TO IVB-IND-IIVB IN IVB-FLD.
      *>>>>
       FIN-VAL-IVB-IND-IIVB.
           EXIT.
       
       VAL-IVB-GLS-IIVB SECTION.
       INI-VAL-IVB-GLS-IIVB.
           IF IVB-GLS-IIVB IN IVB-FLD NOT > SPACES
              MOVE SPACES TO IVB-GLS-IIVB IN IVB-FLD.
      *VAL-GLS para IDC valida glosa ( extension ) de identificador
           MOVE IVB-GLS-IIVB IN IVB-FLD TO IDC-GLS-RUTV.
           PERFORM VAL-GLS.
           MOVE IDC-GLS-RUTV TO IVB-GLS-IIVB IN IVB-FLD.
      *<<<< IVB_GLS_IIVB
           IF IVB-COD-CIVB IN IVB-FLD NOT > SPACES
               MOVE SPACES TO IVB-GLS-IIVB IN IVB-FLD
               GO TO FIN-VAL-IVB-GLS-IIVB.
           MOVE IVB-GLS-IIVB IN IVB-FLD TO IDC-GLS-RUTV.
           PERFORM VAL-GLS.
           IF IVB-COD-SIVB IN IVB-FLD > SPACES
               MOVE IVB-COD-TTAB IN IVB-FLD TO IVB-COD-TTAB IN IVB
               MOVE IVB-COD-CIVB IN IVB-FLD TO IVB-COD-CIVB IN IVB
               MOVE SPACES TO IVB-COD-SIVB IN IVB
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM TAB-FIO-IVB
               IF ( MSC-CIC-CMSC NOT = 'R' AND
                    IVB-NUM-IIVB IN IVB = IVB-NUM-IIVB IN IVB-FLD ) OR
                  ( MSC-CIC-CMSC = 'R' AND
                    IVB-NUM-IIVB IN IVB NOT = IVB-NUM-IIVB IN IVB-FLD )
                   MOVE 'IDC    NCS' TO MSG-COD-MENS
                   MOVE 'TAB' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>>
       FIN-VAL-IVB-GLS-IIVB.
           EXIT.
       
       VAL-IVB-NUM-IIVB SECTION.
       INI-VAL-IVB-NUM-IIVB.
       FIN-VAL-IVB-NUM-IIVB.
           EXIT.
       
       VAL-IVB-VRF-IIVB SECTION.
       INI-VAL-IVB-VRF-IIVB.
           IF IVB-VRF-IIVB IN IVB-FLD NOT > SPACES
              MOVE SPACES TO IVB-VRF-IIVB IN IVB-FLD.
      *VAL-RUT Valida Rut para campo IDC
           MOVE IDC-ZERO-S TO IDC-ZERO.
           MOVE IVB-NUM-IIVB IN IVB-FLD TO IDC-NUM-RUTV.
           MOVE IVB-VRF-IIVB IN IVB-FLD TO IDC-VRF-RUTV.
           PERFORM VAL-RUT.
      *<<<< IVB_VRF_IIVB
           IF IVB-COD-CIVB IN IVB-FLD NOT > SPACES
               MOVE ZEROES TO IVB-NUM-IIVB IN IVB-FLD
               MOVE SPACES TO IVB-VRF-IIVB IN IVB-FLD
           ELSE
               MOVE IVB-NUM-IIVB IN IVB-FLD TO IDC-NUM-RUTV
               MOVE IVB-VRF-IIVB IN IVB-FLD TO IDC-VRF-RUTV
               PERFORM VAL-RUT
               IF MSG-COD-MENS = SPACES
                   MOVE IDC-NUM-RUTV TO IVB-NUM-IIVB IN IVB-FLD.
      *>>>>
       FIN-VAL-IVB-VRF-IIVB.
           EXIT.
       
       VAL-IVB-GLS-SIVB SECTION.
       INI-VAL-IVB-GLS-SIVB.
           IF IVB-GLS-SIVB IN IVB-FLD NOT > SPACES
              MOVE SPACES TO IVB-GLS-SIVB IN IVB-FLD.
       FIN-VAL-IVB-GLS-SIVB.
           EXIT.
       
       VAL-IVB-COD-DPTO SECTION.
       INI-VAL-IVB-COD-DPTO.
           IF IVB-COD-DPTO IN IVB-FLD NOT > SPACES
              MOVE SPACES TO IVB-COD-DPTO IN IVB-FLD.
      *<<<< IVB_COD_DPTO
           IF IVB-COD-CIVB IN IVB-FLD > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'DPT' TO TAB-COD-TTAB IN TAB
               MOVE IVB-COD-DPTO IN IVB-FLD TO TAB-COD-CTAB IN TAB
               PERFORM VAL-COD
               IF MSG-COD-MENS NOT = 'COD    NEX' AND
                  IVB-COD-DPTO IN IVB-FLD > SPACES
                   MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-DPTO IN IVB-FLD
               ELSE
                   MOVE SPACES TO FRM-GLS-DPTO IN IVB-FLD
           ELSE
               MOVE SPACES TO IVB-COD-DPTO IN IVB-FLD.
      *>>>>
      *VAL-COD Valida codigo de tabla
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'DPT' TO TAB-COD-TTAB IN TAB.
           MOVE IVB-COD-DPTO IN IVB-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              IVB-COD-DPTO IN IVB-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-DPTO IN IVB-FLD
           ELSE
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-DPTO IN IVB-FLD.
       FIN-VAL-IVB-COD-DPTO.
           EXIT.
       
       VAL-FRM-GLS-DPTO SECTION.
       INI-VAL-FRM-GLS-DPTO.
       FIN-VAL-FRM-GLS-DPTO.
           EXIT.
       
       VAL-CON-FLD-IVB SECTION.
       INI-VAL-CON-FLD-IVB.
       FIN-VAL-CON-FLD-IVB.
           EXIT.
       
       VAL-CON-IVB SECTION.
       INI-VAL-CON-IVB.
       FIN-VAL-CON-IVB.
           EXIT.
       
       VAL-NUM-KEY-IVB SECTION.
       INI-VAL-NUM-KEY-IVB.
           PERFORM VAL-NUM-IKY-IVB.
           PERFORM VAL-NUM-MKY-IVB.
           PERFORM VAL-NUM-AKY-IVB.
       FIN-VAL-NUM-KEY-IVB.
           EXIT.
       
       VAL-NUM-IKY-IVB SECTION.
       INI-VAL-NUM-IKY-IVB.
       FIN-VAL-NUM-IKY-IVB.
           EXIT.
       
       VAL-NUM-MKY-IVB SECTION.
       INI-VAL-NUM-MKY-IVB.
       FIN-VAL-NUM-MKY-IVB.
           EXIT.
       
       VAL-NUM-AKY-IVB SECTION.
       INI-VAL-NUM-AKY-IVB.
       FIN-VAL-NUM-AKY-IVB.
           EXIT.
       
       VAL-NUM-FLD-IVB SECTION.
       INI-VAL-NUM-FLD-IVB.
       VAL-NUM-IVB-NUM-IIVB.
           MOVE IVB-NUM-IIVB-ALF IN IVB-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           CALL 'GNSPVNUM' USING NUM-VARI.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO IVB-NUM-IIVB IN IVB-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO IVB-NUM-IIVB-LEN IN IVB-FLD
                GO TO FIN-VAL-NUM-FLD-IVB.
       FIN-VAL-NUM-FLD-IVB.
           EXIT.
       
       EDT-KEY-IVB SECTION.
       INI-EDT-KEY-IVB.
           PERFORM EDT-IKY-IVB.
           PERFORM EDT-MKY-IVB.
           PERFORM EDT-AKY-IVB.
       FIN-EDT-KEY-IVB.
           EXIT.
       
       EDT-IKY-IVB SECTION.
       INI-EDT-IKY-IVB.
       FIN-EDT-IKY-IVB.
           EXIT.
       
       EDT-MKY-IVB SECTION.
       INI-EDT-MKY-IVB.
       FIN-EDT-MKY-IVB.
           EXIT.
       
       EDT-AKY-IVB SECTION.
       INI-EDT-AKY-IVB.
       FIN-EDT-AKY-IVB.
           EXIT.
       
       EDT-FLD-IVB SECTION.
       INI-EDT-FLD-IVB.
           IF IVB-NUM-IIVB-LEN IN IVB-FLD = -9
                MOVE -1 TO IVB-NUM-IIVB-LEN IN IVB-FLD
                GO TO FIN-EDT-FLD-IVB.
           MOVE IVB-NUM-IIVB IN IVB-FLD TO IVB-NUM-IIVB-EDT IN IVB-FLD.
       FIN-EDT-FLD-IVB.
           EXIT.
