       VAL-FLD-TIO SECTION.
       INI-VAL-FLD-TIO.
           PERFORM VAL-TIO-CIC-TIOP.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO TIO-CIC-TIOP-LEN IN TIO-FLD
                GO TO FIN-VAL-FLD-TIO.
           PERFORM VAL-TIO-EXT-CTAB.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO TIO-EXT-CTAB-LEN IN TIO-FLD
                GO TO FIN-VAL-FLD-TIO.
           PERFORM VAL-TIO-GLS-DESC.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO TIO-GLS-DESC-LEN IN TIO-FLD
                GO TO FIN-VAL-FLD-TIO.
           PERFORM VAL-TIO-GLS-ABRV.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO TIO-GLS-ABRV-LEN IN TIO-FLD
                GO TO FIN-VAL-FLD-TIO.
           PERFORM VAL-TIO-GLS-DCOR.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO TIO-GLS-DCOR-LEN IN TIO-FLD
                GO TO FIN-VAL-FLD-TIO.
           PERFORM VAL-TIO-IND-VIGE.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO TIO-IND-VIGE-LEN IN TIO-FLD
                GO TO FIN-VAL-FLD-TIO.
           PERFORM VAL-TIO-FEC-FTRN.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO TIO-FEC-FTRN-LEN IN TIO-FLD
                GO TO FIN-VAL-FLD-TIO.
           PERFORM VAL-TIO-GLS-SIST.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO TIO-GLS-SIST-LEN IN TIO-FLD
                GO TO FIN-VAL-FLD-TIO.
           PERFORM VAL-TIO-GLS-SBCC.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO TIO-GLS-SBCC-LEN IN TIO-FLD
                GO TO FIN-VAL-FLD-TIO.
