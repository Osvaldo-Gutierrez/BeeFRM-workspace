      * 05-Jan-93 10:50 PM
       RET-ALL-RES SECTION.
       INI-RET-ALL-RES.
           PERFORM KEY-ALL-RES.
           PERFORM RET-TAB-RES.
           PERFORM RET-USR-RES.
       FIN-RET-ALL-RES.
           EXIT.
       
       RET-TAB-RES SECTION.
       INI-RET-TAB-RES.
           MOVE TAB-GLS-DESC IN RES-FLD TO TAB-GLS-DESC IN TAB.
           MOVE TAB-COD-ATRN IN RES-FLD TO TAB-COD-ATRN IN TAB.
           MOVE TAB-NUM-DTRN IN RES-FLD TO TAB-NUM-DTRN IN TAB.
           MOVE TAB-NUM-MTRN IN RES-FLD TO TAB-NUM-MTRN IN TAB.
           MOVE TAB-NUM-STRN IN RES-FLD TO TAB-NUM-STRN IN TAB.
           MOVE TAB-NUM-ATRN IN RES-FLD TO TAB-NUM-ATRN IN TAB.
       FIN-RET-TAB-RES.
           EXIT.
       
       RET-USR-RES SECTION.
       INI-RET-USR-RES.
       FIN-RET-USR-RES.
           EXIT.
       
       KEY-ALL-RES SECTION.
       INI-KEY-ALL-RES.
           PERFORM KEY-TAB-RES.
           PERFORM KEY-USR-RES.
       FIN-KEY-ALL-RES.
           EXIT.
       
       KEY-TAB-RES SECTION.
       INI-KEY-TAB-RES.
           MOVE TAB-COD-TABL IN RES-FLD TO TAB-COD-TABL IN TAB.
       FIN-KEY-TAB-RES.
           EXIT.
       
       KEY-USR-RES SECTION.
       INI-KEY-USR-RES.
           MOVE USR-COD-USER IN RES-FLD TO USR-COD-USER IN USR.
       FIN-KEY-USR-RES.
           EXIT.
       
       SCR-CLRA-RES SECTION.
       INI-SCR-CLRA-RES.
           PERFORM CLR-KEY-RES.
           PERFORM CLR-FLD-RES.
       FIN-SCR-CLRA-RES.
           EXIT.
       
       CLR-KEY-RES SECTION.
       INI-CLR-KEY-RES.
           MOVE SPACES TO TAB-COD-TABL IN RES-FLD.
           MOVE SPACES TO USR-COD-USER IN RES-FLD.
       FIN-CLR-KEY-RES.
           EXIT.
       
       CLR-FLD-RES SECTION.
       INI-CLR-FLD-RES.
           MOVE SPACES TO FRM-GLS-USER IN RES-FLD.
           MOVE SPACES TO FRM-GLS-DUSR IN RES-FLD.
           PERFORM CLR-TAB-RES.
           PERFORM CLR-USR-RES.
       FIN-CLR-FLD-RES.
           EXIT.
       
       CLR-TAB-RES SECTION.
       INI-CLR-TAB-RES.
           MOVE SPACES TO TAB-GLS-DESC IN RES-FLD.
           MOVE SPACES TO TAB-COD-ATRN IN RES-FLD.
           MOVE SPACES TO TAB-FEC-FTRN IN RES-FLD.
       FIN-CLR-TAB-RES.
           EXIT.
       
       CLR-USR-RES SECTION.
       INI-CLR-USR-RES.
       FIN-CLR-USR-RES.
           EXIT.
       
       FST-KEY-RES SECTION.
       INI-FST-KEY-RES.
           MOVE -1 TO TAB-COD-TABL-LEN IN RES-FLD.
       FIN-FST-KEY-RES.
           EXIT.
       
       FST-FLD-RES SECTION.
       INI-FST-FLD-RES.
       FIN-FST-FLD-RES.
           EXIT.
       
       PRO-KEY-RES SECTION.
       INI-PRO-KEY-RES.
           PERFORM PRO-MKY-RES.
       FIN-PRO-KEY-RES.
           EXIT.
       
       PRO-MKY-RES SECTION.
       INI-PRO-MKY-RES.
       FIN-PRO-MKY-RES.
           EXIT.
       
       PRO-FLD-RES SECTION.
       INI-PRO-FLD-RES.
           PERFORM PRO-TAB-RES.
           PERFORM PRO-USR-RES.
       FIN-PRO-FLD-RES.
           EXIT.
       
       PRO-TAB-RES SECTION.
       INI-PRO-TAB-RES.
       FIN-PRO-TAB-RES.
           EXIT.
       
       PRO-USR-RES SECTION.
       INI-PRO-USR-RES.
       FIN-PRO-USR-RES.
           EXIT.
       
       UNP-KEY-RES SECTION.
       INI-UNP-KEY-RES.
           PERFORM UNP-IKY-RES.
           PERFORM UNP-AKY-RES.
           PERFORM UNP-MKY-RES.
       FIN-UNP-KEY-RES.
           EXIT.
       
       UNP-IKY-RES SECTION.
       INI-UNP-IKY-RES.
           MOVE FRM-CPIM-UBY TO TAB-COD-TABL-ATR.
           MOVE FRM-CPIM-UBY TO USR-COD-USER-ATR.
       FIN-UNP-IKY-RES.
           EXIT.
       
       UNP-MKY-RES SECTION.
       INI-UNP-MKY-RES.
       FIN-UNP-MKY-RES.
           EXIT.
       
       UNP-AKY-RES SECTION.
       INI-UNP-AKY-RES.
       FIN-UNP-AKY-RES.
           EXIT.
       
       UNP-FLD-RES SECTION.
       INI-UNP-FLD-RES.
           PERFORM UNP-TAB-RES.
           PERFORM UNP-USR-RES.
       FIN-UNP-FLD-RES.
           EXIT.
       
       UNP-TAB-RES SECTION.
       INI-UNP-TAB-RES.
       FIN-UNP-TAB-RES.
