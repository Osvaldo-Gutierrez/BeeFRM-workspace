       VAL-MKY-RES SECTION.
       INI-VAL-MKY-RES.
       FIN-VAL-MKY-RES.
           EXIT.
       
       VAL-FLD-RES SECTION.
       INI-VAL-FLD-RES.
           PERFORM VAL-TAB-GLS-DESC.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO TAB-GLS-DESC-LEN IN RES-FLD
                GO TO FIN-VAL-FLD-RES.
           PERFORM VAL-FRM-GLS-USER.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-USER-LEN IN RES-FLD
                GO TO FIN-VAL-FLD-RES.
           PERFORM VAL-TAB-COD-ATRN.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO TAB-COD-ATRN-LEN IN RES-FLD
                GO TO FIN-VAL-FLD-RES.
           PERFORM VAL-TAB-FEC-FTRN.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO TAB-FEC-FTRN-LEN IN RES-FLD
                GO TO FIN-VAL-FLD-RES.
           PERFORM VAL-FRM-GLS-DUSR.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-DUSR-LEN IN RES-FLD
                GO TO FIN-VAL-FLD-RES.
       FIN-VAL-FLD-RES.
           EXIT.
       
       VAL-TAB-GLS-DESC SECTION.
       INI-VAL-TAB-GLS-DESC.
      *<<<< TAB_GLS_DESC
      *>>>>
       FIN-VAL-TAB-GLS-DESC.
           EXIT.
       
       VAL-FRM-GLS-USER SECTION.
       INI-VAL-FRM-GLS-USER.
       FIN-VAL-FRM-GLS-USER.
