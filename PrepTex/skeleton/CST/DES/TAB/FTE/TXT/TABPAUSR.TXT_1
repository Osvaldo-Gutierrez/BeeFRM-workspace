*%! Codigo adicional
*% IF WSS
      *<<<< WSS
       COPY GNSWGHOY.
      *>>>>
*% END
*% IF INI_FIO AND PGM_VMS
      *<<<< INI_FIO

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'TAB'        TO FIO-SIST.
           PERFORM TAB-FIO-OFI.
           MOVE FIO-INP TO FIO-CMND.
           PERFORM TAB-FIO-OFI.

      *>>>>
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-INST = SCR-INST-HKY
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'USR' TO TAB-COD-TTAB IN TAB
               PERFORM ERR-TAB.
      *>>>>
*% END
*% IF FND_KEY
      *<<<< FND_KEY
           IF USR-COD-TTAB IN USR NOT > SPACES
               IF USR-COD-USER IN USR > SPACES
                   MOVE 'USR-COD-USER' TO FIO-AKEY
                   MOVE FIO-GET-KEY    TO FIO-CMND
                   PERFORM TAB-FIO-USR
                   IF NOT FIO-STAT-OKS
                       MOVE FIO-STAT-NEX TO FIO-STAT
                       GO TO FIN-FND-KEY
                   ELSE
                       MOVE FIO-STAT TO PGM-STAT-USR
                       GO TO FIN-FND-KEY
               ELSE
               IF USR-NUM-IUSR IN USR > ZEROES
                   MOVE 'USR-IDC-IUSR'  TO FIO-AKEY
                   MOVE FIO-GET-KEY-ALT TO FIO-CMND
                   PERFORM TAB-FIO-USR
                   IF NOT FIO-STAT-OKS
                       MOVE FIO-STAT-NEX TO FIO-STAT
                       GO TO FIN-FND-KEY
                   ELSE
                       MOVE FIO-STAT TO PGM-STAT-USR
                       GO TO FIN-FND-KEY
               ELSE
                   MOVE 'USR-GLS-ABRV'  TO FIO-AKEY
                   MOVE FIO-GET-KEY-ALT TO FIO-CMND
                   PERFORM TAB-FIO-USR
                   IF NOT FIO-STAT-OKS
                       MOVE FIO-STAT-NEX TO FIO-STAT
                       GO TO FIN-FND-KEY
                   ELSE
                       MOVE FIO-STAT TO PGM-STAT-USR
                       GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF CMN_ING OR CMN_MOD
      *<<<< CMN_ING OR CMN_MOD
           PERFORM GET-FHOY.
           MOVE HOY-STP-ITRN TO USR-STP-ITRN IN USR.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBETAB.
       COPY GNSBGHOY.
      *>>>>
*% END
