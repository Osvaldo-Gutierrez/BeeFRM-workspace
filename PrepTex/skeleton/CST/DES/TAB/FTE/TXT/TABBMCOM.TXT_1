*%! Codigo adicional
*% IF FIN_PUT_ALL_COM
      *<<<< FIN_PUT_ALL_COM
           IF COM-COD-DPTO IN COM-FLD > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'DPT' TO TAB-COD-TTAB IN TAB
               MOVE COM-COD-DPTO IN COM-FLD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-DPTO IN COM-FLD.
           IF COM-COD-PRVC IN COM-FLD > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'PRV' TO TAB-COD-TTAB IN TAB
               MOVE COM-COD-PRVC IN COM-FLD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-PRVC IN COM-FLD.
           IF COM-COD-REGI IN COM-FLD > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'REG' TO TAB-COD-TTAB IN TAB
               MOVE COM-COD-REGI IN COM-FLD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-REGI IN COM-FLD.
           IF COM-COD-PAIS IN COM-FLD > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'PAI' TO TAB-COD-TTAB IN TAB
               MOVE COM-COD-PAIS IN COM-FLD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-PAIS IN COM-FLD.
      *<<<<
*% END
*% IF COM_COD_COMU
      *<<<< COM_COD_COMU
      *>>>>
*% END
*% IF COM_CIC_COMU
      *<<<< COM_CIC_COMU
           IF COM-CIC-COMU IN COM-FLD NOT > SPACES
               MOVE COM-COD-COMU IN COM-FLD TO COM-CIC-COMU IN COM-FLD.
      *<<<<
*% END
*% IF COM_EXT_CTAB
      *<<<< COM_EXT_CTAB
      *>>>>
*% END
*% IF COM_GLS_COMU
      *<<<< COM_GLS_COMU
           IF COM-GLS-COMU IN COM-FLD NOT > SPACES
               PERFORM ERR-GLS.
      *<<<<
*% END
*% IF COM_GLS_CIUD
      *<<<< COM_GLS_CIUD
           IF COM-GLS-CIUD IN COM-FLD NOT > SPACES
