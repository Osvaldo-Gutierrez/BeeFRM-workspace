*%! Codigo adicional
*% IF WSS_DTC
      *<<<< WSS_DTC
      * COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-TAB-REQA==.
      * COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSG-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-USR-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-AUX-REQA==.
      *>>>>
*% END
*% IF WSS
      *<<<< WSS
      * COPY GNSBRTAB.
      * COPY GNSBRMSG.
       COPY TABBRUSR.
       01  WSS-VARI.
           03  WSS-COD-TABL.
               05  WSS-COD-TTAB                 PIC X(03).
               05  WSS-COD-CTAB                 PIC X(12).
           03  WSS-IPPH                 VALUE 0 PIC 9(01).
               88 PRIMER-PAGE-HEADING   VALUE 0.
      *>>>>
*% END
*% IF CH_IVB_IVB_COD_TTAB
      *<<<< CH_IVB_IVB_COD_TTAB
      *BUS-TAB busca glosa de codigo en tabla
           IF IVB-COD-TTAB IN SRT > SPACES
      *JSS
*%!RVM:
*% IF PGM_DTC
               MOVE ADR-TAB-REQA TO ADR-AUX-REQA
*% END
               MOVE TAB-COD-TABL IN TAB TO WSS-COD-TABL
      *
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'IVB' TO TAB-COD-TTAB IN TAB
               MOVE IVB-COD-TTAB IN SRT TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO WSS-GLS-DTAB IN WSS-IVB
      *JSS
*%!RVM:
*% IF PGM_DTC
               MOVE ADR-AUX-REQA TO ADR-TAB-REQA
*% END
               MOVE WSS-COD-TABL TO TAB-COD-TABL IN TAB
      *
           ELSE
               MOVE SPACES TO WSS-GLS-DTAB IN WSS-IVB.
      *>>>>
*% END
*% IF PH_IVB
      *<<<< PH_IVB
      *     IF PRIMER-PAGE-HEADING
      *         MOVE 1 TO PRINT-SWITCH
      *         MOVE 0 TO PAGE-COUNTER
      *         MOVE 1 TO WSS-IPPH.
      *>>>>
*% END
*% IF INI_FST_INP
      *<<<< INI_FST_INP
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR DE APERTURA EN ARCHIVO IVB ' FIO-STAT
               PERFORM GNS-PRO-END.
           GO TO INI-OUT-SRT.
      *>>>>
*% END
*% IF INI_OUT
      *<<<< INI_OUT
           MOVE 'IVB'  TO TAB-COD-TTAB IN TAB.
           MOVE SPACES TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-NLS TO FIO-CMND.
       LUP-INI-OUT-SRT.
           MOVE 'TAB'  TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
           IF NOT ( FIO-STAT-OKS AND TAB-COD-TTAB IN TAB = 'IVB' )
               MOVE 'TAB'        TO MSG-COD-SIST
               MOVE 'TAB    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               DISPLAY MSG-GLS-DESC
               PERFORM GNS-PRO-END.
           IF TAB-COD-CTAB IN TAB NOT > SPACES
               MOVE FIO-GET-NXT TO FIO-CMND
               GO TO LUP-INI-OUT-SRT.
           MOVE TAB-GLS-DESC IN TAB TO WSS-GLS-DTAB IN WSS-IVB.
      *>>>>
*% END
*% IF LUP_OUT
      *<<<< LUP_OUT
           IF IVB-COD-CIVB IN SRT NOT > SPACES AND
              IVB-COD-SIVB IN SRT NOT > SPACES
               GO TO NXT-OUT-SRT.
           IF IVB-COD-DPTO IN SRT > SPACES
      *JSS
*%!RVM:
*% IF PGM_DTC
               MOVE ADR-TAB-REQA TO ADR-AUX-REQA
*% END
               MOVE TAB-COD-TABL IN TAB TO WSS-COD-TABL
      *
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'DPT' TO TAB-COD-TTAB
               MOVE IVB-COD-DPTO IN SRT TO TAB-COD-CTAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC TO WSS-GLS-DPTO
      *JSS
*%!RVM:
*% IF PGM_DTC
               MOVE ADR-AUX-REQA TO ADR-TAB-REQA
*% END
               MOVE WSS-COD-TABL TO TAB-COD-TABL IN TAB
      *
           ELSE
               MOVE SPACES TO WSS-GLS-DPTO.
      *>>>>
*% END
*% IF FST_OUT
      *<<<< FST_OUT
       NXT-IVB.
           MOVE TAB-COD-CTAB IN TAB TO IVB-COD-TTAB IN IVB.
           MOVE SPACES TO IVB-COD-EIVB IN IVB.
           MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM TAB-FIO-IVB.
      *<<<<
*% END
*% IF LUP_INP
      *<<<< LUP_INP
           IF IVB-COD-TTAB IN IVB NOT = TAB-COD-CTAB IN TAB
               GO TO NXT-TAB-IVB.
      *>>>>
*% END
*% IF FIN_INP
      *<<<< FIN_INP
       NXT-TAB-IVB.
           MOVE FIO-GET-NXT TO FIO-CMND.
           MOVE 'TAB' TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
           IF FIO-STAT-OKS AND TAB-COD-TTAB IN TAB = 'IVB'
               GO TO NXT-IVB.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
      * COPY GNSBFTAB.
      * COPY GNSBFMSG.
       COPY TABBFUSR.
       COPY TABBBUSR.
       COPY GNSBBMSG.
       COPY GNSBBTAB.
      *>>>>
*% END
