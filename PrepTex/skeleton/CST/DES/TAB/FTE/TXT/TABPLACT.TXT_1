*%! Codigo adicional
*% IF WSS_DTC
      *<<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-USR-REQA==.
      * COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSG-REQA==.
      * COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-TAB-REQA==.
      *>>>>
*% END
*% IF WSS
      *<<<< WSS
      * COPY GNSBRTAB.
      * COPY GNSBRMSG.
       COPY TABBRUSR.
      *>>>>
*% END
*% IF INI_FST_INP
      *<<<< INI_FST_INP
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR DE APERTURA EN ARCHIVO ACT ' FIO-STAT
               PERFORM GNS-PRO-END.
           GO TO INI-OUT-SRT.
      *>>>>
*% END
*% IF INI_OUT
      *<<<< INI_OUT
           MOVE LOW-VALUES TO ACT.
           MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM TAB-FIO-ACT.
           IF NOT FIO-STAT-OKS OR
              ACT-COD-TTAB IN ACT NOT = 'ACT' OR
              ACT-COD-ACTI IN ACT > SPACES
               MOVE 'TAB'        TO MSG-COD-SIST
               MOVE 'TAB    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               DISPLAY MSG-GLS-DESC
               PERFORM GNS-PRO-END.
           MOVE ACT-GLS-DESC IN ACT TO WSS-GLS-DTAB.
           IF ACT-COD-ATRN IN ACT > SPACES
               MOVE FIO-INP TO FIO-CMND
               PERFORM TAB-FIO-USR
               MOVE ACT-COD-ATRN IN ACT TO USR-COD-USER IN USR
               PERFORM BUS-USR
               IF NOT FIO-STAT-OKS
                   MOVE '00'   TO FIO-STAT
                   MOVE SPACES TO USR-GLS-DESC IN USR
               ELSE
                   NEXT SENTENCE      
           ELSE
               MOVE SPACES TO USR-GLS-DESC.
           MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM TAB-FIO-ACT.
      *>>>>
*% END
*% IF LUP_INP
      *<<<< LUP_INP
           IF ACT-COD-TTAB > 'ACT'
               GO TO FIN-INP-SRT.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBBMSG.
       COPY TABBBUSR.
      * COPY GNSBFTAB.
      * COPY GNSBFMSG.
       COPY TABBFUSR.
      *>>>>
*% END
