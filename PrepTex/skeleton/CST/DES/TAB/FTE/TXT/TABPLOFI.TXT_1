*%! Codigo adicional
*% IF WSS_DTC
      *<<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-USR-REQA==.
      * COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSG-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSC-REQA==.
      * COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-TAB-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-AUX-REQA==.
      *>>>>
*% END
*% IF WSS
      *<<<< WSS
      * COPY GNSBRTAB.
       COPY GNSBRMSC.
*% IF PGM_MCP
       COPY GNSWRMSC.
*% END
      * COPY GNSBRMSG.
       COPY TABBRUSR.
       COPY TABBROFI REPLACING ==OFI== BY ==AUX==.
       01  WSS-VARI.
           03 WSS-COD-OFCO                      PIC X(03) VALUE SPACES.
      *>>>>
*% END
*% IF INI_INP
      *<<<< INI_INP
           MOVE FIO-INP TO FIO-CMND.
           MOVE 'TAB'   TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
           MOVE FIO-INP TO FIO-CMND.
           PERFORM TAB-FIO-USR.
           MOVE LOW-VALUES TO TAB.
           MOVE 'OFI' TO TAB-COD-TTAB IN TAB.
           MOVE FIO-GET-NLS TO FIO-CMND.
           MOVE 'TAB'       TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS OR
              TAB-COD-TTAB IN TAB NOT = 'OFI' OR
              TAB-COD-CTAB IN TAB > SPACES
               MOVE 'TAB'        TO MSG-COD-SIST
               MOVE 'TAB    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               DISPLAY MSG-GLS-DESC
               PERFORM GNS-PRO-END.
           MOVE TAB-GLS-DESC IN TAB TO WSS-GLS-DTAB.
           IF TAB-COD-ATRN IN TAB > SPACES
               MOVE TAB-COD-ATRN IN TAB TO USR-COD-USER IN USR
               PERFORM BUS-USR
           ELSE
               MOVE SPACES TO USR-GLS-DESC.
           MOVE USR-GLS-DESC IN USR TO WSS-GLS-RESP.
      *>>>>
*% END
*% IF INI_FST_INP
      *<<<< INI_FST_INP
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR DE APERTURA EN ARCHIVO OFICINAS ' FIO-STAT
               PERFORM GNS-PRO-END.
      *>>>>
*% END
*% IF LUP_INP
      *<<<< LUP_INP
           IF NOT FIO-STAT-OKS
                GO TO FIN-MAIN.
      * RESPALDO LLAVE CURRENT RECORD
           MOVE OFI          TO AUX.
*%!RVM:
*% IF PGM_DTC
           MOVE ADR-OFI-REQA TO ADR-AUX-REQA.
*% END
      * COMUNA
           IF OFI-COD-COMU IN OFI > SPACES
               MOVE 'TAB'               TO TAB-COD-SIST
               MOVE 'COM'               TO TAB-COD-TTAB IN TAB
               MOVE OFI-COD-COMU IN OFI TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO WSS-GLS-COMU IN WSS-OFI
           ELSE
               MOVE SPACES TO WSS-GLS-COMU IN WSS-OFI.
      * PLAZA
           IF OFI-COD-PLZA IN OFI > SPACES
               MOVE 'TAB'               TO TAB-COD-SIST
               MOVE 'PZA'               TO TAB-COD-TTAB IN TAB
               MOVE OFI-COD-PLZA IN OFI TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO WSS-GLS-PLZA IN WSS-OFI
           ELSE
               MOVE SPACES TO WSS-GLS-PLZA IN WSS-OFI.
      * JEFE OFICINA
           IF OFI-COD-JFOF IN OFI > SPACES
               MOVE OFI-COD-JFOF IN OFI TO USR-COD-USER IN USR
               PERFORM BUS-USR
               MOVE USR-GLS-DESC IN USR TO WSS-GLS-JFOF IN WSS-OFI
           ELSE
               MOVE SPACES TO WSS-GLS-JFOF IN WSS-OFI.
      * JEFE OPERACIONES OFICINA
           IF OFI-COD-JOOF IN OFI > SPACES
               MOVE OFI-COD-JOOF IN OFI TO USR-COD-USER IN USR
               PERFORM BUS-USR
               MOVE USR-GLS-DESC IN USR TO WSS-GLS-JOOF IN WSS-OFI
           ELSE
               MOVE SPACES TO WSS-GLS-JOOF IN WSS-OFI.
      * CLASIFICACION OPERATIVA
           IF OFI-IND-COPE IN OFI > SPACES
               MOVE 'TAB'               TO MSC-COD-SIST
               MOVE 'COPE'              TO MSC-COD-TMSC IN MSC
               MOVE OFI-IND-COPE IN OFI TO MSC-COD-CMSC IN MSC
               PERFORM BUS-MSC
               MOVE MSC-GLS-DESC IN MSC TO WSS-GLS-COPE IN WSS-OFI
           ELSE
               MOVE SPACES TO WSS-GLS-COPE IN WSS-OFI.
      * CLASIFICACION COMERCIAL
           IF OFI-IND-CCOM IN OFI > SPACES
               MOVE 'TAB'               TO MSC-COD-SIST
               MOVE 'CCOM'              TO MSC-COD-TMSC IN MSC
               MOVE OFI-IND-CCOM IN OFI TO MSC-COD-CMSC IN MSC
               PERFORM BUS-MSC
               MOVE MSC-GLS-DESC IN MSC TO WSS-GLS-CCOM IN WSS-OFI
           ELSE
               MOVE SPACES TO WSS-GLS-CCOM IN WSS-OFI.
      * RESPALDO OFICINA CENTRALIZACION OPERATIVA
           MOVE OFI-COD-OFCO IN OFI TO WSS-COD-OFCO IN WSS-VARI.
      * OFICINA REGIONAL
           IF OFI-COD-OREG IN OFI > SPACES
               MOVE OFI-COD-OREG IN OFI TO OFI-COD-OFIC IN OFI
               PERFORM BUS-OFI
               MOVE OFI-GLS-DESC IN OFI TO WSS-GLS-OREG IN WSS-OFI
               IF OFI-COD-JFOF IN OFI > SPACES
                   MOVE OFI-COD-JFOF IN OFI TO USR-COD-USER IN USR
                   PERFORM BUS-USR
                   MOVE USR-GLS-DESC IN USR TO WSS-GLS-GREG IN WSS-OFI
               ELSE
                   MOVE SPACES TO WSS-GLS-GREG IN WSS-OFI
           ELSE
               MOVE SPACES TO WSS-GLS-OREG IN WSS-OFI
               MOVE SPACES TO WSS-GLS-GREG IN WSS-OFI.
      * OFICINA CENTRALIZACION OPERATIVA
           IF WSS-COD-OFCO > SPACES
               MOVE WSS-COD-OFCO IN WSS-VARI TO OFI-COD-OFIC IN OFI
               PERFORM BUS-OFI
               MOVE OFI-GLS-DESC IN OFI TO WSS-GLS-OFCO IN WSS-OFI
           ELSE
               MOVE SPACES TO WSS-GLS-OFCO IN WSS-OFI.
      * RECUPERACION CURRENT RECORD
           MOVE AUX          TO OFI.
*%!RVM:
*% IF PGM_DTC
           MOVE ADR-AUX-REQA TO ADR-OFI-REQA.
*% END
      *    MOVE FIO-GET-KEY TO FIO-CMND.
      *    PERFORM TAB-FIO-OFI.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
      * COPY GNSBFTAB.
       COPY GNSBFMSC.
      * COPY GNSBFMSG.
       COPY TABBFUSR.
       COPY TABBBUSR.
       COPY GNSBBTAB.
       COPY GNSBBMSC.
       COPY GNSBBMSG.
       COPY TABBBOFI.
      *>>>>
*% END
