*%! Codigo adicional
*% IF WSS_DTC
      *<<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-USR-REQA==.
      * COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSG-REQA==.
      *JSS COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-TAB-REQA==.
      *>>>>
*% END
*% IF WSS
      *<<<< WSS
      * COPY GNSBRMSG.
       COPY TABBRUSR.
       01  WSS-VARI.
           03 WSS-SIST                               PIC X(03).
      *>>>>
*% END
*% IF INI_INP
      *<<<< INI_INP
           MOVE FIO-INP TO FIO-CMND.
           PERFORM TAB-FIO-USR.
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR DE APERTURA EN ARCHIVO USUARIOS ' FIO-STAT
               PERFORM GNS-PRO-END.

           MOVE 'TAB' TO FIO-SIST.
      *>>>>
*% END
*% IF INI_FST_INP
      *<<<< INI_FST_INP
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR DE APERTURA EN ARCHIVO TABLAS ' FIO-STAT
               PERFORM GNS-PRO-END.

           MOVE LOW-VALUES TO TAB.
           MOVE 'RES' TO TAB-COD-TTAB IN TAB.
           MOVE FIO-GET-NLS TO FIO-CMND.
           MOVE 'TAB'       TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS OR
              TAB-COD-TTAB IN TAB NOT = 'RES' OR
              TAB-COD-CTAB IN TAB > SPACES
               MOVE 'TAB    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               DISPLAY MSG-GLS-DESC
               PERFORM GNS-PRO-END.
           MOVE TAB-GLS-DESC IN TAB TO WSS-GLS-DTAB.

           MOVE TAB-COD-ATRN IN TAB TO TAB-COD-ATRN IN SRT.
           PERFORM BUS-RES.
           MOVE USR-GLS-DESC IN USR TO WSS-GLS-RESP.
      *>>>>
*% END
*% IF LUP_INP
      *<<<< LUP_INP
           IF TAB-COD-CTAB IN TAB > SPACES
               GO TO NXT-INP-SRT.
      *>>>>
*% END
*% IF LUP_OUT
      *<<<< LUP_OUT      
           PERFORM BUS-RES.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       BUS-RES SECTION.
       INI-BUS-RES.
           IF TAB-COD-ATRN IN SRT > SPACES
               MOVE TAB-COD-ATRN IN SRT TO USR-COD-USER IN USR
               PERFORM BUS-USR
           ELSE
               MOVE SPACES TO USR-GLS-DESC IN USR.
       FIN-BUS-RES.
           EXIT.

       COPY GNSBBMSG.
      * COPY GNSBFMSG.
       COPY TABBBUSR.
       COPY TABBFUSR.
      *>>>>
*% END
