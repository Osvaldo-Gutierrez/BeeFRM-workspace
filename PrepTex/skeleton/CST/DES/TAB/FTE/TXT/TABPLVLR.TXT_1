*%! Codigo adicional
*% IF WSS_DTC
      *<<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-USR-REQA==.
      * COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSG-REQA==.
      * COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-TAB-REQA==.
      *>>>>
*% END
*% IF WSS
      *<<<< WSS
      * COPY GNSBRTAB.
      * COPY GNSBRMSG.
       COPY TABBRUSR.
      *>>>>
*% END
*% IF INI_FST_INP 
      *<<<< INI_FST_INP
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR DE APERTURA EN ARCHIVO VLR ' FIO-STAT
               PERFORM GNS-PRO-END.
           GO TO INI-OUT-SRT.
      *>>>>
*% END
*% IF INI_OUT
      *<<<< INI_OUT
           MOVE LOW-VALUES TO VLR.
           MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM TAB-FIO-VLR.
           IF NOT FIO-STAT-OKS OR
              VLR-COD-TTAB IN VLR NOT = 'VLR' OR
              VLR-COD-VCAM IN VLR > SPACES
               MOVE 'TAB'        TO MSG-COD-SIST
               MOVE 'TAB    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               DISPLAY MSG-GLS-DESC
               PERFORM GNS-PRO-END.
           MOVE VLR-GLS-DESC IN VLR TO WSS-GLS-DTAB.
           IF VLR-COD-ATRN IN VLR > SPACES
               MOVE FIO-INP TO FIO-CMND
               PERFORM TAB-FIO-USR
               MOVE VLR-COD-ATRN IN VLR TO USR-COD-USER IN USR
               PERFORM BUS-USR
               IF NOT FIO-STAT-OKS
                   MOVE '00'   TO FIO-STAT
                   MOVE SPACES TO USR-GLS-DESC IN USR
               ELSE
                   NEXT SENTENCE      
           ELSE
               MOVE SPACES TO USR-GLS-DESC.
           MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM TAB-FIO-VLR.
      *>>>>
*% END
*% IF LUP_INP
      *<<<< LUP_INP
           IF VLR-COD-TTAB > 'VLR'
               GO TO FIN-INP-SRT.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
      * COPY GNSBFTAB.
      * COPY GNSBFMSG.
       COPY TABBFUSR.
       COPY TABBBUSR.
       COPY GNSBBMSG.
      *>>>>
*% END
