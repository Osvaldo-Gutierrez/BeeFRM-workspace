      * 16-Mar-94 04:14 PM
       RET-ALL-T13 SECTION.
       INI-RET-ALL-T13.
           PERFORM KEY-ALL-T13.
           PERFORM RET-T13-T13.
       FIN-RET-ALL-T13.
           EXIT.
       
       RET-T13-T13 SECTION.
       INI-RET-T13-T13.
           MOVE T13-GLS-FLAG IN T13-FLD TO T13-GLS-FLAG IN T13.
           MOVE T13-COD-SIST IN T13-FLD TO T13-COD-SIST IN T13.
           MOVE T13-VAL-MONT IN T13-FLD TO T13-VAL-MONT IN T13.
           MOVE T13-GLS-DISP IN T13-FLD TO T13-GLS-DISP IN T13.
       FIN-RET-T13-T13.
           EXIT.
       
       KEY-ALL-T13 SECTION.
       INI-KEY-ALL-T13.
           PERFORM KEY-T13-T13.
       FIN-KEY-ALL-T13.
           EXIT.
       
       KEY-T13-T13 SECTION.
       INI-KEY-T13-T13.
           MOVE T13-COD-TIPO IN T13-FLD TO T13-COD-TIPO IN T13.
           MOVE T13-CIC-VCAM IN T13-FLD TO T13-CIC-VCAM IN T13.
           MOVE T13-COD-CORR IN T13-FLD TO T13-COD-CORR IN T13.
           MOVE T13-NUM-LINE IN T13-FLD TO T13-NUM-LINE IN T13.
       FIN-KEY-T13-T13.
           EXIT.
       
       SCR-CLRA-T13 SECTION.
       INI-SCR-CLRA-T13.
           PERFORM CLR-KEY-T13.
           PERFORM CLR-FLD-T13.
       FIN-SCR-CLRA-T13.
           EXIT.
       
       CLR-KEY-T13 SECTION.
       INI-CLR-KEY-T13.
           MOVE SPACES TO T13-COD-TIPO IN T13-FLD.
           MOVE SPACES TO T13-CIC-VCAM IN T13-FLD.
           MOVE SPACES TO T13-COD-CORR IN T13-FLD.
           MOVE ZEROES TO T13-NUM-LINE IN T13-FLD.
       FIN-CLR-KEY-T13.
           EXIT.
       
       CLR-FLD-T13 SECTION.
       INI-CLR-FLD-T13.
           PERFORM CLR-T13-T13.
       FIN-CLR-FLD-T13.
           EXIT.
       
       CLR-T13-T13 SECTION.
       INI-CLR-T13-T13.
           MOVE SPACES TO T13-GLS-FLAG IN T13-FLD.
           MOVE SPACES TO T13-COD-SIST IN T13-FLD.
           MOVE ZEROES TO T13-VAL-MONT IN T13-FLD.
           MOVE SPACES TO T13-GLS-DISP IN T13-FLD.
       FIN-CLR-T13-T13.
           EXIT.
       
       FST-KEY-T13 SECTION.
       INI-FST-KEY-T13.
           MOVE -1 TO T13-COD-TIPO-LEN IN T13-FLD.
       FIN-FST-KEY-T13.
           EXIT.
       
       FST-FLD-T13 SECTION.
       INI-FST-FLD-T13.
           MOVE -1 TO T13-GLS-FLAG-LEN IN T13-FLD.
       FIN-FST-FLD-T13.
           EXIT.
       
       PRO-KEY-T13 SECTION.
       INI-PRO-KEY-T13.
           PERFORM PRO-MKY-T13.
       FIN-PRO-KEY-T13.
           EXIT.
       
       PRO-MKY-T13 SECTION.
       INI-PRO-MKY-T13.
       FIN-PRO-MKY-T13.
           EXIT.
       
       PRO-FLD-T13 SECTION.
       INI-PRO-FLD-T13.
           PERFORM PRO-T13-T13.
       FIN-PRO-FLD-T13.
           EXIT.
       
       PRO-T13-T13 SECTION.
       INI-PRO-T13-T13.
           MOVE FRM-CPIM-AEY TO T13-GLS-FLAG-ATR IN T13-FLD.
           MOVE FRM-CPIM-AEY TO T13-COD-SIST-ATR IN T13-FLD.
           MOVE FRM-CPIM-AEY TO T13-VAL-MONT-ATR IN T13-FLD.
           MOVE FRM-CPIM-AEY TO T13-GLS-DISP-ATR IN T13-FLD.
       FIN-PRO-T13-T13.
           EXIT.
       
       UNP-KEY-T13 SECTION.
       INI-UNP-KEY-T13.
           PERFORM UNP-IKY-T13.
           PERFORM UNP-AKY-T13.
           PERFORM UNP-MKY-T13.
       FIN-UNP-KEY-T13.
           EXIT.
       
       UNP-IKY-T13 SECTION.
       INI-UNP-IKY-T13.
           MOVE FRM-CPIM-UBY TO T13-COD-TIPO-ATR.
           MOVE FRM-CPIM-UBY TO T13-CIC-VCAM-ATR.
           MOVE FRM-CPIM-UBY TO T13-COD-CORR-ATR.
           MOVE FRM-CPIM-UBY TO T13-NUM-LINE-ATR.
       FIN-UNP-IKY-T13.
           EXIT.
       
       UNP-MKY-T13 SECTION.
       INI-UNP-MKY-T13.
       FIN-UNP-MKY-T13.
           EXIT.
       
       UNP-AKY-T13 SECTION.
       INI-UNP-AKY-T13.
       FIN-UNP-AKY-T13.
           EXIT.
       
       UNP-FLD-T13 SECTION.
       INI-UNP-FLD-T13.
           PERFORM UNP-T13-T13.
       FIN-UNP-FLD-T13.
           EXIT.
       
       UNP-T13-T13 SECTION.
       INI-UNP-T13-T13.
           MOVE FRM-CPIM-UBY TO T13-GLS-FLAG-ATR IN T13-FLD.
           MOVE FRM-CPIM-UBY TO T13-COD-SIST-ATR IN T13-FLD.
           MOVE FRM-CPIM-UBY TO T13-VAL-MONT-ATR IN T13-FLD.
           MOVE FRM-CPIM-UBY TO T13-GLS-DISP-ATR IN T13-FLD.
       FIN-UNP-T13-T13.
           EXIT.
       
       VAL-MKY-T13 SECTION.
       INI-VAL-MKY-T13.
       FIN-VAL-MKY-T13.
           EXIT.
       
       VAL-FLD-T13 SECTION.
       INI-VAL-FLD-T13.
           PERFORM VAL-T13-GLS-FLAG.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO T13-GLS-FLAG-LEN IN T13-FLD
                GO TO FIN-VAL-FLD-T13.
           PERFORM VAL-T13-COD-SIST.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO T13-COD-SIST-LEN IN T13-FLD
                GO TO FIN-VAL-FLD-T13.
           PERFORM VAL-T13-VAL-MONT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO T13-VAL-MONT-LEN IN T13-FLD
                GO TO FIN-VAL-FLD-T13.
           PERFORM VAL-T13-GLS-DISP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO T13-GLS-DISP-LEN IN T13-FLD
                GO TO FIN-VAL-FLD-T13.
       FIN-VAL-FLD-T13.
           EXIT.
       
       VAL-T13-GLS-FLAG SECTION.
       INI-VAL-T13-GLS-FLAG.
           IF T13-GLS-FLAG IN T13-FLD NOT > SPACES
              MOVE SPACES TO T13-GLS-FLAG IN T13-FLD.
       FIN-VAL-T13-GLS-FLAG.
           EXIT.
       
       VAL-T13-COD-SIST SECTION.
       INI-VAL-T13-COD-SIST.
           IF T13-COD-SIST IN T13-FLD NOT > SPACES
              MOVE SPACES TO T13-COD-SIST IN T13-FLD.
       FIN-VAL-T13-COD-SIST.
           EXIT.
       
       VAL-T13-VAL-MONT SECTION.
       INI-VAL-T13-VAL-MONT.
       FIN-VAL-T13-VAL-MONT.
           EXIT.
       
       VAL-T13-GLS-DISP SECTION.
       INI-VAL-T13-GLS-DISP.
           IF T13-GLS-DISP IN T13-FLD NOT > SPACES
              MOVE SPACES TO T13-GLS-DISP IN T13-FLD.
       FIN-VAL-T13-GLS-DISP.
           EXIT.
       
       VAL-CON-FLD-T13 SECTION.
       INI-VAL-CON-FLD-T13.
       FIN-VAL-CON-FLD-T13.
           EXIT.
       
       VAL-CON-T13 SECTION.
       INI-VAL-CON-T13.
       FIN-VAL-CON-T13.
           EXIT.
       
       VAL-NUM-KEY-T13 SECTION.
       INI-VAL-NUM-KEY-T13.
           PERFORM VAL-NUM-IKY-T13.
           PERFORM VAL-NUM-MKY-T13.
           PERFORM VAL-NUM-AKY-T13.
       FIN-VAL-NUM-KEY-T13.
           EXIT.
       
       VAL-NUM-IKY-T13 SECTION.
       INI-VAL-NUM-IKY-T13.
       VAL-NUM-T13-NUM-LINE.
           MOVE T13-NUM-LINE-ALF IN T13-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO T13-NUM-LINE IN T13-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO T13-NUM-LINE-LEN IN T13-FLD
                GO TO FIN-VAL-NUM-IKY-T13.
       FIN-VAL-NUM-IKY-T13.
           EXIT.
       
       VAL-NUM-MKY-T13 SECTION.
       INI-VAL-NUM-MKY-T13.
       FIN-VAL-NUM-MKY-T13.
           EXIT.
       
       VAL-NUM-AKY-T13 SECTION.
       INI-VAL-NUM-AKY-T13.
       FIN-VAL-NUM-AKY-T13.
           EXIT.
       
       VAL-NUM-FLD-T13 SECTION.
       INI-VAL-NUM-FLD-T13.
       VAL-NUM-T13-VAL-MONT.
           MOVE T13-VAL-MONT-ALF IN T13-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 12 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO T13-VAL-MONT IN T13-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO T13-VAL-MONT-LEN IN T13-FLD
                GO TO FIN-VAL-NUM-FLD-T13.
       FIN-VAL-NUM-FLD-T13.
           EXIT.
       
       EDT-KEY-T13 SECTION.
       INI-EDT-KEY-T13.
           PERFORM EDT-IKY-T13.
           PERFORM EDT-MKY-T13.
           PERFORM EDT-AKY-T13.
       FIN-EDT-KEY-T13.
           EXIT.
       
       EDT-IKY-T13 SECTION.
       INI-EDT-IKY-T13.
           IF T13-NUM-LINE-LEN IN T13-FLD = -9
                MOVE -1 TO T13-NUM-LINE-LEN IN T13-FLD
                GO TO FIN-EDT-IKY-T13.
           MOVE T13-NUM-LINE IN T13-FLD TO T13-NUM-LINE-EDT IN T13-EDT.
           MOVE T13-NUM-LINE-EDT IN T13-EDT TO 
                T13-NUM-LINE-ALF IN T13-FLD.
       FIN-EDT-IKY-T13.
           EXIT.
       
       EDT-MKY-T13 SECTION.
       INI-EDT-MKY-T13.
       FIN-EDT-MKY-T13.
           EXIT.
       
       EDT-AKY-T13 SECTION.
       INI-EDT-AKY-T13.
       FIN-EDT-AKY-T13.
           EXIT.
       
       EDT-FLD-T13 SECTION.
       INI-EDT-FLD-T13.
           IF T13-VAL-MONT-LEN IN T13-FLD = -9
                MOVE -1 TO T13-VAL-MONT-LEN IN T13-FLD
                GO TO FIN-EDT-FLD-T13.
           MOVE T13-VAL-MONT IN T13-FLD TO T13-VAL-MONT-EDT IN T13-EDT.
           MOVE T13-VAL-MONT-EDT IN T13-EDT TO 
                T13-VAL-MONT-ALF IN T13-FLD.
       FIN-EDT-FLD-T13.
           EXIT.
