      * 10-Mar-94 12:22 PM
       PUT-ALL-P11 SECTION.
       INI-PUT-ALL-P11.
           PERFORM PUT-P11-P11.
      *<<<< FIN_PUT_ALL_P11
       GET-FRM-GLS-PROD.
           MOVE 'SUP' TO TAB-COD-SIST.
           MOVE 'PRD' TO TAB-COD-TTAB IN TAB.
           MOVE P11-COD-PROD IN P11-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM BUS-TAB.
           MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-PROD IN P11-FLD.
       GET-FRM-GLS-TMON.
           MOVE 'SUP' TO MSC-COD-SIST.
           MOVE 'TVAL' TO MSC-COD-TMSC IN MSC.
           MOVE P11-IND-TMON IN P11-FLD TO MSC-COD-CMSC IN MSC.
           PERFORM BUS-MSC.
           MOVE MSC-GLS-DESC IN MSC TO FRM-GLS-TMON IN P11-FLD.
       GET-FRM-GLS-SECT.
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'ODS' TO TAB-EXT-TTAB IN TAB.
           MOVE P11-COD-SECT IN P11-FLD TO TAB-EXT-CTAB IN TAB.
           MOVE 'TAB-EXT-TABL' TO FIO-AKEY.
           PERFORM BUS-TAB.
           MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-SECT IN P11-FLD.
       GET-FRM-GLS-LOCA.
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'COM' TO TAB-EXT-TTAB IN TAB.
           MOVE P11-COD-LOCA IN P11-FLD  TO WSS-COD-LOCA IN WSS-VARI.
           MOVE WSS-COD-PLZA IN WSS-VARI TO TAB-EXT-CTAB IN TAB.
           MOVE 'TAB-EXT-TABL' TO FIO-AKEY.
           PERFORM BUS-TAB.
           MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-LOCA IN P11-FLD.
      *>>>>
       FIN-PUT-ALL-P11.
           EXIT.
       
       PUT-P11-P11 SECTION.
       INI-PUT-P11-P11.
           IF PGM-STAT-P11-NUL
               GO TO FIN-PUT-P11-P11.
           IF NOT PGM-STAT-P11-OKS
               PERFORM CLR-P11-P11
               GO TO FIN-PUT-P11-P11.
           MOVE P11-COD-PROD IN P11 TO P11-COD-PROD IN P11-FLD.
           MOVE P11-IND-TMON IN P11 TO P11-IND-TMON IN P11-FLD.
           MOVE P11-COD-SECT IN P11 TO P11-COD-SECT IN P11-FLD.
           MOVE P11-COD-LOCA IN P11 TO P11-COD-LOCA IN P11-FLD.
           MOVE P11-VAL-VSFM IN P11 TO P11-VAL-VSFM IN P11-FLD.
       FIN-PUT-P11-P11.
           EXIT.
       
       KEY-ALL-P11 SECTION.
       INI-KEY-ALL-P11.
           PERFORM KEY-P11-P11.
       FIN-KEY-ALL-P11.
           EXIT.
       
       KEY-P11-P11 SECTION.
       INI-KEY-P11-P11.
           MOVE P11-COD-PROD IN P11-FLD TO P11-COD-PROD IN P11.
           MOVE P11-IND-TMON IN P11-FLD TO P11-IND-TMON IN P11.
           MOVE P11-COD-SECT IN P11-FLD TO P11-COD-SECT IN P11.
           MOVE P11-COD-LOCA IN P11-FLD TO P11-COD-LOCA IN P11.
       FIN-KEY-P11-P11.
           EXIT.
       
       SCR-CLRA-P11 SECTION.
       INI-SCR-CLRA-P11.
           PERFORM CLR-KEY-P11.
           PERFORM CLR-FLD-P11.
       FIN-SCR-CLRA-P11.
           EXIT.
       
       CLR-KEY-P11 SECTION.
       INI-CLR-KEY-P11.
           MOVE SPACES TO P11-COD-PROD IN P11-FLD.
           MOVE SPACES TO P11-IND-TMON IN P11-FLD.
           MOVE SPACES TO P11-COD-SECT IN P11-FLD.
           MOVE SPACES TO P11-COD-LOCA IN P11-FLD.
       FIN-CLR-KEY-P11.
           EXIT.
       
       CLR-FLD-P11 SECTION.
       INI-CLR-FLD-P11.
           MOVE SPACES TO FRM-GLS-PROD IN P11-FLD.
           MOVE SPACES TO FRM-GLS-TMON IN P11-FLD.
           MOVE SPACES TO FRM-GLS-SECT IN P11-FLD.
           MOVE SPACES TO FRM-GLS-LOCA IN P11-FLD.
           PERFORM CLR-P11-P11.
       FIN-CLR-FLD-P11.
           EXIT.
       
       CLR-P11-P11 SECTION.
       INI-CLR-P11-P11.
           MOVE ZEROES TO P11-VAL-VSFM IN P11-FLD.
       FIN-CLR-P11-P11.
           EXIT.
       
       FST-KEY-P11 SECTION.
       INI-FST-KEY-P11.
           MOVE -1 TO P11-COD-PROD-LEN IN P11-FLD.
       FIN-FST-KEY-P11.
           EXIT.
       
       FST-FLD-P11 SECTION.
       INI-FST-FLD-P11.
           MOVE -1 TO P11-VAL-VSFM-LEN IN P11-FLD.
       FIN-FST-FLD-P11.
           EXIT.
       
       PRO-KEY-P11 SECTION.
       INI-PRO-KEY-P11.
           PERFORM PRO-IKY-P11.
           PERFORM PRO-AKY-P11.
           PERFORM PRO-MKY-P11.
       FIN-PRO-KEY-P11.
           EXIT.
       
       PRO-IKY-P11 SECTION.
       INI-PRO-IKY-P11.
           MOVE FRM-CPIM-AEY TO P11-COD-PROD-ATR IN P11-FLD.
           MOVE FRM-CPIM-AEY TO P11-IND-TMON-ATR IN P11-FLD.
           MOVE FRM-CPIM-AEY TO P11-COD-SECT-ATR IN P11-FLD.
           MOVE FRM-CPIM-AEY TO P11-COD-LOCA-ATR IN P11-FLD.
       FIN-PRO-IKY-P11.
           EXIT.
       
       PRO-MKY-P11 SECTION.
       INI-PRO-MKY-P11.
       FIN-PRO-MKY-P11.
           EXIT.
       
       PRO-AKY-P11 SECTION.
       INI-PRO-AKY-P11.
       FIN-PRO-AKY-P11.
           EXIT.
       
       UNP-KEY-P11 SECTION.
       INI-UNP-KEY-P11.
           PERFORM UNP-IKY-P11.
           PERFORM UNP-AKY-P11.
           PERFORM UNP-MKY-P11.
       FIN-UNP-KEY-P11.
           EXIT.
       
       UNP-IKY-P11 SECTION.
       INI-UNP-IKY-P11.
           MOVE FRM-CPIM-UBY TO P11-COD-PROD-ATR.
           MOVE FRM-CPIM-UBY TO P11-IND-TMON-ATR.
           MOVE FRM-CPIM-UBY TO P11-COD-SECT-ATR.
           MOVE FRM-CPIM-UBY TO P11-COD-LOCA-ATR.
       FIN-UNP-IKY-P11.
           EXIT.
       
       UNP-MKY-P11 SECTION.
       INI-UNP-MKY-P11.
       FIN-UNP-MKY-P11.
           EXIT.
       
       UNP-AKY-P11 SECTION.
       INI-UNP-AKY-P11.
       FIN-UNP-AKY-P11.
           EXIT.
       
       UNP-FLD-P11 SECTION.
       INI-UNP-FLD-P11.
           PERFORM UNP-P11-P11.
       FIN-UNP-FLD-P11.
           EXIT.
       
       UNP-P11-P11 SECTION.
       INI-UNP-P11-P11.
           MOVE FRM-CPIM-UBY TO P11-VAL-VSFM-ATR IN P11-FLD.
       FIN-UNP-P11-P11.
           EXIT.
       
       VAL-KEY-P11 SECTION.
       INI-VAL-KEY-P11.
           PERFORM VAL-IKY-P11.
           PERFORM VAL-MKY-P11.
           PERFORM VAL-AKY-P11.
       FIN-VAL-KEY-P11.
           EXIT.
       
       VAL-IKY-P11 SECTION.
       INI-VAL-IKY-P11.
           PERFORM VAL-P11-COD-PROD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO P11-COD-PROD-LEN IN P11-FLD
                GO TO FIN-VAL-IKY-P11.
           PERFORM VAL-P11-IND-TMON.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO P11-IND-TMON-LEN IN P11-FLD
                GO TO FIN-VAL-IKY-P11.
           PERFORM VAL-P11-COD-SECT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO P11-COD-SECT-LEN IN P11-FLD
                GO TO FIN-VAL-IKY-P11.
           PERFORM VAL-P11-COD-LOCA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO P11-COD-LOCA-LEN IN P11-FLD
                GO TO FIN-VAL-IKY-P11.
       FIN-VAL-IKY-P11.
           EXIT.
       
       VAL-MKY-P11 SECTION.
       INI-VAL-MKY-P11.
       FIN-VAL-MKY-P11.
           EXIT.
       
       VAL-AKY-P11 SECTION.
       INI-VAL-AKY-P11.
       FIN-VAL-AKY-P11.
           EXIT.
       
       VAL-P11-COD-PROD SECTION.
       INI-VAL-P11-COD-PROD.
           IF P11-COD-PROD IN P11-FLD NOT > SPACES
              MOVE SPACES TO P11-COD-PROD IN P11-FLD.
      *<<<< P11_COD_PROD
      *    Validacion codigo de producto
           MOVE 'SUP' TO TAB-COD-SIST.
           MOVE 'PRD' TO TAB-COD-TTAB IN TAB.
           MOVE P11-COD-PROD IN P11-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT = 'COD    NEX'
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-PROD IN P11-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-PROD IN P11-FLD.
      *>>>>
       FIN-VAL-P11-COD-PROD.
           EXIT.
       
       VAL-P11-IND-TMON SECTION.
       INI-VAL-P11-IND-TMON.
           IF P11-IND-TMON IN P11-FLD NOT > SPACES
              MOVE SPACES TO P11-IND-TMON IN P11-FLD.
      *<<<< P11_IND_TMON
      *    Validacion codigo de moneda
           MOVE 'SUP' TO MSC-COD-SIST.
           MOVE 'TVAL' TO MSC-COD-TMSC IN MSC.
           MOVE P11-IND-TMON IN P11-FLD TO MSC-COD-CMSC IN MSC.
           PERFORM VAL-IND.
           IF MSG-COD-MENS NOT = 'COD    NEX'
               MOVE MSC-GLS-DESC IN MSC TO FRM-GLS-TMON IN P11-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-TMON IN P11-FLD.
      *>>>>
       FIN-VAL-P11-IND-TMON.
           EXIT.
       
       VAL-P11-COD-SECT SECTION.
       INI-VAL-P11-COD-SECT.
           IF P11-COD-SECT IN P11-FLD NOT > SPACES
              MOVE SPACES TO P11-COD-SECT IN P11-FLD.
      *<<<< P11_COD_SECT
      *    Validacion codigo de sector
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'ODS' TO TAB-EXT-TTAB IN TAB.
           MOVE P11-COD-SECT IN P11-FLD TO TAB-EXT-CTAB IN TAB.
           MOVE 'TAB-EXT-TABL' TO FIO-AKEY.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT = 'COD    NEX'
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-SECT IN P11-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-SECT IN P11-FLD.
      *>>>>
       FIN-VAL-P11-COD-SECT.
           EXIT.
       
       VAL-P11-COD-LOCA SECTION.
       INI-VAL-P11-COD-LOCA.
           IF P11-COD-LOCA IN P11-FLD NOT > SPACES
              MOVE SPACES TO P11-COD-LOCA IN P11-FLD.
      *<<<< P11_COD_LOCA
      *    Validacion codigo de localidad
           MOVE P11-COD-LOCA IN P11-FLD TO WSS-COD-LOCA IN WSS-VARI.
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'COM' TO TAB-EXT-TTAB IN TAB.
           MOVE WSS-COD-PLZA IN WSS-VARI TO TAB-EXT-CTAB IN TAB.
           MOVE 'TAB-EXT-TABL' TO FIO-AKEY.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT = 'COD    NEX'
               MOVE TAB TO COM
               IF COM-COD-REGI IN COM = WSS-COD-REGI
                   MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-LOCA IN P11-FLD
               ELSE
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   MOVE 'CODIGO INEXISTENTE' TO FRM-MENS
                   MOVE SPACES TO FRM-GLS-LOCA IN P11-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-LOCA IN P11-FLD.
      *>>>>
       FIN-VAL-P11-COD-LOCA.
           EXIT.
       
       VAL-CON-KEY-P11 SECTION.
       INI-VAL-CON-KEY-P11.
           IF     P11-COD-PROD IN P11-FLD NOT > SPACES 
              AND P11-IND-TMON IN P11-FLD NOT > SPACES 
              AND P11-COD-SECT IN P11-FLD NOT > SPACES 
              AND P11-COD-LOCA IN P11-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-P11.
       FIN-VAL-CON-KEY-P11.
           EXIT.
       
       VAL-CON-IKY-P11 SECTION.
       INI-VAL-CON-IKY-P11.
           IF     P11-COD-PROD IN P11-FLD NOT > SPACES 
              AND P11-IND-TMON IN P11-FLD NOT > SPACES 
              AND P11-COD-SECT IN P11-FLD NOT > SPACES 
              AND P11-COD-LOCA IN P11-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-P11.
       FIN-VAL-CON-IKY-P11.
           EXIT.
       
       VAL-NUM-KEY-P11 SECTION.
       INI-VAL-NUM-KEY-P11.
           PERFORM VAL-NUM-IKY-P11.
           PERFORM VAL-NUM-MKY-P11.
           PERFORM VAL-NUM-AKY-P11.
       FIN-VAL-NUM-KEY-P11.
           EXIT.
       
       VAL-NUM-IKY-P11 SECTION.
       INI-VAL-NUM-IKY-P11.
       FIN-VAL-NUM-IKY-P11.
           EXIT.
       
       VAL-NUM-MKY-P11 SECTION.
       INI-VAL-NUM-MKY-P11.
       FIN-VAL-NUM-MKY-P11.
           EXIT.
       
       VAL-NUM-AKY-P11 SECTION.
       INI-VAL-NUM-AKY-P11.
       FIN-VAL-NUM-AKY-P11.
           EXIT.
       
       VAL-NUM-FLD-P11 SECTION.
       INI-VAL-NUM-FLD-P11.
       VAL-NUM-P11-VAL-VSFM.
           MOVE P11-VAL-VSFM-ALF IN P11-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 2 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9V TO P11-VAL-VSFM IN P11-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO P11-VAL-VSFM-LEN IN P11-FLD
                GO TO FIN-VAL-NUM-FLD-P11.
       FIN-VAL-NUM-FLD-P11.
           EXIT.
       
       EDT-KEY-P11 SECTION.
       INI-EDT-KEY-P11.
           PERFORM EDT-IKY-P11.
           PERFORM EDT-MKY-P11.
           PERFORM EDT-AKY-P11.
       FIN-EDT-KEY-P11.
           EXIT.
       
       EDT-IKY-P11 SECTION.
       INI-EDT-IKY-P11.
       FIN-EDT-IKY-P11.
           EXIT.
       
       EDT-MKY-P11 SECTION.
       INI-EDT-MKY-P11.
       FIN-EDT-MKY-P11.
           EXIT.
       
       EDT-AKY-P11 SECTION.
       INI-EDT-AKY-P11.
       FIN-EDT-AKY-P11.
           EXIT.
       
       EDT-FLD-P11 SECTION.
       INI-EDT-FLD-P11.
           IF P11-VAL-VSFM-LEN IN P11-FLD = -9
                MOVE -1 TO P11-VAL-VSFM-LEN IN P11-FLD
                GO TO FIN-EDT-FLD-P11.
           MOVE P11-VAL-VSFM IN P11-FLD TO P11-VAL-VSFM-EDT IN P11-EDT.
           MOVE P11-VAL-VSFM-EDT IN P11-EDT TO 
                P11-VAL-VSFM-ALF IN P11-FLD.
       FIN-EDT-FLD-P11.
           EXIT.
