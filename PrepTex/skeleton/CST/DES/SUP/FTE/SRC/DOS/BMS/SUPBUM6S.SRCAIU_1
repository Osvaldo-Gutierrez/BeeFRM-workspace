      * 16-Mar-94 04:14 PM
       RET-ALL-M6S SECTION.
       INI-RET-ALL-M6S.
           PERFORM KEY-ALL-M6S.
           PERFORM RET-M6S-M6S.
       FIN-RET-ALL-M6S.
           EXIT.
       
       RET-M6S-M6S SECTION.
       INI-RET-M6S-M6S.
           MOVE M6S-COD-SIST IN M6S-FLD TO M6S-COD-SIST IN M6S.
           MOVE M6S-VAL-MONT IN M6S-FLD TO M6S-VAL-MONT IN M6S.
           MOVE M6S-GLS-DISP IN M6S-FLD TO M6S-GLS-DISP IN M6S.
       FIN-RET-M6S-M6S.
           EXIT.
       
       KEY-ALL-M6S SECTION.
       INI-KEY-ALL-M6S.
           PERFORM KEY-M6S-M6S.
       FIN-KEY-ALL-M6S.
           EXIT.
       
       KEY-M6S-M6S SECTION.
       INI-KEY-M6S-M6S.
           MOVE M6S-COD-REGI IN M6S-FLD TO M6S-COD-REGI IN M6S.
           MOVE M6S-COD-CORR IN M6S-FLD TO M6S-COD-CORR IN M6S.
           MOVE M6S-NUM-LINE IN M6S-FLD TO M6S-NUM-LINE IN M6S.
           MOVE M6S-CIC-VCAM IN M6S-FLD TO M6S-CIC-VCAM IN M6S.
           MOVE M6S-COD-ORDE IN M6S-FLD TO M6S-COD-ORDE IN M6S.
       FIN-KEY-M6S-M6S.
           EXIT.
       
       SCR-CLRA-M6S SECTION.
       INI-SCR-CLRA-M6S.
           PERFORM CLR-KEY-M6S.
           PERFORM CLR-FLD-M6S.
       FIN-SCR-CLRA-M6S.
           EXIT.
       
       CLR-KEY-M6S SECTION.
       INI-CLR-KEY-M6S.
           MOVE SPACES TO M6S-COD-REGI IN M6S-FLD.
           MOVE SPACES TO M6S-COD-CORR IN M6S-FLD.
           MOVE ZEROES TO M6S-NUM-LINE IN M6S-FLD.
           MOVE SPACES TO M6S-CIC-VCAM IN M6S-FLD.
           MOVE SPACES TO M6S-COD-ORDE IN M6S-FLD.
       FIN-CLR-KEY-M6S.
           EXIT.
       
       CLR-FLD-M6S SECTION.
       INI-CLR-FLD-M6S.
           PERFORM CLR-M6S-M6S.
       FIN-CLR-FLD-M6S.
           EXIT.
       
       CLR-M6S-M6S SECTION.
       INI-CLR-M6S-M6S.
           MOVE SPACES TO M6S-COD-SIST IN M6S-FLD.
           MOVE ZEROES TO M6S-VAL-MONT IN M6S-FLD.
           MOVE SPACES TO M6S-GLS-DISP IN M6S-FLD.
       FIN-CLR-M6S-M6S.
           EXIT.
       
       FST-KEY-M6S SECTION.
       INI-FST-KEY-M6S.
           MOVE -1 TO M6S-COD-REGI-LEN IN M6S-FLD.
       FIN-FST-KEY-M6S.
           EXIT.
       
       FST-FLD-M6S SECTION.
       INI-FST-FLD-M6S.
           MOVE -1 TO M6S-COD-SIST-LEN IN M6S-FLD.
       FIN-FST-FLD-M6S.
           EXIT.
       
       PRO-KEY-M6S SECTION.
       INI-PRO-KEY-M6S.
           PERFORM PRO-MKY-M6S.
       FIN-PRO-KEY-M6S.
           EXIT.
       
       PRO-MKY-M6S SECTION.
       INI-PRO-MKY-M6S.
       FIN-PRO-MKY-M6S.
           EXIT.
       
       PRO-FLD-M6S SECTION.
       INI-PRO-FLD-M6S.
           PERFORM PRO-M6S-M6S.
       FIN-PRO-FLD-M6S.
           EXIT.
       
       PRO-M6S-M6S SECTION.
       INI-PRO-M6S-M6S.
           MOVE FRM-CPIM-AEY TO M6S-COD-SIST-ATR IN M6S-FLD.
           MOVE FRM-CPIM-AEY TO M6S-VAL-MONT-ATR IN M6S-FLD.
           MOVE FRM-CPIM-AEY TO M6S-GLS-DISP-ATR IN M6S-FLD.
       FIN-PRO-M6S-M6S.
           EXIT.
       
       UNP-KEY-M6S SECTION.
       INI-UNP-KEY-M6S.
           PERFORM UNP-IKY-M6S.
           PERFORM UNP-AKY-M6S.
           PERFORM UNP-MKY-M6S.
       FIN-UNP-KEY-M6S.
           EXIT.
       
       UNP-IKY-M6S SECTION.
       INI-UNP-IKY-M6S.
           MOVE FRM-CPIM-UBY TO M6S-COD-REGI-ATR.
           MOVE FRM-CPIM-UBY TO M6S-COD-CORR-ATR.
           MOVE FRM-CPIM-UBY TO M6S-NUM-LINE-ATR.
           MOVE FRM-CPIM-UBY TO M6S-CIC-VCAM-ATR.
           MOVE FRM-CPIM-UBY TO M6S-COD-ORDE-ATR.
       FIN-UNP-IKY-M6S.
           EXIT.
       
       UNP-MKY-M6S SECTION.
       INI-UNP-MKY-M6S.
       FIN-UNP-MKY-M6S.
           EXIT.
       
       UNP-AKY-M6S SECTION.
       INI-UNP-AKY-M6S.
       FIN-UNP-AKY-M6S.
           EXIT.
       
       UNP-FLD-M6S SECTION.
       INI-UNP-FLD-M6S.
           PERFORM UNP-M6S-M6S.
       FIN-UNP-FLD-M6S.
           EXIT.
       
       UNP-M6S-M6S SECTION.
       INI-UNP-M6S-M6S.
           MOVE FRM-CPIM-UBY TO M6S-COD-SIST-ATR IN M6S-FLD.
           MOVE FRM-CPIM-UBY TO M6S-VAL-MONT-ATR IN M6S-FLD.
           MOVE FRM-CPIM-UBY TO M6S-GLS-DISP-ATR IN M6S-FLD.
       FIN-UNP-M6S-M6S.
           EXIT.
       
       VAL-MKY-M6S SECTION.
       INI-VAL-MKY-M6S.
       FIN-VAL-MKY-M6S.
           EXIT.
       
       VAL-FLD-M6S SECTION.
       INI-VAL-FLD-M6S.
           PERFORM VAL-M6S-COD-SIST.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO M6S-COD-SIST-LEN IN M6S-FLD
                GO TO FIN-VAL-FLD-M6S.
           PERFORM VAL-M6S-VAL-MONT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO M6S-VAL-MONT-LEN IN M6S-FLD
                GO TO FIN-VAL-FLD-M6S.
           PERFORM VAL-M6S-GLS-DISP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO M6S-GLS-DISP-LEN IN M6S-FLD
                GO TO FIN-VAL-FLD-M6S.
       FIN-VAL-FLD-M6S.
           EXIT.
       
       VAL-M6S-COD-SIST SECTION.
       INI-VAL-M6S-COD-SIST.
           IF M6S-COD-SIST IN M6S-FLD NOT > SPACES
              MOVE SPACES TO M6S-COD-SIST IN M6S-FLD.
       FIN-VAL-M6S-COD-SIST.
           EXIT.
       
       VAL-M6S-VAL-MONT SECTION.
       INI-VAL-M6S-VAL-MONT.
       FIN-VAL-M6S-VAL-MONT.
           EXIT.
       
       VAL-M6S-GLS-DISP SECTION.
       INI-VAL-M6S-GLS-DISP.
           IF M6S-GLS-DISP IN M6S-FLD NOT > SPACES
              MOVE SPACES TO M6S-GLS-DISP IN M6S-FLD.
       FIN-VAL-M6S-GLS-DISP.
           EXIT.
       
       VAL-CON-FLD-M6S SECTION.
       INI-VAL-CON-FLD-M6S.
       FIN-VAL-CON-FLD-M6S.
           EXIT.
       
       VAL-CON-M6S SECTION.
       INI-VAL-CON-M6S.
       FIN-VAL-CON-M6S.
           EXIT.
       
       VAL-NUM-KEY-M6S SECTION.
       INI-VAL-NUM-KEY-M6S.
           PERFORM VAL-NUM-IKY-M6S.
           PERFORM VAL-NUM-MKY-M6S.
           PERFORM VAL-NUM-AKY-M6S.
       FIN-VAL-NUM-KEY-M6S.
           EXIT.
       
       VAL-NUM-IKY-M6S SECTION.
       INI-VAL-NUM-IKY-M6S.
       VAL-NUM-M6S-NUM-LINE.
           MOVE M6S-NUM-LINE-ALF IN M6S-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO M6S-NUM-LINE IN M6S-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO M6S-NUM-LINE-LEN IN M6S-FLD
                GO TO FIN-VAL-NUM-IKY-M6S.
       FIN-VAL-NUM-IKY-M6S.
           EXIT.
       
       VAL-NUM-MKY-M6S SECTION.
       INI-VAL-NUM-MKY-M6S.
       FIN-VAL-NUM-MKY-M6S.
           EXIT.
       
       VAL-NUM-AKY-M6S SECTION.
       INI-VAL-NUM-AKY-M6S.
       FIN-VAL-NUM-AKY-M6S.
           EXIT.
       
       VAL-NUM-FLD-M6S SECTION.
       INI-VAL-NUM-FLD-M6S.
       VAL-NUM-M6S-VAL-MONT.
           MOVE M6S-VAL-MONT-ALF IN M6S-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 12 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO M6S-VAL-MONT IN M6S-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO M6S-VAL-MONT-LEN IN M6S-FLD
                GO TO FIN-VAL-NUM-FLD-M6S.
       FIN-VAL-NUM-FLD-M6S.
           EXIT.
       
       EDT-KEY-M6S SECTION.
       INI-EDT-KEY-M6S.
           PERFORM EDT-IKY-M6S.
           PERFORM EDT-MKY-M6S.
           PERFORM EDT-AKY-M6S.
       FIN-EDT-KEY-M6S.
           EXIT.
       
       EDT-IKY-M6S SECTION.
       INI-EDT-IKY-M6S.
           IF M6S-NUM-LINE-LEN IN M6S-FLD = -9
                MOVE -1 TO M6S-NUM-LINE-LEN IN M6S-FLD
                GO TO FIN-EDT-IKY-M6S.
           MOVE M6S-NUM-LINE IN M6S-FLD TO M6S-NUM-LINE-EDT IN M6S-EDT.
           MOVE M6S-NUM-LINE-EDT IN M6S-EDT TO 
                M6S-NUM-LINE-ALF IN M6S-FLD.
       FIN-EDT-IKY-M6S.
           EXIT.
       
       EDT-MKY-M6S SECTION.
       INI-EDT-MKY-M6S.
       FIN-EDT-MKY-M6S.
           EXIT.
       
       EDT-AKY-M6S SECTION.
       INI-EDT-AKY-M6S.
       FIN-EDT-AKY-M6S.
           EXIT.
       
       EDT-FLD-M6S SECTION.
       INI-EDT-FLD-M6S.
           IF M6S-VAL-MONT-LEN IN M6S-FLD = -9
                MOVE -1 TO M6S-VAL-MONT-LEN IN M6S-FLD
                GO TO FIN-EDT-FLD-M6S.
           MOVE M6S-VAL-MONT IN M6S-FLD TO M6S-VAL-MONT-EDT IN M6S-EDT.
           MOVE M6S-VAL-MONT-EDT IN M6S-EDT TO 
                M6S-VAL-MONT-ALF IN M6S-FLD.
       FIN-EDT-FLD-M6S.
           EXIT.
