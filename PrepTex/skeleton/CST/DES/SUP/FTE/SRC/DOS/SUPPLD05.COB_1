           REPORT IS RPT-D05
           .

       WORKING-STORAGE SECTION.
      *-----------------------
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-D05-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-TAB-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSG-REQA==.
       COPY SUPBRD05.
       COPY GNSWGRQA.
       COPY GNSWGUIB.
       COPY GNSWGELS.
       COPY GNSWVIDD.
       COPY GNSWCFIO.
       COPY GNSWVFIO.
       COPY GNSWGRPT.
       COPY GNSBRTAB.
       COPY GNSBRMSG.
       COPY SUPRWD05.
       COPY GNSWGSYS.
       01  PGM-STAT.
           03 PGM-STAT-SRT            VALUE ' ' PIC X(01).
              88 PGM-STAT-SRT-OKS     VALUE ' '.
              88 PGM-STAT-SRT-MAL     VALUE 'M'.
           03 PGM-SOKS                VALUE ' ' PIC X(01).
           03 PGM-SMAL                VALUE 'M' PIC X(01).
      *<<<< WSS
       COPY TABBROFI.
       COPY TABBRUSR.
       COPY GNSBRMSG.
       01  WSS-VARI.
           03 WSS-IPPH                    VALUE 0 PIC 9(01).
              88 PRIMER-PAGE-HEADING      VALUE 0.
      *>>>>
      *<<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSG-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OFI-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-USR-REQA==.
      *>>>>

       REPORT SECTION.
      *--------------
       COPY SUPRFD05.
           .
       COPY SUPRTD05.

       PROCEDURE DIVISION.
      *==================
       DECLARATIVES.
       DEC-CH-D05-D05-COD-OFIC SECTION.
           USE BEFORE REPORTING CH-D05-D05-COD-OFIC.
       INI-CH-D05-D05-COD-OFIC.
      *<<<< CH_D05_D05_COD_OFIC
           MOVE D05-COD-OFIC IN SRT TO OFI-COD-OFIC IN OFI.
           PERFORM BUS-OFI.
           MOVE OFI-GLS-DESC IN OFI TO WSS-GLS-OFIC IN WSS-D05.
      *>>>>
       FIN-CH-D05-D05-COD-OFIC.
           EXIT.
       DEC-PH-D05 SECTION.
           USE BEFORE REPORTING PH-D05.
       INI-PH-D05.
      *<<<< PH_D05
           IF PRIMER-PAGE-HEADING
               MOVE 1 TO PRINT-SWITCH
               MOVE 0 TO PAGE-COUNTER
               MOVE 1 TO WSS-IPPH.
      *>>>>   
       FIN-PH-D05.
           EXIT.
       END DECLARATIVES.

       MAIN SECTION.
      *------------
       INI-MAIN.
       COPY GNSBGEDB.
           MOVE 'SUPPLD05' TO FIO-PROG.
           PERFORM GNS-BUS-IDD.
           SORT GNSSRT
       COPY SUPRSD05.
           INPUT  PROCEDURE IS INP-SRT
           OUTPUT PROCEDURE IS OUT-SRT.
       FIN-MAIN.
           IF PGM-STAT-SRT-MAL OR SORT-RETURN > 0
               PERFORM GNS-PRO-ABT
           ELSE
               PERFORM GNS-PRO-END.

       INP-SRT SECTION.
      *---------------
       INI-INP-SRT.
      *<<<< INI_INP
      *    MOVE FIO-INP TO FIO-CMND.
      *    PERFORM TAB-FIO-USR.
      *>>>>
           MOVE FIO-INP TO FIO-CMND.
           PERFORM SUP-FIO-D05.
       FST-INP-SRT.
           MOVE FIO-GET-FST TO FIO-CMND.
           PERFORM SUP-FIO-D05.
       LUP-INP-SRT.
           IF NOT FIO-STAT-OKS
              GO TO FIN-INP-SRT.
      *<<<< LUP_INP
           MOVE D05-COD-ACRG IN D05 TO USR-COD-USER IN USR.
           PERFORM BUS-USR.
           MOVE USR-GLS-DESC IN USR TO WSS-GLS-ACRG IN WSS-D05.
           MOVE D05-COD-AUMO IN D05 TO USR-COD-USER IN USR.
           PERFORM BUS-USR.
           MOVE USR-GLS-DESC IN USR TO WSS-GLS-AUMO IN WSS-D05.
      *>>>> 
       MOV-INP-SRT.
       COPY SUPRMD05.
           RELEASE SRT.
       NXT-INP-SRT.
           MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM SUP-FIO-D05.
           GO TO LUP-INP-SRT.
       FIN-INP-SRT.
      *<<<< FIN_INP
      *    MOVE FIO-CLO TO FIO-CMND.
      *    PERFORM TAB-FIO-USR.
      *>>>>
           MOVE FIO-CLO TO FIO-CMND.
           PERFORM SUP-FIO-D05.

       OUT-SRT SECTION.
      *---------------
       INI-OUT-SRT.
           IF PGM-STAT-SRT-MAL
               GO TO EXT-OUT-SRT.
      *<<<< INI_OUT
      *    MOVE FIO-INP TO FIO-CMND.
      *    PERFORM TAB-FIO-OFI.
      *>>>>
           OPEN OUTPUT RPTD05.
           CALL 'GNSPLHDR' USING RPT-VARI.
           INITIATE RPT-D05.
           RETURN GNSSRT AT END
                GO TO FIN-OUT-SRT.
       LUP-OUT-SRT.
       GEN-OUT-SRT.
           GENERATE DL-D05.
       NXT-OUT-SRT.
           RETURN GNSSRT AT END
                GO TO FIN-OUT-SRT.
           GO TO LUP-OUT-SRT.
       FIN-OUT-SRT.
      *<<<< FIN_OUT
      *    MOVE FIO-CLO TO FIO-CMND.
      *    PERFORM TAB-FIO-OFI.
      *>>>>
           TERMINATE RPT-D05.
           CLOSE RPTD05.
       EXT-OUT-SRT.
           EXIT.
