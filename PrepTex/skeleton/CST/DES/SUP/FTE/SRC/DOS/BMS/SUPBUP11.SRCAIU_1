      * 10-Mar-94 12:22 PM
       RET-ALL-P11 SECTION.
       INI-RET-ALL-P11.
           PERFORM KEY-ALL-P11.
           PERFORM RET-P11-P11.
       FIN-RET-ALL-P11.
           EXIT.
       
       RET-P11-P11 SECTION.
       INI-RET-P11-P11.
           MOVE P11-VAL-VSFM IN P11-FLD TO P11-VAL-VSFM IN P11.
       FIN-RET-P11-P11.
           EXIT.
       
       KEY-ALL-P11 SECTION.
       INI-KEY-ALL-P11.
           PERFORM KEY-P11-P11.
       FIN-KEY-ALL-P11.
           EXIT.
       
       KEY-P11-P11 SECTION.
       INI-KEY-P11-P11.
           MOVE P11-COD-PROD IN P11-FLD TO P11-COD-PROD IN P11.
           MOVE P11-IND-TMON IN P11-FLD TO P11-IND-TMON IN P11.
           MOVE P11-COD-SECT IN P11-FLD TO P11-COD-SECT IN P11.
           MOVE P11-COD-LOCA IN P11-FLD TO P11-COD-LOCA IN P11.
       FIN-KEY-P11-P11.
           EXIT.
       
       SCR-CLRA-P11 SECTION.
       INI-SCR-CLRA-P11.
           PERFORM CLR-KEY-P11.
           PERFORM CLR-FLD-P11.
       FIN-SCR-CLRA-P11.
           EXIT.
       
       CLR-KEY-P11 SECTION.
       INI-CLR-KEY-P11.
           MOVE SPACES TO P11-COD-PROD IN P11-FLD.
           MOVE SPACES TO P11-IND-TMON IN P11-FLD.
           MOVE SPACES TO P11-COD-SECT IN P11-FLD.
           MOVE SPACES TO P11-COD-LOCA IN P11-FLD.
       FIN-CLR-KEY-P11.
           EXIT.
       
       CLR-FLD-P11 SECTION.
       INI-CLR-FLD-P11.
           MOVE SPACES TO FRM-GLS-PROD IN P11-FLD.
           MOVE SPACES TO FRM-GLS-TMON IN P11-FLD.
           MOVE SPACES TO FRM-GLS-SECT IN P11-FLD.
           MOVE SPACES TO FRM-GLS-LOCA IN P11-FLD.
           PERFORM CLR-P11-P11.
       FIN-CLR-FLD-P11.
           EXIT.
       
       CLR-P11-P11 SECTION.
       INI-CLR-P11-P11.
           MOVE ZEROES TO P11-VAL-VSFM IN P11-FLD.
       FIN-CLR-P11-P11.
           EXIT.
       
       FST-KEY-P11 SECTION.
       INI-FST-KEY-P11.
           MOVE -1 TO P11-COD-PROD-LEN IN P11-FLD.
       FIN-FST-KEY-P11.
           EXIT.
       
       FST-FLD-P11 SECTION.
       INI-FST-FLD-P11.
           MOVE -1 TO P11-VAL-VSFM-LEN IN P11-FLD.
       FIN-FST-FLD-P11.
           EXIT.
       
       PRO-KEY-P11 SECTION.
       INI-PRO-KEY-P11.
           PERFORM PRO-MKY-P11.
       FIN-PRO-KEY-P11.
           EXIT.
       
       PRO-MKY-P11 SECTION.
       INI-PRO-MKY-P11.
       FIN-PRO-MKY-P11.
           EXIT.
       
       PRO-FLD-P11 SECTION.
       INI-PRO-FLD-P11.
           PERFORM PRO-P11-P11.
       FIN-PRO-FLD-P11.
           EXIT.
       
       PRO-P11-P11 SECTION.
       INI-PRO-P11-P11.
           MOVE FRM-CPIM-AEY TO P11-VAL-VSFM-ATR IN P11-FLD.
       FIN-PRO-P11-P11.
           EXIT.
       
       UNP-KEY-P11 SECTION.
       INI-UNP-KEY-P11.
           PERFORM UNP-IKY-P11.
           PERFORM UNP-AKY-P11.
           PERFORM UNP-MKY-P11.
       FIN-UNP-KEY-P11.
           EXIT.
       
       UNP-IKY-P11 SECTION.
       INI-UNP-IKY-P11.
           MOVE FRM-CPIM-UBY TO P11-COD-PROD-ATR.
           MOVE FRM-CPIM-UBY TO P11-IND-TMON-ATR.
           MOVE FRM-CPIM-UBY TO P11-COD-SECT-ATR.
           MOVE FRM-CPIM-UBY TO P11-COD-LOCA-ATR.
       FIN-UNP-IKY-P11.
           EXIT.
       
       UNP-MKY-P11 SECTION.
       INI-UNP-MKY-P11.
       FIN-UNP-MKY-P11.
           EXIT.
       
       UNP-AKY-P11 SECTION.
       INI-UNP-AKY-P11.
       FIN-UNP-AKY-P11.
           EXIT.
       
       UNP-FLD-P11 SECTION.
       INI-UNP-FLD-P11.
           PERFORM UNP-P11-P11.
       FIN-UNP-FLD-P11.
           EXIT.
       
       UNP-P11-P11 SECTION.
       INI-UNP-P11-P11.
           MOVE FRM-CPIM-UBY TO P11-VAL-VSFM-ATR IN P11-FLD.
       FIN-UNP-P11-P11.
           EXIT.
       
       VAL-MKY-P11 SECTION.
       INI-VAL-MKY-P11.
       FIN-VAL-MKY-P11.
           EXIT.
       
       VAL-FLD-P11 SECTION.
       INI-VAL-FLD-P11.
           PERFORM VAL-FRM-GLS-PROD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-PROD-LEN IN P11-FLD
                GO TO FIN-VAL-FLD-P11.
           PERFORM VAL-FRM-GLS-TMON.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-TMON-LEN IN P11-FLD
                GO TO FIN-VAL-FLD-P11.
           PERFORM VAL-FRM-GLS-SECT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-SECT-LEN IN P11-FLD
                GO TO FIN-VAL-FLD-P11.
           PERFORM VAL-FRM-GLS-LOCA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-LOCA-LEN IN P11-FLD
                GO TO FIN-VAL-FLD-P11.
           PERFORM VAL-P11-VAL-VSFM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO P11-VAL-VSFM-LEN IN P11-FLD
                GO TO FIN-VAL-FLD-P11.
       FIN-VAL-FLD-P11.
           EXIT.
       
       VAL-FRM-GLS-PROD SECTION.
       INI-VAL-FRM-GLS-PROD.
       FIN-VAL-FRM-GLS-PROD.
           EXIT.
       
       VAL-FRM-GLS-TMON SECTION.
       INI-VAL-FRM-GLS-TMON.
       FIN-VAL-FRM-GLS-TMON.
           EXIT.
       
       VAL-FRM-GLS-SECT SECTION.
       INI-VAL-FRM-GLS-SECT.
       FIN-VAL-FRM-GLS-SECT.
           EXIT.
       
       VAL-FRM-GLS-LOCA SECTION.
       INI-VAL-FRM-GLS-LOCA.
       FIN-VAL-FRM-GLS-LOCA.
           EXIT.
       
       VAL-P11-VAL-VSFM SECTION.
       INI-VAL-P11-VAL-VSFM.
       FIN-VAL-P11-VAL-VSFM.
           EXIT.
       
       VAL-CON-FLD-P11 SECTION.
       INI-VAL-CON-FLD-P11.
       FIN-VAL-CON-FLD-P11.
           EXIT.
       
       VAL-CON-P11 SECTION.
       INI-VAL-CON-P11.
       FIN-VAL-CON-P11.
           EXIT.
       
       VAL-NUM-KEY-P11 SECTION.
       INI-VAL-NUM-KEY-P11.
           PERFORM VAL-NUM-IKY-P11.
           PERFORM VAL-NUM-MKY-P11.
           PERFORM VAL-NUM-AKY-P11.
       FIN-VAL-NUM-KEY-P11.
           EXIT.
       
       VAL-NUM-IKY-P11 SECTION.
       INI-VAL-NUM-IKY-P11.
       FIN-VAL-NUM-IKY-P11.
           EXIT.
       
       VAL-NUM-MKY-P11 SECTION.
       INI-VAL-NUM-MKY-P11.
       FIN-VAL-NUM-MKY-P11.
           EXIT.
       
       VAL-NUM-AKY-P11 SECTION.
       INI-VAL-NUM-AKY-P11.
       FIN-VAL-NUM-AKY-P11.
           EXIT.
       
       VAL-NUM-FLD-P11 SECTION.
       INI-VAL-NUM-FLD-P11.
       VAL-NUM-P11-VAL-VSFM.
           MOVE P11-VAL-VSFM-ALF IN P11-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 2 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9V TO P11-VAL-VSFM IN P11-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO P11-VAL-VSFM-LEN IN P11-FLD
                GO TO FIN-VAL-NUM-FLD-P11.
       FIN-VAL-NUM-FLD-P11.
           EXIT.
       
       EDT-KEY-P11 SECTION.
       INI-EDT-KEY-P11.
           PERFORM EDT-IKY-P11.
           PERFORM EDT-MKY-P11.
           PERFORM EDT-AKY-P11.
       FIN-EDT-KEY-P11.
           EXIT.
       
       EDT-IKY-P11 SECTION.
       INI-EDT-IKY-P11.
       FIN-EDT-IKY-P11.
           EXIT.
       
       EDT-MKY-P11 SECTION.
       INI-EDT-MKY-P11.
       FIN-EDT-MKY-P11.
           EXIT.
       
       EDT-AKY-P11 SECTION.
       INI-EDT-AKY-P11.
       FIN-EDT-AKY-P11.
           EXIT.
       
       EDT-FLD-P11 SECTION.
       INI-EDT-FLD-P11.
           IF P11-VAL-VSFM-LEN IN P11-FLD = -9
                MOVE -1 TO P11-VAL-VSFM-LEN IN P11-FLD
                GO TO FIN-EDT-FLD-P11.
           MOVE P11-VAL-VSFM IN P11-FLD TO P11-VAL-VSFM-EDT IN P11-EDT.
           MOVE P11-VAL-VSFM-EDT IN P11-EDT TO 
                P11-VAL-VSFM-ALF IN P11-FLD.
       FIN-EDT-FLD-P11.
           EXIT.
