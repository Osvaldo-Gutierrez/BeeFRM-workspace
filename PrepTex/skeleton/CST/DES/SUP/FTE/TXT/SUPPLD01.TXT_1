*% Codigo adicional
*% IF WSS
      *<<<< WSS
       COPY GNSBRMSC.
       COPY GNSBRMSG.
       COPY GNSBRTAB.
       01  WSS-VARI.
           03  WSS-COD-LOCA.
               05  WSS-COD-REGI                  PIC X(02).
               05  WSS-COD-PLZA                  PIC X(04). 
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSG-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-TAB-REQA==.
      *>>>>
*% END
*% IF INI_INP
      *<<<< INI_INP
      *    MOVE FIO-INP TO FIO-CMND.
      *    PERFORM GNS-FIO-TAB.
      *>>>>
*% END
*% IF LUP_INP
      *<<<< LUP_INP
           IF D01-IND-CATE > SPACES
               MOVE 'SGC' TO MSC-COD-SIST
               MOVE 'CATE' TO MSC-EXT-TTAB IN MSC
               MOVE D01-IND-CATE IN D01 TO MSC-EXT-CTAB IN MSC
               MOVE 'MSC-EXT-TABL' TO FIO-AKEY
               PERFORM BUS-MSC
               MOVE MSC-GLS-DESC TO WSS-GLS-CATE IN WSS-D01
           ELSE
               MOVE SPACES TO WSS-GLS-CATE IN WSS-D01.
               
           IF D01-COD-ODSE IN D01 > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'ODS' TO TAB-EXT-TTAB IN TAB
               MOVE D01-COD-ODSE IN D01 TO TAB-EXT-CTAB IN TAB
               MOVE 'TAB-EXT-TABL' TO FIO-AKEY
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO WSS-GLS-ODSE IN WSS-D01
           ELSE
               MOVE SPACES TO WSS-GLS-ODSE IN WSS-D01.

           IF D01-IND-CAJU > SPACES
               MOVE 'SGC' TO TAB-COD-SIST
               MOVE 'SOC' TO TAB-EXT-TTAB IN TAB 
               MOVE D01-IND-CAJU IN D01 TO TAB-EXT-CTAB IN TAB
               MOVE 'TAB-EXT-TABL' TO FIO-AKEY
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC TO WSS-GLS-CAJU IN WSS-D01
           ELSE
               MOVE SPACES TO WSS-GLS-CAJU IN WSS-D01.
               
           IF D01-COD-ACTP IN D01 > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'ACT' TO TAB-EXT-TTAB IN TAB
               MOVE D01-COD-ACTP IN D01 TO TAB-EXT-CTAB IN TAB
               MOVE 'TAB-EXT-TABL' TO FIO-AKEY
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO WSS-GLS-ACTP IN WSS-D01
           ELSE
               MOVE SPACES TO WSS-GLS-ACTP IN WSS-D01.

           IF D01-IND-TDEU > SPACES
               MOVE 'SUP' TO MSC-COD-SIST       
               MOVE 'TDEU' TO MSC-COD-TMSC IN MSC
               MOVE D01-IND-TDEU IN D01 TO MSC-COD-CMSC IN MSC
               PERFORM BUS-MSC
               MOVE MSC-GLS-DESC TO WSS-GLS-TDEU IN WSS-D01
           ELSE
               MOVE SPACES TO WSS-GLS-TDEU IN WSS-D01.
               
           IF D01-COD-LOCA IN D01 > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'COM' TO TAB-EXT-TTAB IN TAB
               MOVE D01-COD-LOCA IN D01 TO WSS-COD-LOCA IN WSS-VARI
               MOVE WSS-COD-PLZA IN WSS-VARI TO TAB-EXT-CTAB IN TAB
               MOVE 'TAB-EXT-TABL' TO FIO-AKEY
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO WSS-GLS-LOCA IN WSS-D01
           ELSE
               MOVE SPACES TO WSS-GLS-LOCA IN WSS-D01.
               
           IF D01-COD-ARCV IN D01 > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'ACT' TO TAB-EXT-TTAB IN TAB
               MOVE D01-COD-ARCV IN D01 TO TAB-EXT-CTAB IN TAB
               MOVE 'TAB-EXT-TABL' TO FIO-AKEY
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO WSS-GLS-ARCV IN WSS-D01
           ELSE
               MOVE SPACES TO WSS-GLS-ARCV IN WSS-D01.

           IF D01-IND-ESTA > SPACES
               MOVE 'SGC' TO MSC-COD-SIST
               MOVE 'SCLI' TO MSC-EXT-TTAB IN MSC
               MOVE D01-IND-ESTA IN D01 TO MSC-EXT-CTAB IN MSC
               MOVE 'MSC-EXT-TABL' TO FIO-AKEY
               PERFORM BUS-MSC
               MOVE MSC-GLS-DESC TO WSS-GLS-ESTA IN WSS-D01
           ELSE
               MOVE SPACES TO WSS-GLS-ESTA IN WSS-D01.

           IF D01-IND-TRDB > SPACES
               MOVE 'SGC' TO TAB-COD-SIST
               MOVE 'REC' TO TAB-EXT-TTAB IN TAB
               MOVE D01-IND-TRDB IN D01 TO TAB-EXT-CTAB IN TAB
               MOVE 'TAB-EXT-TABL' TO FIO-AKEY
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC TO WSS-GLS-TRDB IN WSS-D01
           ELSE
               MOVE SPACES TO WSS-GLS-TRDB IN WSS-D01.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBFTAB.
       COPY GNSBFMSC.
       COPY GNSBFMSG.
       COPY GNSBBMSC.
       COPY GNSBBMSG.
       COPY GNSBBTAB.
      *>>>>
*% END
