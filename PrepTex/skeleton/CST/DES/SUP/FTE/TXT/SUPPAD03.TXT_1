*%! Codigo adicional para SUPPAD03
*% IF MI_COMMAREA
      *<<<< FIN_SCR_VARI
           03  SCR-VARI-PFS.
               05  SCR-RUT-DDR      PIC X(09).
               05  SCR-RUT-DUE      PIC X(09).
      *>>>>
*% END
*% LARGO_MI_COMMAREA = 18
*% IF WSS
      *<<<<
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
       COPY SGCBRDBC.
       COPY GNSWGCPT.
       01  W-VARAIBLES.
           02 W-NUM-RUT             PIC X(08) JUST.
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<<
           MOVE SPACES TO SCR-RUT-DUE.
           MOVE SPACES TO SCR-RUT-DDR.
      *>>>>>
*% END
*% IF FIN_FND_KEY
      *<<<<
           IF (PGM-STAT-D03-OKS)
               MOVE D03-NUM-RUT-DUE IN D03 TO SCR-RUT-DUE
               MOVE D03-NUM-RUT-DDR IN D03 TO SCR-RUT-DDR
           ELSE
           IF (SCR-CCPP = 'ING')
    	       MOVE D03-NUM-RUT-DDR IN D03-FLD TO W-NUM-RUT
               PERFORM BUS-CLIENTE
               MOVE DBC-GLS-NOMC IN DBC TO D03-NOM-DDR IN D03-FLD
               MOVE D03-NUM-RUT-DDR IN D03 TO SCR-RUT-DDR.
           MOVE PGM-STAT-D03 TO FIO-STAT.  
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY SGCBFDBC.
       COPY GNSBGCPT.
      *>>>>
*% END
