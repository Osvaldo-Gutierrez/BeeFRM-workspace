*%! Codigo adicional para archivo
*% NO_HDR_CST = TRUE
*% INCLUDE 'GNSSRC:MANEJO_VSI.SRC'
*% IF WSS
      *<<<< WSS
       COPY SGCBRDBC.
       COPY TABBRCOM.
       COPY GNSWGCPT.
       01  WSS-VARI.
           03 WSS-COD-RUTC                              PIC 9(08).
           03 WSS-COD-LOCA.
              05 WSS-COD-REGI                           PIC X(02).
              05 WSS-COD-PLZA                           PIC X(04). 
           03 WSS-IND-TCAP                              PIC X(04).
              88 VAL-TCAP          VALUES '3010', '3020', '3025', '3030'
                                          '3065', '3075', '3110', '3115'
                                          '3305', '3310', '3315'.
           03 WSS-TBL-DCLI.
              05 WSS-GLS-TCLI      VALUE  'TCLI/CAJU/CATE/ODSE/ESTA :'
                                                        PIC X(26).
              05 WSS-IND-TCLI                           PIC X(02).
                 88 VAL-TCLI1      VALUE  '01'.
                 88 VAL-TCLI2      VALUE  '02'.
                 88 VAL-TCLI3      VALUE  '03'.
              05 FILLER            VALUE  '/'           PIC X(01).
              05 WSS-IND-CAJU                           PIC X(02).
                 88 VAL-CAJU1      VALUE  '01'.
                 88 VAL-CAJU3      VALUE  '03'.
              05 FILLER            VALUE  '/'           PIC X(01).
              05 WSS-IND-CATE                           PIC X(01).
                 88 VAL-CATE2      VALUE  '2'.
                 88 VAL-CATE3      VALUE  '3'.
              05 FILLER            VALUE  '/'           PIC X(01).
              05 WSS-COD-ODSE                           PIC X(03). 
                 88 VAL-ODSE1      VALUE  '121'.
                 88 VAL-ODSE2      VALUES '211', '212', '221', '222'.
                 88 VAL-ODSE3      VALUE  '302'.
              05 FILLER            VALUE  '/'           PIC X(01).
              05 WSS-IND-ESTA                           PIC X(01).
                 88 VAL-ESTA4      VALUE  '4'.
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
      *>>>>
*% END
*% IF FND_KEY
      *<<<< FND_KEY
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
           IF FIO-STAT-OKS OR ( NOT ( SCR-CCPP = 'ING' OR 'ACT' ) )
               GO TO FIN-FND-KEY.
           MOVE D04-NUM-RUTC IN D04-FLD TO WSS-COD-RUTC IN WSS-VARI.
           MOVE WSS-COD-RUTC            TO DBC-NUM-ICLI IN DBC.
           MOVE SPACES                  TO DBC-IND-ICLI IN DBC.
           MOVE SPACES                  TO DBC-GLS-ICLI IN DBC.
           MOVE 'DBC-IDC-ICLI' TO FIO-AKEY.
           MOVE FIO-GET-KEY    TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           IF NOT FIO-STAT-OKS
               MOVE 'TAB'        TO MSG-COD-SIST
               MOVE 'CLI    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC IN MSG TO D04-GLS-NOMC IN D04-FLD
           ELSE
               MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
               PERFORM CPT-BLKS
               MOVE CPT-STRN TO DBC-GLS-NOMC IN DBC
               MOVE DBC-GLS-NOMC IN DBC TO D04-GLS-NOMC IN D04-FLD.
           MOVE '23' TO FIO-STAT.
      *>>>>
*% END
*% IF PUT_SCR
      *<<<< PUT_SCR
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY SGCBFDBC.
       COPY GNSBBTAB.
       COPY GNSBVCOD.
       COPY GNSBVTAB.
       COPY GNSBEGLS.
       COPY GNSBGCPT.
      *>>>>
*% END
