*% Codigo adicional para archivo
*% IF WSS
      *<<<< WSS       
       COPY TABBROFI.
       COPY TABBRUSR.
       COPY GNSBRMSC.
       COPY GNSBRMSG.
       01  WSS-VARI.
           03 WSS-IPPH                       VALUE 0 PIC 9(01).
              88 PRIMER-PAGE-HEADING         VALUE 0.
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC       
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSG-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OFI-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-USR-REQA==.
      *>>>>
*% END
*% IF CH_D06_D06_COD_OFIC
      *<<<< CH_D06_D06_COD_OFIC
           MOVE D06-COD-OFIC IN SRT TO OFI-COD-OFIC IN OFI.
           PERFORM BUS-OFI.
           MOVE OFI-GLS-DESC IN OFI TO WSS-GLS-OFIC IN WSS-D06.
      *>>>>
*% END
*% IF PH_D06
      *<<<< PH_D06
           IF PRIMER-PAGE-HEADING
               MOVE 1 TO PRINT-SWITCH
               MOVE 0 TO PAGE-COUNTER
               MOVE 1 TO WSS-IPPH.
      *>>>>
*% END
*% IF INI_INP
      *<<<< INI_INP
      *    MOVE FIO-INP TO FIO-CMND.
      *    PERFORM TAB-FIO-USR.
      *>>>>
*% END
*% IF FIN_INP
      *<<<< FIN_INP
      *    MOVE FIO-CLO TO FIO-CMND.
      *    PERFORM TAB-FIO-USR.
      *>>>>
*% END
*% IF INI_OUT
      *<<<< INI_OUT
      *    MOVE FIO-INP TO FIO-CMND.
      *    PERFORM TAB-FIO-OFI.
      *>>>>
*% END
*% IF LUP_OUT
      *<<<< LUP_OUT
           MOVE D06-COD-ACRG IN SRT TO USR-COD-USER IN USR.
           PERFORM BUS-USR.
           MOVE USR-GLS-DESC IN USR TO WSS-GLS-ACRG IN WSS-D06.

           MOVE D06-COD-AUMO IN SRT TO USR-COD-USER IN USR.
           PERFORM BUS-USR.
           MOVE USR-GLS-DESC IN USR TO WSS-GLS-AUMO IN WSS-D06.

           MOVE 'SUP' TO MSC-COD-SIST.
           MOVE 'ACLA' TO MSC-COD-TMSC IN MSC.
           MOVE D06-COD-CAAC IN SRT TO MSC-COD-CMSC IN MSC.
           PERFORM BUS-MSC.
           MOVE MSC-GLS-DESC IN MSC TO WSS-GLS-CAAC IN WSS-D06.
      *>>>> 
*% END
*% IF FIN_OUT
      *<<<< FIN_OUT
      *    MOVE FIO-CLO TO FIO-CMND.
      *    PERFORM TAB-FIO-OFI.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY TABBBOFI.
       COPY TABBFOFI.
       COPY TABBBUSR.
       COPY TABBFUSR.
       COPY GNSBBMSG.
       COPY GNSBFMSG.
       COPY GNSBBMSC.
       COPY GNSBFMSC.
      *>>>>
*% END
