      * PROCESA LINEA {LINEA}
           IF M6S-NUM-LINE = {LINEA}
               IF M6S-CIC-VCAM = WSS-CIC-1013
                    ADD  M6S-VAL-MONT TO SRT-VAL-{LINEA}3
                    MOVE M6S-VAL-MONT TO WSS-VAL-COL3
               ELSE
               IF M6S-CIC-VCAM = WSS-CIC-0999
                    ADD M6S-VAL-MONT TO SRT-VAL-{LINEA}1
                    MOVE M6S-VAL-MONT TO WSS-VAL-COL1
               ELSE
               IF M6S-COD-CORR IN M6S = '01' OR '02'
                   IF M6S-CIC-VCAM = WSS-CIC-0998
                       ADD  M6S-VAL-MONT TO SRT-VAL-{LINEA}1
                       MOVE M6S-VAL-MONT TO WSS-VAL-COL1
                   ELSE
                   IF M6S-CIC-VCAM = WSS-CIC-0013
                       ADD M6S-VAL-MONT TO SRT-VAL-{LINEA}1
                       MOVE M6S-VAL-MONT TO WSS-VAL-COL1
                   ELSE
                      NEXT SENTENCE
               ELSE
               IF M6S-COD-CORR IN M6S = '03' OR '04'
                   IF M6S-CIC-VCAM = WSS-CIC-0998
                       ADD M6S-VAL-MONT TO SRT-VAL-{LINEA}2
                       MOVE M6S-VAL-MONT TO WSS-VAL-COL2
                   ELSE
                   IF M6S-CIC-VCAM = WSS-CIC-0013
                      ADD M6S-VAL-MONT TO SRT-VAL-{LINEA}2
                      MOVE M6S-VAL-MONT TO WSS-VAL-COL2.
