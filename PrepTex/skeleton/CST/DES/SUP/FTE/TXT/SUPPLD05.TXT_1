*% Codigo adicional para archivo
*% IF WSS
      *<<<< WSS
       COPY TABBROFI.
       COPY TABBRUSR.
       COPY GNSBRMSG.
       01  WSS-VARI.
           03 WSS-IPPH                    VALUE 0 PIC 9(01).
              88 PRIMER-PAGE-HEADING      VALUE 0.
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSG-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OFI-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-USR-REQA==.
      *>>>>
*% END
*% IF CH_D05_D05_COD_OFIC
      *<<<< CH_D05_D05_COD_OFIC
           MOVE D05-COD-OFIC IN SRT TO OFI-COD-OFIC IN OFI.
           PERFORM BUS-OFI.
           MOVE OFI-GLS-DESC IN OFI TO WSS-GLS-OFIC IN WSS-D05.
      *>>>>
*% END
*% IF PH_D05
      *<<<< PH_D05
           IF PRIMER-PAGE-HEADING
               MOVE 1 TO PRINT-SWITCH
               MOVE 0 TO PAGE-COUNTER
               MOVE 1 TO WSS-IPPH.
      *>>>>   
*% END
*% IF INI_INP
      *<<<< INI_INP
      *    MOVE FIO-INP TO FIO-CMND.
      *    PERFORM TAB-FIO-USR.
      *>>>>
*% END
*% IF LUP_INP
      *<<<< LUP_INP
           MOVE D05-COD-ACRG IN D05 TO USR-COD-USER IN USR.
           PERFORM BUS-USR.
           MOVE USR-GLS-DESC IN USR TO WSS-GLS-ACRG IN WSS-D05.
           MOVE D05-COD-AUMO IN D05 TO USR-COD-USER IN USR.
           PERFORM BUS-USR.
           MOVE USR-GLS-DESC IN USR TO WSS-GLS-AUMO IN WSS-D05.
      *>>>> 
*% END
*% IF FIN_INP
      *<<<< FIN_INP
      *    MOVE FIO-CLO TO FIO-CMND.
      *    PERFORM TAB-FIO-USR.
      *>>>>
*% END
*% IF INI_OUT
      *<<<< INI_OUT
      *    MOVE FIO-INP TO FIO-CMND.
      *    PERFORM TAB-FIO-OFI.
      *>>>>
*% END
*% IF FIN_OUT
      *<<<< FIN_OUT
      *    MOVE FIO-CLO TO FIO-CMND.
      *    PERFORM TAB-FIO-OFI.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY TABBBOFI.
       COPY TABBFOFI.
       COPY TABBBUSR.
       COPY TABBFUSR.
       COPY GNSBBMSG.
       COPY GNSBFMSG.
      *>>>>
*% END
