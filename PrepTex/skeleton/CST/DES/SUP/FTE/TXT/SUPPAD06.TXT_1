*%! Codigo adicional para archivo
*% NO_HDR_CST = TRUE
*% INCLUDE 'GNSSRC:MANEJO_VSI.SRC'
*% IF FIN_SCR_VARI
      *<<<< FIN_SCR_VARI
       COPY SGCBRDBC.
       COPY GNSBRMSC.
       COPY TABBROFI.
       COPY TABBRUSR.
       01  WSS-VARI.
           03 WSS-COD-RUTC                               PIC 9(8).
       COPY GNSWGCPT.
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OFI-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-USR-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSC-REQA==.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
           IF FIO-STAT-OKS OR ( NOT ( SCR-CCPP = 'ING' OR 'ACT' ) )
               GO TO FIN-FND-KEY.
           MOVE D06-NUM-RUTD IN D06-FLD      TO WSS-COD-RUTC.
           MOVE WSS-COD-RUTC TO DBC-NUM-ICLI IN DBC.
           MOVE SPACES       TO DBC-IND-ICLI IN DBC.
           MOVE SPACES       TO DBC-GLS-ICLI IN DBC.
           MOVE 'DBC-IDC-ICLI' TO FIO-AKEY.
           MOVE FIO-GET-KEY-ALT  TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           IF NOT FIO-STAT-OKS
               MOVE 'TAB'        TO MSG-COD-SIST
               MOVE 'CLI    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC IN MSG TO D06-GLS-NOMD IN D06-FLD
           ELSE
               MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
               PERFORM CPT-BLKS
               MOVE CPT-STRN TO DBC-GLS-NOMC IN DBC
               MOVE DBC-GLS-NOMC IN DBC TO D06-GLS-NOMD IN D06-FLD.
           MOVE '99' TO FIO-STAT.
      *>>>>
*% END
*% IF CMN_ING
      *<<<< CMN_ING
           MOVE D06-COD-ACRG IN D06-FLD TO D06-COD-AUMO IN D06.
           PERFORM GET-FHOY.
           MOVE HOY-DHOY IN HOY-VARI TO D06-NUM-DCRG IN D06.
           MOVE HOY-MHOY IN HOY-VARI TO D06-NUM-MCRG IN D06.
           MOVE HOY-AHOY IN HOY-VARI TO D06-NUM-ACRG IN D06.
      *>>>>
*% END
*% IF CMN_ING OR CMN_MOD
      *<<<< CMN_ING OR CMN_MOD
           PERFORM GET-FHOY.
           MOVE HOY-DHOY IN HOY-VARI TO D06-NUM-DUMO IN D06.
           MOVE HOY-MHOY IN HOY-VARI TO D06-NUM-MUMO IN D06.
           MOVE HOY-AHOY IN HOY-VARI TO D06-NUM-AUMO IN D06.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY SGCBFDBC.
       COPY TABBBOFI.
       COPY TABBVOFI.
       COPY TABBFOFI.
       COPY TABBBUSR.
       COPY TABBVUSR.
       COPY TABBFUSR.
       COPY GNSBGCPT.
       COPY GNSBVIND.
       COPY GNSBVMSC.
       COPY GNSBBMSC.
       COPY GNSBFMSC.
      *>>>>
*% END
