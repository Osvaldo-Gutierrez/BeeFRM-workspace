*%! Codigo adicional
*% NO_TIENE_TIMESTAMP = TRUE
*% NO_HDR_CST = TRUE
*% LARGO_TOTAL_COMMAREA = 3000
*% FMT_CHI = TRUE
*% GEN_BCI = TRUE
*% PGM_PTC = TRUE
*% GEN_MFC = TRUE
*% IF PGM_PQ
*% CLRMFC = "SGCZQQDI"
*% END
*% IF WSS
      *<<<< WSS 
       01 WSS-VARI.
          03 WSS-COD-LUGA.                                         
             05 WSS-COD-COMU                          PIC X(04).   
             05 WSS-GLS-PAIS REDEFINES WSS-COD-COMU.               
                07 WSS-IND-PAIS                       PIC X(01).   
                07 WSS-COD-PAIS                       PIC X(03).   
       

       COPY GNSWGCPT.
       COPY GNSWGSTR.
       COPY SGCBRDBC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
       COPY SGCWXQDI.
*% IF NOT PGM_MCP AND NOT PGM_PTC
       COPY GNSWGQUE.
*% END
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
       01 WSS-NUM-CCUR.
           03 WSS-NUM-ICUR-EDT  PIC  ZZ9.
           03 WSS-NUM-ICUR-ALF       PIC X(03).
           03 WSS-NUM-ICUR      REDEFINES
              WSS-NUM-ICUR-ALF       PIC 9(03).
      *>>>>
*% END
*% IF FIN_SCR_VARI
           03 SCR-NUM-PANT               PIC 9(0012).
           03 SCR-QDI-FLD                PIC X(1100).
           03 SCR-CIC-ICLI.
              05 SCR-CAI-ICLI            PIC X(04).
              05 SCR-IIC-ICLI            PIC X(08).
*% END
*% IF FIN_PGM_STAT

              05 PGM-STAT-RCD.
                 88 PGM-STAT-RCD-NUL    VALUE '  '.
                 07 PGM-STT1-RCD        VALUE '2' PIC X(01).
                    88 PGM-STAT-RCD-OKS VALUE '0'.
                 07 PGM-STT2-RCD        VALUE '3' PIC X(01).

*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-OFRM-SRV
                MOVE TSK-TERM-ALF TO QDI-COD-TERM
                MOVE TSK-TERM-ALF TO QDI-COD-TERM-MAS
           ELSE
                MOVE EIBTRMID     TO QDI-COD-TERM
                MOVE EIBTRMID     TO QDI-COD-TERM-MAS.
           MOVE +3859 TO SCR-TCMA.
           PERFORM GNS-ERR-QUE.
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )
               MOVE SCR-QDI-FLD TO QDI-FLD
               GO TO FIN-INI.

           IF ( FRM-FFLD = FRM-FFLD-PF6 )
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM
               ADD 1              TO QUE-NITM
               MOVE QDI-NOM-TSQDI TO QUE-COLA
               MOVE QDI-LEN-TSQDI TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT NOT = QUE-STAT-OKS
                  MOVE SCR-QDI-FLD TO QDI-FLD
                  GO TO FIN-INI.
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
           PERFORM CLR-QDI-VEC VARYING WSS-J FROM WSS-J BY 1
                                 UNTIL WSS-J > WSS-JMAX.

           MOVE SCR-CAI-ICLI    TO RCD-CAI-ICLI IN RCD.           
           MOVE SCR-IIC-ICLI    TO RCD-IIC-ICLI IN RCD.           
           IF FRM-IND-ISGT IN QDI-FLD = 'S'  AND
              FRM-NUM-ISGT IN QDI-FLD > ZEROES
              MOVE FRM-NUM-ISGT IN QDI-FLD TO RCD-NUM-DIRE IN RCD
           ELSE
              MOVE ZEROES                  TO RCD-NUM-DIRE IN RCD.      
  
           MOVE ZEROES TO WSS-J IN WSS-VARI.
           MOVE FIO-GET-NLS TO FIO-CMND.
       LUP-LEE-RCD.
           PERFORM SGC-FIO-RCD.
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF
               GO TO CON-FND-KEY.

           IF (SCR-CAI-ICLI NOT = RCD-CAI-ICLI IN RCD OR
               SCR-IIC-ICLI NOT = RCD-IIC-ICLI INRCD)
               GO TO CON-FND-KEY.

           PERFORM CRG-VEC-QDI.

           IF (SCR-OFRM-SRV AND WSS-J > WSS-NITMMAX)
               GO TO CON-FND-KEY.

           IF ((NOT SCR-OFRM-SRV) AND WSS-J > WSS-JMAX)
               GO TO CON-FND-KEY.

       NXT-LEE-RCD.
           MOVE FIO-GET-NXT TO FIO-CMND.
           GO TO LUP-LEE-RCD.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
       CON-FND-KEY.
           PERFORM PUT-TAB.

           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *----------------------------------------------------------
       MSG-FND-KEY.
           MOVE 'TAB' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM VAL-NUM-KEY-QDI
               PERFORM VAL-NUM-FLD-QDI
               PERFORM SCRL-TAB-QDI
               PERFORM SCR-TRL.
      *>>>>
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBBTAB.
       COPY GNSBBMSC.
       COPY GNSBVTAB.
       COPY GNSBVCOD.
       COPY SGCBFDBC.

       PUT-TAB SECTION.
       INI-PUT-TAB.
           MOVE QDI-NOM-TSQDI     TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE QDI-NOM-TSQDI-MAS TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE 1 TO SCR-NUM-PANT.
           MOVE LOW-VALUES TO WSS-GLS-NQDI-IDX-MTI.
           MOVE LOW-VALUES TO WSS-GLS-NQDI-IDX-FRM.
      *
           MOVE 1 TO FRM-IFLD.
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.

           MOVE 1 TO WSS-F IN WSS-VARI.
       LUP-PUT-TAB.
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI
              GO TO FIN-PUT-TAB.
           IF WSS-NUM-DIRE-V(WSS-F) > ZEROES
              PERFORM PUT-TAB-QDI
              PERFORM PUT-QDI-QUE
              PERFORM SET-FRM-IFLD
           ELSE
           IF FRM-IFLD = 1 AND WSS-F = 1
              MOVE 'TAB' TO MSG-COD-SIST
              MOVE 'QDI    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE '00' TO FIO-STAT
      *       MOVE 'S' TO SCR-IERR
              GO TO FIN-PUT-TAB
           ELSE
              MOVE '00' TO FIO-STAT
              GO TO FIN-PUT-TAB.

           ADD 1 TO WSS-F IN WSS-VARI.
           GO TO LUP-PUT-TAB.
       FIN-PUT-TAB.
           MOVE 1                  TO QUE-NITM.
           MOVE QDI-LEN-TSQDI-MAS  TO QUE-LITM.
           MOVE QDI-NOM-TSQDI-MAS  TO QUE-COLA.

           IF WSS-NITM > WSS-NITMMAX
              MOVE 'S'           TO QDI-MAS-IND-HMAS
           ELSE
              MOVE 'N'           TO QDI-MAS-IND-HMAS.

           MOVE WSS-NITM         TO WSS-NUM-ICUR.
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.
           MOVE WSS-NUM-ICUR-ALF TO QDI-MAS-NUM-TOCC.
           MOVE QDI-MAS-ITEM     TO QUE-ITEM.
           MOVE QUE-PUT          TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF WSS-NITM > WSS-IMAX
               MOVE 'TAB' TO MSG-COD-SIST
               MOVE 'QDITABDMAS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       EXT-PUT-TAB.
           EXIT.

       SET-FRM-IFLD SECTION.
       INI-SET-FRM-IFLD.
           IF SCR-OFRM-SRV
                MOVE 1 TO FRM-IFLD
           ELSE
                ADD 1 TO FRM-IFLD.
       FIN-SET-FRM-IFLD.
           EXIT.
      *--------------------------------------------
       SCRL-TAB-QDI SECTION.
       INI-SCRL-TAB-QDI.
           MOVE SPACES TO MSG-COD-MENS.

           IF FRM-FFLD = FRM-FFLD-PF6
               PERFORM PUT-TAB-ASC
           ELSE
               PERFORM PUT-TAB-DSC.
           IF MSG-COD-MENS > SPACES
               MOVE 'TAB' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-QDI.
           EXIT.

      *--------------------------------------------
       PUT-TAB-QDI SECTION.
       INI-PUT-TAB-QDI.

           PERFORM CLR-WSS.
           MOVE WSS-NUM-DIRE-V(WSS-F) TO WSS-NUM-DIRE-QDI IN WSS-VARI.
           MOVE WSS-MSC-TDIR-V(WSS-F) TO WSS-MSC-TDIR-QDI IN WSS-VARI.
           MOVE WSS-GLS-TDIR-V(WSS-F) TO WSS-GLS-TDIR-QDI IN WSS-VARI.
           MOVE WSS-COD-LUGA-V(WSS-F) TO WSS-COD-LUGA-QDI IN WSS-VARI.
           MOVE WSS-GLS-LUGA-V(WSS-F) TO WSS-GLS-LUGA-QDI IN WSS-VARI.
           MOVE WSS-GLS-FONO-V(WSS-F) TO WSS-GLS-FONO-QDI IN WSS-VARI.
           MOVE WSS-MSC-SDIR-V(WSS-F) TO WSS-MSC-SDIR-QDI IN WSS-VARI.
           MOVE WSS-GLS-SDIR-V(WSS-F) TO WSS-GLS-SDIR-QDI IN WSS-VARI.
           MOVE WSS-GLS-DIRE-V(WSS-F) TO WSS-GLS-DIRE-QDI IN WSS-VARI.
           MOVE WSS-FEC-FVRF-V(WSS-F) TO WSS-FEC-FVRF-QDI IN WSS-VARI.


           MOVE WSS-NUM-DIRE-QDI IN WSS-VARI TO WSS-NUM-DIRE-FRM.
           MOVE WSS-MSC-TDIR-QDI IN WSS-VARI TO WSS-MSC-TDIR-FRM.
           MOVE WSS-GLS-TDIR-QDI IN WSS-VARI TO WSS-GLS-TDIR-FRM.
           MOVE WSS-COD-LUGA-QDI IN WSS-VARI TO WSS-COD-LUGA-FRM.
           MOVE WSS-GLS-LUGA-QDI IN WSS-VARI TO WSS-GLS-LUGA-FRM.
           MOVE WSS-GLS-FONO-QDI IN WSS-VARI TO WSS-GLS-FONO-FRM.
           MOVE WSS-MSC-SDIR-QDI IN WSS-VARI TO WSS-MSC-SDIR-FRM.
           MOVE WSS-GLS-SDIR-QDI IN WSS-VARI TO WSS-GLS-SDIR-FRM.
           MOVE WSS-GLS-DIRE-QDI IN WSS-VARI TO WSS-GLS-DIRE-FRM.
           MOVE WSS-FEC-FVRF-QDI IN WSS-VARI TO WSS-FEC-FVRF-FRM.

           IF FRM-IFLD > WSS-IMAX
              GO TO FIN-PUT-TAB-QDI.

           MOVE WSS-NUM-DIRE-QDI IN WSS-VARI TO 
                FRM-NUM-DIRE IN QDI-FLD(FRM-IFLD).
           MOVE WSS-MSC-TDIR-QDI IN WSS-VARI TO 
                FRM-MSC-TDIR IN QDI-FLD(FRM-IFLD).
           MOVE WSS-GLS-TDIR-QDI IN WSS-VARI TO 
                FRM-GLS-TDIR IN QDI-FLD(FRM-IFLD).
           MOVE WSS-COD-LUGA-QDI IN WSS-VARI TO 
                FRM-COD-LUGA IN QDI-FLD(FRM-IFLD).
           MOVE WSS-GLS-LUGA-QDI IN WSS-VARI TO 
                FRM-GLS-LUGA IN QDI-FLD(FRM-IFLD).
           MOVE WSS-GLS-FONO-QDI IN WSS-VARI TO 
                FRM-GLS-FONO IN QDI-FLD(FRM-IFLD).
           MOVE WSS-MSC-SDIR-QDI IN WSS-VARI TO 
                FRM-MSC-SDIR IN QDI-FLD(FRM-IFLD).
           MOVE WSS-GLS-SDIR-QDI IN WSS-VARI TO 
                FRM-GLS-SDIR IN QDI-FLD(FRM-IFLD).
           MOVE WSS-GLS-DIRE-QDI IN WSS-VARI TO 
                FRM-GLS-DIRE IN QDI-FLD(FRM-IFLD).
           MOVE WSS-FEC-FVRF-QDI IN WSS-VARI TO 
                FRM-FEC-FVRF IN QDI-FLD(FRM-IFLD).

       FIN-PUT-TAB-QDI.
           EXIT.
      *----------------------------------------------------
       PUT-QDI-QUE SECTION.
       INI-PUT-QDI-QUE.
           PERFORM PRO-FLD-QUE.
           ADD 1 TO WSS-NITM.
           MOVE QDI-NOM-TSQDI    TO QUE-COLA.
           MOVE QDI-LEN-TSQDI    TO QUE-LITM.
           MOVE WSS-NITM         TO QUE-NITM.
           MOVE SPACES           TO QUE-ITEM.
           IF SCR-OFRM-SRV
              MOVE WSS-GLS-NQDI-IDX-MTI  TO QUE-ITEM
           ELSE
              MOVE WSS-GLS-NQDI-IDX-FRM  TO QUE-ITEM.
           MOVE QUE-PUT  TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
       FIN-PUT-QDI-QUE.
           EXIT.
      *----------------------------------------------------

       PUT-TAB-ASC SECTION.
       INI-PUT-TAB-ASC.
           MOVE SPACES TO MSG-COD-MENS.
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.
           MOVE 0 TO FRM-IFLD.
       LUP-PUT-TAB-ASC.
           ADD 1              TO QUE-NITM.
           MOVE QDI-NOM-TSQDI TO QUE-COLA.
           MOVE QDI-LEN-TSQDI TO QUE-LITM.
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD = 0
                   MOVE 'QDITABNONXT' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-ASC
               ELSE
                   GO TO FIN-PUT-TAB-ASC.
           ADD 1 TO FRM-IFLD.
           MOVE QUE-ITEM TO FRM-NUM-DIRE-IDX(FRM-IFLD).
           IF FRM-IFLD < WSS-IMAX
               GO TO LUP-PUT-TAB-ASC.
       FIN-PUT-TAB-ASC.
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX
               ADD 1 FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1
                                 UNTIL WSS-I > WSS-IMAX.
           IF FRM-IFLD > 0
               ADD 1 TO SCR-NUM-PANT.
           IF FRM-IFLD = WSS-IMAX
               ADD 1              TO QUE-NITM
               MOVE QDI-NOM-TSQDI TO QUE-COLA
               MOVE QDI-LEN-TSQDI TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT = QUE-STAT-OKS
                   MOVE 'SGC'        TO MSG-COD-SIST
                   MOVE 'QDITABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-ASC.
           EXIT.

       PUT-TAB-DSC SECTION.
       INI-PUT-TAB-DSC.
           MOVE SPACES TO MSG-COD-MENS.
           IF SCR-NUM-PANT = 1
               MOVE 'QDITABNOPRV' TO MSG-COD-MENS
               GO TO EXT-PUT-TAB-DSC.

           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.
           ADD 1 TO QUE-NITM.
           ADD 1 WSS-IMAX GIVING FRM-IFLD.
       LUP-PUT-TAB-DSC.
           SUBTRACT  1 FROM QUE-NITM.
           MOVE QDI-NOM-TSQDI TO QUE-COLA
           MOVE QDI-LEN-TSQDI TO QUE-LITM
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD > WSS-IMAX
                   MOVE 'QDITABNOPRV' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-DSC
               ELSE
                   GO TO FIN-PUT-TAB-DSC.
           SUBTRACT 1 FROM FRM-IFLD.
           MOVE QUE-ITEM TO FRM-NUM-DIRE-IDX(FRM-IFLD).
           IF FRM-IFLD > 1
               GO TO LUP-PUT-TAB-DSC.
       FIN-PUT-TAB-DSC.

           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1
                                 UNTIL WSS-I < 1.

           IF FRM-IFLD NOT > WSS-IMAX
               SUBTRACT 1 FROM SCR-NUM-PANT
               MOVE 'SGC'        TO MSG-COD-SIST
               MOVE 'QDITABDMAS' TO MSG-COD-MENS.
       EXT-PUT-TAB-DSC.
           EXIT.

       COPY GNSBGCPT.
       COPY GNSBGSTR.

       CLR-FRM SECTION.
       INI-CLR-FRM.
           MOVE ZEROES TO FRM-NUM-DIRE IN QDI-FLD(WSS-I).
           MOVE SPACES TO FRM-MSC-TDIR IN QDI-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-TDIR IN QDI-FLD(WSS-I).
           MOVE SPACES TO FRM-COD-LUGA IN QDI-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-LUGA IN QDI-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-FONO IN QDI-FLD(WSS-I).
           MOVE SPACES TO FRM-MSC-SDIR IN QDI-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-SDIR IN QDI-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-DIRE IN QDI-FLD(WSS-I).
           MOVE ZEROES TO FRM-FEC-FVRF IN QDI-FLD(WSS-I).
       FIN-CLR-FRM.
           EXIT.

       CLR-WSS SECTION.
       INI-CLR-WSS.
           MOVE ZEROES TO WSS-NUM-DIRE-QDI IN WSS-VARI.
           MOVE SPACES TO WSS-MSC-TDIR-QDI IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-TDIR-QDI IN WSS-VARI.
           MOVE SPACES TO WSS-COD-LUGA-QDI IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-LUGA-QDI IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-FONO-QDI IN WSS-VARI.
           MOVE SPACES TO WSS-MSC-SDIR-QDI IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-SDIR-QDI IN WSS-VARI.
           MOVE SPACES TO WSS-GLS-DIRE-QDI IN WSS-VARI.
           MOVE ZEROES TO WSS-FEC-FVRF-QDI IN WSS-VARI.
           IF SCR-OFRM-SRV
               PERFORM CLR-FRM-MTX.
       FIN-CLR-WSS.
           EXIT.

       CLR-FRM-MTX SECTION.
       INI-CLR-FRM-MTX.
           MOVE ZEROES TO FRM-NUM-DIRE IN QDI-FLD(1).
           MOVE SPACES TO FRM-MSC-TDIR IN QDI-FLD(1).
           MOVE SPACES TO FRM-GLS-TDIR IN QDI-FLD(1).
           MOVE SPACES TO FRM-COD-LUGA IN QDI-FLD(1).
           MOVE SPACES TO FRM-GLS-LUGA IN QDI-FLD(1).
           MOVE SPACES TO FRM-GLS-FONO IN QDI-FLD(1).
           MOVE SPACES TO FRM-MSC-SDIR IN QDI-FLD(1).
           MOVE SPACES TO FRM-GLS-SDIR IN QDI-FLD(1).
           MOVE SPACES TO FRM-GLS-DIRE IN QDI-FLD(1).
           MOVE ZEROES TO FRM-FEC-FVRF IN QDI-FLD(1).
       FIN-CLR-FRM-MTX.
           EXIT.

       CRG-VEC-QDI SECTION.
       INI-CRG-VEC-QDI.
           ADD 1 TO WSS-J.
           IF WSS-J IN WSS-VARI > WSS-JMAX IN WSS-VARI
               GO TO FIN-CRG-VEC-QDI.

           PERFORM CLR-QDI-VEC.
           
           MOVE RCD-NUM-DIRE IN RCD TO
                                WSS-NUM-DIRE-V IN WSS-VARI(WSS-J).
           MOVE RCD-MSC-TDIR IN RCD TO
                                WSS-MSC-TDIR-V IN WSS-VARI(WSS-J).

           IF RCD-MSC-SDIR IN RCD > SPACES                          
               MOVE 'SGC' TO MSC-COD-SIST                               
               MOVE 'TDIR' TO MSC-COD-TMSC IN MSC                       
               MOVE RCD-MSC-TDIR IN RCD TO MSC-COD-CMSC IN MSC      
               PERFORM BUS-MSC                                          
               MOVE MSC-GLS-DESC IN MSC TO 
                                WSS-GLS-TDIR-V IN WSS-VARI(WSS-J)
           ELSE
               MOVE SPACES TO   WSS-GLS-TDIR-V IN WSS-VARI(WSS-J).
                             
           MOVE RCD-COD-LUGA IN RCD TO
                                WSS-COD-LUGA-V IN WSS-VARI(WSS-J).
                    
           IF RCD-COD-LUGA IN RCD > SPACES                         
               MOVE RCD-COD-LUGA IN RCD TO WSS-COD-LUGA            
               IF WSS-IND-PAIS = 'P'                                   
                   MOVE 'TAB' TO TAB-COD-SIST                          
                   MOVE 'PAI' TO TAB-COD-TTAB IN TAB                   
                   MOVE WSS-COD-PAIS TO TAB-COD-CTAB IN TAB            
                   PERFORM BUS-TAB                                     
                   MOVE TAB-GLS-DESC IN TAB TO 
                                     WSS-GLS-LUGA-V IN WSS-VARI(WSS-J)
               ELSE                                                    
                   MOVE 'TAB' TO TAB-COD-SIST                          
                   MOVE 'COM' TO TAB-COD-TTAB IN TAB                   
                   MOVE RCD-COD-LUGA IN RCD TO TAB-COD-CTAB IN TAB 
                   PERFORM BUS-TAB                                     
                   MOVE TAB-GLS-DESC IN TAB TO 
                                     WSS-GLS-LUGA-V IN WSS-VARI(WSS-J).

           MOVE RCD-GLS-FONO IN RCD TO
                                WSS-GLS-FONO-V IN WSS-VARI(WSS-J).

           MOVE RCD-MSC-SDIR IN RCD TO
                                WSS-MSC-SDIR-V IN WSS-VARI(WSS-J).

           IF RCD-MSC-SDIR IN RCD > SPACES                          
               MOVE 'SGC' TO MSC-COD-SIST                               
               MOVE 'SDIR' TO MSC-COD-TMSC IN MSC                       
               MOVE RCD-MSC-SDIR IN RCD TO MSC-COD-CMSC IN MSC      
               PERFORM BUS-MSC                                          
               MOVE MSC-GLS-DESC IN MSC TO                              
                    WSS-GLS-SDIR-V IN WSS-VARI(WSS-J)                             
           ELSE                                                         
               MOVE SPACES TO                                           
                    WSS-GLS-SDIR-V IN WSS-VARI(WSS-J).                             

           MOVE RCD-GLS-DIRE IN RCD TO
                                WSS-GLS-DIRE-V IN WSS-VARI(WSS-J).
           MOVE RCD-FEC-FVRF IN RCD TO
                                WSS-FEC-FVRF-V IN WSS-VARI(WSS-J).

       FIN-CRG-VEC-QDI.
           EXIT.

       CLR-QDI-VEC SECTION.
       INI-CLR-QDI-VEC.
           MOVE ZEROES TO WSS-NUM-DIRE-V(WSS-J).
           MOVE SPACES TO WSS-MSC-TDIR-V(WSS-J).
           MOVE SPACES TO WSS-GLS-TDIR-V(WSS-J).
           MOVE SPACES TO WSS-COD-LUGA-V(WSS-J).
           MOVE SPACES TO WSS-GLS-LUGA-V(WSS-J).
