       COPY GNSBBMSG.
      * COPY GNSBFMSG.
       COPY GNSBTABT.
       COPY TABBVUSR.

       COPY TABBBOFI.
       COPY TABBVOFI.
       COPY TABBFOFI.


       COPY SGCBGCLI.
       COPY SGCBFDBC.
       COPY SGCBPDBC.
       COPY TABBBUSR.
       COPY TABBFUSR.
      *>>>>
*% END
