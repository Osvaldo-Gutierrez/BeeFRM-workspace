*%! Codigo adicional
*% IF SETTAG
*% VSNBQ = "05.12.12.1"
*% FNCBQ = "SGC-BIENES RAICES"
*% END
*% IF DBC_GLS_ICLI 
      *<<<< DBC_GLS_ICLI
           IF DBC-NUM-ICLI IN BRZ-FLD > ZEROES AND
              BRZ-IIC-IFOL IN BRZ-FLD = ZEROES
              MOVE DBC-NUM-ICLI IN BRZ-FLD TO DBC-NUM-ICLI IN DBC
              MOVE DBC-IND-ICLI IN BRZ-FLD TO DBC-IND-ICLI IN DBC
              MOVE DBC-GLS-ICLI IN BRZ-FLD TO DBC-GLS-ICLI IN DBC
              MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY
              MOVE FIO-GET-KEY TO FIO-CMND
              PERFORM SGC-FIO-DBC
              IF NOT FIO-STAT-OKS
                  MOVE 'SGC'        TO MSG-COD-SIST
                  MOVE 'CLI    NEX' TO MSG-COD-MENS
                  PERFORM GET-MSG
                  MOVE SPACES       TO FRM-GLS-NOMC IN BRZ-FLD
                  MOVE MSG-GLS-DESC TO FRM-MENS
                  MOVE FRM-SUAR-MAL TO FRM-SUAR
                  GO TO FIN-VAL-DBC-GLS-ICLI
              ELSE
                  PERFORM CPT-STR-NOMC
                  MOVE 'S'          TO WSS-EXI-RBRZ IN WSS-VARI
                  PERFORM FND-RPR-LAST
                  PERFORM CHK-RPR-LAST
           ELSE
              IF  BRZ-IIC-IFOL IN BRZ-FLD > ZEROES
                  PERFORM FND-RPR-IFOL.
      *>>>>
*% END
*% IF FIN_PUT_BRZ_BRZ
      *<<<< FIN_PUT_BRZ_BRZ
            MOVE RPR-CAI-IFOL IN RPR   TO BRZ-CAI-IFOL IN BRZ-FLD.
            MOVE RPR-IIC-IFOL IN RPR   TO BRZ-IIC-IFOL IN BRZ-FLD.

            MOVE BRZ-NUM-SEFR IN BRZ      TO WSS-SIG-DSPL IN WSS-VARI.
            MOVE BRZ-NUM-AEFR IN BRZ      TO WSS-ANO-DSPL IN WSS-VARI.
            MOVE BRZ-NUM-MEFR IN BRZ      TO WSS-MES-DSPL IN WSS-VARI.
            MOVE BRZ-NUM-DEFR IN BRZ      TO WSS-DIA-DSPL IN WSS-VARI.
            MOVE WSS-FEC-DSPL IN WSS-VARI TO FRM-FEC-FEFR IN BRZ-FLD.

            MOVE BRZ-COD-EEFR IN BRZ   TO BRZ-COD-EEFR IN BRZ-FLD.
            MOVE SPACES                TO FRM-GLS-COMU IN BRZ-FLD. 
            MOVE SPACES                TO FRM-GLS-MONB IN BRZ-FLD.
            MOVE SPACES                TO FRM-GLS-CONS IN BRZ-FLD.
            IF BRZ-COD-COMU IN BRZ-FLD > SPACES
               MOVE 'TAB'  TO TAB-COD-SIST
               MOVE 'COM'  TO TAB-COD-TTAB IN TAB
               MOVE BRZ-COD-COMU IN BRZ-FLD  TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC TO FRM-GLS-COMU IN BRZ-FLD.
            IF BRZ-COD-MONB IN BRZ-FLD > SPACES
               MOVE 'TAB'  TO TAB-COD-SIST
               MOVE 'VLR'  TO TAB-COD-TTAB IN TAB
               MOVE BRZ-COD-MONB IN BRZ-FLD  TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC TO FRM-GLS-MONB IN BRZ-FLD.
            IF BRZ-COD-CONS IN BRZ-FLD > SPACES
               MOVE 'TAB'  TO TAB-COD-SIST
               MOVE 'COM'  TO TAB-COD-TTAB IN TAB
               MOVE BRZ-COD-CONS IN BRZ-FLD     TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC TO FRM-GLS-CONS IN BRZ-FLD.
      *>>>>
*% END
*% IF FIN_BRZ_COD_COMU
      *<<<< FIN_BRZ_COD_COMU
              MOVE 'TAB'  TO TAB-COD-SIST.
              MOVE 'COM'  TO TAB-COD-TTAB IN TAB.
              MOVE BRZ-COD-COMU IN BRZ-FLD     TO TAB-COD-CTAB IN TAB.
              PERFORM VAL-COD.
              IF MSG-COD-MENS NOT > SPACES
                 MOVE TAB-GLS-DESC TO FRM-GLS-COMU IN BRZ-FLD
              ELSE
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 MOVE FRM-SUAR-MAL TO FRM-SUAR
                 GO TO FIN-VAL-BRZ-COD-COMU.
      *>>>>
*% END
*% IF FIN_BRZ_COD_CONS
      *<<<< FIN_BRZ_COD_CONS
              MOVE 'TAB'  TO TAB-COD-SIST.
              MOVE 'COM'  TO TAB-COD-TTAB IN TAB.
              MOVE BRZ-COD-CONS IN BRZ-FLD     TO TAB-COD-CTAB IN TAB.
              PERFORM VAL-COD.
              IF MSG-COD-MENS NOT > SPACES
                 MOVE TAB-GLS-DESC TO FRM-GLS-CONS IN BRZ-FLD
              ELSE
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 MOVE FRM-SUAR-MAL TO FRM-SUAR
                 GO TO FIN-VAL-BRZ-COD-CONS.
      *>>>>
*% END
*% IF FIN_BRZ_COD_MONB
      *<<<< FIN_BRZ_COD_MONB
              MOVE 'TAB'  TO TAB-COD-SIST.
              MOVE 'VLR'  TO TAB-COD-TTAB IN TAB.
              MOVE BRZ-COD-MONB IN BRZ-FLD     TO TAB-COD-CTAB IN TAB.
              PERFORM VAL-COD.
              IF MSG-COD-MENS NOT > SPACES
                 MOVE TAB-GLS-DESC TO FRM-GLS-MONB IN BRZ-FLD
              ELSE
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 MOVE FRM-SUAR-MAL TO FRM-SUAR
                 GO TO FIN-VAL-BRZ-COD-MONB.
      *>>>> 
*% END
*% IF BRZ_NUM_AINS
      *<<<< BRZ_NUM_AINS
           IF BRZ-NUM-AINS IN BRZ-FLD > ZEROES
              IF BRZ-NUM-AINS IN BRZ-FLD < 1900 OR 
                 BRZ-NUM-AINS IN BRZ-FLD > 2500
                 MOVE 'SGC'          TO MSG-COD-SIST
                 MOVE 'DATO NO VAL ' TO MSG-COD-MENS
                 PERFORM GET-MSG
                 MOVE MSG-GLS-DESC   TO FRM-MENS
                 MOVE FRM-SUAR-MAL   TO FRM-SUAR
                 GO TO FIN-VAL-BRZ-NUM-AINS.
      *>>>>
*% END
*% IF BRZ_COD_RLCM
      *<<<< BRZ_COD_RLCM
      * BUS-TAB Busca Comuna del Campo ROL en Tabla
           IF BRZ-COD-RLCM IN BRZ-FLD > SPACES
              MOVE 'TAB'  TO TAB-COD-SIST
              MOVE 'COM'  TO TAB-COD-TTAB IN TAB
              MOVE BRZ-COD-RLCM IN BRZ-FLD TO TAB-COD-CTAB IN TAB
              PERFORM BUS-TAB
              IF MSG-COD-MENS IN MSG > SPACES
                 PERFORM GET-MSG
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 MOVE FRM-SUAR-MAL TO FRM-SUAR
                 GO TO FIN-VAL-BRZ-COD-RLCM.
      *>>>>
*% END
*% IF BRZ_GLS_CALL
      *<<<< BRZ_GLS_CALL
           IF BRZ-GLS-CALL IN BRZ-FLD NOT > SPACES
              PERFORM ERR-GLS.
      *>>>>
*% END
*% IF BRZ_GLS_NMRO
      *<<<< BRZ_GLS_NMRO
           IF BRZ-GLS-NMRO IN BRZ-FLD NOT > SPACES
              PERFORM ERR-GLS.
      *>>>>
*% END
*% IF BRZ_VAL_MDOR
      *<<<< BRZ_VAL_MDOR
           IF BRZ-VAL-MDOR IN BRZ-FLD NOT > ZEROES
              PERFORM ERR-GLS.
      *>>>>
*% END
*% IF BRZ_VAL_MSAL
      *<<<< BRZ_VAL_MSAL
           IF BRZ-VAL-MSAL IN BRZ-FLD NOT > ZEROES
              PERFORM ERR-GLS.
      *>>>>
*% END
*% IF FIN_RET_BRZ_BRZ
      *<<<< FIN_RET_BRZ_BRZ
           IF SCR-CMND = 'ING'
              PERFORM GET-FHOY
              MOVE HOY-FHOY             TO BRZ-FEC-FEFR IN BRZ
              MOVE SCR-USER             TO BRZ-COD-EEFR IN BRZ.
      *>>>>
*% END
*% IF INI_PUT_RPR_BRZ
      *<<<< INI_PUT_RPR_BRZ
           GO TO FIN-PUT-RPR-BRZ.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       FND-RPR-IFOL SECTION.
       INI-FND-RPR-IFOL.
           MOVE 'N'                        TO WSS-FND-INFO.
           MOVE BRZ-CAI-IFOL IN BRZ-FLD    TO RPR-CAI-IFOL IN RPR.
           MOVE BRZ-IIC-IFOL IN BRZ-FLD    TO RPR-IIC-IFOL IN RPR.
           MOVE FIO-GET-KEY                TO FIO-CMND.
           PERFORM SGC-FIO-RPR.
           IF NOT FIO-STAT-OKS
              MOVE 'SGC'          TO MSG-COD-SIST
              MOVE 'FOLRPRNODISP' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE SPACES       TO BRZ-CAI-IFOL IN BRZ-FLD
              MOVE ZEROES       TO BRZ-IIC-IFOL IN BRZ-FLD
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE FRM-SUAR-MAL TO FRM-SUAR
              GO TO FIN-FND-RPR-IFOL
           ELSE
            IF ( SCR-CMND = 'MOD' OR SCR-CMND = 'ELI' ) AND
                 RPR-IND-ESTA IN RPR = 'A'
               MOVE 'SGC'         TO MSG-COD-SIST
               MOVE 'FOLRPRYAACA' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE 'S'          TO WSS-FND-INFO
               GO TO FIN-FND-RPR-IFOL
            ELSE
             IF SCR-CMND = 'MRA'  AND
                RPR-IND-ESTA IN RPR NOT = 'A'
                 MOVE 'SGC'         TO MSG-COD-SIST
                 MOVE 'FOLRPRNOACA' TO MSG-COD-MENS
                 PERFORM GET-MSG
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 MOVE FRM-SUAR-MAL TO FRM-SUAR
                 MOVE 'S'          TO WSS-FND-INFO
                 GO TO FIN-FND-RPR-IFOL
             ELSE
                 PERFORM GET-RPR-DATO
                 PERFORM FND-DBC-ICLI
                 GO TO FIN-FND-RPR-IFOL.
       FIN-FND-RPR-IFOL.
           EXIT.

       FND-DBC-ICLI SECTION.
       INI-FND-DBC-ICLI.
              MOVE RPR-CIC-ICLI IN RPR TO DBC-CIC-ICLI IN DBC
              MOVE SPACES      TO FIO-AKEY
              MOVE FIO-GET-KEY TO FIO-CMND
              PERFORM SGC-FIO-DBC
              IF NOT FIO-STAT-OKS
                  MOVE 'SGC'        TO MSG-COD-SIST
                  MOVE 'CLI    NEX' TO MSG-COD-MENS
                  PERFORM GET-MSG
                  MOVE SPACES       TO FRM-GLS-NOMC IN BRZ-FLD
                  MOVE MSG-GLS-DESC TO FRM-MENS
                  MOVE FRM-SUAR-MAL TO FRM-SUAR
                  GO TO FIN-FND-DBC-ICLI
              ELSE
                  MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN BRZ-FLD
                  MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN BRZ-FLD
                  MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN BRZ-FLD
                  MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN BRZ-FLD
                  PERFORM CPT-STR-NOMC.
       FIN-FND-DBC-ICLI.
           EXIT.

       FND-RPR-LAST SECTION.
       INI-FND-RPR-LAST.
           MOVE 'N'                        TO WSS-FND-INFO.
           MOVE DBC-CIC-ICLI IN DBC        TO RPR-CIC-ICLI IN RPR.
           MOVE '99999999'                 TO RPR-FEC-FRPR IN RPR.
           MOVE FIO-GET-LEQ                TO FIO-CMND.
       LUP-FND-RPR-LAST.
           MOVE 'RPR-AKY-IRPR'             TO FIO-AKEY. 
           PERFORM SGC-FIO-RPR.
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF
              GO TO FIN-FND-RPR-LAST.
           IF RPR-CIC-ICLI IN RPR NOT = DBC-CIC-ICLI IN DBC
              GO TO FIN-FND-RPR-LAST
           ELSE
            IF SCR-CMND = 'ING' AND WSS-EXI-RBRZ = 'N'
               PERFORM GET-RPR-DATO
               GO TO FIN-FND-RPR-LAST
            ELSE
             IF ( SCR-CMND = 'MOD' OR SCR-CMND = 'ELI' ) AND
                RPR-IND-ESTA IN RPR = 'A'
                MOVE 'SGC'         TO MSG-COD-SIST
                MOVE 'FOLRPRYAACA' TO MSG-COD-MENS
                PERFORM GET-MSG
                MOVE MSG-GLS-DESC TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE 'S'          TO WSS-FND-INFO
                GO TO FIN-FND-RPR-LAST
             ELSE
              IF SCR-CMND = 'MRA'  AND
                 RPR-IND-ESTA IN RPR NOT = 'A'
                 MOVE 'SGC'         TO MSG-COD-SIST
                 MOVE 'FOLRPRNOACA' TO MSG-COD-MENS
                 PERFORM GET-MSG
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 MOVE FRM-SUAR-MAL TO FRM-SUAR
                 MOVE 'S'          TO WSS-FND-INFO
                 GO TO FIN-FND-RPR-LAST
              ELSE
                 PERFORM GET-RPR-DATO
                 GO TO FIN-FND-RPR-LAST.
       LEE-PRV-RPR.
           MOVE FIO-GET-PRV            TO FIO-CMND.
           GO TO LUP-FND-RPR-LAST.           
       FIN-FND-RPR-LAST.
           EXIT.

       GET-RPR-DATO SECTION.
       INI-GET-RPR-DATO.
           MOVE 'S'                 TO WSS-FND-INFO IN WSS-VARI.
           MOVE RPR-CAI-IFOL IN RPR TO BRZ-CAI-IFOL IN BRZ-FLD.
           MOVE RPR-IIC-IFOL IN RPR TO BRZ-IIC-IFOL IN BRZ-FLD.
       FIN-GET-RPR-DATO.
           EXIT.

       CHK-RPR-LAST SECTION.
       INI-CHK-RPR-LAST.
           IF WSS-FND-INFO = 'N' 
                  MOVE 'SGC'          TO MSG-COD-SIST
                  MOVE 'FOLRPRNODISP' TO MSG-COD-MENS
                  PERFORM GET-MSG
                  MOVE SPACES       TO BRZ-CAI-IFOL IN BRZ-FLD
                  MOVE ZEROES       TO BRZ-IIC-IFOL IN BRZ-FLD
                  MOVE MSG-GLS-DESC TO FRM-MENS
                  MOVE FRM-SUAR-MAL TO FRM-SUAR                                
                  GO TO FIN-CHK-RPR-LAST.
       FIN-CHK-RPR-LAST.
           EXIT.

       CPT-STR-NOMC SECTION.
       INI-CPT-STR-NOMC.
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.
           PERFORM CPT-BLKS.
           MOVE CPT-STRN  TO FRM-GLS-NOMC IN BRZ-FLD.
           MOVE DBC-CAI-ICLI IN DBC TO SCR-CAI-ICLI.
           MOVE DBC-IIC-ICLI IN DBC TO SCR-IIC-ICLI.
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN BRZ-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN BRZ-FLD.
       FIN-CPT-STR-NOMC.
           EXIT.
      *>>>> EOF
*% END
