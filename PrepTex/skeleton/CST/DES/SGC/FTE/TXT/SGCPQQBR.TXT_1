*%! Codigo adicional para archivo
*% LARGO_TOTAL_COMMAREA = 3000
*% NO_TIENE_TIMESTAMP = TRUE
*% PGM_PTC = TRUE
*% GEN_MFC = TRUE
*% IF PGM_PQ
*% CLRMFC = "SGCZQQBR"
*% END

*% IF FIN_SCR_VARI
           03 SCR-NUM-PANT               PIC 9(0005).                   
           03 SCR-QBR-FLD                PIC X(1100).                   
           03 SCR-CIC-ICLI.                                             
              05 SCR-CAI-ICLI            PIC X(04).                     
              05 SCR-IIC-ICLI            PIC X(08).                     
*% END
*% IF WSS
      *<<<< WSS                                                         
       COPY GNSWGCPT.                                                   
       COPY GNSWGSTR.                                                   
      *COPY GNSWGNUM.                                                   
       COPY SGCBRDBC.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.        
       COPY SGCBRRPR.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RPR-REQA==.        
       COPY SGCWXQBR.                                                   
      *>>>>                                                             
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC                                                     
       01 WSS-NUM-CCUR.                                                 
           03 WSS-NUM-ICUR-EDT  PIC  ZZ9.                               
           03 WSS-NUM-ICUR-ALF       PIC X(03).                         
           03 WSS-NUM-ICUR      REDEFINES                               
              WSS-NUM-ICUR-ALF       PIC 9(03).                         
      *>>>>                                                             
*% END
*% IF INI_FIO
      *<<<< INI_FIO                                                     
           IF SCR-OFRM-SRV                                              
                MOVE TSK-TERM-ALF TO QBR-COD-TERM                       
                MOVE TSK-TERM-ALF TO QBR-COD-TERM-MAS                   
           ELSE                                                         
                MOVE EIBTRMID     TO QBR-COD-TERM                       
                MOVE EIBTRMID     TO QBR-COD-TERM-MAS.                  
           MOVE +3859 TO SCR-TCMA.                                      
           PERFORM GNS-ERR-QUE.                                         
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )      
               MOVE SCR-QBR-FLD TO QBR-FLD                              
               GO TO FIN-INI.                                           
                                                                        
           IF ( FRM-FFLD = FRM-FFLD-PF6 )                               
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM        
               ADD 1              TO QUE-NITM                           
               MOVE QBR-NOM-TSQBR TO QUE-COLA                           
               MOVE QBR-LEN-TSQBR TO QUE-LITM                           
               MOVE QUE-GET       TO QUE-CMND                           
               PERFORM GNS-PRO-QUE                                      
               IF QUE-STAT NOT = QUE-STAT-OKS                           
                  MOVE SCR-QBR-FLD TO QBR-FLD                           
                  GO TO FIN-INI.                                        
      **     PERFORM GET-FHOY.                                          
      *>>>>                                                             
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC                                                 
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6                   
               PERFORM VAL-NUM-KEY-QBR                                  
               PERFORM VAL-NUM-FLD-QBR                                  
               PERFORM SCRL-TAB-QBR                                     
               PERFORM SCR-TRL.                                         
      *>>>>                                                             
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY                                                 
           MOVE BRZ-CAI-IFOL IN QBR-FLD TO BRZ-CAI-IFOL IN BRZ.         
           MOVE BRZ-IIC-IFOL IN QBR-FLD TO BRZ-IIC-IFOL IN BRZ.         
      *INI-RFS                                                          
           IF FRM-IND-SGTE IN QBR-FLD = 'S' AND                         
              BRZ-NUM-IBRZ IN QBR-FLD > ZEROS                           
              MOVE BRZ-NUM-IBRZ IN QBR-FLD TO BRZ-NUM-IBRZ IN BRZ       
           ELSE                                                         
              MOVE ZEROES TO BRZ-NUM-IBRZ IN BRZ.                       
      *FIN-RFS                                                          
           MOVE ZEROES TO WSS-J IN WSS-VARI.                            
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-LEE-BRZ.                                                     
           PERFORM SGC-FIO-BRZ.                                         
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF                          
               GO TO CON-FND-KEY.                                       
                                                                        
           IF BRZ-CAI-IFOL IN QBR-FLD     NOT = BRZ-CAI-IFOL IN BRZ OR  
              BRZ-IIC-IFOL IN QBR-FLD     NOT = BRZ-IIC-IFOL IN BRZ     
              GO TO CON-FND-KEY.                                        
                                                                        
           PERFORM CRG-VEC-QBR.                                         
                                                                        
           IF (SCR-OFRM-SRV AND WSS-J > WSS-NITMMAX)                    
               GO TO CON-FND-KEY.                                       
                                                                        
           IF ((NOT SCR-OFRM-SRV) AND WSS-J > WSS-JMAX)                 
               GO TO CON-FND-KEY.                                       
                                                                        
       NXT-LEE-BRZ.                                                     
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-LEE-BRZ.                                           
      *>>>>                                                             
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY                                                 
       CON-FND-KEY.                                                     
           PERFORM PUT-TAB.                                             
                                                                        
           MOVE '00' TO FIO-STAT.                                       
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           GO TO FIN-FND-KEY.                                           
      *----------------------------------------------------------       
       MSG-FND-KEY.                                                     
           MOVE 'TAB' TO MSG-COD-SIST.                                  
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MENS.                               
           MOVE '00' TO FIO-STAT.                                       
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           GO TO FIN-FND-KEY.                                           
      *>>>>                                                             
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON                                                 
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )           
      *>>>>                                                             
*% END
*% IF EOF
      *<<<< EOF                                                         
       COPY GNSBBTAB.                                                   
       COPY GNSBVTAB.                                                   
       COPY GNSBVCOD.                                                   
      *COPY GNSBPFEC.                                                   
       COPY SGCBFDBC.                                                   
       COPY SGCBFRPR.                                                   
                                                                        
       PUT-TAB SECTION.                                                 
       INI-PUT-TAB.                                                     
           MOVE QBR-NOM-TSQBR     TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE QBR-NOM-TSQBR-MAS TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE 1 TO SCR-NUM-PANT.                                      
           MOVE LOW-VALUES TO WSS-GLS-NQBR-IDX-MTI.                     
           MOVE LOW-VALUES TO WSS-GLS-NQBR-IDX-FRM.                     
      *                                                                 
           MOVE 1 TO FRM-IFLD.                                          
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.                        
                                                                        
           MOVE 1 TO WSS-F IN WSS-VARI.                                 
       LUP-PUT-TAB.                                                     
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI                  
              GO TO FIN-PUT-TAB.                                        
           IF WSS-NUM-CORR-V(WSS-F) > SPACES                            
              PERFORM PUT-TAB-QBR                                       
              PERFORM PUT-QBR-QUE                                       
              PERFORM SET-FRM-IFLD                                      
           ELSE                                                         
           IF FRM-IFLD = 1 AND WSS-F = 1                                
              MOVE 'TAB' TO MSG-COD-SIST                                
              MOVE 'QBR    NEX' TO MSG-COD-MENS                         
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              MOVE '00' TO FIO-STAT                                     
              MOVE 'S' TO SCR-IERR                                      
              GO TO FIN-PUT-TAB                                         
           ELSE                                                         
              MOVE '00' TO FIO-STAT                                     
              GO TO FIN-PUT-TAB.                                        
                                                                        
           ADD 1 TO WSS-F IN WSS-VARI.                                  
           GO TO LUP-PUT-TAB.                                           
       FIN-PUT-TAB.                                                     
           MOVE 1                  TO QUE-NITM.                         
           MOVE QBR-LEN-TSQBR-MAS  TO QUE-LITM.                         
           MOVE QBR-NOM-TSQBR-MAS  TO QUE-COLA.                         
                                                                        
           IF WSS-NITM > WSS-NITMMAX                                    
              MOVE 'S'           TO QBR-MAS-IND-HMAS                    
           ELSE                                                         
              MOVE 'N'           TO QBR-MAS-IND-HMAS.                   
                                                                        
           MOVE WSS-NITM         TO WSS-NUM-ICUR.                       
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.                   
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.                   
           MOVE WSS-NUM-ICUR-ALF TO QBR-MAS-NUM-TOCC.                   
           MOVE QBR-MAS-ITEM     TO QUE-ITEM.                           
           MOVE QUE-PUT          TO QUE-CMND.                           
           PERFORM GNS-PRO-QUE.                                         
           IF WSS-NITM > WSS-IMAX                                       
               MOVE 'SGC' TO MSG-COD-SIST                               
               MOVE 'QBRTABDMAS' TO MSG-COD-MENS                        
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN2.                           
       EXT-PUT-TAB.                                                     
           EXIT.                                                        
                                                                        
       SET-FRM-IFLD SECTION.                                            
       INI-SET-FRM-IFLD.                                                
           IF SCR-OFRM-SRV                                              
                MOVE 1 TO FRM-IFLD                                      
           ELSE                                                         
                ADD 1 TO FRM-IFLD.                                      
       FIN-SET-FRM-IFLD.                                                
           EXIT.                                                        
      *--------------------------------------------                     
       SCRL-TAB-QBR SECTION.                                            
       INI-SCRL-TAB-QBR.                                                
           MOVE SPACES TO MSG-COD-MENS.                                 
                                                                        
           IF FRM-FFLD = FRM-FFLD-PF6                                   
               PERFORM PUT-TAB-ASC                                      
           ELSE                                                         
               PERFORM PUT-TAB-DSC.                                     
           IF MSG-COD-MENS > SPACES                                     
               MOVE 'TAB' TO MSG-COD-SIST                               
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN2.                           
       FIN-SCRL-TAB-QBR.                                                
           EXIT.                                                        
                                                                        
      *--------------------------------------------                     
       PUT-TAB-QBR SECTION.                                             
       INI-PUT-TAB-QBR.                                                 
           PERFORM CLR-WSS.                                             
           MOVE WSS-NUM-CORR-V IN WSS-VARI(WSS-F) TO                    
                                   WSS-NUM-CORR-QBR IN WSS-VARI.        
           MOVE WSS-GLS-CALL-V IN WSS-VARI(WSS-F) TO                    
                                   WSS-GLS-CALL-QBR IN WSS-VARI.        
           MOVE WSS-GLS-NMRO-V IN WSS-VARI(WSS-F) TO                    
                                   WSS-GLS-NMRO-QBR IN WSS-VARI.        
           MOVE WSS-GLS-COMU-V IN WSS-VARI(WSS-F) TO                    
                                   WSS-GLS-COMU-QBR IN WSS-VARI.        
           MOVE WSS-COD-ROLN-V IN WSS-VARI(WSS-F) TO                    
                                   WSS-COD-ROLN-QBR IN WSS-VARI.        
           MOVE WSS-VAL-AFSC-V IN WSS-VARI(WSS-F) TO                    
                                   WSS-VAL-AFSC-QBR IN WSS-VARI.        
           MOVE WSS-VAL-VCOM-V IN WSS-VARI(WSS-F) TO                    
                                   WSS-VAL-VCOM-QBR IN WSS-VARI.        
           MOVE WSS-VAL-MSAL-V IN WSS-VARI(WSS-F) TO                    
                                   WSS-VAL-MSAL-QBR IN WSS-VARI.        
                                                                        
           MOVE WSS-NUM-CORR-QBR IN WSS-VARI TO WSS-NUM-CORR-FRM.       
           MOVE WSS-GLS-CALL-QBR IN WSS-VARI TO WSS-GLS-CALL-FRM.       
           MOVE WSS-GLS-NMRO-QBR IN WSS-VARI TO WSS-GLS-NMRO-FRM.       
           MOVE WSS-GLS-COMU-QBR IN WSS-VARI TO WSS-GLS-COMU-FRM.       
           MOVE WSS-COD-ROLN-QBR IN WSS-VARI TO WSS-COD-ROLN-FRM.       
           MOVE WSS-VAL-AFSC-QBR IN WSS-VARI TO WSS-VAL-AFSC-FRM.       
           MOVE WSS-VAL-VCOM-QBR IN WSS-VARI TO WSS-VAL-VCOM-FRM.       
           MOVE WSS-VAL-MSAL-QBR IN WSS-VARI TO WSS-VAL-MSAL-FRM.       
                                                                        
           IF FRM-IFLD > WSS-IMAX                                       
              GO TO FIN-PUT-TAB-QBR.                                    
                                                                        
           MOVE WSS-NUM-CORR-QBR IN WSS-VARI TO                         
               FRM-NUM-IBRZ IN QBR-FLD(FRM-IFLD).                       
           MOVE WSS-GLS-CALL-QBR IN WSS-VARI TO                         
               FRM-GLS-CALL IN QBR-FLD(FRM-IFLD).                       
           MOVE WSS-GLS-NMRO-QBR IN WSS-VARI TO                         
               FRM-GLS-NMRO IN QBR-FLD(FRM-IFLD).                       
           MOVE WSS-GLS-COMU-QBR IN WSS-VARI TO                         
               FRM-GLS-COMU IN QBR-FLD(FRM-IFLD).                       
           MOVE WSS-COD-ROLN-QBR IN WSS-VARI TO                         
               FRM-COD-ROLN IN QBR-FLD(FRM-IFLD).                       
           MOVE WSS-VAL-AFSC-QBR IN WSS-VARI TO                         
               FRM-VAL-AFSC IN QBR-FLD(FRM-IFLD).                       
           MOVE WSS-VAL-VCOM-QBR IN WSS-VARI TO                         
               FRM-VAL-VCOM IN QBR-FLD(FRM-IFLD).                       
           MOVE WSS-VAL-MSAL-QBR IN WSS-VARI TO                         
               FRM-VAL-MSAL IN QBR-FLD(FRM-IFLD).                       
       FIN-PUT-TAB-QBR.                                                 
           EXIT.                                                        
      *----------------------------------------------------             
       PUT-QBR-QUE SECTION.                                             
       INI-PUT-QBR-QUE.                                                 
           PERFORM PRO-FLD-QUE.                                         
           ADD 1 TO WSS-NITM.                                           
           MOVE QBR-NOM-TSQBR    TO QUE-COLA.                           
           MOVE QBR-LEN-TSQBR    TO QUE-LITM.                           
           MOVE WSS-NITM         TO QUE-NITM.                           
           MOVE SPACES           TO QUE-ITEM.                           
           IF SCR-OFRM-SRV                                              
              MOVE WSS-GLS-NQBR-IDX-MTI  TO QUE-ITEM                    
           ELSE                                                         
              MOVE WSS-GLS-NQBR-IDX-FRM  TO QUE-ITEM.                   
           MOVE QUE-PUT  TO QUE-CMND.                                   
           PERFORM GNS-PRO-QUE.                                         
       FIN-PUT-QBR-QUE.                                                 
           EXIT.                                                        
      *----------------------------------------------------             
                                                                        
       PUT-TAB-ASC SECTION.                                             
       INI-PUT-TAB-ASC.                                                 
           MOVE SPACES TO MSG-COD-MENS.                                 
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.           
           MOVE 0 TO FRM-IFLD.                                          
       LUP-PUT-TAB-ASC.                                                 
           ADD 1              TO QUE-NITM.                              
           MOVE QBR-NOM-TSQBR TO QUE-COLA.                              
           MOVE QBR-LEN-TSQBR TO QUE-LITM.                              
           MOVE QUE-GET       TO QUE-CMND.                              
           PERFORM GNS-PRO-QUE.                                         
           IF QUE-STAT NOT = QUE-STAT-OKS                               
               IF FRM-IFLD = 0                                          
GVI                MOVE 'SGC'        TO MSG-COD-SIST                    
                   MOVE 'QBRTABNONXT' TO MSG-COD-MENS                   
                   GO TO FIN-PUT-TAB-ASC                                
               ELSE                                                     
                   GO TO FIN-PUT-TAB-ASC.                               
           ADD 1 TO FRM-IFLD.                                           
           MOVE QUE-ITEM TO FRM-NUM-IBRZ-IDX(FRM-IFLD).                 
           IF FRM-IFLD < WSS-IMAX                                       
               GO TO LUP-PUT-TAB-ASC.                                   
       FIN-PUT-TAB-ASC.                                                 
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX                      
               ADD 1 FRM-IFLD GIVING WSS-I                              
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1            
                                 UNTIL WSS-I > WSS-IMAX.                
           IF FRM-IFLD > 0                                              
               ADD 1 TO SCR-NUM-PANT.                                   
           IF FRM-IFLD = WSS-IMAX                                       
               ADD 1              TO QUE-NITM                           
               MOVE QBR-NOM-TSQBR TO QUE-COLA                           
               MOVE QBR-LEN-TSQBR TO QUE-LITM                           
               MOVE QUE-GET       TO QUE-CMND                           
               PERFORM GNS-PRO-QUE                                      
               IF QUE-STAT = QUE-STAT-OKS                               
                   MOVE 'SGC'        TO MSG-COD-SIST                    
                   MOVE 'QBRTABDMAS' TO MSG-COD-MENS.                   
       EXT-PUT-TAB-ASC.                                                 
           EXIT.                                                        
                                                                        
       PUT-TAB-DSC SECTION.                                             
       INI-PUT-TAB-DSC.                                                 
           MOVE SPACES TO MSG-COD-MENS.                                 
           IF SCR-NUM-PANT = 1                                          
               MOVE 'QBRTABNOPRV' TO MSG-COD-MENS                       
               GO TO EXT-PUT-TAB-DSC.                                   
                                                                        
           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.                
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.               
           ADD 1 TO QUE-NITM.                                           
           ADD 1 WSS-IMAX GIVING FRM-IFLD.                              
       LUP-PUT-TAB-DSC.                                                 
           SUBTRACT  1 FROM QUE-NITM.                                   
           MOVE QBR-NOM-TSQBR TO QUE-COLA                               
           MOVE QBR-LEN-TSQBR TO QUE-LITM                               
           MOVE QUE-GET       TO QUE-CMND.                              
           PERFORM GNS-PRO-QUE.                                         
           IF QUE-STAT NOT = QUE-STAT-OKS                               
               IF FRM-IFLD > WSS-IMAX                                   
                   MOVE 'QBRTABNOPRV' TO MSG-COD-MENS                   
                   GO TO FIN-PUT-TAB-DSC                                
               ELSE                                                     
                   GO TO FIN-PUT-TAB-DSC.                               
           SUBTRACT 1 FROM FRM-IFLD.                                    
           MOVE QUE-ITEM TO FRM-NUM-IBRZ-IDX(FRM-IFLD).                 
           IF FRM-IFLD > 1                                              
               GO TO LUP-PUT-TAB-DSC.                                   
       FIN-PUT-TAB-DSC.                                                 
                                                                        
           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1                  
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I                    
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1           
                                 UNTIL WSS-I < 1.                       
                                                                        
           IF FRM-IFLD NOT > WSS-IMAX                                   
               SUBTRACT 1 FROM SCR-NUM-PANT                             
               MOVE 'SGC'        TO MSG-COD-SIST                        
               MOVE 'QBRTABDMAS' TO MSG-COD-MENS.                       
       EXT-PUT-TAB-DSC.                                                 
           EXIT.                                                        
                                                                        
       COPY GNSBGCPT.                                                   
       COPY GNSBGSTR.                                                   
                                                                        
       CLR-FRM SECTION.                                                 
       INI-CLR-FRM.                                                     
           MOVE ZEROES TO FRM-NUM-IBRZ IN QBR-FLD(WSS-I).               
           MOVE SPACES TO FRM-GLS-CALL IN QBR-FLD(WSS-I).               
           MOVE SPACES TO FRM-GLS-NMRO IN QBR-FLD(WSS-I).               
           MOVE SPACES TO FRM-GLS-COMU IN QBR-FLD(WSS-I).               
           MOVE SPACES TO FRM-COD-ROLN IN QBR-FLD(WSS-I).               
           MOVE ZEROES TO FRM-VAL-AFSC IN QBR-FLD(WSS-I).               
           MOVE ZEROES TO FRM-VAL-VCOM IN QBR-FLD(WSS-I).               
           MOVE ZEROES TO FRM-VAL-MSAL IN QBR-FLD(WSS-I).               
       FIN-CLR-FRM.                                                     
           EXIT.                                                        
                                                                        
       CLR-WSS SECTION.                                                 
       INI-CLR-WSS.                                                     
           MOVE ZEROES TO WSS-NUM-CORR-QBR IN WSS-VARI.                 
           MOVE SPACES TO WSS-GLS-CALL-QBR IN WSS-VARI.                 
           MOVE SPACES TO WSS-GLS-NMRO-QBR IN WSS-VARI.                 
           MOVE SPACES TO WSS-GLS-COMU-QBR IN WSS-VARI.                 
           MOVE SPACES TO WSS-COD-ROLN-QBR IN WSS-VARI.                 
           MOVE ZEROES TO WSS-VAL-AFSC-QBR IN WSS-VARI.                 
           MOVE ZEROES TO WSS-VAL-VCOM-QBR IN WSS-VARI.                 
           MOVE ZEROES TO WSS-VAL-MSAL-QBR IN WSS-VARI.                 
           IF SCR-OFRM-SRV                                              
               PERFORM CLR-FRM-MTX.                                     
       FIN-CLR-WSS.                                                     
           EXIT.                                                        
                                                                        
       CLR-FRM-MTX SECTION.                                             
       INI-CLR-FRM-MTX.                                                 
           MOVE ZEROES TO FRM-NUM-IBRZ IN QBR-FLD(1).                   
           MOVE SPACES TO FRM-GLS-CALL IN QBR-FLD(1).                   
           MOVE SPACES TO FRM-GLS-NMRO IN QBR-FLD(1).                   
           MOVE SPACES TO FRM-GLS-COMU IN QBR-FLD(1).                   
           MOVE SPACES TO FRM-COD-ROLN IN QBR-FLD(1).                   
           MOVE ZEROES TO FRM-VAL-AFSC IN QBR-FLD(1).                   
           MOVE ZEROES TO FRM-VAL-VCOM IN QBR-FLD(1).                   
           MOVE ZEROES TO FRM-VAL-MSAL IN QBR-FLD(1).                   
       FIN-CLR-FRM-MTX.                                                 
           EXIT.                                                        
                                                                        
       CRG-VEC-QBR SECTION.                                             
       INI-CRG-VEC-QBR.                                                 
           ADD 1 TO WSS-J.                                              
           IF WSS-J IN WSS-VARI > WSS-JMAX IN WSS-VARI                  
               GO TO FIN-CRG-VEC-QBR.                                   
                                                                        
           PERFORM CLR-QBR-VEC.                                         
                                                                        
      *   correlativo                                                   
           MOVE BRZ-NUM-IBRZ IN BRZ  TO                                 
                            WSS-NUM-CORR-V IN WSS-VARI(WSS-J).          
                                                                        
      *   direccion: calle, numero                                      
           MOVE BRZ-GLS-CALL IN BRZ  TO                                 
                            WSS-GLS-CALL-V IN WSS-VARI(WSS-J).          
           MOVE BRZ-GLS-NMRO IN BRZ  TO                                 
                            WSS-GLS-NMRO-V IN WSS-VARI(WSS-J).          
                                                                        
                                                                        
      *   comuna                                                        
           IF BRZ-COD-COMU IN BRZ = SPACES                              
              MOVE SPACES TO                                            
                            WSS-GLS-COMU-V IN WSS-VARI(WSS-J)           
              GO TO COD-ROLN.                                           
           MOVE 'TAB'                   TO TAB-COD-SIST.                
           MOVE 'COM'                   TO TAB-COD-TTAB IN TAB.         
           MOVE BRZ-COD-COMU IN BRZ     TO TAB-COD-CTAB IN TAB.         
           PERFORM BUS-TAB.                                             
           IF MSG-COD-MENS NOT > SPACES                                 
              MOVE TAB-GLS-DESC TO                                      
                            WSS-GLS-DES1 IN WSS-VARI                    
              MOVE WSS-GLS-DESC TO                                      
                            WSS-GLS-COMU-V IN WSS-VARI(WSS-J)           
           ELSE                                                         
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              MOVE FRM-SUAR-MAL TO FRM-SUAR.                            
      * numero de rol                                                   
       COD-ROLN.                                                        
           MOVE BRZ-COD-ROLN IN BRZ TO                                  
                            WSS-COD-ROLN-V IN WSS-VARI(WSS-J).          
      * avaluo fiscal                                                   
           MOVE BRZ-VAL-AFSC IN BRZ  TO                                 
                            WSS-VAL-AFSC-V IN WSS-VARI(WSS-J).          
      * valor comercial                                                 
           MOVE BRZ-VAL-VCOM IN BRZ  TO                                 
                            WSS-VAL-VCOM-V IN WSS-VARI(WSS-J).          
      * saldo de deuda                                                  
           MOVE BRZ-VAL-MSAL IN BRZ  TO                                 
                            WSS-VAL-MSAL-V IN WSS-VARI(WSS-J).          
       FIN-CRG-VEC-QBR.                                                 
           EXIT.                                                        
                                                                        
       CLR-QBR-VEC SECTION.                                             
       INI-CLR-QBR-VEC.                                                 
           MOVE ZEROES TO WSS-NUM-CORR-V IN WSS-VARI(WSS-J).            
           MOVE SPACES TO WSS-GLS-CALL-V IN WSS-VARI(WSS-J).            
