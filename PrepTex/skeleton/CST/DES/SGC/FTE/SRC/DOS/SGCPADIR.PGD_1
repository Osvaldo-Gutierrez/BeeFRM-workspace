       COPY GNSBVIND.
       COPY GNSBVIDC.
       COPY GNSBVCOD.
       COPY GNSBPVRF.
       COPY GNSBVTAB.
       COPY GNSBVMSC.
       COPY GNSBGFEC.
       COPY GNSBGHOY.
       COPY SGCBGCLI.
