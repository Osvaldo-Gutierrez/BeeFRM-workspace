           EXIT.
       
       VAL-KEY-OPE SECTION.
       INI-VAL-KEY-OPE.
           PERFORM VAL-IKY-OPE.
           PERFORM VAL-MKY-OPE.
           PERFORM VAL-AKY-OPE.
       FIN-VAL-KEY-OPE.
           EXIT.
       
       VAL-IKY-OPE SECTION.
       INI-VAL-IKY-OPE.
           PERFORM VAL-OPE-CAI-IOPE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPE-CAI-IOPE-LEN IN OPE-FLD
                GO TO FIN-VAL-IKY-OPE.
           PERFORM VAL-OPE-IIC-IOPE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPE-IIC-IOPE-LEN IN OPE-FLD
                GO TO FIN-VAL-IKY-OPE.
       FIN-VAL-IKY-OPE.
           EXIT.
       
       VAL-MKY-OPE SECTION.
       INI-VAL-MKY-OPE.
           PERFORM VAL-DBC-NUM-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DBC-NUM-ICLI-LEN IN OPE-FLD
                GO TO FIN-VAL-MKY-OPE.
           PERFORM VAL-DBC-VRF-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DBC-VRF-ICLI-LEN IN OPE-FLD
                GO TO FIN-VAL-MKY-OPE.
           PERFORM VAL-DBC-IND-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DBC-IND-ICLI-LEN IN OPE-FLD
                GO TO FIN-VAL-MKY-OPE.
           PERFORM VAL-DBC-GLS-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DBC-GLS-ICLI-LEN IN OPE-FLD
                GO TO FIN-VAL-MKY-OPE.
       FIN-VAL-MKY-OPE.
           EXIT.
       
       VAL-AKY-OPE SECTION.
       INI-VAL-AKY-OPE.
           PERFORM VAL-RCO-CAI-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RCO-CAI-ICLI-LEN IN OPE-FLD
                GO TO FIN-VAL-AKY-OPE.
           PERFORM VAL-RCO-IIC-ICLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RCO-IIC-ICLI-LEN IN OPE-FLD
                GO TO FIN-VAL-AKY-OPE.
           PERFORM VAL-DBC-GLS-NOMC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DBC-GLS-NOMC-LEN IN OPE-FLD
                GO TO FIN-VAL-AKY-OPE.
       FIN-VAL-AKY-OPE.
           EXIT.
       
       VAL-FLD-OPE SECTION.
       INI-VAL-FLD-OPE.
           PERFORM VAL-OPE-MSC-TVOP.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPE-MSC-TVOP-LEN IN OPE-FLD
                GO TO FIN-VAL-FLD-OPE.
           PERFORM VAL-OPE-COD-TOPR.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPE-COD-TOPR-LEN IN OPE-FLD
                GO TO FIN-VAL-FLD-OPE.
           PERFORM VAL-OPE-COD-OFOP.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPE-COD-OFOP-LEN IN OPE-FLD
                GO TO FIN-VAL-FLD-OPE.
           PERFORM VAL-FRM-GLS-TVOP.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-TVOP-LEN IN OPE-FLD
                GO TO FIN-VAL-FLD-OPE.
           PERFORM VAL-FRM-GLS-TOPR.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-TOPR-LEN IN OPE-FLD
                GO TO FIN-VAL-FLD-OPE.
           PERFORM VAL-FRM-GLS-OFOP.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-OFOP-LEN IN OPE-FLD
                GO TO FIN-VAL-FLD-OPE.
           PERFORM VAL-RCO-NUM-DIRE.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RCO-NUM-DIRE-LEN IN OPE-FLD
                GO TO FIN-VAL-FLD-OPE.
           PERFORM VAL-OPE-COD-EJOP.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPE-COD-EJOP-LEN IN OPE-FLD
                GO TO FIN-VAL-FLD-OPE.
           PERFORM VAL-OPE-IND-MCLI.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPE-IND-MCLI-LEN IN OPE-FLD
                GO TO FIN-VAL-FLD-OPE.
           PERFORM VAL-FRM-GLS-EJOP.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-EJOP-LEN IN OPE-FLD
                GO TO FIN-VAL-FLD-OPE.
           PERFORM VAL-FRM-GLS-MCLI.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-MCLI-LEN IN OPE-FLD
                GO TO FIN-VAL-FLD-OPE.
           PERFORM VAL-OPE-COD-VCOP.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPE-COD-VCOP-LEN IN OPE-FLD
                GO TO FIN-VAL-FLD-OPE.
           PERFORM VAL-OPE-COD-AOPR.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPE-COD-AOPR-LEN IN OPE-FLD
                GO TO FIN-VAL-FLD-OPE.
           PERFORM VAL-FRM-GLS-VCOP.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-VCOP-LEN IN OPE-FLD
                GO TO FIN-VAL-FLD-OPE.
           PERFORM VAL-FRM-GLS-AOPR.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-AOPR-LEN IN OPE-FLD
                GO TO FIN-VAL-FLD-OPE.
           PERFORM VAL-OPE-FEC-FIOP.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPE-FEC-FIOP-LEN IN OPE-FLD
                GO TO FIN-VAL-FLD-OPE.
           PERFORM VAL-OPE-COD-STAT.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPE-COD-STAT-LEN IN OPE-FLD
                GO TO FIN-VAL-FLD-OPE.
           PERFORM VAL-FRM-GLS-STAT.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-STAT-LEN IN OPE-FLD
                GO TO FIN-VAL-FLD-OPE.
           PERFORM VAL-OPE-FEC-FTOP.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPE-FEC-FTOP-LEN IN OPE-FLD
                GO TO FIN-VAL-FLD-OPE.
           PERFORM VAL-OPE-COD-MOTT.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPE-COD-MOTT-LEN IN OPE-FLD
                GO TO FIN-VAL-FLD-OPE.
           PERFORM VAL-OPE-FEC-FROP.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPE-FEC-FROP-LEN IN OPE-FLD
                GO TO FIN-VAL-FLD-OPE.
           PERFORM VAL-FRM-GLS-MOTT.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-MOTT-LEN IN OPE-FLD
                GO TO FIN-VAL-FLD-OPE.
       FIN-VAL-FLD-OPE.
           EXIT.
       
       VAL-OPE-MSC-TVOP SECTION.
       INI-VAL-OPE-MSC-TVOP.
           IF OPE-MSC-TVOP IN OPE-FLD NOT > SPACES
              MOVE SPACES TO OPE-MSC-TVOP IN OPE-FLD.
       FIN-VAL-OPE-MSC-TVOP.
           EXIT.
       
       VAL-OPE-COD-TOPR SECTION.
       INI-VAL-OPE-COD-TOPR.
           IF OPE-COD-TOPR IN OPE-FLD NOT > SPACES
              MOVE SPACES TO OPE-COD-TOPR IN OPE-FLD.
      *VAL-COD Valida codigo de tabla
           MOVE 'SGC' TO TAB-COD-SIST.
           MOVE 'TIO' TO TAB-COD-TTAB IN TAB.
           MOVE OPE-COD-TOPR IN OPE-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              OPE-COD-TOPR IN OPE-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-TOPR IN OPE-FLD
           ELSE
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-TOPR IN OPE-FLD.
       FIN-VAL-OPE-COD-TOPR.
           EXIT.
       
       VAL-OPE-COD-OFOP SECTION.
       INI-VAL-OPE-COD-OFOP.
           IF OPE-COD-OFOP IN OPE-FLD NOT > SPACES
              MOVE SPACES TO OPE-COD-OFOP IN OPE-FLD.
      *VAL-OFI Valida codigo de Oficina en TABLAS
           MOVE OPE-COD-OFOP IN OPE-FLD TO OFI-COD-OFIC IN OFI.
           PERFORM VAL-OFI.
           IF MSG-COD-MENS NOT = SPACES
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
           IF MSG-COD-MENS = 'COD    NEX'
               MOVE SPACES TO FRM-GLS-OFOP IN OPE-FLD
           ELSE
               MOVE OFI-GLS-DESC IN OFI TO FRM-GLS-OFOP IN OPE-FLD.
       FIN-VAL-OPE-COD-OFOP.
           EXIT.
       
       VAL-OPE-CAI-IOPE SECTION.
       INI-VAL-OPE-CAI-IOPE.
           IF OPE-CAI-IOPE IN OPE-FLD NOT > SPACES
              MOVE SPACES TO OPE-CAI-IOPE IN OPE-FLD.
      *<<<< OPE_CAI_IOPE
           IF OPE-CAI-IOPE IN OPE-FLD NOT > SPACES
               MOVE SPACES TO OPE-CAI-IOPE IN OPE-FLD.
      *>>>>
       FIN-VAL-OPE-CAI-IOPE.
           EXIT.
       
       VAL-OPE-IIC-IOPE SECTION.
       INI-VAL-OPE-IIC-IOPE.
       FIN-VAL-OPE-IIC-IOPE.
           EXIT.
       
       VAL-FRM-GLS-TVOP SECTION.
       INI-VAL-FRM-GLS-TVOP.
           IF FRM-GLS-TVOP IN OPE-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-TVOP IN OPE-FLD.
       FIN-VAL-FRM-GLS-TVOP.
           EXIT.
       
       VAL-FRM-GLS-TOPR SECTION.
       INI-VAL-FRM-GLS-TOPR.
           IF FRM-GLS-TOPR IN OPE-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-TOPR IN OPE-FLD.
       FIN-VAL-FRM-GLS-TOPR.
           EXIT.
       
       VAL-FRM-GLS-OFOP SECTION.
       INI-VAL-FRM-GLS-OFOP.
           IF FRM-GLS-OFOP IN OPE-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-OFOP IN OPE-FLD.
       FIN-VAL-FRM-GLS-OFOP.
           EXIT.
       
       VAL-RCO-CAI-ICLI SECTION.
       INI-VAL-RCO-CAI-ICLI.
           IF RCO-CAI-ICLI IN OPE-FLD NOT > SPACES
              MOVE SPACES TO RCO-CAI-ICLI IN OPE-FLD.
       FIN-VAL-RCO-CAI-ICLI.
           EXIT.
       
       VAL-RCO-IIC-ICLI SECTION.
       INI-VAL-RCO-IIC-ICLI.
           IF RCO-IIC-ICLI IN OPE-FLD NOT > SPACES
              MOVE SPACES TO RCO-IIC-ICLI IN OPE-FLD.
       FIN-VAL-RCO-IIC-ICLI.
           EXIT.
       
       VAL-DBC-GLS-NOMC SECTION.
       INI-VAL-DBC-GLS-NOMC.
           IF DBC-GLS-NOMC IN OPE-FLD NOT > SPACES
              MOVE SPACES TO DBC-GLS-NOMC IN OPE-FLD.
       FIN-VAL-DBC-GLS-NOMC.
           EXIT.
       
       VAL-DBC-NUM-ICLI SECTION.
       INI-VAL-DBC-NUM-ICLI.
      *<<<< DBC_NUM_ICLI
           IF DBC-IND-ICLI IN OPE-FLD = 'M'
               MOVE ZEROES TO DBC-NUM-ICLI IN OPE-FLD
               MOVE SPACES TO DBC-VRF-ICLI IN OPE-FLD
                              DBC-GLS-ICLI IN OPE-FLD
                              DBC-GLS-NOMC IN OPE-FLD
               GO TO FIN-VAL-DBC-NUM-ICLI.
      *>>>>
       FIN-VAL-DBC-NUM-ICLI.
           EXIT.
       
       VAL-DBC-VRF-ICLI SECTION.
       INI-VAL-DBC-VRF-ICLI.
           IF DBC-VRF-ICLI IN OPE-FLD NOT > SPACES
              MOVE SPACES TO DBC-VRF-ICLI IN OPE-FLD.
      *VAL-VRF Valida Verificador
           MOVE VRF-VAL TO VRF-CMND.
           MOVE VRF-SVRF-K TO VRF-SVRF.
           MOVE VRF-ZERO-S TO VRF-ZERO.
           MOVE DBC-NUM-ICLI IN OPE-FLD TO VRF-NUME.
           MOVE DBC-VRF-ICLI IN OPE-FLD TO VRF-DVRF.
           MOVE 7 TO VRF-LMAX.
           PERFORM PRO-VRF.
      *<<<< DBC_VRF_ICLI
           IF DBC-IND-ICLI IN OPE-FLD = 'M'
               MOVE ZEROES TO DBC-NUM-ICLI IN OPE-FLD
               MOVE SPACES TO DBC-VRF-ICLI IN OPE-FLD
               MOVE SPACES TO DBC-GLS-ICLI IN OPE-FLD
               MOVE SPACES TO RCO-CAI-ICLI IN OPE-FLD
               MOVE SPACES TO RCO-IIC-ICLI IN OPE-FLD
               MOVE 'SGC'          TO MSG-COD-SIST
               MOVE 'OPE    MUCLI' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO DBC-GLS-NOMC IN OPE-FLD
               GO TO FIN-VAL-DBC-VRF-ICLI.
           IF SCR-INST = SCR-INST-GKY
               MOVE IDC-ZERO-S TO IDC-ZERO
           ELSE
               MOVE IDC-ZERO-N TO IDC-ZERO.
           MOVE DBC-NUM-ICLI IN OPE-FLD TO IDC-NUM-RUTV.
           MOVE DBC-VRF-ICLI IN OPE-FLD TO IDC-VRF-RUTV.
           PERFORM VAL-RUT.
           IF MSG-COD-MENS = SPACES AND
              DBC-NUM-ICLI IN OPE-FLD > ZEROES
               MOVE DBC-NUM-ICLI IN OPE-FLD TO DBC-NUM-ICLI IN DBC
               MOVE DBC-IND-ICLI IN OPE-FLD TO DBC-IND-ICLI IN DBC
               MOVE DBC-GLS-ICLI IN OPE-FLD TO DBC-GLS-ICLI IN DBC
               MOVE 'DBC-IDC-ICLI' TO FIO-AKEY
               MOVE FIO-GET-KEY    TO FIO-CMND
               PERFORM SGC-FIO-DBC
               IF NOT FIO-STAT-OKS
                   MOVE 'SGC'        TO MSG-COD-SIST
                   MOVE 'CLI    NEX' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC IN MSG TO DBC-GLS-NOMC IN OPE-FLD
                   MOVE SPACES       TO RCO-CAI-ICLI IN OPE-FLD
                   MOVE SPACES       TO RCO-IIC-ICLI IN OPE-FLD
                   MOVE ZEROES       TO DBC-NUM-ICLI IN OPE-FLD
                   MOVE SPACES       TO DBC-VRF-ICLI IN OPE-FLD
                   MOVE SPACES       TO DBC-IND-ICLI IN OPE-FLD
                   MOVE SPACES       TO DBC-GLS-ICLI IN OPE-FLD
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
               ELSE
                   MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
                   PERFORM CPT-BLKS
                   MOVE CPT-STRN            TO DBC-GLS-NOMC IN OPE-FLD
                   MOVE DBC-CAI-ICLI IN DBC TO RCO-CAI-ICLI IN OPE-FLD
                   MOVE DBC-IIC-ICLI IN DBC TO RCO-IIC-ICLI IN OPE-FLD
                   MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN OPE-FLD
                   MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN OPE-FLD
                   MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN OPE-FLD
                   MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN OPE-FLD.
      *>>>>
       FIN-VAL-DBC-VRF-ICLI.
           EXIT.
       
       VAL-DBC-IND-ICLI SECTION.
       INI-VAL-DBC-IND-ICLI.
           IF DBC-IND-ICLI IN OPE-FLD NOT > SPACES
              MOVE SPACES TO DBC-IND-ICLI IN OPE-FLD.
      *VAL-IDC Valida identificador
           MOVE DBC-IND-ICLI IN OPE-FLD TO IDC-IND-RUTV.
           PERFORM VAL-IDC.
           MOVE IDC-IND-RUTV TO DBC-IND-ICLI IN OPE-FLD.
      *<<<< DBC_IND_ICLI
           MOVE DBC-IND-ICLI IN OPE-FLD TO IDC-IND-RUTV.
           PERFORM VAL-IDC.
           MOVE IDC-IND-RUTV TO DBC-IND-ICLI IN OPE-FLD.
      *>>>>
       FIN-VAL-DBC-IND-ICLI.
           EXIT.
       
       VAL-DBC-GLS-ICLI SECTION.
       INI-VAL-DBC-GLS-ICLI.
           IF DBC-GLS-ICLI IN OPE-FLD NOT > SPACES
              MOVE SPACES TO DBC-GLS-ICLI IN OPE-FLD.
      *VAL-GLS para IDC valida glosa ( extension ) de identificador
           MOVE DBC-GLS-ICLI IN OPE-FLD TO IDC-GLS-RUTV.
           PERFORM VAL-GLS.
           MOVE IDC-GLS-RUTV TO DBC-GLS-ICLI IN OPE-FLD.
      *<<<< DBC_GLS_ICLI
           MOVE DBC-GLS-ICLI IN OPE-FLD TO IDC-GLS-RUTV.
           PERFORM VAL-GLS.
           MOVE IDC-GLS-RUTV TO DBC-GLS-ICLI IN OPE-FLD.
      *>>>>
       FIN-VAL-DBC-GLS-ICLI.
           EXIT.
       
       VAL-RCO-NUM-DIRE SECTION.
       INI-VAL-RCO-NUM-DIRE.
      *<<<< RCO_NUM_DIRE
           IF DBC-IND-ICLI IN OPE-FLD = 'M'
               MOVE ZEROES TO RCO-NUM-DIRE IN OPE-FLD
               GO TO FIN-VAL-RCO-NUM-DIRE.
      *ALD
           IF RCO-NUM-DIRE IN OPE-FLD NOT > ZEROES
               MOVE RCO-CAI-ICLI IN OPE-FLD TO DBC-CAI-ICLI IN DBC
               MOVE RCO-IIC-ICLI IN OPE-FLD TO DBC-IIC-ICLI IN DBC
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM SGC-FIO-DBC
               IF FIO-STAT-OKS
                   MOVE DBC-NUM-DCOR IN DBC TO RCO-NUM-DIRE IN OPE-FLD.
      *
           MOVE RCO-CAI-ICLI IN OPE-FLD TO RCD-CAI-ICLI IN RCD.
           MOVE RCO-IIC-ICLI IN OPE-FLD TO RCD-IIC-ICLI IN RCD.
           MOVE RCO-NUM-DIRE IN OPE-FLD TO RCD-NUM-DIRE IN RCD.
           MOVE FIO-GET-KEY  TO FIO-CMND.
           PERFORM SGC-FIO-RCD.
           IF NOT FIO-STAT-OKS
               MOVE 'SGC'          TO MSG-COD-SIST
               MOVE 'NUMDIRENEX  ' TO MSG-COD-MENS IN MSG
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>>
       FIN-VAL-RCO-NUM-DIRE.
           EXIT.
       
       VAL-OPE-COD-EJOP SECTION.
       INI-VAL-OPE-COD-EJOP.
           IF OPE-COD-EJOP IN OPE-FLD NOT > SPACES
              MOVE SPACES TO OPE-COD-EJOP IN OPE-FLD.
      *<<<< OPE_COD_EJOP
      *ALD
           IF OPE-COD-EJOP IN OPE-FLD NOT > SPACES
               MOVE RCO-CAI-ICLI IN OPE-FLD TO DBC-CAI-ICLI IN DBC
               MOVE RCO-IIC-ICLI IN OPE-FLD TO DBC-IIC-ICLI IN DBC
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM SGC-FIO-DBC
               IF FIO-STAT-OKS
                   MOVE DBC-COD-EJEC IN DBC TO OPE-COD-EJOP IN OPE-FLD.
      *
      *>>>>
      *VAL-TDU Valida Tipo de Usuario
           MOVE 'EJC'        TO USR-COD-TTAB IN USR.
           MOVE OPE-COD-EJOP IN OPE-FLD TO USR-COD-USER IN USR.
           PERFORM VAL-TDU.
           IF MSG-COD-MENS > SPACES
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
           IF MSG-COD-MENS NOT = 'COD    NEX' AND
              OPE-COD-EJOP IN OPE-FLD > SPACES
               MOVE USR-GLS-DESC IN USR TO FRM-GLS-EJOP IN OPE-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-EJOP IN OPE-FLD.
       FIN-VAL-OPE-COD-EJOP.
           EXIT.
       
       VAL-OPE-IND-MCLI SECTION.
       INI-VAL-OPE-IND-MCLI.
           IF OPE-IND-MCLI IN OPE-FLD NOT > SPACES
              MOVE SPACES TO OPE-IND-MCLI IN OPE-FLD.
      *VAL-COD Valida codigo de tabla
           MOVE 'SGC' TO TAB-COD-SIST.
           MOVE 'MCL' TO TAB-COD-TTAB IN TAB.
           MOVE OPE-IND-MCLI IN OPE-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              OPE-IND-MCLI IN OPE-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-MCLI IN OPE-FLD
           ELSE
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-MCLI IN OPE-FLD.
       FIN-VAL-OPE-IND-MCLI.
           EXIT.
       
       VAL-FRM-GLS-EJOP SECTION.
       INI-VAL-FRM-GLS-EJOP.
           IF FRM-GLS-EJOP IN OPE-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-EJOP IN OPE-FLD.
       FIN-VAL-FRM-GLS-EJOP.
           EXIT.
       
       VAL-FRM-GLS-MCLI SECTION.
       INI-VAL-FRM-GLS-MCLI.
           IF FRM-GLS-MCLI IN OPE-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-MCLI IN OPE-FLD.
       FIN-VAL-FRM-GLS-MCLI.
           EXIT.
       
       VAL-OPE-COD-VCOP SECTION.
       INI-VAL-OPE-COD-VCOP.
           IF OPE-COD-VCOP IN OPE-FLD NOT > SPACES
              MOVE SPACES TO OPE-COD-VCOP IN OPE-FLD.
      *<<<< OPE_COD_VCOP
           MOVE 'TAB'        TO TAB-COD-SIST.
           MOVE 'VLR'        TO TAB-COD-TTAB IN TAB.
           MOVE OPE-COD-VCOP IN OPE-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT = 'COD    NEX' AND
              OPE-COD-VCOP IN OPE-FLD > SPACES
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-VCOP IN OPE-FLD
               MOVE 'TAB'        TO MSC-COD-SIST
               MOVE 'TVAL'       TO MSC-COD-TMSC IN MSC
               MOVE OPE-COD-VCOP IN OPE-FLD TO OPE-COD-VCOP IN OPE
               MOVE OPE-MSC-TVOP IN OPE     TO OPE-MSC-TVOP IN OPE-FLD
               MOVE OPE-MSC-TVOP IN OPE-FLD TO MSC-COD-CMSC IN MSC
               PERFORM BUS-MSC
               MOVE MSC-GLS-DESC IN MSC TO FRM-GLS-TVOP IN OPE-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-VCOP IN OPE-FLD
               MOVE SPACES TO OPE-MSC-TVOP IN OPE-FLD
               MOVE SPACES TO FRM-GLS-TVOP IN OPE-FLD.
           GO TO FIN-VAL-OPE-COD-VCOP.
      *>>>>
      *VAL-COD Valida codigo de tabla
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'VLR' TO TAB-COD-TTAB IN TAB.
           MOVE OPE-COD-VCOP IN OPE-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              OPE-COD-VCOP IN OPE-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-VCOP IN OPE-FLD
           ELSE
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-VCOP IN OPE-FLD.
       FIN-VAL-OPE-COD-VCOP.
           EXIT.
       
       VAL-OPE-COD-AOPR SECTION.
       INI-VAL-OPE-COD-AOPR.
           IF OPE-COD-AOPR IN OPE-FLD NOT > SPACES
              MOVE SPACES TO OPE-COD-AOPR IN OPE-FLD.
      *<<<< OPE_COD_AOPR
           MOVE 'TAB'        TO TAB-COD-SIST.
           MOVE 'AUX'        TO TAB-COD-TTAB IN TAB.
           MOVE OPE-COD-TOPR IN OPE-FLD TO OPE-COD-TOPR IN OPE.
           MOVE OPE-COD-AOPR IN OPE-FLD TO OPE-COD-AOPR IN OPE.
           MOVE OPE-COD-COPR IN OPE     TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT = 'COD    NEX' AND
              OPE-COD-AOPR IN OPE-FLD > SPACES
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-AOPR IN OPE-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-AOPR IN OPE-FLD.
      *>>>>
       FIN-VAL-OPE-COD-AOPR.
           EXIT.
       
       VAL-FRM-GLS-VCOP SECTION.
       INI-VAL-FRM-GLS-VCOP.
           IF FRM-GLS-VCOP IN OPE-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-VCOP IN OPE-FLD.
       FIN-VAL-FRM-GLS-VCOP.
           EXIT.
       
       VAL-FRM-GLS-AOPR SECTION.
       INI-VAL-FRM-GLS-AOPR.
           IF FRM-GLS-AOPR IN OPE-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-AOPR IN OPE-FLD.
       FIN-VAL-FRM-GLS-AOPR.
           EXIT.
       
       VAL-OPE-FEC-FIOP SECTION.
       INI-VAL-OPE-FEC-FIOP.
      *VAL-FEC Validar Fecha
           MOVE OPE-FEC-FIOP IN OPE-FLD TO FEC-FECH.
           MOVE FEC-FORM-FEC TO FEC-FORM.
           MOVE FEC-FBLK-N  TO FEC-FBLK.
           MOVE FEC-CHOY-LE TO FEC-CHOY.
           MOVE FEC-VHBL-S TO FEC-VHBL.
           MOVE FEC-VAL-FEC TO FEC-CMND.
           PERFORM PRO-FEC.
           IF FEC-STAT-OKS
               MOVE FEC-FECH TO OPE-FEC-FIOP IN OPE-FLD
           ELSE
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE FEC-MENS     TO FRM-MENS
               GO TO FIN-VAL-OPE-FEC-FIOP.
       FIN-VAL-OPE-FEC-FIOP.
           EXIT.
       
       VAL-OPE-COD-STAT SECTION.
       INI-VAL-OPE-COD-STAT.
           IF OPE-COD-STAT IN OPE-FLD NOT > SPACES
              MOVE SPACES TO OPE-COD-STAT IN OPE-FLD.
      *VAL-COD Valida codigo de tabla
           MOVE 'SGC' TO TAB-COD-SIST.
           MOVE 'STA' TO TAB-COD-TTAB IN TAB.
           MOVE OPE-COD-STAT IN OPE-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              OPE-COD-STAT IN OPE-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-STAT IN OPE-FLD
           ELSE
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-STAT IN OPE-FLD.
       FIN-VAL-OPE-COD-STAT.
           EXIT.
       
       VAL-FRM-GLS-STAT SECTION.
       INI-VAL-FRM-GLS-STAT.
           IF FRM-GLS-STAT IN OPE-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-STAT IN OPE-FLD.
       FIN-VAL-FRM-GLS-STAT.
           EXIT.
       
       VAL-OPE-FEC-FTOP SECTION.
       INI-VAL-OPE-FEC-FTOP.
      *VAL-FEC Validar Fecha
           MOVE OPE-FEC-FTOP IN OPE-FLD TO FEC-FECH.
           MOVE FEC-FORM-FEC TO FEC-FORM.
           MOVE FEC-FBLK-S  TO FEC-FBLK.
           MOVE FEC-CHOY-LE TO FEC-CHOY.
           MOVE FEC-VHBL-S TO FEC-VHBL.
           MOVE FEC-VAL-FEC TO FEC-CMND.
           PERFORM PRO-FEC.
           IF FEC-STAT-OKS
               MOVE FEC-FECH TO OPE-FEC-FTOP IN OPE-FLD
           ELSE
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE FEC-MENS     TO FRM-MENS
               GO TO FIN-VAL-OPE-FEC-FTOP.
       FIN-VAL-OPE-FEC-FTOP.
           EXIT.
       
       VAL-OPE-COD-MOTT SECTION.
       INI-VAL-OPE-COD-MOTT.
           IF OPE-COD-MOTT IN OPE-FLD NOT > SPACES
              MOVE SPACES TO OPE-COD-MOTT IN OPE-FLD.
      *VAL-COD Valida codigo de tabla
           MOVE 'SGC' TO TAB-COD-SIST.
           MOVE 'MTO' TO TAB-COD-TTAB IN TAB.
           MOVE OPE-COD-MOTT IN OPE-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              OPE-COD-MOTT IN OPE-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-MOTT IN OPE-FLD
           ELSE
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-MOTT IN OPE-FLD.
       FIN-VAL-OPE-COD-MOTT.
           EXIT.
       
       VAL-OPE-FEC-FROP SECTION.
       INI-VAL-OPE-FEC-FROP.
      *VAL-FEC Validar Fecha
           MOVE OPE-FEC-FROP IN OPE-FLD TO FEC-FECH.
           MOVE FEC-FORM-FEC TO FEC-FORM.
           MOVE FEC-FBLK-S  TO FEC-FBLK.
           MOVE FEC-CHOY-LE TO FEC-CHOY.
           MOVE FEC-VHBL-S TO FEC-VHBL.
           MOVE FEC-VAL-FEC TO FEC-CMND.
           PERFORM PRO-FEC.
           IF FEC-STAT-OKS
               MOVE FEC-FECH TO OPE-FEC-FROP IN OPE-FLD
           ELSE
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE FEC-MENS     TO FRM-MENS
               GO TO FIN-VAL-OPE-FEC-FROP.
       FIN-VAL-OPE-FEC-FROP.
           EXIT.
       
       VAL-FRM-GLS-MOTT SECTION.
       INI-VAL-FRM-GLS-MOTT.
           IF FRM-GLS-MOTT IN OPE-FLD NOT > SPACES
              MOVE SPACES TO FRM-GLS-MOTT IN OPE-FLD.
       FIN-VAL-FRM-GLS-MOTT.
           EXIT.
       
       VAL-CON-KEY-OPE SECTION.
       INI-VAL-CON-KEY-OPE.
           IF     OPE-CAI-IOPE IN OPE-FLD NOT > SPACES 
              AND OPE-IIC-IOPE IN OPE-FLD NOT > ZEROES 
              AND RCO-CAI-ICLI IN OPE-FLD NOT > SPACES 
              AND RCO-IIC-ICLI IN OPE-FLD NOT > SPACES 
              AND DBC-GLS-NOMC IN OPE-FLD NOT > SPACES 
              AND DBC-NUM-ICLI IN OPE-FLD NOT > ZEROES 
              AND DBC-VRF-ICLI IN OPE-FLD NOT > SPACES 
              AND DBC-IND-ICLI IN OPE-FLD NOT > SPACES 
              AND DBC-GLS-ICLI IN OPE-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-OPE.
       FIN-VAL-CON-KEY-OPE.
           EXIT.
       
       VAL-CON-IKY-OPE SECTION.
       INI-VAL-CON-IKY-OPE.
           IF     OPE-CAI-IOPE IN OPE-FLD NOT > SPACES 
              AND OPE-IIC-IOPE IN OPE-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-OPE.
       FIN-VAL-CON-IKY-OPE.
           EXIT.
       
       VAL-CON-FLD-OPE SECTION.
       INI-VAL-CON-FLD-OPE.
      *<<<< VAL_CON_FLD_OPE
           IF OPE-FEC-FTOP IN OPE-FLD > ZEROS
               MOVE OPE-FEC-FIOP IN OPE-FLD TO FEC-FEC1
               MOVE OPE-FEC-FTOP IN OPE-FLD TO FEC-FEC2
               MOVE FEC-FORM-FEC TO FEC-FORM
               MOVE FEC-CMP-FEC  TO FEC-CMND
               PERFORM PRO-FEC
               IF FEC-STAT-GRT
                   MOVE 'SGC'          TO MSG-COD-SIST
                   MOVE 'FECFIOP>FTOP' TO MSG-COD-MENS IN MSG
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   GO TO  FIN-VAL-CON-FLD-OPE.
           IF OPE-FEC-FTOP IN OPE-FLD     > ZEROES AND
              OPE-COD-MOTT IN OPE-FLD NOT > SPACES
               MOVE 'SGC'          TO MSG-COD-SIST
               MOVE 'FECFTOPMOTTI' TO MSG-COD-MENS IN MSG
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO  FIN-VAL-CON-FLD-OPE.

           IF OPE-FEC-FROP IN OPE-FLD > ZEROS
               IF OPE-FEC-FTOP IN OPE-FLD NOT > ZEROS
                   MOVE 'SGC'          TO MSG-COD-SIST
                   MOVE 'FECFROPFTOP ' TO MSG-COD-MENS IN MSG
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
               ELSE
                   MOVE OPE-FEC-FTOP IN OPE-FLD TO FEC-FEC1
                   MOVE OPE-FEC-FROP IN OPE-FLD TO FEC-FEC2
                   MOVE FEC-FORM-FEC TO FEC-FORM
                   MOVE FEC-CMP-FEC  TO FEC-CMND
                   PERFORM PRO-FEC
                   IF FEC-STAT-GRT
                       MOVE 'SGC'          TO MSG-COD-SIST
                       MOVE 'FECFTOP>FROP' TO MSG-COD-MENS IN MSG
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>>
       FIN-VAL-CON-FLD-OPE.
           EXIT.
       
       VAL-CON-OPE SECTION.
       INI-VAL-CON-OPE.
       FIN-VAL-CON-OPE.
           EXIT.
       
       VAL-NUM-KEY-OPE SECTION.
       INI-VAL-NUM-KEY-OPE.
           PERFORM VAL-NUM-IKY-OPE.
           PERFORM VAL-NUM-MKY-OPE.
           PERFORM VAL-NUM-AKY-OPE.
       FIN-VAL-NUM-KEY-OPE.
           EXIT.
       
       VAL-NUM-IKY-OPE SECTION.
       INI-VAL-NUM-IKY-OPE.
       FIN-VAL-NUM-IKY-OPE.
           EXIT.
       
       VAL-NUM-MKY-OPE SECTION.
       INI-VAL-NUM-MKY-OPE.
       VAL-NUM-DBC-NUM-ICLI.
           MOVE DBC-NUM-ICLI-ALF IN OPE-FLD TO FRM-ANFL.
           MOVE 8 TO FRM-J.
           MOVE 8 TO FRM-K.
       LU1-VAL-NUM-DBC-NUM-ICLI-0.
           IF FRM-ANFL-CHR( FRM-J ) NOT > SPACES
               SUBTRACT 1 FROM FRM-J
               IF FRM-J > 0
                   GO TO LU1-VAL-NUM-DBC-NUM-ICLI-0.
       LU2-VAL-NUM-DBC-NUM-ICLI-0.
           IF ( FRM-J > 0 ) AND ( FRM-J NOT = 8 )
               MOVE FRM-ANFL-CHR( FRM-J ) TO FRM-ANFL-CHR( FRM-K )
               SUBTRACT 1 FROM FRM-J
               SUBTRACT 1 FROM FRM-K
               GO TO LU2-VAL-NUM-DBC-NUM-ICLI-0.
       LU3-VAL-NUM-DBC-NUM-ICLI-0.
           IF FRM-K > 0 AND FRM-K NOT = 8
               MOVE SPACES TO FRM-ANFL-CHR( FRM-K )
               SUBTRACT 1 FROM FRM-K
               GO TO LU3-VAL-NUM-DBC-NUM-ICLI-0.
           MOVE FRM-ANFL TO DBC-NUM-ICLI-ALF IN OPE-FLD.

           INSPECT DBC-NUM-ICLI-ALF IN OPE-FLD
                   REPLACING LEADING SPACES BY ZEROES.
           IF DBC-NUM-ICLI-ALF IN OPE-FLD IS NOT NUMERIC
               MOVE 'FORMATO NUMERICO INVALIDO' TO FRM-MENS
               MOVE -9 TO DBC-NUM-ICLI-LEN IN OPE-FLD
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
           IF FRM-SUAR = FRM-SUAR-MAL OR 
              FRM-SUAR = FRM-SUAR-CLR
                GO TO FIN-VAL-NUM-MKY-OPE.
       FIN-VAL-NUM-MKY-OPE.
           EXIT.
       
       VAL-NUM-AKY-OPE SECTION.
       INI-VAL-NUM-AKY-OPE.
       FIN-VAL-NUM-AKY-OPE.
           EXIT.
       
       VAL-NUM-FLD-OPE SECTION.
       INI-VAL-NUM-FLD-OPE.
       FIN-VAL-NUM-FLD-OPE.
           EXIT.
