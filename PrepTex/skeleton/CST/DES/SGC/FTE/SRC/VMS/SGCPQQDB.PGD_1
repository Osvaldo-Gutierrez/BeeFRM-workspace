       01  DFHCOMMAREA.
           02 DFH-CMMA OCCURS 3000 PIC X(01).
       COPY GNSLGFIO.

       PROCEDURE DIVISION.
      *===================

       MAIN SECTION.
       INI-MAIN.
      *INI-MFC TUXEDO ORACLE
           PERFORM GNS-PRO-LIMPIAR-VARIABLES.
      *FIN-MFC TUXEDO ORACLE
           PERFORM INI.
       LUP-MAIN.
           IF SCR-INST = SCR-INST-HCM
                 PERFORM SCR-COM.
           IF SCR-CCPP = 'ACC'
                 PERFORM SCR-ACC
           ELSE
                 MOVE SCR-INST-HCM TO SCR-INST.
           GO TO LUP-MAIN.
       FIN-MAIN.
           EXIT.

       INI SECTION.
       INI-INI.
       COPY GNSBGEIB.
       COPY GNSBGEDB.
           MOVE LOW-VALUES TO QDB-FLD.
           MOVE LOW-VALUES TO QDB-EDT.
           PERFORM GNS-PRO-RCK.
           PERFORM GNS-BUS-VCA.
           MOVE +3000 TO SCR-TCMA.
           PERFORM GNS-HDL-SYS.
           PERFORM GNS-HDL-FRM.
           MOVE 'SGCPQQDB' TO FIO-PROG.
           PERFORM GNS-BUS-IDD.
      *PTC-INI
           PERFORM GNS-ERR-QUE.
           PERFORM GET-TSK-TERM.
      *PTC-FIN
      *<<<< INI_FIO                                                     
           IF SCR-OFRM-SRV                                              
                MOVE TSK-TERM-ALF TO QDB-COD-TERM                       
                MOVE TSK-TERM-ALF TO QDB-COD-TERM-MAS                   
           ELSE                                                         
                MOVE EIBTRMID     TO QDB-COD-TERM                       
                MOVE EIBTRMID     TO QDB-COD-TERM-MAS.                  
           MOVE +3859 TO SCR-TCMA.                                      
           PERFORM GNS-ERR-QUE.                                         
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )      
               MOVE SCR-QDB-FLD TO QDB-FLD                              
               GO TO FIN-INI.                                           
                                                                        
           IF ( FRM-FFLD = FRM-FFLD-PF6 )                               
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM        
               ADD 1              TO QUE-NITM                           
               MOVE QDB-NOM-TSQDB TO QUE-COLA                           
               MOVE QDB-LEN-TSQDB TO QUE-LITM                           
               MOVE QUE-GET       TO QUE-CMND                           
               PERFORM GNS-PRO-QUE                                      
               IF QUE-STAT NOT = QUE-STAT-OKS                           
                  MOVE SCR-QDB-FLD TO QDB-FLD                           
                  GO TO FIN-INI.                                        
                                                                        
      **     PERFORM GET-FHOY.                                          
                                                                        
      *>>>>                                                             
       FIN-INI.
           EXIT.

       COPY GNSBHSYS.
       COPY GNSBHFRM.
       COPY GNSBPRCK.
       GNS-BUS-VCA SECTION.
       INI-GNS-BUS-VCA.
           MOVE 0 TO FRM-IFLD.
       LUP-GNS-BUS-VCA.
           ADD 1 TO FRM-IFLD.
           IF FRM-IFLD NOT > EIBCALEN AND
              FRM-IFLD NOT > 3000
                MOVE DFH-CMMA(FRM-IFLD) TO PGM-CMMA-CHR(FRM-IFLD)
                GO TO LUP-GNS-BUS-VCA.
           MOVE PGM-CMMA TO SCR-VARI.
       FIN-GNS-BUS-VCA.
           EXIT.
       COPY GNSBBIDD.

       SCR-COM SECTION.
       INI-SCR-COM.
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-CLRA-QDB.
           PERFORM EDT-KEY-QDB.
           PERFORM EDT-FLD-QDB.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-HCM TO SCR-INST.
           MOVE SCR-CMNP     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       XCT-ERR.
           MOVE 'XCTERR    ' TO MSG-COD-MENS.
           MOVE 'GNS'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO SYS-MEN1.
           MOVE SYS-PROG     TO SYS-MEN2.
           MOVE SYS-ABOR     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       COPY GNSBGGBK.
       FIN-SCR-COM.
           EXIT.



       SCR-ACC SECTION.
       INI-SCR-ACC.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ACC
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-ACC.
           PERFORM DESSCRTRL.
      *<<<< PFS_SCR_ACC                                                 
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6                   
               PERFORM VAL-NUM-KEY-QDB                                  
               PERFORM VAL-NUM-FLD-QDB                                  
               PERFORM SCRL-TAB-QDB                                     
               PERFORM SCR-TRL.                                         
      *>>>>                                                             
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ACC.
           EXIT.

       BRW-SCR-ACC SECTION.
       INI-BRW-SCR-ACC.
       FIN-BRW-SCR-ACC.
           EXIT.




       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               PERFORM SCR-CLRA-QDB
               PERFORM EDT-KEY-QDB
               PERFORM EDT-FLD-QDB
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-QDB
               PERFORM EDT-KEY-QDB
           ELSE
               PERFORM CLR-FLD-QDB
               PERFORM EDT-FLD-QDB.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

       SCR-KEY SECTION.
       INI-SCR-KEY.
           PERFORM SCR-RST.
           PERFORM GET-KEY.
           IF FRM-STAT-ABT
                   MOVE SPACES TO FRM-MENS
                   PERFORM SCR-MSSG
                   GO TO FIN-SCR-KEY.
       FND-SCR-KEY.
           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
           IF FIO-STAT-OKS
               PERFORM PUT-SCR
               MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
               GO TO FIN-SCR-KEY.
      *JSS ELSE
      *        PERFORM CLR-FLD-QDB.
       ERR-SCR-KEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR.
           IF FRM-MENS NOT > SPACES
               IF FIO-STAT-EOF
                   MOVE 'REGEOF    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
               IF FIO-STAT = FIO-STAT-NEX
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
           MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-SCR-KEY.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO QDB-FLD.
           MOVE LOW-VALUES TO QDB-EDT.
       FIN-SCR-RST.
           EXIT.

       GET-KEY SECTION.
       INI-GET-KEY.
           PERFORM SCR-GKEY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-KEY.
           EXIT.

       GET-IKY SECTION.
       INI-GET-IKY.
           PERFORM SCR-GIKY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-IKY.
           EXIT.

       FND-KEY SECTION.
       INI-FND-KEY.
      *<<<< INI_FND_KEY                                                 
           MOVE DBN-CAI-IFOL IN QDB-FLD TO DBN-CAI-IFOL IN DBN.         
           MOVE DBN-IIC-IFOL IN QDB-FLD TO DBN-IIC-IFOL IN DBN.         
      *INI-RFS                                                          
           IF FRM-IND-IDBN IN QDB-FLD = 'S' AND                         
              FRM-NUM-IDBN IN QDB-FLD > ZEROS                           
              MOVE FRM-NUM-IDBN IN QDB-FLD TO DBN-NUM-IDBN IN DBN       
           ELSE                                                         
              MOVE ZEROES TO DBN-NUM-IDBN IN DBN.                       
      *FIN-RFS                                                          
           MOVE ZEROES TO WSS-J IN WSS-VARI.                            
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-LEE-DBN.                                                     
           PERFORM SGC-FIO-DBN.                                         
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF                          
               GO TO CON-FND-KEY.                                       
                                                                        
           IF DBN-CAI-IFOL IN QDB-FLD     NOT = DBN-CAI-IFOL IN DBN OR  
              DBN-IIC-IFOL IN QDB-FLD     NOT = DBN-IIC-IFOL IN DBN     
              GO TO CON-FND-KEY.                                        
                                                                        
           PERFORM CRG-VEC-QDB.                                         
                                                                        
           IF (SCR-OFRM-SRV AND WSS-J > WSS-NITMMAX)                    
               GO TO CON-FND-KEY.                                       
                                                                        
           IF ((NOT SCR-OFRM-SRV) AND WSS-J > WSS-JMAX)                 
               GO TO CON-FND-KEY.                                       
                                                                        
       NXT-LEE-DBN.                                                     
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-LEE-DBN.                                           
      *>>>>                                                             
           PERFORM KEY-ALL-QDB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DBN.
           MOVE FIO-STAT TO PGM-STAT-DBN.
      *<<<< FIN_FND_KEY                                                 
       CON-FND-KEY.                                                     
           PERFORM PUT-TAB.                                             
                                                                        
           MOVE '00' TO FIO-STAT.                                       
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           GO TO FIN-FND-KEY.                                           
      *----------------------------------------------------------       
       MSG-FND-KEY.                                                     
           MOVE 'TAB' TO MSG-COD-SIST.                                  
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MENS.                               
           MOVE '00' TO FIO-STAT.                                       
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           GO TO FIN-FND-KEY.                                           
      *>>>>                                                             
       FIN-FND-KEY.
           IF FIO-STAT = FIO-STAT-NEX
               MOVE 'S' TO SCR-IERR.
       EXT-FND-KEY.
           EXIT.
 
       PUT-SCR SECTION.
       INI-PUT-SCR.
           PERFORM PUT-ALL-QDB.
       FIN-PUT-SCR.
           EXIT.

       STP-EXIT SECTION.
       INI-STP-EXIT.
           MOVE SCR-PROG TO PGM-PROG.
           MOVE SCR-NTRN TO PGM-NTRN.

           MOVE SCR-PROV TO SCR-PROG.
           MOVE SCR-NTRV TO SCR-NTRN.

           MOVE PGM-PROG TO SCR-PROV
           MOVE PGM-NTRN TO SCR-NTRV.
       FIN-STP-EXIT.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO SYS-NTRN.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
      *PTC-INI
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE QDB-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO QDB-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GKEY SECTION.
       INI-SCR-GKEY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-QDB
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO QDB-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GKEY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-QDB.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-QDB
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-QDB.
           PERFORM VAL-IKY-QDB.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-MKY-QDB.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-AKY-QDB.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-CON-KEY-QDB.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GKEY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-QDB.
       ERR-SCR-GKEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-QDB.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GKEY.
           PERFORM SCR-MSSG.

       SCR-GIKY SECTION.
       INI-SCR-GIKY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-QDB
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO QDB-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GIKY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-QDB.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-IKY-QDB
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-QDB.
           PERFORM VAL-IKY-QDB.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GIKY.
           PERFORM VAL-CON-IKY-QDB.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GIKY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-QDB.
       ERR-SCR-GIKY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-IKY-QDB.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GIKY.
           PERFORM SCR-MSSG.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
      *<<<< PFS_GET_CON                                                 
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )           
      *>>>>                                                             
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO QDB-GLS-MEN2
               MOVE SCR-PRMT     TO QDB-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO QDB-FLD.
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO QDB-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE QDB-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO QDB-FLD
      * FLB  7-NOV-1994
               PERFORM CLR-KEY-QDB

               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO QDB-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO QDB-GLS-MENS.
           PERFORM PRO-MKY-QDB.
           PERFORM PRO-AKY-QDB.
           PERFORM UNP-IKY-QDB.
           PERFORM FST-KEY-QDB.
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE QDB-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO QDB-FLD
               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO QDB-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO QDB-GLS-MENS.
           PERFORM UNP-KEY-QDB.
           PERFORM FST-KEY-QDB.
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
           PERFORM PRO-IKY-QDB.
           PERFORM PRO-AKY-QDB.
           PERFORM UNP-MKY-QDB.
           PERFORM UNP-FLD-QDB.
           PERFORM FST-FLD-QDB.
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO QDB-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-QDB.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
      *PTC-INI
           IF SCR-STAT NOT = SCR-STPP
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-QDB
               PERFORM EDT-FLD-QDB
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-QDB
               PERFORM EDT-FLD-QDB
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-QDB.
           PERFORM EDT-FLD-QDB.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

      * PGM-EBR-ALL SECTION.
      * INI-PGM-EBR-ALL.
      *     MOVE 'GNS' TO FIO-SIST.
      *     MOVE FIO-EBR-ALL TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
      * FIN-PGM-EBR-ALL.
      *     EXIT.

       COPY SGCBQQDB.
       COPY GNSBVNUM.
       COPY SGCBFDBN.
       COPY GNSBFMSG.
       COPY GNSBGSYS.
       COPY GNSBGDTC.
      *PTC-INI
      *PTC-SUB
      * ISP  10-MAY-2001 13:46:35 
      * COPY GNSBGFRM.
       COPY GNSBG3CF.
      *PTC-FIN
       COPY GNSBGMSG.
       COPY GNSBEKEY.
       COPY GNSBIABT.
       COPY GNSBFTAB.
       COPY GNSBVIND.
       COPY GNSBVMSC.
       COPY GNSBFMSC.
       COPY GNSBVIDC.
       COPY GNSBPVRF.
      *PTC-INI
       COPY GNSBGQUE.
       COPY GNSBGTSK.
      *PTC-FIN

      *<<<< EOF                                                         
       COPY GNSBBTAB.                                                   
       COPY GNSBVTAB.                                                   
       COPY GNSBVCOD.                                                   
      *COPY GNSBPFEC.                                                   
       COPY SGCBFDBC.                                                   
       COPY SGCBFRPR.                                                   
                                                                        
       PUT-TAB SECTION.                                                 
       INI-PUT-TAB.                                                     
           MOVE QDB-NOM-TSQDB     TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE QDB-NOM-TSQDB-MAS TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE 1 TO SCR-NUM-PANT.                                      
           MOVE LOW-VALUES TO WSS-GLS-NQDB-IDX-MTI.                     
           MOVE LOW-VALUES TO WSS-GLS-NQDB-IDX-FRM.                     
      *                                                                 
           MOVE 1 TO FRM-IFLD.                                          
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.                        
                                                                        
           MOVE 1 TO WSS-F IN WSS-VARI.                                 
       LUP-PUT-TAB.                                                     
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI                  
              GO TO FIN-PUT-TAB.                                        
           IF WSS-NUM-CORR-V(WSS-F) > SPACES                            
              PERFORM PUT-TAB-QDB                                       
              PERFORM PUT-QDB-QUE                                       
              PERFORM SET-FRM-IFLD                                      
           ELSE                                                         
           IF FRM-IFLD = 1 AND WSS-F = 1                                
              MOVE 'TAB' TO MSG-COD-SIST                                
              MOVE 'QDB    NEX' TO MSG-COD-MENS                         
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              MOVE '00' TO FIO-STAT                                     
      *       MOVE 'S' TO SCR-IERR                                      
              GO TO FIN-PUT-TAB                                         
           ELSE                                                         
              MOVE '00' TO FIO-STAT                                     
              GO TO FIN-PUT-TAB.                                        
                                                                        
           ADD 1 TO WSS-F IN WSS-VARI.                                  
           GO TO LUP-PUT-TAB.                                           
       FIN-PUT-TAB.                                                     
           MOVE 1                  TO QUE-NITM.                         
           MOVE QDB-LEN-TSQDB-MAS  TO QUE-LITM.                         
           MOVE QDB-NOM-TSQDB-MAS  TO QUE-COLA.                         
                                                                        
           IF WSS-NITM > WSS-NITMMAX                                    
              MOVE 'S'           TO QDB-MAS-IND-HMAS                    
           ELSE                                                         
              MOVE 'N'           TO QDB-MAS-IND-HMAS.                   
                                                                        
           MOVE WSS-NITM         TO WSS-NUM-ICUR.                       
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.                   
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.                   
           MOVE WSS-NUM-ICUR-ALF TO QDB-MAS-NUM-TOCC.                   
           MOVE QDB-MAS-ITEM     TO QUE-ITEM.                           
           MOVE QUE-PUT          TO QUE-CMND.                           
           PERFORM GNS-PRO-QUE.                                         
           IF WSS-NITM > WSS-IMAX                                       
               MOVE 'SGC' TO MSG-COD-SIST                               
               MOVE 'QDBTABDMAS' TO MSG-COD-MENS                        
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       EXT-PUT-TAB. 
           EXIT.                                                        
                                                                        
       SET-FRM-IFLD SECTION.                                            
       INI-SET-FRM-IFLD.                                                
           IF SCR-OFRM-SRV                                              
                MOVE 1 TO FRM-IFLD                                      
           ELSE                                                         
                ADD 1 TO FRM-IFLD.                                      
       FIN-SET-FRM-IFLD.                                                
           EXIT.                                                        
      *--------------------------------------------                     
       SCRL-TAB-QDB SECTION.                                            
       INI-SCRL-TAB-QDB.                                                
           MOVE SPACES TO MSG-COD-MENS.                                 
                                                                        
           IF FRM-FFLD = FRM-FFLD-PF6                                   
               PERFORM PUT-TAB-ASC                                      
           ELSE                                                         
               PERFORM PUT-TAB-DSC.                                     
           IF MSG-COD-MENS > SPACES                                     
               MOVE 'TAB' TO MSG-COD-SIST                               
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN2.                           
       FIN-SCRL-TAB-QDB.                                                
           EXIT.                                                        
                                                                        
      *--------------------------------------------                     
       PUT-TAB-QDB SECTION.                                             
       INI-PUT-TAB-QDB.                                                 
           PERFORM CLR-WSS.                                             
           MOVE WSS-NUM-CORR-V(WSS-F) TO WSS-NUM-CORR-QDB IN WSS-VARI.  
           MOVE WSS-COD-TIPO-V(WSS-F) TO WSS-COD-TIPO-QDB IN WSS-VARI.  
           MOVE WSS-COD-CBCO-V(WSS-F) TO WSS-COD-CBCO-QDB IN WSS-VARI.  
           MOVE WSS-GLS-CBCO-V(WSS-F) TO WSS-GLS-CBCO-QDB IN WSS-VARI.  
           MOVE WSS-GLS-CCTE-V(WSS-F) TO WSS-GLS-CCTE-QDB IN WSS-VARI.  
           MOVE WSS-COD-TDEU-V(WSS-F) TO WSS-COD-TDEU-QDB IN WSS-VARI.  
           MOVE WSS-GLS-TDEU-V(WSS-F) TO WSS-GLS-TDEU-QDB IN WSS-VARI.  
           MOVE WSS-VAL-MDEU-V(WSS-F) TO WSS-VAL-MDEU-QDB IN WSS-VARI.  
                                                                        
           MOVE WSS-NUM-CORR-QDB IN WSS-VARI TO WSS-NUM-CORR-FRM.       
           MOVE WSS-COD-TIPO-QDB IN WSS-VARI TO WSS-COD-TIPO-FRM.       
           MOVE WSS-COD-CBCO-QDB IN WSS-VARI TO WSS-COD-CBCO-FRM.       
           MOVE WSS-GLS-CBCO-QDB IN WSS-VARI TO WSS-GLS-CBCO-FRM.       
           MOVE WSS-GLS-CCTE-QDB IN WSS-VARI TO WSS-GLS-CCTE-FRM.       
           MOVE WSS-COD-TDEU-QDB IN WSS-VARI TO WSS-COD-TDEU-FRM.       
           MOVE WSS-GLS-TDEU-QDB IN WSS-VARI TO WSS-GLS-TDEU-FRM.       
           MOVE WSS-VAL-MDEU-QDB IN WSS-VARI TO WSS-VAL-MDEU-FRM.       
                                                                        
           IF FRM-IFLD > WSS-IMAX                                       
              GO TO FIN-PUT-TAB-QDB.                                    
                                                                        
           MOVE WSS-NUM-CORR-QDB IN WSS-VARI TO                         
               FRM-NUM-CORR IN QDB-FLD(FRM-IFLD).                       
           MOVE WSS-COD-TIPO-QDB IN WSS-VARI TO                         
               FRM-COD-TIPO IN QDB-FLD(FRM-IFLD).                       
           MOVE WSS-COD-CBCO-QDB IN WSS-VARI TO                         
               FRM-COD-CBCO IN QDB-FLD(FRM-IFLD).                       
           MOVE WSS-GLS-CBCO-QDB IN WSS-VARI TO                         
               FRM-GLS-CBCO IN QDB-FLD(FRM-IFLD).                       
           MOVE WSS-GLS-CCTE-QDB IN WSS-VARI TO                         
               FRM-GLS-CCTE IN QDB-FLD(FRM-IFLD).                       
           MOVE WSS-COD-TDEU-QDB IN WSS-VARI TO                         
               FRM-COD-TDEU IN QDB-FLD(FRM-IFLD).                       
           MOVE WSS-GLS-TDEU-QDB IN WSS-VARI TO                         
               FRM-GLS-TDEU IN QDB-FLD(FRM-IFLD).                       
           MOVE WSS-VAL-MDEU-QDB IN WSS-VARI TO                         
               FRM-VAL-MDEU IN QDB-FLD(FRM-IFLD).                       
       FIN-PUT-TAB-QDB.                                                 
           EXIT.                                                        
      *----------------------------------------------------             
       PUT-QDB-QUE SECTION.                                             
       INI-PUT-QDB-QUE.                                                 
           PERFORM PRO-FLD-QUE.                                         
           ADD 1 TO WSS-NITM.                                           
           MOVE QDB-NOM-TSQDB    TO QUE-COLA.                           
           MOVE QDB-LEN-TSQDB    TO QUE-LITM.                           
           MOVE WSS-NITM         TO QUE-NITM.                           
           MOVE SPACES           TO QUE-ITEM.                           
           IF SCR-OFRM-SRV                                              
              MOVE WSS-GLS-NQDB-IDX-MTI  TO QUE-ITEM                    
           ELSE                                                         
              MOVE WSS-GLS-NQDB-IDX-FRM  TO QUE-ITEM.                   
           MOVE QUE-PUT  TO QUE-CMND.                                   
           PERFORM GNS-PRO-QUE.                                         
       FIN-PUT-QDB-QUE.                                                 
           EXIT.                                                        
      *----------------------------------------------------             
                                                                        
       PUT-TAB-ASC SECTION.                                             
       INI-PUT-TAB-ASC.                                                 
           MOVE SPACES TO MSG-COD-MENS.                                 
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.           
           MOVE 0 TO FRM-IFLD.                                          
       LUP-PUT-TAB-ASC.                                                 
           ADD 1              TO QUE-NITM.                              
           MOVE QDB-NOM-TSQDB TO QUE-COLA.                              
           MOVE QDB-LEN-TSQDB TO QUE-LITM.                              
           MOVE QUE-GET       TO QUE-CMND.                              
           PERFORM GNS-PRO-QUE.                                         
           IF QUE-STAT NOT = QUE-STAT-OKS                               
               IF FRM-IFLD = 0                                          
                   MOVE 'QDBTABNONXT' TO MSG-COD-MENS                   
                   GO TO FIN-PUT-TAB-ASC                                
               ELSE                                                     
                   GO TO FIN-PUT-TAB-ASC.                               
           ADD 1 TO FRM-IFLD.                                           
           MOVE QUE-ITEM TO FRM-NUM-CORR-IDX(FRM-IFLD).                 
           IF FRM-IFLD < WSS-IMAX                                       
               GO TO LUP-PUT-TAB-ASC.                                   
       FIN-PUT-TAB-ASC.                                                 
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX                      
               ADD 1 FRM-IFLD GIVING WSS-I                              
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1            
                                 UNTIL WSS-I > WSS-IMAX.                
           IF FRM-IFLD > 0                                              
               ADD 1 TO SCR-NUM-PANT.                                   
           IF FRM-IFLD = WSS-IMAX                                       
               ADD 1              TO QUE-NITM                           
               MOVE QDB-NOM-TSQDB TO QUE-COLA                           
               MOVE QDB-LEN-TSQDB TO QUE-LITM                           
               MOVE QUE-GET       TO QUE-CMND                           
               PERFORM GNS-PRO-QUE                                      
               IF QUE-STAT = QUE-STAT-OKS                               
                   MOVE 'SGC'        TO MSG-COD-SIST                    
                   MOVE 'QDBTABDMAS' TO MSG-COD-MENS.                   
       EXT-PUT-TAB-ASC.                                                 
           EXIT.                                                        
                                                                        
       PUT-TAB-DSC SECTION.                                             
       INI-PUT-TAB-DSC.                                                 
           MOVE SPACES TO MSG-COD-MENS.                                 
           IF SCR-NUM-PANT = 1                                          
               MOVE 'QDBTABNOPRV' TO MSG-COD-MENS                       
               GO TO EXT-PUT-TAB-DSC.                                   
                                                                        
           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.                
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.               
           ADD 1 TO QUE-NITM.                                           
           ADD 1 WSS-IMAX GIVING FRM-IFLD.                              
       LUP-PUT-TAB-DSC.                                                 
           SUBTRACT  1 FROM QUE-NITM.                                   
           MOVE QDB-NOM-TSQDB TO QUE-COLA                               
           MOVE QDB-LEN-TSQDB TO QUE-LITM                               
           MOVE QUE-GET       TO QUE-CMND.                              
           PERFORM GNS-PRO-QUE.                                         
           IF QUE-STAT NOT = QUE-STAT-OKS                               
               IF FRM-IFLD > WSS-IMAX                                   
                   MOVE 'QDBTABNOPRV' TO MSG-COD-MENS                   
                   GO TO FIN-PUT-TAB-DSC                                
               ELSE                                                     
                   GO TO FIN-PUT-TAB-DSC.                               
           SUBTRACT 1 FROM FRM-IFLD.                                    
           MOVE QUE-ITEM TO FRM-NUM-CORR-IDX(FRM-IFLD).                 
           IF FRM-IFLD > 1                                              
               GO TO LUP-PUT-TAB-DSC.                                   
       FIN-PUT-TAB-DSC.                                                 
                                                                        
           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1                  
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I                    
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1           
                                 UNTIL WSS-I < 1.                       
                                                                        
           IF FRM-IFLD NOT > WSS-IMAX                                   
               SUBTRACT 1 FROM SCR-NUM-PANT                             
               MOVE 'SGC'        TO MSG-COD-SIST                        
               MOVE 'QDBTABDMAS' TO MSG-COD-MENS.                       
       EXT-PUT-TAB-DSC.                                                 
           EXIT.                                                        
                                                                        
       COPY GNSBGCPT.                                                   
       COPY GNSBGSTR.                                                   
                                                                        
       CLR-FRM SECTION.                                                 
       INI-CLR-FRM.                                                     
           MOVE ZEROES TO FRM-NUM-CORR IN QDB-FLD(WSS-I).               
           MOVE SPACES TO FRM-COD-TIPO IN QDB-FLD(WSS-I).               
           MOVE SPACES TO FRM-COD-CBCO IN QDB-FLD(WSS-I).               
           MOVE SPACES TO FRM-GLS-CBCO IN QDB-FLD(WSS-I).               
           MOVE SPACES TO FRM-GLS-CCTE IN QDB-FLD(WSS-I).               
           MOVE SPACES TO FRM-COD-TDEU IN QDB-FLD(WSS-I).               
           MOVE SPACES TO FRM-GLS-TDEU IN QDB-FLD(WSS-I).               
           MOVE ZEROES TO FRM-VAL-MDEU IN QDB-FLD(WSS-I).               
       FIN-CLR-FRM.                                                     
           EXIT.                                                        
                                                                        
       CLR-WSS SECTION.                                                 
       INI-CLR-WSS.                                                     
           MOVE ZEROES TO WSS-NUM-CORR-QDB IN WSS-VARI.                 
           MOVE SPACES TO WSS-COD-TIPO-QDB IN WSS-VARI.                 
           MOVE SPACES TO WSS-COD-CBCO-QDB IN WSS-VARI.                 
           MOVE SPACES TO WSS-GLS-CBCO-QDB IN WSS-VARI.                 
           MOVE SPACES TO WSS-GLS-CCTE-QDB IN WSS-VARI.                 
           MOVE SPACES TO WSS-COD-TDEU-QDB IN WSS-VARI.                 
           MOVE SPACES TO WSS-GLS-TDEU-QDB IN WSS-VARI.                 
           MOVE ZEROES TO WSS-VAL-MDEU-QDB IN WSS-VARI.                 
           IF SCR-OFRM-SRV                                              
               PERFORM CLR-FRM-MTX.                                     
       FIN-CLR-WSS.                                                     
           EXIT.                                                        
                                                                        
       CLR-FRM-MTX SECTION.                                             
       INI-CLR-FRM-MTX.                                                 
           MOVE ZEROES TO FRM-NUM-CORR IN QDB-FLD(1).                   
           MOVE SPACES TO FRM-COD-TIPO IN QDB-FLD(1).                   
           MOVE SPACES TO FRM-COD-CBCO IN QDB-FLD(1).                   
           MOVE SPACES TO FRM-GLS-CBCO IN QDB-FLD(1).                   
           MOVE SPACES TO FRM-GLS-CCTE IN QDB-FLD(1).                   
           MOVE SPACES TO FRM-COD-TDEU IN QDB-FLD(1).                   
           MOVE SPACES TO FRM-GLS-TDEU IN QDB-FLD(1).                   
           MOVE ZEROES TO FRM-VAL-MDEU IN QDB-FLD(1).                   
       FIN-CLR-FRM-MTX.                                                 
           EXIT.                                                        
                                                                        
       CRG-VEC-QDB SECTION.                                             
       INI-CRG-VEC-QDB.                                                 
           ADD 1 TO WSS-J.                                              
           IF WSS-J IN WSS-VARI > WSS-JMAX IN WSS-VARI                  
               GO TO FIN-CRG-VEC-QDB.                                   
                                                                        
           PERFORM CLR-QDB-VEC.                                         
                                                                        
      *   correlativo                                                   
           MOVE DBN-NUM-IDBN IN DBN  TO                                 
                             WSS-NUM-CORR-V IN WSS-VARI(WSS-J).         
      *   tipo banco o casa comerc                                      
           MOVE DBN-COD-TIPO IN DBN  TO                                 
                             WSS-COD-TIPO-V IN WSS-VARI(WSS-J).         
      *   cod banco o casa comerc                                       
           MOVE DBN-COD-CBCO IN DBN  TO                                 
                             WSS-COD-CBCO-V IN WSS-VARI(WSS-J).         
      *  busca glosa cod banco o casa comerc                            
           IF WSS-COD-TIPO-V IN WSS-VARI(WSS-J) = SPACES                
              MOVE SPACES TO WSS-GLS-CBCO-V IN WSS-VARI(WSS-J)          
              GO TO CTA-CTE.                                            
                                                                        
           IF WSS-COD-TIPO-V IN WSS-VARI(WSS-J) = 'BCA'                 
              MOVE 'TAB'                   TO TAB-COD-SIST              
              MOVE 'BCO'                   TO TAB-COD-TTAB IN TAB       
              MOVE WSS-COD-CBCO-V IN WSS-VARI(WSS-J)                    
                                           TO TAB-COD-CTAB IN TAB       
              PERFORM BUS-TAB                                           
              MOVE TAB-GLS-DESC IN TAB TO                               
                                WSS-GLS-CBCO-V IN WSS-VARI(WSS-J)       
           ELSE                                                         
              MOVE 'SGC'                   TO TAB-COD-SIST              
              MOVE 'CCM'                   TO TAB-COD-TTAB IN TAB       
              MOVE WSS-COD-CBCO-V IN WSS-VARI(WSS-J)                    
                                           TO TAB-COD-CTAB IN TAB       
              PERFORM BUS-TAB                                           
              MOVE TAB-GLS-DESC IN TAB TO                               
                                WSS-GLS-CBCO-V IN WSS-VARI(WSS-J).      
                                                                        
       CTA-CTE.                                                         
      *    Mueve cta cte                                                
           MOVE DBN-GLS-CCTE IN DBN  TO                                 
                             WSS-GLS-CCTE-V IN WSS-VARI(WSS-J).         
      *    Mueve tipo deuda                                             
           MOVE DBN-COD-TDEU IN DBN  TO                                 
                             WSS-COD-TDEU-V IN WSS-VARI(WSS-J).         
      *    busca glosa tipo deuda                                       
      *     MOVE 'TAB'                   TO TAB-COD-SIST.               
      *     MOVE '???'                   TO TAB-COD-TTAB IN TAB.        
      *     MOVE WSS-COD-TDEU-V IN WSS-VARI(WSS-J)                      
      *                                  TO TAB-COD-CTAB IN TAB.        
      *     PERFORM BUS-TAB.                                            
      *     IF MSG-COD-MENS IN MSG NOT > SPACES                         
      *        MOVE TAB-GLS-DESC IN TAB TO                              
      *                       WSS-GLS-TDEU-V IN WSS-VARI(WSS-J)         
      *     ELSE                                                        
      *        MOVE SPACES TO WSS-GLS-TDEU-V IN WSS-VARI(WSS-J).        
                                                                        
      *    valor monto deuda                                            
           MOVE DBN-VAL-MDEU IN DBN  TO                                 
                              WSS-VAL-MDEU-V IN WSS-VARI(WSS-J).        
       FIN-CRG-VEC-QDB.                                                 
           EXIT.                                                        
                                                                        
       CLR-QDB-VEC SECTION.                                             
