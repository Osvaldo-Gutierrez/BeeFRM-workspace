       01  PGM-VARI.
           03 PGM-ICLR                  VALUE ' '       PIC X(01).
           03 PGM-LIMPIAR-TODO          VALUE ' '       PIC X(01).
           03 PGM-LIMPIAR-LLAVES        VALUE 'K'       PIC X(01).
           03 PGM-LIMPIAR-CAMPOS        VALUE 'F'       PIC X(01).
           03 PGM-NO-LIMPIAR-NADA       VALUE 'N'       PIC X(01).
           03 PGM-STAT.
              05 PGM-STAT-RCE           VALUE '23'      PIC X(02).
                 88 PGM-STAT-RCE-OKS    VALUE '00'.
                 88 PGM-STAT-RCE-NUL    VALUE '  '.
              05 PGM-STAT-DBC           VALUE '23'      PIC X(02).
                 88 PGM-STAT-DBC-OKS    VALUE '00'.
                 88 PGM-STAT-DBC-NUL    VALUE '  '.
              05 PGM-STAT-CMT           VALUE '23'      PIC X(02).
                 88 PGM-STAT-CMT-OKS    VALUE '00'.
                 88 PGM-STAT-CMT-NUL    VALUE '  '.
           03 PGM-SRNF                  VALUE '23'      PIC X(02).
