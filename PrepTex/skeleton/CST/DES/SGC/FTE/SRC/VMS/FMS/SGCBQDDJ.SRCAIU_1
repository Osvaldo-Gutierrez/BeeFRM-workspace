      *
      * SGCBQDDJ
      * Version 3.0.1 Nov-2005
      * 04-Oct-19 11:37 AM
      *
       SET-TAG-SGCBQDDJ SECTION.
       INI-SET-TAG-SGCBQDDJ.
           MOVE
           '<IDNMOD NOM=$SGCBQDDJ$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$04-Oct-19 11:37 AM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-SGCBQDDJ.
           EXIT.
           
       PUT-ALL-DDJ SECTION.
       INI-PUT-ALL-DDJ.
           PERFORM PUT-DBC-DDJ.
       FIN-PUT-ALL-DDJ.
           EXIT.
       
       PUT-DBC-DDJ SECTION.
       INI-PUT-DBC-DDJ.
      *<<<<< INI_PUT_DBC_DDJ
           GO TO FIN-PUT-DBC-DDJ.
      *>>>>>
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-DDJ.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-DDJ
               GO TO FIN-PUT-DBC-DDJ.
      * IDC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN DDJ-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN DDJ-FLD.
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN DDJ-FLD.
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN DDJ-FLD.
      * IDC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN DDJ-FLD.
       FIN-PUT-DBC-DDJ.
           EXIT.
       
       KEY-ALL-DDJ SECTION.
       INI-KEY-ALL-DDJ.
           PERFORM KEY-DBC-DDJ.
       FIN-KEY-ALL-DDJ.
           EXIT.
       
       KEY-DBC-DDJ SECTION.
       INI-KEY-DBC-DDJ.
       FIN-KEY-DBC-DDJ.
           EXIT.
       
       SCR-CLRA-DDJ SECTION.
       INI-SCR-CLRA-DDJ.
           PERFORM CLR-KEY-DDJ.
           PERFORM CLR-FLD-DDJ.
       FIN-SCR-CLRA-DDJ.
           EXIT.
       
       CLR-KEY-DDJ SECTION.
       INI-CLR-KEY-DDJ.
           MOVE SPACES TO FRM-CAI-IFOL IN DDJ-FLD.
           MOVE SPACES TO FRM-IIC-IFOL IN DDJ-FLD.
       FIN-CLR-KEY-DDJ.
           EXIT.
       
       CLR-FLD-DDJ SECTION.
       INI-CLR-FLD-DDJ.
           MOVE SPACES TO FRM-EST-DDJ IN DDJ-FLD.
           MOVE SPACES TO FRM-GLS-DDJ IN DDJ-FLD.
           MOVE SPACES TO FRM-CAI-IFOA IN DDJ-FLD.
           MOVE SPACES TO FRM-IIC-IFOA IN DDJ-FLD.
           PERFORM CLR-DBC-DDJ.
       FIN-CLR-FLD-DDJ.
           EXIT.
       
       CLR-DBC-DDJ SECTION.
       INI-CLR-DBC-DDJ.
      * IDC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DBC-NUM-ICLI IN DDJ-FLD.
           MOVE SPACES TO DBC-VRF-ICLI IN DDJ-FLD.
           MOVE SPACES TO DBC-IND-ICLI IN DDJ-FLD.
           MOVE SPACES TO DBC-GLS-ICLI IN DDJ-FLD.
      * IDC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO DBC-GLS-NOMC IN DDJ-FLD.
       FIN-CLR-DBC-DDJ.
           EXIT.
       
       FST-KEY-DDJ SECTION.
       INI-FST-KEY-DDJ.
           MOVE -1 TO FRM-CAI-IFOL-LEN IN DDJ-FLD.
       FIN-FST-KEY-DDJ.
           EXIT.
       
       FST-FLD-DDJ SECTION.
       INI-FST-FLD-DDJ.
       FIN-FST-FLD-DDJ.
           EXIT.
       
       PRO-KEY-DDJ SECTION.
       INI-PRO-KEY-DDJ.
           MOVE FRM-CPIM-AEY TO FRM-CAI-IFOL-ATR IN DDJ-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IIC-IFOL-ATR IN DDJ-FLD.
           PERFORM PRO-IKY-DDJ.
           PERFORM PRO-AKY-DDJ.
           PERFORM PRO-MKY-DDJ.
       FIN-PRO-KEY-DDJ.
           EXIT.
       
       PRO-IKY-DDJ SECTION.
       INI-PRO-IKY-DDJ.
       FIN-PRO-IKY-DDJ.
           EXIT.
       
       PRO-MKY-DDJ SECTION.
       INI-PRO-MKY-DDJ.
       FIN-PRO-MKY-DDJ.
           EXIT.
       
       PRO-AKY-DDJ SECTION.
       INI-PRO-AKY-DDJ.
       FIN-PRO-AKY-DDJ.
           EXIT.
       
       UNP-KEY-DDJ SECTION.
       INI-UNP-KEY-DDJ.
           MOVE FRM-CPIM-UBY TO FRM-CAI-IFOL-ATR IN DDJ-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IIC-IFOL-ATR IN DDJ-FLD.
           PERFORM UNP-IKY-DDJ.
           PERFORM UNP-AKY-DDJ.
           PERFORM UNP-MKY-DDJ.
       FIN-UNP-KEY-DDJ.
           EXIT.
       
       UNP-IKY-DDJ SECTION.
       INI-UNP-IKY-DDJ.
           MOVE FRM-CPIM-UBY TO FRM-CAI-IFOL-ATR.
           MOVE FRM-CPIM-UBY TO FRM-IIC-IFOL-ATR.
       FIN-UNP-IKY-DDJ.
           EXIT.
       
       UNP-MKY-DDJ SECTION.
       INI-UNP-MKY-DDJ.
       FIN-UNP-MKY-DDJ.
           EXIT.
       
       UNP-AKY-DDJ SECTION.
       INI-UNP-AKY-DDJ.
       FIN-UNP-AKY-DDJ.
           EXIT.
       
       UNP-FLD-DDJ SECTION.
       INI-UNP-FLD-DDJ.
           PERFORM UNP-DBC-DDJ.
       FIN-UNP-FLD-DDJ.
           EXIT.
       
       UNP-DBC-DDJ SECTION.
       INI-UNP-DBC-DDJ.
       FIN-UNP-DBC-DDJ.
           EXIT.
       
       VAL-KEY-DDJ SECTION.
       INI-VAL-KEY-DDJ.
           PERFORM VAL-IKY-DDJ.
           PERFORM VAL-MKY-DDJ.
           PERFORM VAL-AKY-DDJ.
       FIN-VAL-KEY-DDJ.
           EXIT.
       
       VAL-IKY-DDJ SECTION.
       INI-VAL-IKY-DDJ.
           PERFORM VAL-FRM-CAI-IFOL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-CAI-IFOL-LEN IN DDJ-FLD
                GO TO FIN-VAL-IKY-DDJ.
           PERFORM VAL-FRM-IIC-IFOL.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IIC-IFOL-LEN IN DDJ-FLD
                GO TO FIN-VAL-IKY-DDJ.
       FIN-VAL-IKY-DDJ.
           EXIT.
       
       VAL-MKY-DDJ SECTION.
       INI-VAL-MKY-DDJ.
       FIN-VAL-MKY-DDJ.
           EXIT.
       
       VAL-AKY-DDJ SECTION.
       INI-VAL-AKY-DDJ.
       FIN-VAL-AKY-DDJ.
           EXIT.
       
       VAL-FRM-CAI-IFOL SECTION.
       INI-VAL-FRM-CAI-IFOL.
           IF FRM-CAI-IFOL IN DDJ-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-FRM-CAI-IFOL.
       FIN-VAL-FRM-CAI-IFOL.
           EXIT.
       
       VAL-FRM-IIC-IFOL SECTION.
       INI-VAL-FRM-IIC-IFOL.
           IF FRM-IIC-IFOL IN DDJ-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-FRM-IIC-IFOL.
       FIN-VAL-FRM-IIC-IFOL.
           EXIT.
       
       VAL-CON-KEY-DDJ SECTION.
       INI-VAL-CON-KEY-DDJ.
           IF     FRM-CAI-IFOL IN DDJ-FLD NOT > SPACES 
              AND FRM-IIC-IFOL IN DDJ-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-DDJ.
       FIN-VAL-CON-KEY-DDJ.
           EXIT.
       
       VAL-CON-IKY-DDJ SECTION.
       INI-VAL-CON-IKY-DDJ.
           IF     FRM-CAI-IFOL IN DDJ-FLD NOT > SPACES 
              AND FRM-IIC-IFOL IN DDJ-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-DDJ.
       FIN-VAL-CON-IKY-DDJ.
           EXIT.
       
       VAL-NUM-KEY-DDJ SECTION.
       INI-VAL-NUM-KEY-DDJ.
           PERFORM VAL-NUM-IKY-DDJ.
           PERFORM VAL-NUM-MKY-DDJ.
           PERFORM VAL-NUM-AKY-DDJ.
       FIN-VAL-NUM-KEY-DDJ.
           EXIT.
       
       VAL-NUM-IKY-DDJ SECTION.
       INI-VAL-NUM-IKY-DDJ.
       FIN-VAL-NUM-IKY-DDJ.
           EXIT.
       
       VAL-NUM-MKY-DDJ SECTION.
       INI-VAL-NUM-MKY-DDJ.
       FIN-VAL-NUM-MKY-DDJ.
           EXIT.
       
       VAL-NUM-AKY-DDJ SECTION.
       INI-VAL-NUM-AKY-DDJ.
       FIN-VAL-NUM-AKY-DDJ.
           EXIT.
       
       VAL-NUM-FLD-DDJ SECTION.
       INI-VAL-NUM-FLD-DDJ.
      * IDC-INI --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
       VAL-NUM-DBC-NUM-ICLI.
           MOVE DBC-NUM-ICLI-ALF IN DDJ-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO DBC-NUM-ICLI IN DDJ-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO DBC-NUM-ICLI-LEN IN DDJ-FLD
                GO TO FIN-VAL-NUM-FLD-DDJ.
      * IDC-FIN --> BMCHIBCI010.SRCAIU VALIDACION NUMERICA
       FIN-VAL-NUM-FLD-DDJ.
           EXIT.
       
       EDT-KEY-DDJ SECTION.
       INI-EDT-KEY-DDJ.
           PERFORM EDT-IKY-DDJ.
           PERFORM EDT-MKY-DDJ.
           PERFORM EDT-AKY-DDJ.
       FIN-EDT-KEY-DDJ.
           EXIT.
       
       EDT-IKY-DDJ SECTION.
       INI-EDT-IKY-DDJ.
       FIN-EDT-IKY-DDJ.
           EXIT.
       
       EDT-MKY-DDJ SECTION.
       INI-EDT-MKY-DDJ.
       FIN-EDT-MKY-DDJ.
           EXIT.
       
       EDT-AKY-DDJ SECTION.
       INI-EDT-AKY-DDJ.
       FIN-EDT-AKY-DDJ.
           EXIT.
       
       EDT-FLD-DDJ SECTION.
       INI-EDT-FLD-DDJ.
      * IDC-INI --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF DBC-NUM-ICLI-LEN IN DDJ-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DBC-NUM-ICLI-LEN IN DDJ-FLD
                GO TO FIN-EDT-FLD-DDJ.
           MOVE DBC-NUM-ICLI IN DDJ-FLD TO
                DBC-NUM-ICLI-EDT IN DDJ-EDT.
           MOVE DBC-NUM-ICLI-EDT IN DDJ-EDT TO
                DBC-NUM-ICLI-ALF IN DDJ-FLD.
      * IDC-FIN --> BMCHIBCI011.SRCAIU EDICION NUMERICA
       FIN-EDT-FLD-DDJ.
           EXIT.
