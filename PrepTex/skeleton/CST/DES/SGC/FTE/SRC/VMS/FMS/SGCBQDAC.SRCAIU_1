           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9V TO FRM-VAL-PR06 IN DAC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-VAL-PR06-LEN IN DAC-FLD
                GO TO FIN-VAL-NUM-FLD-DAC.
       VAL-NUM-FRM-VAL-PR07.
           MOVE FRM-VAL-PR07-ALF IN DAC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9V TO FRM-VAL-PR07 IN DAC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-VAL-PR07-LEN IN DAC-FLD
                GO TO FIN-VAL-NUM-FLD-DAC.
       VAL-NUM-FRM-VAL-PR08.
           MOVE FRM-VAL-PR08-ALF IN DAC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9V TO FRM-VAL-PR08 IN DAC-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO FRM-VAL-PR08-LEN IN DAC-FLD
                GO TO FIN-VAL-NUM-FLD-DAC.
       FIN-VAL-NUM-FLD-DAC.
           EXIT.
       
       EDT-KEY-DAC SECTION.
       INI-EDT-KEY-DAC.
           PERFORM EDT-IKY-DAC.
           PERFORM EDT-MKY-DAC.
           PERFORM EDT-AKY-DAC.
       FIN-EDT-KEY-DAC.
           EXIT.
       
       EDT-IKY-DAC SECTION.
       INI-EDT-IKY-DAC.
      * IDC-INI --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF DBC-NUM-ICLI-LEN IN DAC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DBC-NUM-ICLI-LEN IN DAC-FLD
                GO TO FIN-EDT-IKY-DAC.
           MOVE DBC-NUM-ICLI IN DAC-FLD TO
                DBC-NUM-ICLI-EDT IN DAC-EDT.
           MOVE DBC-NUM-ICLI-EDT IN DAC-EDT TO
                DBC-NUM-ICLI-ALF IN DAC-FLD.
      * IDC-FIN --> BMCHIBCI011.SRCAIU EDICION NUMERICA
           IF DAC-IIC-IFOL-LEN IN DAC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DAC-IIC-IFOL-LEN IN DAC-FLD
                GO TO FIN-EDT-IKY-DAC.
           MOVE DAC-IIC-IFOL IN DAC-FLD TO DAC-IIC-IFOL-EDT IN DAC-EDT.
           MOVE DAC-IIC-IFOL-EDT IN DAC-EDT TO 
                DAC-IIC-IFOL-ALF IN DAC-FLD.
       FIN-EDT-IKY-DAC.
           EXIT.
       
       EDT-MKY-DAC SECTION.
       INI-EDT-MKY-DAC.
       FIN-EDT-MKY-DAC.
           EXIT.
       
       EDT-AKY-DAC SECTION.
       INI-EDT-AKY-DAC.
       FIN-EDT-AKY-DAC.
           EXIT.
       
       EDT-FLD-DAC SECTION.
       INI-EDT-FLD-DAC.
           IF FRM-VAL-PR01-LEN IN DAC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-PR01-LEN IN DAC-FLD
                GO TO FIN-EDT-FLD-DAC.
           MOVE FRM-VAL-PR01 IN DAC-FLD TO FRM-VAL-PR01-EDT IN DAC-EDT.
           MOVE FRM-VAL-PR01-EDT IN DAC-EDT TO 
                FRM-VAL-PR01-ALF IN DAC-FLD.
           IF FRM-VAL-PR02-LEN IN DAC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-PR02-LEN IN DAC-FLD
                GO TO FIN-EDT-FLD-DAC.
           MOVE FRM-VAL-PR02 IN DAC-FLD TO FRM-VAL-PR02-EDT IN DAC-EDT.
           MOVE FRM-VAL-PR02-EDT IN DAC-EDT TO 
                FRM-VAL-PR02-ALF IN DAC-FLD.
           IF FRM-VAL-PR03-LEN IN DAC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-PR03-LEN IN DAC-FLD
                GO TO FIN-EDT-FLD-DAC.
           MOVE FRM-VAL-PR03 IN DAC-FLD TO FRM-VAL-PR03-EDT IN DAC-EDT.
           MOVE FRM-VAL-PR03-EDT IN DAC-EDT TO 
                FRM-VAL-PR03-ALF IN DAC-FLD.
           IF FRM-VAL-PR04-LEN IN DAC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-PR04-LEN IN DAC-FLD
                GO TO FIN-EDT-FLD-DAC.
           MOVE FRM-VAL-PR04 IN DAC-FLD TO FRM-VAL-PR04-EDT IN DAC-EDT.
           MOVE FRM-VAL-PR04-EDT IN DAC-EDT TO 
                FRM-VAL-PR04-ALF IN DAC-FLD.
           IF FRM-VAL-PR05-LEN IN DAC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-PR05-LEN IN DAC-FLD
                GO TO FIN-EDT-FLD-DAC.
           MOVE FRM-VAL-PR05 IN DAC-FLD TO FRM-VAL-PR05-EDT IN DAC-EDT.
           MOVE FRM-VAL-PR05-EDT IN DAC-EDT TO 
                FRM-VAL-PR05-ALF IN DAC-FLD.
           IF FRM-VAL-PR06-LEN IN DAC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-PR06-LEN IN DAC-FLD
                GO TO FIN-EDT-FLD-DAC.
           MOVE FRM-VAL-PR06 IN DAC-FLD TO FRM-VAL-PR06-EDT IN DAC-EDT.
           MOVE FRM-VAL-PR06-EDT IN DAC-EDT TO 
                FRM-VAL-PR06-ALF IN DAC-FLD.
           IF FRM-VAL-PR07-LEN IN DAC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-PR07-LEN IN DAC-FLD
                GO TO FIN-EDT-FLD-DAC.
           MOVE FRM-VAL-PR07 IN DAC-FLD TO FRM-VAL-PR07-EDT IN DAC-EDT.
           MOVE FRM-VAL-PR07-EDT IN DAC-EDT TO 
                FRM-VAL-PR07-ALF IN DAC-FLD.
           IF FRM-VAL-PR08-LEN IN DAC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-VAL-PR08-LEN IN DAC-FLD
                GO TO FIN-EDT-FLD-DAC.
