       PROCEDURE DIVISION.
      *===================

       MAIN SECTION.
       INI-MAIN.
      *INI-MFC TUXEDO ORACLE
           PERFORM GNS-PRO-LIMPIAR-VARIABLES.
      *FIN-MFC TUXEDO ORACLE
           PERFORM INI.
       LUP-MAIN.
           IF SCR-INST = SCR-INST-HCM
                 PERFORM SCR-COM.
           IF SCR-CCPP = 'MOD'
                 PERFORM SCR-MOD
           ELSE
           IF SCR-CCPP = 'ING'
                 PERFORM SCR-ING
           ELSE
           IF SCR-CCPP = 'ACC'
                 PERFORM SCR-ACC
           ELSE
           IF SCR-CCPP = 'ELI'
                 PERFORM SCR-ELI
           ELSE
           IF SCR-CCPP = 'ACT'
                 PERFORM SCR-ACT
           ELSE
           IF SCR-CCPP = 'PGM'
                 PERFORM SCR-PGM
           ELSE
                 MOVE SCR-INST-HCM TO SCR-INST.
           GO TO LUP-MAIN.
       FIN-MAIN.
           EXIT.

       INI SECTION.
       INI-INI.
       COPY GNSBGEIB.
       COPY GNSBGEDB.
           MOVE LOW-VALUES TO QTX-FLD.
           MOVE LOW-VALUES TO QTX-EDT.
           PERFORM GNS-PRO-RCK.
           PERFORM GNS-BUS-VCA.
           MOVE +3000 TO SCR-TCMA.
           PERFORM GNS-HDL-SYS.
           PERFORM GNS-HDL-FRM.
           MOVE 'SGCPQQTX' TO FIO-PROG.
           PERFORM GNS-BUS-IDD.
      *PTC-INI
           PERFORM GNS-ERR-QUE.
           PERFORM GET-TSK-TERM.
      *PTC-FIN
      *<<<< INI_FIO
           IF SCR-OFRM-SRV
                MOVE TSK-TERM-ALF TO QTX-COD-TERM
                MOVE TSK-TERM-ALF TO QTX-COD-TERM-MAS
           ELSE
                MOVE EIBTRMID     TO QTX-COD-TERM
                MOVE EIBTRMID     TO QTX-COD-TERM-MAS.

           MOVE +3859 TO SCR-TCMA.
           PERFORM GNS-ERR-QUE.
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )
               MOVE SCR-QTX-FLD TO QTX-FLD
               GO TO FIN-INI.

           IF ( FRM-FFLD = FRM-FFLD-PF6 )
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM
               ADD 1              TO QUE-NITM
               MOVE QTX-NOM-TSQTX TO QUE-COLA
               MOVE QTX-LEN-TSQTX TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT NOT = QUE-STAT-OKS
                  MOVE SCR-QTX-FLD TO QTX-FLD
                  GO TO FIN-INI.
      *>>>>
       FIN-INI.
           EXIT.

       COPY GNSBHSYS.
       COPY GNSBHFRM.
       COPY GNSBPRCK.
       GNS-BUS-VCA SECTION.
       INI-GNS-BUS-VCA.
           MOVE 0 TO FRM-IFLD.
       LUP-GNS-BUS-VCA.
           ADD 1 TO FRM-IFLD.
           IF FRM-IFLD NOT > EIBCALEN AND
              FRM-IFLD NOT > 3000
                MOVE DFH-CMMA(FRM-IFLD) TO PGM-CMMA-CHR(FRM-IFLD)
                GO TO LUP-GNS-BUS-VCA.
           MOVE PGM-CMMA TO SCR-VARI.
       FIN-GNS-BUS-VCA.
           EXIT.
       COPY GNSBBIDD.

       SCR-COM SECTION.
       INI-SCR-COM.
      *<<<< INI_SCR_COM
           MOVE QTX-NOM-TSQTX TO QUE-COLA.
           MOVE QTX-LEN-TSQTX TO QUE-LITM.
           MOVE QUE-DEL       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE QTX-NOM-TSQTX-MAS TO QUE-COLA.
           MOVE QTX-LEN-TSQTX-MAS TO QUE-LITM.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
      *>>>>
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-CLRA-QTX.
           PERFORM EDT-KEY-QTX.
           PERFORM EDT-FLD-QTX.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-HCM TO SCR-INST.
           MOVE SCR-CMNP     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       XCT-ERR.
           MOVE 'XCTERR    ' TO MSG-COD-MENS.
           MOVE 'GNS'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO SYS-MEN1.
           MOVE SYS-PROG     TO SYS-MEN2.
           MOVE SYS-ABOR     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       COPY GNSBGGBK.
       FIN-SCR-COM.
           EXIT.

       SCR-MOD SECTION.
       INI-SCR-MOD.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-MOD
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-MOD.
           PERFORM DESSCRTRL.
       MSG-SCR-MOD.
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-MOD.
           EXIT.

       BRW-SCR-MOD SECTION.
       INI-BRW-SCR-MOD.
       FIN-BRW-SCR-MOD.
           EXIT.

       SCR-ING SECTION.
       INI-SCR-ING.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-IKY
               MOVE SCR-INST-GKY TO SCR-INST
      *PTC-INI
               IF NOT SCR-OFRM-SRV
                   PERFORM SCR-DMSG
               ELSE
                   PERFORM SCR-PUTA
      *PTC-FIN
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-RST
                PERFORM GET-IKY
                IF NOT FRM-STAT-ABT
                        MOVE SPACES TO FRM-MENS
                        PERFORM FND-KEY
                        IF NOT FIO-STAT-OKS AND
                           FIO-STAT NOT = FIO-STAT-NEX
      *JSS                  PERFORM CLR-FLD-QTX
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
                            PERFORM SCR-TRL
                        ELSE
      *PTC-INI
                        MOVE 'S' TO SCR-IERR
      *PTC-FIN
                        MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR
                        IF FRM-MENS NOT > SPACES
                            MOVE 'REGEXI    ' TO MSG-COD-MENS
                            MOVE 'GNS' TO MSG-COD-SIST
                            PERFORM GET-MSG
                            MOVE MSG-GLS-DESC TO FRM-MENS
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-ING
                        ELSE
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-ING
                ELSE
                        MOVE SPACES TO FRM-MENS
                        PERFORM SCR-MSSG
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ING
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON.
           PERFORM DESSCRTRL.
       MSG-SCR-ING.
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ING.
           EXIT.

       SCR-ACC SECTION.
       INI-SCR-ACC.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ACC
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-ACC.
           PERFORM DESSCRTRL.
      *<<<< PFS_SCR_ACC
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM VAL-NUM-KEY-QTX
               PERFORM VAL-NUM-FLD-QTX
               PERFORM SCRL-TAB-QTX
               PERFORM SCR-TRL.
      *>>>>
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ACC.
           EXIT.

       BRW-SCR-ACC SECTION.
       INI-BRW-SCR-ACC.
       FIN-BRW-SCR-ACC.
           EXIT.

       SCR-ELI SECTION.
       INI-SCR-ELI.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ELI
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-ELI.
           PERFORM DESSCRTRL.
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ELI.
           EXIT.

       BRW-SCR-ELI SECTION.
       INI-BRW-SCR-ELI.
       FIN-BRW-SCR-ELI.
           EXIT.

       SCR-ACT SECTION.
       INI-SCR-ACT.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-RST
                PERFORM GET-KEY
                IF NOT FRM-STAT-ABT
                        MOVE SPACES TO FRM-MENS
                        PERFORM FND-KEY
                        IF FIO-STAT-OKS
                            MOVE SCR-ACTU-MOD TO SCR-ACTU
                            PERFORM PUT-SCR
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
                            PERFORM SCR-TRL
                        ELSE
                        MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR
                        IF FIO-STAT-EOF
                            IF FRM-MENS NOT > SPACES
                                MOVE 'REGEOF    ' TO MSG-COD-MENS
                                MOVE 'GNS' TO MSG-COD-SIST
                                PERFORM GET-MSG
                                MOVE MSG-GLS-DESC TO FRM-MENS
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                            ELSE
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                        ELSE
                        IF FIO-STAT = FIO-STAT-NEX
                            IF FRM-MENS NOT > SPACES
                                MOVE 'REGNEX    ' TO MSG-COD-MENS
                                MOVE 'GNS' TO MSG-COD-SIST
                                PERFORM GET-MSG
                                MOVE MSG-GLS-DESC TO FRM-MENS
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                            ELSE
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                        ELSE
                            MOVE SCR-ACTU-ING TO SCR-ACTU
      *JSS                  PERFORM CLR-FLD-QTX
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
                            PERFORM SCR-TRL
                ELSE
                        MOVE SPACES TO FRM-MENS
                        PERFORM SCR-MSSG
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ACT
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-ACT.
           PERFORM DESSCRTRL.
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ACT.
           EXIT.

       BRW-SCR-ACT SECTION.
       INI-BRW-SCR-ACT.
       FIN-BRW-SCR-ACT.
           EXIT.

       SCR-PGM SECTION.
       INI-SCR-PGM.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-RST
                PERFORM GET-KEY
                IF FRM-STAT-ABT
                    MOVE SPACES TO FRM-MENS
                    PERFORM SCR-MSSG
                    PERFORM DESSCRTRL
                    GO TO FIN-SCR-PGM
                ELSE
                    MOVE SPACES TO FRM-MENS
                    PERFORM FND-KEY
                    IF FIO-STAT = FIO-STAT-NEX
                        IF FRM-MENS NOT > SPACES
                            MOVE 'REGNEX    ' TO MSG-COD-MENS
                            MOVE 'GNS' TO MSG-COD-SIST
                            PERFORM GET-MSG
                            MOVE MSG-GLS-DESC TO FRM-MENS
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-PGM
                        ELSE
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-PGM
                    ELSE
                        MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
                        PERFORM SCR-TRL
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-PGM.
       TRL-SCR-PGM.
           PERFORM DESSCRTRL.
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-PGM.
           EXIT.

       BRW-SCR-PGM SECTION.
       INI-BRW-SCR-PGM.
       FIN-BRW-SCR-PGM.
           EXIT.

       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               PERFORM SCR-CLRA-QTX
               PERFORM EDT-KEY-QTX
               PERFORM EDT-FLD-QTX
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-QTX
               PERFORM EDT-KEY-QTX
           ELSE
               PERFORM CLR-FLD-QTX
               PERFORM EDT-FLD-QTX.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

       SCR-KEY SECTION.
       INI-SCR-KEY.
           PERFORM SCR-RST.
           PERFORM GET-KEY.
           IF FRM-STAT-ABT
                   MOVE SPACES TO FRM-MENS
                   PERFORM SCR-MSSG
                   GO TO FIN-SCR-KEY.
       FND-SCR-KEY.
           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
           IF FIO-STAT-OKS
               PERFORM PUT-SCR
               MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
               GO TO FIN-SCR-KEY.
      *JSS ELSE
      *        PERFORM CLR-FLD-QTX.
       ERR-SCR-KEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR.
           IF FRM-MENS NOT > SPACES
               IF FIO-STAT-EOF
                   MOVE 'REGEOF    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
               IF FIO-STAT = FIO-STAT-NEX
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
           MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-SCR-KEY.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO QTX-FLD.
           MOVE LOW-VALUES TO QTX-EDT.
       FIN-SCR-RST.
           EXIT.

       GET-KEY SECTION.
       INI-GET-KEY.
           PERFORM SCR-GKEY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-KEY.
           EXIT.

       GET-IKY SECTION.
       INI-GET-IKY.
           PERFORM SCR-GIKY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-IKY.
           EXIT.

       FND-KEY SECTION.
       INI-FND-KEY.
      *<<<< INI_FND_KEY
           MOVE DBC-NUM-ICLI IN QTX-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN QTX-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN QTX-FLD TO DBC-GLS-ICLI IN DBC.
           MOVE 'DBC-IDC-ICLI' TO FIO-AKEY.
           MOVE FIO-GET-KEY    TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           MOVE FIO-STAT TO PGM-STAT-DBC.
           IF NOT FIO-STAT-OKS
               MOVE 'SGC'        TO MSG-COD-SIST
               MOVE 'CLI    NEX' TO MSG-COD-MENS
               GO TO MSG-FND-KEY.

           MOVE DBC-CIC-ICLI IN DBC TO WSS-CIC-ICLI IN WSS-VARI.
           GO TO CON-FND-KEY.
      *>>>>
           PERFORM KEY-ALL-QTX.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-TXO.
           IF FIO-STAT-OKS
               MOVE TXO-STP-ITRN IN TXO TO SCR-UMOD.
           MOVE FIO-STAT TO PGM-STAT-TXO.
     *<<<<<FIN_FND_KEY
       CON-FND-KEY.
           PERFORM LEE-ALL-TXO.
           IF MSG-COD-MENS > SPACES
              MOVE 'S' TO SCR-IERR
              GO TO MSG-FND-KEY.

           PERFORM PUT-TAB.

           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *----------------------------------------------------------
       MSG-FND-KEY.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE '00'         TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *>>>>>
       FIN-FND-KEY.
           IF FIO-STAT = FIO-STAT-NEX
               MOVE 'S' TO SCR-IERR.
       EXT-FND-KEY.
           EXIT.
 
       PUT-SCR SECTION.
       INI-PUT-SCR.
           IF PGM-STAT-TXO-OKS
               MOVE TXO-STP-ITRN IN TXO TO SCR-UMOD.
 *<<<< PUT_SCR
           GO TO FIN-PUT-SCR.
      *>>>>
           PERFORM PUT-ALL-QTX.
       FIN-PUT-SCR.
           EXIT.

       STP-EXIT SECTION.
       INI-STP-EXIT.
           MOVE SCR-PROG TO PGM-PROG.
           MOVE SCR-NTRN TO PGM-NTRN.

           MOVE SCR-PROV TO SCR-PROG.
           MOVE SCR-NTRV TO SCR-NTRN.

           MOVE PGM-PROG TO SCR-PROV
           MOVE PGM-NTRN TO SCR-NTRV.
       FIN-STP-EXIT.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO SYS-NTRN.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
      *PTC-INI
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE QTX-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO QTX-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GKEY SECTION.
       INI-SCR-GKEY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-QTX
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO QTX-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GKEY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-QTX.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-QTX
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-QTX.
           PERFORM VAL-IKY-QTX.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-MKY-QTX.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-AKY-QTX.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-CON-KEY-QTX.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GKEY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-QTX.
       ERR-SCR-GKEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-QTX.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GKEY.
           PERFORM SCR-MSSG.

       SCR-GIKY SECTION.
       INI-SCR-GIKY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-QTX
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO QTX-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GIKY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-QTX.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-IKY-QTX
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-QTX.
           PERFORM VAL-IKY-QTX.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GIKY.
           PERFORM VAL-CON-IKY-QTX.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GIKY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-QTX.
       ERR-SCR-GIKY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-IKY-QTX.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GIKY.
           PERFORM SCR-MSSG.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
      *>>>>
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO QTX-GLS-MEN2
               MOVE SCR-PRMT     TO QTX-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO QTX-FLD.
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO QTX-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE QTX-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO QTX-FLD
      * FLB  7-NOV-1994
               PERFORM CLR-KEY-QTX

               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO QTX-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO QTX-GLS-MENS.
           PERFORM PRO-MKY-QTX.
           PERFORM PRO-AKY-QTX.
           PERFORM UNP-IKY-QTX.
           PERFORM FST-KEY-QTX.
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE QTX-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO QTX-FLD
               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO QTX-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO QTX-GLS-MENS.
           PERFORM UNP-KEY-QTX.
           PERFORM FST-KEY-QTX.
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
           PERFORM PRO-IKY-QTX.
           PERFORM PRO-AKY-QTX.
           PERFORM UNP-MKY-QTX.
           PERFORM UNP-FLD-QTX.
           PERFORM FST-FLD-QTX.
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO QTX-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-QTX.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
      *PTC-INI
           IF SCR-STAT NOT = SCR-STPP
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-QTX
               PERFORM EDT-FLD-QTX
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-QTX
               PERFORM EDT-FLD-QTX
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-QTX.
           PERFORM EDT-FLD-QTX.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

      * PGM-EBR-ALL SECTION.
      * INI-PGM-EBR-ALL.
      *     MOVE 'GNS' TO FIO-SIST.
      *     MOVE FIO-EBR-ALL TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
      * FIN-PGM-EBR-ALL.
      *     EXIT.

       COPY SGCBQQTX.
       COPY GNSBVNUM.
       COPY SGCBFTXO.
       COPY GNSBFMSG.
       COPY GNSBGSYS.
       COPY GNSBGDTC.
      *PTC-INI
      *PTC-SUB
      * ISP  10-MAY-2001 13:46:35 
      * COPY GNSBGFRM.
       COPY GNSBG3CF.
      *PTC-FIN
       COPY GNSBGMSG.
       COPY GNSBEKEY.
       COPY GNSBIABT.
       COPY GNSBFTAB.
       COPY GNSBVIND.
       COPY GNSBVMSC.
       COPY GNSBFMSC.
       COPY GNSBVIDC.
       COPY GNSBPVRF.
       COPY GNSBGFEC.
       COPY GNSBGHOY.
      *PTC-INI
       COPY GNSBGQUE.
       COPY GNSBGTSK.
      *PTC-FIN

      *<<<< EOF                                                   
       COPY SGCBFDBC.
       LEE-ALL-TXO SECTION.
       INI-LEE-ALL-TXO.
           MOVE 'N'    TO WSS-IND-STXO IN WSS-VARI.
           MOVE SPACES TO MSG-COD-MENS.

      *WSS-FEC-FIND
           MOVE FRM-NUM-DIND IN QTX-FLD  TO WSS-NUM-DIND IN WSS-VARI.
           MOVE FRM-NUM-MIND IN QTX-FLD  TO WSS-NUM-MIND IN WSS-VARI.
           MOVE FRM-NUM-SIND IN QTX-FLD  TO WSS-NUM-SIND IN WSS-VARI.
           MOVE FRM-NUM-AIND IN QTX-FLD  TO WSS-NUM-AIND IN WSS-VARI.
      *WSS-FEC-FINH
           MOVE FRM-NUM-DINH IN QTX-FLD  TO WSS-NUM-DINH IN WSS-VARI.
           MOVE FRM-NUM-MINH IN QTX-FLD  TO WSS-NUM-MINH IN WSS-VARI.
           MOVE FRM-NUM-SINH IN QTX-FLD  TO WSS-NUM-SINH IN WSS-VARI.
           MOVE FRM-NUM-AINH IN QTX-FLD  TO WSS-NUM-AINH IN WSS-VARI.
      *WSS-FEC-FFID
           MOVE FRM-NUM-DFID IN QTX-FLD  TO WSS-NUM-DFID IN WSS-VARI.
           MOVE FRM-NUM-MFID IN QTX-FLD  TO WSS-NUM-MFID IN WSS-VARI.
           MOVE FRM-NUM-SFID IN QTX-FLD  TO WSS-NUM-SFID IN WSS-VARI.
           MOVE FRM-NUM-AFID IN QTX-FLD  TO WSS-NUM-AFID IN WSS-VARI.
      *WSS-FEC-FFIH
           MOVE FRM-NUM-DFIH IN QTX-FLD  TO WSS-NUM-DFIH IN WSS-VARI.
           MOVE FRM-NUM-MFIH IN QTX-FLD  TO WSS-NUM-MFIH IN WSS-VARI.
           MOVE FRM-NUM-SFIH IN QTX-FLD  TO WSS-NUM-SFIH IN WSS-VARI.
           MOVE FRM-NUM-AFIH IN QTX-FLD  TO WSS-NUM-AFIH IN WSS-VARI.
      *WSS-FEC-FKIN.
           MOVE FRM-NUM-DKIN IN QTX-FLD  TO WSS-NUM-DKIN IN WSS-VARI.
           MOVE FRM-NUM-MKIN IN QTX-FLD  TO WSS-NUM-MKIN IN WSS-VARI.
           MOVE FRM-NUM-SKIN IN QTX-FLD  TO WSS-NUM-SKIN IN WSS-VARI.
           MOVE FRM-NUM-AKIN IN QTX-FLD  TO WSS-NUM-AKIN IN WSS-VARI.
      *----------------------------------------------------------------*
      *ANALIZA CONSULTA DE SIGUIENTES 50
           IF FRM-IND-NXPR IN QTX-FLD = 'S' OR 'N'
              MOVE WSS-CIC-ICLI IN WSS-VARI TO TXO-CIC-ICLI IN TXO
              MOVE FRM-CIC-KOPE IN QTX-FLD  TO TXO-CIC-IOPE IN TXO
              MOVE WSS-FEC-FKIN IN WSS-VARI TO TXO-FEC-FINI IN TXO
              MOVE FRM-COD-KCPT IN QTX-FLD  TO TXO-COD-CPTO IN TXO
              MOVE FRM-NUM-KTXO IN QTX-FLD  TO TXO-NUM-ITXO IN TXO
           ELSE
              MOVE WSS-CIC-ICLI IN WSS-VARI TO TXO-CIC-ICLI IN TXO
              MOVE FRM-CAI-IOPE IN QTX-FLD  TO TXO-CAI-IOPE IN TXO
              MOVE FRM-IIC-IOPE IN QTX-FLD  TO TXO-IIC-IOPE IN TXO
              MOVE WSS-FEC-FIND IN WSS-VARI TO TXO-FEC-FINI IN TXO
              MOVE FRM-COD-CPTO IN QTX-FLD  TO TXO-COD-CPTO IN TXO
              MOVE 1                        TO TXO-NUM-ITXO IN TXO.
      *----------------------------------------------------------------*
      *
           MOVE ZEROES              TO WSS-J IN WSS-VARI.
           MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM SGC-FIO-TXO.
       LUP-LEE-ALL-TXO.
           MOVE FIO-STAT TO PGM-STAT-TXO.
           IF NOT FIO-STAT-OKS OR FIO-STAT-EOF
              GO TO EXT-LEE-ALL-TXO.

           IF WSS-CIC-ICLI IN WSS-VARI > SPACES AND
              TXO-CIC-ICLI IN TXO NOT = WSS-CIC-ICLI IN WSS-VARI
      *MHM-INI 07-SEP-2016
      *       GO TO NXT-LEE-ALL-TXO.
              GO TO EXT-LEE-ALL-TXO.
      *MHM-FIN 07-SEP-2016

           IF FRM-CAI-IOPE IN QTX-FLD > SPACES AND
              TXO-CAI-IOPE IN TXO NOT = FRM-CAI-IOPE IN QTX-FLD
              GO TO NXT-LEE-ALL-TXO.

           IF FRM-IIC-IOPE IN QTX-FLD > SPACES AND
              TXO-IIC-IOPE IN TXO NOT = FRM-IIC-IOPE IN QTX-FLD
              GO TO NXT-LEE-ALL-TXO.

           IF WSS-FEC-FIND IN WSS-VARI > ZEROES AND
              TXO-FEC-FINI IN TXO < WSS-FEC-FIND IN WSS-VARI
              GO TO NXT-LEE-ALL-TXO.

           IF WSS-FEC-FINH IN WSS-VARI > ZEROES AND
              TXO-FEC-FINI IN TXO > WSS-FEC-FINH IN WSS-VARI
              GO TO NXT-LEE-ALL-TXO.

           IF WSS-FEC-FFID IN WSS-VARI > ZEROES AND
              TXO-FEC-FINI IN TXO < WSS-FEC-FFID IN WSS-VARI
              GO TO NXT-LEE-ALL-TXO.

           IF WSS-FEC-FFIH IN WSS-VARI > ZEROES AND
              TXO-FEC-FINI IN TXO > WSS-FEC-FFIH IN WSS-VARI
              GO TO NXT-LEE-ALL-TXO.

           IF FRM-COD-CPTO IN QTX-FLD > SPACES AND
              TXO-COD-CPTO IN TXO NOT = FRM-COD-CPTO IN QTX-FLD
              GO TO NXT-LEE-ALL-TXO.

           IF FRM-MSC-STAT IN QTX-FLD > SPACES AND
              TXO-MSC-STAT IN TXO NOT = FRM-MSC-STAT IN QTX-FLD
              GO TO NXT-LEE-ALL-TXO.

      *CARGA TXO SELECCIONADA
           MOVE 'S' TO WSS-IND-STXO IN WSS-VARI.
           PERFORM CRG-VEC-TXO.
           IF (SCR-OFRM-SRV AND WSS-J > WSS-NITMMAX)
              GO TO EXT-LEE-ALL-TXO.
           IF ((NOT SCR-OFRM-SRV) AND WSS-J > WSS-JMAX)
              GO TO EXT-LEE-ALL-TXO.
           MOVE SPACES TO MSG-COD-MENS.
       NXT-LEE-ALL-TXO.
           MOVE SPACES TO MSG-COD-MENS.
           MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM SGC-FIO-TXO.
           GO TO LUP-LEE-ALL-TXO.
       EXT-LEE-ALL-TXO.
           MOVE PGM-STAT-TXO TO FIO-STAT.
           IF FIO-STAT-OKS
              MOVE FIO-END-BRW TO FIO-CMND
              PERFORM SGC-FIO-TXO.
           MOVE PGM-STAT-TXO TO FIO-STAT.
           IF WSS-IND-STXO IN WSS-VARI= 'N'
              MOVE 'SGC'        TO MSG-COD-SIST
              MOVE 'TXO    NEX' TO MSG-COD-MENS.
       FIN-LEE-ALL-TXO.
           EXIT.

       PUT-TAB SECTION.
       INI-PUT-TAB.
           MOVE QTX-NOM-TSQTX TO QUE-COLA.
           MOVE QTX-LEN-TSQTX TO QUE-LITM.
           MOVE QUE-DEL       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           MOVE 1          TO SCR-NUM-PANT.
           MOVE LOW-VALUES TO WSS-NUM-ICLI-IDX-MTI.
           MOVE LOW-VALUES TO WSS-NUM-ICLI-IDX-FLD.
      *
           MOVE 1          TO FRM-IFLD.
           MOVE ZEROES     TO WSS-NITM IN WSS-VARI.

           MOVE 1 TO WSS-F IN WSS-VARI.
       LUP-PUT-TAB.
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI
              GO TO FIN-PUT-TAB.
           IF VEC-COD-DCPT(WSS-F) > SPACES
              PERFORM PUT-TAB-QTX
              PERFORM PUT-QTX-QUE
              PERFORM SET-FRM-IFLD
           ELSE
              IF FRM-IFLD = 1 AND WSS-F = 1
                 GO TO MSG-PUT-TAB
              ELSE
                 MOVE '00' TO FIO-STAT
                 GO TO FIN-PUT-TAB.

           ADD 1 TO WSS-F IN WSS-VARI.
           GO TO LUP-PUT-TAB.
       MSG-PUT-TAB.
           MOVE 'SGC' TO MSG-COD-SIST.
           MOVE 'TXO    NEX' TO MSG-COD-MENS.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE '00' TO FIO-STAT.
      *    MOVE 'S' TO SCR-IERR.

       FIN-PUT-TAB.
           IF NOT SCR-OFRM-SRV
              GO TO EXT-MSG-PUT-TAB.
           MOVE 1                  TO QUE-NITM.
           MOVE QTX-LEN-TSQTX-MAS  TO QUE-LITM.
           MOVE QTX-NOM-TSQTX-MAS  TO QUE-COLA.

           IF WSS-NITM > WSS-NITMMAX
              MOVE 'S'           TO QTX-MAS-IND-HMAS
           ELSE
              MOVE 'N'           TO QTX-MAS-IND-HMAS.

           MOVE WSS-NITM         TO WSS-NUM-ICUR.
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.
           MOVE WSS-NUM-ICUR-ALF TO QTX-MAS-NUM-TOCC.
           MOVE QTX-MAS-ITEM     TO QUE-ITEM.
           MOVE QUE-PUT          TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

       EXT-MSG-PUT-TAB.
           IF WSS-NITM > WSS-IMAX
               MOVE 'SGC' TO MSG-COD-SIST
               MOVE 'QTX   DMAS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       EXT-PUT-TAB.
           EXIT.

       SET-FRM-IFLD SECTION.
       INI-SET-FRM-IFLD.
           IF SCR-OFRM-SRV
                MOVE 1 TO FRM-IFLD
           ELSE
                ADD 1  TO FRM-IFLD.
       FIN-SET-FRM-IFLD.
           EXIT.

      *--------------------------------------------
       SCRL-TAB-QTX SECTION.
       INI-SCRL-TAB-QTX.
           MOVE SPACES TO MSG-COD-MENS.

           IF FRM-FFLD = FRM-FFLD-PF6
               PERFORM PUT-TAB-ASC
           ELSE
               PERFORM PUT-TAB-DSC.
           IF MSG-COD-MENS > SPACES
               MOVE 'SGC' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-QTX.
           EXIT.

      *--------------------------------------------
       PUT-TAB-QTX SECTION.
       INI-PUT-TAB-QTX.
           PERFORM CLR-WSS.

           MOVE VEC-CIC-IOPE(WSS-F) TO MTI-CIC-IOPE IN WSS-VARI-QTX.
           MOVE VEC-NUM-DINI(WSS-F) TO MTI-NUM-DINI IN WSS-VARI-QTX.
           MOVE VEC-NUM-MINI(WSS-F) TO MTI-NUM-MINI IN WSS-VARI-QTX.
           MOVE VEC-NUM-SINI(WSS-F) TO MTI-NUM-SINI IN WSS-VARI-QTX.
           MOVE VEC-NUM-AINI(WSS-F) TO MTI-NUM-AINI IN WSS-VARI-QTX.
           MOVE VEC-NUM-DTER(WSS-F) TO MTI-NUM-DTER IN WSS-VARI-QTX.
           MOVE VEC-NUM-MTER(WSS-F) TO MTI-NUM-MTER IN WSS-VARI-QTX.
           MOVE VEC-NUM-STER(WSS-F) TO MTI-NUM-STER IN WSS-VARI-QTX.
           MOVE VEC-NUM-ATER(WSS-F) TO MTI-NUM-ATER IN WSS-VARI-QTX.
           MOVE VEC-COD-DCPT(WSS-F) TO MTI-COD-DCPT IN WSS-VARI-QTX.
           MOVE VEC-NUM-ITXO(WSS-F) TO MTI-NUM-ITXO IN WSS-VARI-QTX.
           MOVE VEC-IND-VIGE(WSS-F) TO MTI-IND-VIGE IN WSS-VARI-QTX.
           MOVE VEC-COD-ASOC(WSS-F) TO MTI-COD-ASOC IN WSS-VARI-QTX.
           MOVE VEC-NUM-ASOC(WSS-F) TO MTI-NUM-ASOC IN WSS-VARI-QTX.
           MOVE VEC-SGV-ASOC(WSS-F) TO MTI-SGV-ASOC IN WSS-VARI-QTX.
           MOVE VEC-COD-VCOC(WSS-F) TO MTI-COD-VCOC IN WSS-VARI-QTX.
           MOVE VEC-COD-EVNT(WSS-F) TO MTI-COD-EVNT IN WSS-VARI-QTX.

           MOVE VEC-CIC-IOPE(WSS-F) TO FLD-CIC-IOPE IN WSS-VARI-QTX.
           MOVE VEC-NUM-DINI(WSS-F) TO FLD-NUM-DINI IN WSS-VARI-QTX.
           MOVE VEC-NUM-MINI(WSS-F) TO FLD-NUM-MINI IN WSS-VARI-QTX.
           MOVE VEC-NUM-SINI(WSS-F) TO FLD-NUM-SINI IN WSS-VARI-QTX.
           MOVE VEC-NUM-AINI(WSS-F) TO FLD-NUM-AINI IN WSS-VARI-QTX.
           MOVE VEC-NUM-DTER(WSS-F) TO FLD-NUM-DTER IN WSS-VARI-QTX.
           MOVE VEC-NUM-MTER(WSS-F) TO FLD-NUM-MTER IN WSS-VARI-QTX.
           MOVE VEC-NUM-STER(WSS-F) TO FLD-NUM-STER IN WSS-VARI-QTX.
           MOVE VEC-NUM-ATER(WSS-F) TO FLD-NUM-ATER IN WSS-VARI-QTX.
           MOVE VEC-COD-DCPT(WSS-F) TO FLD-COD-DCPT IN WSS-VARI-QTX.
           MOVE VEC-NUM-ITXO(WSS-F) TO FLD-NUM-ITXO IN WSS-VARI-QTX.
           MOVE VEC-IND-VIGE(WSS-F) TO FLD-IND-VIGE IN WSS-VARI-QTX.
           MOVE VEC-COD-ASOC(WSS-F) TO FLD-COD-ASOC IN WSS-VARI-QTX.
           MOVE VEC-NUM-ASOC(WSS-F) TO FLD-NUM-ASOC IN WSS-VARI-QTX.
           MOVE VEC-SGV-ASOC(WSS-F) TO FLD-SGV-ASOC IN WSS-VARI-QTX.
           MOVE VEC-COD-VCOC(WSS-F) TO FLD-COD-VCOC IN WSS-VARI-QTX.
           MOVE VEC-COD-EVNT(WSS-F) TO FLD-COD-EVNT IN WSS-VARI-QTX.

           IF FRM-IFLD > WSS-IMAX
              GO TO FIN-PUT-TAB-QTX.

           MOVE FLD-CIC-IOPE IN WSS-VARI-QTX TO
                FRM-CIC-IOPE IN QTX-FLD(FRM-IFLD).
           MOVE FLD-NUM-DINI IN WSS-VARI-QTX TO
                FRM-NUM-DINI IN QTX-FLD(FRM-IFLD).
           MOVE FLD-NUM-MINI IN WSS-VARI-QTX TO
                FRM-NUM-MINI IN QTX-FLD(FRM-IFLD).
           MOVE FLD-NUM-SINI IN WSS-VARI-QTX TO
                FRM-NUM-SINI IN QTX-FLD(FRM-IFLD).
           MOVE FLD-NUM-AINI IN WSS-VARI-QTX TO
                FRM-NUM-AINI IN QTX-FLD(FRM-IFLD).
           MOVE FLD-NUM-DTER IN WSS-VARI-QTX TO
                FRM-NUM-DTER IN QTX-FLD(FRM-IFLD).
           MOVE FLD-NUM-MTER IN WSS-VARI-QTX TO
                FRM-NUM-MTER IN QTX-FLD(FRM-IFLD).
           MOVE FLD-NUM-STER IN WSS-VARI-QTX TO
                FRM-NUM-STER IN QTX-FLD(FRM-IFLD).
           MOVE FLD-NUM-ATER IN WSS-VARI-QTX TO
                FRM-NUM-ATER IN QTX-FLD(FRM-IFLD).
           MOVE FLD-COD-DCPT IN WSS-VARI-QTX TO
                FRM-COD-DCPT IN QTX-FLD(FRM-IFLD).
           MOVE FLD-NUM-ITXO IN WSS-VARI-QTX TO
                FRM-NUM-ITXO IN QTX-FLD(FRM-IFLD).
           MOVE FLD-IND-VIGE IN WSS-VARI-QTX TO
                FRM-IND-VIGE IN QTX-FLD(FRM-IFLD).
           MOVE FLD-COD-ASOC IN WSS-VARI-QTX TO
                FRM-COD-ASOC IN QTX-FLD(FRM-IFLD).
           MOVE FLD-NUM-ASOC IN WSS-VARI-QTX TO
                FRM-NUM-ASOC IN QTX-FLD(FRM-IFLD).
           MOVE FLD-SGV-ASOC IN WSS-VARI-QTX TO
                FRM-SGV-ASOC IN QTX-FLD(FRM-IFLD).
           MOVE FLD-COD-VCOC IN WSS-VARI-QTX TO
                FRM-COD-VCOC IN QTX-FLD(FRM-IFLD).
           MOVE FLD-COD-EVNT IN WSS-VARI-QTX TO
                FRM-COD-EVNT IN QTX-FLD(FRM-IFLD).
       FIN-PUT-TAB-QTX.
           EXIT.
      *----------------------------------------------------
       PUT-QTX-QUE SECTION.
       INI-PUT-QTX-QUE.
           PERFORM PRO-FLD-QUE.
           ADD 1                         TO WSS-NITM.
           IF (SCR-OFRM-SRV AND WSS-NITM > WSS-NITMMAX)
              GO TO FIN-PUT-QTX-QUE.
           IF ((NOT SCR-OFRM-SRV) AND WSS-NITM > WSS-JMAX)
              GO TO FIN-PUT-QTX-QUE.
           MOVE QTX-NOM-TSQTX            TO QUE-COLA.
           MOVE QTX-LEN-TSQTX            TO QUE-LITM.
           MOVE WSS-NITM                 TO QUE-NITM.
           MOVE SPACES                   TO QUE-ITEM.
      *
           IF SCR-OFRM-SRV
              MOVE MTI-SGV-ASOC IN WSS-VARI-QTX TO FRM-SGV-ASOC-EDT
              MOVE FRM-SGV-ASOC-EDT             TO MTI-SGV-ASOC-ALF.
      *
           IF SCR-OFRM-SRV
              MOVE WSS-NUM-ICLI-IDX-MTI  TO QUE-ITEM
           ELSE
              MOVE WSS-NUM-ICLI-IDX-FLD  TO QUE-ITEM.
           MOVE QUE-PUT  TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
       FIN-PUT-QTX-QUE.
           EXIT.
      *----------------------------------------------------

       PUT-TAB-ASC SECTION.
       INI-PUT-TAB-ASC.
           MOVE SPACES TO MSG-COD-MENS.
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.
           MOVE 0 TO FRM-IFLD.
       LUP-PUT-TAB-ASC.
           ADD 1              TO QUE-NITM.
           MOVE QTX-NOM-TSQTX TO QUE-COLA.
           MOVE QTX-LEN-TSQTX TO QUE-LITM.
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD = 0
                   MOVE 'TXO   NONXT' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-ASC
               ELSE
                   GO TO FIN-PUT-TAB-ASC.
           ADD 1 TO FRM-IFLD.
           MOVE QUE-ITEM             TO WSS-NUM-ICLI-IDX-FLD.
           MOVE WSS-NUM-ICLI-IDX-FLD TO FRM-CIC-IOPE-IDX(FRM-IFLD).
           IF FRM-IFLD < WSS-IMAX
               GO TO LUP-PUT-TAB-ASC.
       FIN-PUT-TAB-ASC.
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX
               ADD 1 FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1
                                 UNTIL WSS-I > WSS-IMAX.
           IF FRM-IFLD > 0
               ADD 1 TO SCR-NUM-PANT.
           IF FRM-IFLD = WSS-IMAX
               ADD 1              TO QUE-NITM
               MOVE QTX-NOM-TSQTX TO QUE-COLA
               MOVE QTX-LEN-TSQTX TO QUE-LITM
               MOVE QUE-GET       TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT = QUE-STAT-OKS
                   MOVE 'SGC'        TO MSG-COD-SIST
                   MOVE 'TXO   DMAS' TO MSG-COD-MENS.
      *PVG INI 10-DIC-2008
           PERFORM PRO-FLD-FRM.
      *PVG FIN 10-DIC-2008
       EXT-PUT-TAB-ASC.
           EXIT.

       PUT-TAB-DSC SECTION.
       INI-PUT-TAB-DSC.
           MOVE SPACES TO MSG-COD-MENS.
           IF SCR-NUM-PANT = 1
               MOVE 'TXO   NOPRV' TO MSG-COD-MENS
               GO TO EXT-PUT-TAB-DSC.

           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.
           ADD 1 TO QUE-NITM.
           ADD 1 WSS-IMAX GIVING FRM-IFLD.
       LUP-PUT-TAB-DSC.
           SUBTRACT  1 FROM QUE-NITM.
           MOVE QTX-NOM-TSQTX TO QUE-COLA
           MOVE QTX-LEN-TSQTX TO QUE-LITM
           MOVE QUE-GET       TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               IF FRM-IFLD > WSS-IMAX
                   MOVE 'TXO   NOPRV' TO MSG-COD-MENS
                   GO TO FIN-PUT-TAB-DSC
               ELSE
                   GO TO FIN-PUT-TAB-DSC.
           SUBTRACT 1 FROM FRM-IFLD.
           MOVE QUE-ITEM             TO WSS-NUM-ICLI-IDX-FLD.
           MOVE WSS-NUM-ICLI-IDX-FLD TO FRM-CIC-IOPE-IDX(FRM-IFLD).
           IF FRM-IFLD > 1
               GO TO LUP-PUT-TAB-DSC.
       FIN-PUT-TAB-DSC.

           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1
                                 UNTIL WSS-I < 1.

           IF FRM-IFLD NOT > WSS-IMAX
               SUBTRACT 1 FROM SCR-NUM-PANT
               MOVE 'SGC'        TO MSG-COD-SIST
               MOVE 'TXO   DMAS' TO MSG-COD-MENS.
      *PVG INI 10-DIC-2008
           PERFORM PRO-FLD-FRM.
      *PVG FIN 10-DIC-2008
       EXT-PUT-TAB-DSC.
           EXIT.

      *------------------------------------------------------
       CLR-FRM SECTION.
       INI-CLR-FRM.
           MOVE SPACES TO FRM-CIC-IOPE IN QTX-FLD(WSS-I).
           MOVE ZEROES TO FRM-FEC-FINI IN QTX-FLD(WSS-I).
           MOVE ZEROES TO FRM-FEC-FTER IN QTX-FLD(WSS-I).
           MOVE SPACES TO FRM-COD-DCPT IN QTX-FLD(WSS-I).
           MOVE ZEROES TO FRM-NUM-ITXO IN QTX-FLD(WSS-I).
           MOVE SPACES TO FRM-IND-VIGE IN QTX-FLD(WSS-I).
           MOVE SPACES TO FRM-COD-ASOC IN QTX-FLD(WSS-I).
           MOVE ZEROES TO FRM-NUM-ASOC IN QTX-FLD(WSS-I).
           MOVE ZEROES TO FRM-SGV-ASOC IN QTX-FLD(WSS-I).
           MOVE SPACES TO FRM-COD-VCOC IN QTX-FLD(WSS-I).
           MOVE SPACES TO FRM-COD-EVNT IN QTX-FLD(WSS-I).
       FIN-CLR-FRM.
           EXIT.

       CLR-WSS SECTION.
       INI-CLR-WSS.
           MOVE SPACES TO MTI-CIC-IOPE IN WSS-VARI-QTX.
           MOVE ZEROES TO MTI-FEC-FINI IN WSS-VARI-QTX.
           MOVE ZEROES TO MTI-FEC-FTER IN WSS-VARI-QTX.
           MOVE SPACES TO MTI-COD-DCPT IN WSS-VARI-QTX.
           MOVE ZEROES TO MTI-NUM-ITXO IN WSS-VARI-QTX.
           MOVE SPACES TO MTI-IND-VIGE IN WSS-VARI-QTX.
           MOVE SPACES TO MTI-COD-ASOC IN WSS-VARI-QTX.
           MOVE ZEROES TO MTI-NUM-ASOC IN WSS-VARI-QTX.
           MOVE ZEROES TO MTI-SGV-ASOC IN WSS-VARI-QTX.
           MOVE SPACES TO MTI-COD-VCOC IN WSS-VARI-QTX.
           MOVE SPACES TO MTI-COD-EVNT IN WSS-VARI-QTX.

           MOVE SPACES TO FLD-CIC-IOPE IN WSS-VARI-QTX.
           MOVE ZEROES TO FLD-FEC-FINI IN WSS-VARI-QTX.
           MOVE ZEROES TO FLD-FEC-FTER IN WSS-VARI-QTX.
           MOVE SPACES TO FLD-COD-DCPT IN WSS-VARI-QTX.
           MOVE ZEROES TO FLD-NUM-ITXO IN WSS-VARI-QTX.
           MOVE SPACES TO FLD-IND-VIGE IN WSS-VARI-QTX.
           MOVE SPACES TO FLD-COD-ASOC IN WSS-VARI-QTX.
           MOVE ZEROES TO FLD-NUM-ASOC IN WSS-VARI-QTX.
           MOVE ZEROES TO FLD-SGV-ASOC IN WSS-VARI-QTX.
           MOVE SPACES TO FLD-COD-VCOC IN WSS-VARI-QTX.
           MOVE SPACES TO FLD-COD-EVNT IN WSS-VARI-QTX.

           IF SCR-OFRM-SRV
               PERFORM CLR-FRM-MTX.
       FIN-CLR-WSS.
           EXIT.

       CLR-FRM-MTX SECTION.
       INI-CLR-FRM-MTX.
           MOVE SPACES TO FRM-CIC-IOPE IN QTX-FLD(1).
           MOVE ZEROES TO FRM-FEC-FINI IN QTX-FLD(1).
           MOVE ZEROES TO FRM-FEC-FTER IN QTX-FLD(1).
           MOVE SPACES TO FRM-COD-DCPT IN QTX-FLD(1).
           MOVE ZEROES TO FRM-NUM-ITXO IN QTX-FLD(1).
           MOVE SPACES TO FRM-IND-VIGE IN QTX-FLD(1).
           MOVE SPACES TO FRM-COD-ASOC IN QTX-FLD(1).
           MOVE ZEROES TO FRM-NUM-ASOC IN QTX-FLD(1).
           MOVE ZEROES TO FRM-SGV-ASOC IN QTX-FLD(1).
           MOVE SPACES TO FRM-COD-VCOC IN QTX-FLD(1).
           MOVE SPACES TO FRM-COD-EVNT IN QTX-FLD(1).
       FIN-CLR-FRM-MTX.
           EXIT.

       CRG-VEC-TXO SECTION.
       INI-CRG-VEC-TXO.
           ADD 1 TO WSS-J.
*******    IF SCR-OFRM-SRV AND     WSS-J > WSS-TOPEMAX
*******       GO TO FIN-CRG-VEC-TXO.
*******    IF NOT SCR-OFRM-SRV AND WSS-J > WSS-JMAX
*******       GO TO FIN-CRG-VEC-TXO.

           PERFORM CLR-TXO-VEC.

           MOVE TXO-CIC-IOPE IN TXO TO VEC-CIC-IOPE(WSS-J).
           MOVE TXO-NUM-DINI IN TXO TO VEC-NUM-DINI(WSS-J).
           MOVE TXO-NUM-MINI IN TXO TO VEC-NUM-MINI(WSS-J).
           MOVE TXO-NUM-SINI IN TXO TO VEC-NUM-SINI(WSS-J).
           MOVE TXO-NUM-AINI IN TXO TO VEC-NUM-AINI(WSS-J).
           MOVE TXO-NUM-DTER IN TXO TO VEC-NUM-DTER(WSS-J).
           MOVE TXO-NUM-MTER IN TXO TO VEC-NUM-MTER(WSS-J).
           MOVE TXO-NUM-STER IN TXO TO VEC-NUM-STER(WSS-J).
           MOVE TXO-NUM-ATER IN TXO TO VEC-NUM-ATER(WSS-J).
           MOVE TXO-COD-CPTO IN TXO TO VEC-COD-DCPT(WSS-J).
           MOVE TXO-NUM-ITXO IN TXO TO VEC-NUM-ITXO(WSS-J).
           MOVE TXO-MSC-STAT IN TXO TO VEC-IND-VIGE(WSS-J).
           MOVE TXO-COD-ASOC IN TXO TO VEC-COD-ASOC(WSS-J).
           MOVE TXO-NUM-ASOC IN TXO TO VEC-NUM-ASOC(WSS-J).
           MOVE TXO-SGV-ASOC IN TXO TO VEC-SGV-ASOC(WSS-J).
           MOVE TXO-COD-VCOC IN TXO TO VEC-COD-VCOC(WSS-J).
           MOVE TXO-COD-EVNT IN TXO TO VEC-COD-EVNT(WSS-J).
       FIN-CRG-VEC-TXO.
           EXIT.

       CLR-TXO-VEC SECTION.
       INI-CLR-TXO-VEC.
           MOVE SPACES TO VEC-CIC-IOPE(WSS-J).
           MOVE ZEROES TO VEC-FEC-FINI(WSS-J).
           MOVE ZEROES TO VEC-FEC-FTER(WSS-J).
           MOVE SPACES TO VEC-COD-DCPT(WSS-J).
           MOVE ZEROES TO VEC-NUM-ITXO(WSS-J).
           MOVE SPACES TO VEC-IND-VIGE(WSS-J).
           MOVE SPACES TO VEC-COD-ASOC(WSS-J).
           MOVE ZEROES TO VEC-NUM-ASOC(WSS-J).
           MOVE ZEROES TO VEC-SGV-ASOC(WSS-J).
           MOVE SPACES TO VEC-COD-VCOC(WSS-J).
           MOVE SPACES TO VEC-COD-EVNT(WSS-J).
       FIN-CLR-TXO-VEC.
           EXIT.

       PRO-FLD-QUE SECTION.
       INI-PRO-FLD-QUE.
           MOVE 'P'    TO MTI-CIC-IOPE-ATR IN WSS-VARI-QTX.
           MOVE 'P'    TO MTI-FEC-FINI-ATR IN WSS-VARI-QTX.
           MOVE 'P'    TO MTI-FEC-FTER-ATR IN WSS-VARI-QTX.
           MOVE 'P'    TO MTI-COD-DCPT-ATR IN WSS-VARI-QTX.
           MOVE 'P'    TO MTI-NUM-ITXO-ATR IN WSS-VARI-QTX.
           MOVE 'P'    TO MTI-IND-VIGE-ATR IN WSS-VARI-QTX.
           MOVE 'P'    TO MTI-COD-ASOC-ATR IN WSS-VARI-QTX.
           MOVE 'P'    TO MTI-NUM-ASOC-ATR IN WSS-VARI-QTX.
           MOVE 'P'    TO MTI-SGV-ASOC-ATR IN WSS-VARI-QTX.
           MOVE 'P'    TO MTI-COD-VCOC-ATR IN WSS-VARI-QTX.
           MOVE 'P'    TO MTI-COD-EVNT-ATR IN WSS-VARI-QTX.

           MOVE 'P'    TO FLD-CIC-IOPE-ATR IN WSS-VARI-QTX.
           MOVE 'P'    TO FLD-FEC-FINI-ATR IN WSS-VARI-QTX.
           MOVE 'P'    TO FLD-FEC-FTER-ATR IN WSS-VARI-QTX.
           MOVE 'P'    TO FLD-COD-DCPT-ATR IN WSS-VARI-QTX.
           MOVE 'P'    TO FLD-NUM-ITXO-ATR IN WSS-VARI-QTX.
           MOVE 'P'    TO FLD-IND-VIGE-ATR IN WSS-VARI-QTX.
           MOVE 'P'    TO FLD-COD-ASOC-ATR IN WSS-VARI-QTX.
           MOVE 'P'    TO FLD-NUM-ASOC-ATR IN WSS-VARI-QTX.
           MOVE 'P'    TO FLD-SGV-ASOC-ATR IN WSS-VARI-QTX.
           MOVE 'P'    TO FLD-COD-VCOC-ATR IN WSS-VARI-QTX.
           MOVE 'P'    TO FLD-COD-EVNT-ATR IN WSS-VARI-QTX.

           MOVE ZEROES TO FLD-CIC-IOPE-LEN IN WSS-VARI-QTX.
           MOVE ZEROES TO FLD-FEC-FINI-LEN IN WSS-VARI-QTX.
           MOVE ZEROES TO FLD-FEC-FTER-LEN IN WSS-VARI-QTX.
           MOVE ZEROES TO FLD-COD-DCPT-LEN IN WSS-VARI-QTX.
           MOVE ZEROES TO FLD-NUM-ITXO-LEN IN WSS-VARI-QTX.
           MOVE ZEROES TO FLD-IND-VIGE-LEN IN WSS-VARI-QTX.
           MOVE ZEROES TO FLD-COD-ASOC-LEN IN WSS-VARI-QTX.
           MOVE ZEROES TO FLD-NUM-ASOC-LEN IN WSS-VARI-QTX.
           MOVE ZEROES TO FLD-SGV-ASOC-LEN IN WSS-VARI-QTX.
           MOVE ZEROES TO FLD-COD-VCOC-LEN IN WSS-VARI-QTX.
           MOVE ZEROES TO FLD-COD-EVNT-LEN IN WSS-VARI-QTX.
       FIN-PRO-FLD-QUE.
           EXIT.

       COPY GNSBBTAB.
       COPY GNSBVTAB.
       COPY GNSBVCOD.
      *PVG INI 10-DIC-2008
       PRO-FLD-FRM SECTION.
