                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-RCO-COD-STAT.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RCO-COD-STAT-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-OPE-IND-MCLI.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPE-IND-MCLI-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-FRM-GLS-STAT.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-STAT-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-FRM-GLS-MCLI.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-MCLI-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-RCO-COD-RCOP.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RCO-COD-RCOP-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-RCO-VAL-PRCO.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RCO-VAL-PRCO-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-FRM-GLS-RCOP.
          IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-RCOP-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-OPE-COD-COPR.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPE-COD-COPR-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           GO TO FIN-VAL-FLD-RCO.
      *>>>>
           PERFORM VAL-OPE-MSC-TVOP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPE-MSC-TVOP-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-OPE-COD-TOPR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPE-COD-TOPR-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-OPE-COD-OFOP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPE-COD-OFOP-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-FRM-GLS-TVOP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TVOP-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-FRM-GLS-TOPR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TOPR-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-FRM-GLS-OFOP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-OFOP-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-OPE-COD-COPR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPE-COD-COPR-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-RCO-NUM-DIRE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RCO-NUM-DIRE-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-RCO-COD-STAT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RCO-COD-STAT-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-OPE-IND-MCLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPE-IND-MCLI-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-FRM-GLS-STAT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-STAT-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-FRM-GLS-MCLI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-MCLI-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-RCO-COD-RCOP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RCO-COD-RCOP-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-RCO-VAL-PRCO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO RCO-VAL-PRCO-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
           PERFORM VAL-FRM-GLS-RCOP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-RCOP-LEN IN RCO-FLD
                GO TO FIN-VAL-FLD-RCO.
