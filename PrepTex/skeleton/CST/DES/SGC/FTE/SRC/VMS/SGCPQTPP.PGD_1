       COPY GNSBVTAB.                                                   
       COPY GNSBVCOD.                                                   
       COPY GNSBFMSC.                                                   
       COPY GNSBVIND.                                                   
       COPY GNSBBMSC.                                                   
       COPY GNSBVMSC.                                                   
