*%! Codigo adicional para programa COLPAPCG.
*% IF WSS
      *<<<< WSS
       COPY COLBRPOC.
       COPY GNSBRTAB.
       COPY TABBROFI.
       COPY TABBRUSR.
       COPY GNSWGFEC.
       COPY GNSWGHOY.
      * COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-POC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-TAB-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OFI-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-USR-REQA==.
       01  WSS-COD-COOC.
           03  WSS-COD-TOOC                       PIC X(03).
           03  WSS-COD-AOOC                       PIC X(03).
      *>>>>
*% END
*% IF FND_KEY
      *<<<< FND-KEY
           MOVE PCG-COD-VCOC IN PCG-FLD TO POC-COD-VCOC IN POC.
           MOVE PCG-COD-TOOC IN PCG-FLD TO POC-COD-TOOC IN POC.
           MOVE PCG-COD-AOOC IN PCG-FLD TO POC-COD-AOOC IN POC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-POC.
           IF NOT FIO-STAT-OKS
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'POC    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FIO-STAT-NEX TO FIO-STAT
               GO TO FIN-FND-KEY
           ELSE
               PERFORM PUT-POC-PCG.
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND-KEY
           PERFORM GET-FHOY.
           MOVE HOY-SHOY TO PCG-NUM-STRN IN PCG-FLD.
           MOVE HOY-AHOY TO PCG-NUM-ATRN IN PCG-FLD.
           MOVE HOY-MHOY TO PCG-NUM-MTRN IN PCG-FLD.
           MOVE HOY-DHOY TO PCG-NUM-DTRN IN PCG-FLD.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY COLBFPOC.
       COPY GNSBBTAB.
       COPY GNSBFTAB.
       COPY GNSBVTAB.
       COPY TABBBOFI.
       COPY TABBFOFI.
       COPY TABBVOFI.
       COPY TABBBUSR.
       COPY TABBFUSR.
       COPY TABBVUSR.
       COPY GNSBVCOD.
       COPY GNSBVIND.
       COPY GNSBBMSC.
       COPY GNSBVMSC.
       COPY GNSBGFEC.
       COPY GNSBGHOY.
      *>>>>
*% END
