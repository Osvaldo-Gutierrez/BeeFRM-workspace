*%! Codigo adicional para archivo
*% IF WSS
      *<<<< WSS
       COPY COLWGADO.
       COPY GNSWGBCD.
       COPY GNSWGDSP.
       COPY GNSWGTDQ.
       COPY COLWGTDS.
       01  WSS-VARI.
           03 WSS-OL1.
              05 OL1-KEY-CRE                          PIC X(12).
              05 OL1-NUM-VTO-PAG                      PIC 9(01).
              05 OL1-COD-OFI-REC                      PIC X(03).
              05 OL1-FEC-CAN-REA.
                 07 OL1-NUM-ACAN                      PIC X(02).
                 07 OL1-NUM-MCAN                      PIC X(02).
                 07 OL1-NUM-DCAN                      PIC X(02).
              05 OL1-VAL-PAG                          PIC 9(10).
           03  WSS-CRS-CIC-IOPC.
               05  WSS-CRS-CAI-IOPC                   PIC X(04).
               05  WSS-CRS-IIC-IOPC                   PIC 9(08).
      *>>>>
*% END
*% IF INI_CMN_ACC
      *<<<< INI_CMN_ACC
           IF NOT (SCR-CMND  = 'PAG' OR
              SCR-CMND  = 'PSN'  OR
              SCR-CMND  = 'RCS'  OR
              SCR-CMND  = 'PPP'  OR
              SCR-CMND  = 'PNG'  OR
              SCR-CMND  = 'PPR'  OR
              SCR-CMND  = 'PPA' )
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'CMDINV' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-ACC.
           MOVE FRM-CAI-ICRS IN CRS-FLD TO WSS-CRS-CAI-IOPC.
           MOVE FRM-IIC-ICRS IN CRS-FLD TO WSS-CRS-IIC-IOPC.
           MOVE WSS-CRS-CIC-IOPC        TO KOL1-KEY-CRE
                                           KOL1-KEY-OPE
                                           ADO-KEY-IOPC.
           MOVE CRS-VAL-VCNC IN CRS-FLD TO KOL1-VAL-PAG.
           MOVE CRS-VAL-VCNC IN CRS-FLD TO ADO-VAL-TPAG.
           MOVE '1' TO ADO-ET21.
           MOVE ADO-ET21 TO KOL1-COD-EST21.
           MOVE CRS-NUM-IVEN IN CRS-FLD TO KOL1-NUM-VTO-PAG.
           IF SCR-CMND = 'PSN'
               MOVE CRS-NUM-IVEN IN CRS-FLD TO OL1-NUM-VTO-PAG
               MOVE WSS-CRS-CIC-IOPC        TO OL1-KEY-CRE
               MOVE 50000 TO OL1-VAL-PAG
               MOVE '069' TO OL1-COD-OFI-REC
               MOVE CRS-NUM-DVCN IN CRS-FLD TO OL1-NUM-DCAN
               MOVE CRS-NUM-MVCN IN CRS-FLD TO OL1-NUM-MCAN
               MOVE CRS-NUM-AVCN IN CRS-FLD TO OL1-NUM-ACAN
               MOVE WSS-OL1 TO SYS-CMMA
               MOVE +32     TO SYS-TCMA
               MOVE 'COLPPACS' TO SYS-PROG
               MOVE SYS-XCTL    TO SYS-CMND
               PERFORM GNS-PRO-SYS.
           IF SCR-CMND = 'PNG'
               MOVE '0'                     TO KOL1-IND-PAGO
               MOVE '1'                     TO KOL1-COD-EST21
               MOVE '069' TO KOL1-COD-OFI-REC
               MOVE CRS-NUM-DVCN IN CRS-FLD TO OL1-NUM-DCAN
               MOVE CRS-NUM-MVCN IN CRS-FLD TO OL1-NUM-MCAN
               MOVE CRS-NUM-AVCN IN CRS-FLD TO OL1-NUM-ACAN
               MOVE OL1-FEC-CAN-REA TO KOL1-FEC-CAN-REA.
           IF SCR-CMND = 'PPA'
               MOVE 'S' TO ADO-IND-ORIG
               SUBTRACT 1 FROM KOL1-NUM-VTO-PAG GIVING
                          ADO-NUM-IVCT
               MOVE IFD-FSIS TO ADO-FSIS
               MOVE ADO-CMMA TO SYS-CMMA
               MOVE SYS-LINK TO SYS-CMND
               MOVE 'COLPOCAN' TO SYS-PROG
               MOVE ADO-TCMA    TO SYS-TCMA
               PERFORM GNS-PRO-SYS
               GO TO NXT-1.
           IF SCR-CMND = 'PPR'
               MOVE 'S' TO ADO-IND-ORIG
               SUBTRACT 1 FROM KOL1-NUM-VTO-PAG GIVING
                          ADO-NUM-IVCT
               MOVE 'REV'    TO ADO-CMND
               MOVE IFD-FSIS TO ADO-FSIS
               MOVE ADO-CMMA TO SYS-CMMA
               MOVE SYS-LINK TO SYS-CMND
               MOVE 'COLPOCAN' TO SYS-PROG
               MOVE ADO-TCMA    TO SYS-TCMA
               PERFORM GNS-PRO-SYS
               GO TO NXT-1.
           IF SCR-CMND = 'RCS'
               MOVE '0'                     TO KOL1-IND-PAGO
               MOVE 'KOL2' TO TDQ-COLA
           ELSE
               MOVE 'KOL1' TO TDQ-COLA.
           PERFORM GNS-ERR-TDQ THRU FIN-GNS-ERR-TDQ.
           MOVE TDQ-PUT TO TDQ-CMND.
      *    MOVE 'KOL1' TO TDQ-COLA.
           MOVE SAFTKOL1 TO TDQ-ITEM.
           MOVE +100 TO TDQ-LITM.
           PERFORM GNS-PRO-TDQ.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBGTDQ.
       COPY GNSBGDSP.
      *>>>>
*% END
