*%! Codigo adicional para archivo
*% IF INI_FIO
      *<<<< INI_FIO
           PERFORM GNS-HDL-VSM.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBGVSM.
       COPY GNSBHVSM.
      *>>>>
*% END
*% IF WSS
      *<<<< WSS
       COPY COLWGADO.
       COPY GNSWGBCD.
       COPY GNSWGDSP.
       COPY GNSWGTDQ.
       COPY COLWGTDS.
       01  WSS-VARI.
           03  WSS-CRS-CIC-IOPC.
               05  WSS-CRS-CAI-IOPC                   PIC X(04).
               05  WSS-CRS-IIC-IOPC                   PIC 9(08).
      *>>>>
*% END
*% IF INI_CMN_ACC
      *<<<< INI_CMN_ACC
           IF SCR-CMND NOT = 'PAG'
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'CMDINV' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-ACC.
           MOVE FRM-CAI-ICRS IN CRS-FLD TO WSS-CRS-CAI-IOPC.
           MOVE FRM-IIC-ICRS IN CRS-FLD TO WSS-CRS-IIC-IOPC.
           MOVE WSS-CRS-CIC-IOPC        TO KOL1-KEY-CRE
                                           KOL1-KEY-OPE.
           MOVE CRS-NUM-IVEN IN CRS-FLD TO KOL1-NUM-VTO-PAG.
           PERFORM GNS-ERR-TDQ THRU FIN-GNS-ERR-TDQ.
           MOVE TDQ-PUT TO TDQ-CMND.
           MOVE 'KOL1' TO TDQ-COLA.
           MOVE SAFTKOL1 TO TDQ-ITEM.
           MOVE +100 TO TDQ-LITM.
           PERFORM GNS-PRO-TDQ.
      *>>>> END
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBGTDQ.
       COPY GNSBGDSP.
      *>>>>
*% END
*% IF BKP
      *RESPALDO DE INSTRUCCIONES PARA INVOCAR AL PROGRAMA
           MOVE 'S' TO ADO-IND-ORIG.
           MOVE IFD-FSIS IN SCR-VARI TO ADO-FSIS.
           MOVE ADO-CMMA   TO SYS-CMMA.
           MOVE SYS-LINK   TO SYS-CMND.
           MOVE 'COLPOCAN' TO SYS-PROG.
           MOVE ADO-TCMA   TO SYS-TCMA.
           PERFORM GNS-PRO-SYS.
           MOVE SYS-CMMA TO ADO-CMMA.
           IF ADO-GLS-MENS > SPACES
                 MOVE ADO-GLS-MENS TO MSG-GLS-DESC FRM-MENS
           ELSE
                 MOVE 'COL' TO MSG-COD-SIST
                 MOVE 'CAN    PAGOK' TO MSG-COD-MENS
                 PERFORM GET-MSG
                 MOVE MSG-GLS-DESC TO FRM-MENS.
           GO TO FIN-CMN-ACC.
*% END
