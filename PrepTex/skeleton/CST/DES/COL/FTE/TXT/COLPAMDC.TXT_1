*%! Codigo adicional para archivo
*% IF WSS
      *<<<< WSS
       COPY COLBROPC.
       COPY SGCBRDBC.
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OPC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
      *>>>>
*% END
*% IF FND_KEY
      *<<<< FND_KEY
      * Busca datos en registro RDC.
           MOVE 'DBC-IDC-ICLI' TO FIO-AKEY.
           MOVE FIO-GET-KEY-ALT TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           IF FIO-STAT-OKS
               MOVE DBC-CAI-ICLI IN DBC TO RDC-CAI-ICLI IN RDC
               MOVE DBC-IIC-ICLI IN DBC TO RDC-IIC-ICLI IN RDC
           ELSE
               GO TO FIN-FND-KEY.
           MOVE RDC-CAI-IOPC IN MDC-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE RDC-IIC-IOPC IN MDC-FLD TO OPC-IIC-IOPC IN OPC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-OPC.           
      *>>>>
*% END
*% IF INI_CMN_MOD
      *<<<< INI_CMN_MOD
      * Modifica datos registro RDC.
           MOVE 'DBC-IDC-ICLI' TO FIO-AKEY.
           MOVE FIO-GET-KEY-ALT TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           IF FIO-STAT-OKS
               MOVE DBC-CAI-ICLI IN DBC TO RDC-CAI-ICLI IN RDC
               MOVE DBC-IIC-ICLI IN DBC TO RDC-IIC-ICLI IN RDC.
      *>>>>
*% END
*% IF INI_CMN_ING
      *<<<< INI_CMN_ING
           PERFORM PUT-REG-RDC.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       PUT-REG-RDC SECTION.
       INI-PUT-REG-RDC.
      * Ingresa datos en registro RDC.
           MOVE 'DBC-IDC-ICLI' TO FIO-AKEY.
           MOVE FIO-GET-KEY-ALT TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           IF FIO-STAT-OKS
               MOVE DBC-CAI-ICLI IN DBC TO RDC-CAI-ICLI IN RDC
               MOVE DBC-IIC-ICLI IN DBC TO RDC-IIC-ICLI IN RDC
           ELSE
               GO TO FIN-CMN-ING. 
       FIN-PUT-REG-RDC.
           EXIT.

       COPY COLBFOPC.
       COPY SGCBFDBC.
      *>>>>
*% END
