*%! Codigo adicional para archivo
*% IF WSS
      *<<<< WSS
       COPY GNSBRMSC.
       COPY TABBROFI.
       COPY TABBRUSR.
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSC-REQA==. 
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OFI-REQA==. 
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-USR-REQA==. 
      *>>>>
*% END
*% IF INI_FST_INP
      *<<<<
      *Busca Nombre de la Tabla.
           MOVE 'COL'  TO FIO-SIST.
           MOVE 'NTR'  TO TAB-COD-TTAB IN TAB.
           MOVE SPACES TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF FIO-STAT-OKS
               MOVE TAB-GLS-DESC IN TAB        TO WSS-GLS-DESC
           ELSE     
               MOVE 'TABLA SIN REGISTRO PADRE' TO WSS-GLS-DESC.
      *Busca Responsable de la Tabla.
           IF TAB-COD-ATRN IN TAB > SPACES
               MOVE TAB-COD-ATRN IN TAB TO USR-COD-USER IN USR
               PERFORM BUS-USR
           ELSE
               MOVE SPACES TO USR-GLS-DESC.
      *>>>>
*% END
*% IF LUP_OUT
      *<<<< LUP_OUT
      *BUS-TAB busca glosa de codigo NOTARIO en tabla NOT
           IF NTR-COD-NOTA IN SRT > SPACES
               MOVE 'TAB'        TO TAB-COD-SIST
               MOVE 'NOT'        TO TAB-COD-TTAB IN TAB
               MOVE NTR-COD-NOTA IN SRT TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO WSS-GLS-NOTA IN WSS-NTR
           ELSE
               MOVE SPACES TO WSS-GLS-NOTA IN WSS-NTR.

      *BUS-OFI busca oficina en tablas
           IF NTR-COD-OFIC IN SRT > SPACES
               MOVE NTR-COD-OFIC IN SRT TO OFI-COD-OFIC IN OFI
               PERFORM BUS-OFI
               MOVE OFI-GLS-DESC IN OFI TO WSS-GLS-OFIC IN WSS-NTR
           ELSE
               MOVE SPACES TO WSS-GLS-OFIC IN WSS-NTR.

      *BUS-TAB busca glosa de codigo TIPO DOCUMENTO en tabla DOC
           IF NTR-COD-TDOC IN SRT > SPACES
               MOVE 'TAB'        TO TAB-COD-SIST
               MOVE 'DOC'        TO TAB-COD-TTAB IN TAB
               MOVE NTR-COD-TDOC IN SRT TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO WSS-GLS-TDOC IN WSS-NTR
           ELSE
               MOVE SPACES TO WSS-GLS-TDOC IN WSS-NTR.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBFMSC.
       COPY TABBFOFI.
       COPY TABBFUSR.
       COPY GNSBBMSG.
       COPY GNSBBMSC.
       COPY GNSBBTAB.
       COPY TABBBOFI.
       COPY TABBBUSR.
      *>>>>
*% END
