           IF OPC-PENDIENTE-CTC
               MOVE FRM-CPIM-AEY TO OPC-VAL-CRED-ATR IN OPC-FLD.
      *>>>>
*% END
*% IF INI_VAL_FLD_OPC
      *<<<< INI_VAL_FLD_OPC
           IF SCR-CMND NOT = 'MCA'
               GO TO NXT-VAL-FLD-OPC.
           PERFORM VAL-RDC-NUM-DIRE.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RDC-NUM-DIRE-LEN IN OPC-FLD
                GO TO FIN-VAL-FLD-OPC.
      *DAD ini  12-AUG-1992   (Memo.24-07-92 Pto.1)
           PERFORM VAL-OPC-VAL-SEGD.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPC-VAL-SEGD-LEN IN OPC-FLD
                GO TO FIN-VAL-FLD-OPC.
      *DAD fin  12-AUG-1992
           PERFORM VAL-OPC-COD-RMOC.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPC-COD-RMOC-LEN IN OPC-FLD
                GO TO FIN-VAL-FLD-OPC.
           PERFORM VAL-AUT-CAI-ICYA.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO AUT-CAI-ICYA-LEN IN OPC-FLD
                GO TO FIN-VAL-FLD-OPC.
           PERFORM VAL-AUT-IIC-ICYA.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO AUT-IIC-ICYA-LEN IN OPC-FLD
                GO TO FIN-VAL-FLD-OPC.
           PERFORM VAL-OPC-COD-CDOC.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO OPC-COD-CDOC-LEN IN OPC-FLD
                GO TO FIN-VAL-FLD-OPC.
           GO TO FIN-VAL-FLD-OPC.
       NXT-VAL-FLD-OPC.
