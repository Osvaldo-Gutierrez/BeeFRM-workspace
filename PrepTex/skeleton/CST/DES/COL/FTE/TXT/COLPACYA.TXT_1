*% IF FIN_PUT_SCR
      *<<<< FIN_PUT_SCR
      *PTC-INI:   12-NOV-1996 08:50:01 
           IF SCR-CMND = 'CON' AND
              SCR-OFRM-SRV
                 MOVE CYA-CAI-IOPC IN CYA-FLD TO CYA-CAI-IOPC IN CYA
                 MOVE CYA-IIC-IOPC IN CYA-FLD TO CYA-IIC-IOPC IN CYA
                 MOVE CYA-NUM-ICAN IN CYA-FLD TO CYA-NUM-ICAN IN CYA
                 MOVE ZEROES                  TO CYA-NUM-ICYA IN CYA
                 MOVE ZEROES TO QUE-NITM
                 MOVE FIO-GET-NLS TO FIO-CMND.

       CIC-PTC-CYA.
           IF SCR-CMND = 'CON' AND
              SCR-OFRM-SRV
                 PERFORM COL-FIO-CYA
                 IF FIO-STAT-OKS AND
                    CYA-CAI-IOPC IN CYA = CYA-CAI-IOPC IN CYA-FLD AND
                    CYA-IIC-IOPC IN CYA = CYA-IIC-IOPC IN CYA-FLD AND
                    CYA-NUM-ICAN IN CYA = CYA-NUM-ICAN IN CYA-FLD

                     MOVE CYA-NUM-ICYA IN CYA TO PTC-CYA-NUM-ICYA
                     MOVE CYA-IND-TCYA IN CYA TO PTC-CYA-IND-TCYA
                     MOVE CYA-CAI-ICYA IN CYA TO PTC-CYA-CAI-ICYA
                     MOVE CYA-IIC-ICYA IN CYA TO PTC-CYA-IIC-ICYA
                     PERFORM SET-MONX-VPES
                     
                     ADD 1 TO QUE-NITM
                     MOVE TSK-TERM-ALF TO QUE-TERM
                     MOVE PTC-CYA-TYPE TO QUE-TYPE
                     MOVE PTC-CYA-ITEM TO QUE-ITEM
                     MOVE PTC-CYA-LENG TO QUE-LITM
                     MOVE QUE-PUT  TO QUE-CMND
                     PERFORM GNS-PRO-QUE

                     MOVE FIO-GET-NXT TO FIO-CMND
                     GO TO CIC-PTC-CYA.

      *PTC-FIN:   12-NOV-1996 08:50:01 
      *>>>>
*% END
*% IF FIN_HBL_FLD    
      *<<<< FIN_HBL_FLD
           MOVE CYA-CAI-IOPC IN CYA-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE CYA-IIC-IOPC IN CYA-FLD TO OPC-IIC-IOPC IN OPC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-OPC.
           MOVE FIO-STAT TO PGM-STAT-OPC.
           MOVE 'TVAL' TO MSC-CIC-TMSC IN MSC.
           MOVE '0'    TO MSC-CIC-CMSC IN MSC.
           MOVE 'TAB' TO FIO-SIST.
           MOVE 'MSC-CIC-TABL' TO FIO-AKEY.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-MSC.
           MOVE MSC-COD-CMSC IN MSC TO WSS-COD-TVOC IN WSS-VARI.
           IF WSS-COD-TVOC IN WSS-VARI = OPC-MSC-TVOC IN OPC
               MOVE FRM-CPIM-AEY TO FRM-VAL-MONX-ATR IN CYA-FLD
           ELSE
               MOVE FRM-CPIM-AEY TO FRM-VAL-VPES-ATR IN CYA-FLD.
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
           IF CYA-CAI-IOPC IN CYA-FLD NOT > SPACES
               MOVE SPACES TO CYA-CAI-IOPC IN CYA-FLD.
           IF CYA-NUM-ICAN IN CYA-FLD NOT > SPACES
               MOVE ZEROES TO CYA-NUM-ICAN IN CYA-FLD.
      *>>>>
*% END
*% IF FND_KEY
      *<<<< FND_KEY

      *PTC-INI :   12-NOV-1996 08:50:01 
           MOVE 'N' TO PTC-DEL-CYA.
           IF SCR-CCPP = 'ING' AND
              SCR-OFRM-SRV AND
              CYA-NUM-ICYA IN CYA-FLD = 9
                 MOVE ZEROES TO CYA-NUM-ICYA IN CYA-FLD
                 MOVE ZEROES TO CYA-NUM-ICYA IN CYA
                 MOVE 'S' TO PTC-DEL-CYA.

       CIC-DEL-CYA.
           IF PTC-DEL-CYA =  'S'
                 MOVE FIO-GET-NLS TO FIO-CMND
                 PERFORM COL-FIO-CYA
                 IF FIO-STAT-OKS AND
                    CYA-CAI-IOPC IN CYA = CYA-CAI-IOPC IN CYA-FLD AND
                    CYA-IIC-IOPC IN CYA = CYA-IIC-IOPC IN CYA-FLD AND
                    CYA-NUM-ICAN IN CYA = CYA-NUM-ICAN IN CYA-FLD
                       MOVE FIO-GET-KEY-UPD TO FIO-CMND
                       PERFORM COL-FIO-CYA
                       IF FIO-STAT-OKS
                           MOVE FIO-DEL TO FIO-CMND
                           PERFORM COL-FIO-CYA
                           GO TO CIC-DEL-CYA.
               
           IF PTC-DEL-CYA =  'S'
               MOVE CYA-CAI-IOPC IN CYA-FLD TO CYA-CAI-IOPC IN CYA
               MOVE CYA-IIC-IOPC IN CYA-FLD TO CYA-IIC-IOPC IN CYA
               MOVE CYA-NUM-ICAN IN CYA-FLD TO CYA-NUM-ICAN IN CYA
               MOVE ZEROES TO CYA-NUM-ICYA IN CYA-FLD
               MOVE ZEROES TO CYA-NUM-ICYA IN CYA.

      *PTC-FIN :   12-NOV-1996 08:50:01 


           IF SCR-CCPP = 'ING'
               PERFORM BUS-COR-CYA
               GO TO CON-FND-KEY.
           IF DBC-NUM-ICLI IN CYA-FLD NOT = ZEROES
      *IDC-CLI Busca Cliente por medio de IdC
               MOVE DBC-NUM-ICLI IN CYA-FLD TO DBC-NUM-ICLI IN DBC
               MOVE DBC-IND-ICLI IN CYA-FLD TO DBC-IND-ICLI IN DBC
               MOVE DBC-GLS-ICLI IN CYA-FLD TO DBC-GLS-ICLI IN DBC
               MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY
               MOVE FIO-GET-KEY-ALT TO FIO-CMND
               PERFORM ANL-KEY-CYA
               IF NOT FIO-STAT-OKS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE '00' TO FIO-STAT
                   MOVE SCR-STAT-CON TO SCR-STAT
                   GO TO FIN-FND-KEY
               ELSE
                   MOVE BUS-POR-CLTE TO BUS-POR-KEYS
                   GO TO CON-FIN-FND-KEY
           ELSE
           IF DBC-GLS-NOMC IN CYA-FLD > SPACES
      *NOM-CLI Busca Cliente por medio del Nombre

               MOVE DBC-GLS-NOMC IN CYA-FLD TO STR-GLS-GSTR
               PERFORM STR-UNS
               MOVE STR-GLS-FUNS IN STR-VARI(1) TO DBC-GLS-APAT IN DBC
               MOVE STR-GLS-FUNS IN STR-VARI(2) TO DBC-GLS-AMAT IN DBC
               MOVE STR-GLS-FUNS IN STR-VARI(3) TO DBC-GLS-NOMB IN DBC

               MOVE 'DBC-GLS-NOMC'  TO FIO-AKEY
               MOVE FIO-GET-NLS-ALT TO FIO-CMND
               PERFORM ANL-KEY-CYA
               IF NOT FIO-STAT-OKS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE '00' TO FIO-STAT
                   MOVE SCR-STAT-CON TO SCR-STAT
                   GO TO FIN-FND-KEY
               ELSE
                   MOVE BUS-POR-CLTE TO BUS-POR-KEYS
                   GO TO CON-FIN-FND-KEY.
           IF CYA-NUM-ICYA IN CYA NOT > ZEROES
               PERFORM BUS-COR-CYA.
       CON-FND-KEY.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
           IF SCR-CCPP = 'ACT' AND NO-ASIGNE-CORRELATIVO AND 
              NOT PGM-STAT-CYA-OKS
               MOVE FIO-STAT-NEX TO FIO-STAT
               GO TO FIN-FND-KEY.
       CON-FIN-FND-KEY.
           PERFORM ANL-KEY-OPC.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FIO-STAT-NEX TO FIO-STAT
               GO TO FIN-FND-KEY.

      *ISP CODIGO COMUN
           PERFORM GET-CUR-RDC.

           IF NOT FIO-STAT-OKS
               MOVE 'SGC'        TO MSG-COD-SIST
               MOVE 'CLI    NEX' TO MSG-COD-MENS
               MOVE SPACES TO DBC-IND-ICLI IN DBC
               PERFORM SET-MSG-CLTE
               GO TO CON-RST-STAT.
           PERFORM GET-NXT-RDC.
           IF MSG-COD-MENS = 'OPC    MDDIR'
               IF BUS-POR-KEYS = BUS-POR-CLTE
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MEN2
               ELSE
                   MOVE 'M'     TO DBC-IND-ICLI IN DBC
                   PERFORM SET-MSG-CLTE
           ELSE
           IF NOT BUS-POR-KEYS = BUS-POR-CLTE
               MOVE RDC-CIC-ICLI IN RDC TO DBC-CIC-ICLI IN DBC
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM SGC-FIO-DBC
               MOVE FIO-STAT TO PGM-STAT-DBC
               IF NOT FIO-STAT-OKS
                   MOVE 'SGC'        TO MSG-COD-SIST
                   MOVE 'CLI    NEX' TO MSG-COD-MENS
                   MOVE SPACES TO DBC-IND-ICLI IN DBC
                   PERFORM SET-MSG-CLTE
                   GO TO CON-RST-STAT.

      *ISP: SI CYA-NUM-ICAN <> 0 ==> CANCELACION

           IF CYA-NUM-ICAN IN CYA NOT = ZEROES
                 PERFORM ANL-KEY-CAN
                 GO TO FIN-FND-KEY.

       CON-RST-STAT.
           IF NOT PGM-STAT-CYA-OKS AND
              ( SCR-CCPP = 'ACT' OR SCR-CCPP = 'ING' )
               PERFORM PUT-OPC-CYA
               PERFORM PUT-RDC-CYA
               PERFORM PUT-DBC-CYA.
           MOVE PGM-STAT-CYA TO FIO-STAT.
      *ALD
      *    IF NOT ( OPC-MSC-STAT IN OPC = 'L' OR 'A' )
      *

*% IF NOT PGM_PER
      *    IF OPC-MSC-STAT IN OPC NOT = 'A' AND
      *       OPC-MSC-STAT IN OPC NOT = 'D'
           IF ( NOT OPC-ACTIVADA ) AND ( NOT OPC-DESACTIVADA )
               GO TO FIN-FND-KEY.
*% END
*% IF PGM_PER
      *    IF OPC-MSC-STAT IN OPC NOT = 'A' AND
      *       OPC-MSC-STAT IN OPC NOT = 'D' AND
      *       OPC-MSC-STAT IN OPC NOT = 'F'
           IF ( NOT OPC-ACTIVADA ) AND ( NOT OPC-DESACTIVADA )
                                   AND ( NOT OPC-APROBADA-CTC )
               GO TO FIN-FND-KEY.
*% END
      *    IF OPC-MSC-STAT IN OPC = 'L'
      *         MOVE 'OPC    LIQYA' TO MSG-COD-MENS.

      *    IF OPC-MSC-STAT IN OPC = 'A'
           IF OPC-ACTIVADA
               MOVE 'OPC    ACAYA' TO MSG-COD-MENS.
      *    IF OPC-MSC-STAT IN OPC = 'D'
           IF OPC-DESACTIVADA
               MOVE 'OPC    DESYA' TO MSG-COD-MENS.
*% IF PGM_PER
      *    IF OPC-MSC-STAT IN OPC = 'F'
           IF OPC-APROBADA-CTC
               MOVE 'OPC    APDYA' TO MSG-COD-MENS.
*% END
           MOVE 'COL' TO MSG-COD-SIST.
           PERFORM GET-MSG.

           MOVE MSG-GLS-DESC TO CPT-STR1
           MOVE MSG-GLS-DATA TO CPT-STR2
           PERFORM CPT-BLKS
           MOVE CPT-STRN     TO FRM-MENS

           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF SCR_GFLD_VAL_FLD
      *<<<< SCR_GFLD_VAL_FLD
      *ISP ES NECESARIO TENER LOS DATOS DE LA OPERACION
      *    PARA LA VALIDACION DE CAMPOS
           MOVE CYA-CAI-IOPC IN CYA-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE CYA-IIC-IOPC IN CYA-FLD TO OPC-IIC-IOPC IN OPC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-OPC.
           IF NOT FIO-STAT-OKS
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'OPC    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO ERR-SCR-GFLD.
      *>>>>
*% END
*% IF FIN_SCR_VARI
      *<<<< FIN_SCR_VARI
       COPY COLWGENG.
      * ISP ES NECESARIO GUARDAR RQA CUANDO SE MUEVE POR PFS
       COPY GNSWGRQA REPLACING ==01  ADR-REQA== BY ==02  SCR-RQA1==.
       COPY GNSWGRQA REPLACING ==01  ADR-REQA== BY ==02  SCR-RQA2==.
      * ISP WSS-GLS-DAT1 SE DEJO AQUI, PUES SE OCUPA EN DIFERENTES
      *     INSTANCIAS DEL PROGRAMA
           02  WSS-GLS-DAT1                  PIC X(01).
      *>>>>
*% END
*% IF WSS
      *<<<< WSS
       COPY COLWGPMP.
      *>>>>
*% END
