*%! Codigo adicional
*% IF WSS
      *<<<< WSS
       COPY COLBRCAN.
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CAN-REQA==.
      *>>>>
*% END
*% IF PUT_SCR
      *<<<< PUT_SCR
           IF PGM-STAT-RCV-NUL
               MOVE PGM-SNUL TO PGM-STAT-CAN
           ELSE
           IF NOT PGM-STAT-RCV-OKS
               MOVE PGM-SRNF TO PGM-STAT-CAN
           ELSE
               MOVE RCV-CAI-IOPC IN RCV TO CAN-CAI-IOPC IN CAN
               MOVE RCV-IIC-IOPC IN RCV TO CAN-IIC-IOPC IN CAN
               MOVE RCV-NUM-ICAN IN RCV TO CAN-NUM-ICAN IN CAN
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM COL-FIO-CAN
               MOVE FIO-STAT TO PGM-STAT-CAN
               IF NOT FIO-STAT-OKS
                   MOVE 'COL'        TO MSG-COD-SIST
                   MOVE 'CAN    NEX' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC        TO FRM-MEN1
                   MOVE CAN-KEY-ICAN IN CAN TO FRM-MEN2
                   MOVE SCR-STAT-CON TO SCR-STAT.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY COLBFCAN.
       COPY GNSBVCOD.
       COPY GNSBVTAB.
      *>>>>
*% END
*%!---------------------------------------------------------------------
*% FRM = "QCV"
*% IF PFS_GET_CON OR PFS_SCR_GFLD
      *<<<< PFS_GET_CON OR PFS_SCR_GFLD
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 OR FRM-FFLD = FRM-FFLD-PF7 )
      *>>>>
*% END
*% IF PFS_SCR_ACC OR PFS_SCR_MOD
      *<<<< PFS_SCR_ACC OR PFS_SCR_MOD
           IF FRM-FFLD = FRM-FFLD-PF8 OR FRM-FFLD-PF7
               PERFORM VAL-NUM-KEY-{frm}
               PERFORM VAL-NUM-FLD-{frm}
               PERFORM RST-STAT
               PERFORM LEE-F7F8
               PERFORM PUT-SCR
               PERFORM SCR-TRL.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       LEE-F7F8 SECTION.
       INI-LEE-F7F8.
           IF FRM-FFLD = FRM-FFLD-PF8
               PERFORM LEE-NEXT
           ELSE
               PERFORM LEE-PREV.
           IF MSG-COD-MENS > SPACES
               MOVE 'COL' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS.
       FIN-LEE-F7F8.
           EXIT.

       LEE-NEXT SECTION.
       INI-LEE-NEXT.
           MOVE PGM-SNUL TO PGM-STAT-RCV.
           MOVE SPACES TO MSG-COD-MENS.
           PERFORM KEY-RCV-QCV.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-RCV.
           IF NOT FIO-STAT-OKS
               MOVE 'RCV    NOCURR' TO MSG-COD-MENS
               MOVE SCR-STAT-ABT TO SCR-STAT
               GO TO FIN-LEE-NEXT.
           MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM COL-FIO-RCV.
           IF NOT FIO-STAT-OKS
               MOVE 'RCV    NONXT' TO MSG-COD-MENS
               GO TO FIN-LEE-NEXT.
           MOVE FIO-STAT TO PGM-STAT-RCV.
       FIN-LEE-NEXT.
           EXIT.

       LEE-PREV SECTION.
       INI-LEE-PREV.
           MOVE PGM-SNUL TO PGM-STAT-RCV.
           MOVE SPACES TO MSG-COD-MENS.
           PERFORM KEY-RCV-QCV.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-RCV.
           IF NOT FIO-STAT-OKS
               MOVE 'RCV    NOCURR' TO MSG-COD-MENS
               MOVE SCR-STAT-ABT TO SCR-STAT
               GO TO FIN-LEE-PREV.
           MOVE FIO-GET-PRV TO FIO-CMND.
           PERFORM COL-FIO-RCV.
           IF NOT FIO-STAT-OKS
               MOVE 'RCV    NOPRV' TO MSG-COD-MENS
               GO TO FIN-LEE-PREV.
           MOVE FIO-STAT TO PGM-STAT-RCV.
       FIN-LEE-PREV.
           EXIT.

       RST-STAT SECTION.
       INI-RST-STAT.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
       FIN-RST-STAT.
           EXIT.
      *>>>>
*% END
