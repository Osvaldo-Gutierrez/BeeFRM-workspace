*%! Codigo adicional para archivo
*% IF FIN_PUT_PTI_PTI
      *<<<< FIN_PUT_PTI_PTI
           IF SCR-CMND = 'CON' AND PTI-COD-OFOC IN PTI-FLD = '*  '
               MOVE SPACES TO FRM-GLS-OFOC IN PTI-FLD.
           IF SCR-CMND = 'CON' AND PTI-COD-AOOC IN PTI-FLD = '*  '
               MOVE SPACES TO FRM-GLS-AOOC IN PTI-FLD.
      *>>>>
*% END
*% IF FIN_PUT_POC_PTI
      *<<<< FIN_PUT_POC_PTI
      *BUSCA GLOSA EN TABLAS
           IF POC-COD-CVTI IN PTI-FLD > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'VLR' TO TAB-COD-TTAB IN TAB
               MOVE 'I' TO WSS-COD-TVTI
               MOVE POC-COD-CVTI IN PTI-FLD TO WSS-COD-CVTI
               MOVE POC-MSC-PBTI IN PTI-FLD TO WSS-COD-TCTI
               MOVE WSS-COD-VCAM TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               IF POC-COD-CVTI IN PTI-FLD > SPACES
                   MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-CVTI IN PTI-FLD
               ELSE
                   MOVE SPACES TO FRM-GLS-CVTI IN PTI-FLD.
      *BUS-MSC BUSCA GLOSA DE MISCELANEO EN TABLA
           IF POC-MSC-UTPR IN PTI-FLD > SPACES
               MOVE 'TAB' TO MSC-COD-SIST
               MOVE 'IPER' TO MSC-COD-TMSC IN MSC
               MOVE POC-MSC-UTPR IN PTI-FLD TO MSC-COD-CMSC IN MSC
               PERFORM BUS-MSC
               MOVE MSC-GLS-DCOR IN MSC TO FRM-GLS-UTPR IN PTI-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-UTPR IN PTI-FLD.
      *>>>>
*% END
*% IF INI_PTI_FEC_FTRN
      *<<<< INI_PTI_FEC_FTRN
           MOVE FEC-CHOY-N TO FEC-CHOY.
      *>>>>
*% END
*% IF PTI_FEC_FTRN
      *<<<< PTI_FEC_FTRN
           IF FRM-SUAR = FRM-SUAR-MAL
               GO TO FIN-VAL-PTI-FEC-FTRN.
           IF PTI-FEC-FTRN IN PTI-FLD NOT > ZEROES AND
              ( SCR-CMND NOT = 'OFC' AND SCR-CMND NOT = 'DFC' )
               PERFORM GET-FHOY
               MOVE HOY-DHOY IN HOY-VARI TO PTI-NUM-DTRN IN PTI-FLD
               MOVE HOY-MHOY IN HOY-VARI TO PTI-NUM-MTRN IN PTI-FLD
               MOVE HOY-SHOY IN HOY-VARI TO PTI-NUM-STRN IN PTI-FLD
               MOVE HOY-AHOY IN HOY-VARI TO PTI-NUM-ATRN IN PTI-FLD.
      *>>>>
*% END
*% IF PTI_HRA_HRTR
      *<<<< PTI_HRA_HRTR
           MOVE PTI-HRA-HRTR IN PTI-FLD TO PTI-HRA-HRTR IN PTI.
           IF PTI-NUM-HHTR IN PTI < ZEROES OR
              PTI-NUM-HHTR IN PTI > 24
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'NUMHHTRNOVA' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-PTI-HRA-HRTR.
           IF PTI-NUM-MMTR IN PTI < ZEROES OR
              PTI-NUM-MMTR IN PTI > 60
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'NUMMMTRNOVA' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-PTI-HRA-HRTR.
      *>>>>
*% END
*% IF INI_PTI_COD_OFOC
      *<<<< INI_PTI_COD_OFOC
           IF ( SCR-CMND NOT = 'CON' ) AND
              ( PTI-COD-OFOC IN PTI-FLD = '*  ' )
               GO TO FIN-VAL-PTI-COD-OFOC.
      *>>>>
*% END
*% IF PTI_COD_AOOC
      *<<<< PTI_COD_AOOC
           IF ( SCR-CMND NOT = 'CON' ) AND
              ( PTI-COD-AOOC IN PTI-FLD = '*  ' )
               GO TO FIN-VAL-PTI-COD-AOOC.
      *VAL-COD Valida codigo de tabla
           MOVE 'COL' TO TAB-COD-SIST.
           MOVE 'AUX' TO TAB-COD-TTAB IN TAB.
           MOVE PTI-COD-TOOC IN PTI-FLD TO WSS-COD-TOOC IN WSS-VARI.
           MOVE PTI-COD-AOOC IN PTI-FLD TO WSS-COD-AOOC IN WSS-VARI.
           MOVE WSS-COD-CAUX IN WSS-VARI TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT = 'COD    NEX' AND
              PTI-COD-AOOC IN PTI-FLD > SPACES
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-AOOC IN PTI-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-AOOC IN PTI-FLD.
           IF FRM-SUAR = FRM-SUAR-MAL
               GO TO FIN-VAL-PTI-COD-AOOC.
           MOVE PTI-COD-VCOC IN PTI-FLD TO POC-COD-VCOC IN POC.
           MOVE PTI-COD-TOOC IN PTI-FLD TO POC-COD-TOOC IN POC.
           MOVE PTI-COD-AOOC IN PTI-FLD TO POC-COD-AOOC IN POC.
           MOVE ZEROES                  TO POC-NUM-IPOC IN POC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-POC.
           IF NOT FIO-STAT-OKS
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'POC    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               PERFORM EDT-FLD-PTI
               GO TO FIN-VAL-PTI-COD-AOOC.
           IF POC-MSC-STAT IN POC = 'N'
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'POC    NVG' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               PERFORM EDT-FLD-PTI
               GO TO FIN-VAL-PTI-COD-AOOC.
      *>>>>
*% END
*%! Codigo adicional para archivo
*% IF FIN_FST_KEY_PTI
      *<<<< FIN_FST_KEY_PTI
           IF SCR-CMND = 'OFC' OR 'DFC'
               MOVE 0  TO PTI-FEC-FTRN-LEN IN PTI-FLD
               MOVE -1 TO PTI-COD-OFOC-LEN IN PTI-FLD.
      *>>>>
*% END
*% IF FIN_FST_FLD_PTI
      *<<<< FIN_FST_FLD_PTI
           IF SCR-CMND = 'OFC' OR 'DFC'
               MOVE 0  TO FRM-SGV-MTO1-LEN IN PTI-FLD
               MOVE -1 TO PTI-COD-OFCB-LEN IN PTI-FLD.
      *>>>>
*% END
*% IF FRM_SGV_MTO2
      *<<<< FRM_SGV_MTO2
           IF FRM-SGV-MTO2 IN PTI-FLD < FRM-SGV-MTO1 IN PTI-FLD
              AND SCR-CMND NOT = 'OFC'
              AND FRM-SGV-MTO2 IN PTI-FLD > ZEROES
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'MTOMENORANT' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-FRM-SGV-MTO2.
      *>>>>
*% END
*% IF FRM_SGV_MTO3
      *<<<< FRM_SGV_MTO3
           IF FRM-SGV-MTO3 IN PTI-FLD < FRM-SGV-MTO2 IN PTI-FLD
              AND SCR-CMND NOT = 'OFC'
              AND FRM-SGV-MTO3 IN PTI-FLD > ZEROES
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'MTOMENORANT' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-FRM-SGV-MTO3.
           IF FRM-SGV-MTO3 IN PTI-FLD > ZEROES
              AND SCR-CMND NOT = 'OFC'
              AND FRM-SGV-MTO2 IN PTI-FLD NOT > ZEROES
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'MONTOINVALI' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-FRM-SGV-MTO3.
      *>>>>
*% END
*% IF FRM_SGV_MTO3
      *<<<< FRM_SGV_MTO3
           IF FRM-SGV-MTO4 IN PTI-FLD < FRM-SGV-MTO3 IN PTI-FLD
              AND SCR-CMND NOT = 'OFC'
              AND FRM-SGV-MTO4 IN PTI-FLD > ZEROES
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'MTOMENORANT' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-FRM-SGV-MTO4.
           IF FRM-SGV-MTO4 IN PTI-FLD > ZEROES
              AND SCR-CMND NOT = 'OFC'
              AND FRM-SGV-MTO3 IN PTI-FLD NOT > ZEROES
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'MONTOINVALI' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-FRM-SGV-MTO4.
      *>>>>
*% END
*% IF FRM_NUM_NDIA
      *<<<< FRM_NUM_NDIA
           IF FRM-IFLD = 1 OR
              SCR-CMND = 'OFC'
               GO TO FIN-VAL-FRM-NUM-NDIA.
           SUBTRACT 1 FROM FRM-IFLD GIVING WSS-J IN WSS-VARI.
           IF FRM-NUM-NDIA IN PTI-FLD(FRM-IFLD) < FRM-NUM-NDIA IN
              PTI-FLD(WSS-J IN WSS-VARI) AND
              FRM-NUM-NDIA IN PTI-FLD(FRM-IFLD) > ZEROES
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'DIAMENORANT' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-FRM-NUM-NDIA.
           IF FRM-IFLD > 2 AND
              FRM-NUM-NDIA IN PTI-FLD(WSS-J IN WSS-VARI) NOT > ZEROES
              AND FRM-NUM-NDIA IN PTI-FLD(FRM-IFLD) > ZEROES
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'DIAINVALIDO' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-FRM-NUM-NDIA.
           IF FRM-NUM-NDIA IN PTI-FLD(FRM-IFLD) = FRM-NUM-NDIA IN
              PTI-FLD(WSS-J IN WSS-VARI) AND
              FRM-NUM-NDIA IN PTI-FLD(FRM-IFLD) NOT = ZEROES
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'DIAINVALIDO' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-FRM-NUM-NDIA.
      *>>>>
*% END
*% IF FRM_SGV_INT1
      *<<<< FRM_SGV_INT1
           IF FRM-IFLD = 1 OR
              SCR-CMND = 'OFC'
               GO TO FIN-VAL-FRM-SGV-INT1.
           IF FRM-NUM-NDIA IN PTI-FLD(FRM-IFLD) NOT > ZEROES
              AND FRM-SGV-INT1 IN PTI-FLD(FRM-IFLD) > ZEROES
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'TASAINVALID' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-FRM-SGV-INT1.
      *>>>>
*% END
*% IF FRM_SGV_INT2
      *<<<< FRM_SGV_INT2
           IF SCR-CMND = 'OFC'
               GO TO FIN-VAL-FRM-SGV-INT2.
           IF FRM-IFLD = 1
              IF FRM-SGV-MTO2 IN PTI-FLD NOT > ZEROES
                 AND FRM-SGV-INT2 IN PTI-FLD(FRM-IFLD) > ZEROES
                  MOVE 'COL' TO MSG-COD-SIST
                  MOVE 'TASAINVALID' TO MSG-COD-MENS
                  PERFORM GET-MSG
                  MOVE MSG-GLS-DESC TO FRM-MENS
                  MOVE FRM-SUAR-MAL TO FRM-SUAR
                  GO TO FIN-VAL-FRM-SGV-INT2
               ELSE
                   GO TO FIN-VAL-FRM-SGV-INT2.
           IF (FRM-NUM-NDIA IN PTI-FLD(FRM-IFLD) NOT > ZEROES
              OR FRM-SGV-MTO2 IN PTI-FLD NOT > ZEROES )
              AND FRM-SGV-INT2 IN PTI-FLD(FRM-IFLD) > ZEROES
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'TASAINVALID' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-FRM-SGV-INT2.
      *>>>>
*% END
*% IF FRM_SGV_INT3
      *<<<< FRM_SGV_INT3
           IF SCR-CMND = 'OFC'
               GO TO FIN-VAL-FRM-SGV-INT3.
           IF FRM-IFLD = 1
              IF FRM-SGV-MTO3 IN PTI-FLD NOT > ZEROES
                 AND FRM-SGV-INT3 IN PTI-FLD(FRM-IFLD) > ZEROES
                  MOVE 'COL' TO MSG-COD-SIST
                  MOVE 'TASAINVALID' TO MSG-COD-MENS
                  PERFORM GET-MSG
                  MOVE MSG-GLS-DESC TO FRM-MENS
                  MOVE FRM-SUAR-MAL TO FRM-SUAR
                  GO TO FIN-VAL-FRM-SGV-INT3
              ELSE
                  GO TO FIN-VAL-FRM-SGV-INT3.
           IF (FRM-NUM-NDIA IN PTI-FLD(FRM-IFLD) NOT > ZEROES
              OR FRM-SGV-MTO3 IN PTI-FLD NOT > ZEROES )
              AND FRM-SGV-INT3 IN PTI-FLD(FRM-IFLD) > ZEROES
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'TASAINVALID' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-FRM-SGV-INT3.
      *>>>>
*% END
*% IF FRM_SGV_INT4
      *<<<< FRM_SGV_INT4
           IF SCR-CMND = 'OFC'
               GO TO FIN-VAL-FRM-SGV-INT4.
           IF FRM-IFLD = 1
              IF FRM-SGV-MTO4 IN PTI-FLD NOT > ZEROES
                 AND FRM-SGV-INT4 IN PTI-FLD(FRM-IFLD) > ZEROES
                  MOVE 'COL' TO MSG-COD-SIST
                  MOVE 'TASAINVALID' TO MSG-COD-MENS
                  PERFORM GET-MSG
                  MOVE MSG-GLS-DESC TO FRM-MENS
                  MOVE FRM-SUAR-MAL TO FRM-SUAR
                  GO TO FIN-VAL-FRM-SGV-INT4
              ELSE
                  GO TO FIN-VAL-FRM-SGV-INT4.
           IF (FRM-NUM-NDIA IN PTI-FLD(FRM-IFLD) NOT > ZEROES
              OR FRM-SGV-MTO4 IN PTI-FLD NOT > ZEROES )
              AND FRM-SGV-INT4 IN PTI-FLD(FRM-IFLD) > ZEROES
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'TASAINVALID' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-FRM-SGV-INT4.
      *>>>>
*% END
*% IF FRM_SGV_INTM
      *<<<< FRM_SGV_INTM
           IF SCR-CMND = 'OFC'
               GO TO FIN-VAL-FRM-SGV-INTM.
           IF FRM-IFLD = 1
               GO TO FIN-VAL-FRM-SGV-INTM.
           IF FRM-NUM-NDIA IN PTI-FLD(FRM-IFLD) NOT > ZEROES
              AND FRM-SGV-INTM IN PTI-FLD(FRM-IFLD) > ZEROES
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'TASAINVALID' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-FRM-SGV-INTM.
      *>>>>
*% END
*% IF INI_VAL_PTI_COD_OFCB
      *<<<< INI_VAL_PTI_COD_OFCB
           IF SCR-CMND NOT = 'OFC'
               MOVE SPACES TO FRM-GLS-OFCB IN PTI-FLD
               GO TO FIN-VAL-PTI-COD-OFCB.
      *>>>>
*% END
*% IF PTI_MSC_STAT
      *<<<< PTI_MSC_STAT
           IF SCR-CMND = 'OFC'
               GO TO FIN-VAL-PTI-MSC-STAT.
      *>>>>
*% END
*% IF VAL_CON_FLD_PTI
      *<<<< VAL_CON_FLD_PTI
           IF SCR-CMND = 'OFC'
               GO TO FIN-VAL-CON-FLD-PTI.
           IF PTI-COD-AOOC IN PTI-FLD = '*  '
               GO TO FIN-VAL-CON-FLD-PTI.
           MOVE PTI-COD-VCOC IN PTI-FLD TO POC-COD-VCOC IN POC.
           MOVE PTI-COD-TOOC IN PTI-FLD TO POC-COD-TOOC IN POC.
           MOVE PTI-COD-AOOC IN PTI-FLD TO POC-COD-AOOC IN POC.
           MOVE ZEROES                  TO POC-NUM-IPOC IN POC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-POC.
           IF FRM-SGV-MTO4 IN PTI-FLD > ZEROES
               IF FRM-SGV-MTO4 IN PTI-FLD < POC-VAL-CMAX IN POC
                   MOVE 'COL' TO MSG-COD-SIST
                   MOVE 'MTO4<POCCMAX' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   GO TO FIN-VAL-CON-FLD-PTI
               ELSE
                   GO TO END-VAL-CON-FLD-PTI.
           IF FRM-SGV-MTO3 IN PTI-FLD > ZEROES
               IF FRM-SGV-MTO3 IN PTI-FLD < POC-VAL-CMAX IN POC
                   MOVE 'COL' TO MSG-COD-SIST
                   MOVE 'MTO3<POCCMAX' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   GO TO FIN-VAL-CON-FLD-PTI
               ELSE
                   GO TO END-VAL-CON-FLD-PTI.
           IF FRM-SGV-MTO2 IN PTI-FLD > ZEROES
               IF FRM-SGV-MTO2 IN PTI-FLD < POC-VAL-CMAX IN POC
                   MOVE 'COL' TO MSG-COD-SIST
                   MOVE 'MTO2<POCCMAX' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   GO TO FIN-VAL-CON-FLD-PTI
               ELSE
                   GO TO END-VAL-CON-FLD-PTI.
           IF FRM-SGV-MTO1 IN PTI-FLD < POC-VAL-CMAX IN POC
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'MTO1<POCCMAX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CON-FLD-PTI.
       END-VAL-CON-FLD-PTI.
           MOVE WSS-IMAX IN WSS-VARI TO WSS-I IN WSS-VARI.
       LOOP-VAL-NDIA.
           IF FRM-NUM-NDIA IN PTI-FLD(WSS-I) > ZEROES
               IF FRM-NUM-NDIA IN PTI-FLD(WSS-I) < POC-NUM-PMAX IN POC
                   MOVE 'COL' TO MSG-COD-SIST
                   MOVE 'ULTPZO<PMAX' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   GO TO FIN-VAL-CON-FLD-PTI
               ELSE
                   GO TO FIN-VAL-CON-FLD-PTI.
           SUBTRACT 1 FROM WSS-I IN WSS-VARI.
           IF WSS-I IN WSS-VARI = 1
              IF FRM-NUM-NDIA IN PTI-FLD(WSS-I) < POC-NUM-PMAX IN POC
                  MOVE 'COL' TO MSG-COD-SIST
                  MOVE 'ULTPZO<PMAX' TO MSG-COD-MENS
                  PERFORM GET-MSG
                  MOVE MSG-GLS-DESC TO FRM-MENS
                  MOVE FRM-SUAR-MAL TO FRM-SUAR
                  GO TO FIN-VAL-CON-FLD-PTI
              ELSE
                  GO TO FIN-VAL-CON-FLD-PTI.
           GO TO LOOP-VAL-NDIA.
      *>>>>
*% END
