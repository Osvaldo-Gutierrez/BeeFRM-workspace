*%! Codigo adicional para archivo
*% FRM = "QVE"
*%
*% PADRE = "OPC"
*% KEY_PADRE = "OPC-KEY-IOPC"
*% LARGO_KEY_PADRE = 12
*% RELACION_PADRE_HIJO = "OPC-CIC-IOPC"
*% LARGO_RELACION_PADRE_HIJO = 12
*% FIO_PADRE = "COL-FIO-OPC"
*% SIST_PADRE = "COL"
*% KEY_BROWSE_PADRE = "OPC-KEY-IOPC"
*%
*% HIJO  = "VEN"
*% KEY_HIJO = "VEN-KEY-IVEN"
*% LARGO_KEY_HIJO = 15
*% RELACION_HIJO_PADRE = "VEN-CIC-IOPC"
*% FIO_HIJO = "COL-FIO-VEN"
*% SIST_HIJO = "COL"
*% KEY_BROWSE_HIJO = "VEN-KEY-IVEN"
*%
*% ACEPTE_PF5 = TRUE
*% ACEPTE_PF6 = TRUE
*% ACEPTE_PF7 = TRUE
*% ACEPTE_PF8 = TRUE
*% INICIALIZAR_VARIABLES = TRUE
*% COLAR_HIJOS = FALSE
*% LEER_TODOS_LOS_HIJOS = TRUE
*% NUMERO_LINEAS_TABLA = 10
*%
*% INCLUDE 'GNSSRC:PFSCONIDX.SRC'
*% IF WSS AND PGM_DTC
      *<<<< WSS AND PGM_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-VEN-REQA==.
      *>>>>
*% END
*% IF WSS
      *<<<< WSS
       COPY COLBRVEN.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       CARGAR-TABLA SECTION.
       INI-CARGAR-TABLA.
*% IF COLAR_HIJOS = TRUE
           IF VEN-NUM-IVCT IN VEN = 1 OR 12
               MOVE 'N' TO PFS-IVEN-SEL
               GO TO FIN-CARGAR-TABLA.
*% END
           MOVE VEN-NUM-IVEN IN VEN TO
                FRM-NUM-IVEN IN QVE-FLD(FRM-IFLD).
           MOVE VEN-NUM-IVCT IN VEN TO
                FRM-NUM-IVCT IN QVE-FLD(FRM-IFLD).
      *DAD ini  17-JUL-1992
           MOVE VEN-IND-SCTB IN VEN TO
                FRM-IND-SCTB IN QVE-FLD(FRM-IFLD).
      *DAD fin  17-JUL-1992
           MOVE VEN-NUM-DVEN IN VEN TO
                FRM-NUM-DVEN IN QVE-FLD(FRM-IFLD).
           MOVE VEN-NUM-MVEN IN VEN TO
                FRM-NUM-MVEN IN QVE-FLD(FRM-IFLD).
           MOVE VEN-NUM-SVEN IN VEN TO
                FRM-NUM-SVEN IN QVE-FLD(FRM-IFLD).
           MOVE VEN-NUM-AVEN IN VEN TO
                FRM-NUM-AVEN IN QVE-FLD(FRM-IFLD).
           MOVE VEN-VAL-CAPI IN VEN TO
                FRM-VAL-CAPI IN QVE-FLD(FRM-IFLD).
           MOVE VEN-VAL-SCAP IN VEN TO
                FRM-VAL-SCAP IN QVE-FLD(FRM-IFLD).
           MOVE VEN-VAL-FINA IN VEN TO
                FRM-VAL-FINA IN QVE-FLD(FRM-IFLD).
           MOVE VEN-VAL-SFIN IN VEN TO
                FRM-VAL-SFIN IN QVE-FLD(FRM-IFLD).
           MOVE VEN-IND-CALC IN VEN TO
                FRM-IND-CALC IN QVE-FLD(FRM-IFLD).
       FIN-CARGAR-TABLA.
           EXIT.

       PROCESO-CADA-HIJO SECTION.
       INI-PROCESO-CADA-HIJO.
           ADD 1                   TO FRM-NUM-VGRA IN QVE-FLD.
           ADD VEN-VAL-FINA IN VEN TO FRM-VAL-TFIN IN QVE-FLD.
           ADD VEN-VAL-SFIN IN VEN TO FRM-VAL-TSFI IN QVE-FLD.
       FIN-PROCESO-CADA-HIJO.
           EXIT.

       LIMPIAR-TABLA SECTION.
       INI-LIMPIAR-TABLA.
           MOVE ZEROES TO FRM-NUM-IVEN IN QVE-FLD(FRM-IFLD).
           MOVE ZEROES TO FRM-NUM-IVCT IN QVE-FLD(FRM-IFLD).
      *DAD ini  17-JUL-1992
           MOVE SPACES TO FRM-IND-SCTB IN QVE-FLD(FRM-IFLD).
      *DAD fin  17-JUL-1992
           MOVE ZEROES TO FRM-NUM-DVEN IN QVE-FLD(FRM-IFLD).
           MOVE ZEROES TO FRM-NUM-MVEN IN QVE-FLD(FRM-IFLD).
           MOVE ZEROES TO FRM-NUM-SVEN IN QVE-FLD(FRM-IFLD).
           MOVE ZEROES TO FRM-NUM-AVEN IN QVE-FLD(FRM-IFLD).
           MOVE ZEROES TO FRM-VAL-CAPI IN QVE-FLD(FRM-IFLD).
           MOVE ZEROES TO FRM-VAL-SCAP IN QVE-FLD(FRM-IFLD).
           MOVE ZEROES TO FRM-VAL-FINA IN QVE-FLD(FRM-IFLD).
           MOVE ZEROES TO FRM-VAL-SFIN IN QVE-FLD(FRM-IFLD).
           MOVE SPACES TO FRM-IND-CALC IN QVE-FLD(FRM-IFLD).
       FIN-LIMPIAR-TABLA.
           EXIT.

       INIT-VARI SECTION.
       INI-INIT-VARI.
           MOVE 0 TO FRM-NUM-VGRA IN QVE-FLD.
           MOVE 0 TO FRM-VAL-TFIN IN QVE-FLD.
           MOVE 0 TO FRM-VAL-TSFI IN QVE-FLD.
       FIN-INIT-VARI.
           EXIT.

       COPY COLBFVEN.
      *>>>>
*% END
