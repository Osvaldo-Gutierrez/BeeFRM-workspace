*%! Codigo adicional para archivo
*% RA1 = "CUF"
*% FRM = "CUF"
*% NRA = 1
*% INCLUDE 'PFSALLF78'
*% IF WSS
      *<<<< WSS
       01  WSS-VARI.
           03  WSS-FEC-FCOM.
               05  WSS-NUM-SCOM                       PIC 9(02).
               05  WSS-NUM-ACOM                       PIC 9(02).
               05  WSS-NUM-MCOM                       PIC 9(02).
               05  WSS-NUM-DCOM                       PIC 9(02).
           03  BUS-POR-KEYS             VALUE SPACES  PIC X(01).
           03  BUS-POR-CLIE             VALUE 'C'     PIC X(01).
           03  WSS-ICOR                 VALUE 0       PIC 9(01).
               88 NO-ASIGNE-CORRELATIVO VALUE 0.
           03 WSS-ITEM.
              05 FILLER                               PIC  X(11).
              05 WSS-OFIC                             PIC  X(03).
           03  WSS-UBFD.
               05  WSS-TAB-UBFD                       PIC X(03).
               05  WSS-COD-UBFD.
                   07  WSS-GLS-UFD3                   PIC X(03).
                   07  WSS-GLS-UFD9                   PIC X(09).
      *     03  PGM-STAT-DLC           VALUE '23'     PIC X(02).
      *         88 PGM-STAT-DLC-OKS    VALUE '00'.
      *         88 PGM-STAT-DLC-NUL    VALUE '  '.
           03 WSS-FEC-FEC1.
              05 WSS-NUM-SEC1                         PIC 9(02).
              05 WSS-NUM-AEC1                         PIC 9(02).
              05 WSS-NUM-MEC1                         PIC 9(02).
              05 WSS-NUM-DEC1                         PIC 9(02).
           03 WSS-FEC-FEC2.
              05 WSS-NUM-SEC2                         PIC 9(02).
              05 WSS-NUM-AEC2                         PIC 9(02).
              05 WSS-NUM-MEC2                         PIC 9(02).
              05 WSS-NUM-DEC2                         PIC 9(02).
*% IF PGM_CHI AND PGM_BCI
      *LRH ini 15-MAR-1994 (Memo.23-NOV-1993, punto 2)
           03 WSS-DLC-COD-PLZA.
              05 WSS-COD-PLZA                         PIC X(01).
              05 WSS-NUM-PLZA                         PIC X(03).
      *LRH fin 15-MAR-1994
*% END

       COPY GNSWGSTR.
       COPY SGCBRDBC.
       COPY COLBROPC.
       COPY COLBRDLC.
       COPY COLBRRDC.
       COPY GNSWGCPT.
       COPY GNSWGOPI.
       COPY GNSWGQUE.
*% IF PGM_CHI AND PGM_BCI
      *LRH ini 15-MAR-1994 (Memo.23-NOV-1993, punto 2)
           COPY USPC113.
      *LRH fin 15-MAR-1994
*% END
      *>>>>
*% END
*% IF WSS_DTC 
      *<<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OPC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DLC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RDC-REQA==.
      *>>>>
*% END
*% IF FND_KEY
      *<<<< FND_KEY
*% IF PGM_CHI AND PGM_BCI
      *LRH ini 25-ABR-1994 (Memo.25-ABR-1994, punto 1)
           IF SCR-CMND NOT = 'CON'
      *LRH fin 25-ABR-1994 
      *LRH ini 15-MAR-1994 (Memo.23-NOV-1993, punto 2)
               MOVE 'COL'       TO FIO-SIST
               MOVE 'VSN'       TO TAB-COD-TTAB IN TAB
               MOVE 'OFIUSPCUF' TO TAB-COD-CTAB IN TAB
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM GNS-FIO-TAB
               IF FIO-STAT-OKS AND
                  TAB-MSC-STAT IN TAB = 'S'
                   PERFORM VAL-OFI-USP
                   IF SCR-STAT = SCR-STAT-CON
                       GO TO FIN-FND-KEY.
      *LRH fin 15-MAR-1994
*% END
           IF SCR-CCPP = 'ING' OR 'ELI'
               PERFORM BUS-COR-CUF
               PERFORM CLR-CUF
               GO TO CON-FND-KEY.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8
               PERFORM LEE-NXT-PRV
               IF FIO-STAT-OKS
                   PERFORM SCR-CLRA-CUF
                   GO TO CON-FND-KEY
               ELSE
                   MOVE SPACES TO PGM-STAT
                   IF FRM-FFLD = FRM-FFLD-PF7
                       MOVE 'COL' TO MSG-COD-SIST
                       MOVE 'CUF    NOPRV' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE '00' TO FIO-STAT
                       MOVE SCR-STAT-CON TO SCR-STAT
                       GO TO FIN-FND-KEY
                   ELSE
                       MOVE 'COL' TO MSG-COD-SIST
                       MOVE 'CUF    NONXT' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE '00' TO FIO-STAT
                       MOVE SCR-STAT-CON TO SCR-STAT
                       GO TO FIN-FND-KEY.
           IF CUF-IIC-IOPC IN CUF NOT > ZEROES
               PERFORM ANL-KEY-CUF
               IF NOT FIO-STAT-OKS
                   GO TO NXT-FND-KEY.
           IF CUF-NUM-ICUF IN CUF NOT > ZEROES 
               PERFORM BUS-COR-CUF
               PERFORM CLR-CUF.
       CON-FND-KEY.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
           IF SCR-CCPP = 'ACT' AND NO-ASIGNE-CORRELATIVO AND 
              NOT FIO-STAT-OKS
               PERFORM BUS-COR-CUF
               PERFORM CLR-CUF
               MOVE '23' TO FIO-STAT.

           IF FIO-STAT-OKS AND (SCR-CCPP = 'MOD' OR SCR-CCPP = 'ELI' OR 
              (SCR-CCPP = 'ACT' AND SCR-ACTU = SCR-ACTU-MOD))
                PERFORM BUS-SIG-CUF
                IF FIO-STAT-OKS
                    MOVE 'COL' TO MSG-COD-SIST
                    MOVE 'NUMICUFINV' TO MSG-COD-MENS
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    MOVE '00' TO FIO-STAT
                    MOVE SCR-STAT-CON TO SCR-STAT
                    GO TO FIN-FND-KEY
                ELSE
                    MOVE '00' TO FIO-STAT.

           IF FIO-STAT-OKS
               MOVE ADR-CUF-REQA TO SCR-CUF-REQA.
           IF  BUS-POR-KEYS = BUS-POR-CLIE
                IF FIO-STAT-OKS
                    PERFORM BUS-CUF-UFAD
                    MOVE '00' TO FIO-STAT
                    GO TO NXT-FND-KEY
                ELSE
                    PERFORM BUS-CUF-UFAD
                    MOVE '23' TO FIO-STAT
                    GO TO NXT-FND-KEY.
           IF FIO-STAT-OKS
               PERFORM BUS-CUF-UFAD
               PERFORM ANL-CIC-IOPC
               GO TO NXT-FND-KEY.
           IF ( SCR-CCPP = 'ING' OR SCR-CCPP = 'ACT' )
               PERFORM BUS-CUF-UFAD
               PERFORM ANL-CIC-IOPC
               IF FIO-STAT-OKS
                   MOVE '23' TO FIO-STAT
               ELSE
                   MOVE SCR-STAT-CON TO SCR-STAT
                   MOVE '00' TO FIO-STAT.
       NXT-FND-KEY.
           IF PGM-STAT-OPC-OKS AND 
              OPC-MSC-STAT IN OPC NOT = 'A'
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'OPC    NOACA' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE SCR-STAT-CON TO SCR-STAT
               IF  SCR-CCPP = 'ING'
                   MOVE '23' TO FIO-STAT
               ELSE
                   MOVE '00' TO FIO-STAT.
      *>>>>
*% END
*% IF FIN_CMN_ING
      *<<<< FIN_CMN_ING
           IF NOT FIO-STAT-OKS
      *        Error inconsistencia en el ingreso : informar
               MOVE FIO-MENS TO FRM-MENS
               GO TO FIN-CMN-ING.
           PERFORM MOD-DLC.
           IF NOT FIO-STAT-OKS
               PERFORM BCK-OUT
               GO TO FIN-CMN-ING.
      *>>>>
*% END
*% IF FIN_CMN_MOD
      *<<<< FIN_CMN_MOD
           IF NOT FIO-STAT-OKS
      *        Error inconsistencia en el ingreso : informar
               MOVE FIO-MENS TO FRM-MENS
               GO TO FIN-CMN-MOD.
           PERFORM MOD-DLC.
           IF NOT FIO-STAT-OKS
               PERFORM BCK-OUT
               GO TO FIN-CMN-MOD.
      *>>>>
*% END
*% IF FIN_CMN_ELI
      *<<<< FIN_CMN_ELI
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ELI.
           PERFORM KEY-CUF-CUF.
           SUBTRACT 1 FROM CUF-NUM-ICUF IN CUF.
           IF CUF-NUM-ICUF IN CUF > ZEROES
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM COL-FIO-CUF
               IF FIO-STAT-OKS
                   NEXT SENTENCE
               ELSE
                   MOVE 'COL' TO MSG-COD-SIST
                   MOVE 'CUF-1    NEX' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   GO TO FIN-CMN-ELI
           ELSE
               MOVE SPACES TO CUF-COD-UBFD IN CUF.
           MOVE CUF-COD-UBFD IN CUF TO CUF-COD-UBFD IN CUF-FLD.
           PERFORM MOD-DLC.
           IF NOT FIO-STAT-OKS
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'DLC    NOMOD' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS.
      *>>>>
*% END
*% IF FIN_HBL_FLD
      *<<<< FIN_HBL_FLD
      *BUS-USR busca usuario en tablas
           IF SCR-USER > SPACES
               MOVE SCR-USER TO USR-COD-USER IN USR
                                CUF-COD-RCUF IN CUF-FLD
               PERFORM BUS-USR
               MOVE USR-GLS-DESC IN USR TO FRM-GLS-RCUF IN CUF-FLD.
*% IF PGM_CHI AND PGM_BCI
      *LRH ini 15-MAR-1994 (Memo.23-NOV-1993, punto 2)
           PERFORM GET-FHOY.
           MOVE HOY-DHOY     TO CUF-NUM-DCUF IN CUF-FLD.
           MOVE HOY-MHOY     TO CUF-NUM-MCUF IN CUF-FLD.
           MOVE HOY-SHOY     TO CUF-NUM-SCUF IN CUF-FLD.
           MOVE HOY-AHOY     TO CUF-NUM-ACUF IN CUF-FLD.
           MOVE FRM-CPIM-AEY TO CUF-FEC-FCUF-ATR IN CUF-FLD.
      *LRH fin 15-MAR-1994
*% END
      *>>>>
*% END
*% IF INI_SCR_TRL
      *<<<< INI_SCR_TRL
      *DAD
      *BUS-USR busca usuario en tablas
           IF SCR-STAT NOT = SCR-STAT-CON
               IF SCR-USER > SPACES
                   MOVE SCR-USER TO CUF-COD-RCUF IN CUF-FLD
                   MOVE SCR-USER TO CUF-COD-RCUF IN CUF
                   MOVE SCR-USER TO USR-COD-USER IN USR
                   PERFORM BUS-USR
                   MOVE USR-GLS-DESC IN USR TO FRM-GLS-RCUF IN CUF-FLD.
      *
      *>>>>
*% END
*% IF EOF_PQ
      *<<<< EOF_PQ
       RET-ALL-CUF SECTION.
       INI-RET-ALL-CUF.
           PERFORM KEY-ALL-CUF.
           PERFORM RET-OPC-CUF.
           PERFORM RET-CUF-CUF.
           PERFORM RET-DLC-CUF.
       FIN-RET-ALL-CUF.
           EXIT.

       RET-OPC-CUF SECTION.
       INI-RET-OPC-CUF.
           MOVE OPC-MSC-TVOC IN CUF-FLD TO OPC-MSC-TVOC IN OPC.
           MOVE OPC-COD-TOOC IN CUF-FLD TO OPC-COD-TOOC IN OPC.
           MOVE OPC-COD-OFOC IN CUF-FLD TO OPC-COD-OFOC IN OPC.
       FIN-RET-OPC-CUF.
           EXIT.
       
       RET-CUF-CUF SECTION.
       INI-RET-CUF-CUF.
           MOVE CUF-COD-UFAD IN CUF-FLD TO CUF-COD-UFAD IN CUF.
           MOVE CUF-NUM-DCUF IN CUF-FLD TO CUF-NUM-DCUF IN CUF.
           MOVE CUF-NUM-MCUF IN CUF-FLD TO CUF-NUM-MCUF IN CUF.
           MOVE CUF-NUM-SCUF IN CUF-FLD TO CUF-NUM-SCUF IN CUF.
           MOVE CUF-NUM-ACUF IN CUF-FLD TO CUF-NUM-ACUF IN CUF.
*% IF ( NOT PGM_CHI ) OR ( NOT PGM_BCI )
           MOVE CUF-NUM-DING IN CUF-FLD TO CUF-NUM-DING IN CUF.
           MOVE CUF-NUM-MING IN CUF-FLD TO CUF-NUM-MING IN CUF.
           MOVE CUF-NUM-SING IN CUF-FLD TO CUF-NUM-SING IN CUF.
           MOVE CUF-NUM-AING IN CUF-FLD TO CUF-NUM-AING IN CUF.
*% END
           MOVE CUF-COD-UBFD IN CUF-FLD TO CUF-COD-UBFD IN CUF.
           MOVE CUF-COD-RCUF IN CUF-FLD TO CUF-COD-RCUF IN CUF.
       FIN-RET-CUF-CUF.
           EXIT.
       
       RET-DLC-CUF SECTION.
       INI-RET-DLC-CUF.
           MOVE DLC-COD-UBFD IN CUF-FLD TO DLC-COD-UBFD IN DLC.
       FIN-RET-DLC-CUF.
           EXIT.

*% IF PGM_CHI AND PGM_BCI
      *LRH ini 15-MAR-1994 (Memo.23-NOV-1993, punto 2)
       VAL-OFI-USP SECTION.
       INI-VAL-OFI-USP.
           COPY USPC143.
      *lectura OPC
           MOVE CUF-CAI-IOPC IN CUF-FLD TO OPC-CAI-IOPC IN OPC.
           MOVE CUF-IIC-IOPC IN CUF-FLD TO OPC-IIC-IOPC IN OPC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-OPC.
           IF NOT FIO-STAT-OKS
              MOVE 'COL'        TO MSG-COD-SIST
              MOVE 'OPC    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE SCR-STAT-CON TO SCR-STAT
              MOVE '00' TO FIO-STAT
              GO TO FIN-VAL-OFI-USP.
      *lectura DLC  
           MOVE CUF-CAI-IOPC IN CUF-FLD TO DLC-CAI-IOPC IN DLC.
           MOVE CUF-IIC-IOPC IN CUF-FLD TO DLC-IIC-IOPC IN DLC.
           MOVE CUF-NUM-IDLC IN CUF-FLD TO DLC-NUM-IDLC IN DLC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-DLC.
           IF NOT FIO-STAT-OKS
              MOVE 'COL'        TO MSG-COD-SIST
              MOVE 'DLC    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE SCR-STAT-CON TO SCR-STAT
              MOVE '00' TO FIO-STAT
              GO TO FIN-VAL-OFI-USP.
      *
           MOVE DLC-COD-PLZA IN DLC TO WSS-DLC-COD-PLZA.
           IF NOT (USP-COD-OFICINA = OPC-COD-OFOC IN OPC OR
                   (WSS-COD-PLZA = 'O' AND
                    USP-COD-OFICINA = WSS-NUM-PLZA))
              MOVE 'COL'          TO MSG-COD-SIST
              MOVE 'CUFUSP NOOFI' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE SCR-STAT-CON TO SCR-STAT
              MOVE '00' TO FIO-STAT.
       FIN-VAL-OFI-USP.
           EXIT.
      *LRH fin 15-MAR-1994
*% END
      *>>>>
*% END
*% IF EOF_PU
      *<<<< EOF_PU
       PUT-ALL-CUF SECTION.
       INI-PUT-ALL-CUF.
           PERFORM PUT-OPC-CUF.
           PERFORM PUT-CUF-CUF.
           PERFORM PUT-DLC-CUF.
       FIN-PUT-ALL-CUF.
           EXIT.
       
       PUT-OPC-CUF SECTION.
       INI-PUT-OPC-CUF.
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-CUF.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-CUF
               GO TO FIN-PUT-OPC-CUF.
           MOVE OPC-MSC-TVOC IN OPC TO OPC-MSC-TVOC IN CUF-FLD.
           MOVE OPC-COD-TOOC IN OPC TO OPC-COD-TOOC IN CUF-FLD.
           MOVE OPC-COD-OFOC IN OPC TO OPC-COD-OFOC IN CUF-FLD.
      *BUS-MSC busca glosa de miscelaneo en tabla
           IF OPC-MSC-TVOC IN CUF-FLD > SPACES
               MOVE 'TAB' TO MSC-COD-SIST
               MOVE 'TVAL' TO MSC-COD-TMSC IN MSC
               MOVE OPC-MSC-TVOC IN CUF-FLD TO MSC-COD-CMSC IN MSC
               PERFORM BUS-MSC
               MOVE MSC-GLS-DESC IN MSC TO
                    FRM-GLS-TVOC IN CUF-FLD.
      *BUS-TAB busca glosa de codigo en tabla
           IF OPC-COD-TOOC IN CUF-FLD > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'TIO' TO TAB-COD-TTAB IN TAB
               MOVE OPC-COD-TOOC IN CUF-FLD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-TOOC IN CUF-FLD.
      *BUS-OFI busca oficina en tablas
           IF OPC-COD-OFOC IN CUF-FLD > SPACES
               MOVE OPC-COD-OFOC IN CUF-FLD TO OFI-COD-OFIC IN OFI
               PERFORM BUS-OFI
               MOVE OFI-GLS-DESC IN OFI TO FRM-GLS-OFOC IN CUF-FLD.
       FIN-PUT-OPC-CUF.
           EXIT.
       
       PUT-CUF-CUF SECTION.
       INI-PUT-CUF-CUF.
           IF PGM-STAT-CUF-NUL
               GO TO FIN-PUT-CUF-CUF.
           IF NOT PGM-STAT-CUF-OKS
               PERFORM CLR-CUF-CUF
               GO TO FIN-PUT-CUF-CUF.
           MOVE CUF-CAI-IOPC IN CUF TO CUF-CAI-IOPC IN CUF-FLD.
           MOVE CUF-IIC-IOPC IN CUF TO CUF-IIC-IOPC IN CUF-FLD.
           MOVE CUF-NUM-IDLC IN CUF TO CUF-NUM-IDLC IN CUF-FLD.
           MOVE CUF-NUM-ICUF IN CUF TO CUF-NUM-ICUF IN CUF-FLD.
           MOVE CUF-COD-UFAD IN CUF TO CUF-COD-UFAD IN CUF-FLD.
           MOVE CUF-NUM-DCUF IN CUF TO CUF-NUM-DCUF IN CUF-FLD.
           MOVE CUF-NUM-MCUF IN CUF TO CUF-NUM-MCUF IN CUF-FLD.
           MOVE CUF-NUM-SCUF IN CUF TO CUF-NUM-SCUF IN CUF-FLD.
           MOVE CUF-NUM-ACUF IN CUF TO CUF-NUM-ACUF IN CUF-FLD.
*% IF ( NOT PGM_CHI ) OR ( NOT PGM_BCI )
           MOVE CUF-NUM-DING IN CUF TO CUF-NUM-DING IN CUF-FLD.
           MOVE CUF-NUM-MING IN CUF TO CUF-NUM-MING IN CUF-FLD.
           MOVE CUF-NUM-SING IN CUF TO CUF-NUM-SING IN CUF-FLD.
           MOVE CUF-NUM-AING IN CUF TO CUF-NUM-AING IN CUF-FLD.
*% END
           MOVE CUF-COD-UBFD IN CUF TO CUF-COD-UBFD IN CUF-FLD.
           MOVE CUF-COD-RCUF IN CUF TO CUF-COD-RCUF IN CUF-FLD.
      *BUS-USR busca usuario en tablas
           IF CUF-COD-RCUF IN CUF-FLD > SPACES
               MOVE CUF-COD-RCUF IN CUF-FLD TO USR-COD-USER IN USR
               PERFORM BUS-USR
               MOVE USR-GLS-DESC IN USR TO FRM-GLS-RCUF IN CUF-FLD.
      *<<<< FIN_PUT_CUF_CUF
           PERFORM BUS-GLS-UFAD.
      *BUS-TAB busca glosa de codigo en tabla
           MOVE CUF-COD-UBFD IN CUF-FLD TO WSS-UBFD.
           IF WSS-TAB-UBFD NOT > SPACES
               MOVE SPACES TO FRM-GLS-UBFD IN CUF-FLD
           ELSE
           IF WSS-TAB-UBFD = 'OFI'
               MOVE WSS-COD-UBFD TO OFI-COD-OFIC IN OFI
               PERFORM BUS-OFI
               IF MSG-COD-MENS = 'COD    NEX'
                   MOVE SPACES TO FRM-GLS-UBFD IN CUF-FLD
               ELSE
                   MOVE OFI-GLS-DESC IN OFI TO
                        FRM-GLS-UBFD IN CUF-FLD
            ELSE
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE WSS-TAB-UBFD TO TAB-COD-TTAB IN TAB
               MOVE WSS-COD-UBFD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-UBFD IN CUF-FLD.
      *>>>>
       FIN-PUT-CUF-CUF.
           EXIT.
       
       PUT-DLC-CUF SECTION.
       INI-PUT-DLC-CUF.
           IF PGM-STAT-DLC-NUL
               GO TO FIN-PUT-DLC-CUF.
           IF NOT PGM-STAT-DLC-OKS
               PERFORM CLR-DLC-CUF
               GO TO FIN-PUT-DLC-CUF.
           MOVE DLC-COD-UBFD IN DLC TO DLC-COD-UBFD IN CUF-FLD.
      *<<<< FIN_PUT_DLC_CUF
            IF CUF-NUM-ICUF IN CUF-FLD = 1 AND
              (SCR-CCPP = 'ING' OR (SCR-CCPP = 'ACT' AND
               SCR-ACTU = SCR-ACTU-ING))
                MOVE DLC-COD-UBFD IN CUF-FLD TO CUF-COD-UFAD IN CUF-FLD
                IF CUF-COD-UFAD IN CUF-FLD > SPACES
                    PERFORM BUS-GLS-UFAD.
      *>>>>
       FIN-PUT-DLC-CUF.
           EXIT.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       BUS-CUF-UFAD SECTION.
       INI-BUS-CUF-UFAD.
           IF CUF-NUM-ICUF IN CUF = 1
               GO TO FIN-BUS-CUF-UFAD.

           PERFORM PUT-ALL-CUF.
           SUBTRACT 1 FROM CUF-NUM-ICUF IN CUF.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-CUF.
           IF FIO-STAT-OKS
               MOVE CUF-COD-UBFD IN CUF TO CUF-COD-UFAD IN CUF-FLD
               IF CUF-COD-UFAD IN CUF > SPACES
                   PERFORM BUS-GLS-UFAD
               ELSE
                   NEXT SENTENCE
           ELSE
               MOVE SPACES TO CUF-COD-UFAD IN CUF-FLD.
           PERFORM BUS-GLS-UFAD.
           PERFORM RET-ALL-CUF.
       FIN-BUS-CUF-UFAD.
           EXIT.

       BUS-GLS-UFAD SECTION.
       INI-BUS-GLS-UFAD.
      *BUS-TAB busca glosa de codigo en tabla
           MOVE CUF-COD-UFAD IN CUF-FLD TO WSS-UBFD.
           IF WSS-TAB-UBFD NOT > SPACES
               MOVE SPACES TO FRM-GLS-UFAD IN CUF-FLD
           ELSE
           IF WSS-TAB-UBFD = 'OFI'
               MOVE WSS-COD-UBFD TO OFI-COD-OFIC IN OFI
               PERFORM BUS-OFI
               IF MSG-COD-MENS = 'COD    NEX'
                   MOVE SPACES TO FRM-GLS-UFAD IN CUF-FLD
               ELSE
                   MOVE OFI-GLS-DESC IN OFI TO
                        FRM-GLS-UFAD IN CUF-FLD
            ELSE
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE WSS-TAB-UBFD TO TAB-COD-TTAB IN TAB
               MOVE WSS-COD-UBFD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-UFAD IN CUF-FLD.
       FIN-BUS-GLS-UFAD.
           EXIT.

       CLR-CUF SECTION.
       INI-CLR-CUF.
           MOVE SPACES TO CUF-COD-UFAD IN CUF.
           MOVE SPACES TO CUF-COD-ATRN IN CUF.
           MOVE SPACES TO CUF-FEC-FCUF IN CUF.
           MOVE SPACES TO CUF-FEC-FING IN CUF.
           MOVE SPACES TO CUF-COD-UBFD IN CUF.
           MOVE SPACES TO CUF-COD-RCUF IN CUF.
       FIN-CLR-CUF.
           EXIT.

       MOD-DLC SECTION.
       INI-MOD-DLC.
           MOVE CUF-CAI-IOPC IN CUF TO DLC-CAI-IOPC IN DLC.
           MOVE CUF-IIC-IOPC IN CUF TO DLC-IIC-IOPC IN DLC.
           MOVE CUF-NUM-IDLC IN CUF TO DLC-NUM-IDLC IN DLC.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM COL-FIO-DLC.
           IF NOT FIO-STAT-OKS
               GO TO FIN-MOD-DLC.
           IF SCR-CCPP = 'ELI'
               MOVE CUF-COD-UFAD IN CUF-FLD TO DLC-COD-UBFD IN DLC
           ELSE
               MOVE CUF-COD-UBFD IN CUF-FLD TO DLC-COD-UBFD IN DLC.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM COL-FIO-DLC.
       FIN-MOD-DLC.
           EXIT.

       BCK-OUT SECTION.
       INI-BCK-OUT.
           MOVE FIO-BCK-OUT TO FIO-CMND.
           PERFORM COL-FIO-DLC.
           IF FIO-STAT-OKS
      *        Para informar que el backout se realizo con exito
               MOVE '49' TO FIO-STAT.
       FIN-BCK-OUT.
           EXIT.

       LEE-NXT-PRV SECTION.
       INI-LEE-NXT-PRV.
           IF SCR-CUF-REQA > SPACES
               PERFORM LEE-NXT-PRV-CUF
           ELSE
               PERFORM LEE-NXT-PRV-CIC.
       FIN-LEE-NXT-PRV.
           EXIT.

       LEE-NXT-PRV-CUF SECTION.
       INI-LEE-NXT-PRV-CUF.
           MOVE SCR-CUF-REQA TO ADR-CUF-REQA.
           MOVE FIO-KDEF-S TO FIO-KDEF.
           IF FRM-FFLD = FRM-FFLD-PF7
               MOVE FIO-GET-PRV TO FIO-CMND
           ELSE
               MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM COL-FIO-CUF.
           IF FIO-STAT-OKS
               MOVE '00' TO PGM-STAT-CUF
               MOVE ADR-CUF-REQA TO SCR-CUF-REQA.
       FIN-LEE-NXT-PRV-CUF.
           EXIT.

       LEE-NXT-PRV-CIC SECTION.
       INI-LEE-NXT-PRV-CIC.
           IF CUF-IIC-IOPC IN CUF > ZEROES AND
              CUF-NUM-IDLC IN CUF > ZEROES AND
              CUF-NUM-ICUF IN CUF > ZEROES
               MOVE FIO-GET-KEY    TO FIO-CMND
               PERFORM COL-FIO-CUF
               IF FIO-STAT-OKS
                   IF FRM-FFLD = FRM-FFLD-PF7
                       MOVE FIO-GET-PRV TO FIO-CMND
                   ELSE
                       MOVE FIO-GET-NXT TO FIO-CMND
               ELSE
                   IF FRM-FFLD = FRM-FFLD-PF7
                       MOVE FIO-GET-LEQ TO FIO-CMND
                   ELSE
                       MOVE FIO-GET-NLS TO FIO-CMND
           ELSE
               IF FRM-FFLD = FRM-FFLD-PF7
                   MOVE FIO-GET-LEQ TO FIO-CMND
               ELSE
                   MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM COL-FIO-CUF.
           IF FIO-STAT-OKS
               MOVE '00' TO PGM-STAT-CUF
               MOVE ADR-CUF-REQA TO SCR-CUF-REQA.
       FIN-LEE-NXT-PRV-CIC.
           EXIT.

       BUS-SIG-CUF SECTION.
       INI-BUS-SIG-CUF.
           PERFORM PUT-ALL-CUF.
           ADD 1 TO CUF-NUM-ICUF IN CUF.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-CUF.
           PERFORM RET-ALL-CUF.
       FIN-BUS-SIG-CUF.
           EXIT.

