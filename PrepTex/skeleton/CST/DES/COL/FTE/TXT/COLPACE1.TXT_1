*%! Codigo adicional
*% IF SETTAG
*% FNCPQ = "CAE"
*% VSNPQ = "12.06.00"
*% END
*% IF WSS
      *<<<< WSS
       COPY COLBRDLC.
           COPY MSGINP47.
       01  WSS-VARI.
           05 WX-CUENT-X                    PIC X(08).
           05 WX-CUENT-N REDEFINES WX-CUENT-X  PIC 9(15)       COMP-3.
           05 WW-CUENT-N                    PIC 9(15).
           05 WW-CUENT-X REDEFINES WW-CUENT-N.
              10 WW-CUENT-FILLER               PIC X(03).
              10 WW-CUENT-CLAVE.
                 15 WW-CUENT-CLAVE-CAI         PIC X(04).
                 15 WW-CUENT-CLAVE-IIC         PIC X(08).


           05 MSGINP47-PASO.
              10 MSGINP47-CUENTA       PIC X(8).
              10 MSGINP47-FECVCT.
                 15 MSGINP47-FECVCT-DD PIC X(2).
                 15 MSGINP47-FECVCT-MM PIC X(2).
                 15 MSGINP47-FECVCT-AA PIC X(2).
                 15 FILLER             PIC X(2).
              10 MSGINP47-TIPO-CAMBIO  PIC X(1).
              10 MSGINP47-VALOR-UF     PIC X(8).
              10 MSGINP47-MONTO-ORIG   PIC X(8).
              10 MSGINP47-FILLER       PIC X(15).
      *>>>>
*% END
*% IF FND_KEY
      *<<<< FND_KEY
      *>>>>
*% END
*% IF CMN_ACC
      *<<<< CMN_ACC
           MOVE SPACES TO MSGINP47-REG.
           MOVE FRM-COD-OFIC IN CE1-FLD TO MSGINP47-SUCURSAL.

           MOVE OPC-CIC-IOPC IN OPC TO DLC-CIC-IOPC IN DLC.

           IF DLC-COD-TOPC IN DLC = 'D'
               MOVE '4'                   TO DLC-COD-TOPC IN DLC.
           MOVE 1 TO DLC-NUM-IDLC IN DLC.

           MOVE DLC-KEY-IDLC IN DLC TO WX-CUENT-N.
           MOVE WX-CUENT-X TO MSGINP47-CUENTA.

           MOVE FRM-NUM-DVEN IN CE1-FLD TO MSGINP47-FECVCT-DD.
           MOVE FRM-NUM-MVEN IN CE1-FLD TO MSGINP47-FECVCT-MM.
           MOVE FRM-NUM-AVEN IN CE1-FLD TO MSGINP47-FECVCT-AA.

           MOVE FRM-COD-TCAM IN CE1-FLD TO MSGINP47-TIPO-CAMBIO.
           MOVE ZEROES TO MSGINP47-VALOR-UF.

           MOVE FRM-VAL-MNTO IN CE1-FLD TO WX-CUENT-N.
           MOVE WX-CUENT-X TO MSGINP47-MONTO-ORIG.

           EXEC CICS XCTL PROGRAM('COLPHADM')
                           COMMAREA(MSGINP47-REG)
           END-EXEC.

      *>>>> CMN_ACC
*% END
*% IF EOF
      *<<<< EOF
      *>>>>
*% END
