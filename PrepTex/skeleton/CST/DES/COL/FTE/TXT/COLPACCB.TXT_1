*%! Codigo adicional para archivo
*% RA1 = "CCB"
*% FRM = "CCB"
*% NRA = 1
*% INCLUDE 'PFSALLF78'
*% IF WSS
      *<<<< WSS
       01  WSS-VARI.
           03  WSS-FEC-FCOM.
               05  WSS-NUM-SCOM                       PIC 9(02).
               05  WSS-NUM-ACOM                       PIC 9(02).
               05  WSS-NUM-MCOM                       PIC 9(02).
               05  WSS-NUM-DCOM                       PIC 9(02).
           03  WSS-UBFD.
               05  WSS-TAB-UBFD                      PIC X(03).
               05  WSS-COD-UBFD                      PIC X(12).
           03  BUS-POR-KEYS             VALUE SPACES PIC X(01).
           03  BUS-POR-CLIE             VALUE 'C'    PIC X(01).
           03  WSS-ICOR                 VALUE 0      PIC 9(01).
               88 NO-ASIGNE-CORRELATIVO VALUE 0.
           03 WSS-FEC-FEC1.
              05 WSS-NUM-SEC1           PIC 9(02).
              05 WSS-NUM-AEC1           PIC 9(02).
              05 WSS-NUM-MEC1           PIC 9(02).
              05 WSS-NUM-DEC1           PIC 9(02).
           03 WSS-FEC-FEC2.
              05 WSS-NUM-SEC2           PIC 9(02).
              05 WSS-NUM-AEC2           PIC 9(02).
              05 WSS-NUM-MEC2           PIC 9(02).
              05 WSS-NUM-DEC2           PIC 9(02).
      * var necesarias para controlar que el valor gastos cambio
      * sea ingresado sin decimales
           03 WSS-SGV-GAST VALUE ZEROES PIC 9(11).
           03 WSS-DIFE     VALUE ZEROES PIC 9(02)V9(04).
       COPY GNSWGSTR.
       COPY SGCBRDBC.
       COPY COLBROPC.
       COPY COLBRCCT.
       COPY COLBRDLC.
       COPY COLBRRDC.
       COPY GNSWGCPT.
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OPC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CCT-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DLC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RDC-REQA==.
      *>>>>
*% END
*% IF INI_FIO
      *<<<< INI_FIO
            MOVE ZEROES TO CCB-SGV-GAST IN CCB.
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
      *>>>>
*% END
*% IF FND_KEY
      *<<<< FND_KEY
           IF SCR-CCPP = 'ING' OR 'ELI'
               PERFORM BUS-COR-CCB
               PERFORM CLR-CCB
               GO TO CON-FND-KEY.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8
               PERFORM LEE-NXT-PRV
               IF FIO-STAT-OKS
                   PERFORM SCR-CLRA-CCB
                   GO TO CON-FND-KEY
               ELSE
                   MOVE SPACES TO PGM-STAT
                   IF FRM-FFLD = FRM-FFLD-PF7
                       MOVE 'COL' TO MSG-COD-SIST
                       MOVE 'CCB    NOPRV' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE '00' TO FIO-STAT
                       MOVE SCR-STAT-CON TO SCR-STAT
                       GO TO FIN-FND-KEY
                   ELSE
                       MOVE 'COL' TO MSG-COD-SIST
                       MOVE 'CCB    NONXT' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE '00' TO FIO-STAT
                       MOVE SCR-STAT-CON TO SCR-STAT
                       GO TO FIN-FND-KEY.
           IF CCB-IIC-IOPC IN CCB NOT > ZEROES
               PERFORM ANL-KEY-CCB
               IF NOT FIO-STAT-OKS
                   GO TO NXT-FND-KEY.
           IF CCB-NUM-ICCB IN CCB NOT > ZEROES 
               PERFORM BUS-COR-CCB
               PERFORM CLR-CCB.
       CON-FND-KEY.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
           IF SCR-CCPP = 'ACT' AND NO-ASIGNE-CORRELATIVO AND 
              NOT FIO-STAT-OKS
               PERFORM BUS-COR-CCB
               PERFORM CLR-CCB
               MOVE '23' TO FIO-STAT.

           IF FIO-STAT-OKS AND (SCR-CCPP = 'MOD' OR SCR-CCPP = 'ELI' OR 
              (SCR-CCPP = 'ACT' AND SCR-ACTU = SCR-ACTU-MOD))
                PERFORM BUS-SIG-CCB
                IF FIO-STAT-OKS
                    MOVE 'COL' TO MSG-COD-SIST
                    MOVE 'NUMICCBINV' TO MSG-COD-MENS
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    MOVE '00' TO FIO-STAT
                    MOVE SCR-STAT-CON TO SCR-STAT
                    GO TO FIN-FND-KEY
                ELSE
                    MOVE '00' TO FIO-STAT.

           IF FIO-STAT-OKS
               MOVE ADR-CCB-REQA TO SCR-CCB-REQA.
           IF  BUS-POR-KEYS = BUS-POR-CLIE
                IF FIO-STAT-OKS
                    PERFORM BUS-CCB-SCBA
                    MOVE '00' TO FIO-STAT
                    GO TO NXT-FND-KEY
                ELSE
                    PERFORM BUS-CCB-SCBA
                    MOVE '23' TO FIO-STAT
                    GO TO NXT-FND-KEY.
           IF FIO-STAT-OKS
               PERFORM BUS-CCB-SCBA
               PERFORM ANL-CIC-IOPC
               GO TO NXT-FND-KEY.
           IF ( SCR-CCPP = 'ING' OR SCR-CCPP = 'ACT' )
               PERFORM BUS-CCB-SCBA
               PERFORM ANL-CIC-IOPC
               IF FIO-STAT-OKS
                   PERFORM PUT-OPC-CCB
                   PERFORM PUT-DLC-CCB
                   MOVE '23' TO FIO-STAT
               ELSE
                   MOVE SCR-STAT-CON TO SCR-STAT
                   MOVE '00' TO FIO-STAT.
       NXT-FND-KEY.
           IF PGM-STAT-OPC-OKS AND 
              OPC-MSC-STAT IN OPC NOT = 'A'
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'OPC    NOACA' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE SCR-STAT-CON TO SCR-STAT
               IF  SCR-CCPP = 'ING'
                   MOVE '23' TO FIO-STAT
               ELSE
                   MOVE '00' TO FIO-STAT.
      *>>>>
*% END
*% IF FIN_CMN_MOD
      *<<<< FIN_CMN_MOD
           IF NOT FIO-STAT-OKS
      *        Error inconsistencia en el ingreso : informar
               MOVE FIO-MENS TO FRM-MENS
               GO TO FIN-CMN-MOD.
           PERFORM MOD-DLC.
           IF NOT FIO-STAT-OKS
               PERFORM BCK-OUT
               GO TO FIN-CMN-MOD.
      *>>>>
*% END
*% IF FIN_CMN_ING
      *<<<< FIN_CMN_ING
           IF NOT FIO-STAT-OKS
      *        Error inconsistencia en el ingreso : informar
               MOVE FIO-MENS TO FRM-MENS
               GO TO FIN-CMN-ING.
           PERFORM MOD-DLC.
           IF NOT FIO-STAT-OKS
               PERFORM BCK-OUT
               GO TO FIN-CMN-ING.
      *>>>>
*% END
*% IF FIN_CMN_ELI
      *<<<< FIN_CMN_ELI
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ELI.
           PERFORM KEY-CCB-CCB.
           SUBTRACT 1 FROM CCB-NUM-ICCB IN CCB.
           IF CCB-NUM-ICCB IN CCB > ZEROES
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM COL-FIO-CCB
               IF FIO-STAT-OKS
                   NEXT SENTENCE
               ELSE
                   MOVE 'COL' TO MSG-COD-SIST
                   MOVE 'CCB-1    NEX' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   GO TO FIN-CMN-ELI
           ELSE
               MOVE SPACES TO CCB-IND-SCBZ IN CCB.
           MOVE CCB-IND-SCBZ IN CCB TO CCB-IND-SCBZ IN CCB-FLD.
           PERFORM MOD-DLC.
           IF NOT FIO-STAT-OKS
               MOVE 'COL' TO MSG-COD-SIST
               MOVE 'DLC    NOMOD' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS.
      *>>>>
*% END
*% IF FIN_HBL_FLD
      *<<<< FIN_HBL_FLD
      *BUS-USR busca usuario en tablas
           IF SCR-USER > SPACES
               MOVE SCR-USER TO USR-COD-USER IN USR
                                CCB-COD-RCCB IN CCB-FLD
               PERFORM BUS-USR
               MOVE USR-GLS-DESC IN USR TO FRM-GLS-RCCB IN CCB-FLD.
      *>>>>
*% END
*% IF INI_SCR_TRL
      *<<<< INI_SCR_TRL
      *DAD
      *BUS-USR busca usuario en tablas
           IF SCR-STAT NOT = SCR-STAT-CON
               IF SCR-USER > SPACES
                   MOVE SCR-USER TO CCB-COD-RCCB IN CCB-FLD
                   MOVE SCR-USER TO CCB-COD-RCCB IN CCB
                   MOVE SCR-USER TO USR-COD-USER IN USR
                   PERFORM BUS-USR
                   MOVE USR-GLS-DESC IN USR TO FRM-GLS-RCCB IN CCB-FLD.
      *
      *>>>>
*% END
*% IF EOF_PQ
      *<<<< EOF_PQ
       RET-ALL-CCB SECTION.
       INI-RET-ALL-CCB.
           PERFORM KEY-ALL-CCB.
           PERFORM RET-OPC-CCB.
           PERFORM RET-CCB-CCB.
           PERFORM RET-DLC-CCB.
       FIN-RET-ALL-CCB.
           EXIT.

       RET-OPC-CCB SECTION.
       INI-RET-OPC-CCB.
           MOVE OPC-MSC-TVOC IN CCB-FLD TO OPC-MSC-TVOC IN OPC.
           MOVE OPC-COD-TOOC IN CCB-FLD TO OPC-COD-TOOC IN OPC.
           MOVE OPC-COD-OFOC IN CCB-FLD TO OPC-COD-OFOC IN OPC.
           MOVE OPC-NUM-DCOL IN CCB-FLD TO OPC-NUM-DCOL IN OPC.
           MOVE OPC-NUM-MCOL IN CCB-FLD TO OPC-NUM-MCOL IN OPC.
           MOVE OPC-NUM-SCOL IN CCB-FLD TO OPC-NUM-SCOL IN OPC.
           MOVE OPC-NUM-ACOL IN CCB-FLD TO OPC-NUM-ACOL IN OPC.
           MOVE OPC-COD-VCOC IN CCB-FLD TO OPC-COD-VCOC IN OPC.
       FIN-RET-OPC-CCB.
           EXIT.
       
       RET-CCB-CCB SECTION.
       INI-RET-CCB-CCB.
           MOVE CCB-IND-SCBA IN CCB-FLD TO CCB-IND-SCBA IN CCB.
           MOVE CCB-NUM-DCCB IN CCB-FLD TO CCB-NUM-DCCB IN CCB.
           MOVE CCB-NUM-MCCB IN CCB-FLD TO CCB-NUM-MCCB IN CCB.
           MOVE CCB-NUM-SCCB IN CCB-FLD TO CCB-NUM-SCCB IN CCB.
           MOVE CCB-NUM-ACCB IN CCB-FLD TO CCB-NUM-ACCB IN CCB.
           MOVE CCB-NUM-DING IN CCB-FLD TO CCB-NUM-DING IN CCB.
           MOVE CCB-NUM-MING IN CCB-FLD TO CCB-NUM-MING IN CCB.
           MOVE CCB-NUM-SING IN CCB-FLD TO CCB-NUM-SING IN CCB.
           MOVE CCB-NUM-AING IN CCB-FLD TO CCB-NUM-AING IN CCB.
           MOVE CCB-IND-SCBZ IN CCB-FLD TO CCB-IND-SCBZ IN CCB.
           MOVE CCB-COD-RCCB IN CCB-FLD TO CCB-COD-RCCB IN CCB.
           MOVE CCB-SGV-GAST IN CCB-FLD TO CCB-SGV-GAST IN CCB.
      *<<<< FIN_RET_CCB_CCB 
           IF CCB-IND-SCBZ IN CCB-FLD = 'VIG'
               MOVE SPACES TO CCB-IND-SCBZ IN CCB.
      *>>>>
       FIN-RET-CCB-CCB.
           EXIT.
       
       RET-DLC-CCB SECTION.
       INI-RET-DLC-CCB.
           MOVE DLC-VAL-SDLC IN CCB-FLD TO DLC-VAL-SDLC IN DLC.
           MOVE DLC-COD-UBFD IN CCB-FLD TO DLC-COD-UBFD IN DLC.
           MOVE DLC-IND-SCTB IN CCB-FLD TO DLC-IND-SCTB IN DLC.
           MOVE DLC-IND-SCBZ IN CCB-FLD TO DLC-IND-SCBZ IN DLC.
       FIN-RET-DLC-CCB.
           EXIT.
      *>>>>
*% END
*% IF EOF_PU
      *<<<< EOF_PU
       PUT-ALL-CCB SECTION.
       INI-PUT-ALL-CCB.
           PERFORM PUT-OPC-CCB.
           PERFORM PUT-CCB-CCB.
           PERFORM PUT-DLC-CCB.
       FIN-PUT-ALL-CCB.
           EXIT.
       
       PUT-OPC-CCB SECTION.
       INI-PUT-OPC-CCB.
           IF PGM-STAT-OPC-NUL
               GO TO FIN-PUT-OPC-CCB.
           IF NOT PGM-STAT-OPC-OKS
               PERFORM CLR-OPC-CCB
               GO TO FIN-PUT-OPC-CCB.
           MOVE OPC-MSC-TVOC IN OPC TO OPC-MSC-TVOC IN CCB-FLD.
           MOVE OPC-COD-TOOC IN OPC TO OPC-COD-TOOC IN CCB-FLD.
           MOVE OPC-COD-OFOC IN OPC TO OPC-COD-OFOC IN CCB-FLD.
           MOVE OPC-NUM-DCOL IN OPC TO OPC-NUM-DCOL IN CCB-FLD.
           MOVE OPC-NUM-MCOL IN OPC TO OPC-NUM-MCOL IN CCB-FLD.
           MOVE OPC-NUM-SCOL IN OPC TO OPC-NUM-SCOL IN CCB-FLD.
           MOVE OPC-NUM-ACOL IN OPC TO OPC-NUM-ACOL IN CCB-FLD.
           MOVE OPC-COD-VCOC IN OPC TO OPC-COD-VCOC IN CCB-FLD.
      *BUS-MSC busca glosa de miscelaneo en tabla
           IF OPC-MSC-TVOC IN CCB-FLD > SPACES
               MOVE 'TAB' TO MSC-COD-SIST
               MOVE 'TVAL' TO MSC-COD-TMSC IN MSC
               MOVE OPC-MSC-TVOC IN CCB-FLD TO MSC-COD-CMSC IN MSC
               PERFORM BUS-MSC
               MOVE MSC-GLS-DESC IN MSC TO
                    FRM-GLS-TVOC IN CCB-FLD.
      *BUS-TAB busca glosa de codigo en tabla
           IF OPC-COD-TOOC IN CCB-FLD > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'TIO' TO TAB-COD-TTAB IN TAB
               MOVE OPC-COD-TOOC IN CCB-FLD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-TOOC IN CCB-FLD.
      *BUS-OFI busca oficina en tablas
           IF OPC-COD-OFOC IN CCB-FLD > SPACES
               MOVE OPC-COD-OFOC IN CCB-FLD TO OFI-COD-OFIC IN OFI
               PERFORM BUS-OFI
               MOVE OFI-GLS-DESC IN OFI TO FRM-GLS-OFOC IN CCB-FLD.
      *BUS-TAB busca glosa de codigo en tabla
           IF OPC-COD-VCOC IN CCB-FLD > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'VLR' TO TAB-COD-TTAB IN TAB
               MOVE OPC-COD-VCOC IN CCB-FLD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-VCOC IN CCB-FLD.
       FIN-PUT-OPC-CCB.
           EXIT.
       
       PUT-CCB-CCB SECTION.
       INI-PUT-CCB-CCB.
           IF PGM-STAT-CCB-NUL
               GO TO FIN-PUT-CCB-CCB.
           IF NOT PGM-STAT-CCB-OKS
               PERFORM CLR-CCB-CCB
               GO TO FIN-PUT-CCB-CCB.
           MOVE CCB-CAI-IOPC IN CCB TO CCB-CAI-IOPC IN CCB-FLD.
           MOVE CCB-IIC-IOPC IN CCB TO CCB-IIC-IOPC IN CCB-FLD.
           MOVE CCB-NUM-IDLC IN CCB TO CCB-NUM-IDLC IN CCB-FLD.
           MOVE CCB-NUM-ICCB IN CCB TO CCB-NUM-ICCB IN CCB-FLD.
           MOVE CCB-IND-SCBA IN CCB TO CCB-IND-SCBA IN CCB-FLD.
           MOVE CCB-NUM-DCCB IN CCB TO CCB-NUM-DCCB IN CCB-FLD.
           MOVE CCB-NUM-MCCB IN CCB TO CCB-NUM-MCCB IN CCB-FLD.
           MOVE CCB-NUM-SCCB IN CCB TO CCB-NUM-SCCB IN CCB-FLD.
           MOVE CCB-NUM-ACCB IN CCB TO CCB-NUM-ACCB IN CCB-FLD.
           MOVE CCB-NUM-DING IN CCB TO CCB-NUM-DING IN CCB-FLD.
           MOVE CCB-NUM-MING IN CCB TO CCB-NUM-MING IN CCB-FLD.
           MOVE CCB-NUM-SING IN CCB TO CCB-NUM-SING IN CCB-FLD.
           MOVE CCB-NUM-AING IN CCB TO CCB-NUM-AING IN CCB-FLD.
           MOVE CCB-IND-SCBZ IN CCB TO CCB-IND-SCBZ IN CCB-FLD.
           MOVE CCB-COD-RCCB IN CCB TO CCB-COD-RCCB IN CCB-FLD.
           MOVE CCB-SGV-GAST IN CCB TO CCB-SGV-GAST IN CCB-FLD.
      *BUS-MSC busca glosa de miscelaneo en tabla
           IF CCB-IND-SCBA IN CCB-FLD > SPACES
               MOVE 'COL' TO MSC-COD-SIST
               MOVE 'SCBZ' TO MSC-COD-TMSC IN MSC
               MOVE CCB-IND-SCBA IN CCB-FLD TO MSC-COD-CMSC IN MSC
               PERFORM BUS-MSC
               MOVE MSC-GLS-DESC IN MSC TO
                    FRM-GLS-SCBA IN CCB-FLD.
      *BUS-MSC busca glosa de miscelaneo en tabla
           IF CCB-IND-SCBZ IN CCB-FLD > SPACES
               MOVE 'COL' TO MSC-COD-SIST
               MOVE 'SCBZ' TO MSC-COD-TMSC IN MSC
               MOVE CCB-IND-SCBZ IN CCB-FLD TO MSC-COD-CMSC IN MSC
               PERFORM BUS-MSC
               MOVE MSC-GLS-DESC IN MSC TO
                    FRM-GLS-SCBZ IN CCB-FLD.
      *BUS-USR busca usuario en tablas
           IF CCB-COD-RCCB IN CCB-FLD > SPACES
               MOVE CCB-COD-RCCB IN CCB-FLD TO USR-COD-USER IN USR
               PERFORM BUS-USR
               MOVE USR-GLS-DESC IN USR TO FRM-GLS-RCCB IN CCB-FLD.
       FIN-PUT-CCB-CCB.
           EXIT.
       
       PUT-DLC-CCB SECTION.
       INI-PUT-DLC-CCB.
           IF PGM-STAT-DLC-NUL
               GO TO FIN-PUT-DLC-CCB.
           IF NOT PGM-STAT-DLC-OKS
               PERFORM CLR-DLC-CCB
               GO TO FIN-PUT-DLC-CCB.
           MOVE DLC-VAL-SDLC IN DLC TO DLC-VAL-SDLC IN CCB-FLD.
           MOVE DLC-COD-UBFD IN DLC TO DLC-COD-UBFD IN CCB-FLD.
           MOVE DLC-IND-SCTB IN DLC TO DLC-IND-SCTB IN CCB-FLD.
           MOVE DLC-IND-SCBZ IN DLC TO DLC-IND-SCBZ IN CCB-FLD.
      *BUS-MSC busca glosa de miscelaneo en tabla
           IF DLC-IND-SCTB IN CCB-FLD > SPACES
               MOVE 'COL' TO MSC-COD-SIST
               MOVE 'SCTB' TO MSC-COD-TMSC IN MSC
               MOVE DLC-IND-SCTB IN CCB-FLD TO MSC-COD-CMSC IN MSC
               PERFORM BUS-MSC
               MOVE MSC-GLS-DESC IN MSC TO
                    FRM-GLS-SCTB IN CCB-FLD.
      *BUS-MSC busca glosa de miscelaneo en tabla
           IF DLC-IND-SCBZ IN CCB-FLD > SPACES
               MOVE 'COL' TO MSC-COD-SIST
               MOVE 'SCBZ' TO MSC-COD-TMSC IN MSC
               MOVE DLC-IND-SCBZ IN CCB-FLD TO MSC-COD-CMSC IN MSC
               PERFORM BUS-MSC
               MOVE MSC-GLS-DESC IN MSC TO
                    FRM-GLS-SCBZ IN CCB-FLD.
      *<<<< FIN_PUT_DLC_CCB
            IF CCB-NUM-ICCB IN CCB-FLD = 1 AND
              (SCR-CCPP = 'ING' OR (SCR-CCPP = 'ACT' AND
               SCR-ACTU = SCR-ACTU-ING))
                MOVE DLC-IND-SCBZ IN CCB-FLD TO CCB-IND-SCBA IN CCB-FLD
                IF CCB-IND-SCBA IN CCB-FLD > SPACES
                    MOVE 'COL' TO MSC-COD-SIST
                    MOVE 'SCBZ' TO MSC-COD-TMSC IN MSC
                    MOVE CCB-IND-SCBA IN CCB-FLD TO MSC-COD-CMSC IN MSC
                    PERFORM BUS-MSC
                    MOVE MSC-GLS-DESC IN MSC TO FRM-GLS-SCBA IN CCB-FLD.
      *>>>>
      *<<<< FIN_PUT_DLC_CCB
      *BUS-TAB busca glosa de codigo en tabla
           MOVE DLC-COD-UBFD IN CCB-FLD TO WSS-UBFD.
           IF WSS-TAB-UBFD NOT > SPACES
               MOVE SPACES TO FRM-GLS-UBFD IN CCB-FLD
           ELSE
           IF WSS-TAB-UBFD = 'OFI'
               MOVE WSS-COD-UBFD TO OFI-COD-OFIC IN OFI
               PERFORM BUS-OFI
               IF MSG-COD-MENS = 'COD    NEX'
                   MOVE SPACES TO FRM-GLS-UBFD IN CCB-FLD
               ELSE
                   MOVE OFI-GLS-DESC IN OFI TO
                        FRM-GLS-UBFD IN CCB-FLD
            ELSE
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE WSS-TAB-UBFD TO TAB-COD-TTAB IN TAB
               MOVE WSS-COD-UBFD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-UBFD IN CCB-FLD.
      *>>>>
       FIN-PUT-DLC-CCB.
           EXIT.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       BUS-CCB-SCBA SECTION.
       INI-BUS-CCB-SCBA.
           IF CCB-NUM-ICCB IN CCB = 1
               GO TO FIN-BUS-CCB-SCBA.

           PERFORM PUT-ALL-CCB.
           SUBTRACT 1 FROM CCB-NUM-ICCB IN CCB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-CCB.
           IF FIO-STAT-OKS
               MOVE CCB-IND-SCBZ IN CCB TO CCB-IND-SCBA IN CCB-FLD
           ELSE
               MOVE SPACES TO CCB-IND-SCBA IN CCB-FLD.
      *BUS-MSC busca glosa de miscelaneo en tabla
           IF CCB-IND-SCBA IN CCB-FLD > SPACES
               MOVE 'COL' TO MSC-COD-SIST
               MOVE 'SCBZ' TO MSC-COD-TMSC IN MSC
               MOVE CCB-IND-SCBA IN CCB-FLD TO MSC-COD-CMSC IN MSC
               PERFORM BUS-MSC
               MOVE MSC-GLS-DESC IN MSC TO
                    FRM-GLS-SCBA IN CCB-FLD.
           PERFORM RET-ALL-CCB.
       FIN-BUS-CCB-SCBA.
           EXIT.

       CLR-CCB SECTION.
       INI-CLR-CCB.
           MOVE SPACES TO CCB-IND-SCBA IN CCB.
           MOVE SPACES TO CCB-COD-ATRN IN CCB.
           MOVE SPACES TO CCB-FEC-FCCB IN CCB.
           MOVE SPACES TO CCB-FEC-FING IN CCB.
           MOVE SPACES TO CCB-IND-SCBZ IN CCB.
           MOVE SPACES TO CCB-COD-RCCB IN CCB.
           MOVE ZEROES TO CCB-SGV-GAST IN CCB.
       FIN-CLR-CCB.
           EXIT.

       MOD-DLC SECTION.
       INI-MOD-DLC.
           MOVE CCB-CAI-IOPC IN CCB TO DLC-CAI-IOPC IN DLC.
           MOVE CCB-IIC-IOPC IN CCB TO DLC-IIC-IOPC IN DLC.
           MOVE CCB-NUM-IDLC IN CCB TO DLC-NUM-IDLC IN DLC.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM COL-FIO-DLC.
           IF NOT FIO-STAT-OKS
               GO TO FIN-MOD-DLC.
           IF SCR-CCPP = 'ELI'
               MOVE CCB-IND-SCBA IN CCB-FLD TO DLC-IND-SCBZ IN DLC
           ELSE
               MOVE CCB-IND-SCBZ IN CCB-FLD TO DLC-IND-SCBZ IN DLC.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM COL-FIO-DLC.
       FIN-MOD-DLC.
           EXIT.
       BCK-OUT SECTION.
       INI-BCK-OUT.
           MOVE FIO-BCK-OUT TO FIO-CMND.
           PERFORM COL-FIO-DLC.
           IF FIO-STAT-OKS
      *        Para informar que el backout se realizo con exito
               MOVE '49' TO FIO-STAT.
       FIN-BCK-OUT.
           EXIT.

       LEE-NXT-PRV SECTION.
       INI-LEE-NXT-PRV.
           IF SCR-CCB-REQA > SPACES
               PERFORM LEE-NXT-PRV-CCB
           ELSE
               PERFORM LEE-NXT-PRV-CIC.
       FIN-LEE-NXT-PRV.
           EXIT.

       LEE-NXT-PRV-CCB SECTION.
       INI-LEE-NXT-PRV-CCB.
           MOVE SCR-CCB-REQA TO ADR-CCB-REQA.
           MOVE FIO-KDEF-S TO FIO-KDEF.
           IF FRM-FFLD = FRM-FFLD-PF7
               MOVE FIO-GET-PRV TO FIO-CMND
           ELSE
               MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM COL-FIO-CCB.
           IF FIO-STAT-OKS
               MOVE '00' TO PGM-STAT-CCB
               MOVE ADR-CCB-REQA TO SCR-CCB-REQA.
       FIN-LEE-NXT-PRV-CCB.
           EXIT.

       LEE-NXT-PRV-CIC SECTION.
       INI-LEE-NXT-PRV-CIC.
           IF CCB-IIC-IOPC IN CCB > ZEROES AND
              CCB-NUM-IDLC IN CCB > ZEROES AND
              CCB-NUM-ICCB IN CCB > ZEROES
               MOVE FIO-GET-KEY    TO FIO-CMND
               PERFORM COL-FIO-CCB
               IF FIO-STAT-OKS
                   IF FRM-FFLD = FRM-FFLD-PF7
                       MOVE FIO-GET-PRV TO FIO-CMND
                   ELSE
                       MOVE FIO-GET-NXT TO FIO-CMND
               ELSE
                   IF FRM-FFLD = FRM-FFLD-PF7
                       MOVE FIO-GET-LEQ TO FIO-CMND
                   ELSE
                       MOVE FIO-GET-NLS TO FIO-CMND
           ELSE
               IF FRM-FFLD = FRM-FFLD-PF7
                   MOVE FIO-GET-LEQ TO FIO-CMND
               ELSE
                   MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM COL-FIO-CCB.
           IF FIO-STAT-OKS
               MOVE '00' TO PGM-STAT-CCB
               MOVE ADR-CCB-REQA TO SCR-CCB-REQA.
       FIN-LEE-NXT-PRV-CIC.
           EXIT.

       BUS-SIG-CCB SECTION.
       INI-BUS-SIG-CCB.
           PERFORM PUT-ALL-CCB.
           ADD 1 TO CCB-NUM-ICCB IN CCB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM COL-FIO-CCB.
           PERFORM RET-ALL-CCB.
       FIN-BUS-SIG-CCB.
           EXIT.

       COPY GNSBGSTR.
       COPY COLBGKCB.
       COPY COLBGCCB.
       COPY SGCBFDBC.
       COPY COLBFCCT.
       COPY COLBFDLC.
       COPY COLBFOPC.
       COPY COLBFRDC.
       COPY SGCBGCLI.
       COPY GNSBGCPT.
      *>>>>
