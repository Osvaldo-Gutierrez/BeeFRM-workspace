*% IF WSS
      *<<<< IF WSS
       COPY CTBBRCPB.
       COPY CTBBRCTB.
       COPY CTBBRCDC.
       COPY CTBBRMDC.
       COPY CTBBRCOR.
       COPY CTBBRCVG.
       01  WSS-VARI.
           03 WSS-OPID                         PIC X(03).
           03 WSS-IND-PF3                      PIC X(03).
      *>>>> END
*% END
*% IF INI_FIO_VAX
      *<<<< INI_FIO_VAX

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           PERFORM CTB-FIO-CTB.
           MOVE FIO-INP TO FIO-CMND.
           PERFORM CTB-FIO-CTB.
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR DE APERTURA ARCHIVO CTBCTB: ABORTO'
               STOP RUN.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           PERFORM CTB-FIO-CDC.
           MOVE FIO-INP TO FIO-CMND.
           PERFORM CTB-FIO-CDC.
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR DE APERTURA ARCHIVO CTBCDC: ABORTO'
               STOP RUN.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           PERFORM CTB-FIO-MDC.
           MOVE FIO-INP TO FIO-CMND.
           PERFORM CTB-FIO-MDC.
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR DE APERTURA ARCHIVO CTBMDC: ABORTO'
               STOP RUN.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           PERFORM CTB-FIO-COR.
           MOVE FIO-UPD TO FIO-CMND.
           PERFORM CTB-FIO-COR.
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR DE APERTURA ARCHIVO CTBCOR: ABORTO'
               STOP RUN.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           PERFORM CTB-FIO-CVG.
           MOVE FIO-INP TO FIO-CMND.
           PERFORM CTB-FIO-CVG.
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR DE APERTURA ARCHIVO CTBCVG: ABORTO'
               STOP RUN.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'CTB'        TO FIO-SIST.
           PERFORM GNS-FIO-MSG.
           MOVE FIO-INP TO FIO-CMND.
           MOVE 'CTB'   TO FIO-SIST.
           PERFORM GNS-FIO-MSG.
      *>>>> END
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
           PERFORM GET-TMP.
      *>>>> END
*% END
*% IF CMN_ING
      *<<<< CMN_ING
           IF FRM-IND-OBDC IN MVT-FLD = 'S'
               PERFORM SET-TMP
               MOVE 'CTBACTCPM   ' TO SCR-FRET
               MOVE SCR-INST-HCM TO SCR-INST
               GO TO FIN-CMN-ING.
      *>>>> END
*% END
*% IF FIN_CMN_ING
      *<<<< FIN_CMN_ING
           ADD 1 TO MOV-NUM-MVTO IN MOV.
           IF MOV-NUM-MVTO IN MOV > CPB-NUM-TMOV IN CPB
               MOVE 'CTBACTMVT   ' TO SCR-FRET
               MOVE SCR-INST-HCM TO SCR-INST.
      *>>>> END
*% END
*% IF EOF
      *<<<< EOF

       GET-TMP SECTION.
       INI-GET-TMP.
           CALL 'LIB$GET_SYMBOL' USING BY DESCRIPTOR
                               'CTB$CPB' CPB.
       FIN-GET-TMP.
           EXIT.

       SET-TMP SECTION.
       INI-SET-TMP.
           CALL 'LIB$SET_SYMBOL' USING BY DESCRIPTOR
                               'CTB$MOV' MOV.
       FIN-SET-TMP.
           EXIT.
       COPY CTBBFCTB.
       COPY CTBBFCDC.
       COPY CTBBFMDC.
       COPY CTBBFCOR.
       COPY CTBBFCVG.
      *>>>> END
*% END
