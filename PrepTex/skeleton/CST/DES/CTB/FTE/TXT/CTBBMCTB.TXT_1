*%       MANEJO FORMULARIO CTB. 
*% IF FIN_PUT_ALL_CTB
      *<<<< IF FIN_PUT_ALL_CTB
           MOVE CTB-NUM-CCSA IN CTB-FLD TO CCS-NUM-CCSA IN CCS.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM CTB-FIO-CCS.
           IF NOT FIO-STAT-OKS
               MOVE 'CCS    NEX' TO MSG-COD-MENS IN MSG
               MOVE 'CTB' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-GLS-CCSA IN CTB-FLD
           ELSE
               MOVE CCS-GLS-CCSA IN CCS TO FRM-GLS-CCSA IN CTB-FLD.
      *<<<< END FIN_PUT_ALL_CTB
*% END
*% IF CTB_NUM_ICTB
      *<<<< IF CTB_NUM_ICTB
           IF CTB-NUM-ICTB IN CTB-FLD = ZEROES
               MOVE CTB-NUM-CCTB IN CTB-FLD TO CTB-NUM-ICTB IN CTB-FLD
               MOVE CTB-VRF-CCTB IN CTB-FLD TO CTB-VRF-ICTB IN CTB-FLD.
      *>>>> END CTB_NUM_ICTB
*% END
*% IF CTB_NUM_CCSA
      *<<<< IF CTB_NUM_CCSA
           IF CTB-NUM-CCSA IN CTB-FLD NOT  > ZEROES
               MOVE 'CTB' TO TAB-COD-SIST
               MOVE 'OCS' TO TAB-COD-TTAB IN TAB
               MOVE CTB-NUM-CCTB IN CTB-FLD TO CTB-NUM-CCTB IN CTB
               MOVE CTB-IND-GGRP IN CTB TO TAB-COD-CTAB IN TAB
               PERFORM VAL-COD
               IF FRM-SUAR = FRM-SUAR-MAL
                   MOVE 'CCS    OBL' TO MSG-COD-MENS IN MSG
                   MOVE 'CTB' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE SPACES TO FRM-GLS-CCSA IN CTB-FLD
                   GO TO FIN-VAL-CTB-NUM-CCSA
               ELSE
                   MOVE SPACES TO FRM-GLS-CCSA IN CTB-FLD
                   GO TO FIN-VAL-CTB-NUM-CCSA.
           MOVE CTB-NUM-CCSA IN CTB-FLD TO VRF-NUME.
           MOVE CTB-VRF-CCSA IN CTB-FLD TO VRF-DVRF.
           MOVE VRF-ZERO-N TO VRF-ZERO.
           MOVE VRF-SVRF-K TO VRF-SVRF.
           MOVE 7 TO VRF-LMAX.
           MOVE VRF-VAL TO VRF-CMND.
           PERFORM PRO-VRF.
           IF FRM-SUAR NOT = FRM-SUAR-MAL
               MOVE CTB-NUM-CCSA IN CTB-FLD TO CCS-NUM-CCSA IN CCS
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM CTB-FIO-CCS
               IF NOT FIO-STAT-OKS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   MOVE 'CCS    NEX' TO MSG-COD-MENS IN MSG
                   MOVE 'CTB' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE SPACES TO FRM-GLS-CCSA IN CTB-FLD
               ELSE
                   MOVE CCS-GLS-CCSA IN CCS TO FRM-GLS-CCSA IN CTB-FLD.
      *>>>> END CTB_NUM_CCSA
*% END
*% IF INI_CTB_VRF_CCTB
      *<<<< INI_CTB_VRF_CCTB
           MOVE VRF-SVRF-0 TO VRF-SVRF.
      *>>>> END
*% END
*% IF INI_CTB_VRF_ICTB
      *<<<< INI_CTB_VRF_ICTB
           MOVE VRF-SVRF-0 TO VRF-SVRF.
      *>>>> END
*% END
*% IF INI_CTB_VRF_CCSA
      *<<<< INI_CTB_VRF_CCSA
           MOVE VRF-SVRF-0 TO VRF-SVRF.
      *>>>> END
*% END
