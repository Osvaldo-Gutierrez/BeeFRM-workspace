*%      CODIGO ADICIONAL PARA PROGRAMA LISTADOR CTBPLCPB.
*% IF WSS
      *<<<< IF WSS
       COPY CTBBRMOV.
       COPY CTBBRCTB.
       COPY CTBBRCDC.
       COPY CTBBRCOR.
       01  WSS-VARI.
           03 WSS-IND-ESTA                       PIC X(01).
           03 WSS-NUM-CORR                       PIC 9(08).
           03 WSS-IND-PMOV    VALUE 'S'          PIC X(01).
              88 PRIMER-MOVE  VALUE 'S'.
      *>>>> END
*% END
*% IF INI_INP
      *<<<< IF INI_INP

           MOVE FIO-INP TO FIO-CMND.
           PERFORM CTB-FIO-CTB.
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR APERTURA ARCHIVO CTBCTB: ABORTO'
               STOP RUN.

           MOVE FIO-INP TO FIO-CMND.
           PERFORM CTB-FIO-CDC.
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR APERTURA ARCHIVO CTBCDC: ABORTO'
               STOP RUN.

           MOVE FIO-INP TO FIO-CMND.
           PERFORM CTB-FIO-MOV.
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR APERTURA ARCHIVO CTBMOV: ABORTO'
               STOP RUN.

           MOVE 'CTB' TO FIO-SIST.
           MOVE FIO-INP TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR APERTURA ARCHIVO GNSTAB: ABORTO'
               STOP RUN.

           MOVE FIO-UPD TO FIO-CMND.
           PERFORM CTB-FIO-COR.
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR APERTURA ARCHIVO CTBCOR: ABORTO'
               STOP RUN.

           DISPLAY ' INGRESE ESTADO DEL COMPROBANTE : M,I,V,* '.
           ACCEPT WSS-IND-ESTA.
      *>>>> END
*% END
*% IF INI_OUT
      *<<<< IF INI_OUT
           MOVE FIO-CLO TO FIO-CMND.
           PERFORM CTB-FIO-CPB.
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR CIERRE ARCHIVO CTBCPB: ABORTO'
               STOP RUN.

           MOVE FIO-UPD TO FIO-CMND.
           PERFORM CTB-FIO-CPB.
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR APERTURA ARCHIVO CTBCPB: ABORTO'
               STOP RUN.
           MOVE FIO-GET-FST TO FIO-CMND.
           PERFORM CTB-FIO-CPB.
      *>>>> END
*% END
*% IF CF_CPB_CPB_NUM_COMP
      *<<<< IF CF_CPB_CPB_NUM_COMP
      *OJO: NO SE PERMITE PERFORM EN DECLARATIVAS

      * INI BUSQUEDA DE USUARIO
      *========================
           MOVE 'CTB' TO TAB-COD-SIST.
           MOVE 'USR' TO TAB-COD-TTAB IN TAB.
           MOVE CPB-COD-AUTO IN SRT TO TAB-COD-CTAB IN TAB.
      *     PERFORM BUS-TAB.
      *INI BUS-TAB
           MOVE FIO-GET-KEY TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
           CALL TAB-GLS-FTAB USING FIO-CMND TAB.

           IF NOT FIO-STAT-OKS
               MOVE 'CODIGO INEXISTENTE' TO TAB-GLS-DESC.
      *FIN BUS-TAB
           MOVE TAB-GLS-DESC IN TAB TO WSS-GLS-AUTO IN WSS-CPB.

      * INI BUSQUEDA ESTADO DE COMPROBANTE
      *===================================
           MOVE 'CTB' TO TAB-COD-SIST.
           MOVE 'EST'              TO TAB-COD-TTAB IN TAB.
           MOVE CPB-IND-ESTA IN SRT TO TAB-COD-CTAB IN TAB.
      *     PERFORM BUS-TAB.
      *INI BUS-TAB
           MOVE FIO-GET-KEY TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
           CALL TAB-GLS-FTAB USING FIO-CMND TAB.

           IF NOT FIO-STAT-OKS
               MOVE 'CODIGO INEXISTENTE' TO TAB-GLS-DESC.
      *FIN BUS-TAB
           MOVE TAB-GLS-DESC IN TAB TO WSS-GLS-ESTA IN WSS-CPB.

      * INI BUSQUEDA TIPO DE COMPROBANTE
      *=================================
           MOVE 'CTB' TO TAB-COD-SIST.
           MOVE 'TCM'              TO TAB-COD-TTAB IN TAB.
           MOVE CPB-IND-TCOM IN SRT TO TAB-COD-CTAB IN TAB.
      *     PERFORM BUS-TAB.
      *INI BUS-TAB
           MOVE FIO-GET-KEY TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
           CALL TAB-GLS-FTAB USING FIO-CMND TAB.

           IF NOT FIO-STAT-OKS
               MOVE 'CODIGO INEXISTENTE' TO TAB-GLS-DESC.
      *FIN BUS-TAB

           MOVE TAB-GLS-DESC IN TAB TO WSS-GLS-TCOM IN WSS-CPB.
*%  END
*% IF FST_OUT 
      *<<<< IF FST_OUT
           IF PRIMER-MOVE
               MOVE CPB-IND-ESTA IN CPB TO CPB-IND-ESTA IN SRT
               MOVE CPB-IND-TCOM IN CPB TO CPB-IND-TCOM IN SRT
               PERFORM BUS-FST.
      *>>>> END
*% END
*% IF LUP_INP
      *<<<< IF LUP_INP
           IF WSS-IND-ESTA NOT = '*'
               IF WSS-IND-ESTA NOT = CPB-IND-ESTA IN CPB
                   MOVE FIO-GET-NXT TO FIO-CMND
                   PERFORM CTB-FIO-CPB
                   GO TO LUP-MAIN.
           PERFORM CPB-ESTA.
           IF FIO-STAT-OKS
               MOVE ZEROES TO MOV
               MOVE CPB-FEC-FCPB IN CPB TO MOV-FEC-FMOV IN MOV
               MOVE CPB-NUM-COMP IN CPB TO MOV-NUM-COMP IN MOV
               MOVE ZEROES TO MOV-NUM-MVTO IN MOV
               MOVE FIO-GET-NLS TO FIO-CMND
               PERFORM CTB-FIO-MOV.
       INI-MOV-INP-SRT.
           IF FIO-STAT-OKS
               IF ( CPB-FEC-FCPB IN CPB = MOV-FEC-FMOV IN MOV AND
                    CPB-NUM-COMP IN CPB = MOV-NUM-COMP IN MOV )
                     MOVE 'SUBCUENTA CONTABLE INEXISTENTE' TO
                                            CTB-GLS-CCTB
                     MOVE MOV-NUM-CCTB IN MOV TO CTB-NUM-CCTB IN CTB
                     MOVE FIO-GET-KEY TO FIO-CMND
                     PERFORM CTB-FIO-CTB
                     MOVE 'CENTRO DE COSTOS INEXISTENTE' TO
                                             CDC-GLS-CCDC
                     MOVE MOV-NUM-CCDC IN MOV TO CDC-NUM-CCDC IN CDC
                     MOVE '0' TO CDC-VRF-CCDC IN CDC
                     MOVE FIO-GET-KEY TO FIO-CMND
                     PERFORM CTB-FIO-CDC
                     IF MOV-IND-TRAT IN MOV = 'D'
                         MOVE MOV-VAL-MVTO IN MOV TO 
                         WSS-VAL-DEBE IN WSS-CPB
                         MOVE ZEROES              TO 
                         WSS-VAL-HABE IN WSS-CPB
                      ELSE
                         MOVE ZEROES              TO 
                         WSS-VAL-DEBE IN WSS-CPB
                         MOVE MOV-VAL-MVTO IN MOV TO 
                         WSS-VAL-HABE IN WSS-CPB
               ELSE
                      PERFORM CPB-ESTA
                      MOVE CPB-NUM-COMP IN CPB TO CPB-NUM-COMP IN SRT
                      MOVE CPB-NUM-DMOV IN CPB TO CPB-NUM-DMOV IN SRT
                      MOVE CPB-NUM-MMOV IN CPB TO CPB-NUM-MMOV IN SRT
                      MOVE CPB-NUM-SMOV IN CPB TO CPB-NUM-SMOV IN SRT
                      MOVE CPB-NUM-AMOV IN CPB TO CPB-NUM-AMOV IN SRT
                      MOVE CPB-GLS-COMP IN CPB TO CPB-GLS-COMP IN SRT
                      MOVE CPB-COD-AUTO IN CPB TO CPB-COD-AUTO IN SRT
                      MOVE CPB-IND-ESTA IN CPB TO CPB-IND-ESTA IN SRT
                      MOVE CPB-IND-TCOM IN CPB TO CPB-IND-TCOM IN SRT
                      GENERATE DL-NULL
                      MOVE FIO-GET-NXT TO FIO-CMND
                      PERFORM CTB-FIO-CPB
                      GO TO LUP-MAIN.
      *>>>> END
*% END
*% IF FIN_GEN_OUT
      *<<<< IF FIN-GEN_OUT
           MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM CTB-FIO-MOV.
           IF ( CPB-FEC-FCPB IN CPB = MOV-FEC-FMOV IN MOV AND
                CPB-NUM-COMP IN CPB = MOV-NUM-COMP IN MOV AND
                FIO-STAT-OKS )
                  GO TO INI-MOV-INP-SRT.
*% END
*% IF EOF
      *<<<< IF EOF

       ADD-COR SECTION.
       INI-ADD-COR.
           PERFORM KEY-ALL-COR.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM CTB-FIO-COR.
           IF FIO-STAT-OKS
               ADD  1 TO COR-NUM-CORR IN COR
               MOVE COR-NUM-CORR IN COR TO WSS-NUM-CORR
               MOVE FIO-MOD TO FIO-CMND
               PERFORM CTB-FIO-COR
           ELSE
               MOVE 1 TO COR-NUM-CORR IN COR
               MOVE COR-NUM-CORR IN COR TO WSS-NUM-CORR
               MOVE FIO-PUT TO FIO-CMND
               PERFORM CTB-FIO-COR.
           IF NOT FIO-STAT-OKS
               DISPLAY 'ASIGNACION DE CORRELATIVO ERRORNEA: ABORTO'
               STOP RUN.
       FIN-ADD-COR.
           EXIT.

       KEY-ALL-COR SECTION.
       INI-KEY-ALL-COR.
           MOVE ZEROES              TO COR-NUM-DCOR IN COR.
           MOVE CPB-NUM-MMOV IN CPB TO COR-NUM-MCOR IN COR.
           MOVE CPB-NUM-SMOV IN CPB TO COR-NUM-SCOR IN COR.
           MOVE CPB-NUM-AMOV IN CPB TO COR-NUM-ACOR IN COR.
       FIN-KEY-ALL-COR.
           EXIT.

       CPB-ESTA SECTION.
       INI-CPB-ESTA.
           IF CPB-IND-ESTA IN CPB = 'V'
               PERFORM ADD-COR
               MOVE 'I' TO CPB-IND-ESTA IN CPB
               MOVE WSS-NUM-CORR TO CPB-NUM-CUNA IN CPB
               MOVE WSS-NUM-CORR TO WSS-NUM-COMP IN WSS-CPB
               MOVE FIO-MOD TO FIO-CMND
               PERFORM CTB-FIO-CPB
               IF NOT FIO-STAT-OKS
                   DISPLAY 'ERROR MODIFICACION STATUS '
                           'COMPROBANTE V --> I : ABORTO'
                   STOP RUN
               ELSE
                   MOVE FIO-GET-KEY TO FIO-CMND
                   PERFORM CTB-FIO-CPB
           ELSE
           IF CPB-IND-ESTA IN CPB = 'I'
               MOVE CPB-NUM-CUNA IN CPB TO WSS-NUM-COMP IN WSS-CPB
           ELSE
               MOVE CPB-NUM-COMP IN CPB TO WSS-NUM-COMP IN WSS-CPB.
       FIN-CPB-ESTA.
           EXIT.

       BUS-FST SECTION.
       INI-BUS-FST.

      * INI BUSQUEDA ESTADO DE COMPROBANTE
      *===================================
           MOVE 'CTB' TO TAB-COD-SIST.
           MOVE 'EST'              TO TAB-COD-TTAB IN TAB.
           MOVE CPB-IND-ESTA IN SRT TO TAB-COD-CTAB IN TAB.
      *     PERFORM BUS-TAB.
      *INI BUS-TAB
           MOVE FIO-GET-KEY TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
           CALL TAB-GLS-FTAB USING FIO-CMND TAB.

           IF NOT FIO-STAT-OKS
               MOVE 'CODIGO INEXISTENTE' TO TAB-GLS-DESC.
      *FIN BUS-TAB
           MOVE TAB-GLS-DESC IN TAB TO WSS-GLS-ESTA IN WSS-CPB.

      * INI BUSQUEDA TIPO DE COMPROBANTE
      *=================================
           MOVE 'CTB' TO TAB-COD-SIST.
           MOVE 'TCM'              TO TAB-COD-TTAB IN TAB.
           MOVE CPB-IND-TCOM IN SRT TO TAB-COD-CTAB IN TAB.
      *     PERFORM BUS-TAB.
      *INI BUS-TAB
           MOVE FIO-GET-KEY TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
           CALL TAB-GLS-FTAB USING FIO-CMND TAB.

           IF NOT FIO-STAT-OKS
               MOVE 'CODIGO INEXISTENTE' TO TAB-GLS-DESC.
      *FIN BUS-TAB

           MOVE TAB-GLS-DESC IN TAB TO WSS-GLS-TCOM IN WSS-CPB.

       FIN-BUS-FST.
           EXIT.

       COPY CTBBFMOV.
       COPY CTBBFCTB.
       COPY CTBBFCDC.
       COPY GNSBBTAB.
       COPY GNSBBMSG.
       COPY CTBBFCOR.
      *>>>> END
*% END
