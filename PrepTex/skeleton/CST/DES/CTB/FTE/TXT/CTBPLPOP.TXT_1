*% IF WSS
      *<<<< IF WSS
       COPY CTBBRCTB.
       COPY CTBBRCDC.
       01  WSS-VARI.
           03 WSS-NUM-MPOP VALUE 0                    PIC 9(01).
           03 WSS-NUM-APOP VALUE 0                    PIC 9(01).
       01  WSS-PER.
           03 WSS-NUM-MPER                            PIC 9(02).
           03 WSS-NUM-SPER                            PIC 9(02).
           03 WSS-NUM-APER                            PIC 9(02).
      *>>>> END WSS
*% END
*% IF CH_POP_POP_NUM_MPOP
      *<<<< IF CH_POP_POP_NUM_MPOP
           IF WSS-NUM-MPOP = 0 
               MOVE 1 TO WSS-NUM-MPOP 
       COPY GNSBGSPI.
           .
      *>>>> END CH_POP_POP_NUM_MPOP
*% END
*% IF CH_POP_POP_NUM_APOP
      *<<<< IF CH_POP_POP_NUM_APOP
           IF WSS-NUM-APOP = 0 
               MOVE 1 TO WSS-NUM-APOP 
       COPY GNSBGSPI.
           .
      *>>>> END CH_POP_POP_NUM_APOP
*% END
*% IF CF_POP_POP_COD_NCTA
      *<<<< IF CF_POP_POP_COD_NCTA
      *     MOVE POP-COD-NCTA IN SRT TO WSS-NUM-SCT1.
      *     MOVE ZEROES              TO WSS-NUM-SCT2.
           MOVE POP-COD-NCTA IN SRT TO WSS-COD-NCTA.
           MOVE POP-COD-CMAY IN SRT TO WSS-COD-CMAY.
           MOVE ZEROES              TO WSS-COD-SCTA.
           MOVE POP-COD-NCTA IN SRT TO CTB-COD-NCTA IN CTB.
           MOVE POP-COD-CMAY IN SRT TO CTB-COD-CMAY IN CTB.
           MOVE ZEROES              TO CTB-COD-SCTA IN CTB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           CALL 'CTBPFCTB' USING FIO-VARI CTB.
      *     PERFORM CTB-FIO-CTB.
           IF FIO-STAT-OKS
               MOVE CTB-GLS-CCTB IN CTB TO WSS-GLS-CCTB
               MOVE CTB-VRF-CCTB IN CTB TO WSS-VRF-CCTB
           ELSE 
               MOVE 'CUENTA INEXISTENTE' TO WSS-GLS-CCTB
               MOVE SPACES TO WSS-VRF-CCTB.
      *     IF NOT FIO-STAT-OKS
      *         MOVE 'CUENTA INEXISTENTE' TO CTB-GLS-CCTB IN CTB.
      *>>>> END CF_POP_POP_COD_NCTA
*% END
*% IF INI_INP
      *<<<< IF INI_INP
           MOVE FIO-INP TO FIO-CMND.
           PERFORM CTB-FIO-CDC.
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR DE APERTURA ARCHIVO CDC: ABORTO'
               STOP RUN.
           MOVE FIO-INP TO FIO-CMND.
           PERFORM CTB-FIO-CTB.
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR DE APERTURA ARCHIVO CTB: ABORTO'
               STOP RUN.
           DISPLAY 'PERIODO: MMSSAA'.
           ACCEPT WSS-PER.
      *>>>> IF INI_INP
*% END
*% IF INI_FST_INP
      *     MOVE WSS-NUM-SPER TO POP-NUM-SPOP IN POP.
      *     MOVE WSS-NUM-APER TO POP-NUM-APOP IN POP.
      *     MOVE WSS-NUM-MPER TO POP-NUM-MPOP IN POP.
      *     MOVE FIO-GET-NLS TO FIO-CMND.
      *     PERFORM CTB-FIO-POP.
      *     GO TO LUP-INP-SRT.
*% END
*% IF FIN_FST_INP
           MOVE POP-COD-NCTA IN POP TO CTB-COD-NCTA IN CTB.
           MOVE POP-COD-CMAY IN POP TO CTB-COD-CMAY IN CTB.
           MOVE POP-COD-SCTA IN POP TO CTB-COD-SCTA IN CTB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM CTB-FIO-CTB.
           IF FIO-STAT-OKS
               MOVE CTB-GLS-CCTB IN CTB TO CTB-GLS-CCTB IN CTB
               MOVE CTB-VRF-CCTB IN CTB TO CTB-VRF-CCTB IN CTB
           ELSE 
               MOVE 'CUENTA INEXISTENTE' TO CTB-GLS-CCTB IN CTB
               MOVE SPACES               TO CTB-VRF-CCTB IN CTB.
*% END
*% IF LUP_INP

           IF NOT ( WSS-NUM-SPER =  POP-NUM-SPOP IN POP AND
                    WSS-NUM-APER =  POP-NUM-APOP IN POP AND
                    WSS-NUM-MPER =  POP-NUM-MPOP IN POP )
               GO TO NXT-INP-SRT.
      *    MOVE POP-COD-NCTA IN POP TO WSS-COD-NCTA.
      *    MOVE POP-COD-CMAY IN POP TO WSS-COD-CMAY.
      *    MOVE POP-COD-SCTA IN POP TO WSS-COD-SCTA.
      *    MOVE POP-NUM-CCTB IN POP TO CTB-NUM-CCTB IN CTB.
      *    MOVE FIO-GET-KEY TO FIO-CMND.
      *    PERFORM CTB-FIO-CTB.
      *    IF FIO-STAT-OKS
      *        MOVE CTB-GLS-CCTB IN CTB TO WSS-GLS-CCTB
      *        MOVE CTB-VRF-CCTB IN CTB TO WSS-VRF-CCTB
      *    ELSE 
      *        MOVE 'CUENTA INEXISTENTE' TO WSS-GLS-CCTB
      *        MOVE  SPACES              TO WSS-VRF-CCTB.
*% END
*% IF LUP_OUT
      *<<<< IF LUP_OUT
           MOVE POP-NUM-CCDC IN SRT TO CDC-NUM-CCDC IN CDC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM CTB-FIO-CDC.
           IF NOT FIO-STAT-OKS
               MOVE 'CENTRO DE COSTOS INEXISTENTE' 
                    TO CDC-GLS-CCDC
               MOVE SPACES TO CDC-VRF-CCDC.
           MOVE POP-COD-NCTA IN SRT TO CTB-COD-NCTA IN CTB.
           MOVE POP-COD-SCTA IN SRT TO CTB-COD-SCTA IN CTB.
           MOVE POP-COD-SCTA IN SRT TO CTB-COD-SCTA IN CTB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM CTB-FIO-CTB.
           IF FIO-STAT-OKS
               MOVE CTB-GLS-CCTB IN CTB TO WSS-GLS-CCTB
               MOVE CTB-VRF-CCTB IN CTB TO WSS-VRF-CCTB
           ELSE 
               MOVE 'CUENTA INEXISTENTE' TO WSS-GLS-CCTB
               MOVE SPACES TO WSS-VRF-CCTB.
      *>>>> END LUP_OUT
*% END
*% IF EOF
      *<<<< IF EOF
       COPY CTBBFCTB.
       COPY CTBBFCDC.
      *>>>> END EOF
*% END
