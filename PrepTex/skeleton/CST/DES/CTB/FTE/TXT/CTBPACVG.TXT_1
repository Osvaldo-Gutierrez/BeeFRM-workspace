*% IF FIN_SCR_VARI
      *<<<< IF FIN_SCR_VARI
           03  SCR-CVG.
               05  SCR-COD-ICVG.
                   07  SCR-NUM-CCTB                   PIC 9(08).
                   07  SCR-NUM-CCDC                   PIC 9(05).
      *>>>> END
*% END
*% IF WSS_DTC
      *<<<< IF WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSC-REQA==.
      *>>>> END
*% END
*% IF WSS
      *<<<< IF WSS
       COPY CTBBRCTB.
       COPY CTBBRCDC.
      *>>>> END
*% END
*% IF INI_FIO_VAX
      *<<<< INI_FIO_VAX
           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           PERFORM CTB-FIO-CTB.
           MOVE FIO-UPD TO FIO-CMND.
           PERFORM CTB-FIO-CTB.
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR DE APERTURA ARCHIVO CTBCTB: ABORTO'
               STOP RUN.
           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           PERFORM CTB-FIO-CDC.
           MOVE FIO-UPD TO FIO-CMND.
           PERFORM CTB-FIO-CDC.
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR DE APERTURA ARCHIVO CTBCDC: ABORTO'
               STOP RUN.
           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'CTB'        TO FIO-SIST.
           PERFORM GNS-FIO-MSG.
           MOVE FIO-INP TO FIO-CMND.
           MOVE 'CTB'   TO FIO-SIST.
           PERFORM GNS-FIO-MSG.
      *>>>> END
*% END
*% CMA_USR = TRUE
*% IF CMA
      *<<<< IF CMA
       COPY CTBLGCVG.
      *<<<< END CMA
*% END
*% IF INI_FIO_IBM
      *<<<< IF INI_FIO_IBM
           MOVE +1123 TO SCR-TCMA.
      *>>>> END INI_FIO_IBM
*% END
*% IF INI_FND_KEY
      *<<<< IF INI_FND_KEY
           MOVE CVG-NUM-CCTB IN CVG-FLD TO SCR-NUM-CCTB.
           MOVE CVG-NUM-CCDC IN CVG-FLD TO SCR-NUM-CCDC.
           IF SCR-CCPP = 'ING'
               MOVE 1 TO CVG-NUM-PERI IN CVG-FLD.
      *<<<< END INI_FND_KEY
*% END
*% IF FIN_FND_KEY
      *<<<< IF FIN_FND_KEY
           IF SCR-CCPP = 'ING'
               IF FIO-STAT-OKS
                  MOVE CVG-NUM-TPER IN CVG TO FRM-NUM-TPER IN CVG-FLD
                  COMPUTE CVG-NUM-PERI IN CVG =
                          CVG-NUM-TPER IN CVG + 1
                  MOVE CVG-NUM-PERI IN CVG TO CVG-NUM-PERI IN CVG-FLD
                  MOVE FIO-GET-KEY TO FIO-CMND
                  PERFORM CTB-FIO-CVG
               ELSE
                  MOVE ZEROES TO FRM-NUM-TPER IN CVG-FLD
                  MOVE 1 TO CVG-NUM-PERI IN CVG-FLD.
      *>>>> END FIN_FND_KEY
*% END
*% IF INI_CMN_ING
      *<<<< IF INI_CMN_ING
           IF CVG-NUM-PERI IN CVG-FLD = 1
               MOVE ZEROES TO CVG-NUM-TPER IN CVG.
      *>>>> END INI_CMN_ING
*% END
*% IF FIN_CMN_ING
      *<<<< IF FIN_CMN_ING
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ING.
           MOVE SCR-NUM-CCTB IN SCR-CVG TO CVG-NUM-CCTB IN CVG.
           MOVE SCR-NUM-CCDC IN SCR-CVG TO CVG-NUM-CCDC IN CVG.
           MOVE 1 TO CVG-NUM-PERI IN CVG.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM CTB-FIO-CVG.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ING.
           ADD 1 TO CVG-NUM-TPER IN CVG.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM CTB-FIO-CVG.
      *>>>> IF FIN_CMN_ING
*% END
*% IF EOF
      *<<<< IF EOF
       COPY CTBBFCTB.
       COPY CTBBFCDC.
      *>>>> IF EOF
*% END
