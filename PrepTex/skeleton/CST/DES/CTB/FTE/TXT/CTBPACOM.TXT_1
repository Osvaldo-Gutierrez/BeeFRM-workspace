      *  CODIGO ADICIONAL PARA PROGRAMA CTBPACOM.
*% IF FIN_SCR_VARI
       COPY CTBWIMOV REPLACING ==01  MOV-FLD== BY ==03  MOV-FLD==.
       COPY CTBWICOM REPLACING ==01  COM-FLD== BY ==03  COM-FLD==.
       COPY CTBWGMOV.
       COPY GNSWCVRF.
       COPY GNSWVVRF.
       COPY CTBBRCTB.
       COPY CTBBRCDC.
       COPY GNSBRTAB.
*% END
*% IF WSS
       COPY CTBBRCPB.
*% END
*% WIG_USR = TRUE
*% CMA_USR = TRUE
*% IF CMA
       COPY CTBLGMOV.
*% END
*% IF FIN_LUP_MAIN
           MOVE FRM-MENS TO SCR-MENS-EXT.
           IF SCR-INST = SCR-INST-HCM
               PERFORM EGR-FTER-CMN
           ELSE
               PERFORM EGR-FTER-CIC.
*% END
*% IF INI_FIO
           MOVE WSS-TCMA TO SCR-TCMA.
           IF SCR-INST-EXT = WSS-INST-FPEN-CIC
               MOVE WSS-INST-NULL TO SCR-INST-EXT
               PERFORM ING-FPEN-CIC.
*% END
*% IF FIN_CMN_ING
           IF MOV-NUM-MVTO IN MOV-FLD = 1
               PERFORM RET-CPB-MOV
               MOVE FIO-PUT TO FIO-CMND
               PERFORM CTB-FIO-CPB.
           ADD 1 TO MOV-NUM-MVTO IN MOV-FLD.
           IF MOV-NUM-MVTO IN MOV-FLD NOT > CPB-NUM-TMOV IN MOV-FLD
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM HBL-FLD
               PERFORM EGR-FPEN-BCK
           ELSE
               PERFORM EGR-FTER-CIC.
*% END
*% IF FIN_CMN_MOD
*% END
*% IF PFS_SCR_GFLD OR PFS_SCR_GCNF OR PFS_GET_ACC OR PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF7 )
*% END
*% IF SCR_GFLD_VAL_CON_FLD OR FIN_SCR_GCNF OR FIN_GET_ACC OR FIN_GET_CON
           IF FRM-FFLD = FRM-FFLD-PF7
               PERFORM EGR-FPEN-BCK.
*% END
*% IF EOF
       COPY CTBBMMOV.
       COPY CTBBFCPB.
       COPY CTBBFCDC.
       COPY CTBBFCTB.
       COPY GNSBPVRF.
       COPY GNSBVCOD.
       COPY GNSBVTAB.
       COPY GNSBFTAB.
       COPY GNSBVIND.
       COPY GNSBVMSC.
*% FRM = "COM"
*% INCLUDE 'GNSMMP'
*% END
