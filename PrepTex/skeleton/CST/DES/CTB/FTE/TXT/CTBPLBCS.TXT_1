*%      CODIGO ADICIONAL PARA PROGRAMA LISTADOR CTBPLBCS
*% IF WSS
       COPY GNSBRTAB.
       COPY GNSBRMSG.
       COPY CTBBRCTB.
       01  WSS-PER.
           03 WSS-NUM-MPER                            PIC 9(02).
           03 WSS-NUM-SPER                            PIC 9(02).
           03 WSS-NUM-APER                            PIC 9(02).
       01  WSS-VARI.
           03 WSS-NUM-CCTB                            PIC X(08).
*% END
*% IF CH_BCS_CTB_IND_GGRP
           IF CTB-NUM-CCTB IN CTB = WSS-NUM-CCTB
       COPY GNSBGSPI.
           ELSE
               MOVE CTB-NUM-CCTB IN CTB TO WSS-NUM-CCTB.
           ADD BCC-VAL-ADEB IN BCC TO WSS-VAL-TDEB IN WSS-BCS.
           ADD BCC-VAL-AHAB IN BCC TO WSS-VAL-THAB IN WSS-BCS.
           ADD BCC-VAL-SDEU IN BCC TO WSS-VAL-TDEU IN WSS-BCS.
           ADD BCC-VAL-SACR IN BCC TO WSS-VAL-TACR IN WSS-BCS.
*% END
*% IF CH_BCS_CTB_COD_SCTA
           IF CTB-NUM-CCTB IN CTB = WSS-NUM-CCTB
       COPY GNSBGSPI.
           ELSE
               MOVE CTB-NUM-CCTB IN CTB TO WSS-NUM-CCTB.
*% END
*% IF CH_BCS_CTB_IND_GRUP
           IF CTB-NUM-CCTB IN CTB = WSS-NUM-CCTB
       COPY GNSBGSPI.
           ELSE
               MOVE CTB-NUM-CCTB IN CTB TO WSS-NUM-CCTB.
*% END
*% IF CH_BCS_CTB_IND_SGRP
           IF CTB-NUM-CCTB IN CTB = WSS-NUM-CCTB
       COPY GNSBGSPI.
           ELSE
               MOVE CTB-NUM-CCTB IN CTB TO WSS-NUM-CCTB.
*% END
*% IF CH_BCS_CTB_COD_CMAY
           IF CTB-NUM-CCTB IN CTB = WSS-NUM-CCTB
       COPY GNSBGSPI.
           ELSE
               MOVE CTB-NUM-CCTB IN CTB TO WSS-NUM-CCTB.
*% END
*% IF INI_INP
           MOVE ZEROES TO WSS-VAL-TDEB IN WSS-BCS.
           MOVE ZEROES TO WSS-VAL-THAB IN WSS-BCS.
           MOVE ZEROES TO WSS-VAL-TDEU IN WSS-BCS.
           MOVE ZEROES TO WSS-VAL-TACR IN WSS-BCS.
           MOVE FIO-INP TO FIO-CMND.
           PERFORM CTB-FIO-CTB.
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR APERTURA ARCHIVO CTB: ABORTO'
               STOP RUN.
           MOVE 'CTB' TO FIO-SIST.
           MOVE FIO-INP TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR APERTURA ARCHIVO CTBTAB: ABORTO'
               STOP RUN.
           MOVE 'GNS' TO FIO-SIST.
           MOVE FIO-INP TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR APERTURA ARCHIVO GNSTAB: ABORTO'
               STOP RUN.
           DISPLAY ' PRIMER PERIODO DE PROCESO MMSSAA : '.
           ACCEPT WSS-PER.
           MOVE WSS-NUM-SPER IN WSS-PER TO WSS-NUM-SANO IN WSS-BCS.
           MOVE WSS-NUM-APER IN WSS-PER TO WSS-NUM-AANO IN WSS-BCS.
           MOVE WSS-NUM-MPER IN WSS-PER TO TAB-COD-CTAB.
           MOVE 'CTB' TO TAB-COD-SIST.
           MOVE 'MES' TO TAB-COD-TTAB.
           PERFORM BUS-TAB.
           MOVE TAB-GLS-DESC TO WSS-GLS-MES1 IN WSS-BCS.

           DISPLAY ' SEGUNDO PERIODO DE PROCESO MMSSAA : '.
           ACCEPT WSS-PER.
           MOVE 'MES' TO TAB-COD-TTAB.
           MOVE WSS-NUM-SPER IN WSS-PER TO WSS-NUM-SANO IN WSS-BCS.
           MOVE WSS-NUM-APER IN WSS-PER TO WSS-NUM-AANO IN WSS-BCS.
           MOVE WSS-NUM-MPER IN WSS-PER TO TAB-COD-CTAB.
           PERFORM BUS-TAB.
           MOVE TAB-GLS-DESC TO WSS-GLS-MES2 IN WSS-BCS.
*% END
*% IF LUP_INP
           MOVE BCC-NUM-CCTB TO CTB-NUM-CCTB IN CTB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM CTB-FIO-CTB.
           IF NOT FIO-STAT-OKS
               MOVE SPACES TO CTB-VRF-CCTB IN CTB
               MOVE 'CUENTA INEXISTENTE' TO CTB-GLS-CCTB IN CTB.
           IF BCC-VAL-ADEB IN BCC > BCC-VAL-AHAB IN BCC
               MOVE ZEROES TO BCC-VAL-SACR IN BCC
               COMPUTE BCC-VAL-SDEU IN BCC = 
                    BCC-VAL-ADEB IN BCC - BCC-VAL-AHAB IN BCC
           ELSE
               MOVE ZEROES TO BCC-VAL-SDEU IN BCC
               COMPUTE BCC-VAL-SACR IN BCC = 
                    BCC-VAL-AHAB IN BCC - BCC-VAL-ADEB IN BCC.
*% END
*% IF EOF   
       COPY CTBBFCTB.
       COPY GNSBSTAB.
       COPY GNSBSMSG.
       COPY GNSBFTAB.
       COPY GNSBFMSG.
*% END
