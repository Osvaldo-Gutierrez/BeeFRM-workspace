           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8
               PERFORM LEE-NXT-PRV
               IF FIO-STAT-OKS
                   PERFORM SCR-CLRA-CAZ
                   MOVE ZEROES TO VEZ-NUM-IVEN IN VEZ
                   MOVE ZEROES TO VEZ-NUM-IVEN IN CAZ-FLD
                   GO TO CON-FND-KEY
               ELSE
                   MOVE SPACES TO PGM-STAT
                   IF FRM-FFLD = FRM-FFLD-PF7
                       MOVE 'CBZ'          TO MSG-COD-SIST
                       MOVE 'CAZ    NOPRV' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE '00'         TO FIO-STAT
                       MOVE SCR-STAT-CON TO SCR-STAT
                       GO TO FIN-FND-KEY
                   ELSE
                       MOVE 'CBZ'          TO MSG-COD-SIST
                       MOVE 'CAZ    NONXT' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE '00'         TO FIO-STAT
                       MOVE SCR-STAT-CON TO SCR-STAT
                       GO TO FIN-FND-KEY.

           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM LEE-NXT-PRV-RCZ
               IF MSG-COD-MENS = 'RCVSI'
                   PERFORM SCR-CLRA-CAZ
                   MOVE RCZ-NUM-IVCT IN RCZ TO VEZ-NUM-IVEN IN CAZ-FLD
                   GO TO CON-FND-KEY
               ELSE
                   MOVE SPACES TO PGM-STAT
                   IF FRM-FFLD = FRM-FFLD-PF5
                       MOVE 'DEU'          TO MSG-COD-SIST
                       MOVE 'RCZIVCTNOPRV' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE '00'         TO FIO-STAT
                       MOVE SCR-STAT-CON TO SCR-STAT
                       GO TO FIN-FND-KEY
                   ELSE
                       MOVE 'DEU'          TO MSG-COD-SIST
                       MOVE 'RCZIVCTNONXT' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE '00'         TO FIO-STAT
                       MOVE SCR-STAT-CON TO SCR-STAT
                       GO TO FIN-FND-KEY.
       CON-FND-KEY.
           PERFORM GET-OPZ.
           MOVE FIO-STAT TO PGM-STAT-OPZ.
           IF NOT FIO-STAT-OKS
               MOVE 'CBZ'          TO MSG-COD-SIST
               MOVE 'CICIOPCNOOPZ' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FIO-STAT-NEX TO FIO-STAT
               GO TO FIN-FND-KEY.

           MOVE OPZ-COD-VCOC IN OPZ TO SCR-COD-VCOC.
           IF SCR-CCPP = 'ING'
              IF OPZ-COD-RMOC IN OPZ > SPACES
                 MOVE OPZ-COD-RMOC IN OPZ TO CAZ-COD-RCAN IN CAZ-FLD
                 MOVE CAZ-COD-RCAN IN CAZ-FLD TO USR-COD-USER IN USR
                 PERFORM BUS-USR
                 MOVE USR-GLS-DESC IN USR TO FRM-GLS-RCAN IN CAZ-FLD.

           IF OPZ-MSC-STAT IN OPZ NOT = 'A'
               MOVE 'CBZ'          TO MSG-COD-SIST
               MOVE 'OPZ    NOACA' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FIO-STAT-NEX TO FIO-STAT
               MOVE SCR-STAT-CON TO SCR-STAT
               GO TO FIN-FND-KEY.
           IF SCR-CCPP = 'ING'
               IF OPZ-VAL-SCRE IN OPZ NOT > ZEROES
                   MOVE 'CBZ'          TO MSG-COD-SIST
                   MOVE 'OPZ    YACAN' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FIO-STAT-NEX TO FIO-STAT
                   GO TO FIN-FND-KEY.

           IF SCR-CMND = 'REV'
               IF VEZ-NUM-IVCT IN CAZ-FLD NOT > ZEROES AND
                  OPZ-NUM-VCOR IN OPZ > 1
                   MOVE 'CBZ'          TO MSG-COD-SIST
                   MOVE 'REVIVCTDDZER' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FIO-STAT-NEX TO FIO-STAT
                   GO TO FIN-FND-KEY
               ELSE
                   MOVE CAZ-CAI-IOPC IN CAZ-FLD TO RCZ-CAI-IOPV IN RCZ
                   MOVE CAZ-IIC-IOPC IN CAZ-FLD TO RCZ-IIC-IOPV IN RCZ
                   MOVE VEZ-NUM-IVCT IN VEZ     TO RCZ-NUM-IVCT IN RCZ
                   MOVE 'RCZ-KEY-IVCT' TO FIO-AKEY
                   MOVE FIO-GET-KEY    TO FIO-CMND
                   PERFORM CBZ-FIO-RCZ
                   IF FIO-STAT-OKS
                       PERFORM ULT-RCZ-IVCT
                       MOVE RCZ-NUM-ICAN IN RCZ
                                         TO CAZ-NUM-ICAN IN CAZ-FLD
                       IF CAZ-MSC-CANC IN CAZ = 'ATT'
                           MOVE 'CBZ'          TO MSG-COD-SIST
                           MOVE 'REV    NOATT' TO MSG-COD-MENS
                           PERFORM GET-MSG
                           MOVE MSG-GLS-DESC TO FRM-MENS
                           MOVE FIO-STAT-NEX TO FIO-STAT
                           GO TO FIN-FND-KEY
                        ELSE
                           GO TO CON-BUS-KEY-IVEN
                     ELSE
                        MOVE 'CBZ'       TO MSG-COD-SIST
                        MOVE 'CAZNEXVEZ' TO MSG-COD-MENS
                        PERFORM GET-MSG
                        MOVE MSG-GLS-DESC TO FRM-MENS
                        MOVE FIO-STAT-NEX TO FIO-STAT
                        GO TO FIN-FND-KEY.
           IF SCR-CCPP = 'ING'
               PERFORM BUS-COR-CAZ
               MOVE CAZ-NUM-ICAN IN CAZ TO CAZ-NUM-ICAN IN CAZ-FLD
               GO TO CON-BUS-KEY-IVEN.
           IF CAZ-NUM-ICAN IN CAZ-FLD NOT > ZEROES AND
              VEZ-NUM-IVEN IN VEZ         > ZEROES
               MOVE CAZ-CAI-IOPC IN CAZ-FLD TO RCZ-CAI-IOPV IN RCZ
               MOVE CAZ-IIC-IOPC IN CAZ-FLD TO RCZ-IIC-IOPV IN RCZ
               MOVE VEZ-NUM-IVEN IN VEZ     TO RCZ-NUM-IVCT IN RCZ
               MOVE 'RCZ-KEY-IVCT' TO FIO-AKEY
               MOVE FIO-GET-KEY    TO FIO-CMND
               PERFORM CBZ-FIO-RCZ
               IF FIO-STAT-OKS
                   PERFORM ULT-RCZ-IVCT
                   MOVE RCZ-NUM-ICAN IN RCZ TO CAZ-NUM-ICAN IN CAZ-FLD
               ELSE
                   MOVE 'CBZ'       TO MSG-COD-SIST
                   MOVE 'CAZNEXVEZ' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FIO-STAT-NEX TO FIO-STAT
                   GO TO FIN-FND-KEY
           ELSE
             IF CAZ-NUM-ICAN IN CAZ-FLD NOT > ZEROES
                PERFORM BUS-COR-CAZ
                IF FRM-MENS > SPACES
                    GO TO FIN-FND-KEY
                ELSE
                   MOVE CAZ-NUM-ICAN IN CAZ TO CAZ-NUM-ICAN IN CAZ-FLD.
       CON-BUS-KEY-IVEN.
           MOVE CAZ-CAI-IOPC IN CAZ-FLD TO CAZ-CAI-IOPC IN CAZ.
           MOVE CAZ-IIC-IOPC IN CAZ-FLD TO CAZ-IIC-IOPC IN CAZ.
           MOVE CAZ-NUM-ICAN IN CAZ-FLD TO CAZ-NUM-ICAN IN CAZ.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM CBZ-FIO-CAZ.
           IF ( NOT FIO-STAT-OKS ) AND
              ( SCR-CCPP NOT = 'ING' ) AND
              ( CAZ-NUM-ICAN IN CAZ > 0 )
               MOVE 'CBZ' TO MSG-COD-SIST
               MOVE 'CAZ    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC IN MSG TO FRM-MENS
               MOVE FIO-STAT-NEX TO FIO-STAT
               GO TO FIN-FND-KEY.
           IF FIO-STAT-OKS
               MOVE CAZ-CIC-IOPC IN CAZ TO RCZ-CIC-IOPC IN RCZ
               MOVE CAZ-NUM-ICAN IN CAZ TO RCZ-NUM-ICAN IN RCZ
               MOVE SPACES              TO RCZ-CIC-IOPV IN RCZ
               MOVE ZEROES              TO RCZ-NUM-IVCT IN RCZ
               MOVE FIO-GET-NLS TO FIO-CMND
               PERFORM CBZ-FIO-RCZ
               IF NOT (FIO-STAT-OKS AND
                  CAZ-CIC-IOPC IN CAZ = RCZ-CIC-IOPC IN RCZ AND
                  CAZ-NUM-ICAN IN CAZ = RCZ-NUM-ICAN IN RCZ )
                   MOVE '23' TO FIO-STAT
               ELSE
                   MOVE '00' TO FIO-STAT
                   IF RCZ-NUM-IVCT IN RCZ > ZEROES
                       MOVE RCZ-NUM-IVCT IN RCZ TO
                                       VEZ-NUM-IVEN IN VEZ
                       MOVE RCZ-NUM-IVCT IN RCZ TO
                                   VEZ-NUM-IVEN IN CAZ-FLD
                       MOVE RCZ-NUM-IVCT IN RCZ TO
                                   VEZ-NUM-IVCT IN CAZ-FLD.
           MOVE FIO-STAT TO PGM-STAT-RCZ.
           IF VEZ-NUM-IVEN IN VEZ NOT > ZEROES
               MOVE 'CBZ'          TO MSG-COD-SIST
               MOVE 'CAZVEZDESCNU' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FIO-STAT-NEX TO FIO-STAT
               GO TO FIN-FND-KEY
           ELSE
              IF SCR-CCPP = 'ING'
                 IF VEZ-NUM-IVCT IN CAZ-FLD < OPZ-NUM-IVEN IN OPZ
                     MOVE OPZ-CIC-IOPC IN OPZ TO VEZ-CIC-IOPC
                                                       IN VEZ
                     MOVE VEZ-NUM-IVCT IN CAZ-FLD TO VEZ-NUM-IVEN
                                                       IN VEZ
                     MOVE FIO-GET-KEY TO FIO-CMND
                     PERFORM CBZ-FIO-VEZ
                     IF NOT FIO-STAT-OKS
                         MOVE 'CBZ'          TO MSG-COD-SIST
                         MOVE 'VEZ    NEX'   TO MSG-COD-MENS
                         PERFORM GET-MSG
                         MOVE MSG-GLS-DESC TO FRM-MENS
                         MOVE FIO-STAT-NEX TO FIO-STAT
                         GO TO FIN-FND-KEY
                     ELSE
                         IF VEZ-VAL-SFIN IN VEZ NOT > 0
                             MOVE 'CBZ'          TO MSG-COD-SIST
                             MOVE 'VEZ  YACAN'   TO MSG-COD-MENS
                             PERFORM GET-MSG
                             MOVE MSG-GLS-DESC TO FRM-MENS
                             MOVE FIO-STAT-NEX TO FIO-STAT
                             GO TO FIN-FND-KEY
                         ELSE
                             NEXT SENTENCE
                 ELSE
                     MOVE OPZ-CIC-IOPC IN OPZ TO VEZ-CIC-IOPC IN VEZ
                     MOVE VEZ-NUM-IVCT IN CAZ-FLD TO VEZ-NUM-IVEN
                                                              IN VEZ
                     MOVE FIO-GET-KEY TO FIO-CMND
                     PERFORM CBZ-FIO-VEZ
                     IF NOT FIO-STAT-OKS
                         MOVE 'CBZ'          TO MSG-COD-SIST
                         MOVE 'VEZ    NEX'   TO MSG-COD-MENS
                         PERFORM GET-MSG
                         MOVE MSG-GLS-DESC TO FRM-MENS
                         MOVE FIO-STAT-NEX TO FIO-STAT
                         GO TO FIN-FND-KEY
                     ELSE
                         IF VEZ-VAL-SFIN IN VEZ NOT > 0
                             MOVE 'CBZ'          TO MSG-COD-SIST
                             MOVE 'VEZ  YACAN'   TO MSG-COD-MENS
                             PERFORM GET-MSG
                             MOVE MSG-GLS-DESC TO FRM-MENS
                             MOVE FIO-STAT-NEX TO FIO-STAT
                             GO TO FIN-FND-KEY.
       NOT-VUV.
           MOVE VEZ-NUM-IVEN IN VEZ TO SCR-VEN-NUM-IVEN.
           MOVE CAZ-CIC-IOPC IN CAZ TO VEZ-CIC-IOPC IN VEZ.
           MOVE SCR-VEN-NUM-IVEN TO VEZ-NUM-IVEN IN VEZ.
           MOVE SCR-VEN-NUM-IVEN TO VEZ-NUM-IVEN IN CAZ-FLD.

           MOVE FIO-GET-KEY TO FIO-CMND.

           PERFORM CBZ-FIO-VEZ.

           MOVE FIO-STAT TO PGM-STAT-VEZ.
           IF NOT FIO-STAT-OKS
               MOVE 'CBZ' TO MSG-COD-SIST
               IF OPZ-VAL-SCRE IN OPZ NOT > ZEROES
                   MOVE 'OPZ    YACAN' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FIO-STAT-NEX TO FIO-STAT
                   GO TO FIN-FND-KEY
               ELSE
                   MOVE 'CICIOPCNOVEZ' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FIO-STAT-NEX TO FIO-STAT
                   GO TO FIN-FND-KEY.
       VEZ-ESTR.
           IF SCR-CCPP NOT = 'ING'
                GO TO FIN-VRF-RCZ.
           MOVE CAZ-CIC-IOPC IN CAZ TO RCZ-CIC-IOPV IN RCZ.
           MOVE ZEROES              TO RCZ-NUM-IVCT IN RCZ.
           MOVE SPACES TO WSS-RCV-CIC-IOPC.
           MOVE 0      TO WSS-RCV-NUM-ICAN.
           MOVE 'P'    TO WSS-COD-IPAR.
           MOVE FIO-GET-NLS TO FIO-CMND.
       LUR-FND-KEY.
           MOVE 'RCZ-KEY-IVCT' TO FIO-AKEY.
           PERFORM CBZ-FIO-RCZ.
           IF NOT ( FIO-STAT-OKS AND
                    RCZ-CIC-IOPV IN RCZ = CAZ-CIC-IOPC IN CAZ AND
                    RCZ-NUM-IVCT IN RCZ = 0 )
               GO TO SK1-FND-KEY.
           IF RCZ-NUM-ICAN IN RCZ > WSS-RCV-NUM-ICAN
               MOVE RCZ-KEY-IRCV IN RCZ TO WSS-RCV-KEY-IRCV.
           IF WSS-COD-IPAR = 'P'
               MOVE 'I' TO WSS-COD-IPAR
           ELSE
               MOVE 'P' TO WSS-COD-IPAR.
           MOVE FIO-GET-NXT TO FIO-CMND.
           GO TO LUR-FND-KEY.
       SK1-FND-KEY.
           IF WSS-RCV-CIC-IOPC > SPACES
               MOVE WSS-RCV-KEY-IRCV TO RCZ-KEY-IRCV IN RCZ
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM CBZ-FIO-RCZ
           ELSE
               MOVE '23' TO FIO-STAT.
           IF NOT FIO-STAT-OKS
               GO TO SK2-FND-KEY.
           MOVE RCZ-KEY-ICAN IN RCZ TO CAZ-KEY-ICAN IN CAZ.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM CBZ-FIO-CAZ.
           IF FIO-STAT-OKS
               IF ( RCZ-SGV-FINA IN RCZ < ZEROES   OR
                    RCZ-SGV-CAPI IN RCZ < ZEROES ) OR
                    WSS-COD-IPAR = 'P'
                     NEXT SENTENCE
               ELSE
                   MOVE VEZ-NUM-IVEN IN VEZ TO VEZ-NUM-IVCT IN CAZ-FLD
                   IF CAZ-MSC-STAT IN CAZ = 'A'
                       MOVE 'CBZ' TO MSG-COD-SIST
                       MOVE 'OPZ    YACAN' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE FIO-STAT-NEX TO FIO-STAT
                       GO TO FIN-FND-KEY
                   ELSE
                       MOVE 'CBZ' TO MSG-COD-SIST
                       MOVE 'CAZTOTAYAEXI' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC        TO FRM-MEN1
                       MOVE CAZ-MSC-STAT IN CAZ TO FRM-MEN2
                       MOVE FIO-STAT-NEX TO FIO-STAT
                       GO TO FIN-FND-KEY
           ELSE
               MOVE 'CBZ' TO MSG-COD-SIST
               MOVE 'RCZIVCTNOCAN' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC        TO FRM-MEN1
               MOVE RCZ-KEY-IRCV IN RCZ TO FRM-MEN2
               MOVE FIO-STAT-NEX TO FIO-STAT
               GO TO FIN-FND-KEY.
       SK2-FND-KEY.
           MOVE OPZ-IND-PORC IN OPZ TO FRM-IND-PORT IN CAZ-FLD.
           IF OPZ-IND-PORC IN OPZ NOT =  SPACES
              MOVE FRM-CPIM-AEY TO FRM-IND-PORT-ATR IN CAZ-FLD.
           MOVE CAZ-CAI-IOPC IN CAZ-FLD TO ICZ-CAI-IOPC IN ICZ.
           MOVE CAZ-IIC-IOPC IN CAZ-FLD TO ICZ-IIC-IOPC IN ICZ.
           MOVE CAZ-NUM-ICAN IN CAZ-FLD TO ICZ-NUM-ICAN IN ICZ.
           MOVE CAZ-CAI-IOPC IN CAZ-FLD TO ICZ-CAI-IOPV IN ICZ.
           MOVE CAZ-IIC-IOPC IN CAZ-FLD TO ICZ-IIC-IOPV IN ICZ.
           IF SCR-MSC-FCAN = 'T' AND
              SCR-MSC-ECAN = 'T'
              MOVE ZEROES                  TO ICZ-NUM-IVCT IN ICZ
           ELSE
              MOVE VEZ-NUM-IVEN IN CAZ-FLD TO ICZ-NUM-IVCT IN ICZ.
           MOVE 'COMFLV'                TO ICZ-IND-TTAS IN ICZ.
           MOVE CAZ-NUM-SCAN IN CAZ-FLD TO ICZ-NUM-SINI IN ICZ.
           MOVE CAZ-NUM-ACAN IN CAZ-FLD TO ICZ-NUM-AINI IN ICZ.
           MOVE CAZ-NUM-MCAN IN CAZ-FLD TO ICZ-NUM-MINI IN ICZ.
           MOVE CAZ-NUM-DCAN IN CAZ-FLD TO ICZ-NUM-DINI IN ICZ.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM CBZ-FIO-ICZ.
           IF FIO-STAT-OKS
              MOVE ICZ-SGV-TINT IN ICZ TO FRM-SGV-COMI IN CAZ-FLD
           ELSE
              PERFORM LEE-COZ-CAZ-CUR
              IF FIO-STAT-OKS
                 MOVE ICZ-SGV-TINT IN ICZ TO FRM-SGV-COMI IN CAZ-FLD
              ELSE
                 MOVE ZEROES TO FRM-SGV-COMI IN CAZ-FLD.
           MOVE 'RCZ-KEY-IVCT' TO FIO-AKEY.
           MOVE CAZ-CIC-IOPC IN CAZ TO RCZ-CIC-IOPV IN RCZ.
           MOVE VEZ-NUM-IVEN IN VEZ TO RCZ-NUM-IVCT IN RCZ.
           MOVE FIO-GET-LEQ TO FIO-CMND.
           PERFORM CBZ-FIO-RCZ.
           IF NOT ( FIO-STAT-OKS AND
              CAZ-CIC-IOPC IN CAZ = RCZ-CIC-IOPV IN RCZ )
               NEXT SENTENCE
           ELSE
               ADD 1 RCZ-NUM-IVCT IN RCZ GIVING WSS-NUM-IVCT
               PERFORM ULT-RCZ-IVCT
               MOVE RCZ-NUM-ICAN IN RCZ TO CAZ-NUM-ICAN IN CAZ
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM CBZ-FIO-CAZ
               IF FIO-STAT-OKS
                   IF ( NOT ( CAZ-MSC-STAT IN CAZ = 'A' OR
                                   'R' OR 'X' ) )
                         MOVE VEZ-NUM-IVEN IN VEZ TO
                              VEZ-NUM-IVCT IN CAZ-FLD
                         MOVE 'CBZ' TO MSG-COD-SIST
                         MOVE 'RCZIVCTIVEN' TO MSG-COD-MENS
                         PERFORM GET-MSG
                         MOVE MSG-GLS-DESC TO FRM-MENS
                         MOVE FIO-STAT-NEX TO FIO-STAT
                         GO TO FIN-FND-KEY
                     ELSE
                        IF NOT ( CAZ-MSC-STAT IN CAZ =
                        'A' OR 'S' OR 'C' OR 'W' OR 'R' OR
                        'G' OR 'P' OR 'X' )
                            MOVE VEZ-NUM-IVEN IN VEZ TO
                                  VEZ-NUM-IVCT IN CAZ-FLD
                            MOVE 'CBZ' TO MSG-COD-SIST
                            MOVE 'RCZIVCTIVEN' TO MSG-COD-MENS
                            PERFORM GET-MSG
                            MOVE MSG-GLS-DESC TO FRM-MENS
                            MOVE FIO-STAT-NEX TO FIO-STAT
                            GO TO FIN-FND-KEY
                        ELSE
                            IF VEZ-NUM-IVEN IN VEZ NOT = RCZ-NUM-IVCT
                               IN RCZ AND CAZ-MSC-FCAN IN CAZ NOT = 'T'
                                MOVE VEZ-NUM-IVEN IN VEZ TO
                                     VEZ-NUM-IVCT IN CAZ-FLD
                                MOVE 'CBZ' TO MSG-COD-SIST
                                MOVE 'RCZIVCTIVEN' TO MSG-COD-MENS
                                PERFORM GET-MSG
                                MOVE MSG-GLS-DESC TO FRM-MENS
                                MOVE FIO-STAT-NEX TO FIO-STAT
                                GO TO FIN-FND-KEY
                            ELSE
                                NEXT SENTENCE
                    ELSE
                        MOVE 'CBZ' TO MSG-COD-SIST
                        MOVE 'RCZIVCTNOCAN' TO MSG-COD-MENS
                        PERFORM GET-MSG
                        MOVE MSG-GLS-DESC TO FRM-MENS
                        MOVE FIO-STAT-NEX TO FIO-STAT
                        GO TO FIN-FND-KEY.

           MOVE CAZ-NUM-ICAN IN CAZ-FLD TO CAZ-NUM-ICAN IN CAZ.
       FIN-VRF-RCZ.
           PERFORM GET-CUR-ICZ-MOR.
           IF PGM-STAT-ICZ-OKS
               MOVE ICZ-SGV-TINT IN ICZ TO ICZ-SGV-TINT  IN CAZ-FLD
               MOVE 'N'                 TO WSS-IBLK-TINT IN WSS-VARI
               PERFORM GET-MON
               IF WSS-CIC-TVAL = '1'
                   MOVE 'EFE ANUA'    TO FRM-GLS-TINT IN CAZ-FLD
               ELSE
                   MOVE 'EFE MENS'    TO FRM-GLS-TINT IN CAZ-FLD
           ELSE
               MOVE ZEROES              TO ICZ-SGV-TINT  IN CAZ-FLD
               MOVE 'S'                 TO WSS-IBLK-TINT IN WSS-VARI
               MOVE SPACES              TO FRM-GLS-TINT  IN CAZ-FLD.

           MOVE CAZ-CAI-IOPC IN CAZ TO RDZ-CAI-IOPC IN RDZ.
           MOVE CAZ-IIC-IOPC IN CAZ TO RDZ-IIC-IOPC IN RDZ.
           MOVE ZEROES              TO RDZ-NUM-IDLC IN RDZ.
           MOVE 'RDZ-KEY-IDLC' TO FIO-AKEY.
           MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM CBZ-FIO-RDZ.
           IF NOT (FIO-STAT-OKS AND
                   RDZ-CAI-IOPC IN RDZ = CAZ-CAI-IOPC IN CAZ AND
                   RDZ-IIC-IOPC IN RDZ = CAZ-IIC-IOPC IN CAZ AND
                   RDZ-NUM-IDLC IN RDZ = ZEROES )
               MOVE 'SGC'        TO MSG-COD-SIST
               MOVE 'CLI    NEX' TO MSG-COD-MENS
               MOVE SPACES TO DBC-IND-ICLI IN DBC
               PERFORM SET-MSG-CLTE
               GO TO CON-PGD.
           MOVE RDZ-CIC-ICLI IN RDZ TO DBC-CIC-ICLI IN DBC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           MOVE FIO-STAT TO PGM-STAT-DBC.
           IF NOT FIO-STAT-OKS
               MOVE 'SGC'        TO MSG-COD-SIST
               MOVE 'CLI    NEX' TO MSG-COD-MENS
               MOVE SPACES TO DBC-IND-ICLI IN DBC
               PERFORM SET-MSG-CLTE
               GO TO CON-PGD.
           MOVE 'RDZ-KEY-IDLC' TO FIO-AKEY.
           MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM CBZ-FIO-RDZ.
           IF FIO-STAT-OKS AND
              RDZ-CAI-IOPC IN RDZ = CAZ-CAI-IOPC IN CAZ AND
              RDZ-IIC-IOPC IN RDZ = CAZ-IIC-IOPC IN CAZ AND
              RDZ-NUM-IDLC IN RDZ = ZEROES
               MOVE 'CBZ'          TO MSG-COD-SIST
               MOVE 'OPZ    MDDIR' TO MSG-COD-MENS
               MOVE 'M'     TO DBC-IND-ICLI IN DBC
               PERFORM SET-MSG-CLTE.
       CON-PGD.

           PERFORM FND-KEY-CYZ.
       NXT-FND-KEY.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8
               GO TO FND-RST-STAT.
