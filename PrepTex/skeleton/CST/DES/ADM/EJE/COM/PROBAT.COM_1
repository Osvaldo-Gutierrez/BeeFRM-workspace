$ OPR = F$EDIT("''GNS$OPR'","TRIM")
$ T = F$EDIT("''F$TIME()'","COLLAPSE")
$ T1 = F$ELEMENT(0,":",T)
$ T2 = F$ELEMENT(1,":",T)
$ T3 = F$ELEMENT(2,":",T)
$ T = F$ELEMENT(0,"-",T1)+F$ELEMENT(1,"-",T1)+F$ELEMENT(2,"-",T1)
$ T = T + T2 + F$ELEMENT(0,".",T3)+F$ELEMENT(1,".",T3)
$ DEFINE/NOLOG BCH TMP:'OPR''T'.TMP
$ DEFINE/NOLOG SYS$INPUT SYS$COMMAND
$ RUN GNSEXE:GNSPPBAT
$ EXIT
