       PROGRAM-ID. ADMPAPDT.
       AUTHOR.     CONSIST.
       DATE-WRITTEN. 19-May-89 08:28 PM.

      * ACTUALIZA ARCHIVO ADMPDT EN FORMA INTERACTIVA.

       ENVIRONMENT DIVISION.
      *=====================
       CONFIGURATION SECTION.
      *----------------------
       SPECIAL-NAMES.
               DECIMAL-POINT IS COMMA.

       DATA DIVISION.
      *==============

       WORKING-STORAGE SECTION.
      *------------------------
       COPY ADMBRPDT.
       COPY GNSBRTAB.
       COPY GNSBRMSG.
       COPY GNSWCFIO.
       COPY GNSWVFIO.
       COPY GNSWGFRM.
       COPY GNSWGSCR.


       COPY ADMWIPDT.
       COPY GNSWGFEC.
       COPY GNSWGHOY.
       COPY GNSWGSYS.
      *<<<< WSS
       COPY ADMBRRAA.
      *>>>>
      *<<<< WSS_VAX
       COPY GNSBRMSC.
       01 WSS-VARI.
           03  WSS-COD-TACC                           PIC X(03).
           03  WSS-NUM-CON                            PIC 9(03).
           03  WSS-FEC-FTPR.
               05  WSS-NUM-DTPR                       PIC 9(02).
               05  WSS-NUM-MTPR                       PIC 9(02).
               05  WSS-NUM-STPR                       PIC 9(02).
               05  WSS-NUM-ATPR                       PIC 9(02).
           03  WSS-COD-FUNC.  
               05  WSS-COD-APRG                       PIC X(12).
               05  WSS-COD-RESP                       PIC X(12).
               05  WSS-COD-INGR                       PIC X(12).
               05  WSS-COD-EJEC                       PIC X(12).
           03  WSS-IND-ACTI                           PIC X(01).
       01  WSS-PDT.
           03  WSS-REG-PDT                            PIC X(117).
      *>>>>
       01  PGM-VARI.
           03 PGM-ICLR                  VALUE ' '       PIC X(01).
           03 PGM-LIMPIAR-TODO          VALUE ' '       PIC X(01).
           03 PGM-LIMPIAR-LLAVES        VALUE 'K'       PIC X(01).
           03 PGM-LIMPIAR-CAMPOS        VALUE 'F'       PIC X(01).
           03 PGM-NO-LIMPIAR-NADA       VALUE 'N'       PIC X(01).
           03 PGM-STAT.
              05 PGM-STAT-PDT           VALUE '23'      PIC X(02).
                 88 PGM-STAT-PDT-OKS    VALUE '00'.
                 88 PGM-STAT-PDT-NUL    VALUE '  '.
              05 PGM-STAT-RAA           VALUE '23'      PIC X(02).
                 88 PGM-STAT-RAA-OKS    VALUE '00'.
                 88 PGM-STAT-RAA-NUL    VALUE '  '.
           03 PGM-SRNF                  VALUE '23'      PIC X(02).
           03 PGM-SOKS                  VALUE '00'      PIC X(02).
           03 PGM-SNUL                  VALUE '  '      PIC X(02).
           03 PGM-NCUR                  VALUE "[?25l" PIC X(06).
           03 PGM-SCUR                  VALUE "[?25h" PIC X(06).

       PROCEDURE DIVISION.
      *===================

       MAIN SECTION.
       INI-MAIN.
           PERFORM INI.
       LUP-MAIN.
           IF SCR-INST = SCR-INST-HCM
                 PERFORM SCR-COM.
           IF SCR-CCPP = 'MOD'
                 PERFORM SCR-MOD
           ELSE
           IF SCR-CCPP = 'ING'
                 PERFORM SCR-ING
           ELSE
           IF SCR-CCPP = 'ACC'
                 PERFORM SCR-ACC
           ELSE
           IF SCR-CCPP = 'ELI'
                 PERFORM SCR-ELI
           ELSE
           IF SCR-CCPP = 'ACT'
                 PERFORM SCR-ACT
           ELSE
                 PERFORM SCR-PGM.
           GO TO LUP-MAIN.
       FIN-MAIN.
           EXIT.

       INI SECTION.
       INI-INI.
           MOVE 'ADMPAPDT' TO SCR-PROG.
      *<<<< INI_FIO
           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           PERFORM ADM-FIO-RAA.
           MOVE FIO-UPD      TO FIO-CMND.
           PERFORM ADM-FIO-RAA.
      *>>>>
           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'GNS'        TO FIO-SIST.
           PERFORM GNS-FIO-MSG.
           MOVE FIO-INP TO FIO-CMND.
           MOVE 'GNS'   TO FIO-SIST.
           PERFORM GNS-FIO-MSG.
      *<<<< INI_FIO_VAX
           MOVE 'ADM'        TO FIO-SIST.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           PERFORM GNS-FIO-TAB.
           MOVE 'ADM'        TO FIO-SIST.
           MOVE FIO-INP      TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
      *>>>>
           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           PERFORM ADM-FIO-PDT.
           MOVE FIO-UPD TO FIO-CMND.
           PERFORM ADM-FIO-PDT.
           CALL 'GNSPPHDR'.
      *    MOVE SCR-NLIB TO FRM-NLIB.
      *    MOVE SCR-NMAP TO FRM-NAME.
           MOVE 'ADMFMS' TO FRM-NLIB.
           MOVE 'PDT' TO FRM-NAME.
           MOVE FRM-SINI-DBNF TO FRM-SINI.
           MOVE FRM-INI TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       CMN-INI.
           MOVE SCR-INST-HCM TO SCR-INST.
           INITIALIZE PDT-DFLD.
           INITIALIZE PDT-DFLD-ATR.
           INITIALIZE PDT-DFLD-LEN.
           MOVE PDT-CFLD TO FRM-CFLD.
       FIN-INI.
           EXIT.

       SCR-COM SECTION.
       INI-SCR-COM.
      *     MOVE FRM-MSG TO FRM-CMND.
      *     MOVE PDT-GLS-MENS TO FRM-MENS.
      *     PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
      *     PERFORM SCR-CLRA-PDT.
           CALL 'GNSPPCMN'.
       FIN-SCR-COM.
           EXIT.

       SCR-MOD SECTION.
       INI-SCR-MOD.
           PERFORM HBL-KEY.
           PERFORM SCR-KEY.
           IF SCR-STAT = SCR-STAT-ABT
               PERFORM DESSCRTRL
               GO TO FIN-SCR-MOD.
       TRL-SCR-MOD.
           PERFORM SCR-TRL.
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD.
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON.
           PERFORM DESSCRTRL.
           IF SCR-FFLD-BSP
               GO TO TRL-SCR-MOD.
       MSG-SCR-MOD.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'MODNOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-MOD
               IF FIO-STAT-OKS
                   IF FRM-MENS NOT > SPACES
                       MOVE 'MODYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
       FIN-SCR-MOD.
           PERFORM SCR-DMSG.

       SCR-ING SECTION.
       INI-SCR-ING.
           PERFORM HBL-IKY.
           PERFORM SCR-RST.
           PERFORM GET-IKY.

           IF FRM-STAT-ABT
               MOVE SPACES TO FRM-MENS
               PERFORM DESSCRTRL
               GO TO FIN-SCR-ING.

           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
           IF FIO-STAT-OKS OR FIO-STAT = FIO-STAT-NEX
               MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
               IF FRM-MENS NOT > SPACES
                   MOVE 'REGEXI    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE SCR-STAT-ABT TO SCR-STAT
                   PERFORM DESSCRTRL
                   GO TO FIN-SCR-ING
               ELSE
                   MOVE SCR-STAT-ABT TO SCR-STAT
                   PERFORM DESSCRTRL
                   GO TO FIN-SCR-ING.
           MOVE PGM-LIMPIAR-TODO TO PGM-ICLR.
       TRL-SCR-ING.
           PERFORM SCR-TRL.
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD.
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON.
           PERFORM DESSCRTRL.
           IF SCR-FFLD-BSP
               GO TO TRL-SCR-ING.
       MSG-SCR-ING.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'INGNOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-ING
               IF FIO-STAT-OKS
                   IF FRM-MENS NOT > SPACES
                       MOVE 'INGYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
       FIN-SCR-ING.
           PERFORM SCR-DMSG.

       SCR-ACC SECTION.
       INI-SCR-ACC.
           PERFORM HBL-KEY.
           PERFORM SCR-KEY.
           IF SCR-STAT = SCR-STAT-ABT
               PERFORM DESSCRTRL
               GO TO FIN-SCR-ACC.
       TRL-SCR-ACC.
           PERFORM SCR-TRL.
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD.
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON.
           PERFORM DESSCRTRL.
           IF SCR-FFLD-BSP
               GO TO TRL-SCR-ACC.
           MOVE SPACES TO FRM-MENS.
           IF SCR-STAT NOT = SCR-STAT-CON
               PERFORM CMN-ACC.
       FIN-SCR-ACC.
           PERFORM SCR-DMSG.

       SCR-ELI SECTION.
       INI-SCR-ELI.
           PERFORM HBL-KEY.
           PERFORM SCR-KEY.
           IF SCR-STAT = SCR-STAT-ABT
               PERFORM DESSCRTRL
               GO TO FIN-SCR-ELI.
       TRL-SCR-ELI.
           PERFORM SCR-TRL.
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON.
           PERFORM DESSCRTRL.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'ELINOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-ELI
               IF FIO-STAT-OKS
                   IF FRM-MENS NOT > SPACES
                       MOVE 'ELIYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
       FIN-SCR-ELI.
           PERFORM SCR-DMSG.

       SCR-ACT SECTION.
       INI-SCR-ACT.
           PERFORM HBL-KEY.
           PERFORM SCR-RST.
           PERFORM GET-KEY.

           IF FRM-STAT-ABT
               MOVE SPACES TO FRM-MENS
               PERFORM DESSCRTRL
               GO TO FIN-SCR-ACT.

           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
           IF FIO-STAT-OKS
               MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
               MOVE SCR-ACTU-MOD TO SCR-ACTU
               PERFORM PUT-SCR
           ELSE
           MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
           IF FIO-STAT-EOF
               IF FRM-MENS NOT > SPACES
                   MOVE 'REGEOF    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE SCR-STAT-ABT TO SCR-STAT
                   PERFORM DESSCRTRL
                   GO TO FIN-SCR-ACT
               ELSE
                   MOVE SCR-STAT-ABT TO SCR-STAT
                   PERFORM DESSCRTRL
                   GO TO FIN-SCR-ACT
           ELSE
           IF FIO-STAT = FIO-STAT-NEX
               IF FRM-MENS NOT > SPACES
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE SCR-STAT-ABT TO SCR-STAT
                   PERFORM DESSCRTRL
                   GO TO FIN-SCR-ACT
               ELSE
                   MOVE SCR-STAT-ABT TO SCR-STAT
                   PERFORM DESSCRTRL
                   GO TO FIN-SCR-ACT
           ELSE
               MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
               MOVE SCR-ACTU-ING TO SCR-ACTU.
       TRL-SCR-ACT.
           PERFORM SCR-TRL.
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD.
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON.
           PERFORM DESSCRTRL.
           IF SCR-FFLD-BSP
               GO TO TRL-SCR-ACT.
           IF SCR-ACTU = SCR-ACTU-MOD
                  PERFORM MSG-SCR-MOD
           ELSE
                  PERFORM MSG-SCR-ING.
       FIN-SCR-ACT.
           PERFORM SCR-DMSG.

       SCR-PGM SECTION.
       INI-SCR-PGM.
           PERFORM HBL-KEY.
           PERFORM SCR-RST.
           PERFORM GET-KEY.

           IF FRM-STAT-ABT
               MOVE SPACES TO FRM-MENS
               PERFORM DESSCRTRL
               GO TO FIN-SCR-PGM.

           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
           IF FIO-STAT = FIO-STAT-NEX
               IF FRM-MENS NOT > SPACES
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE SCR-STAT-ABT TO SCR-STAT
                   PERFORM DESSCRTRL
                   GO TO FIN-SCR-PGM
               ELSE
                   MOVE SCR-STAT-ABT TO SCR-STAT
                   PERFORM DESSCRTRL
                   GO TO FIN-SCR-PGM.
       TRL-SCR-PGM.
           PERFORM DESSCRTRL.
           MOVE SPACES TO FRM-MENS.
           IF SCR-STAT NOT = SCR-STAT-CON
               PERFORM CMN-PGM.
       FIN-SCR-PGM.
           PERFORM SCR-DMSG.

       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               PERFORM SCR-DSP-MSSG
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               DISPLAY PGM-NCUR WITH NO ADVANCING
               PERFORM SCR-CLRA-PDT
               PERFORM SCR-DSP-MSSG
               DISPLAY PGM-SCUR WITH NO ADVANCING
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-PDT
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
           ELSE
               PERFORM CLR-FLD-PDT
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA.
       FIN-SCR-DMSG.
           EXIT.

       SCR-KEY SECTION.
       INI-SCR-KEY.
           PERFORM SCR-RST.
           PERFORM GET-KEY.
           IF FRM-STAT-ABT
               MOVE SPACES TO FRM-MENS
               PERFORM SCR-MSSG
               GO TO FIN-SCR-KEY.
       FND-SCR-KEY.
           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
           IF FIO-STAT-OKS
               PERFORM PUT-SCR
               MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
               GO TO FIN-SCR-KEY.
       ERR-SCR-KEY.
           MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR.
           IF FRM-MENS NOT > SPACES
               IF FIO-STAT-EOF
                   MOVE 'REGEOF    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
               IF FIO-STAT = FIO-STAT-NEX
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
           MOVE SCR-STAT-ABT TO SCR-STAT.
        FIN-SCR-KEY.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           INITIALIZE           PDT-DFLD.
           MOVE PGM-SRNF TO PGM-STAT-PDT.
           MOVE PGM-SRNF TO PGM-STAT-RAA.
       FIN-SCR-RST.
           EXIT.

       GET-KEY SECTION.
       INI-GET-KEY.
           PERFORM SCR-GKEY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-KEY.
           EXIT.

       GET-IKY SECTION.
       INI-GET-IKY.
           PERFORM SCR-GIKY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-IKY.
           EXIT.

       FND-KEY SECTION.
       INI-FND-KEY.
      *<<<< INI_FND_KEY
           IF SCR-CCPP = 'MOD' AND SCR-CMND = 'MOD'
               GO TO FIN-ING-KEY.           
           PERFORM GET-FHOY.
           MOVE PDT-COD-CLIE IN PDT-FLD TO PDT-COD-CLIE IN PDT.
           MOVE PDT-COD-PROY IN PDT-FLD TO PDT-COD-PROY IN PDT.
           MOVE PDT-COD-ACTI IN PDT-FLD TO PDT-COD-ACTI IN PDT.
           MOVE 'PGM'                   TO PDT-COD-TACC IN PDT.
           MOVE ZEROES TO PDT-FEC-FING IN PDT.
           MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM ADM-FIO-PDT.
           MOVE FIO-STAT TO PGM-STAT-PDT.
           IF SCR-CCPP = 'ING' AND SCR-CMND = 'PGM'
               IF ( FIO-STAT-OKS AND 
                  PDT-COD-ACTI IN PDT-FLD = PDT-COD-ACTI IN PDT AND  
                  PDT-COD-PROY IN PDT-FLD = PDT-COD-PROY IN PDT AND  
                  PDT-COD-CLIE IN PDT-FLD = PDT-COD-CLIE IN PDT ) 
                    MOVE 'ADM' TO MSG-COD-SIST
                    MOVE 'REGEXI    ' TO MSG-COD-MENS
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    MOVE '00' TO FIO-STAT
                    GO TO FIN-FND-KEY
               ELSE
                    MOVE 'PGM' TO PDT-COD-TACC IN PDT
                    MOVE 'PGM'    TO PDT-COD-TACC IN PDT-FLD
                    MOVE HOY-DHOY TO PDT-NUM-DING IN PDT-FLD
                    MOVE HOY-MHOY TO PDT-NUM-MING IN PDT-FLD
                    MOVE HOY-SHOY TO PDT-NUM-SING IN PDT-FLD
                    MOVE HOY-AHOY TO PDT-NUM-AING IN PDT-FLD
                    MOVE '23' TO FIO-STAT                   
                    GO TO FIN-FND-KEY.
           IF SCR-CCPP = 'ING' AND SCR-CMND = 'RPG'
               IF ( FIO-STAT-OKS AND
                 PDT-COD-ACTI IN PDT-FLD = PDT-COD-ACTI IN PDT AND
                 PDT-COD-PROY IN PDT-FLD = PDT-COD-PROY IN PDT AND
                 PDT-COD-CLIE IN PDT-FLD = PDT-COD-CLIE IN PDT )
                   MOVE 'RPG' TO PDT-COD-TACC IN PDT
                   MOVE 'RPG'               TO PDT-COD-TACC IN PDT-FLD
                   MOVE PDT-NUM-DINI IN PDT TO FRM-NUM-DIPR IN PDT-FLD
                   MOVE PDT-NUM-MINI IN PDT TO FRM-NUM-MIPR IN PDT-FLD
                   MOVE PDT-NUM-SINI IN PDT TO FRM-NUM-SIPR IN PDT-FLD
                   MOVE PDT-NUM-AINI IN PDT TO FRM-NUM-AIPR IN PDT-FLD
                   PERFORM BUS-ULT-RPG
                   IF WSS-COD-TACC IN WSS-VARI NOT = 'STR'
                       PERFORM PUT-ALL-PDT
                       MOVE PDT-NUM-DTER IN PDT TO FRM-NUM-DTPR
                       IN PDT-FLD
                       MOVE PDT-NUM-MTER IN PDT TO FRM-NUM-MTPR
                       IN PDT-FLD
                       MOVE PDT-NUM-STER IN PDT TO FRM-NUM-STPR
                       IN PDT-FLD
                       MOVE PDT-NUM-ATER IN PDT TO FRM-NUM-ATPR
                       IN PDT-FLD
                       MOVE HOY-DHOY TO PDT-NUM-DING IN PDT-FLD
                       MOVE HOY-MHOY TO PDT-NUM-MING IN PDT-FLD
                       MOVE HOY-SHOY TO PDT-NUM-SING IN PDT-FLD
                       MOVE HOY-AHOY TO PDT-NUM-AING IN PDT-FLD
                       MOVE '23' TO FIO-STAT
                       GO TO FIN-FND-KEY
                   ELSE
                       MOVE 'ADM' TO MSG-COD-SIST
                       MOVE 'REGEXIINI' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE '00' TO FIO-STAT
                       GO TO FIN-FND-KEY
               ELSE
                   MOVE 'ADM' TO MSG-COD-SIST
                   MOVE 'REGEXINPGM' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE '00' TO FIO-STAT
                   GO TO FIN-FND-KEY.
           IF SCR-CCPP = 'ING' AND SCR-CMND = 'INI'
               IF ( FIO-STAT-OKS AND
                 PDT-COD-ACTI IN PDT-FLD = PDT-COD-ACTI IN PDT AND
                 PDT-COD-PROY IN PDT-FLD = PDT-COD-PROY IN PDT AND 
                 PDT-COD-CLIE IN PDT-FLD = PDT-COD-CLIE IN PDT ) 
                   MOVE PDT-NUM-DINI IN PDT TO FRM-NUM-DIPR IN PDT-FLD
                   MOVE PDT-NUM-MINI IN PDT TO FRM-NUM-MIPR IN PDT-FLD
                   MOVE PDT-NUM-SINI IN PDT TO FRM-NUM-SIPR IN PDT-FLD
                   MOVE PDT-NUM-AINI IN PDT TO FRM-NUM-AIPR IN PDT-FLD
                   PERFORM BUS-ULT-RPG
                   PERFORM PUT-ALL-PDT
                   MOVE PDT-NUM-DTER IN PDT TO FRM-NUM-DTPR IN PDT-FLD
                   MOVE PDT-NUM-MTER IN PDT TO FRM-NUM-MTPR IN PDT-FLD
                   MOVE PDT-NUM-STER IN PDT TO FRM-NUM-STPR IN PDT-FLD
                   MOVE PDT-NUM-ATER IN PDT TO FRM-NUM-ATPR IN PDT-FLD
                   IF WSS-COD-TACC IN WSS-VARI = 'STR'
                       MOVE 'ADM' TO MSG-COD-SIST
                       MOVE 'REGEXIINI' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE '00' TO FIO-STAT
                       GO TO FIN-FND-KEY
                   ELSE
                       MOVE 'INI'    TO PDT-COD-TACC IN PDT-FLD
                       MOVE HOY-DHOY TO PDT-NUM-DING IN PDT-FLD
                       MOVE HOY-MHOY TO PDT-NUM-MING IN PDT-FLD
                       MOVE HOY-SHOY TO PDT-NUM-SING IN PDT-FLD
                       MOVE HOY-AHOY TO PDT-NUM-AING IN PDT-FLD
                       MOVE '23' TO FIO-STAT
                       GO TO FIN-FND-KEY
               ELSE
                   MOVE 'ADM' TO MSG-COD-SIST
                   MOVE 'REGEXINPGM' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE '00' TO FIO-STAT
                   GO TO FIN-FND-KEY.

           IF SCR-CCPP = 'ING' AND SCR-CMND = 'TER'
               IF ( FIO-STAT-OKS AND
                 PDT-COD-ACTI IN PDT-FLD = PDT-COD-ACTI IN PDT AND 
                 PDT-COD-PROY IN PDT-FLD = PDT-COD-PROY IN PDT AND 
                 PDT-COD-CLIE IN PDT-FLD = PDT-COD-CLIE IN PDT )
                   MOVE PDT-NUM-DINI IN PDT TO FRM-NUM-DIPR IN PDT-FLD
                   MOVE PDT-NUM-MINI IN PDT TO FRM-NUM-MIPR IN PDT-FLD
                   MOVE PDT-NUM-SINI IN PDT TO FRM-NUM-SIPR IN PDT-FLD
                   MOVE PDT-NUM-AINI IN PDT TO FRM-NUM-AIPR IN PDT-FLD
                   PERFORM BUS-ULT-RPG
                   IF WSS-COD-TACC IN WSS-VARI = 'STR'
                       PERFORM PUT-ALL-PDT
                       MOVE PDT-NUM-DTER IN PDT TO FRM-NUM-DTPR
                       IN PDT-FLD
                       MOVE PDT-NUM-MTER IN PDT TO FRM-NUM-MTPR
                       IN PDT-FLD
                       MOVE PDT-NUM-STER IN PDT TO FRM-NUM-STPR
                       IN PDT-FLD
                       MOVE PDT-NUM-ATER IN PDT TO FRM-NUM-ATPR
                       IN PDT-FLD
                       MOVE WSS-COD-APRG TO PDT-COD-APRG IN PDT-FLD
                       MOVE WSS-COD-RESP TO PDT-COD-RESP IN PDT-FLD
                       MOVE WSS-COD-INGR TO PDT-COD-INGR IN PDT-FLD
                       MOVE WSS-COD-EJEC TO PDT-COD-EJEC IN PDT-FLD
                       MOVE FIO-GET-NXT TO FIO-CMND
                       PERFORM ADM-FIO-PDT
                       IF FIO-STAT-OKS AND PDT-COD-TACC IN PDT = 'TER'
                           MOVE 'ADM' TO MSG-COD-SIST
                           MOVE 'REGEXITER' TO MSG-COD-MENS
                           PERFORM GET-MSG
                           MOVE MSG-GLS-DESC TO FRM-MENS
                           MOVE '00' TO FIO-STAT
                           GO TO FIN-FND-KEY
                       ELSE
                          MOVE 'TER'    TO PDT-COD-TACC IN PDT
                          MOVE 'TER'    TO PDT-COD-TACC IN PDT-FLD
                          MOVE HOY-DHOY TO PDT-NUM-DING IN PDT-FLD
                          MOVE HOY-MHOY TO PDT-NUM-MING IN PDT-FLD
                          MOVE HOY-SHOY TO PDT-NUM-SING IN PDT-FLD
                          MOVE HOY-AHOY TO PDT-NUM-AING IN PDT-FLD
                          MOVE '23' TO FIO-STAT
                          GO TO FIN-FND-KEY
                   ELSE
                       MOVE 'ADM' TO MSG-COD-SIST
                       MOVE 'REGEXININI' TO MSG-COD-MENS
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                       MOVE '00' TO FIO-STAT
                       GO TO FIN-FND-KEY
               ELSE
                   MOVE 'ADM' TO MSG-COD-SIST
                   MOVE 'REGEXINPGM' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE '00' TO FIO-STAT
                   GO TO FIN-FND-KEY.
       FIN-ING-KEY.           
      *>>>>      
           PERFORM KEY-ALL-PDT.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM ADM-FIO-PDT.
           MOVE FIO-STAT TO PGM-STAT-PDT.
      *<<<< FIN_FND_KEY
           IF FIO-STAT-OKS
               IF PDT-COD-TACC IN PDT-FLD = 'PGM'
                   MOVE PDT-NUM-DINI IN PDT TO FRM-NUM-DIPR IN PDT-FLD
                   MOVE PDT-NUM-MINI IN PDT TO FRM-NUM-MIPR IN PDT-FLD
                   MOVE PDT-NUM-SINI IN PDT TO FRM-NUM-SIPR IN PDT-FLD
                   MOVE PDT-NUM-AINI IN PDT TO FRM-NUM-AIPR IN PDT-FLD
                   MOVE PDT-NUM-DTER IN PDT TO FRM-NUM-DTPR IN PDT-FLD
                   MOVE PDT-NUM-MTER IN PDT TO FRM-NUM-MTPR IN PDT-FLD
                   MOVE PDT-NUM-STER IN PDT TO FRM-NUM-STPR IN PDT-FLD
                   MOVE PDT-NUM-ATER IN PDT TO FRM-NUM-ATPR IN PDT-FLD.
               IF PDT-COD-TACC IN PDT-FLD = 'RPG'
                   MOVE PDT-NUM-DINI IN PDT TO FRM-NUM-DIPR IN PDT-FLD
                   MOVE PDT-NUM-MINI IN PDT TO FRM-NUM-MIPR IN PDT-FLD
                   MOVE PDT-NUM-SINI IN PDT TO FRM-NUM-SIPR IN PDT-FLD
                   MOVE PDT-NUM-AINI IN PDT TO FRM-NUM-AIPR IN PDT-FLD
                   MOVE PDT-NUM-DTER IN PDT TO FRM-NUM-DTPR IN PDT-FLD
                   MOVE PDT-NUM-MTER IN PDT TO FRM-NUM-MTPR IN PDT-FLD
                   MOVE PDT-NUM-STER IN PDT TO FRM-NUM-STPR IN PDT-FLD
                   MOVE PDT-NUM-ATER IN PDT TO FRM-NUM-ATPR IN PDT-FLD.
               IF PDT-COD-TACC IN PDT-FLD = 'STR'
                   MOVE PDT-NUM-DINI IN PDT TO FRM-NUM-DIRE IN PDT-FLD
                   MOVE PDT-NUM-MINI IN PDT TO FRM-NUM-MIRE IN PDT-FLD
                   MOVE PDT-NUM-SINI IN PDT TO FRM-NUM-SIRE IN PDT-FLD
                   MOVE PDT-NUM-AINI IN PDT TO FRM-NUM-AIRE IN PDT-FLD
                   MOVE PDT-NUM-DTER IN PDT TO FRM-NUM-DTPR IN PDT-FLD
                   MOVE PDT-NUM-MTER IN PDT TO FRM-NUM-MTPR IN PDT-FLD
                   MOVE PDT-NUM-STER IN PDT TO FRM-NUM-STPR IN PDT-FLD
                   MOVE PDT-NUM-ATER IN PDT TO FRM-NUM-ATPR IN PDT-FLD.
               IF PDT-COD-TACC IN PDT-FLD = 'TER'
                   MOVE PDT-NUM-DINI IN PDT TO FRM-NUM-DIRE IN PDT-FLD
                   MOVE PDT-NUM-MINI IN PDT TO FRM-NUM-MIRE IN PDT-FLD
                   MOVE PDT-NUM-SINI IN PDT TO FRM-NUM-SIRE IN PDT-FLD
                   MOVE PDT-NUM-AINI IN PDT TO FRM-NUM-AIRE IN PDT-FLD
                   MOVE PDT-NUM-DTER IN PDT TO FRM-NUM-DTRE IN PDT-FLD
                   MOVE PDT-NUM-MTER IN PDT TO FRM-NUM-MTRE IN PDT-FLD
                   MOVE PDT-NUM-STER IN PDT TO FRM-NUM-STRE IN PDT-FLD
                   MOVE PDT-NUM-ATER IN PDT TO FRM-NUM-ATRE IN PDT-FLD.
      *>>>>
       FIN-FND-KEY.
           EXIT.

       PUT-SCR SECTION.
       INI-PUT-SCR.
      *<<<< PUT_SCR
           MOVE PDT-COD-CLIE IN PDT-FLD TO RAA-COD-CLAC IN RAA.
           MOVE PDT-COD-PROY IN PDT-FLD TO RAA-COD-PRAC IN RAA.
           MOVE PDT-COD-ACTI IN PDT-FLD TO RAA-COD-ACAC IN RAA.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM ADM-FIO-RAA.
           IF FIO-STAT-OKS
               MOVE PGM-SOKS TO PGM-STAT-RAA
               MOVE RAA-GLS-AACT IN RAA TO FRM-GLS-ACTI IN PDT-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-ACTI IN PDT-FLD.
      *>>>>
           PERFORM PUT-ALL-PDT.
       FIN-PUT-SCR.
           EXIT.

       GET-FLD SECTION.
       INI-GET-FLD.
           PERFORM SCR-GFLD.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-FLD.
           EXIT.

       CMN-MOD SECTION.
       INI-CMN-MOD.
           PERFORM RET-ALL-PDT.
      *<<<< CMN_MOD 
           IF PDT-COD-TACC IN PDT-FLD = 'PGM' OR 
              PDT-COD-TACC IN PDT-FLD = 'RPG'
              MOVE FRM-NUM-DIPR IN PDT-FLD TO PDT-NUM-DINI IN PDT
              MOVE FRM-NUM-MIPR IN PDT-FLD TO PDT-NUM-MINI IN PDT
              MOVE FRM-NUM-SIPR IN PDT-FLD TO PDT-NUM-SINI IN PDT
              MOVE FRM-NUM-AIPR IN PDT-FLD TO PDT-NUM-AINI IN PDT
              MOVE FRM-NUM-DTPR IN PDT-FLD TO PDT-NUM-DTER IN PDT
              MOVE FRM-NUM-MTPR IN PDT-FLD TO PDT-NUM-MTER IN PDT
              MOVE FRM-NUM-STPR IN PDT-FLD TO PDT-NUM-STER IN PDT
              MOVE FRM-NUM-ATPR IN PDT-FLD TO PDT-NUM-ATER IN PDT.
      
           IF PDT-COD-TACC IN PDT-FLD = 'STR'
              MOVE FRM-NUM-DIRE IN PDT-FLD TO PDT-NUM-DINI IN PDT
              MOVE FRM-NUM-MIRE IN PDT-FLD TO PDT-NUM-MINI IN PDT
              MOVE FRM-NUM-SIRE IN PDT-FLD TO PDT-NUM-SINI IN PDT
              MOVE FRM-NUM-AIRE IN PDT-FLD TO PDT-NUM-AINI IN PDT
              MOVE FRM-NUM-DTPR IN PDT-FLD TO PDT-NUM-DTER IN PDT
              MOVE FRM-NUM-MTPR IN PDT-FLD TO PDT-NUM-MTER IN PDT
              MOVE FRM-NUM-STPR IN PDT-FLD TO PDT-NUM-STER IN PDT
              MOVE FRM-NUM-ATPR IN PDT-FLD TO PDT-NUM-ATER IN PDT.

           IF PDT-COD-TACC IN PDT-FLD = 'TER'
              MOVE FRM-NUM-DIRE IN PDT-FLD TO PDT-NUM-DINI IN PDT
              MOVE FRM-NUM-MIRE IN PDT-FLD TO PDT-NUM-MINI IN PDT
              MOVE FRM-NUM-SIRE IN PDT-FLD TO PDT-NUM-SINI IN PDT
              MOVE FRM-NUM-AIRE IN PDT-FLD TO PDT-NUM-AINI IN PDT
              MOVE FRM-NUM-DTRE IN PDT-FLD TO PDT-NUM-DTER IN PDT
              MOVE FRM-NUM-MTRE IN PDT-FLD TO PDT-NUM-MTER IN PDT
              MOVE FRM-NUM-STRE IN PDT-FLD TO PDT-NUM-STER IN PDT
              MOVE FRM-NUM-ATRE IN PDT-FLD TO PDT-NUM-ATER IN PDT.
      *>>>>
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM ADM-FIO-PDT.
      *<<<< FIN_CMN_MOD
           IF NOT FIO-STAT-OKS
              GO TO FIN-CMN-MOD.
           MOVE PDT-COD-CLIE IN PDT-FLD TO RAA-COD-CLAC IN RAA.
           MOVE PDT-COD-PROY IN PDT-FLD TO RAA-COD-PRAC IN RAA.
           MOVE PDT-COD-ACTI IN PDT-FLD TO RAA-COD-ACAC IN RAA.

           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM ADM-FIO-RAA.
           IF NOT FIO-STAT-OKS
              GO TO MSG-CMN-MOD.

           MOVE PDT-COD-CLIE IN PDT-FLD TO RAA-COD-CLAN IN RAA.
           MOVE PDT-COD-PROY IN PDT-FLD TO RAA-COD-PRAN IN RAA.
           MOVE RAA-COD-ACAN IN PDT-FLD TO RAA-COD-ACAN IN RAA.

           MOVE PDT-COD-CLIE IN PDT-FLD TO RAA-COD-CLSG IN RAA.
           MOVE PDT-COD-PROY IN PDT-FLD TO RAA-COD-PRSG IN RAA.
           MOVE RAA-COD-ACSG IN PDT-FLD TO RAA-COD-ACSG IN RAA.

           MOVE FRM-GLS-ACTI IN PDT-FLD TO RAA-GLS-AACT IN RAA.
   
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM ADM-FIO-RAA.
           IF FIO-STAT-OKS
              GO TO FIN-CMN-MOD.
       MSG-CMN-MOD.
           MOVE 'ADM' TO MSG-COD-SIST.
           MOVE 'RAA    NOMOD' TO MSG-COD-MENS.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
      *>>>>
       FIN-CMN-MOD.
           EXIT.

       CMN-ING SECTION.
       INI-CMN-ING.
           PERFORM RET-ALL-PDT.
      *<<<< CMN_ING 
           IF SCR-CMND = 'PGM' OR SCR-CMND = 'RPG'
              MOVE FRM-NUM-DIPR IN PDT-FLD TO PDT-NUM-DINI IN PDT
              MOVE FRM-NUM-MIPR IN PDT-FLD TO PDT-NUM-MINI IN PDT
              MOVE FRM-NUM-SIPR IN PDT-FLD TO PDT-NUM-SINI IN PDT
              MOVE FRM-NUM-AIPR IN PDT-FLD TO PDT-NUM-AINI IN PDT
              MOVE FRM-NUM-DTPR IN PDT-FLD TO PDT-NUM-DTER IN PDT
              MOVE FRM-NUM-MTPR IN PDT-FLD TO PDT-NUM-MTER IN PDT
              MOVE FRM-NUM-STPR IN PDT-FLD TO PDT-NUM-STER IN PDT
              MOVE FRM-NUM-ATPR IN PDT-FLD TO PDT-NUM-ATER IN PDT.
      
           IF SCR-CMND = 'INI'
              MOVE 'STR'                   TO PDT-COD-TACC IN PDT
              MOVE FRM-NUM-DIRE IN PDT-FLD TO PDT-NUM-DINI IN PDT
              MOVE FRM-NUM-MIRE IN PDT-FLD TO PDT-NUM-MINI IN PDT
              MOVE FRM-NUM-SIRE IN PDT-FLD TO PDT-NUM-SINI IN PDT
              MOVE FRM-NUM-AIRE IN PDT-FLD TO PDT-NUM-AINI IN PDT
              MOVE FRM-NUM-DTPR IN PDT-FLD TO PDT-NUM-DTER IN PDT
              MOVE FRM-NUM-MTPR IN PDT-FLD TO PDT-NUM-MTER IN PDT
              MOVE FRM-NUM-STPR IN PDT-FLD TO PDT-NUM-STER IN PDT
              MOVE FRM-NUM-ATPR IN PDT-FLD TO PDT-NUM-ATER IN PDT.

           IF SCR-CMND = 'TER'
              MOVE FRM-NUM-DIRE IN PDT-FLD TO PDT-NUM-DINI IN PDT
              MOVE FRM-NUM-MIRE IN PDT-FLD TO PDT-NUM-MINI IN PDT
              MOVE FRM-NUM-SIRE IN PDT-FLD TO PDT-NUM-SINI IN PDT
              MOVE FRM-NUM-AIRE IN PDT-FLD TO PDT-NUM-AINI IN PDT
              MOVE FRM-NUM-DTRE IN PDT-FLD TO PDT-NUM-DTER IN PDT
              MOVE FRM-NUM-MTRE IN PDT-FLD TO PDT-NUM-MTER IN PDT
              MOVE FRM-NUM-STRE IN PDT-FLD TO PDT-NUM-STER IN PDT
              MOVE FRM-NUM-ATRE IN PDT-FLD TO PDT-NUM-ATER IN PDT.
      *>>>>
           MOVE FIO-PUT TO FIO-CMND.
           PERFORM ADM-FIO-PDT.
      *<<<< FIN_CMN_ING
           IF NOT FIO-STAT-OKS
              GO TO FIN-CMN-ING.
           MOVE PDT-COD-CLIE IN PDT-FLD TO RAA-COD-CLAC IN RAA.
           MOVE PDT-COD-PROY IN PDT-FLD TO RAA-COD-PRAC IN RAA.
           MOVE PDT-COD-ACTI IN PDT-FLD TO RAA-COD-ACAC IN RAA.

           MOVE PDT-COD-CLIE IN PDT-FLD TO RAA-COD-CLAN IN RAA.
           MOVE PDT-COD-PROY IN PDT-FLD TO RAA-COD-PRAN IN RAA.
           MOVE RAA-COD-ACAN IN PDT-FLD TO RAA-COD-ACAN IN RAA.

           MOVE PDT-COD-CLIE IN PDT-FLD TO RAA-COD-CLSG IN RAA.
           MOVE PDT-COD-PROY IN PDT-FLD TO RAA-COD-PRSG IN RAA.
           MOVE RAA-COD-ACSG IN PDT-FLD TO RAA-COD-ACSG IN RAA.

           MOVE FRM-GLS-ACTI IN PDT-FLD TO RAA-GLS-AACT IN RAA.
   
           MOVE FIO-PUT TO FIO-CMND.
           PERFORM ADM-FIO-RAA.
           IF FIO-STAT-OKS
              GO TO FIN-CMN-ING.
       MSG-CMN-ING.
           MOVE 'ADM' TO MSG-COD-SIST.
           MOVE 'RAA    NOING' TO MSG-COD-MENS.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
      *>>>>
       FIN-CMN-ING.
           EXIT.

       CMN-ACC SECTION.
       INI-CMN-ACC.
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'ACCION NO REALIZADA' TO FRM-MENS
               GO TO FIN-CMN-ACC.
           PERFORM RET-ALL-PDT.
       FIN-CMN-ACC.
           EXIT.

       CMN-ELI SECTION.
       INI-CMN-ELI.
           PERFORM KEY-ALL-PDT.
           MOVE FIO-DEL TO FIO-CMND.
           PERFORM ADM-FIO-PDT.
       FIN-CMN-ELI.
           EXIT.

       CMN-PGM SECTION.
       INI-CMN-PGM.
           PERFORM RET-ALL-PDT.
       FIN-CMN-PGM.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE SPACES TO FRM-MENS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
           DISPLAY PGM-NCUR WITH NO ADVANCING.
           MOVE PDT-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE PDT-GLS-MENS TO FRM-MENS.
           MOVE FRM-MSG TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           DISPLAY PGM-SCUR WITH NO ADVANCING.
       FIN-SCR-PUTA.
           EXIT.

       SCR-DSP-MSSG SECTION.
       INI-SCR-DSP-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO PDT-GLS-MENS.
           MOVE FRM-MSG TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-DSP-MSSG.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO PDT-GLS-MENS.
      *    MOVE FRM-MSG TO FRM-CMND.
      *    PERFORM GNS-PRO-FRM.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GKEY SECTION.
       INI-SCR-GKEY.
           MOVE PDT-NFLD       TO FRM-VNOM.
       LUP-SCR-GKEY.
           MOVE PDT-DFLD-ATR   TO FRM-VATR.
           MOVE PDT-DFLD-LEN   TO FRM-VLEN.
           MOVE FRM-GET-MAP      TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-PDT
               MOVE FRM-MSG TO FRM-CMND
               PERFORM GNS-PRO-FRM
               MOVE SPACES TO FRM-MENS
               GO TO LUP-SCR-GKEY.
           MOVE FRM-DFLD TO PDT-DFLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GKEY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           INITIALIZE PDT-DFLD-LEN.
           PERFORM VAL-NUM-KEY-PDT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-CON-KEY-PDT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               PERFORM FST-KEY-PDT
               GO TO ERR-SCR-GKEY.
      *JSS
           PERFORM VAL-IKY-PDT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-MKY-PDT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-AKY-PDT.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GKEY.
       ERR-SCR-GKEY.
           MOVE FRM-MSG TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           GO TO LUP-SCR-GKEY.
       FIN-SCR-GKEY.
           PERFORM SCR-MSSG.

       SCR-GIKY SECTION.
       INI-SCR-GIKY.
           MOVE PDT-NFLD       TO FRM-VNOM.
       LUP-SCR-GIKY.
           MOVE PDT-DFLD-ATR   TO FRM-VATR.
           MOVE PDT-DFLD-LEN   TO FRM-VLEN.
           MOVE FRM-GET-MAP      TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-PDT
               MOVE FRM-MSG TO FRM-CMND
               PERFORM GNS-PRO-FRM
               MOVE SPACES TO FRM-MENS
               GO TO LUP-SCR-GIKY.
           MOVE FRM-DFLD TO PDT-DFLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GIKY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           INITIALIZE PDT-DFLD-LEN.
           PERFORM VAL-NUM-IKY-PDT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GIKY.
           PERFORM VAL-CON-IKY-PDT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               PERFORM FST-KEY-PDT
               GO TO ERR-SCR-GIKY.
      *JSS
           PERFORM VAL-IKY-PDT.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GIKY.
       ERR-SCR-GIKY.
           MOVE FRM-MSG TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           GO TO LUP-SCR-GIKY.
       FIN-SCR-GIKY.
           PERFORM SCR-MSSG.

       SCR-GFLD SECTION.
       INI-SCR-GFLD.
           MOVE PDT-NFLD       TO FRM-VNOM.
       LUP-SCR-GFLD.
           MOVE PDT-DFLD-ATR   TO FRM-VATR.
           MOVE PDT-DFLD-LEN   TO FRM-VLEN.
           MOVE FRM-GET-MAP      TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-FLD-PDT
               MOVE FRM-MSG TO FRM-CMND
               PERFORM GNS-PRO-FRM
               MOVE SPACES TO FRM-MENS
               GO TO LUP-SCR-GFLD.
           MOVE FRM-DFLD TO PDT-DFLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GFLD.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           INITIALIZE PDT-DFLD-LEN.
           PERFORM VAL-NUM-KEY-PDT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GFLD.
           PERFORM VAL-NUM-FLD-PDT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GFLD.
           PERFORM VAL-MKY-PDT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GFLD.
           PERFORM VAL-FLD-PDT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GFLD.
           PERFORM VAL-CON-FLD-PDT.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GFLD.
           PERFORM FST-FLD-PDT.
       ERR-SCR-GFLD.
           PERFORM SCR-MSSG.
           PERFORM SCR-PUTA.
           GO TO LUP-SCR-GFLD.
       FIN-SCR-GFLD.
           MOVE SCR-STAT-MOD TO SCR-STAT.

       GET-ACC SECTION.
       INI-GET-ACC.
      *    MOVE FDV-WAIT TO FRM-CMND.
           MOVE 0 TO FRM-PLIN.
      *    MOVE FRM-MENS TO FRM-PRMT.
           MOVE PDT-GLS-MENS TO FRM-PRMT.
           MOVE FDV-GETDL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC          TO PDT-GLS-MEN2
               MOVE SCR-GLS-PRMT( SCR-I ) TO PDT-GLS-MEN1
               MOVE PDT-GLS-MENS TO FRM-MENS
               GO TO INI-GET-ACC.
           MOVE FRM-VFLD TO SCR-VFLD.
           MOVE FRM-VFLD TO SCR-VFLD.
           IF FRM-FFLD = FRM-FFLD-RET
               IF SCR-VFLD = SCR-VFLD-NOT
                   MOVE FRM-FFLD-PF12 TO FRM-FFLD
               ELSE
                   PERFORM VAL-NUM-KEY-PDT
                   PERFORM VAL-NUM-FLD-PDT
           ELSE
               IF SCR-VFLD = SCR-VFLD-YES
                   MOVE FRM-FFLD-PF10 TO FRM-FFLD
                   PERFORM VAL-NUM-KEY-PDT
                   PERFORM VAL-NUM-FLD-PDT.
       FIN-GET-ACC.
           MOVE SCR-STAT-SON TO SCR-STAT.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE FDV-WAIT TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC          TO PDT-GLS-MEN2
               MOVE SCR-GLS-PRMT( SCR-I ) TO PDT-GLS-MEN1
               MOVE FRM-MSG TO FRM-CMND
               PERFORM GNS-PRO-FRM
               MOVE SPACES TO FRM-MENS
               GO TO INI-GET-CON.
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO PDT-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
           PERFORM PRO-FLD-PDT.
           PERFORM PRO-MKY-PDT.
           PERFORM PRO-AKY-PDT.
           PERFORM UNP-IKY-PDT.
           PERFORM FST-KEY-PDT.
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
           PERFORM PRO-FLD-PDT.
           PERFORM UNP-KEY-PDT.
           PERFORM FST-KEY-PDT.
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
           PERFORM PRO-IKY-PDT.
           PERFORM PRO-AKY-PDT.
           PERFORM UNP-MKY-PDT.
           PERFORM UNP-FLD-PDT.
           PERFORM FST-FLD-PDT.
      *<<<< FIN_HBL_FLD

           IF SCR-CMND = 'PGM'
               MOVE FRM-CPIM-AEY TO PDT-COD-TACC-ATR IN PDT-FLD
               MOVE FRM-CPIM-AEY TO PDT-FEC-FING-ATR IN PDT-FLD
               MOVE FRM-CPIM-AEY TO FRM-FEC-FIRE-ATR IN PDT-FLD
               MOVE FRM-CPIM-AEY TO FRM-FEC-FTRE-ATR IN PDT-FLD
               MOVE  0           TO PDT-COD-TACC-LEN IN PDT-FLD
               MOVE -1           TO FRM-FEC-FIPR-LEN IN PDT-FLD.

           IF SCR-CMND = 'RPG'
               MOVE FRM-CPIM-AEY TO PDT-COD-TACC-ATR IN PDT-FLD
               MOVE FRM-CPIM-AEY TO PDT-FEC-FING-ATR IN PDT-FLD
               MOVE FRM-CPIM-AEY TO FRM-FEC-FIPR-ATR IN PDT-FLD
               MOVE FRM-CPIM-AEY TO FRM-FEC-FIRE-ATR IN PDT-FLD
               MOVE FRM-CPIM-AEY TO FRM-FEC-FTRE-ATR IN PDT-FLD
               MOVE  0           TO PDT-COD-TACC-LEN IN PDT-FLD
               MOVE -1           TO PDT-NUM-DHBL-LEN IN PDT-FLD.

           IF SCR-CMND = 'INI'
               MOVE FRM-CPIM-AEY TO PDT-COD-TACC-ATR IN PDT-FLD
               MOVE FRM-CPIM-AEY TO FRM-FEC-FIPR-ATR IN PDT-FLD
               MOVE FRM-CPIM-AEY TO FRM-FEC-FTRE-ATR IN PDT-FLD
               MOVE FRM-CPIM-AEY TO PDT-NUM-DHBL-ATR IN PDT-FLD
               MOVE FRM-CPIM-AEY TO PDT-NUM-PRIO-ATR IN PDT-FLD
               MOVE FRM-CPIM-AEY TO PDT-COD-APRG-ATR IN PDT-FLD
               MOVE FRM-CPIM-AEY TO PDT-FEC-FING-ATR IN PDT-FLD
               MOVE  0           TO PDT-COD-TACC-LEN IN PDT-FLD
               MOVE -1           TO FRM-FEC-FIRE-LEN IN PDT-FLD.

           IF SCR-CMND = 'TER'
               MOVE FRM-CPIM-AEY TO PDT-COD-TACC-ATR IN PDT-FLD
               MOVE FRM-CPIM-AEY TO PDT-FEC-FING-ATR IN PDT-FLD
               MOVE FRM-CPIM-AEY TO FRM-FEC-FIPR-ATR IN PDT-FLD
               MOVE FRM-CPIM-AEY TO FRM-FEC-FIRE-ATR IN PDT-FLD
               MOVE FRM-CPIM-AEY TO PDT-NUM-DHBL-ATR IN PDT-FLD
               MOVE FRM-CPIM-AEY TO PDT-NUM-PRIO-ATR IN PDT-FLD
               MOVE FRM-CPIM-AEY TO PDT-COD-APRG-ATR IN PDT-FLD
               MOVE FRM-CPIM-AEY TO PDT-COD-RESP-ATR IN PDT-FLD
               MOVE FRM-CPIM-AEY TO PDT-COD-EJEC-ATR IN PDT-FLD
               MOVE  0           TO PDT-COD-TACC-LEN IN PDT-FLD
               MOVE -1           TO FRM-FEC-FTRE-LEN IN PDT-FLD.

           IF SCR-CMND = 'MOD'
               MOVE FRM-CPIM-AEY TO PDT-COD-TACC-ATR IN PDT-FLD
               MOVE FRM-CPIM-AEY TO PDT-FEC-FING-ATR IN PDT-FLD
               MOVE  0           TO PDT-COD-TACC-LEN IN PDT-FLD
               IF PDT-COD-TACC IN PDT-FLD = 'PGM'
                  MOVE FRM-CPIM-AEY TO FRM-FEC-FIRE-ATR IN PDT-FLD
                  MOVE FRM-CPIM-AEY TO FRM-FEC-FTRE-ATR IN PDT-FLD
                  MOVE -1 TO FRM-FEC-FIPR-LEN IN PDT-FLD
               ELSE
               IF PDT-COD-TACC IN PDT-FLD = 'RPG'
                  MOVE FRM-CPIM-AEY TO FRM-FEC-FIRE-ATR IN PDT-FLD
                  MOVE FRM-CPIM-AEY TO FRM-FEC-FTRE-ATR IN PDT-FLD
                  MOVE -1 TO FRM-FEC-FIPR-LEN IN PDT-FLD
               ELSE
               IF PDT-COD-TACC IN PDT-FLD = 'STR'
                  MOVE FRM-CPIM-AEY TO FRM-FEC-FIPR-ATR IN PDT-FLD
                  MOVE FRM-CPIM-AEY TO FRM-FEC-FTRE-ATR IN PDT-FLD
                  MOVE FRM-CPIM-AEY TO PDT-NUM-DHBL-ATR IN PDT-FLD
                  MOVE FRM-CPIM-AEY TO PDT-NUM-PRIO-ATR IN PDT-FLD
                  MOVE FRM-CPIM-AEY TO PDT-COD-APRG-ATR IN PDT-FLD
                  MOVE -1 TO FRM-FEC-FIRE-LEN IN PDT-FLD
               ELSE
               IF PDT-COD-TACC IN PDT-FLD = 'TER'
                  MOVE FRM-CPIM-AEY TO FRM-FEC-FIPR-ATR IN PDT-FLD
                  MOVE FRM-CPIM-AEY TO PDT-NUM-DHBL-ATR IN PDT-FLD
                  MOVE FRM-CPIM-AEY TO PDT-NUM-PRIO-ATR IN PDT-FLD
                  MOVE FRM-CPIM-AEY TO PDT-COD-APRG-ATR IN PDT-FLD
                  MOVE FRM-CPIM-AEY TO PDT-COD-RESP-ATR IN PDT-FLD
                  MOVE FRM-CPIM-AEY TO PDT-COD-EJEC-ATR IN PDT-FLD
                  MOVE -1 TO FRM-FEC-FIRE-LEN IN PDT-FLD.

      *>>>>
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO PDT-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-PDT.
           PERFORM PRO-FLD-PDT.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-GLS-PRMT( SCR-I ) TO FRM-MEN1.
           IF SCR-STAT = SCR-STAT-MOD
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               IF FRM-FFLD NOT = FRM-FFLD-BSP
                   PERFORM SCR-PUTA
                   PERFORM SCR-EXIT
                   GO TO FIN-SCR-TRL
               ELSE
                   MOVE FRM-MSG TO FRM-CMND
                   PERFORM GNS-PRO-FRM
                   PERFORM SCR-EXIT
                   GO TO FIN-SCR-TRL.
           IF SCR-STAT = SCR-STAT-SON
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               IF FRM-FFLD NOT = FRM-FFLD-BSP
                   PERFORM SCR-PUTA
                   PERFORM SCR-EXIT
                   GO TO FIN-SCR-TRL
               ELSE
                   MOVE FRM-MSG TO FRM-CMND
                   PERFORM GNS-PRO-FRM
                   PERFORM SCR-EXIT
                   GO TO FIN-SCR-TRL.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           IF FRM-FFLD NOT = FRM-FFLD-BSP
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT
           ELSE
               MOVE FRM-MSG TO FRM-CMND
               PERFORM GNS-PRO-FRM
               PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           IF SCR-INST = SCR-INST-GCF
                  PERFORM SCR-GCNF
                  GO TO FIN-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
           IF SCR-STAT = SCR-STAT-ABT OR SCR-STAT-CON
                  GO TO FIN-DESSCRTRL.
           IF SCR-FFLD-RET OR SCR-FFLD-PF10
                  MOVE SCR-VFLD-YES TO SCR-VFLD
                  PERFORM PRO-CONF
                  IF SCR-INST = SCR-INST-GCF
                      GO TO INI-DESSCRTRL.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

       PRO-CONF SECTION.
       INI-PRO-CONF.
           IF FRM-MENS NOT > SPACES
               IF SCR-GLS-CONF( SCR-I ) NOT > SPACES
                   GO TO FIN-PRO-CONF
               ELSE
                   MOVE SCR-GLS-CONF( SCR-I ) TO FRM-MENS
           ELSE
               IF FRM-MEN1 NOT > SPACES
                   MOVE SCR-GLS-CONF( SCR-I ) TO FRM-MEN1.
           MOVE SCR-INST-GCF TO SCR-INST.
           PERFORM SCR-MSSG.
           PERFORM PRO-KEY-PDT.
           PERFORM PRO-FLD-PDT.
           PERFORM FST-MSG.
      *JSS PERFORM SCR-PUTA.
           DISPLAY PGM-NCUR WITH NO ADVANCING.
           MOVE PDT-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           DISPLAY PGM-SCUR WITH NO ADVANCING.
       FIN-PRO-CONF.
           EXIT.

       SCR-GCNF SECTION.
       INI-SCR-GCNF.
      *    MOVE FDV-WAIT TO FRM-CMND.
           MOVE 0 TO FRM-PLIN.
           MOVE FRM-MENS  TO FRM-PRMT.
           MOVE FDV-GETDL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET OR FRM-FFLD-PF4 OR
                               FRM-FFLD-BSP )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC          TO PDT-GLS-MEN2
               MOVE SCR-GLS-CONF( SCR-I ) TO PDT-GLS-MEN1
               MOVE PDT-GLS-MENS TO FRM-MENS
               GO TO INI-SCR-GCNF.
           MOVE FRM-VFLD TO SCR-VFLD.
           IF NOT ( SCR-VFLD = SCR-VFLD-YES OR SCR-VFLD-NOT OR SPACES )
               MOVE 'CNF    INV' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               STRING MSG-GLS-DCOR SCR-GLS-CONF( SCR-I )
                      DELIMITED BY SIZE INTO PDT-GLS-MENS
               MOVE PDT-GLS-MENS TO FRM-MENS
               GO TO INI-SCR-GCNF.
           IF FRM-FFLD = FRM-FFLD-RET
               IF SCR-VFLD = SCR-VFLD-NOT
                   MOVE FRM-FFLD-PF12 TO FRM-FFLD
               ELSE
                   MOVE SCR-VFLD-YES TO SCR-VFLD
                   PERFORM VAL-NUM-KEY-PDT
                   PERFORM VAL-NUM-FLD-PDT
           ELSE
           IF FRM-FFLD = FRM-FFLD-PF4
               IF SCR-VFLD = SCR-VFLD-YES
                   MOVE FRM-FFLD-PF10 TO FRM-FFLD
                   PERFORM VAL-NUM-KEY-PDT
                   PERFORM VAL-NUM-FLD-PDT
               ELSE
                   MOVE SCR-VFLD-NOT TO SCR-VFLD
           ELSE
               MOVE SCR-STPP TO SCR-STAT
               IF SCR-STAT NOT = SCR-STAT-MOD
                   MOVE FRM-FFLD-RET TO FRM-FFLD
                   MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
           MOVE SPACES TO FRM-MENS.
       FIN-SCR-GCNF.
           EXIT.

       COPY ADMBMPDT.
       COPY ADMBFPDT.
       COPY GNSBFMSG.
       COPY GNSBGSYS.
       COPY GNSBGFRM.
       COPY GNSBGMSG.
       COPY GNSBEKEY.
       COPY GNSBIABT.
       COPY GNSBBTAB.
       COPY GNSBVCOD.
       COPY GNSBVTAB.
       COPY GNSBFTAB.
       COPY GNSBGFEC.
       COPY GNSBGHOY.

      *<<<< EOF
       COPY ADMBFRAA.     
       COPY GNSBEGLS.
      *>>>>      
      *<<<< EOF_VAX
       COPY GNSBFMSC.
       COPY GNSBBMSC.

       BUS-ULT-RPG SECTION.
       INI-BUS-ULT-RPG.
           MOVE PDT TO WSS-PDT.
           MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM ADM-FIO-PDT.
           IF FIO-STAT-OKS AND PDT-COD-TACC IN PDT = 'RPG' 
                MOVE PDT-NUM-DING IN PDT TO FRM-NUM-DURP IN PDT-FLD
                MOVE PDT-NUM-MING IN PDT TO FRM-NUM-MURP IN PDT-FLD
                MOVE PDT-NUM-SING IN PDT TO FRM-NUM-SURP IN PDT-FLD
                MOVE PDT-NUM-AING IN PDT TO FRM-NUM-AURP IN PDT-FLD
                GO TO INI-BUS-ULT-RPG.
            IF SCR-CMND = 'TER'
                MOVE PDT-NUM-DINI IN PDT TO FRM-NUM-DIRE IN PDT-FLD
                MOVE PDT-NUM-MINI IN PDT TO FRM-NUM-MIRE IN PDT-FLD
                MOVE PDT-NUM-SINI IN PDT TO FRM-NUM-SIRE IN PDT-FLD
                MOVE PDT-NUM-AINI IN PDT TO FRM-NUM-AIRE IN PDT-FLD
                MOVE PDT-COD-APRG IN PDT TO WSS-COD-APRG IN WSS-VARI
                MOVE PDT-COD-RESP IN PDT TO WSS-COD-RESP IN WSS-VARI
                MOVE PDT-COD-INGR IN PDT TO WSS-COD-INGR IN WSS-VARI
                MOVE PDT-COD-EJEC IN PDT TO WSS-COD-EJEC IN WSS-VARI.
            MOVE PDT-COD-TACC IN PDT TO WSS-COD-TACC IN WSS-VARI.
            MOVE WSS-PDT TO PDT.
       FIN-BUS-ULT-RPG.
           EXIT.

      *>>>> 
