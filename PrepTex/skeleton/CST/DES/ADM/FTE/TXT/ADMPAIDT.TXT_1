*%! Codigo adicional para archivo
*% IF WSS
      *<<<< WSS
       COPY GNSBRMSC.
       01  WSS-VARI.
              03  WSS-NUM-MINI                     PIC 9(06).
              03  WSS-NUM-MTER                     PIC 9(06).
              03  WSS-NUM-MAUX                     PIC 9(06).
              03  WSS-KEY-IIDT.
               05  WSS-FEC-FACT.
                    07 WSS-NUM-SACT             PIC 9(02).
                    07 WSS-NUM-AACT             PIC 9(02).
                    07 WSS-NUM-MACT             PIC 9(02).
                    07 WSS-NUM-DACT             PIC 9(02).
               05  WSS-COD-FUNC                 PIC X(04).
               05  WSS-NUM-CORR                 PIC 9(02).
              03  WSS-ICOR               VALUE ' ' PIC X(01).
               88 WSS-ASIGNE-CORRELATIVO VALUE 'S'.
              03 WSS-COD-REQU.
                 05 WSS-COD-CLIR                   PIC X(03).
                 05 WSS-COD-PROR                   PIC X(03).
                 05 WSS-COD-CODR                   PIC X(06).
              03 WSS-COD-CCOS.
                 05 WSS-COD-CCLI                   PIC X(03).
                 05 WSS-COD-CPRO                   PIC X(03).
                 05 WSS-COD-REST                   PIC X(06).
      *>>>>
*% END
*% IF INI_FIO_VAX
      *<<<< INI_FIO_VAX
      *     MOVE 'ADM'        TO FIO-SIST.
      *     MOVE FIO-ACC      TO FIO-CMND.
      *     MOVE FIO-ACCS-UPD TO FIO-ACCS.
      *     PERFORM GNS-FIO-TAB.
      *     MOVE 'ADM'        TO FIO-SIST.
      *     MOVE FIO-INP      TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
      *>>>>
*% END
*% IF FND_KEY
      *<<<< FND_KEY
           MOVE 'N' TO WSS-ICOR.
           IF SCR-CCPP = 'ING'
               PERFORM BUS-CORR
           ELSE
           IF IDT-NUM-CORR IN IDT-FLD = ZEROES
               PERFORM BUS-CORR.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
           IF ( NOT FIO-STAT-OKS ) AND
              ( SCR-CCPP = 'ACT' ) AND
              ( NOT WSS-ASIGNE-CORRELATIVO )
                 MOVE FIO-STAT-NEX TO FIO-STAT.
      *>>>>
*% END
*% IF FIN_SCR_MOD
      *<<<< FIN_SCR_MOD
           ADD 1 TO IDT-NUM-CORR IN IDT-FLD.
           PERFORM SCR-PUTA.
           MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR.
      *>>>>
*% END
*% IF FIN_SCR_ING
      *<<<< FIN_SCR_ING
           MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR.
           MOVE IDT-NUM-HTER IN IDT-FLD TO IDT-NUM-HINI IN IDT-FLD.
           MOVE IDT-NUM-MTER IN IDT-FLD TO IDT-NUM-MINI IN IDT-FLD.
           MOVE ZEROES                  TO IDT-NUM-HTER IN IDT-FLD.
           MOVE ZEROES                  TO IDT-NUM-MTER IN IDT-FLD.
           MOVE ZEROES                  TO IDT-NUM-HTOT IN IDT-FLD.
           MOVE ZEROES                  TO IDT-NUM-MTOT IN IDT-FLD.
           MOVE SPACES                  TO IDT-COD-CCOS IN IDT-FLD.
           MOVE SPACES                  TO IDT-GLS-COME IN IDT-FLD.
      *>>>>
*% END
*% IF CMN_ING OR CMN_MOD
      *<<<< CMN_ING OR CMN_MOD
      *JUS-INI 7-DEC-1995 18:19:08 
           PERFORM GET-FHOY.
      *    Fecha Transaccion
           MOVE HOY-FHOY TO IDT-FEC-FTRN IN IDT.
      *    Hora Transaccion
           MOVE HOY-HHOY TO IDT-HRA-HRTR IN IDT.
           MOVE SCR-USER TO IDT-COD-ATRN IN IDT.
      *JUS-FIN
      *>>>>
*% END
*% IF FIN_CMN_ING
      *<<<< FIN_CMN_ING
           MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBFMSC.
       COPY GNSBBMSC.

       BUS-REQ SECTION.
       INI-BUS-REQ.
           MOVE IDT-COD-CLIE IN IDT-FLD TO IDT-COD-CLIR IN IDT-FLD.
           MOVE IDT-COD-PROY IN IDT-FLD TO IDT-COD-PROR IN IDT-FLD.
           MOVE SPACES TO FRM-GLS-REQU IN IDT-FLD.
           IF IDT-COD-CODR IN IDT-FLD > SPACES
                MOVE 'ADM'                   TO TAB-COD-SIST
                MOVE 'REQ'                   TO TAB-COD-TTAB IN TAB
                MOVE IDT-COD-CLIR IN IDT-FLD TO WSS-COD-CLIR IN WSS-VARI
                MOVE IDT-COD-PROR IN IDT-FLD TO WSS-COD-PROR IN WSS-VARI
                MOVE IDT-COD-CODR IN IDT-FLD TO WSS-COD-CODR IN WSS-VARI
                MOVE WSS-COD-REQU IN WSS-VARI TO TAB-COD-CTAB IN TAB
                PERFORM VAL-COD
                IF MSG-COD-MENS = 'COD    NEX' OR
                   MSG-COD-MENS = 'COD    NVG'
                     MOVE 'ADM'          TO MSG-COD-SIST
                     MOVE 'REQ    NEX' TO MSG-COD-MENS IN MSG
                     PERFORM GET-MSG
                     MOVE MSG-GLS-DESC   TO FRM-MENS
                     MOVE FRM-SUAR-MAL   TO FRM-SUAR
                     GO TO FIN-BUS-REQ
                ELSE
                     MOVE TAB-GLS-DESC TO FRM-GLS-REQU IN IDT-FLD
           ELSE
                 MOVE 'ADM'          TO MSG-COD-SIST
                 MOVE 'REQNO>SP  ' TO MSG-COD-MENS IN MSG
                 PERFORM GET-MSG
                 MOVE MSG-GLS-DESC   TO FRM-MENS
                 MOVE FRM-SUAR-MAL   TO FRM-SUAR.

       FIN-BUS-REQ.
           EXIT.

       BUS-CCOS SECTION.
       INI-BUS-CCOS.
           MOVE SPACES TO FRM-GLS-CCOS IN IDT-FLD.
           IF IDT-COD-CCOS IN IDT-FLD > SPACES
                MOVE 'ADM'                   TO TAB-COD-SIST
                MOVE 'CCO'                   TO TAB-COD-TTAB IN TAB
                MOVE IDT-COD-CCOS IN IDT-FLD TO TAB-COD-CTAB IN TAB
                PERFORM VAL-COD
                IF MSG-COD-MENS = 'COD    NEX' OR
                   MSG-COD-MENS = 'COD    NVG'
                     MOVE 'ADM'          TO MSG-COD-SIST
                     MOVE 'CCO    NEX' TO MSG-COD-MENS IN MSG
                     PERFORM GET-MSG
                     MOVE MSG-GLS-DESC   TO FRM-MENS
                     MOVE FRM-SUAR-MAL   TO FRM-SUAR
                     GO TO FIN-BUS-CCOS
                ELSE
                     MOVE TAB-GLS-DESC TO FRM-GLS-CCOS IN IDT-FLD.

       FIN-BUS-CCOS.
           EXIT.

       BUS-ACT SECTION.
       INI-BUS-ACT.
           MOVE SPACES TO FRM-GLS-ACTI IN IDT-FLD.
           IF IDT-COD-ACTI IN IDT-FLD > SPACES
                MOVE 'ADM'                   TO TAB-COD-SIST
                MOVE 'ACT'                   TO TAB-COD-TTAB IN TAB
                MOVE IDT-COD-ACTI IN IDT-FLD TO TAB-COD-CTAB IN TAB
                PERFORM VAL-COD
                IF MSG-COD-MENS = 'COD    NEX' OR
                   MSG-COD-MENS = 'COD    NVG'
                     MOVE 'ADM'          TO MSG-COD-SIST
                     MOVE 'CCO    NEX' TO MSG-COD-MENS IN MSG
                     PERFORM GET-MSG
                     MOVE MSG-GLS-DESC   TO FRM-MENS
                     MOVE FRM-SUAR-MAL   TO FRM-SUAR
                     GO TO FIN-BUS-ACT
                ELSE
                  MOVE TAB-GLS-DESC TO FRM-GLS-ACTI IN IDT-FLD
                  IF TAB-GLS-DATA = 'NO' AND 
                     IDT-COD-CLIE IN IDT-FLD NOT = 'CST'
                     MOVE 'ACTIVIDAD VALIDA SOLO PARA CLIENTE = CST'
                           TO FRM-MENS
                     MOVE FRM-SUAR-MAL TO FRM-SUAR.

       FIN-BUS-ACT.
           EXIT.
      *>>>>
*% END
*% IF EOF_VAX
      *<<<< EOF_VAX
       BUS-CORR SECTION.
       INI-BUS-CORR.
           MOVE 'S' TO WSS-ICOR.
           MOVE 00 TO IDT-NUM-CORR IN IDT-FLD.
           MOVE 00 TO IDT-NUM-CORR IN IDT.

           MOVE IDT-FEC-FACT IN IDT TO WSS-FEC-FACT.
           MOVE IDT-COD-FUNC IN IDT TO WSS-COD-FUNC.

           MOVE FIO-GET-NLS TO FIO-CMND.
       LUP-BUS-CORR.
           PERFORM ADM-FIO-IDT.
           IF FIO-STAT-OKS AND
              IDT-FEC-FACT IN IDT = WSS-FEC-FACT AND
              IDT-COD-FUNC IN IDT = WSS-COD-FUNC
               MOVE IDT-NUM-CORR IN IDT TO IDT-NUM-CORR IN IDT-FLD
               MOVE FIO-GET-NXT TO FIO-CMND
               GO TO LUP-BUS-CORR.

           IF SCR-CCPP = 'ING' OR 'ACT'
               ADD 1 TO IDT-NUM-CORR IN IDT-FLD.
           MOVE WSS-FEC-FACT            TO IDT-FEC-FACT IN IDT.
           MOVE WSS-COD-FUNC            TO IDT-COD-FUNC IN IDT.
           MOVE IDT-NUM-CORR IN IDT-FLD TO IDT-NUM-CORR IN IDT.
       FIN-BUS-CORR.
           EXIT.
      *>>>>
*% END
