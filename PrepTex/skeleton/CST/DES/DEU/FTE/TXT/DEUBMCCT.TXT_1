*% IF INI_FRM_FEC_FINI
      *<<<< INI_FRM_FEC_FINI
           MOVE FEC-CHOY-N  TO FEC-CHOY.
           MOVE FEC-VHBL-N TO FEC-VHBL.
           MOVE FEC-VAL-FEC TO FEC-CMND.
           PERFORM PRO-FEC.
           IF FEC-STAT-OKS
               MOVE FEC-FECH TO FRM-FEC-FINI IN CCT-FLD
           ELSE
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE FEC-MENS     TO FRM-MENS.
               
           GO TO FIN-VAL-FRM-FEC-FINI.
      *>>>>
*% END
*% IF INI_FRM_FEC_FTER
      *<<<< INI_FRM_FEC_FTER
           MOVE FEC-CHOY-N  TO FEC-CHOY.
           MOVE FEC-VHBL-N TO FEC-VHBL.
           MOVE FEC-VAL-FEC TO FEC-CMND.
           PERFORM PRO-FEC.
           IF FEC-STAT-OKS
               MOVE FEC-FECH TO FRM-FEC-FTER IN CCT-FLD
           ELSE
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE FEC-MENS     TO FRM-MENS.
               
           GO TO FIN-VAL-FRM-FEC-FTER.
      *>>>>
*% END
*% IF INI_VAL_CON_IKY_CCT
      *<<<< INI_VAL_CON_IKY_CCT
           IF     DBC-NUM-ICLI IN CCT-FLD NOT > ZEROES
              AND DBC-VRF-ICLI IN CCT-FLD NOT > SPACES
              AND DBC-IND-ICLI IN CCT-FLD NOT > SPACES
              AND DBC-GLS-ICLI IN CCT-FLD NOT > IDC-INIC-GLS
              AND OPC-CAI-IOPC IN CCT-FLD NOT > SPACES
              AND OPC-IIC-IOPC IN CCT-FLD NOT > ZEROES
              AND FRM-FEC-FINI IN CCT-FLD NOT > ZEROES
              AND FRM-FEC-FTER IN CCT-FLD NOT > ZEROES
              AND FRM-IND-CONS IN CCT-FLD NOT > SPACES
                PERFORM ERR-KEY
                PERFORM FST-KEY-CCT.
                
           GO TO FIN-VAL-CON-IKY-CCT.      
      *>>>>
*% END
*% IF INI_VAL_CON_KEY_CCT
      *<<<< INI_VAL_CON_KEY_CCT
           IF     DBC-NUM-ICLI IN CCT-FLD NOT > ZEROES
              AND DBC-VRF-ICLI IN CCT-FLD NOT > SPACES
              AND DBC-IND-ICLI IN CCT-FLD NOT > SPACES
              AND DBC-GLS-ICLI IN CCT-FLD NOT > IDC-INIC-GLS
              AND OPC-CAI-IOPC IN CCT-FLD NOT > SPACES
              AND OPC-IIC-IOPC IN CCT-FLD NOT > ZEROES
              AND FRM-FEC-FINI IN CCT-FLD NOT > ZEROES
              AND FRM-FEC-FTER IN CCT-FLD NOT > ZEROES
              AND FRM-IND-CONS IN CCT-FLD NOT > SPACES
                PERFORM ERR-KEY
                PERFORM FST-KEY-CCT.  
                
            GO TO FIN-VAL-CON-KEY-CCT.
      *>>>>
*% END
