*%! Codigo adicional
*% PGM_PTC = TRUE
*% GEN_MFC = TRUE
*% IF PGM_PQ
*% CLRMFC = "DEUZQQMD"
*% ELSE
*% CLRMFC = "DEUZUQMD"
*% END
*% IF WSS
      *<<<< WSS
      *AFRI-INI 05-MAY-2015 LIMITE DE CREDITO SEGUN ARTICULO 84
       01 WSS-VARI.
          03 WCC-LDCPSMAR     VALUE 'LDCPSMAR'      PIC X(08).
      *AFRI-FIN 05-MAY-2015 LIMITE DE CREDITO SEGUN ARTICULO 84
       COPY GNSWGTSK.
       COPY COLWGMAR.
       COPY LDCWGCCU.

       COPY GNSWGPES.
       COPY TABBRCAM.
       COPY TABBROFI.
       COPY COLBRTOC.
       COPY GNSWGHOY.
       COPY GNSWGFEC.
       COPY GNSWGCPT.
       COPY GNSWGSTR.
       77  WSS-I                            PIC  9(04).
       77  WSS-IND-PANT                     PIC  9(04).
       77  WSS-IMAX                         PIC  9(04) VALUE  5.
      *77  FRM-POS-CURS                COMP PIC S9(04) VALUE -1.
      *77  FRM-IND-ERRO                COMP PIC S9(04) VALUE -1.
      *77  FRM-ERR-NUME                COMP PIC S9(04) VALUE -9.

       01  WSS-RSP-QMD-CERR.
           03  WSS-RSP-QMD-CERR-1             PIC X(090).
           03  WSS-RSP-QMD-CERR-2             PIC X(001).
           03  WSS-RSP-QMD-CERR-3             PIC X(382).
       
       01  WSS-RSP-QMD-SERR.
           03  WSS-RSP-QMD-SERR-1             PIC X(090).
           03  WSS-RSP-QMD-SERR-3             PIC X(382).

       01  WSS-QIDD.
           03 WSS-NIDD       VALUE 'GIDD'    PIC X(04).
           03 WSS-TIDD       VALUE SPACES    PIC X(04).
                                                                        
      *LOP-INI 03-04-2003                                               
       01 WSS-IND-ERR-MAR    VALUE ZEROES     PIC 9(03).                
       01 WSS-GLS-ERR-MAR    VALUE SPACES     PIC X(40).                
      *LOP-FIN 03-04-2003 
      *>>>>
*% END
*% IF FIN_SCR_VARI
      *<<<< FIN_SCR_VARI
           03  SCR-DISP-RED REDEFINES SCR-DISP.
               05 SCR-NPAN                   PIC 9(03).
               05 SCR-TSK-TERM-NUM  COMP-3   PIC S9(07).
               05 SCR-TSK-TERM-ALF REDEFINES
                  SCR-TSK-TERM-NUM  PIC  X(04).
               05 SCR-DISP-DISP              PIC  X(12).
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
      *>>>>
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           PERFORM GNS-ERR-QUE.
           PERFORM GET-TSK-TERM.
      *>>>>
*% END
*% IF INI_SCR_COM
      *<<<< INI_SCR_COM
           MOVE SCR-TSK-TERM-ALF TO TSK-TERM-ALF.
           MOVE TSK-TERM-ALF        TO QUE-TERM.                       
           MOVE MAR-TYPE-P          TO QUE-TYPE.                       
           MOVE QUE-DEL             TO QUE-CMND.                       
           PERFORM GNS-PRO-QUE.                                        
      *>>>>
*% END
*% IF INI_FIO
      *<<<< INI_FIO
      *>>>>
*% END
*% IF SCR_GKEY_VAL_IKY
      *<<<< SCR_GKEY_VAL_IKY
      *>>>>
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM VAL-NUM-KEY-QMD
               PERFORM VAL-NUM-FLD-QMD
               PERFORM SCRL-TAB-QMD
               PERFORM SCR-TRL.
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
           PERFORM KEY-ALL-QMD.
           IF DBC-NUM-ICLI IN QMD-FLD NOT > ZEROES
               PERFORM BUS-CLI-NMB
               IF NOT FIO-STAT-OKS
                   MOVE SCR-STAT-CON TO SCR-STAT
                   MOVE '00' TO FIO-STAT
                   GO TO CONT-FND-KEY
               ELSE
                       NEXT SENTENCE
           ELSE
               PERFORM BUS-CLI-IDC
               IF NOT FIO-STAT-OKS
                   MOVE SCR-STAT-CON TO SCR-STAT
                   MOVE '00' TO FIO-STAT
                   GO TO CONT-FND-KEY.
      *>>>>
*% END
*% IF FND_KEY
      *<<<< FND_KEY
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
       CONT-FND-KEY.
           IF NOT FIO-STAT-OKS
               GO TO FIN-FND-KEY.

           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.
           PERFORM CPT-BLKS.
           MOVE CPT-STRN TO DBC-GLS-NOMC IN DBC.

           PERFORM PUT-TAB.
      *>>>>
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       PUT-TAB SECTION.
       INI-PUT-TAB.
           MOVE TSK-TERM-ALF        TO QUE-TERM.
           MOVE MAR-TYPE-PPTPG      TO QUE-TYPE.
           MOVE QUE-DEL             TO QUE-CMND.
           PERFORM GNS-PRO-QUE.                 
                                                
           MOVE TSK-TERM-ALF        TO QUE-TERM.
           MOVE MAR-TYPE-PPTPG      TO QUE-TYPE.
           MOVE QUE-PUT             TO QUE-CMND.
           MOVE 'C' TO QUE-ITEM.                
           MOVE +1 TO QUE-LITM.                 
           PERFORM GNS-PRO-QUE.

           MOVE TSK-TERM-ALF        TO QUE-TERM.                
           MOVE MAR-TYPE-G          TO QUE-TYPE.                
           MOVE QUE-DEL             TO QUE-CMND.                
           PERFORM GNS-PRO-QUE.                                 
                                                                
           MOVE DBC-NUM-ICLI IN QMD-FLD TO MAR-ENT-NUM-ICLI.
           MOVE DBC-VRF-ICLI IN QMD-FLD TO MAR-ENT-VRF-ICLI.

           MOVE MAR-ENTRADA TO QUE-ITEM.                   
           MOVE MAR-LENG-G  TO QUE-LITM.                   
           MOVE QUE-PUT     TO QUE-CMND.                   
           PERFORM GNS-PRO-QUE.                            
                                                           
           MOVE SCR-QIDD TO CCU-QIDD.                      
           MOVE SCR-LIDD TO CCU-LIDD.                      
                  
           MOVE 'LDCPSCCU'      TO SYS-PROG.                            
           MOVE CCU-TCMA        TO SYS-TCMA.                            
           MOVE 'S'             TO CCU-IND-MONI.                        
           MOVE 'M'             TO CCU-IND-MARG.  

      *RRM, HABILIAR CUANDO SE LLEVE A BEEBANK:
      **AFRI-INI 05-MAY-2015 LIMITE DE CREDITO SEGUN ARTICULO 84
      **     MOVE 'LDCPSCCU'      TO SYS-PROG.
      *      MOVE WCC-LDCPSMAR    TO SYS-PROG.
      **AFRI-FIN 05-MAY-2015 LIMITE DE CREDITO SEGUN ARTICULO 84
      *     MOVE CCU-TCMA        TO SYS-TCMA.
      *     MOVE 'S'             TO CCU-IND-MONI.                       
      **AFRI-INI 05-MAY-2015 LIMITE DE CREDITO SEGUN ARTICULO 84
      **     MOVE 'M'             TO CCU-IND-MARG.
      **AFRI-FIN 05-MAY-2015 LIMITE DE CREDITO SEGUN ARTICULO 84
           MOVE CCU-VARI        TO SYS-CMMA.                           
           MOVE SYS-LINK        TO SYS-CMND.                           
           PERFORM GNS-PRO-SYS.                                        
           MOVE SYS-CMMA TO CCU-CMMA.                                  
           IF CCU-NUM-MENS > ZEROES                                    
      *       MOVE '1'          TO WSS-SND-STAT                        
      *       MOVE CCU-GLS-MONI TO WSS-SND-MENS
      *LOP-INI 03-04-2003                                               
      *       MOVE CCU-GLS-MONI TO FRM-GLS-MONI IN QMD-FLD          
      *     ELSE                                                        
      **      MOVE '0'          TO WSS-SND-STAT                        
      **      MOVE SPACES       TO WSS-SND-MENS.                       
      *       MOVE SPACES       TO FRM-GLS-MONI IN QMD-FLD.
              MOVE CCU-GLS-MON1 TO FRM-MENS                             
              MOVE CCU-GLS-MON2 TO FRM-MEN2                             
              MOVE SCR-STAT-CON TO SCR-STAT                             
              MOVE FIO-STAT-NEX TO FIO-STAT                             
              GO TO FIN-PUT-TAB.                                        
      *LOP-INI 03-04-2003
                                                                       

           MOVE 1            TO SCR-NPAN.
           MOVE ZEROES TO WSS-IND-PANT.
           MOVE TSK-TERM-ALF TO SCR-TSK-TERM-ALF.
           PERFORM PUT-TAB-ELM.

      *LOP-INI 03-04-2003                                               
           IF WSS-IND-ERR-MAR = ZEROES                                  
              MOVE 'DEU'           TO MSG-COD-SIST                      
              MOVE 'MARGENES  OK'  TO MSG-COD-MENS                      
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC    TO FRM-GLS-MONI IN QMD-FLD           
              MOVE '00'            TO FIO-STAT                          
           ELSE                                                         
           IF WSS-IND-ERR-MAR = 001                                     
              MOVE WSS-GLS-ERR-MAR TO FRM-GLS-MONI IN QMD-FLD           
              MOVE '00'            TO FIO-STAT                          
           ELSE                                                         
              MOVE 'DEU'           TO MSG-COD-SIST                      
              MOVE 'ERRVALMARGEN'  TO MSG-COD-MENS                      
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC    TO FRM-GLS-MONI IN QMD-FLD           
              MOVE '00'            TO FIO-STAT.                         
      *LOP-FIN 03-04-2003

       FIN-PUT-TAB.
           EXIT.

       PUT-TAB-ELM SECTION.
       INI-PUT-TAB-ELM.
      *LOP-INI 03-04-2003                                               
           MOVE ZEROES       TO WSS-IND-ERR-MAR.                        
      *LOP-INI 03-04-2003 
           MOVE ZEROES       TO FRM-IFLD.
           MULTIPLY WSS-IND-PANT BY WSS-IMAX GIVING QUE-NITM.
           MOVE TSK-TERM-ALF        TO QUE-TERM.                       
           MOVE MAR-TYPE-P          TO QUE-TYPE.
       CIC-GET-QUE-QMD.                                                
           ADD 1                    TO QUE-NITM.                       
           MOVE MAR-LENG-P          TO QUE-LITM.                       
           MOVE QUE-GET             TO QUE-CMND.                       
           PERFORM GNS-PRO-QUE.                                        
           IF QUE-STAT = QUE-STAT-OKS                                  
                MOVE QUE-ITEM TO MAR-MENSAJE
      *LOP-INI 03-04-2003                                               
                MOVE QUE-ITEM TO WSS-RSP-QMD-CERR                       
                PERFORM SET-ERR-MAR                                     
      *LOP-FIN 03-04-2003
                ADD 1         TO FRM-IFLD
                PERFORM PRO-RESP-018                                   
                IF FRM-IFLD < WSS-IMAX
                     GO TO CIC-GET-QUE-QMD.                            

       FIN-PUT-TAB-ELM.
           EXIT.

      *LOP-INI 03-04-2003                                               
       SET-ERR-MAR SECTION.                                             
       INI-SET-ERR-MAR.                                                 
           IF WSS-RSP-QMD-CERR-2 = 'S'                                  
              MOVE MAR-MEN-GLS-MENS TO WSS-GLS-ERR-MAR                  
              ADD 1                 TO WSS-IND-ERR-MAR.                 
       FIN-SET-ERR-MAR.                                                 
           EXIT.                                                        
      *LOP-INI 03-04-2003 

       PRO-RESP-018 SECTION.
       INI-PRO-RESP-018.
           MOVE MAR-MEN-NUM-ICLI TO FRM-NUM-ICLI IN QMD-FLD(FRM-IFLD).
           MOVE MAR-MEN-VRF-ICLI TO FRM-VRF-ICLI IN QMD-FLD(FRM-IFLD).
           MOVE MAR-MEN-GLS-NOMC TO FRM-GLS-NOMC IN QMD-FLD(FRM-IFLD).
      *LOP-INI 24-ENE-2003                                              
      *    IF MAR-MEN-GLS-MENS NOT > SPACES                             
      *        MOVE SPACES TO FRM-GLS-MSJE IN QMD-FLD(FRM-IFLD)         
      *    ELSE                                                         
      *        MOVE 'DEU' TO MSG-COD-SIST                               
      *        MOVE MAR-MEN-GLS-MENS TO MSG-COD-MENS                    
      *        PERFORM GET-MSG                                          
      *        MOVE MSG-GLS-DESC TO FRM-GLS-MSJE IN QMD-FLD(FRM-IFLD).  
           MOVE MAR-MEN-GLS-MENS TO FRM-GLS-MSJE IN QMD-FLD(FRM-IFLD).  
      *LOP-FIN 24-ENE-2003 
           MOVE SPACES TO MSG-COD-MENS.
       FIN-PRO-RESP-018.
           EXIT.

       BUS-CLI-NMB SECTION.
       INI-BUS-CLI-NMB.

      *NOM-CLI Busca Cliente por medio del Nombre
           MOVE DBC-GLS-NOMC IN QMD-FLD TO STR-GLS-GSTR
           PERFORM STR-UNS
           MOVE STR-GLS-FUNS IN STR-VARI(1) TO DBC-GLS-APAT IN DBC
           MOVE STR-GLS-FUNS IN STR-VARI(2) TO DBC-GLS-AMAT IN DBC
           MOVE STR-GLS-FUNS IN STR-VARI(3) TO DBC-GLS-NOMB IN DBC.
           MOVE 'DBC-GLS-NOMC'  TO FIO-AKEY.
           MOVE FIO-GET-NLS-ALT TO FIO-CMND.
           PERFORM GET-CLI.
           IF MSG-COD-MENS > SPACES
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FIO-STAT-NEX TO FIO-STAT
           ELSE
               MOVE '00' TO PGM-STAT-DBC
      *         MOVE ADR-DBC-REQA TO SCR-DBC-REQA
               MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
               PERFORM CPT-BLKS
               MOVE CPT-STRN            TO DBC-GLS-NOMC IN QMD-FLD
               MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN QMD-FLD
               MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN QMD-FLD
               MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN QMD-FLD
               MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN QMD-FLD
               MOVE DBC-CAI-ICLI IN DBC TO DBC-CAI-ICLI IN QMD-FLD
               MOVE DBC-IIC-ICLI IN DBC TO DBC-IIC-ICLI IN QMD-FLD
               IF SCR-CCPP = 'ING'
      *             PERFORM PUT-FRM-QMD
                   MOVE '00' TO FIO-STAT.

       FIN-BUS-CLI-NMB.
           EXIT.

       BUS-CLI-IDC SECTION.
       INI-BUS-CLI-IDC.
      *IDC-CLI Busca Cliente por medio de IdC
           MOVE DBC-NUM-ICLI IN QMD-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN QMD-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN QMD-FLD TO DBC-GLS-ICLI IN DBC.
           MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY.
           MOVE FIO-GET-KEY-ALT TO FIO-CMND.
           PERFORM GET-CLI.
           IF MSG-COD-MENS > SPACES
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FIO-STAT-NEX TO FIO-STAT
           ELSE
               MOVE '00' TO PGM-STAT-DBC
      *         MOVE ADR-DBC-REQA TO SCR-DBC-REQA
               MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
               PERFORM CPT-BLKS
               MOVE CPT-STRN            TO DBC-GLS-NOMC IN QMD-FLD
               MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN QMD-FLD
               MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN QMD-FLD
               MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN QMD-FLD
               MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN QMD-FLD
               MOVE DBC-CAI-ICLI IN DBC TO DBC-CAI-ICLI IN QMD-FLD
               MOVE DBC-IIC-ICLI IN DBC TO DBC-IIC-ICLI IN QMD-FLD
               IF SCR-CCPP = 'ING'
      *             PERFORM PUT-FRM-QMD
                   MOVE '00' TO FIO-STAT.
       FIN-BUS-CLI-IDC.
           EXIT.


       SCRL-TAB-QMD SECTION.
       INI-SCRL-TAB-QMD.
           MOVE SPACES TO MSG-COD-MENS.
           IF FRM-FFLD = FRM-FFLD-PF6
               PERFORM PUT-TAB-ASC
           ELSE
               PERFORM PUT-TAB-DSC.
           IF MSG-COD-MENS > SPACES
               MOVE 'DEU' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-QMD.
           EXIT.

       PUT-TAB-ASC SECTION.
       INI-PUT-TAB-ASC.
           MOVE SPACES TO MSG-COD-MENS.
           MOVE SCR-TSK-TERM-ALF TO TSK-TERM-ALF.
           MOVE SCR-NPAN TO WSS-IND-PANT.
           PERFORM PUT-TAB-ELM.

           IF FRM-IFLD = 0
               MOVE 'QMDNONXT' TO MSG-COD-MENS
               GO TO FIN-PUT-TAB-ASC.

           ADD 1 TO SCR-NPAN.

           IF FRM-IFLD < WSS-IMAX
               ADD 1 FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1
                                 UNTIL WSS-I > WSS-IMAX.

           IF FRM-IFLD = WSS-IMAX
               ADD  1                TO QUE-NITM
               MOVE SCR-TSK-TERM-ALF TO QUE-TERM
               MOVE MAR-TYPE-P       TO QUE-TYPE
               MOVE MAR-LENG-P       TO QUE-LITM
               MOVE QUE-GET  TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT = QUE-STAT-OKS
                   MOVE 'MASQMD' TO MSG-COD-MENS.
       FIN-PUT-TAB-ASC.
           EXIT.

       PUT-TAB-DSC SECTION.
       INI-PUT-TAB-DSC.
           MOVE SPACES TO MSG-COD-MENS.
           MOVE SCR-TSK-TERM-ALF TO TSK-TERM-ALF.
           IF SCR-NPAN = 1
               MOVE 'QMDNOPRV' TO MSG-COD-MENS
               GO TO FIN-PUT-TAB-DSC.

           SUBTRACT 2 FROM SCR-NPAN GIVING WSS-IND-PANT.
            
           PERFORM PUT-TAB-ELM.
           SUBTRACT 1 FROM SCR-NPAN.

           IF SCR-NPAN = 1
               MOVE 'QMDNOPRV' TO MSG-COD-MENS
               GO TO FIN-PUT-TAB-DSC.
           
       FIN-PUT-TAB-DSC.
           EXIT.


       CLR-FRM SECTION.
       INI-CLR-FRM.
           MOVE ZEROES TO FRM-NUM-ICLI IN QMD-FLD(WSS-I).
           MOVE SPACES TO FRM-VRF-ICLI IN QMD-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-NOMC IN QMD-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-MSJE IN QMD-FLD(WSS-I).
       FIN-CLR-FRM.
           EXIT.


       COPY GNSBBTAB.
       COPY GNSBVTAB.
      *COPY GNSBVCOD.
       COPY GNSBBMSC.
       COPY GNSBGCPT.
       COPY GNSBGSTR.
       COPY SGCBGCLI.
       COPY GNSBGTSK.
      *>>>> EOF
*% END
