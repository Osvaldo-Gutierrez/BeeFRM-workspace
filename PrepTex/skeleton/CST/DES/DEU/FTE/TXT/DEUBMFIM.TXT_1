*% IF       FIN_PUT_ESC_FIM
      *<<<< FIN_PUT_ESC_FIM
           IF WSS-LIMP-KEY-ESC = WSS-CTE-S
               MOVE SPACES TO ESC-CAI-IESC IN FIM-FLD
               MOVE ZEROES TO ESC-IIC-IESC IN FIM-FLD.
      *>>>> FIN_PUT_ESC_FIM
*% END      FIN_PUT_ESC_FIM
*% IF       INI_PUT_FIA_FIM
      *<<<< INI_PUT_FIA_FIM
      *  ISP-INI 20-12-2018 CGM; Viaje Cliente 53600
           GO TO END-PUT-FIA-FIM.
      *  ISP-FIN 20-12-2018 CGM; Viaje Cliente 53600
      *>>>> INI_PUT_FIA_FIM
*% END      INI_PUT_FIA_FIM
*% IF       FIN_PUT_FIA_FIM
      *<<<< FIN_PUT_FIA_FIM
      *  ISP-INI 20-12-2018 CGM; Viaje Cliente 53600
      *BUS-TAB busca glosa de codigo en tabla
       END-PUT-FIA-FIM.
           IF PGM-STAT-FIA-NUL
               GO TO FIN-PUT-FIA-FIM.
           IF NOT PGM-STAT-FIA-OKS
               PERFORM CLR-FIA-FIM
               GO TO FIN-PUT-FIA-FIM.
           IF WSS-BKP-FIA > SPACES
                MOVE WSS-BKP-FIA TO FIA.
           MOVE FIA-COD-TOFS IN FIA TO FIA-COD-TOFS IN FIM-FLD.
           MOVE FIA-MSC-STAT IN FIA TO FIA-MSC-STAT IN FIM-FLD.
      *BUS-TAB busca glosa de codigo en tabla
           IF FIA-COD-TOFS IN FIM-FLD > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'TIO ' TO TAB-COD-TTAB IN TAB
               MOVE FIA-COD-TOFS IN FIM-FLD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-TOFS IN FIM-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-TOFS IN FIM-FLD.
      *  ISP-FIN 20-12-2018 CGM; Viaje Cliente 53600
      *>>>> FIN_PUT_FIA_FIM
*% END      FIN_PUT_FIA_FIM
*% IF       FIN_VAL_CON_KEY_FIM
      *<<<< FIN_VAL_CON_KEY_FIM
      *  ISP-INI 20-12-2018 CGM; Viaje Cliente 53600
           MOVE SPACES TO FRM-GLS-NOMC IN FIM-FLD.
      *  ISP-FIN 20-12-2018 CGM; Viaje Cliente 53600
      *>>>> FIN_VAL_CON_KEY_FIM
*% END      FIN_VAL_CON_KEY_FIM
*% IF       INI_FRM_COD_TTIO
      *<<<< INI_FRM_COD_TTIO 
      *  ISP-INI 20-12-2018 CGM; Viaje Cliente 53600
           MOVE SPACES TO FRM-GLS-NOMC IN FIM-FLD.
      *  ISP-FIN 20-12-2018 CGM; Viaje Cliente 53600
           IF FRM-COD-TTIO IN FIM-FLD NOT > SPACES
              MOVE SPACES TO FRM-COD-TTIO IN FIM-FLD.
           IF SCR-CMND = WSS-CTE-ING
               MOVE SPACES TO FRM-GLS-TTIO IN FIM-FLD
               GO TO FIN-VAL-FRM-COD-TTIO
           ELSE
           IF FRM-COD-TTIO IN FIM-FLD NOT > SPACES
               PERFORM ERR-GLS
               GO TO FIN-VAL-FRM-COD-TTIO.
      *>>>> INI_FRM_COD_TTIO 
*% END      INI_FRM_COD_TTIO 
*% IF       FIN_FRM_COD_TTIO
      *<<<< FIN_FRM_COD_TTIO
      *  ISP-INI 20-12-2018 CGM; Viaje Cliente 53600
           IF FRM-SUAR = FRM-SUAR-MAL
               GO TO FIN-VAL-FRM-COD-TTIO.
           MOVE TAB TO TIO.
           IF TIO-GLS-SIST NOT = WSS-CTE-COL
               MOVE 'TAB' TO MSG-COD-SIST
               MOVE 'CODTIOPNOCOL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *  ISP-FIN 20-12-2018 CGM; Viaje Cliente 53600
      *>>>> FIN_FRM_COD_TTIO
*% END      FIN_FRM_COD_TTIO
*% IF       FIN_VAL_CON_IKY_FIM
      *<<<< FIN_VAL_CON_IKY_FIM
           MOVE SPACES TO FRM-GLS-NOMC IN FIM-FLD.
      *>>>> FIN_VAL_CON_IKY_FIM
*% END      FIN_VAL_CON_IKY_FIM
*% IF       FRM_VRF_ICLI
      *<<<< FRM_VRF_ICLI

           MOVE FRM-NUM-ICLI IN FIM-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE FRM-IND-ICLI IN FIM-FLD TO DBC-IND-ICLI IN DBC.
           MOVE FRM-GLS-ICLI IN FIM-FLD TO DBC-GLS-ICLI IN DBC.
           MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           IF NOT FIO-STAT-OKS
               MOVE 'SGC'        TO MSG-COD-SIST
               MOVE 'CLI    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC IN MSG TO FRM-GLS-NOMC IN FIM-FLD
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
           ELSE
               MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
               PERFORM CPT-BLKS
               MOVE CPT-STRN            TO FRM-GLS-NOMC IN FIM-FLD
               MOVE DBC-NUM-ICLI IN DBC TO FRM-NUM-ICLI IN FIM-FLD
               MOVE DBC-IND-ICLI IN DBC TO FRM-IND-ICLI IN FIM-FLD
               MOVE DBC-GLS-ICLI IN DBC TO FRM-GLS-ICLI IN FIM-FLD
               MOVE DBC-VRF-ICLI IN DBC TO FRM-VRF-ICLI IN FIM-FLD.

      *>>>> FRM_VRF_ICLI
*% END      FRM_VRF_ICLI
*% IF       ESC_CAI_IESC
      *<<<< ESC_CAI_IESC
      *  ISP-INI 20-12-2018 CGM; Viaje Cliente 53600
           IF SCR-CMND = WSS-CTE-ING
               MOVE SPACES TO ESC-CAI-IESC IN FIM-FLD.
      *  ISP-FIN 20-12-2018 CGM; Viaje Cliente 53600
      *>>>> ESC_CAI_IESC
*% END      ESC_CAI_IESC
*% IF       ESC_IIC_IESC
      *<<<< ESC_IIC_IESC
      *  ISP-INI 20-12-2018 CGM; Viaje Cliente 53600
           IF SCR-CMND = WSS-CTE-ING
               MOVE ZEROES TO ESC-IIC-IESC IN FIM-FLD.
      *  ISP-FIN 20-12-2018 CGM; Viaje Cliente 53600
      *>>>> ESC_IIC_IESC
*% END      ESC_IIC_IESC
*% IF       INI_VAL_FLD_FIM
      *<<<< INI_VAL_FLD_FIM
           MOVE SPACES TO FRM-GLS-CCLI IN FIM-FLD.
      *>>>> INI_VAL_FLD_FIM
*% END      INI_VAL_FLD_FIM
*% IF       FRM_VRF_ICFS
      *<<<< FRM_VRF_ICFS

           IF FRM-NUM-ICFS IN FIM-FLD(FRM-IFLD) = 
              FRM-NUM-ICLI IN FIM-FLD
                MOVE 'SGC'        TO MSG-COD-SIST
                MOVE 'ICFS=ICLI ' TO MSG-COD-MENS
                PERFORM GET-MSG
                MOVE MSG-GLS-DESC TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                GO TO FIN-VAL-FRM-VRF-ICFS.

           IF FRM-NUM-ICFS IN FIM-FLD(FRM-IFLD) NOT > ZEROES
                GO TO FIN-VAL-FRM-VRF-ICFS.

           MOVE FRM-NUM-ICFS IN FIM-FLD(FRM-IFLD) TO
                DBC-NUM-ICLI IN DBC.
           MOVE FRM-IND-ICFS IN FIM-FLD(FRM-IFLD) TO
                DBC-IND-ICLI IN DBC.
           MOVE FRM-GLS-ICFS IN FIM-FLD(FRM-IFLD) TO
                DBC-GLS-ICLI IN DBC.
           MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DBC.

           IF NOT FIO-STAT-OKS
               MOVE 'SGC'        TO MSG-COD-SIST
               MOVE 'CLI    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
           ELSE
               IF FRM-NUM-CCLI IN FIM-FLD > ZEROES AND
                  FRM-NUM-CCLI IN FIM-FLD = FRM-IFLD

                    MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
                    PERFORM CPT-BLKS
                    MOVE CPT-STRN TO FRM-GLS-CCLI IN FIM-FLD.
      *>>>> FRM_VRF_ICFS
*%  END     FRM_VRF_ICFS
*% IF FIN_PUT_FIM_FIM
      *<<<< FIN_PUT_FIM_FIM
      *>>>>
*% END
