       77  WSS-CTE-FFIM       VALUE 'FFIM'         PIC X(04).
       77  WSS-CTE-ING        VALUE 'ING'          PIC X(03).
       77  WSS-CTE-CON        VALUE 'CON'          PIC X(03).
       77  WSS-CTE-AFS        VALUE 'AFS'          PIC X(03).
       77  WSS-CTE-23         VALUE '23'           PIC X(02).
       77  WSS-CTE-DEU        VALUE 'DEU'          PIC X(03).
       77  WSS-CTE-COL        VALUE 'COL'          PIC X(03).
       77  WSS-CTE-400        VALUE 400            PIC 9(03).
      
       77  WSS-IDX-IFIA      VALUE ZEROES     PIC 9(03).
       77  WSS-TOT-IFIA      VALUE ZEROES     PIC 9(03).
       01  WSS-TBL-IFIA.
           03 WSS-TBL-IFIA-OCC   OCCURS 400 TIMES.
              05 WSS-TBL-IFIA-ELM   PIC X(15).
      
       01  WSS-TBL-ICLI.
           03 WSS-TBL-ICLI-OCC   OCCURS 40 TIMES.
              05 WSS-TBL-ICLI-ELM   PIC X(12).
       77  WSS-TOT-ICLI      VALUE ZEROES     PIC 9(03).
       77  WSS-IDX-ICLI      VALUE ZEROES     PIC 9(03).
      
