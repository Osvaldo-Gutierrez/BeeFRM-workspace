*%! Codigo adicional
*% IF SETTAG
*% VSNBQ = "01-00"
*% FNCBQ = "1711_05"
*% VSNBU = "01-00"
*% FNCBU = "1711_05"
*% END
*% IF       FRM_VRF_ICLI
      *<<<< FRM_VRF_ICLI

           MOVE FRM-NUM-ICLI IN FIM-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE FRM-IND-ICLI IN FIM-FLD TO DBC-IND-ICLI IN DBC.
           MOVE FRM-GLS-ICLI IN FIM-FLD TO DBC-GLS-ICLI IN DBC.
           MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           IF NOT FIO-STAT-OKS
               MOVE 'SGC'        TO MSG-COD-SIST
               MOVE 'CLI    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC IN MSG TO FRM-GLS-NOMC IN FIM-FLD
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
           ELSE
               MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
               PERFORM CPT-BLKS
               MOVE CPT-STRN            TO FRM-GLS-NOMC IN FIM-FLD
               MOVE DBC-NUM-ICLI IN DBC TO FRM-NUM-ICLI IN FIM-FLD
               MOVE DBC-IND-ICLI IN DBC TO FRM-IND-ICLI IN FIM-FLD
               MOVE DBC-GLS-ICLI IN DBC TO FRM-GLS-ICLI IN FIM-FLD
               MOVE DBC-VRF-ICLI IN DBC TO FRM-VRF-ICLI IN FIM-FLD.

      *>>>> FRM_VRF_ICLI
*% END      FRM_VRF_ICLI
*% IF       INI_VAL_FLD_FIM
      *<<<< INI_VAL_FLD_FIM
           MOVE SPACES TO FRM-GLS-CCLI IN FIM-FLD.
      *>>>> INI_VAL_FLD_FIM
*% END      INI_VAL_FLD_FIM
*% IF       FRM_VRF_ICFS
      *<<<< FRM_VRF_ICFS

           IF FRM-NUM-ICFS IN FIM-FLD(FRM-IFLD) = 
              FRM-NUM-ICLI IN FIM-FLD
                MOVE 'SGC'        TO MSG-COD-SIST
                MOVE 'ICFS=ICLI ' TO MSG-COD-MENS
                PERFORM GET-MSG
                MOVE MSG-GLS-DESC TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                GO TO FIN-VAL-FRM-VRF-ICFS.

           IF FRM-NUM-ICFS IN FIM-FLD(FRM-IFLD) NOT > ZEROES
                GO TO FIN-VAL-FRM-VRF-ICFS.

           MOVE FRM-NUM-ICFS IN FIM-FLD(FRM-IFLD) TO
                DBC-NUM-ICLI IN DBC.
           MOVE FRM-IND-ICFS IN FIM-FLD(FRM-IFLD) TO
                DBC-IND-ICLI IN DBC.
           MOVE FRM-GLS-ICFS IN FIM-FLD(FRM-IFLD) TO
                DBC-GLS-ICLI IN DBC.
           MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DBC.

           IF NOT FIO-STAT-OKS
               MOVE 'SGC'        TO MSG-COD-SIST
               MOVE 'CLI    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
           ELSE
               IF FRM-NUM-CCLI IN FIM-FLD > ZEROES AND
                  FRM-NUM-CCLI IN FIM-FLD = FRM-IFLD

                    MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
                    PERFORM CPT-BLKS
                    MOVE CPT-STRN TO FRM-GLS-CCLI IN FIM-FLD.
      *>>>> FRM_VRF_ICFS
*%  END     FRM_VRF_ICFS
*% IF FIN_PUT_FIM_FIM
      *<<<< FIN_PUT_FIM_FIM
      *>>>>
*% END
*% IF FIN_PUT_AMF_FIM
      *<<<< FIN_PUT_AMF_FIM
      *>>>>
*% END
*% IF FIN_RET_FIM_FIM OR FIN_KEY_FIM_FIM
      *<<<< FIN_RET_FIM_FIM OR FIN_KEY_FIM_FIM
      *>>>>
*% END
*% IF FIN_RET_AMF_FIM OR FIN_KEY_AMF_FIM
      *<<<< FIN_RET_AMF_FIM OR FIN_KEY_AMF_FIM
      *>>>>
*% END
*% IF       FIN_UNP_IKY_FIM
      *<<<< FIN_UNP_IKY_FIM
           IF SCR-CMND = WSS-CTE-AFS OR
              SCR-CMND = WSS-CTE-ING
               PERFORM PRO-IKY-FIM.

      *>>>> FIN_UNP_IKY_FIM
*% END      FIN_UNP_IKY_FIM
*% IF FIA_COD_TOFS
      *<<<< FIA_COD_TOFS
      *>>>>
*% END
*% IF       FIN_FIA_COD_TOFS
      *<<<< FIN_FIA_COD_TOFS
           IF FRM-SUAR = FRM-SUAR-MAL
               GO TO FIN-VAL-FIA-COD-TOFS.
           MOVE TAB TO TIO.
           IF TIO-GLS-SIST NOT = WSS-CTE-DEU
                 MOVE 'TAB' TO MSG-COD-SIST
                 MOVE 'CODTIOPNOSIS' TO MSG-COD-MENS
                 PERFORM GET-MSG
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 MOVE FRM-SUAR-MAL TO FRM-SUAR.

      *>>>> FIN_FIA_COD_TOFS
*% END      FIN_FIA_COD_TOFS
*% IF       FIN_AMF_COD_TOOA
      *<<<< FIN_AMF_COD_TOOA
           IF FRM-SUAR = FRM-SUAR-MAL
               GO TO FIN-VAL-AMF-COD-TOOA.
           MOVE TAB TO TIO.
           IF TIO-GLS-SIST NOT = WSS-CTE-COL
               MOVE 'TAB' TO MSG-COD-SIST
               MOVE 'CODTIOPNOSIS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>> FIN_AMF_COD_TOOA
*% END      FIN_AMF_COD_TOOA
*% IF FIN_FIM_COD_TOFS
      *<<<< FIN_FIM_COD_TOFS
      *>>>>
*% END
*% IF INI_ESC_COD_RCTT AND PGM_PER
      *<<<< INI_ESC_COD_RCTT
      *>>>>
*% END
*% IF FIA_MSC_STAT
      *<<<< FIA_MSC_STAT
      *VAL-IND Valida miscelaneo de tabla
           MOVE 'GNS' TO MSC-COD-SIST.
           MOVE 'VIGE' TO MSC-COD-TMSC IN MSC.
           MOVE FIA-MSC-STAT IN FIM-FLD TO MSC-COD-CMSC IN MSC.
           PERFORM VAL-IND.
      *>>>>
*% END
