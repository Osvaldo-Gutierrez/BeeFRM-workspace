*% PGM_PTC = TRUE
*% LARGO_MI_COMMAREA = 2750
*% LARGO_TOTAL_COMMAREA = 3000
*% GEN_MFC = TRUE
*% IF PGM_PQ
*% CLRMFC = "DEUZQICT"
*% ELSE
*% CLRMFC = "DEUZUICT"
*% END
*% IF WSS
      *<<<< WSS
       COPY DEUWXICT.
       COPY SGCBRDBC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
       COPY GNSBRCIC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CIC-REQA==.
       COPY GNSWGCIC.
       COPY GNSWGCPT.
       
       01 WSS-VARI.
          03 WCC-O              VALUE 'O'        PIC X(01).
          03 WCC-N              VALUE 'N'        PIC X(01).
          03 WCC-PGM-PU         VALUE 'DEUPUICT' PIC X(08).
*% IF PGM_PQ
          03 WCC-PGM-PU-IC2     VALUE 'DEUPUIC2' PIC X(08).
*% END
          03 WCC-IC2            VALUE 'DEUPQIC2' PIC X(08).
          03 WCC-3              VALUE '3'        PIC X(01).
          03 WCC-S              VALUE 'S'        PIC X(01).
          03 WCC-CIP            VALUE 'CIP '     PIC X(04).
          03 WCC-PGM-PQ         VALUE 'DEUPQICT' PIC X(08).
          03 WCC-1              VALUE 1          PIC 9(01).
          03 WCC-S1             VALUE -1         PIC S9(01).
          03 WCC-ELI            VALUE 'ELI'      PIC X(03).
          03 WCC-MOD            VALUE 'MOD'      PIC X(03).
          03 WCC-ING            VALUE 'ING'      PIC X(03).
          03 WCC-CON            VALUE 'CON'      PIC X(03).
          03 WCC-I              VALUE 'I'        PIC X(01).
          03 WCC-DEU        VALUE 'DEU'          PIC X(03).
          03 WCC-CLINEX     VALUE 'CLI    NEX'   PIC X(10).
          03 WCC-CODNEX     VALUE 'COD    NEX'   PIC X(10).
          03 WCC-TAB        VALUE 'TAB'          PIC X(03).
          03 WCC-VLR        VALUE 'VLR'          PIC X(03).
          03 WCC-CERTIENUSO VALUE 'CERTI EN USO' PIC X(12).
*% IF PGM_PQ
          03 WCC-CERTIEMITI VALUE 'CERTIEMITIDO' PIC X(12).
*% END
          03 WCC-NOMOD      VALUE 'NOMOD'        PIC X(05).
          03 WCC-ERRORBORRCIP VALUE 'ERRORBORRCIP' PIC X(12).
          
          03 WSS-CAM-MOD.
             05 WSS-CIC-ICLI                     PIC X(12).
             05 WSS-NUM-ICLI                     PIC 9(08).
             05 WSS-VRF-ICLI                     PIC X(01).
             05 WSS-IND-ICLI                     PIC X(01).
             05 WSS-GLS-ICLI                     PIC X(03).
             05 WSS-GLS-NOMC                     PIC X(75).
             05 WSS-FEC-FVIG                     PIC 9(08).
             05 WSS-FEC-FEMI                     PIC 9(08).
             05 WSS-COD-IEMI                     PIC X(10).
             05 WSS-COD-ICIE                     PIC X(18).
             05 WSS-FEC-FLIQ                     PIC 9(08).
             05 WSS-COD-OPCO                     PIC X(20).
             05 WSS-COD-MONE                     PIC X(06).
             05 WSS-CAI-IORI                     PIC X(04).
             05 WSS-IIC-IORI                     PIC 9(08).
             05 WSS-SGV-MORI                     PIC 9(11)V9(04).
             05 WSS-VAL-3475                     PIC 9(11)V9(04).
             05 WSS-VAL-TPAG                     PIC 9(11)V9(04).

           03 WSS-CPT-NOMB.
             05 WSS-CPT-DESC                          PIC X(40).
             05 WSS-CPT-CAI                           PIC X(04).
             05 WSS-CPT-IIC                           PIC X(08).
             05 WSS-CPT-NUM                           PIC 9(03).
             05 WSS-GLS-DATA                          PIC X(40).
             03 WSS-CPT-NOMB-R  REDEFINES  WSS-CPT-NOMB  PIC X(95).
      *>>>>
*% END
*% IF FIN_SCR_VARI
      *<<<< FIN_SCR_VARI
       COPY DEUWGENG.
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
           PERFORM KEY-ALL-ICT.
           MOVE WCC-1 TO CIP-NUM-ICIP IN CIP.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM DEU-FIO-CIP.
           MOVE FIO-STAT TO PGM-STAT-CIP.
           IF NOT FIO-STAT-OKS
              GO TO FIN-FND-KEY.

           IF SCR-CMND = WCC-MOD OR WCC-ELI
              IF CIP-IND-DUSO IN CIP = WCC-S
                 MOVE WCC-CERTIENUSO TO MSG-COD-MENS
                 MOVE WCC-DEU TO MSG-COD-SIST
                 PERFORM GET-MSG
                 MOVE MSG-GLS-DESC TO FRM-MEN1
                 SET FIO-STAT-ERR TO TRUE
                 GO TO FIN-FND-KEY.

           IF SCR-CMND = WCC-MOD OR WCC-ELI
              IF CIP-COD-ATRN IN CIP NOT = WCC-PGM-PU AND
                 CIP-COD-ATRN IN CIP NOT = WCC-PGM-PU-IC2 AND
                 CIP-COD-ATRN IN CIP NOT = SPACES
                  MOVE WCC-CERTIEMITI TO MSG-COD-MENS
                  MOVE WCC-DEU TO MSG-COD-SIST
                  PERFORM GET-MSG
                  MOVE MSG-GLS-DESC TO FRM-MEN1
                  SET FIO-STAT-ERR TO TRUE
                  GO TO FIN-FND-KEY.

           IF SCR-CMND = WCC-ING
               MOVE WCC-I TO CIP-IND-DUSO IN CIP
               MOVE WCC-I TO CIP-IND-DUSO IN ICT-FLD
           ELSE
               MOVE CIP-IND-DUSO IN CIP TO CIP-IND-DUSO IN ICT-FLD.

           MOVE SPACES TO DBC.
           MOVE CIP-CIC-ICLI IN CIP TO DBC-CIC-ICLI IN DBC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           IF NOT FIO-STAT-OKS
              GO TO FIN-FND-KEY.

           SET PGM-STAT-DBC-OKS TO TRUE.
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.
           PERFORM CPT-BLKS.
           MOVE CPT-STRN TO FRM-GLS-NOMC IN ICT-FLD.

           GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
           GO TO EXT-FND-KEY.
      *>>>>
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-OFRM-SRV
               MOVE TSK-TERM-ALF TO ICT-COD-TERM-TSS
           ELSE
               MOVE EIBTRMID     TO ICT-COD-TERM-TSS.
           PERFORM GNS-ERR-QUE.
      *>>>>
*% END
*% IF INI_CMN_MOD
      *<<<< INI_CMN_MOD
           PERFORM KEY-ALL-ICT.
           MOVE WCC-1 TO CIP-NUM-ICIP IN CIP.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM DEU-FIO-CIP.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-MOD.

           IF CIP-IND-DUSO IN CIP = WCC-S
      * DEBE INFORMAR QUE CERTIFICADO YA FUE ACTIVADO Y NO SE PUEDE
      * MODIFICAR
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM DEU-FIO-CIP
               MOVE WCC-S TO SCR-IERR
               MOVE WCC-CERTIENUSO TO FRM-MENS
               GO TO FIN-CMN-MOD.

           PERFORM MOV-PAN-VARI.

           PERFORM RET-ALL-ICT.
           PERFORM RET-HDR-CIP.
           MOVE WCC-PGM-PU TO CIP-COD-ATRN IN CIP.
NEW        MOVE CIP-COD-MONE IN CIP  TO CIP-COD-VCOC IN CIP.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM DEU-FIO-CIP.
           IF NOT FIO-STAT-OKS
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM DEU-FIO-CIP
               MOVE WCC-NOMOD TO FRM-MENS
               GO TO FIN-CMN-MOD.
       MOD-ACT-TODO.
           ADD WCC-1 TO CIP-NUM-ICIP IN CIP.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM DEU-FIO-CIP.
           IF NOT FIO-STAT-OKS
               MOVE ZEROES TO FIO-STAT
               GO TO FIN-CMN-MOD.

           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM DEU-FIO-CIP.
           PERFORM MOV-VARI-CIP.
           MOVE FIO-MOD TO FIO-CMND.
           MOVE WCC-PGM-PU TO CIP-COD-ATRN IN CIP.
           PERFORM DEU-FIO-CIP.
           IF NOT FIO-STAT-OKS
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM DEU-FIO-CIP
               MOVE WCC-NOMOD TO FRM-MENS
               GO TO FIN-CMN-MOD.
           GO TO MOD-ACT-TODO.
      *>>>>
*% END
*% IF INI_CMN_ING
      *<<<< INI_CMN_ING
           PERFORM RET-ALL-ICT.
           MOVE WCC-N TO SCR-IND-ENG.
      *GET-CIC Obtener CIC
           MOVE WCC-CIP IN WSS-VARI TO CIC-CAI-ICIC IN CIC.
           MOVE CIC-GET             TO CIC-CMND.
           PERFORM PRO-CIC.
           IF CIC-STAT NOT = CIC-STAT-OKS
               MOVE CIC-MENS        TO FRM-MENS
               GO TO FIN-CMN-ING.
      * PARA EL ENGANCHE
           MOVE CIC-CAI-ICIC TO SCR-CAI-ICER.
           MOVE CIC-IIC-ICIC TO SCR-IIC-ICER.
           MOVE WCC-1        TO SCR-NUM-ICIP.
           MOVE CIC-CAI-ICIC TO CIP-CAI-ICER IN ICT-FLD.
           MOVE CIC-IIC-ICIC TO CIP-IIC-ICER-EDT IN ICT-EDT.
           MOVE CIP-IIC-ICER-EDT IN ICT-EDT TO
                                CIP-IIC-ICER-ALF IN ICT-FLD.
           MOVE MSG-GLS-DESC            TO WSS-CPT-DESC.
           MOVE CIP-CAI-ICER IN ICT-FLD TO WSS-CPT-CAI.
           MOVE CIP-IIC-ICER IN ICT-FLD TO WSS-CPT-IIC.
           MOVE WCC-1                   TO WSS-CPT-NUM.
           MOVE MSG-GLS-DATA            TO WSS-GLS-DATA.
           MOVE WSS-CPT-NOMB-R          TO CPT-STRN.
           PERFORM CPT-BLKS.
           MOVE CPT-STRN                TO FRM-MENS.
           MOVE FRM-MENS TO SCR-MENS-EXT.
      *
           MOVE CIC-CIC-ICIC        TO CIP-CIC-ICER IN CIP.
           MOVE WCC-1               TO CIP-NUM-ICIP IN CIP.
           MOVE CIC-CIC-ICIC        TO SCR-CIC-ICER.
           MOVE WCC-1               TO SCR-NUM-ICIP.

           MOVE SPACES TO DBC.
           MOVE DBC-NUM-ICLI IN ICT-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN ICT-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN ICT-FLD TO DBC-GLS-ICLI IN DBC.
           MOVE 'DBC-IDC-ICLI'          TO FIO-AKEY.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           IF NOT FIO-STAT-OKS
      * INFORMAR QUE CLIENTE NO EXISTE
               MOVE 'CLINOEXI' TO FRM-MENS
               GO TO FIN-CMN-ING.

           MOVE DBC-CIC-ICLI IN DBC  TO CIP-CIC-ICLI IN CIP.

           MOVE WCC-PGM-PU           TO CIP-COD-ATRN IN CIP.
           MOVE WCC-I                TO CIP-IND-DUSO IN CIP.

           PERFORM MOV-VLR-DEFE.
NEW        MOVE CIP-COD-MONE IN CIP  TO CIP-COD-VCOC IN CIP.

           MOVE FIO-PUT TO FIO-CMND.
           PERFORM DEU-FIO-CIP.

           PERFORM LLA-PAN-IC2.

           GO TO FIN-CMN-ING.
           MOVE SPACES TO CIP.
      *>>>>
*% END
*% IF INI_CMN_ELI
      *<<<< INI_CMN_ELI
           MOVE SPACES TO MSG-COD-MENS.
           PERFORM KEY-ALL-ICT.
       LOOP-ELI-CIP.
           MOVE ZEROES TO CIP-NUM-ICIP IN CIP.
           MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM DEU-FIO-CIP.
           IF NOT FIO-STAT-OKS OR
              CIP-CAI-ICER IN ICT-FLD NOT = CIP-CAI-ICER IN CIP OR
              CIP-IIC-ICER IN ICT-FLD NOT = CIP-IIC-ICER IN CIP
               MOVE ZEROES TO FIO-STAT
               GO TO LIB-MEM.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM DEU-FIO-CIP.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ELI.
           MOVE FIO-DEL TO FIO-CMND.
           PERFORM DEU-FIO-CIP.
           IF FIO-STAT-OKS
               MOVE SPACES TO CIP
              MOVE CIP-CAI-ICER IN ICT-FLD TO CIP-CAI-ICER IN CIP
               MOVE CIP-IIC-ICER IN ICT-FLD TO CIP-IIC-ICER IN CIP
               GO TO LOOP-ELI-CIP.
           MOVE WCC-ERRORBORRCIP TO MSG-COD-MENS.
           MOVE WCC-S TO SCR-IERR.
       LIB-MEM.
           IF MSG-COD-MENS = SPACES
               MOVE SPACES TO FRM-MEN1
               MOVE SPACES TO FRM-MENS
           ELSE
               MOVE MSG-COD-MENS TO FRM-MENS.
           GO TO FIN-CMN-ELI.
      *>>>>
*% END  
*% IF EOF
      *<<<< EOF
       COPY SGCBFDBC.
       COPY GNSBFCIC.
       COPY GNSBGCIC.
       COPY GNSBGCPT.
      *>>>>
*% END
*% IF EOF_PU
      *<<<< EOF
       MOV-PAN-VARI SECTION.
       INI-MOV-PAN-VARI.
           MOVE SPACES TO WSS-CAM-MOD.
           MOVE DBC-CIC-ICLI IN DBC     TO WSS-CIC-ICLI IN WSS-VARI.
           MOVE CIP-NUM-ICLI IN ICT-FLD TO WSS-NUM-ICLI IN WSS-VARI.
           MOVE CIP-VRF-ICLI IN ICT-FLD TO WSS-VRF-ICLI IN WSS-VARI.
           MOVE CIP-IND-ICLI IN ICT-FLD TO WSS-IND-ICLI IN WSS-VARI.
           MOVE CIP-GLS-ICLI IN ICT-FLD TO WSS-GLS-ICLI IN WSS-VARI.
           MOVE CIP-GLS-NOMC IN ICT-FLD TO WSS-GLS-NOMC IN WSS-VARI.
           MOVE CIP-FEC-FVIG IN ICT-FLD TO WSS-FEC-FVIG IN WSS-VARI.
           MOVE CIP-FEC-FEMI IN ICT-FLD TO WSS-FEC-FEMI IN WSS-VARI.
           MOVE CIP-COD-IEMI IN ICT-FLD TO WSS-COD-IEMI IN WSS-VARI.
           MOVE CIP-COD-ICIE IN ICT-FLD TO WSS-COD-ICIE IN WSS-VARI.
           MOVE CIP-FEC-FLIQ IN ICT-FLD TO WSS-FEC-FLIQ IN WSS-VARI.
           MOVE CIP-COD-OPCO IN ICT-FLD TO WSS-COD-OPCO IN WSS-VARI.
           MOVE CIP-COD-MONE IN ICT-FLD TO WSS-COD-MONE IN WSS-VARI.
           MOVE CIP-CAI-IORI IN ICT-FLD TO WSS-CAI-IORI IN WSS-VARI.
           MOVE CIP-IIC-IORI IN ICT-FLD TO WSS-IIC-IORI IN WSS-VARI.
           MOVE CIP-VAL-MCRE IN ICT-FLD TO WSS-SGV-MORI IN WSS-VARI.
           MOVE CIP-VAL-3475 IN ICT-FLD TO WSS-VAL-3475 IN WSS-VARI.
           MOVE CIP-VAL-TPAG IN ICT-FLD TO WSS-VAL-TPAG IN WSS-VARI.
       FIN-MOV-PAN-VARI.
           EXIT.

       MOV-VARI-CIP SECTION.
       INI-MOV-VARI-CIP.

           MOVE SPACES TO CIP-CIC-IOPC IN CIP.
           MOVE SPACES TO CIP-COD-EVEN IN CIP.
           MOVE ZEROES TO CIP-NUM-ICAN IN CIP.
           MOVE SPACES TO CIP-COD-CPTO IN CIP.
           MOVE ZEROES TO CIP-NUM-CPTO IN CIP.
           MOVE ZEROES TO CIP-FEC-FTRN IN CIP.
           MOVE ZEROES TO CIP-HRA-HRTR IN CIP.
           MOVE SPACES TO CIP-MSC-TACC IN CIP.
           MOVE SPACES TO CIP-MSC-STAT IN CIP.
           MOVE SPACES TO CIP-COD-OTRN IN CIP.
           MOVE SPACES TO CIP-CIC-IORI IN CIP.
           MOVE ZEROES TO CIP-FEC-FINI IN CIP.
           MOVE ZEROES TO CIP-SGV-TUNI IN CIP.
           MOVE ZEROES TO CIP-SGV-TINT IN CIP.
           MOVE ZEROES TO CIP-SGV-MNTO IN CIP.
           MOVE ZEROES TO CIP-SGV-MCAL IN CIP.
           MOVE ZEROES TO CIP-VAL-CTRA IN CIP.
           MOVE ZEROES TO CIP-FEC-FTER IN CIP.
           MOVE ZEROES TO CIP-FEC-FMAV IN CIP.
           MOVE ZEROES TO CIP-FEC-FCOL IN CIP.
           MOVE ZEROES TO CIP-NUM-IVEN IN CIP.
           MOVE ZEROES TO CIP-SGV-CUOT IN CIP.
           MOVE SPACES TO CIP-IND-CXE1 IN CIP.
           MOVE ZEROES TO CIP-NUM-CPRO IN CIP.
           MOVE SPACES TO CIP-CIC-ORIG IN CIP.
           MOVE SPACES TO CIP-COD-IEMI IN CIP.
           MOVE SPACES TO CIP-COD-ICIE IN CIP.
           MOVE SPACES TO CIP-COD-TIPO IN CIP.
           MOVE ZEROES TO CIP-NUM-PTRM IN CIP.
           MOVE ZEROES TO CIP-FEC-FORI IN CIP.
           MOVE ZEROES TO CIP-SGV-TOPE IN CIP.
           MOVE ZEROES TO CIP-SGV-TACU IN CIP.
           MOVE ZEROES TO CIP-NUM-FOLI IN CIP.
           MOVE SPACES TO CIP-GLS-NORM IN CIP.
           MOVE ZEROES TO CIP-NUM-PDES IN CIP.
           MOVE ZEROES TO CIP-SGV-VDES IN CIP.
           MOVE ZEROES TO CIP-SGV-VTOT IN CIP.
      *    MOVE SPACES TO CIP-COD-VCOC IN CIP.
           MOVE ZEROES TO CIP-FEC-TTRI IN CIP.
           MOVE SPACES TO CIP-COD-ITRU IN CIP.
           MOVE SPACES TO CIP-COD-TDOC IN CIP.
           MOVE SPACES TO CIP-MSC-TIMP IN CIP.
           MOVE ZEROES TO CIP-VAL-CAMO IN CIP.
           MOVE ZEROES TO CIP-VAL-INTN IN CIP.
           MOVE ZEROES TO CIP-VAL-CPRE IN CIP.
           MOVE ZEROES TO CIP-VAL-CFOG IN CIP.
           MOVE ZEROES TO CIP-VAL-CMOR IN CIP.
           MOVE SPACES TO CIP-GLS-DISP IN CIP.
           MOVE ZEROES TO CIP-SGV-MORI IN CIP.
           MOVE ZEROES TO CIP-SGV-TINT IN CIP.

           MOVE WSS-CIC-ICLI IN WSS-VARI TO CIP-CIC-ICLI IN CIP.
           MOVE WSS-NUM-ICLI IN WSS-VARI TO CIP-NUM-ICLI IN CIP.
           MOVE WSS-VRF-ICLI IN WSS-VARI TO CIP-VRF-ICLI IN CIP
           MOVE WSS-IND-ICLI IN WSS-VARI TO CIP-IND-ICLI IN CIP.
           MOVE WSS-GLS-ICLI IN WSS-VARI TO CIP-GLS-ICLI IN CIP.
           MOVE WSS-GLS-NOMC IN WSS-VARI TO CIP-GLS-NOMC IN CIP.
           MOVE WSS-FEC-FVIG IN WSS-VARI TO CIP-FEC-FVIG IN CIP.
           MOVE WSS-FEC-FEMI IN WSS-VARI TO CIP-FEC-FEMI IN CIP.
           MOVE WSS-COD-IEMI IN WSS-VARI TO CIP-COD-IEMI IN CIP.
           MOVE WSS-COD-ICIE IN WSS-VARI TO CIP-COD-ICIE IN CIP.
           MOVE WSS-FEC-FLIQ IN WSS-VARI TO CIP-FEC-FLIQ IN CIP.
           MOVE WSS-COD-OPCO IN WSS-VARI TO CIP-COD-OPCO IN CIP.
           MOVE WSS-COD-MONE IN WSS-VARI TO CIP-COD-MONE IN CIP.
NEW        MOVE WSS-COD-MONE IN WSS-VARI TO CIP-COD-VCOC IN CIP.
           MOVE WSS-CAI-IORI IN WSS-VARI TO CIP-CAI-IORI IN CIP.
           MOVE WSS-IIC-IORI IN WSS-VARI TO CIP-IIC-IORI IN CIP.
           MOVE WSS-COD-OPCO IN WSS-VARI TO CIP-CIC-ORIG IN CIP.
           MOVE WSS-SGV-MORI IN WSS-VARI TO CIP-VAL-MCRE IN CIP.
           MOVE WSS-VAL-3475 IN WSS-VARI TO CIP-VAL-3475 IN CIP.
           MOVE WSS-VAL-TPAG IN WSS-VARI TO CIP-VAL-TPAG IN CIP.

       FIN-MOV-VARI-CIP.
           EXIT.

       MOV-VLR-DEFE SECTION.
       INI-MOV-VLR-DEFE.
           MOVE SPACES TO CIP-CIC-IOPC IN CIP.
           MOVE SPACES TO CIP-COD-EVEN IN CIP.
           MOVE ZEROES TO CIP-NUM-ICAN IN CIP.
           MOVE SPACES TO CIP-COD-CPTO IN CIP.
           MOVE ZEROES TO CIP-NUM-CPTO IN CIP.
           MOVE ZEROES TO CIP-FEC-FTRN IN CIP.
           MOVE ZEROES TO CIP-HRA-HRTR IN CIP.
           MOVE SPACES TO CIP-MSC-TACC IN CIP.
           MOVE SPACES TO CIP-MSC-STAT IN CIP.
           MOVE SPACES TO CIP-COD-OTRN IN CIP.
           MOVE SPACES TO CIP-CIC-IORI IN CIP.
           MOVE ZEROES TO CIP-FEC-FINI IN CIP.
           MOVE ZEROES TO CIP-SGV-TUNI IN CIP.
           MOVE ZEROES TO CIP-SGV-TINT IN CIP.
           MOVE ZEROES TO CIP-SGV-MNTO IN CIP.
           MOVE ZEROES TO CIP-SGV-MCAL IN CIP.
           MOVE ZEROES TO CIP-VAL-CTRA IN CIP.
           MOVE ZEROES TO CIP-FEC-FTER IN CIP.
           MOVE ZEROES TO CIP-FEC-FMAV IN CIP.
           MOVE ZEROES TO CIP-FEC-FCOL IN CIP.
           MOVE ZEROES TO CIP-NUM-IVEN IN CIP.
           MOVE ZEROES TO CIP-SGV-CUOT IN CIP.
           MOVE SPACES TO CIP-IND-CXE1 IN CIP.
           MOVE ZEROES TO CIP-NUM-CPRO IN CIP.
           MOVE SPACES TO CIP-CIC-ORIG IN CIP.
           MOVE SPACES TO CIP-COD-TIPO IN CIP.
           MOVE ZEROES TO CIP-NUM-PTRM IN CIP.
           MOVE ZEROES TO CIP-FEC-FORI IN CIP.
           MOVE ZEROES TO CIP-SGV-TOPE IN CIP.
           MOVE ZEROES TO CIP-SGV-TACU IN CIP.
           MOVE ZEROES TO CIP-NUM-FOLI IN CIP.
           MOVE SPACES TO CIP-GLS-NORM IN CIP.
           MOVE ZEROES TO CIP-NUM-PDES IN CIP.
           MOVE ZEROES TO CIP-SGV-VDES IN CIP.
           MOVE ZEROES TO CIP-SGV-VTOT IN CIP.
      *    MOVE SPACES TO CIP-COD-VCOC IN CIP.
           MOVE ZEROES TO CIP-FEC-TTRI IN CIP.
           MOVE SPACES TO CIP-COD-ITRU IN CIP.
           MOVE SPACES TO CIP-COD-TDOC IN CIP.
           MOVE SPACES TO CIP-MSC-TIMP IN CIP.
           MOVE ZEROES TO CIP-VAL-CAMO IN CIP.
           MOVE ZEROES TO CIP-VAL-INTN IN CIP.
           MOVE ZEROES TO CIP-VAL-CPRE IN CIP.
           MOVE ZEROES TO CIP-VAL-CFOG IN CIP.
           MOVE ZEROES TO CIP-VAL-CMOR IN CIP.
           MOVE SPACES TO CIP-GLS-DISP IN CIP.
           MOVE ZEROES TO CIP-VAL-SIMO IN CIP.
           MOVE ZEROES TO CIP-SGV-TINT IN CIP.
       FIN-MOV-VLR-DEFE.
