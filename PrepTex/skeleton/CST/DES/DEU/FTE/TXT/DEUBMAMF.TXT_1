*% IF AMF_IND_TVAL
           MOVE 'TAB' TO MSC-COD-SIST.
           MOVE 'TVAL' TO MSC-COD-TMSC IN MSC.
           MOVE AMF-IND-TVAL IN AMF-FLD TO MSC-COD-CMSC IN MSC.
           PERFORM VAL-IND.
           IF MSG-COD-MENS NOT = 'COD    NEX'
               MOVE MSC-GLS-DESC IN MSC TO FRM-GLS-TVAL IN AMF-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-TVAL IN AMF-FLD.
*% END
*% IF AMF_COD_TIOP
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'TIO' TO TAB-COD-TTAB IN TAB.
           MOVE AMF-COD-TIOP IN AMF-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT = 'COD    NEX'
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-TIOP IN AMF-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-TIOP IN AMF-FLD.
*% END
*% IF AMF_COD_OFIC
           MOVE AMF-COD-OFIC IN AMF-FLD TO OFI-COD-OFIC IN OFI.
           PERFORM VAL-OFI.
           IF MSG-COD-MENS NOT = SPACES
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
           IF MSG-COD-MENS NOT = 'COD    NEX'
               MOVE OFI-GLS-DESC IN OFI TO FRM-GLS-OFIC IN AMF-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-OFIC IN AMF-FLD.
*% END
*% IF AMF_COD_OPER
          IF SCR-CCPP = 'ING'
               MOVE OPE-VAL TO OPE-CMND
               MOVE AMF-IND-TVAL IN AMF-FLD TO OPE-TVAL
               MOVE AMF-COD-TIOP IN AMF-FLD TO OPE-TIOP
               MOVE AMF-COD-OFIC IN AMF-FLD TO OPE-OFIC
               PERFORM PRO-OPE. 
*% END
*% IF AMF_VRF_OPER
           IF SCR-CCPP NOT = 'ING'
                   MOVE AMF-COD-OPER IN AMF-FLD TO VRF-NUME
                   MOVE AMF-VRF-OPER IN AMF-FLD TO VRF-DVRF
                   MOVE VRF-SVRF-0 TO VRF-SVRF
                   MOVE VRF-ZERO-S TO VRF-ZERO
                   MOVE 7          TO VRF-LMAX
                   MOVE VRF-VAL TO VRF-CMND
                   PERFORM PRO-VRF
                   IF MSG-COD-MENS = SPACES
                       MOVE VRF-NUME TO AMF-COD-OPER IN AMF-FLD.
*% END
*% IF AMF_COD_VALO
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'VAL' TO TAB-COD-TTAB IN TAB.
           MOVE SPACES TO TAB-COD-CTAB.
           STRING 
               '  ' 
               AMF-COD-VALO IN AMF-FLD DELIMITED BY SIZE
               INTO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT = 'COD    NEX'
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-VALO IN AMF-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-VALO IN AMF-FLD.
*% END
*% IF AMF_COD_AUXI
           MOVE 'KOL' TO TAB-COD-SIST.
           MOVE 'AUX' TO TAB-COD-TTAB IN TAB.
           MOVE AMF-COD-AUXI IN AMF-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT = 'COD    NEX'
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-AUXI IN AMF-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-AUXI IN AMF-FLD.
*% END
*% IF AMF_COD_DEST
           MOVE 'KOL' TO TAB-COD-SIST.
           MOVE 'DES' TO TAB-COD-TTAB IN TAB.
           MOVE AMF-COD-DEST IN AMF-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT = 'COD    NEX'
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-DEST IN AMF-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-DEST IN AMF-FLD.
*% END
*% IF AMF_VAL_LIMI
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE AMF-VAL-LIMI IN AMF-FLD TO NUM-NUME.
           PERFORM VAL-NUM.
           IF MSG-COD-MENS = SPACES
               MOVE NUM-NUME TO AMF-VAL-LIMI IN AMF-FLD.
*% END
*% IF AMF_COD_VLIM
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'VAL' TO TAB-COD-TTAB IN TAB.
           MOVE SPACES TO TAB-COD-CTAB.
           STRING 
               '  ' 
               AMF-COD-VLIM IN AMF-FLD DELIMITED BY SIZE
               INTO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT = 'COD    NEX'
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-VLIM IN AMF-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-VLIM IN AMF-FLD.
*% END
*% IF AMF_IND_TDEU
           MOVE 'DEU' TO MSC-COD-SIST.
           MOVE 'TDEU' TO MSC-COD-TMSC IN MSC.
           MOVE AMF-IND-TDEU IN AMF-FLD TO MSC-COD-CMSC IN MSC.
           PERFORM VAL-IND.
           IF MSG-COD-MENS NOT = 'COD    NEX'
               MOVE MSC-GLS-DESC IN MSC TO FRM-GLS-TDEU IN AMF-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-TDEU IN AMF-FLD.
*% END
