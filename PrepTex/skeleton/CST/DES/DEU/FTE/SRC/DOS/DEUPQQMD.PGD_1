       FIN-FND-KEY.
           IF FIO-STAT = FIO-STAT-NEX
               MOVE 'S' TO SCR-IERR.
       EXT-FND-KEY.
           EXIT.
 
       PUT-SCR SECTION.
       INI-PUT-SCR.
           IF PGM-STAT-DBC-OKS
               MOVE DBC-STP-ITRN IN DBC TO SCR-UMOD.
           PERFORM PUT-ALL-QMD.
       FIN-PUT-SCR.
           EXIT.

       STP-EXIT SECTION.
       INI-STP-EXIT.
           MOVE SCR-PROG TO PGM-PROG.
           MOVE SCR-NTRN TO PGM-NTRN.

           MOVE SCR-PROV TO SCR-PROG.
           MOVE SCR-NTRV TO SCR-NTRN.

           MOVE PGM-PROG TO SCR-PROV
           MOVE PGM-NTRN TO SCR-NTRV.
       FIN-STP-EXIT.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO SYS-NTRN.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
      *PTC-INI
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE QMD-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO QMD-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GKEY SECTION.
       INI-SCR-GKEY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-QMD
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO QMD-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GKEY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-QMD.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-QMD
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-QMD.
      *<<<< SCR_GKEY_VAL_IKY
      *>>>>
           PERFORM VAL-IKY-QMD.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-MKY-QMD.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-AKY-QMD.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-CON-KEY-QMD.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GKEY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-QMD.
       ERR-SCR-GKEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-QMD.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GKEY.
           PERFORM SCR-MSSG.

       SCR-GIKY SECTION.
       INI-SCR-GIKY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-QMD
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO QMD-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GIKY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-QMD.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-IKY-QMD
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-QMD.
           PERFORM VAL-IKY-QMD.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GIKY.
           PERFORM VAL-CON-IKY-QMD.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GIKY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-QMD.
       ERR-SCR-GIKY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-IKY-QMD.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GIKY.
           PERFORM SCR-MSSG.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
      *>>>>
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO QMD-GLS-MEN2
               MOVE SCR-PRMT     TO QMD-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO QMD-FLD.
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO QMD-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE QMD-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO QMD-FLD
      * FLB  7-NOV-1994
               PERFORM CLR-KEY-QMD

               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO QMD-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO QMD-GLS-MENS.
           PERFORM PRO-MKY-QMD.
           PERFORM PRO-AKY-QMD.
           PERFORM UNP-IKY-QMD.
           PERFORM FST-KEY-QMD.
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE QMD-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO QMD-FLD
               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO QMD-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO QMD-GLS-MENS.
           PERFORM UNP-KEY-QMD.
           PERFORM FST-KEY-QMD.
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
           PERFORM PRO-IKY-QMD.
           PERFORM PRO-AKY-QMD.
           PERFORM UNP-MKY-QMD.
           PERFORM UNP-FLD-QMD.
           PERFORM FST-FLD-QMD.
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO QMD-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-QMD.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
      *PTC-INI
           IF SCR-STAT NOT = SCR-STPP
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-QMD
               PERFORM EDT-FLD-QMD
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-QMD
               PERFORM EDT-FLD-QMD
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-QMD.
           PERFORM EDT-FLD-QMD.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

      * PGM-EBR-ALL SECTION.
      * INI-PGM-EBR-ALL.
      *     MOVE 'GNS' TO FIO-SIST.
      *     MOVE FIO-EBR-ALL TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
      * FIN-PGM-EBR-ALL.
      *     EXIT.

       COPY DEUBQQMD.
       COPY GNSBVNUM.
       COPY SGCBFDBC.
       COPY GNSBFMSG.
       COPY GNSBGSYS.
       COPY GNSBGDTC.
      *PTC-INI
      *PTC-SUB
      * ISP  10-MAY-2001 13:46:35 
      * COPY GNSBGFRM.
       COPY GNSBG3CF.
      *PTC-FIN
       COPY GNSBGMSG.
       COPY GNSBEKEY.
       COPY GNSBIABT.
       COPY GNSBFTAB.
       COPY GNSBVIND.
       COPY GNSBVMSC.
       COPY GNSBFMSC.
       COPY GNSBVIDC.
       COPY GNSBPVRF.
      *PTC-INI
       COPY GNSBGQUE.
       COPY GNSBGTSK.
      *PTC-FIN

      *<<<< EOF
       PUT-TAB SECTION.
       INI-PUT-TAB.
           MOVE TSK-TERM-ALF        TO QUE-TERM.
           MOVE MAR-TYPE-PPTPG      TO QUE-TYPE.
           MOVE QUE-DEL             TO QUE-CMND.
           PERFORM GNS-PRO-QUE.                 
                                                
           MOVE TSK-TERM-ALF        TO QUE-TERM.
           MOVE MAR-TYPE-PPTPG      TO QUE-TYPE.
           MOVE QUE-PUT             TO QUE-CMND.
           MOVE 'C' TO QUE-ITEM.                
           MOVE +1 TO QUE-LITM.                 
           PERFORM GNS-PRO-QUE.

           MOVE TSK-TERM-ALF        TO QUE-TERM.                
           MOVE MAR-TYPE-G          TO QUE-TYPE.                
           MOVE QUE-DEL             TO QUE-CMND.                
           PERFORM GNS-PRO-QUE.                                 
                                                                
           MOVE DBC-NUM-ICLI IN QMD-FLD TO MAR-ENT-NUM-ICLI.
           MOVE DBC-VRF-ICLI IN QMD-FLD TO MAR-ENT-VRF-ICLI.

           MOVE MAR-ENTRADA TO QUE-ITEM.                   
           MOVE MAR-LENG-G  TO QUE-LITM.                   
           MOVE QUE-PUT     TO QUE-CMND.                   
           PERFORM GNS-PRO-QUE.                            
                                                           
           MOVE SCR-QIDD TO CCU-QIDD.                      
           MOVE SCR-LIDD TO CCU-LIDD.                      
                  
           MOVE 'LDCPSCCU'      TO SYS-PROG.                            
           MOVE CCU-TCMA        TO SYS-TCMA.                            
           MOVE 'S'             TO CCU-IND-MONI.                        
           MOVE 'M'             TO CCU-IND-MARG.  

      *RRM, HABILIAR CUANDO SE LLEVE A BEEBANK:
      **AFRI-INI 05-MAY-2015 LIMITE DE CREDITO SEGUN ARTICULO 84
      **     MOVE 'LDCPSCCU'      TO SYS-PROG.
      *      MOVE WCC-LDCPSMAR    TO SYS-PROG.
      **AFRI-FIN 05-MAY-2015 LIMITE DE CREDITO SEGUN ARTICULO 84
      *     MOVE CCU-TCMA        TO SYS-TCMA.
      *     MOVE 'S'             TO CCU-IND-MONI.                       
      **AFRI-INI 05-MAY-2015 LIMITE DE CREDITO SEGUN ARTICULO 84
      **     MOVE 'M'             TO CCU-IND-MARG.
      **AFRI-FIN 05-MAY-2015 LIMITE DE CREDITO SEGUN ARTICULO 84
           MOVE CCU-VARI        TO SYS-CMMA.                           
           MOVE SYS-LINK        TO SYS-CMND.                           
           PERFORM GNS-PRO-SYS.                                        
           MOVE SYS-CMMA TO CCU-CMMA.                                  
           IF CCU-NUM-MENS > ZEROES                                    
      *       MOVE '1'          TO WSS-SND-STAT                        
      *       MOVE CCU-GLS-MONI TO WSS-SND-MENS
      *LOP-INI 03-04-2003                                               
      *       MOVE CCU-GLS-MONI TO FRM-GLS-MONI IN QMD-FLD          
      *     ELSE                                                        
      **      MOVE '0'          TO WSS-SND-STAT                        
      **      MOVE SPACES       TO WSS-SND-MENS.                       
      *       MOVE SPACES       TO FRM-GLS-MONI IN QMD-FLD.
              MOVE CCU-GLS-MON1 TO FRM-MENS                             
              MOVE CCU-GLS-MON2 TO FRM-MEN2                             
              MOVE SCR-STAT-CON TO SCR-STAT                             
              MOVE FIO-STAT-NEX TO FIO-STAT                             
              GO TO FIN-PUT-TAB.                                        
      *LOP-INI 03-04-2003
                                                                       

           MOVE 1            TO SCR-NPAN.
           MOVE ZEROES TO WSS-IND-PANT.
           MOVE TSK-TERM-ALF TO SCR-TSK-TERM-ALF.
           PERFORM PUT-TAB-ELM.

      *LOP-INI 03-04-2003                                               
           IF WSS-IND-ERR-MAR = ZEROES                                  
              MOVE 'DEU'           TO MSG-COD-SIST                      
              MOVE 'MARGENES  OK'  TO MSG-COD-MENS                      
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC    TO FRM-GLS-MONI IN QMD-FLD           
              MOVE '00'            TO FIO-STAT                          
           ELSE                                                         
           IF WSS-IND-ERR-MAR = 001                                     
              MOVE WSS-GLS-ERR-MAR TO FRM-GLS-MONI IN QMD-FLD           
              MOVE '00'            TO FIO-STAT                          
           ELSE                                                         
              MOVE 'DEU'           TO MSG-COD-SIST                      
              MOVE 'ERRVALMARGEN'  TO MSG-COD-MENS                      
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC    TO FRM-GLS-MONI IN QMD-FLD           
              MOVE '00'            TO FIO-STAT.                         
      *LOP-FIN 03-04-2003

       FIN-PUT-TAB.
           EXIT.

       PUT-TAB-ELM SECTION.
       INI-PUT-TAB-ELM.
      *LOP-INI 03-04-2003                                               
           MOVE ZEROES       TO WSS-IND-ERR-MAR.                        
      *LOP-INI 03-04-2003 
           MOVE ZEROES       TO FRM-IFLD.
           MULTIPLY WSS-IND-PANT BY WSS-IMAX GIVING QUE-NITM.
           MOVE TSK-TERM-ALF        TO QUE-TERM.                       
           MOVE MAR-TYPE-P          TO QUE-TYPE.
       CIC-GET-QUE-QMD.                                                
           ADD 1                    TO QUE-NITM.                       
           MOVE MAR-LENG-P          TO QUE-LITM.                       
           MOVE QUE-GET             TO QUE-CMND.                       
           PERFORM GNS-PRO-QUE.                                        
           IF QUE-STAT = QUE-STAT-OKS                                  
                MOVE QUE-ITEM TO MAR-MENSAJE
      *LOP-INI 03-04-2003                                               
                MOVE QUE-ITEM TO WSS-RSP-QMD-CERR                       
                PERFORM SET-ERR-MAR                                     
      *LOP-FIN 03-04-2003
                ADD 1         TO FRM-IFLD
                PERFORM PRO-RESP-018                                   
                IF FRM-IFLD < WSS-IMAX
                     GO TO CIC-GET-QUE-QMD.                            

       FIN-PUT-TAB-ELM.
           EXIT.

      *LOP-INI 03-04-2003                                               
       SET-ERR-MAR SECTION.                                             
       INI-SET-ERR-MAR.                                                 
           IF WSS-RSP-QMD-CERR-2 = 'S'                                  
              MOVE MAR-MEN-GLS-MENS TO WSS-GLS-ERR-MAR                  
              ADD 1                 TO WSS-IND-ERR-MAR.                 
       FIN-SET-ERR-MAR.                                                 
           EXIT.                                                        
      *LOP-INI 03-04-2003 

       PRO-RESP-018 SECTION.
       INI-PRO-RESP-018.
           MOVE MAR-MEN-NUM-ICLI TO FRM-NUM-ICLI IN QMD-FLD(FRM-IFLD).
           MOVE MAR-MEN-VRF-ICLI TO FRM-VRF-ICLI IN QMD-FLD(FRM-IFLD).
           MOVE MAR-MEN-GLS-NOMC TO FRM-GLS-NOMC IN QMD-FLD(FRM-IFLD).
      *LOP-INI 24-ENE-2003                                              
      *    IF MAR-MEN-GLS-MENS NOT > SPACES                             
      *        MOVE SPACES TO FRM-GLS-MSJE IN QMD-FLD(FRM-IFLD)         
      *    ELSE                                                         
      *        MOVE 'DEU' TO MSG-COD-SIST                               
      *        MOVE MAR-MEN-GLS-MENS TO MSG-COD-MENS                    
      *        PERFORM GET-MSG                                          
      *        MOVE MSG-GLS-DESC TO FRM-GLS-MSJE IN QMD-FLD(FRM-IFLD).  
           MOVE MAR-MEN-GLS-MENS TO FRM-GLS-MSJE IN QMD-FLD(FRM-IFLD).  
      *LOP-FIN 24-ENE-2003 
           MOVE SPACES TO MSG-COD-MENS.
       FIN-PRO-RESP-018.
           EXIT.

       BUS-CLI-NMB SECTION.
       INI-BUS-CLI-NMB.

      *NOM-CLI Busca Cliente por medio del Nombre
           MOVE DBC-GLS-NOMC IN QMD-FLD TO STR-GLS-GSTR
           PERFORM STR-UNS
           MOVE STR-GLS-FUNS IN STR-VARI(1) TO DBC-GLS-APAT IN DBC
           MOVE STR-GLS-FUNS IN STR-VARI(2) TO DBC-GLS-AMAT IN DBC
           MOVE STR-GLS-FUNS IN STR-VARI(3) TO DBC-GLS-NOMB IN DBC.
           MOVE 'DBC-GLS-NOMC'  TO FIO-AKEY.
           MOVE FIO-GET-NLS-ALT TO FIO-CMND.
           PERFORM GET-CLI.
           IF MSG-COD-MENS > SPACES
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FIO-STAT-NEX TO FIO-STAT
           ELSE
               MOVE '00' TO PGM-STAT-DBC
      *         MOVE ADR-DBC-REQA TO SCR-DBC-REQA
               MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
               PERFORM CPT-BLKS
               MOVE CPT-STRN            TO DBC-GLS-NOMC IN QMD-FLD
               MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN QMD-FLD
               MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN QMD-FLD
               MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN QMD-FLD
               MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN QMD-FLD
               MOVE DBC-CAI-ICLI IN DBC TO DBC-CAI-ICLI IN QMD-FLD
               MOVE DBC-IIC-ICLI IN DBC TO DBC-IIC-ICLI IN QMD-FLD
               IF SCR-CCPP = 'ING'
      *             PERFORM PUT-FRM-QMD
                   MOVE '00' TO FIO-STAT.

       FIN-BUS-CLI-NMB.
           EXIT.

       BUS-CLI-IDC SECTION.
       INI-BUS-CLI-IDC.
      *IDC-CLI Busca Cliente por medio de IdC
           MOVE DBC-NUM-ICLI IN QMD-FLD TO DBC-NUM-ICLI IN DBC.
           MOVE DBC-IND-ICLI IN QMD-FLD TO DBC-IND-ICLI IN DBC.
           MOVE DBC-GLS-ICLI IN QMD-FLD TO DBC-GLS-ICLI IN DBC.
           MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY.
           MOVE FIO-GET-KEY-ALT TO FIO-CMND.
           PERFORM GET-CLI.
           IF MSG-COD-MENS > SPACES
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FIO-STAT-NEX TO FIO-STAT
           ELSE
               MOVE '00' TO PGM-STAT-DBC
      *         MOVE ADR-DBC-REQA TO SCR-DBC-REQA
               MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
               PERFORM CPT-BLKS
               MOVE CPT-STRN            TO DBC-GLS-NOMC IN QMD-FLD
               MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN QMD-FLD
               MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN QMD-FLD
               MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN QMD-FLD
               MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN QMD-FLD
               MOVE DBC-CAI-ICLI IN DBC TO DBC-CAI-ICLI IN QMD-FLD
               MOVE DBC-IIC-ICLI IN DBC TO DBC-IIC-ICLI IN QMD-FLD
               IF SCR-CCPP = 'ING'
      *             PERFORM PUT-FRM-QMD
                   MOVE '00' TO FIO-STAT.
       FIN-BUS-CLI-IDC.
           EXIT.


       SCRL-TAB-QMD SECTION.
       INI-SCRL-TAB-QMD.
           MOVE SPACES TO MSG-COD-MENS.
           IF FRM-FFLD = FRM-FFLD-PF6
               PERFORM PUT-TAB-ASC
           ELSE
               PERFORM PUT-TAB-DSC.
           IF MSG-COD-MENS > SPACES
               MOVE 'DEU' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-QMD.
           EXIT.

       PUT-TAB-ASC SECTION.
       INI-PUT-TAB-ASC.
           MOVE SPACES TO MSG-COD-MENS.
           MOVE SCR-TSK-TERM-ALF TO TSK-TERM-ALF.
           MOVE SCR-NPAN TO WSS-IND-PANT.
           PERFORM PUT-TAB-ELM.

           IF FRM-IFLD = 0
               MOVE 'QMDNONXT' TO MSG-COD-MENS
               GO TO FIN-PUT-TAB-ASC.

           ADD 1 TO SCR-NPAN.

           IF FRM-IFLD < WSS-IMAX
               ADD 1 FRM-IFLD GIVING WSS-I
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1
                                 UNTIL WSS-I > WSS-IMAX.

           IF FRM-IFLD = WSS-IMAX
               ADD  1                TO QUE-NITM
               MOVE SCR-TSK-TERM-ALF TO QUE-TERM
               MOVE MAR-TYPE-P       TO QUE-TYPE
               MOVE MAR-LENG-P       TO QUE-LITM
               MOVE QUE-GET  TO QUE-CMND
               PERFORM GNS-PRO-QUE
               IF QUE-STAT = QUE-STAT-OKS
                   MOVE 'MASQMD' TO MSG-COD-MENS.
       FIN-PUT-TAB-ASC.
           EXIT.

       PUT-TAB-DSC SECTION.
       INI-PUT-TAB-DSC.
           MOVE SPACES TO MSG-COD-MENS.
           MOVE SCR-TSK-TERM-ALF TO TSK-TERM-ALF.
           IF SCR-NPAN = 1
               MOVE 'QMDNOPRV' TO MSG-COD-MENS
               GO TO FIN-PUT-TAB-DSC.

           SUBTRACT 2 FROM SCR-NPAN GIVING WSS-IND-PANT.
            
           PERFORM PUT-TAB-ELM.
           SUBTRACT 1 FROM SCR-NPAN.

           IF SCR-NPAN = 1
               MOVE 'QMDNOPRV' TO MSG-COD-MENS
               GO TO FIN-PUT-TAB-DSC.
           
       FIN-PUT-TAB-DSC.
           EXIT.


       CLR-FRM SECTION.
       INI-CLR-FRM.
           MOVE ZEROES TO FRM-NUM-ICLI IN QMD-FLD(WSS-I).
           MOVE SPACES TO FRM-VRF-ICLI IN QMD-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-NOMC IN QMD-FLD(WSS-I).
           MOVE SPACES TO FRM-GLS-MSJE IN QMD-FLD(WSS-I).
       FIN-CLR-FRM.
           EXIT.


