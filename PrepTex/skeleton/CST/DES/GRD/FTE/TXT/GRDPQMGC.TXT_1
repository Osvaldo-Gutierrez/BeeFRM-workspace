*%! Codigo adicional para archivo GRDPQMGC
*% IF SETTAG
*% FNCPQ = "COBRJUDICIAL"
*% VSNPQ = "05.11.22.1"
*% END
*% LARGO_MI_COMMAREA = 20
*% PGM_PTC = TRUE
*% IF MI_COMMAREA
      *<<<< MI_COMMAREA
           03 CMA-VARI.
              05 SCR-IMAX      COMP                   PIC S9(04).
              05 SCR-I         COMP                   PIC S9(04).
      *>>>>
*% END
*% IF WSS
      *<<<< WSS
       COPY GRDWGMGC.
       COPY GNSWGPES.
    
       01  WSS-VARI.
           03  WSS-I                              PIC 9(02).
           03  WSS-COD-NIVEL                      PIC X(02).            
           03  WSS-TOT        VALUE ZEROES        PIC 9(03).            
           03  WSS-INICIO     VALUE ZEROES        PIC 9(01).            
           03  WSS-CIC-ICLI   VALUE SPACES        PIC X(12).            
      *>>>>
*% END
*% IF FIN_PGM_STAT
      *<<<< FIN_PGM_STAT                                                     
           03 PGM-STAT.                                                 
              05 PGM-STAT-DBC.                                          
                 88 PGM-STAT-DBC-NUL    VALUE '  '.                     
                 07 PGM-STT1-DBC        VALUE '2' PIC X(01).            
                    88 PGM-STAT-DBC-OKS VALUE '0'.                      
                 07 PGM-STT2-DBC        VALUE '3' PIC X(01).            
      *>>>>                                                             
*% END
*% IF INI_FIO
      *<<<< INI_FIO                                                     
           IF SCR-OFRM-SRV                                              
              MOVE TSK-TERM-ALF TO MGC-COD-TERM                         
              MOVE TSK-TERM-ALF TO MGC-COD-TERM-MAS                     
                                                                        
           ELSE                                                         
              MOVE EIBTRMID     TO MGC-COD-TERM                         
              MOVE EIBTRMID     TO MGC-COD-TERM-MAS.                    
      *>>>>                                                             
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC
           IF FRM-FFLD = FRM-FFLD-PF6
                    OR   FRM-FFLD-PF5
               PERFORM VAL-NUM-KEY-MGC
               PERFORM VAL-NUM-FLD-MGC
               PERFORM RST-STAT
               PERFORM FND-SCR-KEY THRU FIN-SCR-KEY
               IF SCR-STAT NOT = SCR-STAT-ABT
                   PERFORM SCR-TRL
               ELSE
                   PERFORM DESSCRTRL
                   GO TO FIN-SCR-ACC.
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
           MOVE SPACES TO FRM-MEN1.
           IF FRM-FFLD = FRM-FFLD-PF5
               MOVE PGM-SNUL TO PGM-STAT-DBC
               SUBTRACT 1 FROM SCR-I GIVING MGC-I
               IF MGC-I NOT > ZEROES
                   MOVE 'MGC DBCPRV' TO MSG-COD-MENS
                   GO TO ERR-FND-KEY
               ELSE
                   GO TO CARGA-MGC
           ELSE
           IF FRM-FFLD = FRM-FFLD-PF6
               MOVE PGM-SNUL TO PGM-STAT-DBC
               ADD 1 SCR-I GIVING MGC-I
               IF MGC-I > SCR-IMAX
                   MOVE 'MGC DBCNXT' TO MSG-COD-MENS
                   GO TO ERR-FND-KEY
               ELSE
                   GO TO CARGA-MGC.
           GO TO CON-FND-KEY.

      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
           IF NOT FIO-STAT-OKS
               GO TO FIN-FND-KEY.
       CON-FND-KEY.
      *
           MOVE ZEROES TO SCR-IMAX.
           MOVE 1 TO SCR-I.
           MOVE '00' TO FIO-STAT.
           PERFORM OBT-DAT-TS.
           MOVE 1 TO SCR-I.
           MOVE SCR-I TO MGC-I.
       CARGA-MGC.
           MOVE SPACES TO MSG-COD-MENS.
           IF NOT SCR-OFRM-SRV
           PERFORM PUT-TAB-MGC.
           MOVE '00' TO FIO-STAT.
           GO TO FIN-FND-KEY.
      *
       ERR-FND-KEY.
           MOVE SCR-STAT-CON TO SCR-STAT.
     
           MOVE 'GRD' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MEN1.
           MOVE '00' TO FIO-STAT.
           GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF PFS_GET_CON OR PFS_GET_ACC
      *<<<< PFS_GET_CON OR PFS_GET_ACC
           IF NOT (FRM-FFLD = FRM-FFLD-PF6
                         OR   FRM-FFLD-PF5)
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBGPES.
       COPY TABBFCAM.

       CAR-MOV-MGC SECTION.
       INI-CAR-MOV-MGC.
       FIN-CAR-MOV-MGC.
           EXIT.

   
       OBT-DAT-TS SECTION.
       INI-OBT-DAT-TS.
           EXEC CICS HANDLE CONDITION QIDERR(CON-OBT-DAT-TS)
                                     ITEMERR(CON-OBT-DAT-TS)
           END-EXEC.

      *     EXEC CICS DELETEQ TS QUEUE(MGC-NAME-QUE)
      *     END-EXEC.
      *<<<<
           MOVE MGC-NOM-TSMGC     TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
      *>>>>
       CON-OBT-DAT-TS.
           MOVE MGC-SI-IMP TO MGC-IMP.
           PERFORM CLR-VEC-TS VARYING MGC-I FROM 1 BY 1 UNTIL
                   MGC-I > MGC-IMAX-MTX.
           MOVE 1 TO MGC-I.
           PERFORM CAR-MOV-MGC.
      
       NXT-OBT-DAT-TS.
           IF MGC-IMP = MGC-SI-IMP
               PERFORM WRT-TS.
       FIN-OBT-DAT-TS.
           EXIT.

       PUT-TAB-MGC SECTION.
       INI-PUT-TAB-MGC.
           PERFORM RED-TS.
           PERFORM LLE-DAT-PNT VARYING MGC-I FROM 1 BY 1
                   UNTIL MGC-I > MGC-IMAX.
       FIN-PUT-TAB-MGC.
           EXIT.

       LLE-DAT-PNT SECTION.
       INI-LLE-DAT-PNT.
           MOVE MGC-GLS-DESC(MGC-I) TO FRM-GLS-DESC IN MGC-FLD(MGC-I).  
           MOVE MGC-NUM-ICLI(MGC-I) TO FRM-NUM-ICLI IN MGC-FLD(MGC-I).  
           MOVE MGC-VRF-ICLI(MGC-I) TO FRM-VRF-ICLI IN MGC-FLD(MGC-I).  
           MOVE MGC-SGV-SVIG(MGC-I) TO FRM-SGV-SVIG IN MGC-FLD(MGC-I).  
           MOVE MGC-SGV-SMOR(MGC-I) TO FRM-SGV-SMOR IN MGC-FLD(MGC-I).  
           MOVE MGC-SGV-SVEN(MGC-I) TO FRM-SGV-SVEN IN MGC-FLD(MGC-I).  
           MOVE MGC-SGV-SCAS(MGC-I) TO FRM-SGV-SCAS IN MGC-FLD(MGC-I).  
       FIN-LLE-DAT-PNT.
           EXIT.

       INC-REG-TS SECTION.
       INI-INC-REG-TS.
            MOVE MGC-SI-IMP TO MGC-IMP.
            IF SCR-OFRM-SRV
              MOVE MGC-SGV-SVIG(MGC-I) TO MGC-SGV-SVIG-EDT              
              MOVE MGC-SGV-SVIG-EDT TO    MGC-SGV-SVIG-ALF(MGC-I)       
              MOVE MGC-SGV-SMOR(MGC-I) TO MGC-SGV-SMOR-EDT              
              MOVE MGC-SGV-SMOR-EDT TO    MGC-SGV-SMOR-ALF(MGC-I)       
              MOVE MGC-SGV-SVEN(MGC-I) TO MGC-SGV-SVEN-EDT              
              MOVE MGC-SGV-SVEN-EDT TO    MGC-SGV-SVEN-ALF(MGC-I)       
              MOVE MGC-SGV-SCAS(MGC-I) TO MGC-SGV-SCAS-EDT              
              MOVE MGC-SGV-SCAS-EDT TO    MGC-SGV-SCAS-ALF(MGC-I).      
           ADD 1 TO MGC-I.
           IF MGC-I > MGC-IMAX
               PERFORM WRT-TS
               PERFORM CLR-VEC-TS VARYING MGC-I FROM 1 BY 1
                                     UNTIL MGC-I > MGC-IMAX
               MOVE 1 TO MGC-I
               MOVE MGC-NO-IMP TO MGC-IMP.
       FIN-INC-REG-TS.
           EXIT.

      *<<<<
      *>>>>

       WRT-TS SECTION.
       INI-WRT-TS.
           ADD 1 TO SCR-IMAX.
      *     EXEC CICS WRITEQ TS QUEUE(MGC-NAME-QUE)
      *                         FROM(MGC-TS)
      *                         LENGTH(MGC-LONG-TS)
      *     END-EXEC.
      *<<<<
           MOVE MGC-NOM-TSMGC  TO QUE-COLA.
           MOVE MGC-TS         TO QUE-ITEM.
           MOVE MGC-LEN-TSMGC  TO QUE-LITM.
           MOVE QUE-PUT        TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
      *>>>>
       FIN-WRT-TS.
           EXIT.

       RED-TS SECTION.
       INI-RED-TS.
           MOVE MGC-I TO SCR-I.
           EXEC CICS HANDLE CONDITION QIDERR(FIN-RED-TS)
                                     ITEMERR(FIN-RED-TS)
           END-EXEC.
      *     EXEC CICS READQ TS QUEUE(DEC-NAME-QUE)
      *                         INTO(DEC-TS)
      *                          LENGTH(DEC-LONG-TS)
      *                         ITEM(DEC-I)
      *     END-EXEC.
      *<<<<
           MOVE MGC-NOM-TSMGC      TO QUE-COLA.
           MOVE MGC-TS             TO QUE-ITEM.
           MOVE MGC-LEN-TSMGC      TO QUE-LITM.
           MOVE MGC-I              TO QUE-NITM.
           MOVE QUE-GET            TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           MOVE QUE-ITEM          TO MGC-TS.
      *>>>>
       FIN-RED-TS.
           EXIT.

       CLR-VEC-TS SECTION.
       INI-CLR-VEC-TS.
           MOVE SPACES TO MGC-GLS-DESC(MGC-I).                          
           MOVE ZEROES TO MGC-NUM-ICLI(MGC-I).                          
           MOVE SPACES TO MGC-VRF-ICLI(MGC-I).                          
           MOVE ZEROES TO MGC-SGV-SVIG(MGC-I).                          
           MOVE ZEROES TO MGC-SGV-SMOR(MGC-I).                          
           MOVE ZEROES TO MGC-SGV-SVEN(MGC-I).                          
           MOVE ZEROES TO MGC-SGV-SCAS(MGC-I).                          
       FIN-CLR-VEC-TS.
           EXIT.

       RST-STAT SECTION.
       INI-RST-STAT.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
       FIN-RST-STAT.
           EXIT.

      *>>>>
*% END
