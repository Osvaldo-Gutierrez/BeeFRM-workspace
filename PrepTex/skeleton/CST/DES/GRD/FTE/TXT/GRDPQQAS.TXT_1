*%! Codigo adicional para archivo GRDPQQAS
*% IF SETTAG
*% FNCPQ = "COBRJUDICIAL"
*% VSNPQ = "05.11.22.1"
*% END
*% LARGO_MI_QASMAREA = 20
*% PGM_PTC = TRUE
*% IF MI_COMMAREA
      *<<<< MI_COMMAREA
           03 CMA-VARI.
              05 SCR-IMAX      COMP                   PIC S9(04).
              05 SCR-I         COMP                   PIC S9(04).
      *>>>>
*% END
*% IF WSS
      *<<<< WSS
       COPY GRDWGQAS.
       COPY GNSWGPES.
       COPY TABBRCAM.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CAM-REQA==.
    
       01  WSS-VARI.
           03  WSS-I                              PIC 9(02).
      *>>>>
*% END
*% IF INI_FIO
      *<<<< INI_FIO                                                     
           IF SCR-OFRM-SRV                                              
              MOVE TSK-TERM-ALF TO COM-COD-TERM                         
              MOVE TSK-TERM-ALF TO COM-COD-TERM-MAS                     
                                                                        
           ELSE                                                         
              MOVE EIBTRMID     TO COM-COD-TERM                         
              MOVE EIBTRMID     TO COM-COD-TERM-MAS.                    
      *>>>>                                                             
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC
           IF FRM-FFLD = FRM-FFLD-PF6
                    OR   FRM-FFLD-PF5
               PERFORM VAL-NUM-KEY-QAS
               PERFORM VAL-NUM-FLD-QAS
               PERFORM RST-STAT
               PERFORM FND-SCR-KEY THRU FIN-SCR-KEY
               IF SCR-STAT NOT = SCR-STAT-ABT
                   PERFORM SCR-TRL
               ELSE
                   PERFORM DESSCRTRL
                   GO TO FIN-SCR-ACC.
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
           MOVE SPACES TO FRM-MEN1.
           IF FRM-FFLD = FRM-FFLD-PF5
               MOVE PGM-SNUL TO PGM-STAT-ASI
               SUBTRACT 1 FROM SCR-I GIVING QAS-I
               IF QAS-I NOT > ZEROES
                   MOVE 'QAS ASIPRV' TO MSG-COD-MENS
                   GO TO ERR-FND-KEY
               ELSE
                   GO TO CARGA-QAS
           ELSE
           IF FRM-FFLD = FRM-FFLD-PF6
               MOVE PGM-SNUL TO PGM-STAT-ASI
               ADD 1 SCR-I GIVING QAS-I
               IF QAS-I > SCR-IMAX
                   MOVE 'QAS ASINXT' TO MSG-COD-MENS
                   GO TO ERR-FND-KEY
               ELSE
                   GO TO CARGA-QAS.
           GO TO CON-FND-KEY.

      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
           IF NOT FIO-STAT-OKS
               GO TO FIN-FND-KEY.
       CON-FND-KEY.
      *
           MOVE ZEROES TO SCR-IMAX.
           MOVE 1 TO SCR-I.
           MOVE '00' TO FIO-STAT.
           PERFORM OBT-DAT-TS.
           MOVE 1 TO SCR-I.
           MOVE SCR-I TO QAS-I.
       CARGA-QAS.
           MOVE SPACES TO MSG-COD-MENS.
           IF NOT SCR-OFRM-SRV
           PERFORM PUT-TAB-QAS.
           MOVE '00' TO FIO-STAT.
           GO TO FIN-FND-KEY.
      *
       ERR-FND-KEY.
           MOVE SCR-STAT-CON TO SCR-STAT.
     
           MOVE 'GRD' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MEN1.
           MOVE '00' TO FIO-STAT.
           GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF PFS_GET_CON OR PFS_GET_ACC
      *<<<< PFS_GET_CON OR PFS_GET_ACC
           IF NOT (FRM-FFLD = FRM-FFLD-PF6
                         OR   FRM-FFLD-PF5)
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBGPES.
       COPY TABBFCAM.

       CAR-MOV-QAS SECTION.
       INI-CAR-MOV-QAS.
       FIN-CAR-MOV-QAS.
           EXIT.

   
       OBT-DAT-TS SECTION.
       INI-OBT-DAT-TS.
           EXEC CICS HANDLE CONDITION QIDERR(CON-OBT-DAT-TS)
                                     ITEMERR(CON-OBT-DAT-TS)
           END-EXEC.

      *     EXEC CICS DELETEQ TS QUEUE(QAS-NAME-QUE)
      *     END-EXEC.
      *<<<<
           MOVE QAS-NOM-TSQAS     TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
      *>>>>
       CON-OBT-DAT-TS.
           MOVE QAS-SI-IMP TO QAS-IMP.
           PERFORM CLR-VEC-TS VARYING QAS-I FROM 1 BY 1 UNTIL
                   QAS-I > QAS-IMAX-MTX.
           MOVE 1 TO QAS-I.
           PERFORM CAR-MOV-QAS.
      
       NXT-OBT-DAT-TS.
           IF QAS-IMP = QAS-SI-IMP
               PERFORM WRT-TS.
       FIN-OBT-DAT-TS.
           EXIT.

       PUT-TAB-QAS SECTION.
       INI-PUT-TAB-QAS.
           PERFORM RED-TS.
           PERFORM LLE-DAT-PNT VARYING QAS-I FROM 1 BY 1
                   UNTIL QAS-I > QAS-IMAX.
       FIN-PUT-TAB-QAS.
           EXIT.

       LLE-DAT-PNT SECTION.
       INI-LLE-DAT-PNT.
           MOVE QAS-CIC-IREC(QAS-I) TO FRM-CIC-IREC IN QAS-FLD(QAS-I).
           MOVE QAS-COD-TARE(QAS-I) TO FRM-COD-TARE IN QAS-FLD(QAS-I).
           MOVE QAS-NUM-ICLI(QAS-I) TO FRM-NUM-ICLI IN QAS-FLD(QAS-I).
           MOVE QAS-VRF-ICLI(QAS-I) TO FRM-VRF-ICLI IN QAS-FLD(QAS-I).
           MOVE QAS-NUM-CLIE(QAS-I) TO FRM-NUM-CLIE IN QAS-FLD(QAS-I).
           MOVE QAS-COD-ESTA(QAS-I) TO FRM-COD-ESTA IN QAS-FLD(QAS-I).
           MOVE QAS-COD-PROD(QAS-I) TO FRM-COD-PROD IN QAS-FLD(QAS-I).
           MOVE QAS-COD-OPER(QAS-I) TO FRM-COD-OPER IN QAS-FLD(QAS-I).
           MOVE QAS-GLS-NOMB(QAS-I) TO FRM-GLS-NOMB IN QAS-FLD(QAS-I).
           MOVE QAS-FEC-FPRO(QAS-I) TO FRM-FEC-FPRO IN QAS-FLD(QAS-I).
           MOVE QAS-FEC-FTER(QAS-I) TO FRM-FEC-FTER IN QAS-FLD(QAS-I).
           MOVE QAS-NUM-DGES(QAS-I) TO FRM-NUM-DGES IN QAS-FLD(QAS-I).
           MOVE QAS-NUM-DMOR(QAS-I) TO FRM-NUM-DMOR IN QAS-FLD(QAS-I).
           MOVE QAS-COD-LUGA(QAS-I) TO FRM-COD-LUGA IN QAS-FLD(QAS-I).
           MOVE QAS-DEU-TOTA(QAS-I) TO FRM-DEU-TOTA IN QAS-FLD(QAS-I).
           MOVE QAS-MTO-PROV(QAS-I) TO FRM-MTO-PROV IN QAS-FLD(QAS-I).
           MOVE QAS-MTO-IMPO(QAS-I) TO FRM-MTO-IMPO IN QAS-FLD(QAS-I).
       FIN-LLE-DAT-PNT.
           EXIT.

       INC-REG-TS SECTION.
       INI-INC-REG-TS.
            MOVE QAS-SI-IMP TO QAS-IMP.
            IF SCR-OFRM-SRV
              MOVE QAS-DEU-TOTA(QAS-I) TO QAS-VAL-CPTO-EDT
              MOVE QAS-VAL-CPTO-EDT TO QAS-DEU-TOTA-ALF(QAS-I)
              MOVE QAS-MTO-PROV(QAS-I) TO QAS-VAL-CPTO-EDT
              MOVE QAS-VAL-CPTO-EDT TO QAS-MTO-PROV-ALF(QAS-I)
              MOVE QAS-MTO-IMPO(QAS-I) TO QAS-VAL-CPTO-EDT
              MOVE QAS-VAL-CPTO-EDT TO QAS-MTO-IMPO-ALF(QAS-I)
           ADD 1 TO QAS-I.
           IF QAS-I > QAS-IMAX
               PERFORM WRT-TS
               PERFORM CLR-VEC-TS VARYING QAS-I FROM 1 BY 1
                                     UNTIL QAS-I > QAS-IMAX
               MOVE 1 TO QAS-I
               MOVE QAS-NO-IMP TO QAS-IMP.
       FIN-INC-REG-TS.
           EXIT.

      *<<<<
      *>>>>

       WRT-TS SECTION.
       INI-WRT-TS.
           ADD 1 TO SCR-IMAX.
      *     EXEC CICS WRITEQ TS QUEUE(QAS-NAME-QUE)
      *                         FROM(QAS-TS)
      *                         LENGTH(QAS-LONG-TS)
      *     END-EXEC.
      *<<<<
           MOVE QAS-NOM-TSQAS  TO QUE-COLA.
           MOVE QAS-TS         TO QUE-ITEM.
           MOVE QAS-LEN-TSQAS  TO QUE-LITM.
           MOVE QUE-PUT        TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
      *>>>>
       FIN-WRT-TS.
           EXIT.

       RED-TS SECTION.
       INI-RED-TS.
           MOVE QAS-I TO SCR-I.
           EXEC CICS HANDLE CONDITION QIDERR(FIN-RED-TS)
                                     ITEMERR(FIN-RED-TS)
           END-EXEC.
      *     EXEC CICS READQ TS QUEUE(DEC-NAME-QUE)
      *                         INTO(DEC-TS)
      *                          LENGTH(DEC-LONG-TS)
      *                         ITEM(DEC-I)
      *     END-EXEC.
      *<<<<
           MOVE EIBTRMID           TO QAS-TERM.
           MOVE QAS-NOM-TSQAS      TO QUE-COLA.
           MOVE QAS-TS             TO QUE-ITEM.
           MOVE QAS-LEN-TSQAS      TO QUE-LITM.
           MOVE QAS-I              TO QUE-NITM.
           MOVE QUE-GET            TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           MOVE QUE-ITEM          TO QAS-TS.
      *>>>>
       FIN-RED-TS.
           EXIT.

       CLR-VEC-TS SECTION.
       INI-CLR-VEC-TS.
           MOVE SPACES TO QAS-CIC-IREC(QAS-I).
           MOVE SPACES TO QAS-COD-TARE(QAS-I).
           MOVE ZEROES TO QAS-NUM-ICLI(QAS-I).
           MOVE SPACES TO QAS-VRF-ICLI(QAS-I).
           MOVE ZEROES TO QAS-NUM-CLIE(QAS-I).
           MOVE SPACES TO QAS-COD-ESTA(QAS-I).
           MOVE SPACES TO QAS-COD-PROD(QAS-I).
           MOVE SPACES TO QAS-COD-OPER(QAS-I).
           MOVE SPACES TO QAS-GLS-NOMB(QAS-I).
           MOVE ZEROES TO QAS-FEC-FPRO(QAS-I).
           MOVE ZEROES TO QAS-FEC-FTER(QAS-I).
           MOVE ZEROES TO QAS-NUM-DGES(QAS-I).
           MOVE ZEROES TO QAS-NUM-DMOR(QAS-I).
           MOVE SPACES TO QAS-COD-LUGA(QAS-I).
           MOVE ZEROES TO QAS-DEU-TOTA(QAS-I).
           MOVE ZEROES TO QAS-MTO-PROV(QAS-I).
           MOVE ZEROES TO QAS-MTO-IMPO(QAS-I).
       FIN-CLR-VEC-TS.
           EXIT.

       RST-STAT SECTION.
       INI-RST-STAT.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
       FIN-RST-STAT.
           EXIT.

      *>>>>
*% END
