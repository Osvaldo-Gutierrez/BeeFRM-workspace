*%! Codigo adicional para archivo GRDPQQDO
*% IF SETTAG
*% FNCPQ = "COBRJUDICIAL"
*% VSNPQ = "05.11.22.1"
*% END
*% LARGO_MI_COMMAREA = 20
*% PGM_PTC = TRUE
*% IF MI_QDOMAREA
      *<<<< MI_COMMAREA
           03 CMA-VARI.
              05 SCR-IMAX      COMP                   PIC S9(04).
              05 SCR-I         COMP                   PIC S9(04).
      *>>>>
*% END
*% IF WSS
      *<<<< WSS
       COPY GRDWGQDO.
       COPY GNSBRMSC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSC-REQA==.
       COPY GNSWGPES.
       COPY TABBRCAM.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CAM-REQA==.
    
       01  WSS-VARI.
           03  WSS-I                              PIC 9(02).
      *>>>>
*% END
*% IF INI_FIO
      *<<<< INI_FIO                                                     
           IF SCR-OFRM-SRV                                              
              MOVE TSK-TERM-ALF TO QDO-COD-TERM                         
              MOVE TSK-TERM-ALF TO QDO-COD-TERM-MAS                     
                                                                        
           ELSE                                                         
              MOVE EIBTRMID     TO QDO-COD-TERM                         
              MOVE EIBTRMID     TO QDO-COD-TERM-MAS.                    
      *>>>>                                                             
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC
           IF FRM-FFLD = FRM-FFLD-PF6
                    OR   FRM-FFLD-PF5
               PERFORM VAL-NUM-KEY-QDO
               PERFORM VAL-NUM-FLD-QDO
               PERFORM RST-STAT
               PERFORM FND-SCR-KEY THRU FIN-SCR-KEY
               IF SCR-STAT NOT = SCR-STAT-ABT
                   PERFORM SCR-TRL
               ELSE
                   PERFORM DESSCRTRL
                   GO TO FIN-SCR-ACC.
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
           MOVE SPACES TO FRM-MEN1.
           IF FRM-FFLD = FRM-FFLD-PF5
               MOVE PGM-SNUL TO PGM-STAT-RRO
               SUBTRACT 1 FROM SCR-I GIVING QDO-I
               IF QDO-I NOT > ZEROES
                   MOVE 'QDO RROPRV' TO MSG-COD-MENS
                   GO TO ERR-FND-KEY
               ELSE
                   GO TO CARGA-QDO
           ELSE
           IF FRM-FFLD = FRM-FFLD-PF6
               MOVE PGM-SNUL TO PGM-STAT-RRO
               ADD 1 SCR-I GIVING QDO-I
               IF QDO-I > SCR-IMAX
                   MOVE 'QDO RRONXT' TO MSG-COD-MENS
                   GO TO ERR-FND-KEY
               ELSE
                   GO TO CARGA-QDO.
           GO TO CON-FND-KEY.

      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
           IF NOT FIO-STAT-OKS
               GO TO FIN-FND-KEY.
       CON-FND-KEY.
      *
           MOVE ZEROES TO SCR-IMAX.
           MOVE 1 TO SCR-I.
           MOVE '00' TO FIO-STAT.
           PERFORM OBT-DAT-TS.
           MOVE 1 TO SCR-I.
           MOVE SCR-I TO QDO-I.
       CARGA-QDO.
           MOVE SPACES TO MSG-COD-MENS.
           IF NOT SCR-OFRM-SRV
           PERFORM PUT-TAB-QDO.
           MOVE '00' TO FIO-STAT.
           GO TO FIN-FND-KEY.
      *
       ERR-FND-KEY.
           MOVE SCR-STAT-CON TO SCR-STAT.
     
           MOVE 'GRD' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MEN1.
           MOVE '00' TO FIO-STAT.
           GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF PFS_GET_CON OR PFS_GET_ACC
      *<<<< PFS_GET_CON OR PFS_GET_ACC
           IF NOT (FRM-FFLD = FRM-FFLD-PF6
                         OR   FRM-FFLD-PF5)
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBFMSC.
       COPY GNSBGPES.
       COPY TABBFCAM.

       CAR-MOV-QDO SECTION.
       INI-CAR-MOV-QDO.
       FIN-CAR-MOV-QDO.
           EXIT.

   
       OBT-DAT-TS SECTION.
       INI-OBT-DAT-TS.
           EXEC CICS HANDLE CONDITION QIDERR(CON-OBT-DAT-TS)
                                     ITEMERR(CON-OBT-DAT-TS)
           END-EXEC.

      *     EXEC CICS DELETEQ TS QUEUE(QDO-NAME-QUE)
      *     END-EXEC.
      *<<<<
           MOVE QDO-NOM-TSQDO     TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
      *>>>>
       CON-OBT-DAT-TS.
           MOVE QDO-SI-IMP TO QDO-IMP.
           PERFORM CLR-VEC-TS VARYING QDO-I FROM 1 BY 1 UNTIL
                   QDO-I > QDO-IMAX-MTX.
           MOVE 1 TO QDO-I.
           PERFORM CAR-MOV-QDO.
      
       NXT-OBT-DAT-TS.
           IF QDO-IMP = QDO-SI-IMP
               PERFORM WRT-TS.
       FIN-OBT-DAT-TS.
           EXIT.

       PUT-TAB-QDO SECTION.
       INI-PUT-TAB-QDO.
           PERFORM RED-TS.
           PERFORM LLE-DAT-PNT VARYING QDO-I FROM 1 BY 1
                   UNTIL QDO-I > QDO-IMAX.
       FIN-PUT-TAB-QDO.
           EXIT.

       LLE-DAT-PNT SECTION.
       INI-LLE-DAT-PNT.
           MOVE QDO-COD-IDEN(QDO-I) TO FRM-COD-IDEN IN QDO-FLD(QDO-I).
           MOVE QDO-IND-CONT(QDO-I) TO FRM-IND-CONT IN QDO-FLD(QDO-I).
           MOVE QDO-GLS-TEXT(QDO-I) TO FRM-GLS-TEXT IN QDO-FLD(QDO-I).
       FIN-LLE-DAT-PNT.
           EXIT.

       INC-REG-TS SECTION.
       INI-INC-REG-TS.
            MOVE QDO-SI-IMP TO QDO-IMP.
           ADD 1 TO QDO-I.
           IF QDO-I > QDO-IMAX
               PERFORM WRT-TS
               PERFORM CLR-VEC-TS VARYING QDO-I FROM 1 BY 1
                                     UNTIL QDO-I > QDO-IMAX
               MOVE 1 TO QDO-I
               MOVE QDO-NO-IMP TO QDO-IMP.
       FIN-INC-REG-TS.
           EXIT.

      *<<<<
      *>>>>

       WRT-TS SECTION.
       INI-WRT-TS.
           ADD 1 TO SCR-IMAX.
      *     EXEC CICS WRITEQ TS QUEUE(QDO-NAME-QUE)
      *                         FROM(QDO-TS)
      *                         LENGTH(QDO-LONG-TS)
      *     END-EXEC.
      *<<<<
           MOVE QDO-NOM-TSQDO  TO QUE-COLA.
           MOVE QDO-TS         TO QUE-ITEM.
           MOVE QDO-LEN-TSQDO  TO QUE-LITM.
           MOVE QUE-PUT        TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
      *>>>>
       FIN-WRT-TS.
           EXIT.

       RED-TS SECTION.
       INI-RED-TS.
           MOVE QDO-I TO SCR-I.
           EXEC CICS HANDLE CONDITION QIDERR(FIN-RED-TS)
                                     ITEMERR(FIN-RED-TS)
           END-EXEC.
      *     EXEC CICS READQ TS QUEUE(DEC-NAME-QUE)
      *                         INTO(DEC-TS)
      *                          LENGTH(DEC-LONG-TS)
      *                         ITEM(DEC-I)
      *     END-EXEC.
      *<<<<
           MOVE QDO-NOM-TSQDO      TO QUE-COLA.
           MOVE QDO-TS             TO QUE-ITEM.
           MOVE QDO-LEN-TSQDO      TO QUE-LITM.
           MOVE QDO-I              TO QUE-NITM.
           MOVE QUE-GET            TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           MOVE QUE-ITEM          TO QDO-TS.
      *>>>>
       FIN-RED-TS.
           EXIT.

       CLR-VEC-TS SECTION.
       INI-CLR-VEC-TS.
           MOVE SPACES TO QDO-COD-IDEN(QDO-I). 
           MOVE SPACES TO QDO-COD-PROD(QDO-I). 
           MOVE SPACES TO QDO-IND-AVAL(QDO-I). 
       FIN-CLR-VEC-TS.
           EXIT.

       RST-STAT SECTION.
       INI-RST-STAT.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
       FIN-RST-STAT.
           EXIT.

      *>>>>
*% END
