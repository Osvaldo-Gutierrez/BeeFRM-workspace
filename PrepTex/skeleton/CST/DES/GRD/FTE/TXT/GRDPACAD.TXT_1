*% END
*% LARGO_MI_COMMAREA = 12
*% PGM_PTC = TRUE
*% IF MI_COMMAREA
      *<<<< MI_COMMAREA
           03 CMA-VARI.
              05 SCR-CIC-ICLI.
                 07 SCR-CAI-ICLI                   PIC X(04).
                 07 SCR-IIC-ICLI                   PIC X(08). 
      *>>>>
*% END
*% IF WSS                                                         
      *<<<< WSS
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
            MOVE DBC-NUM-ICLI IN CAD-FLD TO DBC-NUM-ICLI IN DBC.
            MOVE DBC-IND-ICLI IN CAD-FLD TO DBC-IND-ICLI IN DBC.
            MOVE DBC-GLS-ICLI IN CAD-FLD TO DBC-GLS-ICLI IN DBC.
            MOVE 'DBC-IDC-ICLI' TO FIO-AKEY.
            MOVE FIO-GET-KEY TO FIO-CMND.
            PERFORM SGC-FIO-DBC.
            IF NOT FIO-STAT-OKS
               GO TO FIN-FND-KEY.
            MOVE DBC-CAI-ICLI IN DBC TO SCR-CAI-ICLI.
            MOVE DBC-IIC-ICLI IN DBC TO SCR-IIC-ICLI.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBGVSM.
      *>>>>
*% END
