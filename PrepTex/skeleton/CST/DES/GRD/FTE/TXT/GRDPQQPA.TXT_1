*%! Codigo adicional para archivo GRDPQQPA
*% IF SETTAG
*% FNCPQ = "COBRJUDICIAL"
*% VSNPQ = "05.11.22.1"
*% END
*% LARGO_MI_COMMAREA = 20
*% PGM_PTC = TRUE
*% IF MI_COMMAREA
      *<<<< MI_COMMAREA
           03 CMA-VARI.
              05 SCR-IMAX      COMP                   PIC S9(04).
              05 SCR-I         COMP                   PIC S9(04).
      *>>>>
*% END
*% IF WSS
      *<<<< WSS
       COPY GRDWGQPA.
       COPY GRDBRREC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-REC-REQA==.
       COPY GNSBRMSC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSC-REQA==.
       COPY GNSWGPES.
       COPY TABBRCAM.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CAM-REQA==.
    
       01  WSS-VARI.
           03  WSS-I                              PIC 9(02).
      *>>>>
*% END
*% IF INI_FIO
      *<<<< INI_FIO                                                     
           IF SCR-OFRM-SRV                                              
              MOVE TSK-TERM-ALF TO QPA-COD-TERM                         
              MOVE TSK-TERM-ALF TO QPA-COD-TERM-MAS                     
                                                                        
           ELSE                                                         
              MOVE EIBTRMID     TO QPA-COD-TERM                         
              MOVE EIBTRMID     TO QPA-COD-TERM-MAS.                    
      *>>>>                                                             
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC
           IF FRM-FFLD = FRM-FFLD-PF6
                    OR   FRM-FFLD-PF5
               PERFORM VAL-NUM-KEY-QPA
               PERFORM VAL-NUM-FLD-QPA
               PERFORM RST-STAT
               PERFORM FND-SCR-KEY THRU FIN-SCR-KEY
               IF SCR-STAT NOT = SCR-STAT-ABT
                   PERFORM SCR-TRL
               ELSE
                   PERFORM DESSCRTRL
                   GO TO FIN-SCR-ACC.
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
           MOVE SPACES TO FRM-MEN1.
           IF FRM-FFLD = FRM-FFLD-PF5
               MOVE PGM-SNUL TO PGM-STAT-PAO
               SUBTRACT 1 FROM SCR-I GIVING QPA-I
               IF QPA-I NOT > ZEROES
                   MOVE 'QPA PAOPRV' TO MSG-COD-MENS
                   GO TO ERR-FND-KEY
               ELSE
                   GO TO CARGA-QPA
           ELSE
           IF FRM-FFLD = FRM-FFLD-PF6
               MOVE PGM-SNUL TO PGM-STAT-PAO
               ADD 1 SCR-I GIVING QPA-I
               IF QPA-I > SCR-IMAX
                   MOVE 'QPA PAONXT' TO MSG-COD-MENS
                   GO TO ERR-FND-KEY
               ELSE
                   GO TO CARGA-QPA.
           GO TO CON-FND-KEY.

      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
           IF NOT FIO-STAT-OKS
               GO TO FIN-FND-KEY.
       CON-FND-KEY.
      *
           MOVE ZEROES TO SCR-IMAX.
           MOVE 1 TO SCR-I.
           MOVE '00' TO FIO-STAT.
           PERFORM OBT-DAT-TS.
           MOVE 1 TO SCR-I.
           MOVE SCR-I TO QPA-I.
       CARGA-QPA.
           MOVE SPACES TO MSG-COD-MENS.
           IF NOT SCR-OFRM-SRV
           PERFORM PUT-TAB-QPA.
           MOVE '00' TO FIO-STAT.
           GO TO FIN-FND-KEY.
      *
       ERR-FND-KEY.
           MOVE SCR-STAT-CON TO SCR-STAT.
     
           MOVE 'GRD' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MEN1.
           MOVE '00' TO FIO-STAT.
           GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF PFS_GET_CON OR PFS_GET_ACC
      *<<<< PFS_GET_CON OR PFS_GET_ACC
           IF NOT (FRM-FFLD = FRM-FFLD-PF6
                         OR   FRM-FFLD-PF5)
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GRDBFREC.
       COPY GNSBFMSC.
       COPY GNSBGPES.
       COPY TABBFCAM.

       CAR-MOV-QPA SECTION.
       INI-CAR-MOV-QPA.
       FIN-CAR-MOV-QPA.
           EXIT.

   
       OBT-DAT-TS SECTION.
       INI-OBT-DAT-TS.
           EXEC CICS HANDLE CONDITION QIDERR(CON-OBT-DAT-TS)
                                     ITEMERR(CON-OBT-DAT-TS)
           END-EXEC.

      *     EXEC CICS DELETEQ TS QUEUE(QPA-NAME-QUE)
      *     END-EXEC.
      *<<<<
           MOVE QPA-NOM-TSQPA     TO QUE-COLA.
           MOVE QUE-DEL           TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
      *>>>>
       CON-OBT-DAT-TS.
           MOVE QPA-SI-IMP TO QPA-IMP.
           PERFORM CLR-VEC-TS VARYING QPA-I FROM 1 BY 1 UNTIL
                   QPA-I > QPA-IMAX-MTX.
           MOVE 1 TO QPA-I.
           PERFORM CAR-MOV-QPA.
      
       NXT-OBT-DAT-TS.
           IF QPA-IMP = QPA-SI-IMP
               PERFORM WRT-TS.
       FIN-OBT-DAT-TS.
           EXIT.

       PUT-TAB-QPA SECTION.
       INI-PUT-TAB-QPA.
           PERFORM RED-TS.
           PERFORM LLE-DAT-PNT VARYING QPA-I FROM 1 BY 1
                   UNTIL QPA-I > QPA-IMAX.
       FIN-PUT-TAB-QPA.
           EXIT.

       LLE-DAT-PNT SECTION.
       INI-LLE-DAT-PNT.
           MOVE QPA-CIC-IREC(QPA-I) TO FRM-CIC-IREC IN QPA-FLD(QPA-I).
           MOVE QPA-CIC-IPAO(QPA-I) TO FRM-CIC-IPAO IN QPA-FLD(QPA-I).
           MOVE QPA-NUM-IDLC(QPA-I) TO FRM-NUM-IDLC IN QPA-FLD(QPA-I).
           MOVE QPA-NUM-IVEN(QPA-I) TO FRM-NUM-IVEN IN QPA-FLD(QPA-I).
           MOVE QPA-NUM-ICAN(QPA-I) TO FRM-NUM-ICAN IN QPA-FLD(QPA-I).
           MOVE QPA-FEC-FVEN(QPA-I) TO FRM-FEC-FVEN IN QPA-FLD(QPA-I).
           MOVE QPA-FEC-FCAN(QPA-I) TO FRM-FEC-FCAN IN QPA-FLD(QPA-I).
           MOVE QPA-SGV-TPAG(QPA-I) TO FRM-SGV-TPAG IN QPA-FLD(QPA-I).
           MOVE QPA-SGV-VMOR(QPA-I) TO FRM-SGV-VMOR IN QPA-FLD(QPA-I).
           MOVE QPA-SGV-INTE(QPA-I) TO FRM-SGV-INTE IN QPA-FLD(QPA-I).
           MOVE QPA-SGV-SFIN(QPA-I) TO FRM-SGV-SFIN IN QPA-FLD(QPA-I).
       FIN-LLE-DAT-PNT.
           EXIT.

       INC-REG-TS SECTION.
       INI-INC-REG-TS.
            MOVE QPA-SI-IMP TO QPA-IMP.
            IF SCR-OFRM-SRV
               MOVE QPA-SGV-TPAG(QPA-I) TO QPA-SGV-TPAG-EDT
               MOVE QPA-SGV-TPAG-EDT TO QPA-SGV-TPAG-ALF(QPA-I)
               MOVE QPA-SGV-VMOR(QPA-I) TO QPA-SGV-VMOR-EDT
               MOVE QPA-SGV-VMOR-EDT TO QPA-SGV-VMOR-ALF(QPA-I)
               MOVE QPA-SGV-INTE(QPA-I) TO QPA-SGV-INTE-EDT
               MOVE QPA-SGV-INTE-EDT TO QPA-SGV-INTE-ALF(QPA-I)
               MOVE QPA-SGV-SFIN(QPA-I) TO QPA-SGV-SFIN-EDT
               MOVE QPA-SGV-SFIN-EDT TO QPA-SGV-SFIN-ALF(QPA-I).
           ADD 1 TO QPA-I.
           IF QPA-I > QPA-IMAX
               PERFORM WRT-TS
               PERFORM CLR-VEC-TS VARYING QPA-I FROM 1 BY 1
                                     UNTIL QPA-I > QPA-IMAX
               MOVE 1 TO QPA-I
               MOVE QPA-NO-IMP TO QPA-IMP.
       FIN-INC-REG-TS.
           EXIT.

      *<<<<
      *>>>>

       WRT-TS SECTION.
       INI-WRT-TS.
           ADD 1 TO SCR-IMAX.
      *     EXEC CICS WRITEQ TS QUEUE(QPA-NAME-QUE)
      *                         FROM(QPA-TS)
      *                         LENGTH(QPA-LONG-TS)
      *     END-EXEC.
      *<<<<
           MOVE QPA-NOM-TSQPA  TO QUE-COLA.
           MOVE QPA-TS         TO QUE-ITEM.
           MOVE QPA-LEN-TSQPA  TO QUE-LITM.
           MOVE QUE-PUT        TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
      *>>>>
       FIN-WRT-TS.
           EXIT.

       RED-TS SECTION.
       INI-RED-TS.
           MOVE QPA-I TO SCR-I.
           EXEC CICS HANDLE CONDITION QIDERR(FIN-RED-TS)
                                     ITEMERR(FIN-RED-TS)
           END-EXEC.
      *     EXEC CICS READQ TS QUEUE(DEC-NAME-QUE)
      *                         INTO(DEC-TS)
      *                          LENGTH(DEC-LONG-TS)
      *                         ITEM(DEC-I)
      *     END-EXEC.
      *<<<<
           MOVE QPA-NOM-TSQPA      TO QUE-COLA.
           MOVE QPA-TS             TO QUE-ITEM.
           MOVE QPA-LEN-TSQPA      TO QUE-LITM.
           MOVE QPA-I              TO QUE-NITM.
           MOVE QUE-GET            TO QUE-CMND.
           PERFORM GNS-PRO-QUE.
           MOVE QUE-ITEM          TO QPA-TS.
      *>>>>
       FIN-RED-TS.
           EXIT.

       CLR-VEC-TS SECTION.
       INI-CLR-VEC-TS.
              MOVE SPACES TO QPA-CIC-IREC(QPA-I).
              MOVE SPACES TO QPA-CIC-IPAO(QPA-I).
              MOVE ZEROES TO QPA-NUM-IDLC(QPA-I).
              MOVE ZEROES TO QPA-NUM-IVEN(QPA-I).
              MOVE ZEROES TO QPA-NUM-ICAN(QPA-I).
              MOVE ZEROES TO QPA-FEC-FVEN(QPA-I).
              MOVE ZEROES TO QPA-FEC-FCAN(QPA-I).
              MOVE ZEROES TO QPA-SGV-TPAG(QPA-I).
              MOVE ZEROES TO QPA-SGV-VMOR(QPA-I).
              MOVE ZEROES TO QPA-SGV-INTE(QPA-I).
              MOVE ZEROES TO QPA-SGV-SFIN(QPA-I).
       FIN-CLR-VEC-TS.
           EXIT.

       RST-STAT SECTION.
       INI-RST-STAT.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
       FIN-RST-STAT.
           EXIT.

      *>>>>
*% END
