*%! Codigo adicional para archivo 
*% IF SETTAG
*% FNCPQ = "COBRJUDICIAL"
*% VSNPQ = "05.11.28.1"
*% FNCPU = "COBRJUDICIAL"
*% VSNPU = "05.11.28.1"
*% END
*% LARGO_MI_COMMAREA = 2750
*% PGM_PTC = TRUE
*% IF MI_COMMAREA
      *<<<< MI_COMMAREA
           03 CMA-VARI.
              05 SCR-CIC-ICLI.
                 07 SCR-CAI-ICLI                   PIC X(04).
                 07 SCR-IIC-ICLI                   PIC X(08). 
      *>>>>
*% END
*% IF WSS                                                         
      *<<<< WSS
       COPY GNSWGCPT.                                                   
       COPY GNSWGSTR.                                                   
       COPY SGCBRDBC.                                                   
       COPY GRDBRREC.                                                   
       COPY GRDBRRRO.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-REC-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RRO-REQA==.     
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
           MOVE ROO-CAI-IREC IN ROO-FLD TO REC-CAI-IREC IN REC.         
           MOVE ROO-IIC-IREC IN ROO-FLD TO REC-IIC-IREC IN REC.         
           MOVE FIO-GET-KEY    TO FIO-CMND                              
           PERFORM GRD-FIO-REC.                                         
           IF NOT FIO-STAT-OKS                                          
              MOVE 'RECNEX    ' TO MSG-COD-MENS                         
              MOVE 'GRD'        TO MSG-COD-SIST                         
              PERFORM GET-MSG              
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              MOVE SPACES       TO FRM-GLS-NOMC IN ROO-FLD              
              MOVE FIO-STAT-NEX TO FIO-STAT                             
              GO TO FIN-FND-KEY.                                        
                                                                        
           MOVE ROO-CAI-IREC IN ROO-FLD TO RRO-CAI-IREC IN RRO.         
           MOVE ROO-IIC-IREC IN ROO-FLD TO RRO-IIC-IREC IN RRO.         
           MOVE ROO-CAI-IOPC IN ROO-FLD TO RRO-CAI-IOPC IN RRO.         
           MOVE ROO-IIC-IOPC IN ROO-FLD TO RRO-IIC-IOPC IN RRO.         
           MOVE ROO-NUM-IDLC IN ROO-FLD TO RRO-NUM-IDLC IN RRO.         
           MOVE FIO-GET-KEY    TO FIO-CMND                              
           PERFORM GRD-FIO-RRO.                                         
           IF NOT FIO-STAT-OKS                                          
              MOVE 'RRONEX    ' TO MSG-COD-MENS                         
              MOVE 'GRD'        TO MSG-COD-SIST                         
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              MOVE SPACES       TO FRM-GLS-NOMC IN ROO-FLD              
              MOVE FIO-STAT-NEX TO FIO-STAT                             
              GO TO FIN-FND-KEY.                                        
                                                                        
           MOVE DBC-NUM-ICLI IN ROO-FLD TO DBC-NUM-ICLI IN DBC.         
           MOVE DBC-IND-ICLI IN ROO-FLD TO DBC-IND-ICLI IN DBC.         
           MOVE DBC-GLS-ICLI IN ROO-FLD TO DBC-GLS-ICLI IN DBC.         
           MOVE 'DBC-IDC-ICLI' TO FIO-AKEY.                             
           MOVE FIO-GET-KEY    TO FIO-CMND.                             
           PERFORM GET-CLI.                                             
           IF MSG-COD-MENS > SPACES                                     
              MOVE SPACES TO FRM-GLS-NOMC IN ROO-FLD                    
              MOVE FIO-STAT-NEX TO FIO-STAT                             
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              GO TO FIN-FND-KEY                                         
           ELSE                                                         
              MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN                      
              PERFORM CPT-BLKS                                          
              MOVE CPT-STRN TO FRM-GLS-NOMC IN ROO-FLD                  
              MOVE DBC-CAI-ICLI IN DBC TO SCR-CAI-ICLI IN CMA-VARI      
              MOVE DBC-IIC-ICLI IN DBC TO SCR-IIC-ICLI IN CMA-VARI      
              MOVE '00' TO PGM-STAT-DBC.                                
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY SGCBGCLI.                                                   
       COPY GNSBGCPT.                                                   
       COPY GNSBGSTR.                                                   
       COPY SGCBFDBC.                                                   
       COPY GRDBFRRO.                                                   
       COPY GRDBFREC.                                                   
      *>>>>
*% END
