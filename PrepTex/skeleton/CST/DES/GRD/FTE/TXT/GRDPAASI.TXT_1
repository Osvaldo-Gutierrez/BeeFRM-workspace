*%! Codigo adicional para archivo 
*% IF SETTAG
*% FNCPQ = "COBRJUDICIAL"
*% VSNPQ = "05.11.28.1"
*% FNCPU = "COBRJUDICIAL"
*% VSNPU = "05.11.28.1"
*% END
*% LARGO_MI_COMMAREA = 12
*% PGM_PTC = TRUE
*% IF MI_COMMAREA
      *<<<< MI_COMMAREA
           03 CMA-VARI.
              05 SCR-CIC-ICLI.
                 07 SCR-CAI-ICLI                   PIC X(04).
                 07 SCR-IIC-ICLI                   PIC X(08). 
      *>>>>
*% END
*% IF WSS                                                         
      *<<<< WSS
      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
      *>>>>
*% END
*% IF FND_KEY
      *<<<< FND_KEY
           IF ( (SCR-CMND = 'CON' OR 'ELI' OR 'MOD' )AND                
                ASI-FEC-FPRO IN ASI-FLD NOT > ZEROES AND                
                ASI-HRA-HPRO IN ASI-FLD NOT > ZEROES )                  
              MOVE 999999      TO ASI-HRA-HPRO IN ASI                   
              MOVE 99999999    TO ASI-FEC-FPRO IN ASI                   
              MOVE FIO-GET-LEQ TO FIO-CMND                              
              PERFORM GRD-FIO-ASI                                       
              IF NOT ( FIO-STAT-OKS AND                                 
                ASI-CAI-IREC IN ASI = ASI-CAI-IREC IN ASI-FLD AND       
                ASI-IIC-IREC IN ASI = ASI-IIC-IREC IN ASI-FLD )         
                 MOVE '23'     TO FIO-STAT                              
                 MOVE FIO-STAT TO PGM-STAT-ASI                          
                 GO TO FIN-FND-KEY                                      
              ELSE                                                      
                 MOVE ASI-NUM-DPRO IN ASI TO ASI-NUM-DPRO IN ASI-FLD    
                 MOVE ASI-NUM-MPRO IN ASI TO ASI-NUM-MPRO IN ASI-FLD    
                 MOVE ASI-NUM-SPRO IN ASI TO ASI-NUM-SPRO IN ASI-FLD    
                 MOVE ASI-NUM-APRO IN ASI TO ASI-NUM-APRO IN ASI-FLD    
                 MOVE ASI-HRA-HPRO IN ASI TO ASI-HRA-HPRO IN ASI-FLD.   
                                                                        
                                                                        
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
      *>>>>
*% END
*% IF FIN_HBL_FLD
      *<<<<
           IF SCR-CMND = 'EST'                                          
              MOVE FRM-CPIM-AEY TO ASI-COD-ROLR-ATR IN ASI-FLD          
              MOVE FRM-CPIM-AEY TO ASI-COD-USRR-ATR IN ASI-FLD          
              MOVE FRM-CPIM-AEY TO ASI-COD-SITA-ATR IN ASI-FLD          
              MOVE FRM-CPIM-AEY TO ASI-COD-ETAP-ATR IN ASI-FLD          
              MOVE FRM-CPIM-AEY TO ASI-FEC-FTER-ATR IN ASI-FLD          
              MOVE FRM-CPIM-AEY TO ASI-COD-TARE-ATR IN ASI-FLD          
              MOVE 0            TO ASI-COD-ROLR-LEN IN ASI-FLD          
              MOVE -1           TO ASI-COD-SITA-LEN IN ASI-FLD          
              MOVE FRM-CPIM-UBY TO ASI-COD-SITA-ATR IN ASI-FLD          
              MOVE FRM-CPIM-UBY TO ASI-FEC-FFIN-ATR IN ASI-FLD          
              MOVE FRM-CPIM-UBY TO ASI-HRA-HRFI-ATR IN ASI-FLD.         
      *>>>>
*% END
