       SET-TAG-GRDBQCOM SECTION.
       INI-SET-TAG-GRDBQCOM.
           MOVE
           '<IDNMOD NOM=$GRDBQCOM$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$14-Dec-05 10:09 AM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-GRDBQCOM.
           EXIT.
           
       PUT-ALL-COM SECTION.
       INI-PUT-ALL-COM.
           PERFORM PUT-RRO-COM.
       FIN-PUT-ALL-COM.
           EXIT.
       
       PUT-RRO-COM SECTION.
       INI-PUT-RRO-COM.
           IF PGM-STAT-RRO-NUL
               GO TO FIN-PUT-RRO-COM.
           IF NOT PGM-STAT-RRO-OKS
               PERFORM CLR-RRO-COM
               GO TO FIN-PUT-RRO-COM.
           MOVE RRO-CAI-IREC IN RRO TO RRO-CAI-IREC IN COM-FLD.
           MOVE RRO-IIC-IREC IN RRO TO RRO-IIC-IREC IN COM-FLD.
           MOVE RRO-COD-ESTA IN RRO TO RRO-COD-ESTA IN COM-FLD.
       FIN-PUT-RRO-COM.
           EXIT.
       
       KEY-ALL-COM SECTION.
       INI-KEY-ALL-COM.
           PERFORM KEY-RRO-COM.
       FIN-KEY-ALL-COM.
           EXIT.
       
       KEY-RRO-COM SECTION.
       INI-KEY-RRO-COM.
           MOVE RRO-CAI-IREC IN COM-FLD TO RRO-CAI-IREC IN RRO.
           MOVE RRO-IIC-IREC IN COM-FLD TO RRO-IIC-IREC IN RRO.
           MOVE RRO-COD-ESTA IN COM-FLD TO RRO-COD-ESTA IN RRO.
       FIN-KEY-RRO-COM.
           EXIT.
       
       SCR-CLRA-COM SECTION.
       INI-SCR-CLRA-COM.
           PERFORM CLR-KEY-COM.
           PERFORM CLR-FLD-COM.
       FIN-SCR-CLRA-COM.
           EXIT.
       
       CLR-KEY-COM SECTION.
       INI-CLR-KEY-COM.
           MOVE SPACES TO RRO-CAI-IREC IN COM-FLD.
           MOVE ZEROES TO RRO-IIC-IREC IN COM-FLD.
           MOVE SPACES TO RRO-COD-ESTA IN COM-FLD.
       FIN-CLR-KEY-COM.
           EXIT.
       
