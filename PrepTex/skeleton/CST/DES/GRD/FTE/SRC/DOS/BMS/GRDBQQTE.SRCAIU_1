       SET-TAG-GRDBQQTE SECTION.
       INI-SET-TAG-GRDBQQTE.
           MOVE
           '<IDNMOD NOM=$GRDBQQTE$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$22-Dec-05 02:08 AM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-GRDBQQTE.
           EXIT.
           
       PUT-ALL-QTE SECTION.
       INI-PUT-ALL-QTE.
           PERFORM PUT-GNS-QTE.
       FIN-PUT-ALL-QTE.
           EXIT.
       
       PUT-GNS-QTE SECTION.
       INI-PUT-GNS-QTE.
           IF PGM-STAT-GNS-NUL
               GO TO FIN-PUT-GNS-QTE.
           IF NOT PGM-STAT-GNS-OKS
               PERFORM CLR-GNS-QTE
               GO TO FIN-PUT-GNS-QTE.
           MOVE GNS-GLS-CFLD IN GNS TO GNS-GLS-CFLD IN QTE-FLD.
           MOVE GNS-GLS-CFLD IN GNS TO GNS-GLS-CFLD IN QTE-FLD.
           MOVE GNS-GLS-CFLD IN GNS TO GNS-GLS-CFLD IN QTE-FLD.
           MOVE GNS-GLS-CFLD IN GNS TO GNS-GLS-CFLD IN QTE-FLD.
           MOVE GNS-GLS-CFLD IN GNS TO GNS-GLS-CFLD IN QTE-FLD.
           MOVE GNS-GLS-CFLD IN GNS TO GNS-GLS-CFLD IN QTE-FLD.
       FIN-PUT-GNS-QTE.
           EXIT.
       
       KEY-ALL-QTE SECTION.
       INI-KEY-ALL-QTE.
           PERFORM KEY-GNS-QTE.
       FIN-KEY-ALL-QTE.
           EXIT.
       
       KEY-GNS-QTE SECTION.
       INI-KEY-GNS-QTE.
       FIN-KEY-GNS-QTE.
           EXIT.
       
       SCR-CLRA-QTE SECTION.
       INI-SCR-CLRA-QTE.
           PERFORM CLR-KEY-QTE.
           PERFORM CLR-FLD-QTE.
       FIN-SCR-CLRA-QTE.
           EXIT.
       
       CLR-KEY-QTE SECTION.
