       PROCEDURE DIVISION.
      *===================

       MAIN SECTION.
       INI-MAIN.
           PERFORM INI.
       LUP-MAIN.
           IF SCR-INST = SCR-INST-HCM
                 PERFORM SCR-COM.
           IF SCR-CCPP = 'ACC'
                 PERFORM SCR-ACC
           ELSE
                 MOVE SCR-INST-HCM TO SCR-INST.
           GO TO LUP-MAIN.
       FIN-MAIN.
           EXIT.

       INI SECTION.
       INI-INI.
       COPY GNSBGEIB.
       COPY GNSBGEDB.
           MOVE LOW-VALUES TO CRD-FLD.
           MOVE LOW-VALUES TO CRD-EDT.
           PERFORM GNS-PRO-RCK.
           PERFORM GNS-BUS-VCA.
           MOVE +270 TO SCR-TCMA.
           PERFORM GNS-HDL-SYS.
           PERFORM GNS-HDL-FRM.
           MOVE 'GRDPQCRD' TO FIO-PROG.
           PERFORM GNS-BUS-IDD.
      *PTC-INI
           PERFORM GNS-ERR-QUE.
           PERFORM GET-TSK-TERM.
      *PTC-FIN
      *<<<< INI_FIO                                                     
           IF SCR-OFRM-SRV                                              
              MOVE TSK-TERM-ALF TO CRD-COD-TERM                         
              MOVE TSK-TERM-ALF TO CRD-COD-TERM-MAS                     
                                                                        
           ELSE                                                         
              MOVE EIBTRMID     TO CRD-COD-TERM                         
              MOVE EIBTRMID     TO CRD-COD-TERM-MAS.                    
      *>>>>
       FIN-INI.
           EXIT.

       COPY GNSBHSYS.
       COPY GNSBHFRM.
       COPY GNSBPRCK.
       COPY GNSBBVCA.
       COPY GNSBBIDD.

       SCR-COM SECTION.
       INI-SCR-COM.
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-CLRA-CRD.
           PERFORM EDT-KEY-CRD.
           PERFORM EDT-FLD-CRD.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-HCM TO SCR-INST.
           MOVE SCR-CMNP     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       XCT-ERR.
           MOVE 'XCTERR    ' TO MSG-COD-MENS.
           MOVE 'GNS'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO SYS-MEN1.
           MOVE SYS-PROG     TO SYS-MEN2.
           MOVE SYS-ABOR     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       COPY GNSBGGBK.
       FIN-SCR-COM.
           EXIT.



       SCR-ACC SECTION.
       INI-SCR-ACC.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ACC
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-ACC.
           PERFORM DESSCRTRL.
      *<<<< PFS_SCR_ACC                                                 
           IF FRM-FFLD = FRM-FFLD-PF6                                   
                    OR   FRM-FFLD-PF5                                   
               PERFORM VAL-NUM-KEY-CRD                                  
               PERFORM VAL-NUM-FLD-CRD                                  
               PERFORM RST-STAT                                         
               PERFORM FND-SCR-KEY THRU FIN-SCR-KEY                     
               IF SCR-STAT NOT = SCR-STAT-ABT                           
                   PERFORM SCR-TRL                                      
               ELSE                                                     
                   PERFORM DESSCRTRL                                    
                   GO TO FIN-SCR-ACC.                                   
      *>>>>
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ACC.
           EXIT.

       BRW-SCR-ACC SECTION.
       INI-BRW-SCR-ACC.
       FIN-BRW-SCR-ACC.
           EXIT.




       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               PERFORM SCR-CLRA-CRD
               PERFORM EDT-KEY-CRD
               PERFORM EDT-FLD-CRD
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-CRD
               PERFORM EDT-KEY-CRD
           ELSE
               PERFORM CLR-FLD-CRD
               PERFORM EDT-FLD-CRD.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

       SCR-KEY SECTION.
       INI-SCR-KEY.
           PERFORM SCR-RST.
           PERFORM GET-KEY.
           IF FRM-STAT-ABT
                   MOVE SPACES TO FRM-MENS
                   PERFORM SCR-MSSG
                   GO TO FIN-SCR-KEY.
       FND-SCR-KEY.
           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
           IF FIO-STAT-OKS
               PERFORM PUT-SCR
               MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
               GO TO FIN-SCR-KEY.
      *JSS ELSE
      *        PERFORM CLR-FLD-CRD.
       ERR-SCR-KEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR.
           IF FRM-MENS NOT > SPACES
               IF FIO-STAT-EOF
                   MOVE 'REGEOF    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
               IF FIO-STAT = FIO-STAT-NEX
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
           MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-SCR-KEY.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO CRD-FLD.
           MOVE LOW-VALUES TO CRD-EDT.
       FIN-SCR-RST.
           EXIT.

       GET-KEY SECTION.
       INI-GET-KEY.
           PERFORM SCR-GKEY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-KEY.
           EXIT.

       GET-IKY SECTION.
       INI-GET-IKY.
           PERFORM SCR-GIKY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-IKY.
           EXIT.

       FND-KEY SECTION.
       INI-FND-KEY.
      *<<<< INI_FND_KEY                                                 
           MOVE SPACES TO FRM-MEN1.                                     
           IF FRM-FFLD = FRM-FFLD-PF5                                   
               MOVE PGM-SNUL TO PGM-STAT-RRD                           
               SUBTRACT 1 FROM SCR-I GIVING CRD-I                       
               IF CRD-I NOT > ZEROES                                    
                   MOVE 'REC PRV' TO MSG-COD-MENS                    
                   GO TO ERR-FND-KEY                                    
               ELSE                                                     
                   GO TO CARGA-CRD                                      
           ELSE                                                         
           IF FRM-FFLD = FRM-FFLD-PF6                                   
               MOVE PGM-SNUL TO PGM-STAT-RRD                            
               ADD 1 SCR-I GIVING CRD-I                                 
               IF CRD-I > SCR-IMAX                                      
                   MOVE 'REC NXT' TO MSG-COD-MENS                    
                   GO TO ERR-FND-KEY                                    
               ELSE                                                     
                   GO TO CARGA-CRD.                                     

            MOVE DBC-NUM-ICLI IN CRD-FLD TO DBC-NUM-ICLI IN DBC.
            MOVE DBC-IND-ICLI IN CRD-FLD TO DBC-IND-ICLI IN DBC.
            MOVE DBC-GLS-ICLI IN CRD-FLD TO DBC-GLS-ICLI IN DBC.
            MOVE 'DBC-IDC-ICLI' TO FIO-AKEY.
            MOVE FIO-GET-KEY TO FIO-CMND.
            PERFORM SGC-FIO-DBC.
            IF NOT FIO-STAT-OKS
               GO TO FIN-FND-KEY.
            MOVE DBC-CAI-ICLI IN DBC TO SCR-CAI-IREC.
            MOVE DBC-IIC-ICLI IN DBC TO SCR-IIC-IREC.
           GO TO CON-FND-KEY.                                           

      *>>>>
           PERFORM KEY-ALL-CRD.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GRD-FIO-RRD.
           IF FIO-STAT-OKS
               MOVE RRD-STP-ITRN IN RRD TO SCR-UMOD.
           MOVE FIO-STAT TO PGM-STAT-RRD.
      *<<<< FIN_FND_KEY                                                 
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-FND-KEY.                                       
       CON-FND-KEY.                                                     
                                                                        
           MOVE ZEROES TO SCR-IMAX.                                     
           MOVE 1 TO SCR-I.                                             
           MOVE '00' TO FIO-STAT.                                       
           PERFORM OBT-DAT-TS.                                          
           MOVE 1 TO SCR-I.                                             
           MOVE SCR-I TO CRD-I.                                         
       CARGA-CRD.                                                       
           MOVE SPACES TO MSG-COD-MENS.                                 
           IF NOT SCR-OFRM-SRV                                          
           PERFORM PUT-TAB-CRD.                                         
           MOVE '00' TO FIO-STAT.                                       
           GO TO FIN-FND-KEY.                                           
      *                                                                 
       ERR-FND-KEY.                                                     
           MOVE SCR-STAT-CON TO SCR-STAT.                               
                                                                        
           MOVE 'GRD'  TO MSG-COD-SIST.                                 
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MEN1.                               
           MOVE '00' TO FIO-STAT.                                       
           GO TO FIN-FND-KEY.                                           
      *>>>>
       FIN-FND-KEY.
           IF FIO-STAT = FIO-STAT-NEX
               MOVE 'S' TO SCR-IERR.
       EXT-FND-KEY.
           EXIT.
 
       PUT-SCR SECTION.
       INI-PUT-SCR.
           IF PGM-STAT-RRD-OKS
               MOVE RRD-STP-ITRN IN RRD TO SCR-UMOD.
           PERFORM PUT-ALL-CRD.
       FIN-PUT-SCR.
           EXIT.

       STP-EXIT SECTION.
       INI-STP-EXIT.
           MOVE SCR-PROG TO PGM-PROG.
           MOVE SCR-NTRN TO PGM-NTRN.

           MOVE SCR-PROV TO SCR-PROG.
           MOVE SCR-NTRV TO SCR-NTRN.

           MOVE PGM-PROG TO SCR-PROV
           MOVE PGM-NTRN TO SCR-NTRV.
       FIN-STP-EXIT.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO SYS-NTRN.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
      *PTC-INI
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE CRD-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO CRD-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GKEY SECTION.
       INI-SCR-GKEY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-CRD
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO CRD-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GKEY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-CRD.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-CRD
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-CRD.
           PERFORM VAL-IKY-CRD.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-MKY-CRD.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-AKY-CRD.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-CON-KEY-CRD.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GKEY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-CRD.
       ERR-SCR-GKEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-CRD.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GKEY.
           PERFORM SCR-MSSG.

       SCR-GIKY SECTION.
       INI-SCR-GIKY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-CRD
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO CRD-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GIKY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-CRD.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-IKY-CRD
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-CRD.
           PERFORM VAL-IKY-CRD.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GIKY.
           PERFORM VAL-CON-IKY-CRD.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GIKY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-CRD.
       ERR-SCR-GIKY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-IKY-CRD.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GIKY.
           PERFORM SCR-MSSG.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
      *<<<< PFS_GET_CON OR PFS_GET_ACC                                  
           IF NOT (FRM-FFLD = FRM-FFLD-PF6                              
                         OR   FRM-FFLD-PF5)                             
      *>>>>
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO CRD-GLS-MEN2
               MOVE SCR-PRMT     TO CRD-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO CRD-FLD.
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO CRD-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE CRD-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO CRD-FLD
      * FLB  7-NOV-1994
               PERFORM CLR-KEY-CRD

               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO CRD-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO CRD-GLS-MENS.
           PERFORM PRO-MKY-CRD.
           PERFORM PRO-AKY-CRD.
           PERFORM UNP-IKY-CRD.
           PERFORM FST-KEY-CRD.
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE CRD-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO CRD-FLD
               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO CRD-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO CRD-GLS-MENS.
           PERFORM UNP-KEY-CRD.
           PERFORM FST-KEY-CRD.
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
           PERFORM PRO-IKY-CRD.
           PERFORM PRO-AKY-CRD.
           PERFORM UNP-MKY-CRD.
           PERFORM UNP-FLD-CRD.
           PERFORM FST-FLD-CRD.
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO CRD-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-CRD.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
      *PTC-INI
           IF SCR-STAT NOT = SCR-STPP
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-CRD
               PERFORM EDT-FLD-CRD
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-CRD
               PERFORM EDT-FLD-CRD
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-CRD.
           PERFORM EDT-FLD-CRD.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

      * PGM-EBR-ALL SECTION.
      * INI-PGM-EBR-ALL.
      *     MOVE 'GNS' TO FIO-SIST.
      *     MOVE FIO-EBR-ALL TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
      * FIN-PGM-EBR-ALL.
      *     EXIT.

       COPY GRDBQCRD.
       COPY GNSBVNUM.
       COPY GRDBFRRD.
       COPY GNSBFMSG.
       COPY GNSBGSYS.
       COPY GNSBGDTC.
      *PTC-INI
      *PTC-SUB
      * ISP  10-MAY-2001 13:46:35 
      * COPY GNSBGFRM.
       COPY GNSBG3CF.
      *PTC-FIN
       COPY GNSBGMSG.
       COPY GNSBEKEY.
       COPY GNSBIABT.
       COPY GNSBVCOD.
       COPY GNSBVTAB.
       COPY GNSBFTAB.
       COPY GNSBVIND.
       COPY GNSBVMSC.
       COPY GNSBFMSC.
       COPY GNSBVIDC.
       COPY GNSBPVRF.
       COPY GNSBEGLS.
       COPY GNSBGFEC.
       COPY GNSBGHOY.
      *PTC-INI
       COPY GNSBGQUE.
       COPY GNSBGTSK.
      *PTC-FIN

      *<<<< EOF                                                         
       COPY GNSBGCPT.
       COPY GRDBFREC.
       COPY SGCBFDBC.
       COPY GNSBBTAB.
       COPY GNSBGPES.                                                   
                                      
       CAR-MOR-REC SECTION.                                             
       INI-CAR-MOR-REC.                                                 
           MOVE SCR-CAI-IREC            TO RRD-CAI-ICLI IN RRD.         
           MOVE SCR-IIC-IREC            TO RRD-IIC-ICLI IN RRD.         
           MOVE RRD-COD-TREL IN CRD-FLD TO RRD-COD-TREL IN RRD.         
           MOVE FIO-GET-NLS             TO FIO-CMND.                    
       LUP-CAR-MOR-REC.                                                 
           MOVE 'RRD-KEY-ADEU'          TO FIO-AKEY.                    
           PERFORM GRD-FIO-RRD.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
               SCR-CAI-IREC = RRD-CAI-ICLI IN RRD  AND                  
               SCR-IIC-IREC = RRD-IIC-ICLI IN RRD )                     
               GO TO FIN-CAR-MOR-REC.                                   
                                                                        
       VAL-CAR-MOR-REC.                                                 
           MOVE RRD-CAI-IREC IN RRD TO REC-CAI-IREC IN REC.             
           MOVE RRD-IIC-IREC IN RRD TO REC-IIC-IREC IN REC.             
           MOVE FIO-GET-KEY         TO FIO-CMND.                        
           PERFORM GRD-FIO-REC.                                         
           IF NOT FIO-STAT-OKS                                          
              GO TO SIG-CAR-MOR-REC.                                    
                                                                        
           MOVE REC-CAI-IREC IN REC TO CRD-CAI-IREC(CRD-I).             
           MOVE REC-IIC-IREC IN REC TO CRD-IIC-IREC(CRD-I).             
           MOVE REC-IND-SCBZ IN REC TO CRD-IND-SCBZ(CRD-I).             
           MOVE REC-COD-ESTA IN REC TO CRD-COD-ESTA(CRD-I).             
           MOVE REC-COD-ANTE IN REC TO CRD-COD-ANTE(CRD-I).             
           MOVE REC-COD-ORIG IN REC TO CRD-COD-ORIG(CRD-I).             
           MOVE REC-COD-TIPO IN REC TO CRD-COD-TIPO(CRD-I).             
           MOVE REC-COD-SUBT IN REC TO CRD-COD-SUBT(CRD-I).             
           MOVE REC-COD-SEGM IN REC TO CRD-COD-SEGM(CRD-I).             
           MOVE REC-COD-ETAP IN REC TO CRD-COD-ETAP(CRD-I).             
           MOVE REC-NUM-DINI IN REC TO WSS-NUM-DAUX.                    
           MOVE REC-NUM-MINI IN REC TO WSS-NUM-MAUX.                    
           MOVE REC-NUM-SINI IN REC TO WSS-NUM-SAUX.                    
           MOVE REC-NUM-AINI IN REC TO WSS-NUM-AAUX.                    
           MOVE WSS-FEC-FAUX        TO CRD-FEC-FINI(CRD-I).             
                                                                        
           MOVE REC-NUM-DTER IN REC TO WSS-NUM-DAUX.                    
           MOVE REC-NUM-MTER IN REC TO WSS-NUM-MAUX.                    
           MOVE REC-NUM-STER IN REC TO WSS-NUM-SAUX.                    
           MOVE REC-NUM-ATER IN REC TO WSS-NUM-AAUX.                    
           MOVE WSS-FEC-FAUX        TO CRD-FEC-FTER(CRD-I).             
                                                                        
           MOVE REC-NUM-DIIN IN REC TO WSS-NUM-DAUX.                    
           MOVE REC-NUM-MIIN IN REC TO WSS-NUM-MAUX.                    
           MOVE REC-NUM-SIIN IN REC TO WSS-NUM-SAUX.                    
           MOVE REC-NUM-AIIN IN REC TO WSS-NUM-AAUX.                    
           MOVE WSS-FEC-FAUX        TO CRD-FEC-FIIN(CRD-I).             
                                                                        
           MOVE REC-NUM-DTIN IN REC TO WSS-NUM-DAUX.                    
           MOVE REC-NUM-MTIN IN REC TO WSS-NUM-MAUX.                    
           MOVE REC-NUM-STIN IN REC TO WSS-NUM-SAUX.                    
           MOVE REC-NUM-ATIN IN REC TO WSS-NUM-AAUX.                    
           MOVE WSS-FEC-FAUX        TO CRD-FEC-FTIN(CRD-I).             
                                                                        
       NXT-CAR-MOR-REC.                                                 
           PERFORM INC-REG-TS.                                          
       SIG-CAR-MOR-REC.                                                 
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-CAR-MOR-REC.                                       
                                                                        
       FIN-CAR-MOR-REC.                                                 
           EXIT.                                                        
                                                                        
       OBT-PARAM SECTION.                                               
       INI-OBT-PARAM.                                                   
       FIN-OBT-PARAM.                                                   
           EXIT.                                                        
                                                                        
       OBT-DAT-TS SECTION.                                              
       INI-OBT-DAT-TS.                                                  
           EXEC CICS HANDLE CONDITION QIDERR(CON-OBT-DAT-TS)            
                                     ITEMERR(CON-OBT-DAT-TS)            
           END-EXEC.                                                    
                                                                        
      *     EXEC CICS DELETEQ TS QUEUE(CRD-NAME-QUE)                    
      *     END-EXEC.                                                   
      *<<<<                                                             
           MOVE CRD-NOM-TSCRD     TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
      *>>>>                                                             
       CON-OBT-DAT-TS.                                                  
           MOVE CRD-SI-IMP TO CRD-IMP.                                  
           PERFORM CLR-VEC-TS VARYING CRD-I FROM 1 BY 1 UNTIL           
                   CRD-I > CRD-IMAX.                                    
           MOVE 1 TO CRD-I.                                             
           PERFORM CAR-MOR-REC.                                         
                                                                        
       NXT-OBT-DAT-TS.                                                  
           IF CRD-IMP = CRD-SI-IMP                                      
               PERFORM WRT-TS.                                          
       FIN-OBT-DAT-TS.                                                  
           EXIT.                                                        
                                                                        
       PUT-TAB-CRD SECTION.                                             
       INI-PUT-TAB-CRD.                                                 
           PERFORM RED-TS.                                              
           PERFORM LLE-DAT-PNT VARYING CRD-I FROM 1 BY 1                
                   UNTIL CRD-I > CRD-IMAX.                              
       FIN-PUT-TAB-CRD.                                                 
           EXIT.                                                        
                                                                        
       LLE-DAT-PNT SECTION.                                             
       INI-LLE-DAT-PNT.                                                 
           MOVE CRD-CAI-IREC(CRD-I) TO FRM-CAI-IREC IN CRD-FLD(CRD-I). 
           MOVE CRD-IIC-IREC(CRD-I) TO FRM-IIC-IREC IN CRD-FLD(CRD-I). 
           MOVE CRD-IND-SCBZ(CRD-I) TO FRM-IND-SCBZ IN CRD-FLD(CRD-I). 
           MOVE CRD-COD-TIPO(CRD-I) TO FRM-COD-TIPO IN CRD-FLD(CRD-I). 
           MOVE CRD-COD-SUBT(CRD-I) TO FRM-COD-SUBT IN CRD-FLD(CRD-I).
           MOVE CRD-COD-ESTA(CRD-I) TO FRM-COD-ESTA IN CRD-FLD(CRD-I).
           MOVE CRD-COD-ANTE(CRD-I) TO FRM-COD-ANTE IN CRD-FLD(CRD-I).
           MOVE CRD-COD-ORIG(CRD-I) TO FRM-COD-ORIG IN CRD-FLD(CRD-I).
           MOVE CRD-COD-SEGM(CRD-I) TO FRM-COD-SEGM IN CRD-FLD(CRD-I).
           MOVE CRD-COD-ETAP(CRD-I) TO FRM-COD-ETAP IN CRD-FLD(CRD-I).
           MOVE CRD-FEC-FINI(CRD-I) TO FRM-FEC-FINI IN CRD-FLD(CRD-I).
           MOVE CRD-FEC-FTER(CRD-I) TO FRM-FEC-FTER IN CRD-FLD(CRD-I).
           MOVE CRD-FEC-FIIN(CRD-I) TO FRM-FEC-FIIN IN CRD-FLD(CRD-I).
           MOVE CRD-FEC-FTIN(CRD-I) TO FRM-FEC-FTIN IN CRD-FLD(CRD-I). 
       FIN-LLE-DAT-PNT.                                                 
           EXIT.                                                        
                                                                        
       INC-REG-TS SECTION.                                              
       INI-INC-REG-TS.                                                  
            MOVE CRD-SI-IMP TO CRD-IMP.                                 
      *     IF SCR-OFRM-SRV                                             
      *       MOVE CRD-VAL-CPTO(CRD-I) TO CRD-VAL-CPTO-EDT              
      *       MOVE CRD-VAL-CPTO-EDT TO CRD-VAL-CPTO-ALF(CRD-I).         
           ADD 1 TO CRD-I.                                              
           IF CRD-I > CRD-IMAX                                          
               PERFORM WRT-TS                                           
               PERFORM CLR-VEC-TS VARYING CRD-I FROM 1 BY 1             
                                     UNTIL CRD-I > CRD-IMAX             
               MOVE 1 TO CRD-I                                          
               MOVE CRD-NO-IMP TO CRD-IMP.                              
       FIN-INC-REG-TS.                                                  
           EXIT.                                                        
                                                                        
      *<<<<                                                             
      *>>>>                                                             
                                                                        
       WRT-TS SECTION.                                                  
       INI-WRT-TS.
           ADD 1 TO SCR-IMAX.                                           
      *     EXEC CICS WRITEQ TS QUEUE(CRD-NAME-QUE)                     
      *                         FROM(CRD-TS)                            
      *                         LENGTH(CRD-LONG-TS)                     
      *     END-EXEC.                                                   
      *<<<<                                                             
      *    MOVE EIBTRMID      TO CRD-TERM.                              
           MOVE CRD-NOM-TSCRD  TO QUE-COLA.                             
           MOVE CRD-TS         TO QUE-ITEM.                             
           MOVE CRD-LEN-TSCRD  TO QUE-LITM.                             
           MOVE QUE-PUT        TO QUE-CMND.                             
           PERFORM GNS-PRO-QUE.                                         
      *>>>>                                                             
       FIN-WRT-TS.                                                      
           EXIT.                                                        
                                                                        
       RED-TS SECTION.                                                  
       INI-RED-TS.                                                      
           MOVE CRD-I TO SCR-I.                                         
           EXEC CICS HANDLE CONDITION QIDERR(FIN-RED-TS)                
                                     ITEMERR(FIN-RED-TS)                
           END-EXEC.                                                    
      *     EXEC CICS READQ TS QUEUE(CRD-NAME-QUE)                      
      *                         INTO(CRD-TS)                            
      *                          LENGTH(CRD-LONG-TS)                    
      *                         ITEM(CRD-I)                             
      *     END-EXEC.                                                   
      *<<<<                                                             
           MOVE CRD-NOM-TSCRD      TO QUE-COLA.                         
           MOVE CRD-TS             TO QUE-ITEM.                         
           MOVE CRD-LEN-TSCRD      TO QUE-LITM.                         
           MOVE CRD-I              TO QUE-NITM.                         
           MOVE QUE-GET            TO QUE-CMND.                         
           PERFORM GNS-PRO-QUE.                                         
           MOVE QUE-ITEM       TO CRD-TS.                               
      *>>>>                                                             
       FIN-RED-TS.                                                      
           EXIT.                                                        
                                                                        
       CLR-VEC-TS SECTION.                                              
       INI-CLR-VEC-TS.
           MOVE SPACES TO CRD-CAI-IREC(CRD-I).
           MOVE SPACES TO CRD-IIC-IREC(CRD-I).
           MOVE SPACES TO CRD-IND-SCBZ(CRD-I). 
           MOVE SPACES TO CRD-COD-TIPO(CRD-I). 
           MOVE SPACES TO CRD-COD-SUBT(CRD-I). 
           MOVE SPACES TO CRD-COD-ESTA(CRD-I). 
           MOVE SPACES TO CRD-COD-ANTE(CRD-I). 
           MOVE SPACES TO CRD-COD-ORIG(CRD-I). 
           MOVE SPACES TO CRD-COD-SEGM(CRD-I). 
           MOVE SPACES TO CRD-COD-ETAP(CRD-I). 
           MOVE ZEROES TO CRD-FEC-FINI(CRD-I). 
           MOVE ZEROES TO CRD-FEC-FTER(CRD-I). 
           MOVE ZEROES TO CRD-FEC-FIIN(CRD-I). 
           MOVE ZEROES TO CRD-FEC-FTIN(CRD-I). 
       FIN-CLR-VEC-TS. 
           EXIT.                                                        
                                                                        
       RST-STAT SECTION.                                                
       INI-RST-STAT.                                                    
           MOVE SCR-STPP     TO SCR-STAT.                               
           MOVE SCR-MFLD-NOT TO SCR-MFLD.                               
       FIN-RST-STAT.                                                    
           EXIT.                                                        
                                                                             
      *>>>>                                                             
