      *
      * GRDBQQDO
      * Version 3.0.1 Nov-2005
      * 08-Jan-07 03:58 PM
      *
       SET-TAG-GRDBQQDO SECTION.
       INI-SET-TAG-GRDBQQDO.
           MOVE
           '<IDNMOD NOM=$GRDBQQDO$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$08-Jan-07 03:58 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-GRDBQQDO.
           EXIT.
           
       PUT-ALL-QDO SECTION.
       INI-PUT-ALL-QDO.
           PERFORM PUT-DOC-QDO.
       FIN-PUT-ALL-QDO.
           EXIT.
       
       PUT-DOC-QDO SECTION.
       INI-PUT-DOC-QDO.
           IF PGM-STAT-DOC-NUL
               GO TO FIN-PUT-DOC-QDO.
           IF NOT PGM-STAT-DOC-OKS
               PERFORM CLR-DOC-QDO
               GO TO FIN-PUT-DOC-QDO.
           MOVE DOC-COD-TOOC IN DOC TO DOC-COD-TOOC IN QDO-FLD.
           MOVE DOC-COD-AOOC IN DOC TO DOC-COD-AOOC IN QDO-FLD.
           MOVE DOC-COD-CBAN IN DOC TO DOC-COD-CBAN IN QDO-FLD.
           MOVE DOC-COD-AUXI IN DOC TO DOC-COD-AUXI IN QDO-FLD.
       FIN-PUT-DOC-QDO.
           EXIT.
       
       KEY-ALL-QDO SECTION.
       INI-KEY-ALL-QDO.
           PERFORM KEY-DOC-QDO.
       FIN-KEY-ALL-QDO.
           EXIT.
       
       KEY-DOC-QDO SECTION.
       INI-KEY-DOC-QDO.
           MOVE DOC-COD-TOOC IN QDO-FLD TO DOC-COD-TOOC IN DOC.
           MOVE DOC-COD-AOOC IN QDO-FLD TO DOC-COD-AOOC IN DOC.
           MOVE DOC-COD-CBAN IN QDO-FLD TO DOC-COD-CBAN IN DOC.
           MOVE DOC-COD-AUXI IN QDO-FLD TO DOC-COD-AUXI IN DOC.
       FIN-KEY-DOC-QDO.
           EXIT.
       
       SCR-CLRA-QDO SECTION.
       INI-SCR-CLRA-QDO.
           PERFORM CLR-KEY-QDO.
           PERFORM CLR-FLD-QDO.
       FIN-SCR-CLRA-QDO.
           EXIT.
       
       CLR-KEY-QDO SECTION.
       INI-CLR-KEY-QDO.
           MOVE SPACES TO DOC-COD-TOOC IN QDO-FLD.
           MOVE SPACES TO DOC-COD-AOOC IN QDO-FLD.
           MOVE SPACES TO DOC-COD-CBAN IN QDO-FLD.
           MOVE SPACES TO DOC-COD-AUXI IN QDO-FLD.
           MOVE ZEROES TO FRM-NUM-PAGI IN QDO-FLD.
       FIN-CLR-KEY-QDO.
           EXIT.
       
       CLR-FLD-QDO SECTION.
       INI-CLR-FLD-QDO.
           MOVE SPACES TO FRM-GLS-TOOC IN QDO-FLD.
           MOVE SPACES TO FRM-GLS-AOOC IN QDO-FLD.
           MOVE SPACES TO FRM-GLS-CBAN IN QDO-FLD.
           MOVE SPACES TO FRM-GLS-AUXI IN QDO-FLD.
           MOVE SPACES TO FRM-GLS-TEXT IN QDO-FLD(1).
           MOVE SPACES TO FRM-GLS-TEXT IN QDO-FLD(2).
           MOVE SPACES TO FRM-GLS-TEXT IN QDO-FLD(3).
           MOVE SPACES TO FRM-GLS-TEXT IN QDO-FLD(4).
           MOVE SPACES TO FRM-GLS-TEXT IN QDO-FLD(5).
           MOVE SPACES TO FRM-GLS-TEXT IN QDO-FLD(6).
           MOVE SPACES TO FRM-GLS-TEXT IN QDO-FLD(7).
           MOVE SPACES TO FRM-GLS-TEXT IN QDO-FLD(8).
           MOVE SPACES TO FRM-GLS-TEXT IN QDO-FLD(9).
           MOVE SPACES TO FRM-GLS-TEXT IN QDO-FLD(10).
           MOVE SPACES TO FRM-GLS-TEXT IN QDO-FLD(11).
           MOVE SPACES TO FRM-GLS-TEXT IN QDO-FLD(12).
           MOVE SPACES TO FRM-GLS-TEXT IN QDO-FLD(13).
           MOVE SPACES TO FRM-GLS-TEXT IN QDO-FLD(14).
           MOVE SPACES TO FRM-GLS-TEXT IN QDO-FLD(15).
           PERFORM CLR-DOC-QDO.
       FIN-CLR-FLD-QDO.
           EXIT.
       
       CLR-DOC-QDO SECTION.
       INI-CLR-DOC-QDO.
       FIN-CLR-DOC-QDO.
           EXIT.
       
       FST-KEY-QDO SECTION.
       INI-FST-KEY-QDO.
           MOVE -1 TO DOC-COD-TOOC-LEN IN QDO-FLD.
       FIN-FST-KEY-QDO.
           EXIT.
       
       FST-FLD-QDO SECTION.
       INI-FST-FLD-QDO.
       FIN-FST-FLD-QDO.
           EXIT.
       
       PRO-KEY-QDO SECTION.
       INI-PRO-KEY-QDO.
           MOVE FRM-CPIM-AEY TO FRM-NUM-PAGI-ATR IN QDO-FLD.
           PERFORM PRO-IKY-QDO.
           PERFORM PRO-AKY-QDO.
           PERFORM PRO-MKY-QDO.
       FIN-PRO-KEY-QDO.
           EXIT.
       
       PRO-IKY-QDO SECTION.
       INI-PRO-IKY-QDO.
           MOVE FRM-CPIM-AEY TO DOC-COD-TOOC-ATR IN QDO-FLD.
           MOVE FRM-CPIM-AEY TO DOC-COD-AOOC-ATR IN QDO-FLD.
           MOVE FRM-CPIM-AEY TO DOC-COD-CBAN-ATR IN QDO-FLD.
           MOVE FRM-CPIM-AEY TO DOC-COD-AUXI-ATR IN QDO-FLD.
       FIN-PRO-IKY-QDO.
           EXIT.
       
       PRO-MKY-QDO SECTION.
       INI-PRO-MKY-QDO.
       FIN-PRO-MKY-QDO.
           EXIT.
       
       PRO-AKY-QDO SECTION.
       INI-PRO-AKY-QDO.
       FIN-PRO-AKY-QDO.
           EXIT.
       
       UNP-KEY-QDO SECTION.
       INI-UNP-KEY-QDO.
           MOVE FRM-CPIM-UBY TO FRM-NUM-PAGI-ATR IN QDO-FLD.
           PERFORM UNP-IKY-QDO.
           PERFORM UNP-AKY-QDO.
           PERFORM UNP-MKY-QDO.
       FIN-UNP-KEY-QDO.
           EXIT.
       
       UNP-IKY-QDO SECTION.
       INI-UNP-IKY-QDO.
           MOVE FRM-CPIM-UBY TO DOC-COD-TOOC-ATR.
           MOVE FRM-CPIM-UBY TO DOC-COD-AOOC-ATR.
           MOVE FRM-CPIM-UBY TO DOC-COD-CBAN-ATR.
           MOVE FRM-CPIM-UBY TO DOC-COD-AUXI-ATR.
           MOVE FRM-CPIM-UBY TO FRM-NUM-PAGI-ATR.
       FIN-UNP-IKY-QDO.
           EXIT.
       
       UNP-MKY-QDO SECTION.
       INI-UNP-MKY-QDO.
       FIN-UNP-MKY-QDO.
           EXIT.
       
       UNP-AKY-QDO SECTION.
       INI-UNP-AKY-QDO.
       FIN-UNP-AKY-QDO.
           EXIT.
       
       UNP-FLD-QDO SECTION.
       INI-UNP-FLD-QDO.
           PERFORM UNP-DOC-QDO.
       FIN-UNP-FLD-QDO.
           EXIT.
       
       UNP-DOC-QDO SECTION.
       INI-UNP-DOC-QDO.
       FIN-UNP-DOC-QDO.
           EXIT.
       
       VAL-KEY-QDO SECTION.
       INI-VAL-KEY-QDO.
           PERFORM VAL-IKY-QDO.
           PERFORM VAL-MKY-QDO.
           PERFORM VAL-AKY-QDO.
       FIN-VAL-KEY-QDO.
           EXIT.
       
       VAL-IKY-QDO SECTION.
       INI-VAL-IKY-QDO.
           PERFORM VAL-DOC-COD-TOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DOC-COD-TOOC-LEN IN QDO-FLD
                GO TO FIN-VAL-IKY-QDO.
           PERFORM VAL-DOC-COD-AOOC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DOC-COD-AOOC-LEN IN QDO-FLD
                GO TO FIN-VAL-IKY-QDO.
           PERFORM VAL-DOC-COD-CBAN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DOC-COD-CBAN-LEN IN QDO-FLD
                GO TO FIN-VAL-IKY-QDO.
           PERFORM VAL-DOC-COD-AUXI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DOC-COD-AUXI-LEN IN QDO-FLD
                GO TO FIN-VAL-IKY-QDO.
           PERFORM VAL-FRM-NUM-PAGI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-NUM-PAGI-LEN IN QDO-FLD
                GO TO FIN-VAL-IKY-QDO.
       FIN-VAL-IKY-QDO.
           EXIT.
       
       VAL-MKY-QDO SECTION.
       INI-VAL-MKY-QDO.
       FIN-VAL-MKY-QDO.
           EXIT.
       
       VAL-AKY-QDO SECTION.
       INI-VAL-AKY-QDO.
       FIN-VAL-AKY-QDO.
           EXIT.
       
       VAL-DOC-COD-TOOC SECTION.
       INI-VAL-DOC-COD-TOOC.
           IF DOC-COD-TOOC IN QDO-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-DOC-COD-TOOC.
       FIN-VAL-DOC-COD-TOOC.
           EXIT.
       
       VAL-DOC-COD-AOOC SECTION.
       INI-VAL-DOC-COD-AOOC.
           IF DOC-COD-AOOC IN QDO-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-DOC-COD-AOOC.
       FIN-VAL-DOC-COD-AOOC.
           EXIT.
       
       VAL-DOC-COD-CBAN SECTION.
       INI-VAL-DOC-COD-CBAN.
           IF DOC-COD-CBAN IN QDO-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-DOC-COD-CBAN.
       FIN-VAL-DOC-COD-CBAN.
           EXIT.
       
       VAL-DOC-COD-AUXI SECTION.
       INI-VAL-DOC-COD-AUXI.
           IF DOC-COD-AUXI IN QDO-FLD NOT > SPACES
              MOVE SPACES TO DOC-COD-AUXI IN QDO-FLD.
       FIN-VAL-DOC-COD-AUXI.
           EXIT.
       
       VAL-FRM-NUM-PAGI SECTION.
       INI-VAL-FRM-NUM-PAGI.
       FIN-VAL-FRM-NUM-PAGI.
           EXIT.
       
       VAL-CON-KEY-QDO SECTION.
       INI-VAL-CON-KEY-QDO.
           IF     DOC-COD-TOOC IN QDO-FLD NOT > SPACES 
              AND DOC-COD-AOOC IN QDO-FLD NOT > SPACES 
              AND DOC-COD-CBAN IN QDO-FLD NOT > SPACES 
              AND DOC-COD-AUXI IN QDO-FLD NOT > SPACES 
              AND FRM-NUM-PAGI IN QDO-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-QDO.
       FIN-VAL-CON-KEY-QDO.
           EXIT.
       
       VAL-CON-IKY-QDO SECTION.
       INI-VAL-CON-IKY-QDO.
           IF     DOC-COD-TOOC IN QDO-FLD NOT > SPACES 
              AND DOC-COD-AOOC IN QDO-FLD NOT > SPACES 
              AND DOC-COD-CBAN IN QDO-FLD NOT > SPACES 
              AND DOC-COD-AUXI IN QDO-FLD NOT > SPACES 
              AND FRM-NUM-PAGI IN QDO-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-QDO.
       FIN-VAL-CON-IKY-QDO.
           EXIT.
       
       VAL-NUM-KEY-QDO SECTION.
       INI-VAL-NUM-KEY-QDO.
           PERFORM VAL-NUM-IKY-QDO.
           PERFORM VAL-NUM-MKY-QDO.
           PERFORM VAL-NUM-AKY-QDO.
       FIN-VAL-NUM-KEY-QDO.
           EXIT.
       
       VAL-NUM-IKY-QDO SECTION.
       INI-VAL-NUM-IKY-QDO.
       VAL-NUM-FRM-NUM-PAGI.
           MOVE FRM-NUM-PAGI-ALF IN QDO-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 6 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO FRM-NUM-PAGI IN QDO-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO FRM-NUM-PAGI-LEN IN QDO-FLD
                GO TO FIN-VAL-NUM-IKY-QDO.
       FIN-VAL-NUM-IKY-QDO.
           EXIT.
       
       VAL-NUM-MKY-QDO SECTION.
       INI-VAL-NUM-MKY-QDO.
       FIN-VAL-NUM-MKY-QDO.
           EXIT.
       
       VAL-NUM-AKY-QDO SECTION.
       INI-VAL-NUM-AKY-QDO.
       FIN-VAL-NUM-AKY-QDO.
           EXIT.
       
       VAL-NUM-FLD-QDO SECTION.
       INI-VAL-NUM-FLD-QDO.
       FIN-VAL-NUM-FLD-QDO.
           EXIT.
       
       EDT-KEY-QDO SECTION.
       INI-EDT-KEY-QDO.
           PERFORM EDT-IKY-QDO.
           PERFORM EDT-MKY-QDO.
           PERFORM EDT-AKY-QDO.
       FIN-EDT-KEY-QDO.
           EXIT.
       
       EDT-IKY-QDO SECTION.
       INI-EDT-IKY-QDO.
           IF FRM-NUM-PAGI-LEN IN QDO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-PAGI-LEN IN QDO-FLD
                GO TO FIN-EDT-IKY-QDO.
           MOVE FRM-NUM-PAGI IN QDO-FLD TO FRM-NUM-PAGI-EDT IN QDO-EDT.
           MOVE FRM-NUM-PAGI-EDT IN QDO-EDT TO 
                FRM-NUM-PAGI-ALF IN QDO-FLD.
       FIN-EDT-IKY-QDO.
           EXIT.
       
       EDT-MKY-QDO SECTION.
       INI-EDT-MKY-QDO.
       FIN-EDT-MKY-QDO.
           EXIT.
       
       EDT-AKY-QDO SECTION.
       INI-EDT-AKY-QDO.
       FIN-EDT-AKY-QDO.
           EXIT.
       
       EDT-FLD-QDO SECTION.
       INI-EDT-FLD-QDO.
       FIN-EDT-FLD-QDO.
           EXIT.
