           EXIT.
       
       VAL-GER-COD-RESU SECTION.
       INI-VAL-GER-COD-RESU.
           IF GER-COD-RESU IN GER-FLD NOT > SPACES
              MOVE SPACES TO GER-COD-RESU IN GER-FLD.
      *VAL-COD Valida codigo de tabla
           MOVE 'GRD' TO TAB-COD-SIST.
           MOVE 'RSU ' TO TAB-COD-TTAB IN TAB.
           MOVE GER-COD-RESU IN GER-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              GER-COD-RESU IN GER-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-RESU IN GER-FLD
           ELSE
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-RESU IN GER-FLD.
       FIN-VAL-GER-COD-RESU.
           EXIT.
       
       VAL-FRM-GLS-RESU SECTION.
       INI-VAL-FRM-GLS-RESU.
       FIN-VAL-FRM-GLS-RESU.
           EXIT.
       
       VAL-GER-COD-ATRN SECTION.
       INI-VAL-GER-COD-ATRN.
           IF GER-COD-ATRN IN GER-FLD NOT > SPACES
              MOVE SPACES TO GER-COD-ATRN IN GER-FLD.
       FIN-VAL-GER-COD-ATRN.
           EXIT.
       
       VAL-FRM-GLS-ATRN SECTION.
       INI-VAL-FRM-GLS-ATRN.
       FIN-VAL-FRM-GLS-ATRN.
           EXIT.
       
       VAL-GER-COD-ROLR SECTION.
       INI-VAL-GER-COD-ROLR.
           IF GER-COD-ROLR IN GER-FLD NOT > SPACES
              MOVE SPACES TO GER-COD-ROLR IN GER-FLD.
       FIN-VAL-GER-COD-ROLR.
           EXIT.
       
       VAL-FRM-GLS-ROLR SECTION.
       INI-VAL-FRM-GLS-ROLR.
       FIN-VAL-FRM-GLS-ROLR.
           EXIT.
       
       VAL-CON-FLD-GER SECTION.
       INI-VAL-CON-FLD-GER.
       FIN-VAL-CON-FLD-GER.
           EXIT.
       
       VAL-CON-GER SECTION.
       INI-VAL-CON-GER.
       FIN-VAL-CON-GER.
           EXIT.
       
       VAL-NUM-KEY-GER SECTION.
       INI-VAL-NUM-KEY-GER.
           PERFORM VAL-NUM-IKY-GER.
           PERFORM VAL-NUM-MKY-GER.
           PERFORM VAL-NUM-AKY-GER.
       FIN-VAL-NUM-KEY-GER.
           EXIT.
       
       VAL-NUM-IKY-GER SECTION.
       INI-VAL-NUM-IKY-GER.
       VAL-NUM-GER-HRA-HPRO.
           MOVE GER-HRA-HPRO-ALF IN GER-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 6 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO GER-HRA-HPRO IN GER-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO GER-HRA-HPRO-LEN IN GER-FLD
                GO TO FIN-VAL-NUM-IKY-GER.
       FIN-VAL-NUM-IKY-GER.
           EXIT.
       
       VAL-NUM-MKY-GER SECTION.
       INI-VAL-NUM-MKY-GER.
       FIN-VAL-NUM-MKY-GER.
           EXIT.
       
       VAL-NUM-AKY-GER SECTION.
       INI-VAL-NUM-AKY-GER.
       FIN-VAL-NUM-AKY-GER.
           EXIT.
       
       VAL-NUM-FLD-GER SECTION.
       INI-VAL-NUM-FLD-GER.
       VAL-NUM-GER-HRA-HRIN.
           MOVE GER-HRA-HRIN-ALF IN GER-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 6 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO GER-HRA-HRIN IN GER-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO GER-HRA-HRIN-LEN IN GER-FLD
                GO TO FIN-VAL-NUM-FLD-GER.
       VAL-NUM-GER-HRA-HRFI.
           MOVE GER-HRA-HRFI-ALF IN GER-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 6 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO GER-HRA-HRFI IN GER-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO GER-HRA-HRFI-LEN IN GER-FLD
                GO TO FIN-VAL-NUM-FLD-GER.
       FIN-VAL-NUM-FLD-GER.
           EXIT.
       
       EDT-KEY-GER SECTION.
       INI-EDT-KEY-GER.
           PERFORM EDT-IKY-GER.
           PERFORM EDT-MKY-GER.
           PERFORM EDT-AKY-GER.
       FIN-EDT-KEY-GER.
           EXIT.
       
       EDT-IKY-GER SECTION.
       INI-EDT-IKY-GER.
           IF GER-HRA-HPRO-LEN IN GER-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO GER-HRA-HPRO-LEN IN GER-FLD
                GO TO FIN-EDT-IKY-GER.
           MOVE GER-HRA-HPRO IN GER-FLD TO GER-HRA-HPRO-EDT IN GER-EDT.
           MOVE GER-HRA-HPRO-EDT IN GER-EDT TO 
                GER-HRA-HPRO-ALF IN GER-FLD.
       FIN-EDT-IKY-GER.
           EXIT.
       
       EDT-MKY-GER SECTION.
       INI-EDT-MKY-GER.
       FIN-EDT-MKY-GER.
           EXIT.
       
       EDT-AKY-GER SECTION.
       INI-EDT-AKY-GER.
       FIN-EDT-AKY-GER.
           EXIT.
       
       EDT-FLD-GER SECTION.
       INI-EDT-FLD-GER.
           IF GER-HRA-HRIN-LEN IN GER-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO GER-HRA-HRIN-LEN IN GER-FLD
                GO TO FIN-EDT-FLD-GER.
           MOVE GER-HRA-HRIN IN GER-FLD TO GER-HRA-HRIN-EDT IN GER-EDT.
           MOVE GER-HRA-HRIN-EDT IN GER-EDT TO 
                GER-HRA-HRIN-ALF IN GER-FLD.
           IF GER-HRA-HRFI-LEN IN GER-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO GER-HRA-HRFI-LEN IN GER-FLD
                GO TO FIN-EDT-FLD-GER.
           MOVE GER-HRA-HRFI IN GER-FLD TO GER-HRA-HRFI-EDT IN GER-EDT.
           MOVE GER-HRA-HRFI-EDT IN GER-EDT TO 
                GER-HRA-HRFI-ALF IN GER-FLD.
       FIN-EDT-FLD-GER.
           EXIT.
