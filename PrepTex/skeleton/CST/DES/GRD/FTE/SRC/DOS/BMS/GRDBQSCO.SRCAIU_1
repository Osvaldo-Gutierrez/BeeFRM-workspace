      *
      * GRDBQSCO
      * Version 3.0.1 Nov-2005
      * 20-Jan-06 12:26 PM
      *
       SET-TAG-GRDBQSCO SECTION.
       INI-SET-TAG-GRDBQSCO.
           MOVE
           '<IDNMOD NOM=$GRDBQSCO$ VSN=${VSNBQ}$ VGM=$3.0.1$
      -    'GEN=$20-Jan-06 12:26 PM$ FNC=${FNCBQ}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-GRDBQSCO.
           EXIT.
           
       PUT-ALL-SCO SECTION.
       INI-PUT-ALL-SCO.
       FIN-PUT-ALL-SCO.
           EXIT.
       
       KEY-ALL-SCO SECTION.
       INI-KEY-ALL-SCO.
       FIN-KEY-ALL-SCO.
           EXIT.
       
       SCR-CLRA-SCO SECTION.
       INI-SCR-CLRA-SCO.
           PERFORM CLR-KEY-SCO.
           PERFORM CLR-FLD-SCO.
       FIN-SCR-CLRA-SCO.
           EXIT.
       
       CLR-KEY-SCO SECTION.
       INI-CLR-KEY-SCO.
           MOVE SPACES TO FRM-COD-FORM IN SCO-FLD.
           MOVE ZEROES TO FRM-NUM-PAGI IN SCO-FLD.
           MOVE SPACES TO FRM-GLS-CRIT IN SCO-FLD.
       FIN-CLR-KEY-SCO.
           EXIT.
       
       CLR-FLD-SCO SECTION.
       INI-CLR-FLD-SCO.
           MOVE SPACES TO FRM-COD-IDEN IN SCO-FLD(1).
           MOVE SPACES TO FRM-COD-IDEN IN SCO-FLD(2).
           MOVE SPACES TO FRM-COD-IDEN IN SCO-FLD(3).
           MOVE SPACES TO FRM-COD-IDEN IN SCO-FLD(4).
           MOVE SPACES TO FRM-COD-IDEN IN SCO-FLD(5).
           MOVE SPACES TO FRM-COD-IDEN IN SCO-FLD(6).
           MOVE SPACES TO FRM-COD-IDEN IN SCO-FLD(7).
           MOVE SPACES TO FRM-COD-IDEN IN SCO-FLD(8).
           MOVE SPACES TO FRM-COD-IDEN IN SCO-FLD(9).
           MOVE SPACES TO FRM-COD-IDEN IN SCO-FLD(10).
           MOVE SPACES TO FRM-COD-IDEN IN SCO-FLD(11).
           MOVE SPACES TO FRM-COD-IDEN IN SCO-FLD(12).
           MOVE SPACES TO FRM-COD-IDEN IN SCO-FLD(13).
           MOVE SPACES TO FRM-COD-IDEN IN SCO-FLD(14).
           MOVE SPACES TO FRM-COD-IDEN IN SCO-FLD(15).
           MOVE SPACES TO FRM-COD-IDEN IN SCO-FLD(16).
           MOVE SPACES TO FRM-IND-CONT IN SCO-FLD(1).
           MOVE SPACES TO FRM-IND-CONT IN SCO-FLD(2).
           MOVE SPACES TO FRM-IND-CONT IN SCO-FLD(3).
           MOVE SPACES TO FRM-IND-CONT IN SCO-FLD(4).
           MOVE SPACES TO FRM-IND-CONT IN SCO-FLD(5).
           MOVE SPACES TO FRM-IND-CONT IN SCO-FLD(6).
           MOVE SPACES TO FRM-IND-CONT IN SCO-FLD(7).
           MOVE SPACES TO FRM-IND-CONT IN SCO-FLD(8).
           MOVE SPACES TO FRM-IND-CONT IN SCO-FLD(9).
           MOVE SPACES TO FRM-IND-CONT IN SCO-FLD(10).
           MOVE SPACES TO FRM-IND-CONT IN SCO-FLD(11).
           MOVE SPACES TO FRM-IND-CONT IN SCO-FLD(12).
           MOVE SPACES TO FRM-IND-CONT IN SCO-FLD(13).
           MOVE SPACES TO FRM-IND-CONT IN SCO-FLD(14).
           MOVE SPACES TO FRM-IND-CONT IN SCO-FLD(15).
           MOVE SPACES TO FRM-IND-CONT IN SCO-FLD(16).
           MOVE SPACES TO FRM-GLS-TEXT IN SCO-FLD(1).
           MOVE SPACES TO FRM-GLS-TEXT IN SCO-FLD(2).
           MOVE SPACES TO FRM-GLS-TEXT IN SCO-FLD(3).
           MOVE SPACES TO FRM-GLS-TEXT IN SCO-FLD(4).
           MOVE SPACES TO FRM-GLS-TEXT IN SCO-FLD(5).
           MOVE SPACES TO FRM-GLS-TEXT IN SCO-FLD(6).
           MOVE SPACES TO FRM-GLS-TEXT IN SCO-FLD(7).
           MOVE SPACES TO FRM-GLS-TEXT IN SCO-FLD(8).
           MOVE SPACES TO FRM-GLS-TEXT IN SCO-FLD(9).
           MOVE SPACES TO FRM-GLS-TEXT IN SCO-FLD(10).
           MOVE SPACES TO FRM-GLS-TEXT IN SCO-FLD(11).
           MOVE SPACES TO FRM-GLS-TEXT IN SCO-FLD(12).
           MOVE SPACES TO FRM-GLS-TEXT IN SCO-FLD(13).
           MOVE SPACES TO FRM-GLS-TEXT IN SCO-FLD(14).
           MOVE SPACES TO FRM-GLS-TEXT IN SCO-FLD(15).
           MOVE SPACES TO FRM-GLS-TEXT IN SCO-FLD(16).
       FIN-CLR-FLD-SCO.
           EXIT.
       
       FST-KEY-SCO SECTION.
       INI-FST-KEY-SCO.
           MOVE -1 TO FRM-COD-FORM-LEN IN SCO-FLD.
       FIN-FST-KEY-SCO.
           EXIT.
       
       FST-FLD-SCO SECTION.
       INI-FST-FLD-SCO.
       FIN-FST-FLD-SCO.
           EXIT.
       
       PRO-KEY-SCO SECTION.
       INI-PRO-KEY-SCO.
           MOVE FRM-CPIM-AEY TO FRM-COD-FORM-ATR IN SCO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-NUM-PAGI-ATR IN SCO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-CRIT-ATR IN SCO-FLD.
           PERFORM PRO-IKY-SCO.
           PERFORM PRO-AKY-SCO.
           PERFORM PRO-MKY-SCO.
       FIN-PRO-KEY-SCO.
           EXIT.
       
       PRO-IKY-SCO SECTION.
       INI-PRO-IKY-SCO.
       FIN-PRO-IKY-SCO.
           EXIT.
       
       PRO-MKY-SCO SECTION.
       INI-PRO-MKY-SCO.
       FIN-PRO-MKY-SCO.
           EXIT.
       
       PRO-AKY-SCO SECTION.
       INI-PRO-AKY-SCO.
       FIN-PRO-AKY-SCO.
           EXIT.
       
       UNP-KEY-SCO SECTION.
       INI-UNP-KEY-SCO.
           MOVE FRM-CPIM-UBY TO FRM-COD-FORM-ATR IN SCO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-NUM-PAGI-ATR IN SCO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-CRIT-ATR IN SCO-FLD.
           PERFORM UNP-IKY-SCO.
           PERFORM UNP-AKY-SCO.
           PERFORM UNP-MKY-SCO.
       FIN-UNP-KEY-SCO.
           EXIT.
       
       UNP-IKY-SCO SECTION.
       INI-UNP-IKY-SCO.
           MOVE FRM-CPIM-UBY TO FRM-COD-FORM-ATR.
           MOVE FRM-CPIM-UBY TO FRM-NUM-PAGI-ATR.
           MOVE FRM-CPIM-UBY TO FRM-GLS-CRIT-ATR.
       FIN-UNP-IKY-SCO.
           EXIT.
       
       UNP-MKY-SCO SECTION.
       INI-UNP-MKY-SCO.
       FIN-UNP-MKY-SCO.
           EXIT.
       
       UNP-AKY-SCO SECTION.
       INI-UNP-AKY-SCO.
       FIN-UNP-AKY-SCO.
           EXIT.
       
       UNP-FLD-SCO SECTION.
       INI-UNP-FLD-SCO.
       FIN-UNP-FLD-SCO.
           EXIT.
       
       VAL-KEY-SCO SECTION.
       INI-VAL-KEY-SCO.
           PERFORM VAL-IKY-SCO.
           PERFORM VAL-MKY-SCO.
           PERFORM VAL-AKY-SCO.
       FIN-VAL-KEY-SCO.
           EXIT.
       
       VAL-IKY-SCO SECTION.
       INI-VAL-IKY-SCO.
           PERFORM VAL-FRM-COD-FORM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-FORM-LEN IN SCO-FLD
                GO TO FIN-VAL-IKY-SCO.
           PERFORM VAL-FRM-NUM-PAGI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-NUM-PAGI-LEN IN SCO-FLD
                GO TO FIN-VAL-IKY-SCO.
           PERFORM VAL-FRM-GLS-CRIT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-CRIT-LEN IN SCO-FLD
                GO TO FIN-VAL-IKY-SCO.
       FIN-VAL-IKY-SCO.
           EXIT.
       
       VAL-MKY-SCO SECTION.
       INI-VAL-MKY-SCO.
       FIN-VAL-MKY-SCO.
           EXIT.
       
       VAL-AKY-SCO SECTION.
       INI-VAL-AKY-SCO.
       FIN-VAL-AKY-SCO.
           EXIT.
       
       VAL-FRM-COD-FORM SECTION.
       INI-VAL-FRM-COD-FORM.
           IF FRM-COD-FORM IN SCO-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-FRM-COD-FORM.
       FIN-VAL-FRM-COD-FORM.
           EXIT.
       
       VAL-FRM-NUM-PAGI SECTION.
       INI-VAL-FRM-NUM-PAGI.
       FIN-VAL-FRM-NUM-PAGI.
           EXIT.
       
       VAL-FRM-GLS-CRIT SECTION.
       INI-VAL-FRM-GLS-CRIT.
           IF FRM-GLS-CRIT IN SCO-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-FRM-GLS-CRIT.
       FIN-VAL-FRM-GLS-CRIT.
           EXIT.
       
       VAL-CON-KEY-SCO SECTION.
       INI-VAL-CON-KEY-SCO.
           IF     FRM-COD-FORM IN SCO-FLD NOT > SPACES 
              AND FRM-NUM-PAGI IN SCO-FLD NOT > ZEROES 
              AND FRM-GLS-CRIT IN SCO-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-SCO.
       FIN-VAL-CON-KEY-SCO.
           EXIT.
       
       VAL-CON-IKY-SCO SECTION.
       INI-VAL-CON-IKY-SCO.
           IF     FRM-COD-FORM IN SCO-FLD NOT > SPACES 
              AND FRM-NUM-PAGI IN SCO-FLD NOT > ZEROES 
              AND FRM-GLS-CRIT IN SCO-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-SCO.
       FIN-VAL-CON-IKY-SCO.
           EXIT.
       
       VAL-NUM-KEY-SCO SECTION.
       INI-VAL-NUM-KEY-SCO.
           PERFORM VAL-NUM-IKY-SCO.
           PERFORM VAL-NUM-MKY-SCO.
           PERFORM VAL-NUM-AKY-SCO.
       FIN-VAL-NUM-KEY-SCO.
           EXIT.
       
       VAL-NUM-IKY-SCO SECTION.
       INI-VAL-NUM-IKY-SCO.
       VAL-NUM-FRM-NUM-PAGI.
           MOVE FRM-NUM-PAGI-ALF IN SCO-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 6 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO FRM-NUM-PAGI IN SCO-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO FRM-NUM-PAGI-LEN IN SCO-FLD
                GO TO FIN-VAL-NUM-IKY-SCO.
       FIN-VAL-NUM-IKY-SCO.
           EXIT.
       
       VAL-NUM-MKY-SCO SECTION.
       INI-VAL-NUM-MKY-SCO.
       FIN-VAL-NUM-MKY-SCO.
           EXIT.
       
       VAL-NUM-AKY-SCO SECTION.
       INI-VAL-NUM-AKY-SCO.
       FIN-VAL-NUM-AKY-SCO.
           EXIT.
       
       VAL-NUM-FLD-SCO SECTION.
       INI-VAL-NUM-FLD-SCO.
       FIN-VAL-NUM-FLD-SCO.
           EXIT.
       
       EDT-KEY-SCO SECTION.
       INI-EDT-KEY-SCO.
           PERFORM EDT-IKY-SCO.
           PERFORM EDT-MKY-SCO.
           PERFORM EDT-AKY-SCO.
       FIN-EDT-KEY-SCO.
           EXIT.
       
       EDT-IKY-SCO SECTION.
       INI-EDT-IKY-SCO.
           IF FRM-NUM-PAGI-LEN IN SCO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-NUM-PAGI-LEN IN SCO-FLD
                GO TO FIN-EDT-IKY-SCO.
           MOVE FRM-NUM-PAGI IN SCO-FLD TO FRM-NUM-PAGI-EDT IN SCO-EDT.
           MOVE FRM-NUM-PAGI-EDT IN SCO-EDT TO 
                FRM-NUM-PAGI-ALF IN SCO-FLD.
       FIN-EDT-IKY-SCO.
           EXIT.
       
       EDT-MKY-SCO SECTION.
       INI-EDT-MKY-SCO.
       FIN-EDT-MKY-SCO.
           EXIT.
       
       EDT-AKY-SCO SECTION.
       INI-EDT-AKY-SCO.
       FIN-EDT-AKY-SCO.
           EXIT.
       
       EDT-FLD-SCO SECTION.
       INI-EDT-FLD-SCO.
       FIN-EDT-FLD-SCO.
           EXIT.
