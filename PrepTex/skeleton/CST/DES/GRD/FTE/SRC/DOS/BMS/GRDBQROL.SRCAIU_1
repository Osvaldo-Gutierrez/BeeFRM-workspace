      *
      * GRDBQROL
      * Version 3.0.1 Nov-2005
      * 27-Mar-09 02:41 PM
      *

       SET-TAG-GRDBQROL SECTION.
       INI-SET-TAG-GRDBQROL.
           MOVE
           '<IDNMOD NOM=$GRDBQROL$ VSN=$05.11.28.1$ VGM=$3.0.1$
      -    'GEN=$27-Mar-09 02:41 PM$ FNC=$COBRJUDICIAL$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-GRDBQROL.
           EXIT.
           
       PUT-ALL-ROL SECTION.
       INI-PUT-ALL-ROL.

           PERFORM PUT-ROL-ROL.

       FIN-PUT-ALL-ROL.
           EXIT.
       
       PUT-ROL-ROL SECTION.
       INI-PUT-ROL-ROL.

           IF PGM-STAT-ROL-NUL
               GO TO FIN-PUT-ROL-ROL.
           IF NOT PGM-STAT-ROL-OKS
               PERFORM CLR-ROL-ROL
               GO TO FIN-PUT-ROL-ROL.
           MOVE ROL-CAI-IREC IN ROL TO ROL-CAI-IREC IN ROL-FLD.
           MOVE ROL-IIC-IREC IN ROL TO ROL-IIC-IREC IN ROL-FLD.
           MOVE ROL-CAI-IOPC IN ROL TO ROL-CAI-IOPC IN ROL-FLD.
           MOVE ROL-IIC-IOPC IN ROL TO ROL-IIC-IOPC IN ROL-FLD.
           MOVE ROL-NUM-IDLC IN ROL TO ROL-NUM-IDLC IN ROL-FLD.
           MOVE ROL-CAI-ILIQ IN ROL TO ROL-CAI-ILIQ IN ROL-FLD.
           MOVE ROL-IIC-ILIQ IN ROL TO ROL-IIC-ILIQ IN ROL-FLD.

       FIN-PUT-ROL-ROL.
           EXIT.
       
       KEY-ALL-ROL SECTION.
       INI-KEY-ALL-ROL.

           PERFORM KEY-ROL-ROL.

       FIN-KEY-ALL-ROL.
           EXIT.
       
       KEY-ROL-ROL SECTION.
       INI-KEY-ROL-ROL.

      *<<<< INI_KEY_ROL_ROL
      *>>>>
           MOVE ROL-CAI-IREC IN ROL-FLD TO ROL-CAI-IREC IN ROL.
           MOVE ROL-IIC-IREC IN ROL-FLD TO ROL-IIC-IREC IN ROL.
           MOVE ROL-CAI-IOPC IN ROL-FLD TO ROL-CAI-IOPC IN ROL.
           MOVE ROL-IIC-IOPC IN ROL-FLD TO ROL-IIC-IOPC IN ROL.
           MOVE ROL-NUM-IDLC IN ROL-FLD TO ROL-NUM-IDLC IN ROL.
           MOVE ROL-CAI-ILIQ IN ROL-FLD TO ROL-CAI-ILIQ IN ROL.
           MOVE ROL-IIC-ILIQ IN ROL-FLD TO ROL-IIC-ILIQ IN ROL.

       FIN-KEY-ROL-ROL.
           EXIT.
       
       SCR-CLRA-ROL SECTION.
       INI-SCR-CLRA-ROL.

           PERFORM CLR-KEY-ROL.
           PERFORM CLR-FLD-ROL.

       FIN-SCR-CLRA-ROL.
           EXIT.
       
       CLR-KEY-ROL SECTION.
       INI-CLR-KEY-ROL.

           MOVE SPACES TO ROL-CAI-IREC IN ROL-FLD.
           MOVE ZEROES TO ROL-IIC-IREC IN ROL-FLD.
           MOVE SPACES TO ROL-CAI-IOPC IN ROL-FLD.
           MOVE ZEROES TO ROL-IIC-IOPC IN ROL-FLD.
           MOVE ZEROES TO ROL-NUM-IDLC IN ROL-FLD.
           MOVE SPACES TO ROL-CAI-ILIQ IN ROL-FLD.
           MOVE ZEROES TO ROL-IIC-ILIQ IN ROL-FLD.

       FIN-CLR-KEY-ROL.
           EXIT.
       
       CLR-FLD-ROL SECTION.
       INI-CLR-FLD-ROL.

           PERFORM CLR-ROL-ROL.

       FIN-CLR-FLD-ROL.
           EXIT.
       
       CLR-ROL-ROL SECTION.
       INI-CLR-ROL-ROL.


       FIN-CLR-ROL-ROL.
           EXIT.
       
       FST-KEY-ROL SECTION.
       INI-FST-KEY-ROL.

           MOVE -1 TO ROL-CAI-IREC-LEN IN ROL-FLD.

       FIN-FST-KEY-ROL.
           EXIT.
       
       FST-FLD-ROL SECTION.
       INI-FST-FLD-ROL.


       FIN-FST-FLD-ROL.
           EXIT.
       
       PRO-KEY-ROL SECTION.
       INI-PRO-KEY-ROL.

           PERFORM PRO-IKY-ROL.
           PERFORM PRO-AKY-ROL.
           PERFORM PRO-MKY-ROL.

       FIN-PRO-KEY-ROL.
           EXIT.
       
       PRO-IKY-ROL SECTION.
       INI-PRO-IKY-ROL.

           MOVE FRM-CPIM-AEY TO ROL-CAI-IREC-ATR IN ROL-FLD.
           MOVE FRM-CPIM-AEY TO ROL-IIC-IREC-ATR IN ROL-FLD.
           MOVE FRM-CPIM-AEY TO ROL-CAI-IOPC-ATR IN ROL-FLD.
           MOVE FRM-CPIM-AEY TO ROL-IIC-IOPC-ATR IN ROL-FLD.
           MOVE FRM-CPIM-AEY TO ROL-NUM-IDLC-ATR IN ROL-FLD.
           MOVE FRM-CPIM-AEY TO ROL-CAI-ILIQ-ATR IN ROL-FLD.
           MOVE FRM-CPIM-AEY TO ROL-IIC-ILIQ-ATR IN ROL-FLD.

       FIN-PRO-IKY-ROL.
           EXIT.
       
       PRO-MKY-ROL SECTION.
       INI-PRO-MKY-ROL.


       FIN-PRO-MKY-ROL.
           EXIT.
       
       PRO-AKY-ROL SECTION.
       INI-PRO-AKY-ROL.


       FIN-PRO-AKY-ROL.
           EXIT.
       
       UNP-KEY-ROL SECTION.
       INI-UNP-KEY-ROL.

           PERFORM UNP-IKY-ROL.
           PERFORM UNP-AKY-ROL.
           PERFORM UNP-MKY-ROL.

       FIN-UNP-KEY-ROL.
           EXIT.
       
       UNP-IKY-ROL SECTION.
       INI-UNP-IKY-ROL.

           MOVE FRM-CPIM-UBY TO ROL-CAI-IREC-ATR.
           MOVE FRM-CPIM-UBY TO ROL-IIC-IREC-ATR.
           MOVE FRM-CPIM-UBY TO ROL-CAI-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO ROL-IIC-IOPC-ATR.
           MOVE FRM-CPIM-UBY TO ROL-NUM-IDLC-ATR.
           MOVE FRM-CPIM-UBY TO ROL-CAI-ILIQ-ATR.
           MOVE FRM-CPIM-UBY TO ROL-IIC-ILIQ-ATR.

       FIN-UNP-IKY-ROL.
           EXIT.
       
       UNP-MKY-ROL SECTION.
       INI-UNP-MKY-ROL.


       FIN-UNP-MKY-ROL.
           EXIT.
       
       UNP-AKY-ROL SECTION.
       INI-UNP-AKY-ROL.


       FIN-UNP-AKY-ROL.
           EXIT.
       
       UNP-FLD-ROL SECTION.
       INI-UNP-FLD-ROL.

           PERFORM UNP-ROL-ROL.

       FIN-UNP-FLD-ROL.
           EXIT.
       
       UNP-ROL-ROL SECTION.
       INI-UNP-ROL-ROL.


       FIN-UNP-ROL-ROL.
           EXIT.
       
       VAL-KEY-ROL SECTION.
       INI-VAL-KEY-ROL.
           PERFORM VAL-IKY-ROL.
           PERFORM VAL-MKY-ROL.
           PERFORM VAL-AKY-ROL.
       FIN-VAL-KEY-ROL.
           EXIT.
       
       VAL-IKY-ROL SECTION.
       INI-VAL-IKY-ROL.

           PERFORM VAL-ROL-CAI-IREC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO ROL-CAI-IREC-LEN IN ROL-FLD
                GO TO FIN-VAL-IKY-ROL.
           PERFORM VAL-ROL-IIC-IREC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO ROL-IIC-IREC-LEN IN ROL-FLD
                GO TO FIN-VAL-IKY-ROL.
           PERFORM VAL-ROL-CAI-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO ROL-CAI-IOPC-LEN IN ROL-FLD
                GO TO FIN-VAL-IKY-ROL.
           PERFORM VAL-ROL-IIC-IOPC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO ROL-IIC-IOPC-LEN IN ROL-FLD
                GO TO FIN-VAL-IKY-ROL.
           PERFORM VAL-ROL-NUM-IDLC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO ROL-NUM-IDLC-LEN IN ROL-FLD
                GO TO FIN-VAL-IKY-ROL.
           PERFORM VAL-ROL-CAI-ILIQ.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO ROL-CAI-ILIQ-LEN IN ROL-FLD
                GO TO FIN-VAL-IKY-ROL.
           PERFORM VAL-ROL-IIC-ILIQ.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO ROL-IIC-ILIQ-LEN IN ROL-FLD
                GO TO FIN-VAL-IKY-ROL.

       FIN-VAL-IKY-ROL.
           EXIT.
       
       VAL-MKY-ROL SECTION.
       INI-VAL-MKY-ROL.


       FIN-VAL-MKY-ROL.
           EXIT.
       
       VAL-AKY-ROL SECTION.
       INI-VAL-AKY-ROL.


       FIN-VAL-AKY-ROL.
           EXIT.
       
       VAL-ROL-CAI-IREC SECTION.
       INI-VAL-ROL-CAI-IREC.

           IF ROL-CAI-IREC IN ROL-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-ROL-CAI-IREC.


       FIN-VAL-ROL-CAI-IREC.
           EXIT.
       
       VAL-ROL-IIC-IREC SECTION.
       INI-VAL-ROL-IIC-IREC.

           IF ROL-IIC-IREC IN ROL-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-ROL-IIC-IREC.


       FIN-VAL-ROL-IIC-IREC.
           EXIT.
       
       VAL-ROL-CAI-IOPC SECTION.
       INI-VAL-ROL-CAI-IOPC.

           IF ROL-CAI-IOPC IN ROL-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-ROL-CAI-IOPC.


       FIN-VAL-ROL-CAI-IOPC.
           EXIT.
       
       VAL-ROL-IIC-IOPC SECTION.
       INI-VAL-ROL-IIC-IOPC.

           IF ROL-IIC-IOPC IN ROL-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-ROL-IIC-IOPC.


       FIN-VAL-ROL-IIC-IOPC.
           EXIT.
       
       VAL-ROL-NUM-IDLC SECTION.
       INI-VAL-ROL-NUM-IDLC.



       FIN-VAL-ROL-NUM-IDLC.
           EXIT.
       
       VAL-ROL-CAI-ILIQ SECTION.
       INI-VAL-ROL-CAI-ILIQ.

           IF ROL-CAI-ILIQ IN ROL-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-ROL-CAI-ILIQ.


       FIN-VAL-ROL-CAI-ILIQ.
           EXIT.
       
       VAL-ROL-IIC-ILIQ SECTION.
       INI-VAL-ROL-IIC-ILIQ.

           IF ROL-IIC-ILIQ IN ROL-FLD = ZEROES
                PERFORM ERR-GLS
                GO TO FIN-VAL-ROL-IIC-ILIQ.


       FIN-VAL-ROL-IIC-ILIQ.
           EXIT.
       
       VAL-CON-KEY-ROL SECTION.
       INI-VAL-CON-KEY-ROL.

           IF     ROL-CAI-IREC IN ROL-FLD NOT > SPACES 
              AND ROL-IIC-IREC IN ROL-FLD NOT > ZEROES 
              AND ROL-CAI-IOPC IN ROL-FLD NOT > SPACES 
              AND ROL-IIC-IOPC IN ROL-FLD NOT > ZEROES 
              AND ROL-NUM-IDLC IN ROL-FLD NOT > ZEROES 
              AND ROL-CAI-ILIQ IN ROL-FLD NOT > SPACES 
              AND ROL-IIC-ILIQ IN ROL-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-ROL.

       FIN-VAL-CON-KEY-ROL.
           EXIT.
       
       VAL-CON-IKY-ROL SECTION.
       INI-VAL-CON-IKY-ROL.

           IF     ROL-CAI-IREC IN ROL-FLD NOT > SPACES 
              AND ROL-IIC-IREC IN ROL-FLD NOT > ZEROES 
              AND ROL-CAI-IOPC IN ROL-FLD NOT > SPACES 
              AND ROL-IIC-IOPC IN ROL-FLD NOT > ZEROES 
              AND ROL-NUM-IDLC IN ROL-FLD NOT > ZEROES 
              AND ROL-CAI-ILIQ IN ROL-FLD NOT > SPACES 
              AND ROL-IIC-ILIQ IN ROL-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-ROL.

       FIN-VAL-CON-IKY-ROL.
           EXIT.
       
       VAL-NUM-KEY-ROL SECTION.
       INI-VAL-NUM-KEY-ROL.
           PERFORM VAL-NUM-IKY-ROL.
           PERFORM VAL-NUM-MKY-ROL.
           PERFORM VAL-NUM-AKY-ROL.
       FIN-VAL-NUM-KEY-ROL.
           EXIT.
       
       VAL-NUM-IKY-ROL SECTION.
       INI-VAL-NUM-IKY-ROL.

       VAL-NUM-ROL-IIC-IREC.
           MOVE ROL-IIC-IREC-ALF IN ROL-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO ROL-IIC-IREC IN ROL-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO ROL-IIC-IREC-LEN IN ROL-FLD
                GO TO FIN-VAL-NUM-IKY-ROL.
       VAL-NUM-ROL-IIC-IOPC.
           MOVE ROL-IIC-IOPC-ALF IN ROL-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO ROL-IIC-IOPC IN ROL-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO ROL-IIC-IOPC-LEN IN ROL-FLD
                GO TO FIN-VAL-NUM-IKY-ROL.
       VAL-NUM-ROL-NUM-IDLC.
           MOVE ROL-NUM-IDLC-ALF IN ROL-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO ROL-NUM-IDLC IN ROL-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO ROL-NUM-IDLC-LEN IN ROL-FLD
                GO TO FIN-VAL-NUM-IKY-ROL.
       VAL-NUM-ROL-IIC-ILIQ.
           MOVE ROL-IIC-ILIQ-ALF IN ROL-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO ROL-IIC-ILIQ IN ROL-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO ROL-IIC-ILIQ-LEN IN ROL-FLD
                GO TO FIN-VAL-NUM-IKY-ROL.

       FIN-VAL-NUM-IKY-ROL.
           EXIT.
       
       VAL-NUM-MKY-ROL SECTION.
       INI-VAL-NUM-MKY-ROL.


       FIN-VAL-NUM-MKY-ROL.
           EXIT.
       
       VAL-NUM-AKY-ROL SECTION.
       INI-VAL-NUM-AKY-ROL.


       FIN-VAL-NUM-AKY-ROL.
           EXIT.
       
       VAL-NUM-FLD-ROL SECTION.
       INI-VAL-NUM-FLD-ROL.


       FIN-VAL-NUM-FLD-ROL.
           EXIT.
       
       EDT-KEY-ROL SECTION.
       INI-EDT-KEY-ROL.
           PERFORM EDT-IKY-ROL.
           PERFORM EDT-MKY-ROL.
           PERFORM EDT-AKY-ROL.

       FIN-EDT-KEY-ROL.
           EXIT.
       
       EDT-IKY-ROL SECTION.
       INI-EDT-IKY-ROL.

           IF ROL-IIC-IREC-LEN IN ROL-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO ROL-IIC-IREC-LEN IN ROL-FLD
                GO TO FIN-EDT-IKY-ROL.
           MOVE ROL-IIC-IREC IN ROL-FLD TO ROL-IIC-IREC-EDT IN ROL-EDT.
           MOVE ROL-IIC-IREC-EDT IN ROL-EDT TO 
                ROL-IIC-IREC-ALF IN ROL-FLD.
           IF ROL-IIC-IOPC-LEN IN ROL-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO ROL-IIC-IOPC-LEN IN ROL-FLD
                GO TO FIN-EDT-IKY-ROL.
           MOVE ROL-IIC-IOPC IN ROL-FLD TO ROL-IIC-IOPC-EDT IN ROL-EDT.
           MOVE ROL-IIC-IOPC-EDT IN ROL-EDT TO 
                ROL-IIC-IOPC-ALF IN ROL-FLD.
           IF ROL-NUM-IDLC-LEN IN ROL-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO ROL-NUM-IDLC-LEN IN ROL-FLD
                GO TO FIN-EDT-IKY-ROL.
           MOVE ROL-NUM-IDLC IN ROL-FLD TO ROL-NUM-IDLC-EDT IN ROL-EDT.
           MOVE ROL-NUM-IDLC-EDT IN ROL-EDT TO 
                ROL-NUM-IDLC-ALF IN ROL-FLD.
           IF ROL-IIC-ILIQ-LEN IN ROL-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO ROL-IIC-ILIQ-LEN IN ROL-FLD
                GO TO FIN-EDT-IKY-ROL.
           MOVE ROL-IIC-ILIQ IN ROL-FLD TO ROL-IIC-ILIQ-EDT IN ROL-EDT.
           MOVE ROL-IIC-ILIQ-EDT IN ROL-EDT TO 
                ROL-IIC-ILIQ-ALF IN ROL-FLD.

       FIN-EDT-IKY-ROL.
           EXIT.
       
       EDT-MKY-ROL SECTION.
       INI-EDT-MKY-ROL.


       FIN-EDT-MKY-ROL.
           EXIT.
       
       EDT-AKY-ROL SECTION.
       INI-EDT-AKY-ROL.


       FIN-EDT-AKY-ROL.
           EXIT.
       
       EDT-FLD-ROL SECTION.
       INI-EDT-FLD-ROL.


       FIN-EDT-FLD-ROL.
           EXIT.



