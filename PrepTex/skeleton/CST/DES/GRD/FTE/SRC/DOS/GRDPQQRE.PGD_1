       PROCEDURE DIVISION.
      *===================

       MAIN SECTION.
       INI-MAIN.
           PERFORM INI.
       LUP-MAIN.
           IF SCR-INST = SCR-INST-HCM
                 PERFORM SCR-COM.
           IF SCR-CCPP = 'ACC'
                 PERFORM SCR-ACC
           ELSE
                 MOVE SCR-INST-HCM TO SCR-INST.
           GO TO LUP-MAIN.
       FIN-MAIN.
           EXIT.

       INI SECTION.
       INI-INI.
       COPY GNSBGEIB.
       COPY GNSBGEDB.
           MOVE LOW-VALUES TO QRE-FLD.
           MOVE LOW-VALUES TO QRE-EDT.
           PERFORM GNS-PRO-RCK.
           PERFORM GNS-BUS-VCA.
           MOVE +270 TO SCR-TCMA.
           PERFORM GNS-HDL-SYS.
           PERFORM GNS-HDL-FRM.
           MOVE 'GRDPQQRE' TO FIO-PROG.
           PERFORM GNS-BUS-IDD.
      *PTC-INI
           PERFORM GNS-ERR-QUE.
           PERFORM GET-TSK-TERM.
      *PTC-FIN
      *<<<< INI_FIO                                                     
           IF SCR-OFRM-SRV                                              
              MOVE TSK-TERM-ALF TO QRE-COD-TERM                         
              MOVE TSK-TERM-ALF TO QRE-COD-TERM-MAS                     
                                                                        
           ELSE                                                         
              MOVE EIBTRMID     TO QRE-COD-TERM                         
              MOVE EIBTRMID     TO QRE-COD-TERM-MAS.                    
      *>>>>                                                             
       FIN-INI.
           EXIT.

       COPY GNSBHSYS.
       COPY GNSBHFRM.
       COPY GNSBPRCK.
       COPY GNSBBVCA.
       COPY GNSBBIDD.

       SCR-COM SECTION.
       INI-SCR-COM.
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-CLRA-QRE.
           PERFORM EDT-KEY-QRE.
           PERFORM EDT-FLD-QRE.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-HCM TO SCR-INST.
           MOVE SCR-CMNP     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       XCT-ERR.
           MOVE 'XCTERR    ' TO MSG-COD-MENS.
           MOVE 'GNS'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO SYS-MEN1.
           MOVE SYS-PROG     TO SYS-MEN2.
           MOVE SYS-ABOR     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       COPY GNSBGGBK.
       FIN-SCR-COM.
           EXIT.



       SCR-ACC SECTION.
       INI-SCR-ACC.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ACC
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-ACC.
           PERFORM DESSCRTRL.
      *<<<< PFS_SCR_ACC                                                 
           IF FRM-FFLD = FRM-FFLD-PF6                                   
                    OR   FRM-FFLD-PF5                                   
               PERFORM VAL-NUM-KEY-QRE                                  
               PERFORM VAL-NUM-FLD-QRE                                  
               PERFORM RST-STAT                                         
               PERFORM FND-SCR-KEY THRU FIN-SCR-KEY                     
               IF SCR-STAT NOT = SCR-STAT-ABT                           
                   PERFORM SCR-TRL                                      
               ELSE                                                     
                   PERFORM DESSCRTRL                                    
                   GO TO FIN-SCR-ACC.                                   
      *>>>>
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ACC.
           EXIT.

       BRW-SCR-ACC SECTION.
       INI-BRW-SCR-ACC.
       FIN-BRW-SCR-ACC.
           EXIT.




       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               PERFORM SCR-CLRA-QRE
               PERFORM EDT-KEY-QRE
               PERFORM EDT-FLD-QRE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-QRE
               PERFORM EDT-KEY-QRE
           ELSE
               PERFORM CLR-FLD-QRE
               PERFORM EDT-FLD-QRE.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

       SCR-KEY SECTION.
       INI-SCR-KEY.
           PERFORM SCR-RST.
           PERFORM GET-KEY.
           IF FRM-STAT-ABT
                   MOVE SPACES TO FRM-MENS
                   PERFORM SCR-MSSG
                   GO TO FIN-SCR-KEY.
       FND-SCR-KEY.
           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
           IF FIO-STAT-OKS
               PERFORM PUT-SCR
               MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
               GO TO FIN-SCR-KEY.
      *JSS ELSE
      *        PERFORM CLR-FLD-QRE.
       ERR-SCR-KEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR.
           IF FRM-MENS NOT > SPACES
               IF FIO-STAT-EOF
                   MOVE 'REGEOF    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
               IF FIO-STAT = FIO-STAT-NEX
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
           MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-SCR-KEY.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO QRE-FLD.
           MOVE LOW-VALUES TO QRE-EDT.
       FIN-SCR-RST.
           EXIT.

       GET-KEY SECTION.
       INI-GET-KEY.
           PERFORM SCR-GKEY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-KEY.
           EXIT.

       GET-IKY SECTION.
       INI-GET-IKY.
           PERFORM SCR-GIKY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-IKY.
           EXIT.

       FND-KEY SECTION.
       INI-FND-KEY.
      *<<<< INI_FND_KEY                                                 
           MOVE SPACES TO FRM-MEN1.                                     
           IF FRM-FFLD = FRM-FFLD-PF5                                   
               MOVE PGM-SNUL TO PGM-STAT-REC             
               SUBTRACT 1 FROM SCR-I GIVING QRE-I                       
               IF QRE-I NOT > ZEROES                                    
                   MOVE 'REC PRV' TO MSG-COD-MENS                    
                   GO TO ERR-FND-KEY                                    
               ELSE                                                     
                   GO TO CARGA-QRE                                      
           ELSE                                                         
           IF FRM-FFLD = FRM-FFLD-PF6                                   
               MOVE PGM-SNUL TO PGM-STAT-REC                            
               ADD 1 SCR-I GIVING QRE-I                                 
               IF QRE-I > SCR-IMAX                                      
                   MOVE 'REC NXT' TO MSG-COD-MENS                    
                   GO TO ERR-FND-KEY                                    
               ELSE                                                     
                   GO TO CARGA-QRE.                                     
           GO TO CON-FND-KEY.                                           

      *      MOVE DBC-NUM-ICLI IN QRE-FLD TO DBC-NUM-ICLI IN DBC.
      *      MOVE DBC-IND-ICLI IN QRE-FLD TO DBC-IND-ICLI IN DBC.
      *      MOVE DBC-GLS-ICLI IN QRE-FLD TO DBC-GLS-ICLI IN DBC.
      *      MOVE 'DBC-IDC-ICLI' TO FIO-AKEY.
      *      MOVE FIO-GET-KEY TO FIO-CMND.
      *      PERFORM SGC-FIO-DBC.
      *      IF NOT FIO-STAT-OKS
      *         GO TO FIN-FND-KEY.
      *      MOVE DBC-CAI-ICLI IN DBC TO SCR-CAI-IREC.
      *      MOVE DBC-IIC-ICLI IN DBC TO SCR-IIC-IREC.

      *>>>>
           PERFORM KEY-ALL-QRE.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GRD-FIO-REC.
           IF FIO-STAT-OKS
               MOVE REC-STP-ITRN IN REC TO SCR-UMOD.
           MOVE FIO-STAT TO PGM-STAT-REC.
      *<<<< FIN_FND_KEY                                                 
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-FND-KEY.                                       
       CON-FND-KEY.                                                     
                                                                        
           MOVE ZEROES TO SCR-IMAX.                                     
           MOVE 1 TO SCR-I.                                             
           MOVE '00' TO FIO-STAT.                                       
           PERFORM OBT-DAT-TS.                                          
           MOVE 1 TO SCR-I.                                             
           MOVE SCR-I TO QRE-I.                                         
       CARGA-QRE.                                                       
           MOVE SPACES TO MSG-COD-MENS.                                 
           IF NOT SCR-OFRM-SRV                                          
           PERFORM PUT-TAB-QRE.                                         
           MOVE '00' TO FIO-STAT.                                       
           GO TO FIN-FND-KEY.                                           
      *                                                                 
       ERR-FND-KEY.                                                     
           MOVE SCR-STAT-CON TO SCR-STAT.                               
                                                                        
           MOVE 'GRD'  TO MSG-COD-SIST.                                 
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MEN1.                               
           MOVE '00' TO FIO-STAT.                                       
           GO TO FIN-FND-KEY.                                           
      *>>>>
       FIN-FND-KEY.
           IF FIO-STAT = FIO-STAT-NEX
               MOVE 'S' TO SCR-IERR.
       EXT-FND-KEY.
           EXIT.
 
       PUT-SCR SECTION.
       INI-PUT-SCR.
           IF PGM-STAT-REC-OKS
               MOVE REC-STP-ITRN IN REC TO SCR-UMOD.
           PERFORM PUT-ALL-QRE.
       FIN-PUT-SCR.
           EXIT.

       STP-EXIT SECTION.
       INI-STP-EXIT.
           MOVE SCR-PROG TO PGM-PROG.
           MOVE SCR-NTRN TO PGM-NTRN.

           MOVE SCR-PROV TO SCR-PROG.
           MOVE SCR-NTRV TO SCR-NTRN.

           MOVE PGM-PROG TO SCR-PROV
           MOVE PGM-NTRN TO SCR-NTRV.
       FIN-STP-EXIT.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO SYS-NTRN.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
      *PTC-INI
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE QRE-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO QRE-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GKEY SECTION.
       INI-SCR-GKEY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-QRE
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO QRE-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GKEY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-QRE.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-QRE
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-QRE.
           PERFORM VAL-IKY-QRE.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-MKY-QRE.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-AKY-QRE.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-CON-KEY-QRE.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GKEY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-QRE.
       ERR-SCR-GKEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-QRE.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GKEY.
           PERFORM SCR-MSSG.

       SCR-GIKY SECTION.
       INI-SCR-GIKY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-QRE
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO QRE-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GIKY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-QRE.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-IKY-QRE
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-QRE.
           PERFORM VAL-IKY-QRE.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GIKY.
           PERFORM VAL-CON-IKY-QRE.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GIKY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-QRE.
       ERR-SCR-GIKY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-IKY-QRE.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GIKY.
           PERFORM SCR-MSSG.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
      *<<<< PFS_GET_CON OR PFS_GET_ACC                                  
           IF NOT (FRM-FFLD = FRM-FFLD-PF6                              
                         OR   FRM-FFLD-PF5)                             
      *>>>>
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO QRE-GLS-MEN2
               MOVE SCR-PRMT     TO QRE-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO QRE-FLD.
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO QRE-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE QRE-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO QRE-FLD
      * FLB  7-NOV-1994
               PERFORM CLR-KEY-QRE

               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO QRE-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO QRE-GLS-MENS.
           PERFORM PRO-MKY-QRE.
           PERFORM PRO-AKY-QRE.
           PERFORM UNP-IKY-QRE.
           PERFORM FST-KEY-QRE.
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE QRE-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO QRE-FLD
               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO QRE-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO QRE-GLS-MENS.
           PERFORM UNP-KEY-QRE.
           PERFORM FST-KEY-QRE.
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
           PERFORM PRO-IKY-QRE.
           PERFORM PRO-AKY-QRE.
           PERFORM UNP-MKY-QRE.
           PERFORM UNP-FLD-QRE.
           PERFORM FST-FLD-QRE.
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO QRE-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-QRE.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
      *PTC-INI
           IF SCR-STAT NOT = SCR-STPP
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-QRE
               PERFORM EDT-FLD-QRE
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-QRE
               PERFORM EDT-FLD-QRE
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-QRE.
           PERFORM EDT-FLD-QRE.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

      * PGM-EBR-ALL SECTION.
      * INI-PGM-EBR-ALL.
      *     MOVE 'GNS' TO FIO-SIST.
      *     MOVE FIO-EBR-ALL TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
      * FIN-PGM-EBR-ALL.
      *     EXIT.

       COPY GRDBQQRE.
       COPY GNSBVNUM.
       COPY GRDBFREC.
       COPY GNSBFMSG.
       COPY GNSBGSYS.
       COPY GNSBGDTC.
      *PTC-INI
      *PTC-SUB
      * ISP  10-MAY-2001 13:46:35 
      * COPY GNSBGFRM.
       COPY GNSBG3CF.
      *PTC-FIN
       COPY GNSBGMSG.
       COPY GNSBEKEY.
       COPY GNSBIABT.
       COPY GNSBBTAB.
       COPY GNSBVCOD.
       COPY GNSBVTAB.
       COPY GNSBFTAB.
       COPY GNSBVIND.
       COPY GNSBVMSC.
       COPY GNSBFMSC.
       COPY GNSBVIDC.
       COPY GNSBPVRF.
       COPY GNSBGFEC.
       COPY GNSBGHOY.
      *PTC-INI
       COPY GNSBGQUE.
       COPY GNSBGTSK.
      *PTC-FIN

      *<<<< EOF                                                         
       COPY GRDBFRRD.                                                   
       COPY SGCBFDBC.                                                   
       COPY SGCBFDCE.                                                   
       COPY SGCBFDCP.                                                   
       COPY GNSBEGLS.                                                   
       COPY GNSBGCPT.
       COPY GNSBGPES.
       COPY TABBFOFI.                                                   
       COPY TABBVOFI.                                                   
       COPY TABBBOFI.                                                   
       COPY TABBFUSR.                                                   
       COPY TABBVUSR.                                                   
       COPY TABBBUSR.                                                   
                                      
       CAR-MOR-REC SECTION.                                             
       INI-CAR-MOR-REC.                                                 
           MOVE REC-IND-SCBZ IN QRE-FLD TO REC-IND-SCBZ IN REC.         
      *    MOVE REC-COD-TIPO IN QRE-FLD TO REC-COD-TIPO IN REC.         
      *    MOVE REC-COD-SUBT IN QRE-FLD TO REC-COD-SUBT IN REC.         
      *    MOVE ZEROES                  TO REC-FEC-FINI IN REC.         
           MOVE FIO-GET-NLS             TO FIO-CMND.                    
       LUP-CAR-MOR-REC.                                                 
           MOVE 'REC-KEY-AREC'          TO FIO-AKEY.                    
           PERFORM GRD-FIO-REC.                                         
           IF NOT (FIO-STAT-OKS AND                                     
                REC-IND-SCBZ IN QRE-FLD = REC-IND-SCBZ IN REC )         
      *         REC-IND-SCBZ IN QRE-FLD = REC-IND-SCBZ IN REC AND       
      *         REC-COD-TIPO IN QRE-FLD = REC-COD-TIPO IN REC AND       
      *         REC-COD-SUBT IN QRE-FLD = REC-COD-SUBT IN REC)          
                GO TO FIN-CAR-MOR-REC.                                  
                                                                        
           IF NOT ( REC-COD-TIPO IN QRE-FLD = REC-COD-TIPO IN REC AND 
                    REC-COD-SUBT IN QRE-FLD = REC-COD-SUBT IN REC )     
              GO TO SIG-CAR-MOR-REC.                                    
                                                                        
           IF REC-COD-ECLI IN REC NOT = 'PROCESOCBZA'                   
              GO TO SIG-CAR-MOR-REC.                                    

           MOVE REC-CIC-IREC IN REC TO RRD-CIC-IREC IN RRD.             
           MOVE '00'                TO RRD-COD-TREL IN RRD.             
           MOVE SPACES              TO RRD-CIC-ICLI IN RRD.             
           MOVE FIO-GET-NLS TO FIO-CMND.                                
           PERFORM GRD-FIO-RRD.                                         
           IF  NOT ( FIO-STAT-OKS AND                                   
               RRD-CIC-IREC IN RRD = REC-CIC-IREC IN REC AND            
               RRD-COD-TREL IN RRD = '00' )                             
               GO TO SIG-CAR-MOR-REC.                                   
                                                                        
           MOVE RRD-CIC-ICLI IN RRD TO DBC-CIC-ICLI IN DBC.             
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM SGC-FIO-DBC.              
           IF NOT FIO-STAT-OKS 
                GO TO SIG-CAR-MOR-REC.                                   
                                                                        
           MOVE REC-CIC-IREC IN REC TO QRE-CIC-IREC(QRE-I).             
                                                                        
           MOVE REC-NUM-DINI IN REC TO QRE-NUM-DINI(QRE-I).           
           MOVE REC-NUM-MINI IN REC TO QRE-NUM-MINI(QRE-I).             
           MOVE REC-NUM-SINI IN REC TO QRE-NUM-SINI(QRE-I).             
           MOVE REC-NUM-AINI IN REC TO QRE-NUM-AINI(QRE-I).             
                                                                        
           MOVE DBC-NUM-ICLI IN DBC TO QRE-NUM-ICLI(QRE-I).             
           MOVE DBC-VRF-ICLI IN DBC TO QRE-VRF-ICLI(QRE-I).             
           MOVE DBC-IND-ICLI IN DBC TO QRE-IND-ICLI(QRE-I).             
           MOVE DBC-GLS-ICLI IN DBC TO QRE-GLS-ICLI(QRE-I).             
           MOVE DBC-GLS-NOMC IN DBC TO QRE-GLS-NOMC(QRE-I).             
           MOVE DBC-COD-OFIC IN DBC TO QRE-COD-OFIC(QRE-I).             
           MOVE DBC-COD-OFIC IN DBC TO OFI-COD-OFIC IN OFI.             
           MOVE SPACES              TO QRE-GLS-OFIC(QRE-I).             
           PERFORM BUS-OFI                                              
           IF MSG-COD-MENS NOT > SPACES                                 
              MOVE OFI-GLS-DESC IN OFI TO QRE-GLS-OFIC(QRE-I).          
                                                                        
                                                                        
           MOVE DBC-COD-EJEC IN DBC TO QRE-COD-EJEC(QRE-I).            
           MOVE SPACES              TO QRE-GLS-EJEC(QRE-I)              
           MOVE DBC-COD-EJEC IN DBC TO USR-COD-USER IN USR              
           PERFORM BUS-USR                                              
           IF MSG-COD-MENS NOT > SPACES                                 
              MOVE USR-GLS-DESC IN USR TO QRE-GLS-EJEC(QRE-I).          
                                         
           IF DBC-IND-TICL IN DBC = 'P'                                 
               MOVE DBC-CIC-ICLI IN DBC TO DCP-CIC-ICLI IN DCP          
               MOVE FIO-GET-KEY TO FIO-CMND                             
               PERFORM SGC-FIO-DCP                                      
               MOVE DCP-COD-TBAN IN DCP TO QRE-COD-TBAN(QRE-I)          
           ELSE                                                         
               MOVE DBC-CIC-ICLI IN DBC TO DCE-CIC-ICLI IN DCE          
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM SGC-FIO-DCE  
               MOVE DCE-COD-TBAN IN DCE TO QRE-COD-TBAN(QRE-I).         

           MOVE 'SGC'               TO TAB-COD-SIST FIO-SIST.           
           MOVE 'BAN'               TO TAB-COD-TTAB IN TAB.             
           MOVE QRE-COD-TBAN(QRE-I) TO TAB-COD-CTAB IN TAB.             
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM GNS-FIO-TAB                                          
           IF NOT ( FIO-STAT-OKS AND                                    
              TAB-IND-VIGE IN TAB = 'S' )                               
              MOVE SPACES              TO QRE-GLS-TBAN(QRE-I)           
              MOVE '00'                TO FIO-STAT                      
           ELSE                                                         
              MOVE TAB-GLS-DESC IN TAB TO QRE-GLS-TBAN(QRE-I).          
                                                                        
            PERFORM INC-REG-TS.                                         
                                                                        
       SIG-CAR-MOR-REC.                                                 
            MOVE FIO-GET-NXT TO FIO-CMND.                               
            GO TO LUP-CAR-MOR-REC.                                      
       FIN-CAR-MOR-REC.                                                 
           EXIT.                                                        
                                                                        
       OBT-PARAM SECTION.                                               
       INI-OBT-PARAM.                                                   
       FIN-OBT-PARAM.                                                   
           EXIT.                                                        
                                                                        
       OBT-DAT-TS SECTION.                                              
       INI-OBT-DAT-TS.                                                  
           EXEC CICS HANDLE CONDITION QIDERR(CON-OBT-DAT-TS)            
                                     ITEMERR(CON-OBT-DAT-TS)            
           END-EXEC.                                                    
                                                                        
      *     EXEC CICS DELETEQ TS QUEUE(QRE-NAME-QUE)                    
      *     END-EXEC.                                                   
      *<<<<                                                             
           MOVE QRE-NOM-TSQRE     TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
      *>>>>                                                             
       CON-OBT-DAT-TS.                                                  
           MOVE QRE-SI-IMP TO QRE-IMP.                                  
           PERFORM CLR-VEC-TS VARYING QRE-I FROM 1 BY 1 UNTIL           
                   QRE-I > QRE-IMAX.                                    
           MOVE 1 TO QRE-I.                                             
           PERFORM CAR-MOR-REC.                                         
                                                                        
       NXT-OBT-DAT-TS.                                                  
           IF QRE-IMP = QRE-SI-IMP                                      
               PERFORM WRT-TS.                                          
       FIN-OBT-DAT-TS.                                                  
           EXIT.                                                        
                                                                        
       PUT-TAB-QRE SECTION.                                             
       INI-PUT-TAB-QRE.                                                 
           PERFORM RED-TS.                                              
           PERFORM LLE-DAT-PNT VARYING QRE-I FROM 1 BY 1                
                   UNTIL QRE-I > QRE-IMAX.                              
       FIN-PUT-TAB-QRE.                                                 
           EXIT.                                                        
                                                                        
       LLE-DAT-PNT SECTION.                                             
       INI-LLE-DAT-PNT.                                                 
           MOVE QRE-CIC-IREC(QRE-I) TO FRM-CIC-IREC IN QRE-FLD(QRE-I).  
           MOVE QRE-FEC-FINI(QRE-I) TO FRM-FEC-FINI IN QRE-FLD(QRE-I).  
           MOVE QRE-NUM-ICLI(QRE-I) TO FRM-NUM-ICLI IN QRE-FLD(QRE-I).  
           MOVE QRE-VRF-ICLI(QRE-I) TO FRM-VRF-ICLI IN QRE-FLD(QRE-I).  
           MOVE QRE-GLS-ICLI(QRE-I) TO FRM-GLS-ICLI IN QRE-FLD(QRE-I).  
           MOVE QRE-IND-ICLI(QRE-I) TO FRM-IND-ICLI IN QRE-FLD(QRE-I).  
           MOVE QRE-GLS-NOMC(QRE-I) TO FRM-GLS-NOMC IN QRE-FLD(QRE-I).  
           MOVE QRE-COD-OFIC(QRE-I) TO FRM-COD-OFIC IN QRE-FLD(QRE-I).  
           MOVE QRE-GLS-OFIC(QRE-I) TO FRM-GLS-OFIC IN QRE-FLD(QRE-I).  
           MOVE QRE-COD-EJEC(QRE-I) TO FRM-COD-EJEC IN QRE-FLD(QRE-I).  
           MOVE QRE-GLS-EJEC(QRE-I) TO FRM-GLS-EJEC IN QRE-FLD(QRE-I).  
           MOVE QRE-COD-TBAN(QRE-I) TO FRM-COD-TBAN IN QRE-FLD(QRE-I).  
           MOVE QRE-GLS-TBAN(QRE-I) TO FRM-GLS-TBAN IN QRE-FLD(QRE-I).  
       FIN-LLE-DAT-PNT.                                                 
           EXIT.                                                        
                                                                        
       INC-REG-TS SECTION.                                              
       INI-INC-REG-TS.                                                  
            MOVE QRE-SI-IMP TO QRE-IMP.                                 
      *     IF SCR-OFRM-SRV                                             
      *       MOVE CPR-VAL-CPTO(CPR-I) TO CPR-VAL-CPTO-EDT              
      *       MOVE CPR-VAL-CPTO-EDT TO CPR-VAL-CPTO-ALF(CPR-I).         
           ADD 1 TO QRE-I.                                              
           IF QRE-I > QRE-IMAX                                          
               PERFORM WRT-TS                                           
               PERFORM CLR-VEC-TS VARYING QRE-I FROM 1 BY 1             
                                     UNTIL QRE-I > QRE-IMAX             
               MOVE 1 TO QRE-I                                          
               MOVE QRE-NO-IMP TO QRE-IMP.                              
       FIN-INC-REG-TS.                                                  
           EXIT.                                                        
                                                                        
      *<<<<                                                             
      *>>>>                                                             
                                                                        
       WRT-TS SECTION.                                                  
       INI-WRT-TS.                                                      
           ADD 1 TO SCR-IMAX.                                           
      *     EXEC CICS WRITEQ TS QUEUE(CPR-NAME-QUE)                     
      *                         FROM(CPR-TS)                            
      *                         LENGTH(CPR-LONG-TS)                     
      *     END-EXEC.                                                   
      *<<<<                                                             
           MOVE QRE-NOM-TSQRE  TO QUE-COLA.                             
           MOVE QRE-TS         TO QUE-ITEM.                             
           MOVE QRE-LEN-TSQRE  TO QUE-LITM.                             
           MOVE QUE-PUT        TO QUE-CMND.                             
           PERFORM GNS-PRO-QUE.                                         
      *>>>>                                                             
       FIN-WRT-TS.                                                      
           EXIT.                                                        
                                                                        
       RED-TS SECTION.                                                  
       INI-RED-TS.                                                      
           MOVE QRE-I TO SCR-I.                                         
           EXEC CICS HANDLE CONDITION QIDERR(FIN-RED-TS)                
                                     ITEMERR(FIN-RED-TS)                
           END-EXEC.                                                    
      *     EXEC CICS READQ TS QUEUE(CPR-NAME-QUE)                      
      *                         INTO(CPR-TS)                            
      *                          LENGTH(CPR-LONG-TS)                    
      *                         ITEM(CPR-I)                             
      *     END-EXEC.                                                   
      *<<<<                                                             
           MOVE QRE-NOM-TSQRE      TO QUE-COLA.                         
           MOVE QRE-TS             TO QUE-ITEM.                         
           MOVE QRE-LEN-TSQRE      TO QUE-LITM.                         
           MOVE QRE-I              TO QUE-NITM.                         
           MOVE QUE-GET            TO QUE-CMND.                         
           PERFORM GNS-PRO-QUE.                                         
           MOVE QUE-ITEM       TO QRE-TS.                               
      *>>>>                                                             
       FIN-RED-TS.                                                      
           EXIT.                                                        
                                                                        
       CLR-VEC-TS SECTION.                                              
       INI-CLR-VEC-TS.
           MOVE SPACES TO QRE-CIC-IREC(QRE-I).                          
           MOVE ZEROES TO QRE-FEC-FINI(QRE-I).                          
           MOVE SPACES TO QRE-NUM-ICLI(QRE-I).                          
           MOVE SPACES TO QRE-VRF-ICLI(QRE-I).                          
           MOVE SPACES TO QRE-GLS-ICLI(QRE-I).                          
           MOVE SPACES TO QRE-IND-ICLI(QRE-I).                          
           MOVE SPACES TO QRE-GLS-NOMC(QRE-I).                          
           MOVE SPACES TO QRE-COD-OFIC(QRE-I).                          
           MOVE SPACES TO QRE-GLS-OFIC(QRE-I).                          
           MOVE SPACES TO QRE-COD-EJEC(QRE-I).                          
           MOVE SPACES TO QRE-GLS-EJEC(QRE-I).                          
           MOVE SPACES TO QRE-COD-TBAN(QRE-I).                          
           MOVE SPACES TO QRE-GLS-TBAN(QRE-I).                          
       FIN-CLR-VEC-TS. 
           EXIT.                                                        
                                                                        
       RST-STAT SECTION.                                                
       INI-RST-STAT.                                                    
           MOVE SCR-STPP     TO SCR-STAT.                               
           MOVE SCR-MFLD-NOT TO SCR-MFLD.                               
       FIN-RST-STAT.                                                    
           EXIT.                                                        
                                                                             
      *>>>>                                                             
