*% IF SETTAG
*% FNCBQ = "1710_05"
*% VSNBQ = "01-00"
*% FNCBU = "1710_05"
*% VSNBU = "01-00"
*% END
*% IF FIN_PUT_PDP_PDP
      *<<<< FIN_PUT_PDP_PDP

           IF PDP-COD-GRPO IN PDP-FLD = '* '
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'TODOS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-GLS-GRPO IN PDP-FLD.

           IF PDP-COD-SIST IN PDP-FLD = '* '
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'TODOS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-GLS-SIST IN PDP-FLD.

           IF PDP-COD-CMND IN PDP-FLD = '* '
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'TODOS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-GLS-CMND IN PDP-FLD.
      *>>>> FIN_PUT_PDP_PDP
*% END
*% IF INI_PDP_COD_GRPO 
      *<<<< INI_PDP_COD_GRPO
           IF PDP-COD-GRPO IN PDP-FLD = '* '
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'TODOS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-GLS-GRPO IN PDP-FLD
               GO TO FIN-VAL-PDP-COD-GRPO.

      *>>>> INI_PDP_COD_GRPO 
*% END
*% IF INI_PDP_COD_SIST 
      *<<<< INI_PDP_COD_SIST
           IF PDP-COD-SIST IN PDP-FLD = '* '
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'TODOS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-GLS-SIST IN PDP-FLD
               GO TO FIN-VAL-PDP-COD-SIST.

      *>>>> INI_PDP_COD_SIST 
*% END
*% IF INI_PDP_COD_CMND
      *<<<< INI_PDP_COD_CMND
           IF PDP-COD-CMND IN PDP-FLD = '* '
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'TODOS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-GLS-CMND IN PDP-FLD
               GO TO FIN-VAL-PDP-COD-CMND.

      *>>>> INI_PDP_COD_CMND 
*% END
*% IF PDP_COD_TDPU
      *<<<< PDP_COD_TDPU
           MOVE TAB-CIC-CTAB IN TAB TO SCR-CIC-TDPU.
           IF SCR-CIC-TDPU = 'GG'
                MOVE 'GNS' TO MSG-COD-SIST
                MOVE 'TDPU   INV' TO MSG-COD-MENS
                PERFORM GET-MSG
                MOVE MSG-GLS-DESC TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>> PDP_COD_TDPU
*% END
*% IF VAL_CON_FLD_PDP
      *<<<< VAL_CON_FLD_PDP
           IF SCR-CIC-TDPU = 'GI' AND
              PDP-COD-PROC IN PDP-FLD > SPACES              
                MOVE 'GNS' TO MSG-COD-SIST
                MOVE 'TDPUPROCBLK' TO MSG-COD-MENS
                PERFORM GET-MSG
                MOVE MSG-GLS-DESC TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
           ELSE
           IF SCR-CIC-TDPU = 'GB' AND
              PDP-COD-PROC IN PDP-FLD NOT > SPACES              
                MOVE 'GNS' TO MSG-COD-SIST
                MOVE 'TDPUPROCOBL' TO MSG-COD-MENS
                PERFORM GET-MSG
                MOVE MSG-GLS-DESC TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>> VAL_CON_FLD_PDP
*% END
