*% HDR_CST = FALSE
*% NO_TIENE_TIMESTAMP = TRUE
*% IF WSS
      *<<<< WSS
       COPY GNSWGTSK.
*% IF NOT PGM_VMS
       COPY GNSWGQUE.
       COPY GNSWGTSI.
      *>>>>
*% END
*% END
*% IF WSS
       01  WSS-TSI.
              05 WSS-FRM-GLS-FL01            PIC  X(60).
              05 WSS-FRM-GLS-FL02            PIC  X(60).
              05 WSS-FRM-GLS-FL03            PIC  X(60).
              05 WSS-FRM-GLS-FL04            PIC  X(60).
              05 WSS-FRM-GLS-FL05            PIC  X(60).
              05 WSS-FRM-GLS-FL06            PIC  X(60).
              05 WSS-FRM-GLS-FL07            PIC  X(60).
              05 WSS-FRM-GLS-FL08            PIC  X(60).
              05 WSS-FRM-GLS-FL09            PIC  X(60).
              05 WSS-FRM-GLS-FL10            PIC  X(60).
              05 WSS-FRM-GLS-FL11            PIC  X(60).
              05 WSS-FRM-GLS-FL12            PIC  X(60).
              05 WSS-FRM-GLS-FL13            PIC  X(60).
              05 WSS-FRM-GLS-FL14            PIC  X(60).
              05 WSS-FRM-GLS-FL15            PIC  X(60).
              05 WSS-FRM-GLS-FL16            PIC  X(60).
              05 WSS-FRM-GLS-FL17            PIC  X(60).
       01  WSS-TSI-RED REDEFINES WSS-TSI.
              05 WSS-FRM-COD-TSI OCCURS 1020 TIMES
                                             PIC X(01).
       01  WSS-TSI-IDX                       PIC 9(05).
*% END
*% IF INI_FIO
*% IF NOT PGM_VMS
           PERFORM GNS-ERR-QUE.
*% END
*% END
*% IF EOF
       COPY GNSBGTSK.
      *<<<< EOF
*% IF NOT PGM_VMS
       COPY GNSBGQUE.
      *>>>>
*% END
*% END
*% IF EOF
       COR-TSI SECTION.
       INI-COR-TSI.
           IF WSS-FRM-COD-TSI( WSS-TSI-IDX ) = 'A' OR 'B' OR 'C' OR
                                               'D' OR 'E' OR 'F'
               GO TO FIN-COR-TSI
           ELSE
           IF WSS-FRM-COD-TSI( WSS-TSI-IDX ) = 'G' OR 'H' OR 'I' OR
                                               'J' OR 'K' OR 'L'
               GO TO FIN-COR-TSI
           ELSE
           IF WSS-FRM-COD-TSI( WSS-TSI-IDX ) = 'M' OR 'N' OR 'O' OR
                                               'P' OR
                                               'Q' OR 'R' OR 'S'
               GO TO FIN-COR-TSI
           ELSE
           IF WSS-FRM-COD-TSI( WSS-TSI-IDX ) = 'T' OR 'U' OR 'V' OR
                                               'W' OR 'X' OR 'Y' OR 'Z'
               GO TO FIN-COR-TSI
           ELSE
           IF WSS-FRM-COD-TSI( WSS-TSI-IDX ) = '0' OR '1' OR '2' OR
                                               '3' OR '4' OR '5'
               GO TO FIN-COR-TSI
           ELSE
           IF WSS-FRM-COD-TSI( WSS-TSI-IDX ) = '6' OR '7' OR '8' OR
                                               '9' OR '&' OR '.'
               GO TO FIN-COR-TSI
           ELSE
           IF WSS-FRM-COD-TSI( WSS-TSI-IDX ) = ',' OR '%' OR '*' OR
                                               '#' OR '(' OR ')' OR
                                               ' '
               GO TO FIN-COR-TSI.
           MOVE '~' TO WSS-FRM-COD-TSI( WSS-TSI-IDX ).
       FIN-COR-TSI.
           EXIT.
*% END
