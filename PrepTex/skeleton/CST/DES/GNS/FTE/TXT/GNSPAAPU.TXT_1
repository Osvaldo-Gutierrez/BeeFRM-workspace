*% NO_TIENE_TIMESTAMP = TRUE
*% IF WSS
      * <<<< WSS

*% IF PGM_MVS
       01  UYS-USER     VALUE SPACES    PIC X(12).
*% END
       COPY TABBRUSR.
       COPY GNSWGFEC.
       COPY GNSWGHOY.
       01  WSS-VARI.
           03 WSS-COD-USSP VALUE  'BCADS016    ' PIC X(12).
           03 WSS-IND-USSP VALUE  'N'            PIC X(01).
      * >>>>
*% END
*% IF WSS_DTC
      * <<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-USR-REQA==.
      * >>>>
*% END
*% IF INI_FIO
      * <<<< INI_FIO
           PERFORM UYS-GET-USER.
           IF UYS-USER = WSS-COD-USSP
              MOVE 'S' TO WSS-IND-USSP IN WSS-VARI
           ELSE
              MOVE 'N' TO WSS-IND-USSP IN WSS-VARI.
      * >>>>
*% END
*% IF FIN_HBL_FLD
      * <<<< FIN_HBL_FLD
      *     IF WSS-IND-USSP = 'N'
      *         MOVE 'HBLFLDUSSPN' TO MSG-COD-MENS
      *     ELSE
      *         MOVE 'HBLFLDUSSPS' TO MSG-COD-MENS.
      *     MOVE 'GNS' TO MSG-COD-SIST.
      *     PERFORM GET-MSG.
      *     MOVE MSG-GLS-DESC TO FRM-MENS.
            PERFORM BUS-MSG-USUR.
      * >>>>
*% END
*% IF INI_FND_KEY
      * <<<< INI_FND_KEY
           MOVE SPACES TO RUP.
           PERFORM KEY-ALL-APU.
           MOVE '00'     TO FIO-STAT.
           MOVE FIO-STAT TO PGM-STAT-RUP.
           GO TO FIN-FND-KEY.
      * >>>>
*% END
*% IF INI_CMN_MOD
      * <<<< INI_CMN_MOD
           PERFORM BUS-MSG-USUR.
           MOVE '00' TO FIO-STAT.
           GO TO FIN-CMN-MOD.
      * >>>>
*% END
*% IF FIN_SCR_MOD
      * <<<< FIN_SCR_MOD
           MOVE SPACES TO FRM-MENS
                          APU-GLS-MENS.
      * >>>>
*% END
*% IF EOF
      * <<<< EOF
       BUS-MSG-USUR SECTION.
       INI-BUS-MSG-USUR.
           IF WSS-IND-USSP = 'N'
               MOVE 'HBLKEYUSSPN' TO MSG-COD-MENS
           ELSE
               MOVE 'HBLKEYUSSPS' TO MSG-COD-MENS.
           MOVE 'GNS' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE MSG-GLS-DESC TO APU-GLS-MENS.
       FIN-BUS-MSG-USUR.
           EXIT.

       COPY GNSBGFEC.
       COPY GNSBGHOY.
       COPY TABBVUSR.
       COPY TABBBUSR.
       COPY TABBFUSR.
*% IF PGM_MVS
       UYS-GET-USER SECTION.
       INI-UYS-GET-USER.
           IF SCR-NEMO = 'GNSACTAPU'
                MOVE 'BCADS016' TO UYS-USER
           ELSE
                MOVE 'BCADS017' TO UYS-USER.
       FIN-UYS-GET-USER.
           EXIT.
*% END
      * >>>>
*% END
