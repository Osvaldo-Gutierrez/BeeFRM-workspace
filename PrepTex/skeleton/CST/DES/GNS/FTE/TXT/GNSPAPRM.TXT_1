*%! Codigo adicional para programa
*% IF WSS
      *<<<<
       COPY GNSWGHOY.
      *>>>>
*% END
*% IF INI_FIO
      *<<<<
           IF SCR-SISG NOT > SPACES
               MOVE SCR-SIST TO SCR-SISG.
      *>>>>
*% END
*% IF FND_KEY OR CMN_ING OR INI_CMN_MOD OR INI_CMN_ELI OR INI_CMN_ACC OR CMN_PGM
      *<<<<
           MOVE SCR-SISG TO FIO-SIST.
      *>>>>
*% END
*% IF CMN_ING OR CMN_MOD
      *<<<<
           IF TAB-COD-CTAB IN TAB-FLD > SPACES
               PERFORM GET-FHOY
               MOVE HOY-DHOY TO TAB-NUM-DUMO IN TAB
               MOVE HOY-MHOY TO TAB-NUM-MUMO IN TAB
               MOVE HOY-AHOY TO TAB-NUM-AUMO IN TAB
               MOVE HOY-SHOY TO TAB-NUM-SUMO IN TAB.
      *>>>>
*% END
*% IF EOF_IBM
      *<<<<
       COPY GNSBVIND.
       COPY GNSBVMSC.
       COPY GNSBEGLS.
       COPY GNSBGHOY.
      *>>>>
*% END
*% IF EOF_VAX
      *<<<<
       COPY VALVIG IN DESTXT.
       COPY VALMSC IN DESTXT.
       COPY ERRGLS IN DESTXT.
      *>>>>
*% END
