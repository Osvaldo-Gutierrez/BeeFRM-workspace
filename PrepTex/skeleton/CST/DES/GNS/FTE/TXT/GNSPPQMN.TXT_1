*% INCLUDE 'GNSSRC:MANEJO_VSI.SRC'
*% FRM = "QMN"
*% ENTIDAD = "MNU"
*% KEY_ENTIDAD = "MNU-COD-IMEN"
*% LARGO_KEY_ENTIDAD = 6
*% RELACION_ENTIDAD = "MNU-COD-CMEN"
*% LARGO_RELACION_ENTIDAD = 3
*% NUMERO_LINEAS_TABLA = 15
*% FIO_ENTIDAD = "SIS-FIO-MNU"
*% SIST_ENTIDAD = "GNS"
*% ACEPTE_PF5 = TRUE
*% ACEPTE_PF6 = TRUE
*% ACEPTE_PF7 = TRUE
*% ACEPTE_PF8 = TRUE
*% ORG_ENTIDAD_VSI = TRUE
*% INCLUDE 'GNSSRC:PFSCONSEL.SRC'
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-SISG NOT > SPACES
               MOVE SCR-SIST TO SCR-SISG.
      *>>>>
*% END
*% IF FND_KEY
      *<<<< FND_KEY
           MOVE LOW-VALUE               TO MNU-COD-IMEN IN MNU.
           MOVE MNU-COD-CMEN IN QMN-FLD TO MNU-COD-CMEN IN MNU.
           MOVE 'MNU-COD-IMEN' TO FIO-AKEY.
           MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM SIS-FIO-MNU.
           MOVE FIO-STAT TO PGM-STAT-MNU.
           IF NOT FIO-STAT-OKS
               GO TO FIN-FND-KEY.
           MOVE 'MNU-COD-IMEN' TO FIO-AKEY.
           MOVE FIO-END-BRW TO FIO-CMND.
           PERFORM SIS-FIO-MNU.
           IF MNU-COD-CMEN IN MNU NOT = MNU-COD-CMEN IN QMN-FLD AND
              MNU-COD-CMEN IN QMN-FLD > SPACES
               MOVE PGM-SRNF TO FIO-STAT PGM-STAT-MNU.
           GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       SIS-FIO-MNU SECTION.
       INI-SIS-FIO-MNU.
           MOVE SCR-SISG TO FIO-SIST.
           PERFORM GNS-FIO-MNU.
       FIN-SIS-FIO-MNU.
           EXIT.

       CARGAR-TABLA SECTION.
       INI-CARGAR-TABLA.
           MOVE MNU-NUM-ORDI TO FRM-NUM-ORDI IN QMN-FLD(FRM-IFLD).
           MOVE MNU-COD-OPCI TO FRM-COD-OPCI IN QMN-FLD(FRM-IFLD).
           MOVE MNU-COD-TFUN TO FRM-COD-TFUN IN QMN-FLD(FRM-IFLD).
           MOVE MNU-COD-MENU TO FRM-COD-MENU IN QMN-FLD(FRM-IFLD).
           MOVE MNU-GLS-PROG TO FRM-GLS-PROG IN QMN-FLD(FRM-IFLD).
           MOVE MNU-GLS-TRAN TO FRM-GLS-TRAN IN QMN-FLD(FRM-IFLD).
           MOVE MNU-COD-CMND TO FRM-COD-CMND IN QMN-FLD(FRM-IFLD).
           MOVE MNU-COD-TINF TO FRM-COD-TINF IN QMN-FLD(FRM-IFLD).
           MOVE MNU-GLS-FMEN TO FRM-GLS-FMEN IN QMN-FLD(FRM-IFLD).
       FIN-CARGAR-TABLA.
           EXIT.

       LIMPIAR-TABLA SECTION.
       INI-LIMPIAR-TABLA.
           MOVE SPACES TO FRM-NUM-ORDI IN QMN-FLD(FRM-IFLD).
           MOVE SPACES TO FRM-COD-OPCI IN QMN-FLD(FRM-IFLD).
           MOVE SPACES TO FRM-COD-TFUN IN QMN-FLD(FRM-IFLD).
           MOVE SPACES TO FRM-COD-MENU IN QMN-FLD(FRM-IFLD).
           MOVE SPACES TO FRM-GLS-PROG IN QMN-FLD(FRM-IFLD).
           MOVE SPACES TO FRM-GLS-TRAN IN QMN-FLD(FRM-IFLD).
           MOVE SPACES TO FRM-COD-CMND IN QMN-FLD(FRM-IFLD).
           MOVE SPACES TO FRM-COD-TINF IN QMN-FLD(FRM-IFLD).
           MOVE SPACES TO FRM-GLS-FMEN IN QMN-FLD(FRM-IFLD).
       FIN-LIMPIAR-TABLA.
           EXIT.
      *>>>>
*% END
