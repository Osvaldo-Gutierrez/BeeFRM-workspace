*% IF INI_FIO
      *<<<< INI_FIO
           MOVE FIO-SIST TO MNU-COD-SISF IN MNU
                            MNU-COD-SISO IN MNU
                            MNU-COD-SISM IN MNU.
           IF FIO-AKEY = "MNU-COD-NEMO" OR FIO-AKEY NOT > SPACES
               MOVE "MNU-COD-NEMS" TO FIO-AKEY
           ELSE
           IF FIO-AKEY = "MNU-COD-OPCI"
               MOVE "MNU-COD-OPCS" TO FIO-AKEY
           ELSE
           IF FIO-AKEY = "MNU-COD-IMEN"
               MOVE "MNU-COD-IMES" TO FIO-AKEY.
      *>>>> INI_FIO
*% END
*% IF FIN_FIO
      *<<<< FIN_FIO
           IF NOT ( FIO-SIST = MNU-COD-SISF IN MNU AND
                    FIO-SIST = MNU-COD-SISO IN MNU AND
                    FIO-SIST = MNU-COD-SISM IN MNU )
                  MOVE "23" TO FIO-STAT.
      *>>>> FIN_FIO
*% END
