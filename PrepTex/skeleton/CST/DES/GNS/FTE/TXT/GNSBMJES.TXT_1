*%! Codigo adicional para archivo
*% IF FIN_PUT_BCH_JES
      *<<<< FIN_PUT_BCH_JES
      *BUS-TAB busca glosa de codigo en tabla
           IF FRM-IND-IGIA IN JES-FLD > SPACES
               MOVE 'GNS' TO TAB-COD-SIST
               MOVE 'GIA' TO TAB-COD-TTAB IN TAB
               MOVE FRM-IND-IGIA IN JES-FLD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               IF MSG-COD-MENS NOT > SPACES
                   MOVE TAB-CIC-CTAB IN TAB TO FRM-CIC-IGIA IN JES-FLD
               ELSE
                   MOVE SPACES TO FRM-CIC-IGIA IN JES-FLD
           ELSE
               MOVE SPACES TO FRM-CIC-IGIA IN JES-FLD.

      *BUS-TAB busca glosa de codigo en tabla
           IF FRM-IND-IDEL IN JES-FLD > SPACES
               MOVE 'GNS' TO TAB-COD-SIST
               MOVE 'IDL' TO TAB-COD-TTAB IN TAB
               MOVE FRM-IND-IDEL IN JES-FLD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               IF MSG-COD-MENS NOT > SPACES
                   MOVE TAB-CIC-CTAB IN TAB TO FRM-CIC-IDEL IN JES-FLD
               ELSE
                   MOVE SPACES TO FRM-CIC-IDEL IN JES-FLD
           ELSE
               MOVE SPACES TO FRM-CIC-IDEL IN JES-FLD.

      *BUS-TAB busca glosa de codigo en tabla
           IF FRM-IND-RRPT IN JES-FLD > SPACES
               MOVE 'GNS' TO TAB-COD-SIST
               MOVE 'RRP' TO TAB-COD-TTAB IN TAB
               MOVE FRM-IND-RRPT IN JES-FLD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               IF MSG-COD-MENS NOT > SPACES
                   MOVE TAB-CIC-CTAB IN TAB TO FRM-CIC-RRPT IN JES-FLD
               ELSE
                   MOVE SPACES TO FRM-CIC-RRPT IN JES-FLD
           ELSE
               MOVE SPACES TO FRM-CIC-RRPT IN JES-FLD.

      *BUS-TAB busca glosa de codigo en tabla
           IF FRM-COD-DEST IN JES-FLD > SPACES
               MOVE FRM-COD-DEST IN JES-FLD TO BCH-COD-DEST IN BCH
               IF BCH-COD-IDST IN BCH = 'O'
                   MOVE 'GNS'  TO TAB-COD-SIST FIO-SIST
                   MOVE 'DST'  TO TAB-COD-TTAB IN TAB
                   MOVE SPACES TO TAB-COD-CTAB IN TAB
                   PERFORM GNS-FIO-TAB
                   IF FIO-STAT-OKS AND TAB-COD-DAT1 IN TAB( 1 ) = 'O'
                       MOVE BCH-COD-OFIC IN BCH TO OFI-COD-OFIC IN OFI
                       PERFORM BUS-OFI
                       MOVE OFI-GLS-DESC IN OFI TO FRM-GLS-DEST IN
                                                                JES-FLD
                       IF MSG-COD-MENS NOT > SPACES
                           MOVE OFI-CIC-OFIC IN OFI TO FRM-CIC-DEST
                                                       IN JES-FLD
                       ELSE
                           MOVE SPACES TO FRM-CIC-DEST IN JES-FLD
                   ELSE
                       MOVE 'GNS' TO TAB-COD-SIST
                       MOVE 'DST' TO TAB-COD-TTAB IN TAB
                       MOVE FRM-COD-DEST IN JES-FLD TO TAB-COD-CTAB IN
                                                                    TAB
                       PERFORM BUS-TAB
                       MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-DEST IN
                                                                JES-FLD
                       IF MSG-COD-MENS NOT > SPACES
                           MOVE TAB-CIC-CTAB IN TAB TO FRM-CIC-DEST
                                                       IN JES-FLD
                       ELSE
                           MOVE SPACES TO FRM-CIC-DEST IN JES-FLD
               ELSE
                   MOVE 'GNS' TO TAB-COD-SIST
                   MOVE 'DST' TO TAB-COD-TTAB IN TAB
                   MOVE FRM-COD-DEST IN JES-FLD TO TAB-COD-CTAB IN TAB
                   PERFORM BUS-TAB
                   MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-DEST IN JES-FLD
                   IF MSG-COD-MENS NOT > SPACES
                       MOVE TAB-CIC-CTAB IN TAB TO FRM-CIC-DEST
                                                   IN JES-FLD
                   ELSE
                       MOVE SPACES TO FRM-CIC-DEST IN JES-FLD
           ELSE
               MOVE SPACES TO FRM-CIC-DEST IN JES-FLD.

      *BUS-TAB busca glosa de codigo en tabla
           IF FRM-COD-CLSE IN JES-FLD > SPACES
               MOVE 'GNS' TO TAB-COD-SIST
               MOVE 'CLS' TO TAB-COD-TTAB IN TAB
               MOVE FRM-COD-CLSE IN JES-FLD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-CLSE IN JES-FLD
               IF MSG-COD-MENS NOT > SPACES
                   MOVE TAB-CIC-CTAB IN TAB TO FRM-CIC-CLSE IN JES-FLD
               ELSE
                   MOVE SPACES TO FRM-CIC-CLSE IN JES-FLD
           ELSE
               MOVE SPACES TO FRM-CIC-CLSE IN JES-FLD.

      *BUS-TAB busca glosa de codigo en tabla
           IF FRM-COD-FORM IN JES-FLD > SPACES
               MOVE 'GNS' TO TAB-COD-SIST
               MOVE 'FML' TO TAB-COD-TTAB IN TAB
               MOVE FRM-COD-FORM IN JES-FLD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-FORM IN JES-FLD
               IF MSG-COD-MENS NOT > SPACES
                   MOVE TAB-CIC-CTAB IN TAB TO FRM-CIC-FORM IN JES-FLD
               ELSE
                   MOVE SPACES TO FRM-CIC-FORM IN JES-FLD
           ELSE
               MOVE SPACES TO FRM-CIC-FORM IN JES-FLD.
      *>>>>
*% END
*% IF BCH_COD_PROC
      *<<<< BCH_COD_PROC
      *>>>>
*% END
*% IF FRM_COD_CLVE
      *<<<< FRM_COD_CLVE
           IF FRM-IND-PLNT IN JES-FLD(1) = 'N'
               GO TO FIN-VAL-FRM-COD-CLVE.
           IF FRM-COD-CLVE IN JES-FLD NOT > SPACES AND
              FRM-IND-PLNT IN JES-FLD( 1 ) = 'O'
               PERFORM ERR-GLS.
      *>>>>
*% END
*% IF FRM_IND_IGIA
      *<<<< FRM_IND_IGIA
           IF FRM-IND-PLNT IN JES-FLD(2) = 'N'
               GO TO FIN-VAL-FRM-IND-IGIA.
           IF FRM-IND-IGIA IN JES-FLD NOT > SPACES AND
              FRM-IND-PLNT IN JES-FLD( 2 ) = 'O'
               PERFORM ERR-GLS
               GO TO FIN-VAL-FRM-IND-IGIA.
      *VAL-COD Valida codigo de tabla
           MOVE 'GNS' TO TAB-COD-SIST.
           MOVE 'GIA' TO TAB-COD-TTAB IN TAB.
           MOVE FRM-IND-IGIA IN JES-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT = 'COD    NEX' AND
              FRM-IND-IGIA IN JES-FLD > SPACES
               MOVE TAB-CIC-CTAB IN TAB TO FRM-CIC-IGIA IN JES-FLD
           ELSE
               MOVE SPACES TO FRM-CIC-IGIA IN JES-FLD.
      *>>>>
*% END
*% IF FRM_GLS_GLSA
      *<<<< FRM_GLS_GLSA
           IF FRM-IND-PLNT IN JES-FLD(3) = 'N'
               GO TO FIN-VAL-FRM-GLS-GLSA.
           IF FRM-GLS-GLSA IN JES-FLD NOT > SPACES AND
              FRM-IND-PLNT IN JES-FLD( 3 ) = 'O'
               PERFORM ERR-GLS.
      *>>>>
*% END
*% IF FRM_NUM_NEJM
      *<<<< FRM_NUM_NEJM
           IF FRM-IND-PLNT IN JES-FLD(4) = 'N'
               GO TO FIN-VAL-FRM-NUM-NEJM.
           IF FRM-NUM-NEJM IN JES-FLD NOT > ZEROES AND
              FRM-IND-PLNT IN JES-FLD( 4 ) = 'O'
               PERFORM ERR-GLS.
      *>>>>
*% END
*% IF FRM_IND_IDEL
      *<<<< FRM_IND_IDEL
           IF FRM-IND-PLNT IN JES-FLD(5) = 'N'
               GO TO FIN-VAL-FRM-IND-IDEL.
           IF FRM-IND-IDEL IN JES-FLD NOT > SPACES AND
              FRM-IND-PLNT IN JES-FLD( 5 ) = 'O'
               PERFORM ERR-GLS
               GO TO FIN-VAL-FRM-IND-IDEL.
      *VAL-COD Valida codigo de tabla
           MOVE 'GNS' TO TAB-COD-SIST.
           MOVE 'IDL' TO TAB-COD-TTAB IN TAB.
           MOVE FRM-IND-IDEL IN JES-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT = 'COD    NEX' AND
              FRM-IND-IDEL IN JES-FLD > SPACES
               MOVE TAB-CIC-CTAB IN TAB TO FRM-CIC-IDEL IN JES-FLD
           ELSE
               MOVE SPACES TO FRM-CIC-IDEL IN JES-FLD.
      *>>>>
*% END
*% IF FRM_IND_RRPT
      *<<<< FRM_IND_RRPT
      *VAL-COD Valida codigo de tabla
           MOVE 'GNS' TO TAB-COD-SIST.
           MOVE 'RRP' TO TAB-COD-TTAB IN TAB.
           MOVE FRM-IND-RRPT IN JES-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT = 'COD    NEX' AND
              FRM-IND-RRPT IN JES-FLD > SPACES
               MOVE TAB-CIC-CTAB IN TAB TO FRM-CIC-RRPT IN JES-FLD
           ELSE
               MOVE SPACES TO FRM-CIC-RRPT IN JES-FLD.
      *>>>>
*% END
*% IF FRM_COD_DEST
      *<<<< FRM_COD_DEST
           IF FRM-IND-PLNT IN JES-FLD(6) = 'N'
               GO TO FIN-VAL-FRM-COD-DEST.
           IF FRM-COD-DEST IN JES-FLD NOT > SPACES AND
              FRM-IND-PLNT IN JES-FLD( 6 ) = 'O'
               PERFORM ERR-GLS
               GO TO FIN-VAL-FRM-COD-DEST.
           MOVE FRM-COD-DEST IN JES-FLD TO BCH-COD-DEST IN BCH.
           IF BCH-COD-IDST NOT = 'O'
               MOVE 'GNS' TO TAB-COD-SIST
               MOVE 'DST' TO TAB-COD-TTAB IN TAB
               MOVE FRM-COD-DEST IN JES-FLD TO TAB-COD-CTAB IN TAB
               PERFORM VAL-COD
               IF MSG-COD-MENS NOT = 'COD    NEX' AND
                  FRM-COD-DEST IN JES-FLD > SPACES
                   MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-DEST IN JES-FLD
                   MOVE TAB-CIC-CTAB IN TAB TO FRM-CIC-DEST IN JES-FLD
                   GO TO FIN-VAL-FRM-COD-DEST
               ELSE
                   MOVE SPACES TO FRM-GLS-DEST IN JES-FLD
                   MOVE SPACES TO FRM-CIC-DEST IN JES-FLD
                   GO TO FIN-VAL-FRM-COD-DEST.

           MOVE 'GNS'  TO TAB-COD-SIST FIO-SIST.
           MOVE 'DST'  TO TAB-COD-TTAB IN TAB.
           MOVE SPACES TO TAB-COD-CTAB IN TAB.
           PERFORM GNS-FIO-TAB.
           IF NOT ( FIO-STAT-OKS AND TAB-COD-DAT1 IN TAB( 1 ) = 'O' )
               MOVE 'GNS' TO TAB-COD-SIST
               MOVE 'DST' TO TAB-COD-TTAB IN TAB
               MOVE FRM-COD-DEST IN JES-FLD TO TAB-COD-CTAB IN TAB
               PERFORM VAL-COD
               IF MSG-COD-MENS NOT = 'COD    NEX' AND
                  FRM-COD-DEST IN JES-FLD > SPACES
                   MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-DEST IN JES-FLD
                   MOVE TAB-CIC-CTAB IN TAB TO FRM-CIC-DEST IN JES-FLD
                   GO TO FIN-VAL-FRM-COD-DEST
               ELSE
                   MOVE SPACES TO FRM-GLS-DEST IN JES-FLD
                   MOVE SPACES TO FRM-CIC-DEST IN JES-FLD
                   GO TO FIN-VAL-FRM-COD-DEST.

      *VAL-OFI Valida codigo de Oficina en TABLAS
           MOVE BCH-COD-OFIC IN BCH TO OFI-COD-OFIC IN OFI.
           PERFORM VAL-OFI.
           IF MSG-COD-MENS NOT = SPACES
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
           IF MSG-COD-MENS NOT = 'COD    NEX'
               MOVE OFI-GLS-DESC IN OFI TO FRM-GLS-DEST IN JES-FLD
               MOVE OFI-CIC-OFIC IN OFI TO FRM-CIC-DEST IN JES-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-DEST IN JES-FLD
               MOVE SPACES TO FRM-CIC-DEST IN JES-FLD.
      *>>>>
*% END
*% IF FRM_COD_CLSE
      *<<<< FRM_COD_CLSE
           IF FRM-IND-PLNT IN JES-FLD(7) = 'N'
               GO TO FIN-VAL-FRM-COD-CLSE.
           IF FRM-COD-CLSE IN JES-FLD NOT > SPACES AND
              FRM-IND-PLNT IN JES-FLD( 7 ) = 'O'
               PERFORM ERR-GLS
               GO TO FIN-VAL-FRM-COD-CLSE.
      *VAL-COD Valida codigo de tabla
           MOVE 'GNS' TO TAB-COD-SIST.
           MOVE 'CLS' TO TAB-COD-TTAB IN TAB.
           MOVE FRM-COD-CLSE IN JES-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT = 'COD    NEX' AND
              FRM-COD-CLSE IN JES-FLD > SPACES
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-CLSE IN JES-FLD
               MOVE TAB-CIC-CTAB IN TAB TO FRM-CIC-CLSE IN JES-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-CLSE IN JES-FLD
               MOVE SPACES TO FRM-CIC-CLSE IN JES-FLD.
      *>>>>
*% END
*% IF FRM_COD_FORM
      *<<<< FRM_COD_FORM
           IF FRM-IND-PLNT IN JES-FLD(8) = 'N'
               GO TO FIN-VAL-FRM-COD-FORM.
           IF FRM-COD-FORM IN JES-FLD NOT > SPACES AND
              FRM-IND-PLNT IN JES-FLD( 8 ) = 'O'
               PERFORM ERR-GLS
               GO TO FIN-VAL-FRM-COD-FORM.
      *VAL-COD Valida codigo de tabla
           MOVE 'GNS' TO TAB-COD-SIST.
           MOVE 'FML' TO TAB-COD-TTAB IN TAB.
           MOVE FRM-COD-FORM IN JES-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT = 'COD    NEX' AND
              FRM-COD-FORM IN JES-FLD > SPACES
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-FORM IN JES-FLD
               MOVE TAB-CIC-CTAB IN TAB TO FRM-CIC-FORM IN JES-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-FORM IN JES-FLD
               MOVE SPACES TO FRM-CIC-FORM IN JES-FLD.
      *>>>>
*% END
*% IF FRM_DAT_DATO
      *<<<< FRM_DAT_DATO
      *>>>>
*% END
*% IF VAL_CON_FLD_JES
      *<<<< VAL_CON_FLD_JES
           IF FRM-CIC-CLSE IN JES-FLD NOT = 'Z' AND
              FRM-CIC-CLSE IN JES-FLD > SPACES  AND
              FRM-COD-DEST IN JES-FLD NOT > SPACES
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'CICCLSEDSTOB' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CON-FLD-JES.

           IF FRM-CIC-CLSE IN JES-FLD NOT > SPACES AND
              FRM-COD-DEST IN JES-FLD NOT > SPACES
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'CICCLSEDSTOB' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CON-FLD-JES.

           IF FRM-CIC-CLSE IN JES-FLD = 'Z' AND
              FRM-COD-DEST IN JES-FLD > SPACES
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'CICCLSEDSTBL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2
               MOVE FRM-SUAR-WRN TO FRM-SUAR
               MOVE SPACES TO FRM-COD-DEST IN JES-FLD
               MOVE SPACES TO FRM-GLS-DEST IN JES-FLD
               MOVE SPACES TO FRM-CIC-DEST IN JES-FLD
               GO TO FIN-VAL-CON-FLD-JES.
      *>>>>
*% END
*% IF FRM_DAT_DATO
      *<<<< FRM_DAT_DATO
           IF FRM-IND-ILEE IN JES-FLD( FRM-IFLD ) = '0' OR
              FRM-IND-ILEE IN JES-FLD( FRM-IFLD ) NOT > SPACES
                GO TO FIN-VAL-FRM-DAT-DATO.
           PERFORM GET-GLS-DATO.
           MOVE TAB-GLS-DATA IN TAB TO WSS-TAB-GLS-DATA.
           IF WSS-TAB-GLS-DATA-2 > SPACES
               IF WSS-TAB-GLS-DATA-2 =
                    FRM-DAT-DATO IN JES-FLD(FRM-IFLD)
                    MOVE 'GNS' TO MSG-COD-SIST
                    MOVE 'BAT    DATER' TO MSG-COD-MENS
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    MOVE FRM-SUAR-MAL TO FRM-SUAR
                    GO TO FIN-VAL-FRM-DAT-DATO.
      *<<<< END FRM_DAT_DATO
*% END                
