*%! Codigo adicional para programa
*% NO_TIENE_TIMESTAMP = TRUE
*% IF PGM_MVS OR PGM_DOS
*%! Cuando GENERE este programa para el BCI debe hacerlo en ambiente DTC
*% INCLUDE 'GNSSRC:MANEJO_VSI.SRC'
*%! INCLUDE 'GNSSRC:MANEJO_DTC.SRC'
*% END
*%
*% FRM = "MNU"
*% PADRE = "MNU"
*% KEY_PADRE = "MNU-COD-NEMO"
*% LARGO_KEY_PADRE = 12
*% FIO_PADRE = "SIS-FIO-MNU"
*% SIST_PADRE = "GNS"
*% IF PGM_MVS OR PGM_DOS
*%     ORG_PADRE_VSI = TRUE
*% END
*% KEY_BROWSE_PADRE = "MNU-COD-NEMO"
*%
*% ACEPTE_PF7 = TRUE
*% ACEPTE_PF8 = TRUE
*%
*% INCLUDE 'GNSSRC:PFSCONENT.SRC'
*%!
*% NO_HDR_CST=TRUE
*% IF WSS
      *<<<< WSS
       01  WSS-VARI.
           03 WSS-GLS-FORM.
              05 FILLER                                    PIC X(03).
              05 WSS-COD-FORM                              PIC X(01).
              05 FILLER                                    PIC X(03).

       01  CLF-VARI.
           03 CLF-COD-NEMO                                 PIC X(12).
           03 CLF-COD-IMEN.
              05 CLF-COD-CMEN                              PIC X(03).
              05 CLF-NUM-ORDI                              PIC 9(03).

           03 CLF-VEC-SEPA           VALUE
           '2222222222222222222211111111111111111111'.
              05 CLF-SEPA            OCCURS 40             PIC 9(01).

           03 CLF-VEC-FINI           VALUE
           '121110090807060504030707060605050404030307070707060606060505
      -    '05050404040403030303'.
              05 CLF-FINI            OCCURS 40             PIC 9(02).

           03 CLF-TAB-FICO.
              05 CLF-VEC-FICO        OCCURS 40.
                 07 CLF-FICO         OCCURS 2              PIC 9(02).
           03 CLF-VEC-KEYS.
              05 CLF-KEYS            OCCURS 40             PIC X(12).
           03 CLF-REST                                     PIC 9(01).
           03 CLF-FILA                                     PIC 9(02).
           03 CLF-COLU                                     PIC 9(02).
           03 CLF-SIND.
              05 CLF-I                                     PIC 9(02).
              05 CLF-J                                     PIC 9(02).
              05 CLF-IMAX                                  PIC 9(02).
              05 CLF-LMAX                                  PIC 9(02).
              05 CLF-M                                     PIC 9(02).
              05 CLF-N                                     PIC 9(02).
           03 CLF-STAT               COMP                  PIC 9(09).
              88 CLF-STAT-OKS        VALUE 0.
              88 CLF-STAT-MAL        VALUE 1.
           03 CLF-MENS                                     PIC X(80).

      *>>>>
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-SISG NOT > SPACES
               MOVE SCR-SIST TO SCR-SISG.

*% IF NOT PGM_MCP

           MOVE SCR-SISG     TO FIO-SIST.
           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-UPD      TO FIO-CMND.
           PERFORM GNS-FIO-MNU.

           MOVE "GNS"        TO FIO-SIST.
           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-INP      TO FIO-CMND.
           PERFORM GNS-FIO-MSG.
*% END
*% IF PGM_D20 OR PGM_GRD
           GO TO FIN-INI.
*% END
      *>>>>
*% END
*% IF CMN_ING OR INI_CMN_MOD OR INI_CMN_ELI OR INI_CMN_ACC OR CMN_PGM
      *<<<< CMN_ING OR INI_CMN_MOD OR INI_CMN_ELI OR INI_CMN_ACC OR CMN_PGM
           MOVE SCR-SISG TO FIO-SIST.
      *>>>>
*% END
*% IF FND_KEY
      *<<<< FND_KEY
           IF MNU-COD-NEMO IN MNU-FLD NOT > SPACES
               IF MNU-COD-OPCI IN MNU-FLD > SPACES
                   MOVE SCR-SISG        TO FIO-SIST
                   MOVE 'MNU-COD-OPCI'  TO FIO-AKEY
                   MOVE FIO-GET-KEY TO FIO-CMND
                   PERFORM GNS-FIO-MNU
                   MOVE FIO-STAT TO PGM-STAT-MNU
                   IF NOT FIO-STAT-OKS
                       MOVE FIO-STAT-NEX TO FIO-STAT
                       GO TO FIN-FND-KEY
                   ELSE
                       GO TO FIN-FND-KEY
               ELSE
                   MOVE SCR-SISG        TO FIO-SIST
                   MOVE 'MNU-COD-IMEN'  TO FIO-AKEY
                   MOVE FIO-GET-KEY TO FIO-CMND
                   PERFORM GNS-FIO-MNU
                   MOVE FIO-STAT TO PGM-STAT-MNU
                   IF NOT FIO-STAT-OKS
                       MOVE FIO-STAT-NEX TO FIO-STAT
                       GO TO FIN-FND-KEY
                   ELSE
                       GO TO FIN-FND-KEY.
           MOVE SCR-SISG    TO FIO-SIST.
      *>>>>
*% END
*% IF FIN_HBL_FLD
      *<<<<
           IF SCR-CMND = 'COP'
               PERFORM UNP-IKY-MNU
               PERFORM FST-KEY-MNU.
      *>>>>
*% END
*% IF CMN_ING OR CMN_MOD
      *<<<< CMN_ING OR CMN_MOD
      *    IF MNU-COD-NEMO IN MNU = ZEROES
      *        MOVE MNU-COD-CMEN IN MNU TO MNU-COD-SIST IN MNU.
           IF MNU-COD-NEMO IN MNU = 'SIS'
               MOVE SCR-SISG TO MNU-COD-SIST IN MNU.
      *>>>>
*% END
*% IF FIN_CMN_ING OR FIN_CMN_MOD OR FIN_CMN_ELI
      *<<<< FIN_CMN_ING OR FIN_CMN_MOD
           IF FIO-STAT-OKS
               PERFORM BFR-RFSH.
      *>>>>
*% END
*% IF ABT_CMN_ACC
      *<<<< ABT_CMN_ACC
               IF SCR-CMND = 'CAL'
                   MOVE 'CALCULO NO FUE EFECTUADO' TO FRM-MENS
                   GO TO FIN-CMN-ACC
               ELSE
               IF SCR-CMND = 'COP'
                   MOVE 'REGISTRO NO FUE COPIADO' TO FRM-MENS
                   GO TO FIN-CMN-ACC
               ELSE
      *>>>>
*% END
*% IF INI_CMN_ACC
      *<<<< INI_CMN_ACC
           IF SCR-CMND = 'CAL'
               PERFORM RET-ALL-MNU
               PERFORM CAL-MNU
               MOVE CLF-MENS TO FRM-MENS
           ELSE
           IF SCR-CMND = 'COP'
               PERFORM RET-ALL-MNU
               MOVE FIO-PUT  TO FIO-CMND
               MOVE SCR-SISG TO FIO-SIST
               PERFORM GNS-FIO-MNU
               IF FIO-STAT-OKS
                   PERFORM BFR-RFSH
                   MOVE 'REGISTRO FUE COPIADO' TO FRM-MENS
               ELSE
                   MOVE 'ERROR : REGISTRO NO COPIADO' TO FRM-MENS
           ELSE
               MOVE 'COMANDO NO IMPLEMENTADO' TO FRM-MENS.
           GO TO FIN-CMN-ACC.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       SIS-FIO-MNU SECTION.
       INI-SIS-FIO-MNU.
           MOVE SCR-SISG TO FIO-SIST.
           PERFORM GNS-FIO-MNU.
       FIN-SIS-FIO-MNU.
           EXIT.

       CAL-MNU SECTION.
       INI-CAL-MNU.
           PERFORM INI-CAL.
           PERFORM BUS-MNU.
           IF CLF-STAT-MAL
               GO TO FIN-CAL-MNU.
           PERFORM CAL-POS.
           PERFORM PUT-POS VARYING CLF-I FROM 1 BY 1 UNTIL
                           CLF-I > CLF-IMAX.
       FIN-CAL-MNU.
           EXIT.

       INI-CAL SECTION.
       INI-INI-CAL.
           MOVE 0 TO CLF-IMAX.
           MOVE 0 TO CLF-STAT.
           MOVE 'CALCULO EFECTUADO OK' TO CLF-MENS.
       FIN-INI-CAL.
           EXIT.

       BUS-MNU SECTION.
       INI-BUS-MNU.
           IF MNU-COD-TFUN IN MNU NOT = 'MEN'
                MOVE 1 TO CLF-STAT
                MOVE 'TIPO FUNCION debe ser MEN (Menu)' TO CLF-MENS
                GO TO FIN-BUS-MNU.
           MOVE SPACES              TO MNU-COD-IMEN IN MNU.
           MOVE MNU-COD-MENU IN MNU TO MNU-COD-CMEN IN MNU.
           MOVE MNU-COD-MENU IN MNU TO CLF-COD-CMEN.

           MOVE SCR-SISG        TO FIO-SIST.
           MOVE 'MNU-COD-IMEN'  TO FIO-AKEY.
           MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM GNS-FIO-MNU.
           IF NOT ( FIO-STAT-OKS AND
                    MNU-COD-CMEN IN MNU = CLF-COD-CMEN )
               MOVE 1 TO CLF-STAT
               MOVE 'NO EXISTEN OPCIONES PARA ESTE MENU' TO CLF-MENS
               GO TO FIN-BUS-MNU.
       LUP-BUS-MNU.
           ADD 1 TO CLF-IMAX.
           MOVE MNU-COD-NEMO IN MNU TO CLF-KEYS(CLF-IMAX).

           MOVE SCR-SISG        TO FIO-SIST.
           MOVE 'MNU-COD-IMEN'  TO FIO-AKEY.
           MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM GNS-FIO-MNU.
           IF FIO-STAT-OKS AND MNU-COD-CMEN IN MNU = CLF-COD-CMEN
               GO TO LUP-BUS-MNU.
       FIN-BUS-MNU.
           EXIT.

       CAL-POS SECTION.
       INI-CAL-POS.
           MOVE 0 TO CLF-J.
           MOVE CLF-FINI(CLF-IMAX) TO CLF-FILA.
           IF CLF-IMAX NOT > 10
                MOVE 24 TO CLF-COLU
                PERFORM CAL-FIL VARYING CLF-I FROM 1 BY 1
                                UNTIL CLF-I > CLF-IMAX
                GO TO FIN-CAL-POS.
           DIVIDE CLF-IMAX BY 2 GIVING CLF-LMAX REMAINDER CLF-REST.
           IF CLF-REST NOT = 0
               ADD 1 TO CLF-LMAX.
           MOVE 1 TO CLF-COLU.
           PERFORM CAL-FIL VARYING CLF-I FROM 1 BY 1
                           UNTIL CLF-I > CLF-LMAX.
           MOVE CLF-FINI(CLF-IMAX) TO CLF-FILA.
           DIVIDE CLF-IMAX BY 2 GIVING CLF-LMAX.
           MOVE 41 TO CLF-COLU.
           PERFORM CAL-FIL VARYING CLF-I FROM 1 BY 1
                           UNTIL CLF-I > CLF-LMAX.
       FIN-CAL-POS.
           EXIT.

       CAL-FIL SECTION.
       INI-CAL-FIL.
           ADD 1 TO CLF-J.
           MOVE CLF-FILA TO CLF-FICO(CLF-J, 1)
           MOVE CLF-COLU TO CLF-FICO(CLF-J, 2)
           ADD CLF-SEPA(CLF-IMAX) TO CLF-FILA.
       FIN-CAL-FIL.
           EXIT.

       PUT-POS SECTION.
       INI-PUT-POS.
           MOVE CLF-KEYS(CLF-I) TO MNU-COD-NEMO IN MNU.
           MOVE SCR-SISG        TO FIO-SIST.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM GNS-FIO-MNU.
           MOVE CLF-FICO(CLF-I, 1) TO MNU-NUM-FILA IN MNU.
           MOVE CLF-FICO(CLF-I, 2) TO MNU-NUM-COLU IN MNU.

           MOVE SCR-SISG TO FIO-SIST.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM GNS-FIO-MNU.
           IF NOT FIO-STAT-OKS
               MOVE 1 TO CLF-STAT
               MOVE 'ERROR al ACTUALIZAR FILA/COLUMNA' TO CLF-MENS
               MOVE CLF-IMAX TO CLF-I.
       FIN-PUT-POS.
           EXIT.

       BFR-RFSH SECTION.
       INI-BFR-RFSH.
           MOVE 'MNU-COD-OPCI' TO FIO-AKEY.
           MOVE FIO-BFR-RFH    TO FIO-CMND.
           PERFORM GNS-FIO-MNU.
           MOVE 'MNU-COD-IMEN' TO FIO-AKEY.
           MOVE FIO-BFR-RFH    TO FIO-CMND.
           PERFORM GNS-FIO-MNU.
       FIN-BFR-RFSH.
           EXIT.

      *>>>>
*% END
*%! IF EOF_IBM
*%! INCLUDE 'GNSTXT:GNSCFTAB.TXT'
*%! INCLUDE 'GNSTXT:GNSCFMSG.TXT'
*%! END
