*%! Codigo adicional para GNSPACVT
*% IF SETTAG
*% FNCPQ = "CVTWEB"
*% VSNPQ = "07.02.07"
*% FNCPU = "CVTWEB"
*% VSNPU =  "07.02.07"
*% END
*% NO_HDR_CST = TRUE
*% PGM_PTC = TRUE
*% IF WSS
      *<<<< WSS

      *Entidades de GNS
       COPY GNSBRCVM.
      *>>>> WSS
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
      *Entidades de SGC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CVM-REQA==.
      *>>>>
*% END
*% IF FND_KEY
      *<<<< FND_KEY 
           MOVE FRM-NUM-DDES IN CV3-FLD    TO CVP-NUM-DFFF IN CVP.
           MOVE FRM-NUM-MDES IN CV3-FLD    TO CVP-NUM-MFFF IN CVP.
           MOVE FRM-NUM-SDES IN CV3-FLD    TO CVP-NUM-SFFF IN CVP.
           MOVE FRM-NUM-ADES IN CV3-FLD    TO CVP-NUM-AFFF IN CVP.
           MOVE FRM-HRA-HDES IN CV3-FLD    TO CVP-HRA-HFHF IN CVP.

           MOVE FIO-GET-NLS                TO FIO-CMND.
           MOVE 'CVP-KEY-FUNC'             TO FIO-AKEY. 
           PERFORM GNS-FIO-CVP.
           MOVE FIO-STAT TO PGM-STAT-CVP.
           IF FIO-STAT-OKS
               MOVE CVP-COD-PROG IN CVP    TO CVM-COD-PROG IN CVM
               MOVE CVP-FEC-FPFF IN CVP    TO CVM-FEC-FFFP IN CVM
               MOVE CVP-HRA-HPHF IN CVP    TO CVM-HRA-HPHF IN CVM
               MOVE FIO-GET-KEY            TO FIO-CMND
               MOVE 'CVM-KEY-PROG'         TO FIO-AKEY
               PERFORM GNS-FIO-CVM
               MOVE FIO-STAT TO PGM-STAT-CVM.

           GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
      *>>>> FIN_FND_KEY
*% END
*% IF FIN_SCR_TRL
      *<<<< FIN_SCR_TRL
      *>>>> FIN_SCR_TRL
*% END
*% IF FIN_HBL_FLD
      *<<<< FIN_HBL_FLD
      *>>>> FIN_HBL_FLD
*% END
*% IF SCR_GFLD_VAL_FLD
      *<<<< SCR_GFLD_VAL_FLD
      *>>>>
*% END
*% IF CMN_ING
      *<<<< CMN_ING
      *>>>> CMN_ING
*% END
*% IF FIN_CMN_ING
      *<<<< FIN_CMN_ING
      *>>>> FIN_CMN_ING
*% END
*% IF CMN_MOD_RET
      *<<<< CMN_MOD_RET
      *>>>> CMN_MOD_RET
*% END
*% IF INI_CMN_MOD
      *<<<< INI_CMN_MOD
      *>>>> INI_CMN_MOD
*% END
*% IF CMN_ELI
      *<<<< CMN_ELI
      *>>>> CMN_ELI
*% END
*% IF FIN_CMN_ELI
      *<<<< FIN_CMN_ELI
      *>>>> FIN_CMN_ELI
*% END
*% IF CMN_ACC
      *<<<< CMN_ACC
      *>>>> CMN_ACC
*% END
*% IF EOF_PQ
      *<<<< EOF_PQ

      *>>>> EOF_PQ
*% END
*% IF EOF_PU
      *<<<< EOF_PU
      *>>>> EOF_PU
*% END
*% IF EOF
      *<<<< EOF
      *Entidades de DAP
       COPY GNSBFCVM.
      *>>>>
*% END
