%! WF011.SRCAIU 
      * {TIPO}-INI --> WFCLBBCQ011.SRCAIU
%! PARA TIPO FEC
%!
%!     SIGLA    FORMATO             D M S A
%!     FEC      D M S A             X X X X
%!     DMA      D M A               X X   X
%!     AMD      D M A               X X   X
%!     MSA      M S A                 X X X
%!     SAM      M S A                 X X X
%!     FMA      M A                   X   X
%!     FAM      M A                   X   X
%!     FSA      S A                     X X
%!     FAS      S A                     X X
%!     FDM      D M                 X X
%!     FMD      D M                 X X
%!     FFA      A                         X
%!
% TIPO_{TIPO} = TRUE
% IF MSKIDC THEN
% 	MSK = ""
%       PADR = "           03 "
% 	SVRF1 = "         "
% 	SVRF2 = "             "
% 	SVRFF = "                   "
% ELSE
%	VALU = TRUE
% 	MSK = "MSK-"
%       PADR = "           03 "
% 	SVRF1 = "     "
% 	SVRF2 = "         "
% 	SVRFF = "                   "
% END IF
%!
%! TIPO VRF
%!
% IF TIPO_VRF THEN
{PADR}{RREG}-NUM-{NOMB}{IDX}          PIC X(11).
{PADR}{RREG}-VRF-{NOMB}{IDX}          PIC X(01).

% END IF
%! TIPO IDC
%!
% IF TIPO_IDC THEN
{PADR}{RREG}-IND-{NOMB}{IDX}           PIC X(01).
{PADR}{RREG}-GLS-{NOMB}{IDX}           PIC X(03).
% END IF
%! TIPO FEC
%!
% IF TIPO_FEC
% IF NORMA_CST
{PADR}{CAMP}{IDX}.
{HIJO}{RFRM}-NUM-S{PFEC}{IDX}           PIC 9(02).
{HIJO}{RFRM}-NUM-A{PFEC}{IDX}           PIC 9(02).
{HIJO}{RFRM}-NUM-M{PFEC}{IDX}           PIC 9(02).
{HIJO}{RFRM}-NUM-D{PFEC}{IDX}           PIC 9(02).
% ELSE
{PADR}{CAMP}{IDX}.
{HIJO}{RFRM}-FA-{NOMB}{IDX}           PIC 9(02).
{HIJO}{RFRM}-FM-{NOMB}{IDX}           PIC 9(02).
{HIJO}{RFRM}-FD-{NOMB}{IDX}           PIC 9(04).
% END IF
% END IF
% IF TIPO_DMA OR TIPO_AMD OR TIPO_MSA OR TIPO_SAM OR TIPO_FMA OR TIPO_FAM OR TIPO_FSA OR TIPO_FAS OR TIPO_FDM OR TIPO_FMD OR TIPO_FFA THEN
{PADR}{CAMP}{PNTO}{IDX}.
% IF TIPO_MSA OR TIPO_SAM OR TIPO_FSA OR TIPO_FAS THEN
{HIJO}{RFRM}-NUM-S{PFEC}{IDX}                PIC 9(02).
% END IF
% IF NOT ( TIPO_FDM OR TIPO_FMD ) THEN
{HIJO}{RFRM}-NUM-A{PFEC}{IDX}                PIC 9(02).
% END IF
% IF NOT ( TIPO_FSA OR TIPO_FAS OR TIPO_FFA ) THEN
{HIJO}{RFRM}-NUM-M{PFEC}{IDX}                PIC 9(02).
% END IF
% IF TIPO_DMA OR TIPO_AMD OR TIPO_FDM OR TIPO_FMD THEN
{HIJO}{RFRM}-NUM-D{PFEC}{IDX}                PIC 9(02).
% END IF
% END IF
% TIPO_{TIPO} = FALSE
      * {TIPO}-FIN --> WFCLBBCQ011.SRCAIU
