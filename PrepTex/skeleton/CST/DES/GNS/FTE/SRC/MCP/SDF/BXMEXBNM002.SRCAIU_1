%!              BXMEXBNM002.SRCAIU 
      * {TIPO}-INI --> BXMEXBNM002.SRCAIU 
% TIPO_{TIPO} = TRUE
% IF TIDX > 0 THEN
% INDC1 = "(FRM-IFLD)"
% INDC2 = "(FRM-IFLD)"
% IF TIDX_ALL THEN
% INDC1 = "(FRM-IFLD)1"
% INDC2 = "(FRM-IDX-IFLD)1"
% END IF
% IF TIDX_ADD THEN
% INDC1 = "(FRM-IFLD)"
% INDC2 = "(FRM-IDX-IFLD)"
% END IF
% ELSE
% INDC1 = ""
% INDC2 = ""
% IF TIDX_ALL THEN
% INDC1 = ""
% INDC2 = "(FRM-IDX-IFLD)"
% END IF
% IF TIDX_ADD THEN
% INDC1 = ""
% INDC2 = ""
% END IF
% END IF
%! TIPO OFI
% IF TIPO_OFI THEN
           IF SDF-{RFRM}-IND-{NOMB} IN {PROY}F{FRM} NOT > SPACES
                   MOVE  {RFRM}-IND-{NOMB} IN {FRM}-FLD{INDC1} TO 
                SDF-{RFRM}-IND-{NOMB} IN {PROY}F{FRM}.
           IF SDF-{RFRM}-COD-{NOMB} IN {PROY}F{FRM} NOT > SPACES
                   MOVE  {RFRM}-COD-{NOMB} IN {FRM}-FLD{INDC1} TO 
                SDF-{RFRM}-COD-{NOMB} IN {PROY}F{FRM}.
% ELSE
%! TIPO VRF
% IF TIPO_VRF THEN
% IF NOT TIENE_IDC_{RREG}_IDC_{NOMB} THEN
           IF SDF-{RFRM}-NUM-{NOMB} IN {PROY}F{FRM} NOT > SPACES
                   MOVE  {RFRM}-NUM-{NOMB}-ALF IN {FRM}-FLD{INDC1} TO 
                SDF-{RFRM}-NUM-{NOMB} IN {PROY}F{FRM}.
           IF SDF-{RFRM}-VRF-{NOMB} IN {PROY}F{FRM} NOT > SPACES
                   MOVE  {RFRM}-VRF-{NOMB} IN {FRM}-FLD{INDC1} TO 
                SDF-{RFRM}-VRF-{NOMB} IN {PROY}F{FRM}.
% ELSE
           IF SDF-{RFRM}-IDC-{NOMB} IN {PROY}F{FRM} NOT > SPACES
                   MOVE  {RFRM}-IDC-{NOMB} IN {FRM}-FLD{INDC1} TO 
                SDF-{RFRM}-IDC-{NOMB} IN {PROY}F{FRM}.
% END IF
% ELSE
%! EL TIPO FEC TIENE UN TRATAMIENTO ESPECIFICO, PUES SU APARICION ES MUCHO MAS FRECUENTE QUE EL RESTO
% IF TIPO_FEC
           IF SDF-{RFRM}-FEC-{NOMB} IN {PROY}F{FRM} NOT > SPACES
                   MOVE  {RFRM}-FEC-{NOMB} IN {FRM}-FLD{INDC1} TO 
                SDF-{RFRM}-FEC-{NOMB} IN {PROY}F{FRM}.
% END IF
% END IF
% END IF
% TIPO_{TIPO} = FALSE
      * {TIPO}-FIN --> BXMEXBNM002.SRCAIU
