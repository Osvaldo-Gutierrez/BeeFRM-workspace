%!              BXCLBBCQ001.SRCAIU 
      * {TIPO}-INI --> BXCLBBCQ001.SRCAIU 
% TIPO_{TIPO} = TRUE
% IF TIDX > 0 THEN
% INDC1 = "(FRM-IFLD)"
% INDC2 = ""
% IF TIDX_ALL THEN
% INDC1 = "(FRM-IFLD)1"
% INDC2 = "(FRM-IDX-IFLD)1"
% END IF
% IF TIDX_ADD THEN
% INDC1 = "(FRM-IFLD)"
% INDC2 = "(FRM-IDX-IFLD)"
% END IF
% ELSE
% INDC1 = ""
% INDC2 = ""
% IF TIDX_ALL THEN
% INDC1 = ""
% INDC2 = "(FRM-IDX-IFLD)"
% END IF
% IF TIDX_ADD THEN
% INDC1 = ""
% INDC2 = ""
% END IF
% END IF
%! TIPO OFI
% IF TIPO_OFI THEN
	   MOVE {RFRM}-IND-{NOMB} IN {FRM}-FLD{INDC1} TO 
                MTX-{RREG}-IND-{NOMB} IN MTX-{FRM}-FLD{INDC2}.
           IF {RFRM}-IND-{NOMB}-ATR IN {FRM}-FLD{INDC1} = FRM-CPIM-AEY
              MOVE  "P"   TO {RFRM}-IND-{NOMB}-ATR IN MTX-{FRM}-FLD{INDC1}
           ELSE
              MOVE  "D"   TO {RFRM}-IND-{NOMB}-ATR IN MTX-{FRM}-FLD{INDC1}.

	   MOVE {RFRM}-COD-{NOMB} IN {FRM}-FLD{INDC1} TO 
                MTX-{RREG}-COD-{NOMB} IN MTX-{FRM}-FLD{INDC2}.
           IF {RFRM}-COD-{NOMB}-ATR IN {FRM}-FLD{INDC1} = FRM-CPIM-AEY
              MOVE  "P"   TO {RFRM}-COD-{NOMB}-ATR IN MTX-{FRM}-FLD{INDC1}
           ELSE
              MOVE  "D"   TO {RFRM}-DOC-{NOMB}-ATR IN MTX-{FRM}-FLD{INDC1}.
% ELSE
%! TIPO VRF
% IF TIPO_VRF THEN
% IF NOT TIENE_IDC_{RREG}_IDC_{NOMB} THEN
	   MOVE {RFRM}-NUM-{NOMB} IN {FRM}-FLD{INDC1} TO 
                MTX-{RREG}-NUM-{NOMB} IN MTX-{FRM}-FLD{INDC2}.
           IF  {RFRM}-NUM-{NOMB}-ATR IN {FRM}-FLD{INDC1} = FRM-CPIM-AEY 
                MOVE  "P"  TO MTX-{RREG}-NUM-{NOMB} IN MTX-{FRM}-FLD{INDC1}
           ELSE
                MOVE  "D"  TO MTX-{RREG}-NUM-{NOMB} IN MTX-{FRM}-FLD{INDC1}.

	   MOVE {RFRM}-VRF-{NOMB} IN {FRM}-FLD{INDC1} TO 
                MTX-{RREG}-VRF-{NOMB} IN MTX-{FRM}-FLD{INDC2}.
           IF  {RFRM}-VRF-{NOMB}-ATR IN {FRM}-FLD{INDC1} = FRM-CPIM-AEY 
                MOVE  "P" TO MTX-{RREG}-VRF-{NOMB}-ATR IN MTX-{FRM}-FLD{INDC1}
           ELSE
                MOVE  "D" TO MTX-{RREG}-VRF-{NOMB}-ATR IN MTX-{FRM}-FLD{INDC1}.
% ELSE
	   MOVE {RFRM}-IDC-{NOMB} IN {FRM}-FLD{INDC1} TO 
                MTX-{RREG}-IDC-{NOMB} IN MTX-{FRM}-FLD{INDC2}.
           IF  {RFRM}-IDC-{NOMB}-ATR IN {FRM}-FLD{INDC1}= FRM-CPIM-AEY 
                MOVE  "P" TO MTX-{RREG}-IDC-{NOMB}-ATR IN MTX-{FRM}-FLD{INDC1} 
           ELSE
                MOVE  "D" TO MTX-{RREG}-IDC-{NOMB}-ATR IN MTX-{FRM}-FLD{INDC1}.
% END IF
% ELSE
%! EL TIPO FEC TIENE UN TRATAMIENTO ESPECIFICO, PUES SU APARICION ES MUCHO MAS FRECUENTE QUE EL RESTO
% IF TIPO_FEC
           MOVE {RFRM}-FEC-{NOMB} IN {FRM}-FLD{INDC1} TO 
                MTX-{RREG}-FEC-{NOMB} IN MTX-{FRM}-FLD{INDC2}.
           IF  {RFRM}-FEC-{NOMB}-ATR IN {FRM}-FLD{INDC1} = FRM-CPIM-AEY 
                MOVE "P" TO MTX-{RREG}-FEC-{NOMB}-ATR IN MTX-{FRM}-FLD{INDC1}
           ELSE
                MOVE "D" TO MTX-{RREG}-FEC-{NOMB}-ATR IN MTX-{FRM}-FLD{INDC1}.
% END IF
% END IF
% END IF
% TIPO_{TIPO} = FALSE
      * {TIPO}-FIN --> BXCLBBCQ001.SRCAIU
