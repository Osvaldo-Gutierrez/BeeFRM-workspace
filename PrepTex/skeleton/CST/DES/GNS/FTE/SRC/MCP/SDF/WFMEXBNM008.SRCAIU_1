           03 SDF-{RFRM}-IND-{NOMB}      PIC X(02).
           03 FILLER   VALUES SPACES PIC X(01).
           03 SDF-{RFRM}-COD-{NOMB}      PIC X(08).
% ELSE
% IF TIPO_VRF THEN
% IF NOT TIENE_IDC_{RREG}_IDC_{NOMB} THEN
           03 FILLER   VALUE SPACES PIC X(01).
           03 SDF-{RFRM}-NUM-{NOMB}      PIC X(11).
           03 FILLER   VALUES SPACES PIC X(01).
           03 SDF-{RFRM}-VRF-{NOMB}      PIC X(01).
% END IF
% ELSE
% IF TIPO_IDC THEN
           03 FILLER  VALUE SPACES  PIC X(01).
           03 SDF-{RFRM}-IDC-{NOMB}      PIC X(12).
% END IF
% END IF
% END IF
