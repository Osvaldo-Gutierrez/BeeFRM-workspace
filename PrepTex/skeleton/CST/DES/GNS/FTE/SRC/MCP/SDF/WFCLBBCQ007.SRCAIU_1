%! WF007.SRCAIU 
      * {TIPO}-INI --> WFCLBBCQ007.SRCAIU
% TIPO_{TIPO} = TRUE
%! TIPO VRF
%!
% IF TIPO_VRF THEN
% IF USA_PADRE
              05 {RREG}-NUM-{NOMB}-LEN              PIC X(01).
              05 {RREG}-VRF-{NOMB}-LEN              PIC X(01).
% ELSE
                 07 {RREG}-NUM-{NOMB}-LEN           PIC X(01).
                 07 {RREG}-VRF-{NOMB}-LEN           PIC X(01).
% END
% ELSE
%! TIPO IDC
%!
% IF TIPO_IDC THEN
% IF USA_PADRE
              05 {RREG}-IND-{NOMB}-LEN              PIC X(01).
              05 {RREG}-GLS-{NOMB}-LEN              PIC X(01).
% ELSE
                  07 {RREG}-IND-{NOMB}-LEN         PIC X(01).
                  07 {RREG}-GLS-{NOMB}-LEN         PIC X(01).
% END
% END
% END
% NORMA_CST = FALSE
% TIPO_{TIPO} = FALSE
% USA_PADRE = FALSE
      * {TIPO}-FIN --> WFCLBBCQ007.SRCAIU
