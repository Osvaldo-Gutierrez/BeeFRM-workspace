%!              BMMEXBNM012.SRCAIU RESETEO DE ATRIBUTOS
      * {TIPO}-INI --> BMMEXBNM012.SRCAIU RESETEO DE ATRIBUTOS
%! 
% TIPO_{TIPO} = TRUE
% IF TIPO_OFI THEN
% IF TIDX > 0 THEN
% FOR I = 1 TO {TIDX}
           MOVE SPACES TO {RREG}-IND-{NOMB}-LEN IN {FRM}-FLD({I}).
           MOVE SPACES TO {RREG}-COD-{NOMB}-LEN IN {FRM}-FLD({I}).
% NEXT I
% ELSE
           MOVE SPACES TO {RREG}-IND-{NOMB}-LEN IN {FRM}-FLD.
           MOVE SPACES TO {RREG}-COD-{NOMB}-LEN IN {FRM}-FLD.
% END IF
% ELSE
% IF TIPO_VRF THEN
% IF NOT TIENE_IDC_{RREG}_IDC_{NOMB} THEN
% IF TIDX > 0 THEN
% FOR I = 1 TO {TIDX}
      *     MOVE SPACES TO {RREG}-NUM-{NOMB}-ATR IN {FRM}-FLD({I}).
           MOVE SPACES TO {RREG}-NUM-{NOMB}-LEN IN {FRM}-FLD({I}).
      *     MOVE SPACES TO {RREG}-VRF-{NOMB}-ATR IN {FRM}-FLD({I}).
           MOVE SPACES TO {RREG}-VRF-{NOMB}-LEN IN {FRM}-FLD({I}).
% NEXT I
% ELSE
      *     MOVE SPACES TO {RREG}-NUM-{NOMB}-ATR IN {FRM}-FLD.
           MOVE SPACES TO {RREG}-NUM-{NOMB}-LEN IN {FRM}-FLD.
      *     MOVE SPACES TO {RREG}-VRF-{NOMB}-ATR IN {FRM}-FLD.
           MOVE SPACES TO {RREG}-VRF-{NOMB}-LEN IN {FRM}-FLD.
% END IF
% END IF
% ELSE
% IF TIPO_IDC THEN
% IF TIDX > 0 THEN
% FOR I = 1 TO {TIDX}
           MOVE SPACES TO {RREG}-IDC-{NOMB}-LEN IN {FRM}-FLD({I}).
% NEXT I
% ELSE
           MOVE SPACES TO {RREG}-IDC-{NOMB}-LEN IN {FRM}-FLD.
% END IF
% ELSE
%! TIPO FEC Y SUS DERIVADOS
% IF TIPO_FEC OR TIPO_DMA OR TIPO_AMD OR TIPO_MSA OR TIPO_SAM OR TIPO_FMA OR TIPO_FAM OR TIPO_FSA OR TIPO_FAS OR TIPO_FDM OR TIPO_FMD OR TIPO_FFA THEN
% IF TIDX > 0 THEN
% FOR I = 1 TO {TIDX}
      *     MOVE SPACES TO {CAMP}-ATR IN {FRM}-FLD({I}).
           MOVE SPACES TO {CAMP}-LEN IN {FRM}-FLD({I}).
% NEXT I
% ELSE
      *     MOVE SPACES TO {CAMP}-ATR IN {FRM}-FLD.
           MOVE SPACES TO {CAMP}-LEN IN {FRM}-FLD.
% END IF
% END IF
% END IF
% END IF
% END IF
% TIPO_{TIPO} = FALSE
      * {TIPO}-FIN --> BMMEXBNM012.SRCAIU RESETEO DE ATRIBUTOS
