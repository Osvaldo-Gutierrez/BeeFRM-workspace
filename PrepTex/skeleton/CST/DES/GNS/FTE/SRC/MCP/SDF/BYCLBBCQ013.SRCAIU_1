%! BY013.SRCAIU 
      * {TIPO}-INI --> BYCLBBCQ013.SRCAIU
% TIPO_{TIPO} = TRUE
%! PARA TIPO FEC
%!
%!
% IF NOT DISP THEN
% IF TIPO_VRF THEN
           IF {RFRM}-NUM-{NOMB}-ATR = FRM-CPIM-AEY AND 
               SDF-{RFRM}-NUM-{NOMB} IN {RREG}F{FRM}
               NOT = {RFRM}-NUM-{NOMB}-ALF IN {FRM}-FLD
                   MOVE "S" TO FRM-MODIFICO-DATO-PROT
                   GO TO FIN-PUT-SDF-FRM-{FRM}.
           IF {RFRM}-VRF-{NOMB}-ATR = FRM-CPIM-AEY AND 
               SDF-{RFRM}-VRF-{NOMB} IN {RREG}F{FRM}
               NOT = {RFRM}-VRF-{NOMB} IN {FRM}-FLD
                   MOVE "S" TO FRM-MODIFICO-DATO-PROT
                   GO TO FIN-PUT-SDF-FRM-{FRM}.
% END IF
% IF TIPO_IDC THEN
           IF {RFRM}-IND-{NOMB}-ATR = FRM-CPIM-AEY AND 
               SDF-{RFRM}-IND-{NOMB} IN {RREG}F{FRM}
               NOT = {RFRM}-IND-{NOMB} IN {FRM}-FLD
                   MOVE "S" TO FRM-MODIFICO-DATO-PROT
                   GO TO FIN-PUT-SDF-FRM-{FRM}.
           IF {RFRM}-GLS-{NOMB}-ATR = FRM-CPIM-AEY AND 
               SDF-{RFRM}-GLS-{NOMB} IN {RREG}F{FRM}
               NOT = {RFRM}-GLS-{NOMB} IN {FRM}-FLD
                   MOVE "S" TO FRM-MODIFICO-DATO-PROT
                   GO TO FIN-PUT-SDF-FRM-{FRM}.
% END IF
