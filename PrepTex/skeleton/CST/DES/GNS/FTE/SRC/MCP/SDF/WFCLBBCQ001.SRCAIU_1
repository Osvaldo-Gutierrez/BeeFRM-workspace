%! WF001.SRCAIU 
      * {TIPO}-INI --> WFCLBBCQ001.SRCAIU
% TIPO_{TIPO} = TRUE
%!
%! TIPO VRF
% IF TIPO_VRF THEN
% IF NOT TIENE_IDC_{RREG}_IDC_{NOMB} THEN
           05 FILLER                        PIC X(04) VALUE @277F{FCHEX_VRF_{RREG}_{NOMB}}@.
% IF NOT DISP THEN
           05 FILLER                        PIC X(01) VALUE @1C@.
% END
           05 {RREG}-NUM-{NOMB}{IDX}                  PIC X(11)
              VALUE SPACES.
% IF NOT DISP THEN
           05 FILLER                        PIC X(01) VALUE @1E@.
% END
           05 FILLER                        PIC X(01)
              VALUE "-".
% IF NOT DISP THEN
           05 FILLER                        PIC X(01) VALUE @1C@.
% END
           05 {RREG}-VRF-{NOMB}{IDX}                  PIC X(01)
              VALUE SPACES.
% IF NOT DISP THEN
           05 FILLER                        PIC X(01) VALUE @1E@.
% END
% END
% ELSE
%! TIPO IDC
%!
% IF TIPO_IDC THEN
           05 FILLER                        PIC X(04) VALUE @277F{FCHEX_VRF_{RREG}_{NOMB}}@.
% IF NOT DISP THEN
           05 FILLER                        PIC X(01) VALUE @1C@.
% END
           05 {RREG}-NUM-{NOMB}{IDX}                  PIC X(11)
              VALUE SPACES.
% IF NOT DISP THEN
           05 FILLER                        PIC X(01) VALUE @1E@.
% END
           05 FILLER                        PIC X(01)
              VALUE "-".
% IF NOT DISP THEN
           05 FILLER                        PIC X(01) VALUE @1C@.
% END
           05 {RREG}-VRF-{NOMB}{IDX}                  PIC X(01)
              VALUE SPACES.
% IF NOT DISP THEN
           05 FILLER                        PIC X(01) VALUE @1E@.
% END
           05 FILLER                        PIC X(01)
              VALUE SPACES.
           05 FILLER                        PIC X(04) VALUE @277F{FCHEX_IDC_{RREG}_{NOMB}}@.
% IF NOT DISP THEN
           05 FILLER                        PIC X(01) VALUE @1C@.
% END
           05 {RREG}-IND-{NOMB}{IDX}                  PIC X(01)
              VALUE SPACES.
% IF NOT DISP THEN
           05 FILLER                        PIC X(01) VALUE @1E@.
% END
           05 FILLER                        PIC X(01)
              VALUE "-".
% IF NOT DISP THEN
           05 FILLER                        PIC X(01) VALUE @1C@.
% END
           05 {RREG}-GLS-{NOMB}{IDX}                  PIC X(03)
              VALUE SPACES.
% IF NOT DISP THEN
           05 FILLER                        PIC X(01) VALUE @1E@.
% END
% ELSE
%! TIPO FEC
%!
% IF TIPO_FEC
           05 {CAMP}{IDX}            PIC X(08)
              VALUE SPACES.
% ELSE
% IF TIPO_DMA OR TIPO_AMD OR TIPO_MSA OR TIPO_SAM THEN
           05 {CAMP}{IDX}            PIC X(06)
              VALUE SPACES.
% ELSE
% IF TIPO_FMA OR TIPO_FAM OR TIPO_FSA OR TIPO_FAS OR TIPO_FDM OR TIPO_FMD THEN
           05 {CAMP}{IDX}            PIC X(04)
              VALUE SPACES.
% ELSE
% IF TIPO_FFA THEN
           05 {CAMP}{IDX}            PIC X(02)
              VALUE SPACES.
% END IF
% END
% END
% END
% END
% END
      * {TIPO}-FIN --> WFCLBBCQ001.SRCAIU
% NORMA_CST = FALSE
% TIPO_{TIPO} = FALSE
