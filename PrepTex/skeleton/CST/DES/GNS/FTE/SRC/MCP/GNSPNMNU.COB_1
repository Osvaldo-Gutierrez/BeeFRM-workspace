      $SET LIST
       IDENTIFICATION DIVISION.
      *=======================
       PROGRAM-ID.      GNSPNMNU.
       AUTHOR.          CONSIST.
      *
      * LISTA DATOS ESPECIFICOS DE TABLA MNU
      *
      *
       ENVIRONMENT DIVISION.
      *=====================
       CONFIGURATION SECTION.
      *----------------------
       SPECIAL-NAMES.
               DECIMAL-POINT IS COMMA.
      *        DICTIONARY IS "SDFPLUSDICTIONARY"
      *         "OBJECT/GNSXFMNU" IS LIB-MNU.
      
       DATA DIVISION.
      *=============
       FILE SECTION.
      *-------------
       DATA-BASE SECTION.
       DB  BDCST.
       01  AUDITAREA.
       01  GNSMNU.
       01  GNSTAB.
      
       WORKING-STORAGE SECTION.
      *-----------------------
      
       COPY GNSBRMNU OF GNS.
       COPY GNSBRTAB OF GNS.
      
      * WORKING PARA RUTINAS.
       COPY GNSWSFIO OF GNS.
       COPY GNSWCFIO OF GNS.
       COPY GNSWVFIO OF GNS.
       01  REG-CONT             PIC 9(03).
      
       PROCEDURE DIVISION.
      *===================
      
       MAIN SECTION.
      *------------
       INI-MAIN.
      
           MOVE "GNSPNMNU" TO FIO-PROG.
           MOVE "N" TO FIO-BTRN.
           MOVE FIO-UPD TO FIO-CMND.
           MOVE "GNS" TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
      
      *    MOVE FIO-INP TO FIO-CMND.
      *    PERFORM GNS-FIO-MNU.
           DISPLAY "FIO-STAT " FIO-STAT.
           MOVE "GNS"          TO MNU-COD-SISO
           MOVE "MEN"          TO MNU-COD-OPCI
           MOVE "MNU-COD-OPCS" TO  FIO-AKEY.
           MOVE FIO-GET-NLS    TO  FIO-CMND.
       CIC-MAIN.
           PERFORM GNS-FIO-MNU.
           DISPLAY "FIO-STAT " FIO-STAT.
           IF FIO-STAT-OKS
               ADD 1 TO REG-CONT
               DISPLAY "MNU-COD-SISF :" MNU-COD-SISF IN MNU
               DISPLAY "MNU-COD-NEMO :" MNU-COD-NEMO IN MNU
               DISPLAY "MNU-COD-OPCS :" MNU-COD-OPCS IN MNU
               DISPLAY "MNU-COD-IMES :" MNU-COD-IMES IN MNU
               DISPLAY "MNU-COD-TFUN :" MNU-COD-TFUN IN MNU
               DISPLAY "MNU-GLS-FMEN :" MNU-GLS-FMEN IN MNU
               DISPLAY "MNU-GLS-CMND :" MNU-GLS-CMND IN MNU
               DISPLAY "MNU-GLS-TRAN :" MNU-GLS-TRAN IN MNU
               DISPLAY "MNU-COD-CMND :" MNU-COD-CMND IN MNU
      *        DISPLAY "REG-CONT NRO: " REG-CONT
               MOVE FIO-GET-NXT TO FIO-CMND
               GO TO CIC-MAIN
           ELSE
               DISPLAY "TOTAL DE REGISTROS -->" REG-CONT
               DISPLAY "FIN DE PROCESO".
           STOP RUN.
       FIN-MAIN.
           EXIT.
       COPY GNSBFMNU OF GNS.
       COPY GNSBFTAB OF GNS.
       COPY GNSBGDMD OF GNS.
