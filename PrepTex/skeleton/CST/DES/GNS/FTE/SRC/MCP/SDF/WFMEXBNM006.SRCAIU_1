%!              WFMEXBNM006.SRCAIU 
      * {TIPO}-INI --> WFMEXBNM006.SRCAIU 
% TIPO_{TIPO} = TRUE
% IF AIU_PTF THEN
% FLD = "PTF"
% ELSE
% FLD = "PTR"
% END IF

% IF TIPO_FEC THEN
               05 {CAMP}{IDX}-{FLD}  PIC X(02)  VALUE @{POSI_PTR1}@.
% ELSE
% IF TIPO_OFI THEN
               05 {RREG}-IND-{NOMB}{IDX}-{FLD}  PIC X(02)  VALUE @{POSI_PTR1}@.
               05 {RREG}-COD-{NOMB}{IDX}-{FLD}  PIC X(02)  VALUE @{POSI_PTR2}@.
% ELSE
% IF TIPO_VRF THEN
% IF NOT TIENE_IDC_{RREG}_IDC_{NOMB} THEN
               05 {RREG}-NUM-{NOMB}{IDX}-{FLD}  PIC X(02)  VALUE @{POSI_PTR1}@.
               05 {RREG}-VRF-{NOMB}{IDX}-{FLD}  PIC X(02)  VALUE @{POSI_PTR2}@.
% ELSE
               05 {RREG}-IDC-{NOMB}{IDX}-{FLD}  PIC X(02)  VALUE @{POSI_PTR}@.
% END IF
% END IF
% END IF
% END IF
% AIU_PTF  = FALSE
% TIPO_{TIPO} = FALSE
      * {TIPO}-FIN --> WFMEXBNM006.SRCAIU 
