%!              BMMEXBNM008.SRCAIU VAL-[FLD] SECTION
      * {TIPO}-INI --> BMMEXBNM008.SRCAIU VAL-[FLD] SECTION
%! TIPO VRF
% IF TIDX > 0 THEN
% FRM_IFLD = "(FRM-IFLD)"
*% FRM_IFLD = "(FRM-IFLD)"
% ELSE
% FRM_IFLD = ""
*% FRM_IFLD = ""
% END IF
% TIPO_{TIPO} = TRUE
% IF TIPO_OFI THEN
       VAL-{RREG}-IND-{NOMB} SECTION.
       INI-VAL-{RREG}-IND-{NOMB}.
*% INCLUDE INI_{RREG}_IND_{NOMB} {ARCA}
           IF {RREG}-IND-{NOMB} IN {FRM}-FLD{FRM_IFLD} NOT > ZEROES
% IF OBLI THEN
              PERFORM ERR-GLS.
% ELSE
              MOVE ZEROES TO {RREG}-IND-{NOMB} IN {FRM}-FLD{FRM_IFLD}.
% END IF

*% INCLUDE {RREG}_IND_{NOMB} {ARCA}
*% INCLUDE FIN_{RREG}_IND_{NOMB} {ARCA}
       FIN-VAL-{RREG}-IND-{NOMB}.
           EXIT.

       VAL-{RREG}-COD-{NOMB} SECTION.
       INI-VAL-{RREG}-COD-{NOMB}.
*% INCLUDE INI_{RREG}_COD_{NOMB} {ARCA}
           IF {RREG}-COD-{NOMB} IN {FRM}-FLD{FRM_IFLD} NOT > ZEROES
% IF OBLI THEN
              PERFORM ERR-GLS.
% ELSE
              MOVE ZEROES TO {RREG}-COD-{NOMB} IN {FRM}-FLD{FRM_IFLD}.
% END IF
*% INCLUDE {RREG}_COD_{NOMB} {ARCA}
*% INCLUDE FIN_{RREG}_COD_{NOMB} {ARCA}
       FIN-VAL-{RREG}-COD-{NOMB}.
           EXIT.

% END IF

% IF TIPO_VRF THEN
% IF NOT TIENE_IDC_{RREG}_IDC_{NOMB} THEN
       VAL-{RREG}-NUM-{NOMB} SECTION.
       INI-VAL-{RREG}-NUM-{NOMB}.
*% INCLUDE INI_{RREG}_NUM_{NOMB} {ARCA}
*% INCLUDE {RREG}_NUM_{NOMB} {ARCA}
*% INCLUDE FIN_{RREG}_NUM_{NOMB} {ARCA}
       FIN-VAL-{RREG}-NUM-{NOMB}.
           EXIT.

       VAL-{RREG}-VRF-{NOMB} SECTION.
       INI-VAL-{RREG}-VRF-{NOMB}.
*% INCLUDE INI_{RREG}_VRF_{NOMB} {ARCA}
           IF {RREG}-VRF-{NOMB} IN {FRM}-FLD{FRM_IFLD} NOT > SPACES
% IF OBLI THEN
              PERFORM ERR-GLS.
% ELSE
              MOVE SPACES TO {RREG}-VRF-{NOMB} IN {FRM}-FLD{FRM_IFLD}.
% END IF
% IF NOT DISP THEN
*% INCLUDE 'SETVRF'
*% INCLUDE 'PROVRF'
% END IF
*% INCLUDE {RREG}_VRF_{NOMB} {ARCA}
*% INCLUDE FIN_{RREG}_VRF_{NOMB} {ARCA}
       FIN-VAL-{RREG}-VRF-{NOMB}.
           EXIT.

% END IF
% END IF
%! TIPO IDC
% IF TIPO_IDC THEN
       VAL-{RREG}-IDC-{NOMB} SECTION.
       INI-VAL-{RREG}-IDC-{NOMB}.
*% INCLUDE INI_{RREG}_IDC_{NOMB} {ARCA}
           IF {RREG}-IDC-{NOMB} IN {FRM}-FLD{FRM_IFLD} NOT > ZEROES
              MOVE ZEROES TO {RREG}-IDC-{NOMB} IN {FRM}-FLD{FRM_IFLD}.
*% INCLUDE {RREG}_IDC_{NOMB} {ARCA}
*% INCLUDE FIN_{RREG}_IDC_{NOMB} {ARCA}
       FIN-VAL-{RREG}-IDC-{NOMB}.
           EXIT.
% END IF
%! TIPO FEC Y SUS DERIVADOS
% IF TIPO_FEC OR TIPO_DMA OR TIPO_AMD OR TIPO_MSA OR TIPO_SAM OR TIPO_FMA OR TIPO_FAM OR TIPO_FSA OR TIPO_FAS OR TIPO_FDM OR TIPO_FMD OR TIPO_FFA THEN
       VAL-{CAMP} SECTION.
       INI-VAL-{CAMP}.
           IF {CAMP} IN {FRM}-FLD{FRM_IFLD}  NOT > SPACES
% IF OBLI THEN
              PERFORM ERR-GLS.
%ELSE
              MOVE ZEROES TO {CAMP} IN {FRM}-FLD{FRM_IFLD}.
% END IF
% IF NOT DISP THEN
*% INCLUDE 'SETFEC'
% END IF
*% INCLUDE INI_{LBL}
% IF NOT DISP THEN
*% INCLUDE 'PROFEC'
% END IF
*% INCLUDE {LBL}
*% INCLUDE FIN_{LBL}
       FIN-VAL-{CAMP}.
           EXIT.
% END IF
% TIPO_{TIPO} = FALSE
      * {TIPO}-FIN --> BMMEXBNM008.SRCAIU VAL-[FLD] SECTION
