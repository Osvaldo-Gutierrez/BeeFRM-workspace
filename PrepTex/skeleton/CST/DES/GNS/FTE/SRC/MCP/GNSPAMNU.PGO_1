       FIN-SCR-ELI.
       EXT-SCR-ELI.
           EXIT.
      
       SCR-ACT SECTION.
       INI-SCR-ACT.
           IF SCR-INST = SCR-INST-HKY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM HBL-KEY
               PERFORM SCR-DMSG
               GO TO EXT-SCR-ACT
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-RST
                PERFORM GET-KEY
                IF SYS-EXIT-YES
                    GO TO EXT-SCR-ACT
                ELSE
                IF NOT FRM-STAT-ABT
                        MOVE SPACES TO FRM-MENS
                        PERFORM FND-KEY
                        IF FIO-STAT-OKS
                            MOVE SCR-ACTU-MOD TO SCR-ACTU
                            PERFORM PUT-SCR
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
                            PERFORM SCR-TRL
                            GO TO EXT-SCR-ACT
                        ELSE
                        MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR
                        IF FIO-STAT-EOF
                            IF FRM-MENS NOT > SPACES
                                MOVE "REGEOF    " TO MSG-COD-MENS
                                MOVE "GNS" TO MSG-COD-SIST
                                PERFORM GET-MSG
                                MOVE MSG-GLS-DESC TO FRM-MENS
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                            ELSE
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                        ELSE
                        IF FIO-STAT = FIO-STAT-NEX
                            IF FRM-MENS NOT > SPACES
                                MOVE "REGNEX    " TO MSG-COD-MENS
                                MOVE "GNS" TO MSG-COD-SIST
                                PERFORM GET-MSG
                                MOVE MSG-GLS-DESC TO FRM-MENS
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                            ELSE
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                        ELSE
                            MOVE SCR-ACTU-ING TO SCR-ACTU
      *JSS                  PERFORM CLR-FLD-MNU
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
                            PERFORM SCR-TRL
                            GO TO EXT-SCR-ACT
                ELSE
                        MOVE SPACES TO FRM-MENS
                        PERFORM SCR-MSSG
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ACT
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
                IF SYS-EXIT-YES
                    GO TO EXT-SCR-ACT
                ELSE
                    NEXT SENTENCE
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC
                IF SYS-EXIT-YES
                    GO TO EXT-SCR-ACT
                ELSE
                    NEXT SENTENCE
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
                IF SYS-EXIT-YES
                    GO TO EXT-SCR-ACT.
           PERFORM DESSCRTRL.
           IF SYS-EXIT-YES
               GO TO EXT-SCR-ACT.
           IF SCR-ACTU = SCR-ACTU-MOD
                  PERFORM MSG-SCR-MOD
           ELSE
                  PERFORM MSG-SCR-ING.
       FIN-SCR-ACT.
       EXT-SCR-ACT.
           EXIT.
      
       SCR-PGM SECTION.
       INI-SCR-PGM.
           IF SCR-INST = SCR-INST-HKY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM HBL-KEY
               PERFORM SCR-DMSG
               GO TO EXT-SCR-PGM
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-RST
                PERFORM GET-KEY
                IF SYS-EXIT-YES
                    GO TO EXT-SCR-PGM
                ELSE
                IF FRM-STAT-ABT
                        MOVE SPACES TO FRM-MENS
                        PERFORM SCR-MSSG
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-PGM.
           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
      *JSS IF NOT FIO-STAT-OKS
      *        PERFORM CLR-FLD-MNU.
           IF FIO-STAT = FIO-STAT-NEX
               IF FRM-MENS NOT > SPACES
                   MOVE "REGNEX    " TO MSG-COD-MENS
                   MOVE "GNS" TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   PERFORM SCR-MSSG
                   MOVE SCR-STAT-ABT TO SCR-STAT
                   PERFORM DESSCRTRL
                   GO TO FIN-SCR-PGM
               ELSE
                   PERFORM SCR-MSSG
                   MOVE SCR-STAT-ABT TO SCR-STAT
                   PERFORM DESSCRTRL
                   GO TO FIN-SCR-PGM.
       TRL-SCR-PGM.
           PERFORM DESSCRTRL.
           IF SYS-EXIT-YES
               GO TO EXT-SCR-PGM.
           MOVE SPACES TO FRM-MENS.
           IF SCR-STAT NOT = SCR-STAT-CON
               PERFORM CMN-PGM.
           PERFORM SCR-MSSG.
       FIN-SCR-PGM.
       EXT-SCR-PGM.
           EXIT.
      
       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               PERFORM SCR-CLRA-MNU
               PERFORM EDT-KEY-MNU
               PERFORM EDT-FLD-MNU
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-MNU
               PERFORM EDT-KEY-MNU
           ELSE
               PERFORM CLR-FLD-MNU
               PERFORM EDT-FLD-MNU.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.
      
       SCR-KEY SECTION.
       INI-SCR-KEY.
           PERFORM SCR-RST.
           PERFORM GET-KEY.
           IF SYS-EXIT-YES
               GO TO FIN-SCR-KEY.
           IF FRM-STAT-ABT
                   MOVE SPACES TO FRM-MENS
                   PERFORM SCR-MSSG
                   GO TO FIN-SCR-KEY.
       FND-SCR-KEY.
           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
           IF FIO-STAT-OKS
               PERFORM PUT-SCR
               MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
               GO TO FIN-SCR-KEY.
      *JSS ELSE
      *        PERFORM CLR-FLD-MNU.
       ERR-SCR-KEY.
           MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR.
           IF FRM-MENS NOT > SPACES
               IF FIO-STAT-EOF
                   MOVE "REGEOF    " TO MSG-COD-MENS
                   MOVE "GNS" TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
               IF FIO-STAT = FIO-STAT-NEX
                   MOVE "REGNEX    " TO MSG-COD-MENS
                   MOVE "GNS" TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
                   MOVE "FIOSTAT" TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE "GNS"     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
           MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-SCR-KEY.
           EXIT.
      
       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE SPACES       TO SCR-OFOP.
      *     MOVE LOW-VALUES   TO MNU-FLD.
           MOVE "23"         TO PGM-STAT-MNU.
       FIN-SCR-RST.
           EXIT.
      
       GET-KEY SECTION.
       INI-GET-KEY.
           PERFORM SCR-GKEY.
           IF ( NOT SYS-EXIT-YES ) AND FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-KEY.
           EXIT.
      
       GET-IKY SECTION.
       INI-GET-IKY.
           PERFORM SCR-GIKY.
           IF ( NOT SYS-EXIT-YES ) AND FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-IKY.
           EXIT.
      
       FND-KEY SECTION.
       INI-FND-KEY.
           PERFORM KEY-ALL-MNU.
      *<<<< FND_KEY
           IF MNU-COD-NEMO IN MNU-FLD NOT > SPACES
               IF MNU-COD-OPCI IN MNU-FLD > SPACES
                   MOVE SCR-SISG        TO FIO-SIST
                   MOVE "MNU-COD-OPCI"  TO FIO-AKEY
                   MOVE FIO-GET-KEY TO FIO-CMND
                   PERFORM GNS-FIO-MNU
                   MOVE FIO-STAT TO PGM-STAT-MNU
                   IF NOT FIO-STAT-OKS
                       MOVE FIO-STAT-NEX TO FIO-STAT
                       GO TO FIN-FND-KEY
                   ELSE
                       GO TO FIN-FND-KEY
               ELSE
                   MOVE SCR-SISG        TO FIO-SIST
                   MOVE "MNU-COD-IMEN"  TO FIO-AKEY
                   MOVE FIO-GET-KEY TO FIO-CMND
                   PERFORM GNS-FIO-MNU
                   MOVE FIO-STAT TO PGM-STAT-MNU
                   IF NOT FIO-STAT-OKS
                       MOVE FIO-STAT-NEX TO FIO-STAT
                       GO TO FIN-FND-KEY
                   ELSE
                       GO TO FIN-FND-KEY.
           MOVE SCR-SISG    TO FIO-SIST.
      *>>>>
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-MNU.
           IF FIO-STAT-OKS
               MOVE MNU-STP-ITRN IN MNU TO SCR-UMOD.
           MOVE FIO-STAT TO PGM-STAT-MNU.
       FIN-FND-KEY.
           EXIT.
      
       PUT-SCR SECTION.
       INI-PUT-SCR.
           IF PGM-STAT-MNU-OKS
               MOVE MNU-STP-ITRN IN MNU TO SCR-UMOD.
      *<<<< PUT_SCR
           IF PGM-STAT-MNU-OKS
               MOVE MNU-COD-NEMO IN MNU TO SCR-KEY-IMNU-ACTU.
      *>>>>
           PERFORM PUT-ALL-MNU.
       FIN-PUT-SCR.
           EXIT.
      
       GET-FLD SECTION.
       INI-GET-FLD.
           PERFORM SCR-GFLD.
           IF ( NOT SYS-EXIT-YES ) AND FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-FLD.
           EXIT.
      
       CMN-MOD SECTION.
       INI-CMN-MOD.
           MOVE FIO-BEG-TRN TO FIO-CMND.
           MOVE "GNS" TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
      *<<<< CMN_ING OR INI_CMN_MOD OR INI_CMN_ELI OR INI_CMN_ACC OR CMN_
           MOVE SCR-SISG TO FIO-SIST.
      *>>>>
           PERFORM KEY-ALL-MNU.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM GNS-FIO-MNU.
           IF FIO-STAT-OKS AND
              MNU-STP-ITRN IN MNU NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM GNS-FIO-MNU
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM GNS-FIO-MNU
               MOVE "GNS" TO MSG-COD-SIST
               MOVE "GNSACCSSIMUL" TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-MOD.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-MOD.
           PERFORM RET-STT-MNU.
           PERFORM RET-ALL-MNU.
           PERFORM RET-HDR-MNU.
      *<<<< CMN_ING OR CMN_MOD
      *    IF MNU-COD-NEMO IN MNU = ZEROES
      *        MOVE MNU-COD-CMEN IN MNU TO MNU-COD-SIST IN MNU.
           IF MNU-COD-NEMO IN MNU = "SIS"
               MOVE SCR-SISG TO MNU-COD-SIST IN MNU.
      *>>>>
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM GNS-FIO-MNU.
      *<<<< FIN_CMN_ING OR FIN_CMN_MOD
           IF FIO-STAT-OKS
               PERFORM BFR-RFSH.
      *>>>>
       FIN-CMN-MOD.
           MOVE FIO-CHK-PNT TO FIO-CMND.
           MOVE "GNS" TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
       EXT-CMN-MOD.
           EXIT.
      
       CMN-ING SECTION.
       INI-CMN-ING.
           MOVE FIO-BEG-TRN TO FIO-CMND.
           MOVE "GNS" TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
           MOVE SPACES TO MNU.
           PERFORM RET-STT-MNU.
           PERFORM RET-ALL-MNU.
           PERFORM RET-HDR-MNU.
      *<<<< CMN_ING OR INI_CMN_MOD OR INI_CMN_ELI OR INI_CMN_ACC OR CMN_
           MOVE SCR-SISG TO FIO-SIST.
      *>>>>
      *<<<< CMN_ING OR CMN_MOD
      *    IF MNU-COD-NEMO IN MNU = ZEROES
      *        MOVE MNU-COD-CMEN IN MNU TO MNU-COD-SIST IN MNU.
           IF MNU-COD-NEMO IN MNU = "SIS"
               MOVE SCR-SISG TO MNU-COD-SIST IN MNU.
      *>>>>
           MOVE FIO-PUT TO FIO-CMND.
           PERFORM GNS-FIO-MNU.
      *<<<< FIN_CMN_ING OR FIN_CMN_MOD
           IF FIO-STAT-OKS
               PERFORM BFR-RFSH.
      *>>>>
       FIN-CMN-ING.
           MOVE FIO-CHK-PNT TO FIO-CMND.
           MOVE "GNS" TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
       EXT-CMN-ING.
           EXIT.
      
       CMN-ACC SECTION.
       INI-CMN-ACC.
           MOVE FIO-BEG-TRN TO FIO-CMND.
           MOVE "GNS" TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE "ACCION NO REALIZADA" TO FRM-MENS
      *<<<< ABT_CMN_ACC
               IF SCR-CMND = "CAL"
                   MOVE "CALCULO NO FUE EFECTUADO" TO FRM-MENS
                   GO TO FIN-CMN-ACC
               ELSE
               IF SCR-CMND = "COP"
                   MOVE "REGISTRO NO FUE COPIADO" TO FRM-MENS
                   GO TO FIN-CMN-ACC
               ELSE
      *>>>>
               GO TO FIN-CMN-ACC.
      *<<<< CMN_ING OR INI_CMN_MOD OR INI_CMN_ELI OR INI_CMN_ACC OR CMN_
           MOVE SCR-SISG TO FIO-SIST.
      *>>>>
      *<<<< INI_CMN_ACC
           IF SCR-CMND = "CAL"
               PERFORM RET-ALL-MNU
               PERFORM CAL-MNU
               MOVE CLF-MENS TO FRM-MENS
           ELSE
           IF SCR-CMND = "COP"
               PERFORM RET-ALL-MNU
               MOVE FIO-PUT  TO FIO-CMND
               MOVE SCR-SISG TO FIO-SIST
               PERFORM GNS-FIO-MNU
               IF FIO-STAT-OKS
                   PERFORM BFR-RFSH
                   MOVE "REGISTRO FUE COPIADO" TO FRM-MENS
               ELSE
                   MOVE "ERROR : REGISTRO NO COPIADO" TO FRM-MENS
           ELSE
               MOVE "COMANDO NO IMPLEMENTADO" TO FRM-MENS.
           GO TO FIN-CMN-ACC.
      *>>>>
           PERFORM KEY-ALL-MNU.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM GNS-FIO-MNU.
           IF FIO-STAT-OKS AND
              MNU-STP-ITRN IN MNU NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM GNS-FIO-MNU
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM GNS-FIO-MNU
               MOVE "GNS" TO MSG-COD-SIST
               MOVE "GNSACCSSIMUL" TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-ACC.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ACC.
           PERFORM RET-ALL-MNU.
       FIN-CMN-ACC.
           MOVE FIO-CHK-PNT TO FIO-CMND.
           MOVE "GNS" TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
       EXT-CMN-ACC.
           EXIT.
      
       CMN-ELI SECTION.
       INI-CMN-ELI.
           MOVE FIO-BEG-TRN TO FIO-CMND.
           MOVE "GNS" TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
      *<<<< CMN_ING OR INI_CMN_MOD OR INI_CMN_ELI OR INI_CMN_ACC OR CMN_
           MOVE SCR-SISG TO FIO-SIST.
      *>>>>
           PERFORM KEY-ALL-MNU.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM GNS-FIO-MNU.
           IF FIO-STAT-OKS AND
              MNU-STP-ITRN IN MNU NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM GNS-FIO-MNU
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM GNS-FIO-MNU
               MOVE "GNS" TO MSG-COD-SIST
               MOVE "GNSACCSSIMUL" TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-ELI.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ELI.
           MOVE FIO-DEL TO FIO-CMND.
           PERFORM GNS-FIO-MNU.
      *<<<< FIN_CMN_ING OR FIN_CMN_MOD
           IF FIO-STAT-OKS
               PERFORM BFR-RFSH.
      *>>>>
       FIN-CMN-ELI.
           MOVE FIO-CHK-PNT TO FIO-CMND.
           MOVE "GNS" TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
       EXT-CMN-ELI.
           EXIT.
      
       CMN-PGM SECTION.
       INI-CMN-PGM.
           PERFORM RET-ALL-MNU.
      *<<<< CMN_ING OR INI_CMN_MOD OR INI_CMN_ELI OR INI_CMN_ACC OR CMN_
           MOVE SCR-SISG TO FIO-SIST.
      *>>>>
       FIN-CMN-PGM.
           EXIT.
      
       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE "AHGNSPAMNU" TO UYS-AGND.
           PERFORM UYS-GET-AGND.
           MOVE UYS-AGND-R TO AGENDA OF COMS-OUT.
      *    MOVE ATTRIBUTE FORMRECNUM OF GNSFMNU TO
      *                   SDFFORMRECNUM OF COMS-OUT.
      *    MOVE 0 TO SDFINFO OF COMS-OUT.
           MOVE MNU-FLD  TO FRM-DFLD-BKP.
           PERFORM SET-FRM.
           MOVE "P"      TO SCR-ICMN.
           MOVE FIO-BTRN TO SCR-BTRN.
           IF SYS-NTRN = SPACES
               MOVE SCR-NTRN TO SYS-NTRN.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.
      
       SCR-PUTA SECTION.
       INI-SCR-PUTA.
           PERFORM PRO-PROT-MNU.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
      *    MOVE MNU-FLD TO FRM-DFLD.
           PERFORM PUT-SCR-SDF-MNU.
      *    PERFORM PUT-FRM-SDF-MNU.
           MOVE SCR-NTRN TO SDF-HDR-COD-TRCD IN GNSFMNU.
      *    MOVE GNSFMNU TO FRM-DFLD.
           PERFORM BUS-PTR-MNU VARYING WSS-IDX-PTR FROM 1 BY 1
                   UNTIL MNU-FLD-LEN-ELM(WSS-IDX-PTR) NOT = SPACES OR
                         WSS-IDX-PTR NOT < MNU-NUM-LEN.
           IF WSS-IDX-PTR NOT > MNU-NUM-LEN
               MOVE MNU-FLD-PTR-ELM(WSS-IDX-PTR) TO
                    MNU-POSI-CUR IN MNU-FLD
           ELSE
               MOVE @6140@ TO MNU-POSI-CUR IN MNU-FLD.
      
           IF FRM-MENS-MDCM > SPACES
                 MOVE FRM-MENS-MDC  TO MNU-GLS-MENS IN MNU-FLD.
           MOVE MNU-FLD  TO FRM-DFLD.
      
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.
      
       BUS-PTR-MNU SECTION.
       INI-BUS-PTR-MNU.
       FIN-BUS-PTR-MNU.
           EXIT.
      
       SCR-RETA SECTION.
       INI-SCR-RETA.
      *    MOVE FRM-DFLD TO GNSFTAB.
      *    PERFORM PUT-SDF-FRM-MNU.
       FIN-SCR-RETA.
           EXIT.
      
       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO MNU-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.
      
       SCR-GKEY SECTION.
       INI-SCR-GKEY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
      *     MOVE FRM-GET-ALL TO FRM-CMND.
      *     PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE "PFSINV    " TO MSG-COD-MENS
               MOVE "GNS" TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-MNU
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT
               GO TO EXT-SCR-GKEY.
      *     MOVE FRM-DFLD TO MNU-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GKEY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-MNU.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               PERFORM HBL-KEY-PRO
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-MNU
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT
               GO TO EXT-SCR-GKEY.
      *JSS
           PERFORM CLR-FLD-MNU.
           PERFORM VAL-IKY-MNU.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-MKY-MNU.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-AKY-MNU.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-CON-KEY-MNU.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GKEY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-MNU.
       ERR-SCR-GKEY.
           PERFORM HBL-KEY-PRO.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-MNU.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
           GO TO EXT-SCR-GKEY.
       FIN-SCR-GKEY.
           PERFORM SCR-MSSG.
       EXT-SCR-GKEY.
           EXIT.
      
       SCR-GIKY SECTION.
       INI-SCR-GIKY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
      *     MOVE FRM-GET-ALL TO FRM-CMND.
      *     PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE "PFSINV    " TO MSG-COD-MENS
               MOVE "GNS" TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-MNU
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT
               GO TO EXT-SCR-GIKY.
      *     MOVE FRM-DFLD TO MNU-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GIKY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-MNU.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               PERFORM HBL-IKY-PRO
               PERFORM SCR-MSSG
               PERFORM EDT-IKY-MNU
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT
               GO TO EXT-SCR-GIKY.
      *JSS
           PERFORM CLR-FLD-MNU.
           PERFORM VAL-IKY-MNU.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GIKY.
           PERFORM VAL-CON-IKY-MNU.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GIKY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-MNU.
       ERR-SCR-GIKY.
           PERFORM HBL-IKY-PRO.
           PERFORM SCR-MSSG.
           PERFORM EDT-IKY-MNU.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
           GO TO EXT-SCR-GIKY.
       FIN-SCR-GIKY.
           PERFORM SCR-MSSG.
       EXT-SCR-GIKY.
           EXIT.
      
       SCR-GFLD SECTION.
       INI-SCR-GFLD.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
      *     MOVE FRM-GET-ALL TO FRM-CMND.
      *     PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE "PFSINV    " TO MSG-COD-MENS
               MOVE "GNS" TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-FLD-MNU
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT
               GO TO EXT-SCR-GFLD.
      *     MOVE FRM-DFLD TO MNU-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GFLD.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-MNU.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               MOVE "N" TO FRM-PUT-FST-FLD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-MNU
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT
               GO TO EXT-SCR-GFLD.
           PERFORM VAL-NUM-FLD-MNU.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               MOVE "N" TO FRM-PUT-FST-FLD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-MNU
               PERFORM EDT-FLD-MNU
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT
               GO TO EXT-SCR-GFLD.
           PERFORM VAL-MKY-MNU.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GFLD.
           PERFORM VAL-FLD-MNU.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GFLD.
           PERFORM VAL-CON-FLD-MNU.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GFLD.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-FLD-MNU.
       ERR-SCR-GFLD.
           MOVE "N" TO FRM-PUT-FST-FLD.
           PERFORM HBL-FLD.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-MNU.
           PERFORM EDT-FLD-MNU.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
           GO TO EXT-SCR-GFLD.
       FIN-SCR-GFLD.
           MOVE SCR-STAT-MOD TO SCR-STAT.
       EXT-SCR-GFLD.
           EXIT.
      
       GET-ACC SECTION.
       INI-GET-ACC.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
      *     MOVE FRM-GET-ALL TO FRM-CMND.
      *     PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE "PFSINV    " TO MSG-COD-MENS
               MOVE "GNS" TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO MNU-GLS-MEN2
               MOVE SCR-PRMT     TO MNU-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT
               GO TO EXT-GET-ACC.
      *     MOVE FRM-DFLD TO MNU-FLD.
           IF NOT ( ( FRM-FFLD = FRM-FFLD-PF12 OR FRM-FFLD-PF3 ) OR
                      SCR-CONF > SPACES )
               PERFORM VAL-NUM-KEY-MNU
               PERFORM VAL-NUM-FLD-MNU.
       FIN-GET-ACC.
           MOVE SCR-STAT-SON TO SCR-STAT.
       EXT-GET-ACC.
           EXIT.
      
       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
      *     MOVE FRM-GET-ALL TO FRM-CMND.
      *     PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF7 )
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 )
      *>>>>
               MOVE "PFSINV    " TO MSG-COD-MENS
               MOVE "GNS" TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO MNU-GLS-MEN2
               MOVE SCR-PRMT     TO MNU-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT
               GO TO EXT-GET-CON.
      *     MOVE FRM-DFLD TO MNU-FLD.
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
       EXT-GET-CON.
           EXIT.
      
       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO MNU-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.
      
       HBL-IKY SECTION.
       INI-HBL-IKY.
      *    IF SCR-DESDE-CMN
      *         MOVE SCR-TRL-GLS-MENS TO MNU-GLS-MENS IN MNU-FLD.
      *         MOVE MNU-GLS-MENS TO FRM-MENS
      *         MOVE LOW-VALUES TO MNU-FLD
      *         IF FRM-MENS < SPACES
      *             MOVE LOW-VALUES TO MNU-GLS-MENS
      *         ELSE
      *             MOVE FRM-MENS TO MNU-GLS-MENS.
      *     MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-MKY-MNU.
           PERFORM PRO-AKY-MNU.
           PERFORM UNP-IKY-MNU.
           PERFORM PRO-FLD-MNU.
      *     MOVE "1" TO FRM-CPIM-AEY.
           PERFORM UNP-IKY-MNU.
           PERFORM FST-KEY-MNU.
       FIN-HBL-IKY.
           EXIT.
      
       HBL-IKY-PRO SECTION.
       INI-HBL-IKY-PRO.
           PERFORM PRO-MKY-MNU.
           PERFORM PRO-AKY-MNU.
           PERFORM UNP-IKY-MNU.
           PERFORM PRO-FLD-MNU.
           PERFORM UNP-IKY-MNU.
       FIN-HBL-IKY-PRO.
           EXIT.
      
       HBL-KEY SECTION.
       INI-HBL-KEY.
      *    IF SCR-DESDE-CMN
      *         MOVE SCR-TRL-GLS-MENS TO MNU-GLS-MENS IN MNU-FLD.
      *         MOVE MNU-GLS-MENS TO FRM-MENS
      *         MOVE LOW-VALUES TO MNU-FLD
      *         IF FRM-MENS < SPACES
      *             MOVE LOW-VALUES TO MNU-GLS-MENS
      *         ELSE
      *             MOVE FRM-MENS TO MNU-GLS-MENS.
      *     MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM UNP-KEY-MNU.
           PERFORM PRO-FLD-MNU.
           PERFORM FST-KEY-MNU.
      *     MOVE "1" TO FRM-CPIM-AEY.
           PERFORM UNP-KEY-MNU.
       FIN-HBL-KEY.
           EXIT.
      
       HBL-KEY-PRO SECTION.
       INI-HBL-KEY-PRO.
           PERFORM UNP-KEY-MNU.
           PERFORM PRO-FLD-MNU.
           PERFORM UNP-KEY-MNU.
       FIN-HBL-KEY-PRO.
           EXIT.
      
       HBL-FLD SECTION.
       INI-HBL-FLD.
           PERFORM PRO-IKY-MNU.
           PERFORM PRO-AKY-MNU.
           PERFORM UNP-MKY-MNU.
           PERFORM UNP-FLD-MNU.
           IF FRM-PUT-FST-FLD = "S"
               PERFORM FST-FLD-MNU.
      *<<<<
           IF SCR-CMND = "COP"
               PERFORM UNP-IKY-MNU
               PERFORM FST-KEY-MNU.
      *>>>>
       FIN-HBL-FLD.
           EXIT.
      
       FST-MSG SECTION.
       INI-FST-MSG.
      *    MOVE FRM-POS-CURS TO MNU-GLS-MENS-LEN.
           MOVE FRM-POS-CURS TO MNU-COD-CACC-LEN.
       FIN-FST-MSG.
           EXIT.
      
       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-MNU.
           PERFORM PRO-FLD-MNU.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.
      
       SCR-TRL SECTION.
       INI-SCR-TRL.
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-MNU
               PERFORM EDT-FLD-MNU
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT
               GO TO FIN-SCR-TRL.
           IF SCR-STAT = SCR-STAT-SON
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-MNU
               PERFORM EDT-FLD-MNU
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT
               GO TO FIN-SCR-TRL.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
           MOVE SCR-INST-GAB TO SCR-INST.
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-MNU.
           PERFORM EDT-FLD-MNU.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.
      
       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           IF SCR-INST = SCR-INST-GCF
                  PERFORM SCR-GCNF
                  IF SYS-EXIT-YES
                      GO TO EXT-DESSCRTRL
                  ELSE
                      GO TO FIN-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
           IF SCR-STAT = SCR-STAT-ABT OR SCR-STAT-CON
                  GO TO FIN-DESSCRTRL.
           IF SCR-FFLD-RET OR SCR-FFLD-PF10
                  MOVE SCR-VFLD-YES TO SCR-VFLD
                  PERFORM PRO-CONF
                  IF SYS-EXIT-YES
                      GO TO EXT-DESSCRTRL.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.
      
       PRO-CONF SECTION.
       INI-PRO-CONF.
           IF FRM-MENS NOT > SPACES
               IF SCR-CONF NOT > SPACES
                   GO TO FIN-PRO-CONF
               ELSE
                   MOVE SCR-CONF TO FRM-MENS
           ELSE
               IF FRM-MEN1 NOT > SPACES
                   MOVE SCR-CONF TO FRM-MEN1.
           MOVE SCR-INST-GCF TO SCR-INST.
           PERFORM PRO-KEY-MNU.
           PERFORM PRO-FLD-MNU.
           PERFORM FST-MSG.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM EDT-KEY-MNU
               PERFORM EDT-FLD-MNU.
           PERFORM SCR-MSSG.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-PRO-CONF.
           EXIT.
      
       SCR-GCNF SECTION.
       INI-SCR-GCNF.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
      *     MOVE FRM-GET-ALL TO FRM-CMND.
      *     PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3  OR
                               FRM-FFLD-PF4 )
               MOVE "PFSINV    " TO MSG-COD-MENS
               MOVE "GNS" TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO MNU-GLS-MEN2
               MOVE SCR-CONF     TO MNU-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *     MOVE FRM-DFLD TO MNU-FLD.
           MOVE FRM-FFLD TO SCR-FFLD.
           IF FRM-FFLD = FRM-FFLD-PF3
               MOVE SCR-VFLD-NOT TO SCR-VFLD
           ELSE
           IF FRM-FFLD = FRM-FFLD-RET OR FRM-FFLD-PF10 OR FRM-FFLD-PF4
               MOVE SCR-VFLD-YES TO SCR-VFLD
               PERFORM VAL-NUM-KEY-MNU
               PERFORM VAL-NUM-FLD-MNU
           ELSE
           IF FRM-FFLD = FRM-FFLD-PF12
               MOVE SCR-STPP TO SCR-STAT
               IF SCR-STAT = SCR-STAT-MOD
                   PERFORM VAL-NUM-KEY-MNU
                   PERFORM VAL-NUM-FLD-MNU
                   PERFORM SCR-TRL
               ELSE
                   MOVE SCR-VFLD-NOT TO SCR-VFLD.
       FIN-SCR-GCNF.
           EXIT.
      
       RET-STT-MNU SECTION.
       INI-RET-STT-MNU.
           IF SCR-CCPP = "ING"
               MOVE "I" TO MNU-MSC-TACC IN MNU
               MOVE "I" TO MNU-MSC-STAT IN MNU
           ELSE
           IF SCR-CCPP = "MOD"
               MOVE "M" TO MNU-MSC-TACC IN MNU
               MOVE "M" TO MNU-MSC-STAT IN MNU.
       FIN-RET-STT-MNU.
           EXIT.
      
       RET-HDR-MNU SECTION.
       INI-RET-HDR-MNU.
           PERFORM GET-FHOY.
      *    Fecha Transaccion
           MOVE HOY-FHOY TO MNU-FEC-FTRN IN MNU.
      *    Hora Transaccion
           MOVE HOY-HHOY TO MNU-HRA-HRTR IN MNU.
      *    Codigo Origen transaccion
      *<<<< NOT PGM_VMS
           MOVE SCR-OTRN TO MNU-COD-OTRN IN MNU.
      *    Autor Transaccion
           MOVE SCR-USER TO MNU-COD-ATRN IN MNU.
      *>>>>
       FIN-RET-HDR-MNU.
           EXIT.
      
       COPY GNSBMMNU OF GNS.
       COPY GNSBYMNU OF GNS.
       COPY GNSBFMNU OF GNS.
       COPY GNSBVNUM OF GNS.
       COPY GNSBFMSG OF GNS.
       COPY GNSBGSYS OF GNS.
       COPY GNSBGFRM OF GNS.
       COPY GNSBGMSG OF GNS.
       COPY GNSBEKEY OF GNS.
       COPY GNSBIABT OF GNS.
       COPY GNSBVCOD OF GNS.
       COPY GNSBVTAB OF GNS.
       COPY GNSBFTAB OF GNS.
       COPY GNSBEGLS OF GNS.
       COPY GNSBGFEC OF GNS.
       COPY GNSBGHOY OF GNS.
       COPY GNSBGQUE OF GNS.
       COPY GNSBGACI OF GNS.
       COPY GNSBGDMD OF GNS.
      
      *<<<< EOF
       SCRL-TAB-MNU SECTION.
       INI-SCRL-TAB-MNU.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF5
               PERFORM SCRL-TAB-MNU-PF7
               GO TO FIN-SCRL-TAB-MNU.
           IF FRM-FFLD = FRM-FFLD-PF8  OR FRM-FFLD-PF6
               PERFORM SCRL-TAB-MNU-PF8
               GO TO FIN-SCRL-TAB-MNU.
       FIN-SCRL-TAB-MNU.
           EXIT.
      
       SCRL-TAB-MNU-PF7 SECTION.
       INI-SCRL-TAB-MNU-PF7.
           MOVE SCR-KEY-IMNU-ACTU TO MNU-COD-NEMO IN MNU.
           MOVE "MNU-COD-NEMO" TO FIO-AKEY.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SIS-FIO-MNU.
           IF NOT FIO-STAT-OKS
               MOVE "MNUCURRNEX" TO MSG-COD-MENS
               GO TO MSG-SCRL-TAB-MNU-PF7.
       PRV-SCRL-TAB-MNU-PF8.
           MOVE "MNU-COD-NEMO" TO FIO-AKEY.
           MOVE FIO-GET-PRV TO FIO-CMND.
           PERFORM SIS-FIO-MNU.
           IF NOT FIO-STAT-OKS
               MOVE "MNU    NOPRV" TO MSG-COD-MENS
               GO TO MSG-SCRL-TAB-MNU-PF7.
           MOVE FIO-STAT TO PGM-STAT-MNU.
           PERFORM SCR-CLRA-MNU.
           PERFORM PUT-SCR.
           GO TO FIN-SCRL-TAB-MNU-PF7.
       MSG-SCRL-TAB-MNU-PF7.
           MOVE "GNS" TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-MNU-PF7.
           EXIT.
      
       SCRL-TAB-MNU-PF8 SECTION.
       INI-SCRL-TAB-MNU-PF8.
           MOVE SCR-KEY-IMNU-ACTU TO MNU-COD-NEMO IN MNU.
           MOVE "MNU-COD-NEMO" TO FIO-AKEY.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SIS-FIO-MNU.
           IF NOT FIO-STAT-OKS
               MOVE "MNUCURRNEX" TO MSG-COD-MENS
               GO TO MSG-SCRL-TAB-MNU-PF8.
       NXT-SCRL-TAB-MNU-PF8.
           MOVE "MNU-COD-NEMO" TO FIO-AKEY.
           MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM SIS-FIO-MNU.
           IF NOT FIO-STAT-OKS
               MOVE "MNU    NONXT" TO MSG-COD-MENS
               GO TO MSG-SCRL-TAB-MNU-PF8.
           MOVE FIO-STAT TO PGM-STAT-MNU.
           PERFORM SCR-CLRA-MNU.
           PERFORM PUT-SCR.
           GO TO FIN-SCRL-TAB-MNU-PF8.
       MSG-SCRL-TAB-MNU-PF8.
           MOVE "GNS" TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MEN2.
       FIN-SCRL-TAB-MNU-PF8.
           EXIT.
      *>>>>
      *<<<< EOF
       SIS-FIO-MNU SECTION.
       INI-SIS-FIO-MNU.
           MOVE SCR-SISG TO FIO-SIST.
           PERFORM GNS-FIO-MNU.
       FIN-SIS-FIO-MNU.
           EXIT.
      
       CAL-MNU SECTION.
       INI-CAL-MNU.
           PERFORM INI-CAL.
           PERFORM BUS-MNU.
           IF CLF-STAT-MAL
               GO TO FIN-CAL-MNU.
           PERFORM CAL-POS.
           PERFORM PUT-POS VARYING CLF-I FROM 1 BY 1 UNTIL
                           CLF-I > CLF-IMAX.
       FIN-CAL-MNU.
           EXIT.
      
       INI-CAL SECTION.
       INI-INI-CAL.
           MOVE 0 TO CLF-IMAX.
           MOVE 0 TO CLF-STAT.
           MOVE "CALCULO EFECTUADO OK" TO CLF-MENS.
       FIN-INI-CAL.
           EXIT.
      
       BUS-MNU SECTION.
       INI-BUS-MNU.
           IF MNU-COD-TFUN IN MNU NOT = "MEN"
                MOVE 1 TO CLF-STAT
                MOVE "TIPO FUNCION debe ser MEN (Menu)" TO CLF-MENS
                GO TO FIN-BUS-MNU.
           MOVE SPACES              TO MNU-COD-IMEN IN MNU.
           MOVE MNU-COD-MENU IN MNU TO MNU-COD-CMEN IN MNU.
           MOVE MNU-COD-MENU IN MNU TO CLF-COD-CMEN.
      
           MOVE SCR-SISG        TO FIO-SIST.
           MOVE "MNU-COD-IMEN"  TO FIO-AKEY.
           MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM GNS-FIO-MNU.
           IF NOT ( FIO-STAT-OKS AND
                    MNU-COD-CMEN IN MNU = CLF-COD-CMEN )
               MOVE 1 TO CLF-STAT
               MOVE "NO EXISTEN OPCIONES PARA ESTE MENU" TO CLF-MENS
               GO TO FIN-BUS-MNU.
       LUP-BUS-MNU.
           ADD 1 TO CLF-IMAX.
           MOVE MNU-COD-NEMO IN MNU TO CLF-KEYS(CLF-IMAX).
      
           MOVE SCR-SISG        TO FIO-SIST.
           MOVE "MNU-COD-IMEN"  TO FIO-AKEY.
           MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM GNS-FIO-MNU.
           IF FIO-STAT-OKS AND MNU-COD-CMEN IN MNU = CLF-COD-CMEN
               GO TO LUP-BUS-MNU.
       FIN-BUS-MNU.
           EXIT.
      
       CAL-POS SECTION.
       INI-CAL-POS.
           MOVE 0 TO CLF-J.
           MOVE CLF-FINI(CLF-IMAX) TO CLF-FILA.
           IF CLF-IMAX NOT > 10
                MOVE 24 TO CLF-COLU
                PERFORM CAL-FIL VARYING CLF-I FROM 1 BY 1
                                UNTIL CLF-I > CLF-IMAX
                GO TO FIN-CAL-POS.
           DIVIDE CLF-IMAX BY 2 GIVING CLF-LMAX REMAINDER CLF-REST.
           IF CLF-REST NOT = 0
               ADD 1 TO CLF-LMAX.
           MOVE 1 TO CLF-COLU.
           PERFORM CAL-FIL VARYING CLF-I FROM 1 BY 1
                           UNTIL CLF-I > CLF-LMAX.
           MOVE CLF-FINI(CLF-IMAX) TO CLF-FILA.
           DIVIDE CLF-IMAX BY 2 GIVING CLF-LMAX.
           MOVE 41 TO CLF-COLU.
           PERFORM CAL-FIL VARYING CLF-I FROM 1 BY 1
                           UNTIL CLF-I > CLF-LMAX.
       FIN-CAL-POS.
           EXIT.
      
       CAL-FIL SECTION.
       INI-CAL-FIL.
           ADD 1 TO CLF-J.
           MOVE CLF-FILA TO CLF-FICO(CLF-J, 1)
           MOVE CLF-COLU TO CLF-FICO(CLF-J, 2)
           ADD CLF-SEPA(CLF-IMAX) TO CLF-FILA.
       FIN-CAL-FIL.
           EXIT.
      
       PUT-POS SECTION.
       INI-PUT-POS.
           MOVE CLF-KEYS(CLF-I) TO MNU-COD-NEMO IN MNU.
           MOVE SCR-SISG        TO FIO-SIST.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM GNS-FIO-MNU.
           MOVE CLF-FICO(CLF-I, 1) TO MNU-NUM-FILA IN MNU.
           MOVE CLF-FICO(CLF-I, 2) TO MNU-NUM-COLU IN MNU.
      
           MOVE SCR-SISG TO FIO-SIST.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM GNS-FIO-MNU.
           IF NOT FIO-STAT-OKS
               MOVE 1 TO CLF-STAT
               MOVE "ERROR al ACTUALIZAR FILA/COLUMNA" TO CLF-MENS
               MOVE CLF-IMAX TO CLF-I.
       FIN-PUT-POS.
           EXIT.
      
       BFR-RFSH SECTION.
       INI-BFR-RFSH.
           MOVE "MNU-COD-OPCI" TO FIO-AKEY.
           MOVE FIO-BFR-RFH    TO FIO-CMND.
           PERFORM GNS-FIO-MNU.
           MOVE "MNU-COD-IMEN" TO FIO-AKEY.
           MOVE FIO-BFR-RFH    TO FIO-CMND.
           PERFORM GNS-FIO-MNU.
       FIN-BFR-RFSH.
           EXIT.
      
      *>>>>
       INI-HDR-TRL SECTION.
       INI-INI-HDR-TRL.
      
           MOVE SPACES TO BPR-VARI.
           MOVE "GNS"      TO BPR-SIST.
           MOVE "GNSPAMNU" TO BPR-PROG.
           MOVE SCR-NEMO TO BPR-NEMO.
           CALL "GNSPPBPR IN GNS/EXE/GNSPPBPR"
                  USING BPR-VARI.
           MOVE BPR-NUM-DHOY TO MSK-MNUHDR-NUM-DMSA.
           MOVE BPR-NUM-MHOY TO MSK-MNUHDR-NUM-MMSA.
           MOVE BPR-NUM-SHOY TO MSK-MNUHDR-NUM-SMSA.
           MOVE BPR-NUM-AHOY TO MSK-MNUHDR-NUM-AMSA.
           MOVE "/"      TO MSK-MNUHDR-GLS-1MSA.
           MOVE "/"      TO MSK-MNUHDR-GLS-2MSA.
           MOVE "GNS"            TO MSK-MNUHDR-COD-SIST.
           MOVE BPR-HDR-GLS-NFRM TO MSK-MNUHDR-GLS-NFRM.
           MOVE BPR-HDR-GLS-FPRO TO MSK-MNUHDR-GLS-FPRO.
           MOVE BPR-TRL-GLS-TERM TO MSK-MNUTRL-GLS-TERM.
           MOVE BPR-GLS-CMND     TO MSK-MNUTRL-COD-CMND.
           MOVE BPR-TRL-GLS-PRMT TO MSK-MNUTRL-GLS-PRMT.
      
       FIN-INI-HDR-TRL.
           EXIT.
