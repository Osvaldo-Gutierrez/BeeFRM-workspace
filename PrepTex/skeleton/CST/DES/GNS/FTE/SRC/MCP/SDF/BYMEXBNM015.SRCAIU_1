%!              BYMEXBNM015.SRCAIU PUT-LAF
      * {TIPO}-INI --> BYMEXBNM015.SRCAIU PUT-LAF
%!
%!
% TIPO_{TIPO} = TRUE
% IF TIPO_OFI THEN
           IF SDF-{RFRM}-IND-{NOMB} IN {RREG}F{FRM} NOT =
                  {RFRM}-IND-{NOMB}-ALF IN {FRM}-FLD
                   MOVE  "{RFRM}-IND-{NOMB}" TO LAF-COD-NFLD IN LAF
                   MOVE {RFRM}-IND-{NOMB}-ALF IN {FRM}-FLD TO 
                        LAF-GLS-DOLD IN LAF       
                   MOVE SDF-{RFRM}-IND-{NOMB} IN {RREG}F{FRM} TO
                        LAF-GLS-DNEW IN LAF         
                   PERFORM PUT-LAF. 
           IF SDF-{RFRM}-COD-{NOMB} IN {RREG}F{FRM} NOT =
                  {RFRM}-COD-{NOMB} IN {FRM}-FLD
                   MOVE  "{RFRM}-COD-{NOMB}" TO LAF-COD-NFLD IN LAF
                   MOVE {RFRM}-COD-{NOMB} IN {FRM}-FLD TO 
                        LAF-GLS-DOLD IN LAF       
                   MOVE SDF-{RFRM}-COD-{NOMB} IN {RREG}F{FRM} TO
                        LAF-GLS-DNEW IN LAF         
                   PERFORM PUT-LAF. 

      * FIN-VRF
% ELSE
% IF TIPO_VRF THEN
% IF NOT TIENE_IDC_{RFRM}_IDC_{NOMB} THEN
           IF SDF-{RFRM}-NUM-{NOMB} IN {RREG}F{FRM} NOT =
                  {RFRM}-NUM-{NOMB}-ALF IN {FRM}-FLD
                   MOVE  "{RFRM}-NUM-{NOMB}" TO LAF-COD-NFLD IN LAF
                   MOVE {RFRM}-NUM-{NOMB}-ALF IN {FRM}-FLD TO 
                        LAF-GLS-DOLD IN LAF       
                   MOVE SDF-{RFRM}-NUM-{NOMB} IN {RREG}F{FRM} TO
                        LAF-GLS-DNEW IN LAF         
                   PERFORM PUT-LAF. 
           IF SDF-{RFRM}-VRF-{NOMB} IN {RREG}F{FRM} NOT =
                  {RFRM}-VRF-{NOMB} IN {FRM}-FLD
                   MOVE  "{RFRM}-VRF-{NOMB}" TO LAF-COD-NFLD IN LAF
                   MOVE {RFRM}-VRF-{NOMB} IN {FRM}-FLD TO 
                        LAF-GLS-DOLD IN LAF       
                   MOVE SDF-{RFRM}-VRF-{NOMB} IN {RREG}F{FRM} TO
                        LAF-GLS-DNEW IN LAF         
                   PERFORM PUT-LAF. 
% ELSE
           IF SDF-{RFRM}-IDC-{NOMB} IN {RREG}F{FRM} NOT =
                  {RFRM}-IDC-{NOMB} IN {FRM}-FLD
                   MOVE  "{RFRM}-IDC-{NOMB}" TO LAF-COD-NFLD IN LAF
                   MOVE {RFRM}-IDC-{NOMB} IN {FRM}-FLD TO 
                        LAF-GLS-DOLD IN LAF       
                   MOVE SDF-{RFRM}-IDC-{NOMB} IN {RREG}F{FRM} TO
                        LAF-GLS-DNEW IN LAF         
                   PERFORM PUT-LAF. 
% END IF
% END IF
% END IF
% TIPO_{TIPO} = FALSE
      * {TIPO}-FIN --> BYMEXBNM015.SRCAIU PUT-SDF
