%! BY012.SRC MOVE DE KEY-LAF PARA CAMPOS GENERADOS
      * BYCLBBCQ012.SRC 
% TIPO_{TIPO} = TRUE
%! TIPO VRF
%!
% IF TIPO_VRF THEN
% IF NOT TIENE_IDC_{RREG}_IDC_{NOMB} THEN
% IF TIDX > 0 THEN
% FOR I = 1 TO {TIDX}
           MOVE {FFRM}-NUM-{NOMB} IN {FRM}-FLD({I}) TO {RREG}-NUM-{NOMB} IN {frm}-KYS({I}).
           MOVE {RFRM}-VRF-{NOMB} IN {FRM}-FLD({I}) TO {RREG}-VRF-{NOMB} IN {frm}-KYS({I}).
% NEXT I
% ELSE
           MOVE {RFRM}-NUM-{NOMB} IN {FRM}-FLD TO {RREG}-NUM-{NOMB} IN {frm}-KYS.
           MOVE {RFRM}-VRF-{NOMB} IN {FRM}-FLD TO {RREG}-VRF-{NOMB} IN {frm}-KYS.
% END IF
% END IF
% ELSE
%! TIPO IDC
%!
% IF TIPO_IDC THEN
% IF TIDX > 0 THEN
% FOR I = 1 TO {TIDX}
           MOVE {RFRM}-NUM-{NOMB} IN {FRM}-FLD({I}) TO {RREG}-NUM-{NOMB} IN {frm}-KYS({I}).
           MOVE {RFRM}-VRF-{NOMB} IN {FRM}-FLD({I}) TO {RREG}-VRF-{NOMB} IN {frm}-KYS({I}).
           MOVE {RFRM}-IND-{NOMB} IN {FRM}-FLD({I}) TO {RREG}-IND-{NOMB} IN {frm}-KYS({I}).
           MOVE {RFRM}-GLS-{NOMB} IN {FRM}-FLD({I}) TO {RREG}-GLS-{NOMB} IN {frm}-KYS({I}).
% NEXT I
% ELSE
           MOVE {RFRM}-NUM-{NOMB} IN {FRM}-FLD TO {RREG}-NUM-{NOMB} IN {frm}-KYS.
           MOVE {RFRM}-VRF-{NOMB} IN {FRM}-FLD TO {RREG}-VRF-{NOMB} IN {frm}-KYS.
           MOVE {RFRM}-IND-{NOMB} IN {FRM}-FLD TO {RREG}-IND-{NOMB} IN {frm}-KYS.
           MOVE {RFRM}-GLS-{NOMB} IN {FRM}-FLD TO {RREG}-GLS-{NOMB} IN {frm}-KYS.
% END IF
% ELSE
%! TIPO FEC Y SUS DERIVADOS
%! EL TIPO FEC TIENE UN TRATAMIENTO ESPECIFICO, PUES SU APARICION ES MUCHO MAS FRECUENTE QUE EL RESTO
% IF TIPO_FEC
% IF TIDX > 0 THEN
% FOR I = 1 TO {TIDX}
% IF NORMA_CST THEN
           MOVE {RFRM}-NUM-D{PFEC} IN {FRM}-FLD({I}) TO {RREG}-NUM-D{PFEC} IN {frm}-KYS({I}).
           MOVE {RFRM}-NUM-M{PFEC} IN {FRM}-FLD({I}) TO {RREG}-NUM-M{PFEC} IN {frm}-KYS({I}).
           MOVE {RFRM}-NUM-S{PFEC} IN {FRM}-FLD({I}) TO {RREG}-NUM-S{PFEC} IN {frm}-KYS({I}).
           MOVE {RFRM}-NUM-A{PFEC} IN {FRM}-FLD({I}) TO {RREG}-NUM-A{PFEC} IN {frm}-KYS({I}).
% ELSE
           MOVE {RFRM}-FD-{NOMB} IN {FRM}-FLD({I}) TO {RREG}-FD-{NOMB} IN {frm}-KYS({I}).
           MOVE {RFRM}-FM-{NOMB} IN {FRM}-FLD({I}) TO {RREG}-FM-{NOMB} IN {frm}-KYS({I}).
           MOVE {RFRM}-FA-{NOMB} IN {FRM}-FLD({I}) TO {RREG}-FA-{NOMB} IN {frm}-KYS({I}).
% END IF
% NEXT I
% ELSE
% IF NORMA_CST THEN
           MOVE {RFRM}-NUM-D{PFEC} IN {FRM}-FLD TO {RREG}-NUM-D{PFEC} IN {frm}-KYS.
           MOVE {RFRM}-NUM-M{PFEC} IN {FRM}-FLD TO {RREG}-NUM-M{PFEC} IN {frm}-KYS.
           MOVE {RFRM}-NUM-S{PFEC} IN {FRM}-FLD TO {RREG}-NUM-S{PFEC} IN {frm}-KYS.
           MOVE {RFRM}-NUM-A{PFEC} IN {FRM}-FLD TO {RREG}-NUM-A{PFEC} IN {frm}-KYS.
% ELSE
           MOVE {RFRM}-FD-{NOMB} IN {FRM}-FLD TO {RREG}-FD-{NOMB} IN {frm}-KYS.
           MOVE {RFRM}-FM-{NOMB} IN {FRM}-FLD TO {RREG}-FM-{NOMB} IN {frm}-KYS.
           MOVE {RFRM}-FA-{NOMB} IN {FRM}-FLD TO {RREG}-FA-{NOMB} IN {frm}-KYS.
% END IF
% END IF
% ELSE
% IF TIPO_DMA OR TIPO_AMD OR TIPO_MSA OR TIPO_SAM OR TIPO_FMA OR TIPO_FAM OR TIPO_FSA OR TIPO_FAS OR TIPO_FDM OR TIPO_FMD OR TIPO_FFA THEN
% IF TIDX > 0 THEN
% FOR I = 1 TO {TIDX}
% IF NORMA_CST THEN
% IF TIPO_DMA OR TIPO_AMD OR TIPO_FDM OR TIPO_FMD THEN
           MOVE {RFRM}-NUM-D{PFEC} IN {FRM}-FLD({I}) TO {RREG}-NUM-D{PFEC} IN {frm}-KYS({I}).
% END IF
% IF NOT ( TIPO_FSA OR TIPO_FAS OR TIPO_FFA ) THEN
           MOVE {RFRM}-NUM-M{PFEC} IN {FRM}-FLD({I}) TO {RREG}-NUM-M{PFEC} IN {frm}-KYS({I}).
% END IF
% IF TIPO_MSA OR TIPO_SAM OR TIPO_FSA OR TIPO_FAS THEN
           MOVE {RFRM}-NUM-S{PFEC} IN {FRM}-FLD({I}) TO {RREG}-NUM-S{PFEC} IN {frm}-KYS({I}).
% END IF
% IF NOT ( TIPO_FDM OR TIPO_FMD ) THEN
           MOVE {RFRM}-NUM-A{PFEC} IN {FRM}-FLD({I}) TO {RREG}-NUM-A{PFEC} IN {frm}-KYS({I}).
% END IF
% ELSE
% IF TIPO_DMA OR TIPO_AMD OR TIPO_FDM OR TIPO_FMD THEN
           MOVE {RFRM}-FD-{NOMB} IN {FRM}-FLD({I}) TO {RREG}-FD-{NOMB} IN {frm}-KYS({I}).
% END IF
% IF NOT ( TIPO_FSA OR TIPO_FAS OR TIPO_FFA ) THEN
           MOVE {RFRM}-FM-{NOMB} IN {FRM}-FLD({I}) TO {RREG}-FM-{NOMB} IN {frm}-KYS({I}).
% END IF
% IF TIPO_MSA OR TIPO_SAM OR TIPO_FSA OR TIPO_FAS THEN
           MOVE {RFRM}-FA-{NOMB} IN {FRM}-FLD({I}) TO {RREG}-FA-{NOMB} IN {frm}-KYS({I}).
% END IF
% END IF
% NEXT I
% ELSE
% IF NORMA_CST THEN
% IF TIPO_DMA OR TIPO_AMD OR TIPO_FDM OR TIPO_FMD THEN
           MOVE {RFRM}-NUM-D{PFEC} IN {FRM}-FLD TO {RREG}-NUM-D{PFEC} IN {frm}-KYS.
% END IF
% IF NOT ( TIPO_FSA OR TIPO_FAS OR TIPO_FFA ) THEN
           MOVE {RFRM}-NUM-M{PFEC} IN {FRM}-FLD TO {RREG}-NUM-M{PFEC} IN {frm}-KYS.
% END IF
% IF TIPO_MSA OR TIPO_SAM OR TIPO_FSA OR TIPO_FAS THEN
           MOVE {RFRM}-NUM-S{PFEC} IN {FRM}-FLD TO {RREG}-NUM-S{PFEC} IN {frm}-KYS.
% END IF
% IF TIPO_MSA OR TIPO_SAM OR TIPO_FSA OR TIPO_FAS THEN
           MOVE {RFRM}-NUM-A{PFEC} IN {FRM}-FLD TO {RREG}-NUM-A{PFEC} IN {frm}-KYS.
% END IF
% ELSE
% IF TIPO_DMA OR TIPO_AMD OR TIPO_FDM OR TIPO_FMD THEN
           MOVE {RFRM}-FD-{NOMB} IN {FRM}-FLD TO {RREG}-FD-{NOMB} IN {frm}-KYS.
% END IF
% IF NOT ( TIPO_FSA OR TIPO_FAS OR TIPO_FFA ) THEN
           MOVE {RFRM}-FM-{NOMB} IN {FRM}-FLD TO {RREG}-FM-{NOMB} IN {frm}-KYS.
% END IF
% IF NOT ( TIPO_FDM OR TIPO_FMD ) THEN
           MOVE {RFRM}-FA-{NOMB} IN {FRM}-FLD TO {RREG}-FA-{NOMB} IN {frm}-KYS.
% END IF
% END IF
% END IF
% END IF
% END IF
% END IF
% END IF
% TIPO_{TIPO} = FALSE
