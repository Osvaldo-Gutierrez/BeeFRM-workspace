%!              BMMEXBNM003.SRCAIU CLR-KEY Y CLR-FLD
      * {TIPO}-INI --> BMMEXBNM003.SRCAIU CLR-KEY Y CLR-FLD
% TIPO_{TIPO} = TRUE
%! OFI
% IF TIPO_OFI THEN
% IF TIDX > 0 THEN
% FOR I = 1 TO {TIDX}
           MOVE ZEROES TO {RREG}-IND-{NOMB} IN {FRM}-FLD({I}).
           MOVE ZEROES TO {RREG}-COD-{NOMB} IN {FRM}-FLD({I}).
% NEXT I
% ELSE
           MOVE ZEROES TO {RREG}-IND-{NOMB} IN {FRM}-FLD.
           MOVE ZEROES TO {RREG}-COD-{NOMB} IN {FRM}-FLD.
% END IF
% ELSE
%! VRF
% IF TIPO_VRF THEN
% IF NOT TIENE_IDC_{RREG}_IDC_{NOMB} THEN
% IF TIDX > 0 THEN
% FOR I = 1 TO {TIDX}
           MOVE ZEROES TO {RREG}-NUM-{NOMB} IN {FRM}-FLD({I}).
           MOVE SPACES TO {RREG}-VRF-{NOMB} IN {FRM}-FLD({I}).
% NEXT I
% ELSE
           MOVE ZEROES TO {RREG}-NUM-{NOMB} IN {FRM}-FLD.
           MOVE SPACES TO {RREG}-VRF-{NOMB} IN {FRM}-FLD.
% END IF
% END IF
% ELSE
%! IDC
% IF TIPO_IDC THEN
% IF TIDX > 0 THEN
% FOR I = 1 TO {TIDX}
           MOVE ZEROES TO {RREG}-IDC-{NOMB} IN {FRM}-FLD({I}).
% NEXT I
% ELSE
           MOVE ZEROES TO {RREG}-IDC-{NOMB} IN {FRM}-FLD.
% END IF
% ELSE
%!
% IF TIPO_FEC OR TIPO_DMA OR TIPO_AMD OR TIPO_MSA OR TIPO_SAM OR TIPO_FMA OR TIPO_FAM OR TIPO_FSA OR TIPO_FAS OR TIPO_FDM OR TIPO_FMD OR TIPO_FFA THEN
% IF TIDX > 0 THEN
% FOR I = 1 TO {TIDX}
           MOVE ZEROES TO {CAMP} IN {FRM}-FLD({I}).
% NEXT I
% ELSE
           MOVE ZEROES TO {CAMP} IN {FRM}-FLD.
% END IF
% END IF
% END IF
% END IF
% END IF
% TIPO_{TIPO} = FALSE
      * {TIPO}-FIN --> BMMEXBNM003.SRCAIU CLR-KEY Y CLR-FLD
