$	FIL = P1
$	ORG = P2
$	SIS = P3
$	RGT = P4
$
$	COPY/NOLOG 'FIL'.PTX 'FIL'PRB.PTX
$
$	ON WARNING   THEN GOTO CLO
$	ON CONTROL_Y THEN GOTO CLO
$
$	OPEN/APPEND FILE_PTXFIO 'FIL'.PTX
$	WRITE FILE_PTXFIO "*% INCLUDE 'GNSSRCAIO:GNSPF''ORG'.SRCAIO'"
$
$	OPEN/APPEND FILE_PTXPRB 'FIL'PRB.PTX
$	WRITE FILE_PTXPRB "*% INCLUDE 'GNSSRCAIO:GNSPR''ORG'.SRCAIO'"
$
$	CLOSE FILE_PTXFIO
$	CLOSE FILE_PTXPRB
$
$	PTX 'FIL'.PTX    'SIS'PF'RGT'.AIO/CONTROL="*%"/UNDEFINED=ZERO/NOSHOW
$	PTX 'FIL'PRB.PTX 'SIS'PR'RGT'.AIO/CONTROL="*%"/UNDEFINED=ZERO/NOSHOW
$
$	PTH = "''SIS'PF''RGT'.AIO"
$	TPR = "A"
$!	RUN GNSEXE:GNSDOCPRO
$
$	CREM = """
$	ON WARNING   THEN GOTO ERR
$	ON CONTROL_Y THEN GOTO ERR
$	OPEN/WRITE FILE_OUTPUT 'SIS'BF'RGT'.AIO
$	WRITE FILE_OUTPUT "      * Modulo FIO para archivo RMS ''P3'''P4', ''F$TIME()'"
$	WRITE FILE_OUTPUT "       ''P3'-FIO-''P4' SECTION."
$	WRITE FILE_OUTPUT "       INI-''P3'-FIO-''P4'."
$	WRITE FILE_OUTPUT "           CALL " + CREM + "''P3'PF''P4' IN OBJECT/''P3'PF''P4'" + CREM
$	WRITE FILE_OUTPUT "                                   USING FIO-VARI ''P4'."
$	WRITE FILE_OUTPUT "           MOVE FIO-STAT-LNK TO FIO-STAT."
$	WRITE FILE_OUTPUT "       FIN-''P3'-FIO-''P4'."
$	WRITE FILE_OUTPUT "           EXIT."
$	CLOSE FILE_OUTPUT
$ FIN:
$	SET NOON
$	DELETE/NOLOG/NOCONF 'FIL'.PTX;
$	DELETE/NOLOG/NOCONF 'FIL'PRB.PTX;
$	WRITE SYS$OUTPUT "[A[2K"
$	WRITE SYS$OUTPUT "[1m	",-
"''SIS'PF''RGT'.AIO, ''SIS'PR''RGT'.AIO, ''SIS'BF''RGT'.AIO Generado[0m"
$	EXIT
$ CLO:
$	SET NOON
$	CLOSE FILE_PTXFIO
$	DELETE/NOLOG/NOCONF 'FIL'.PTX;
$	DELETE/NOLOG/NOCONF 'FIL'PRB.PTX;
$	EXIT 6
$ ERR:
$	SET NOON
$	CLOSE FILE_OUTPUT
$	DELETE/NOLOG/NOCONF 'FIL'.PTX;
$	DELETE/NOLOG/NOCONF 'FIL'PRB.PTX;
$	EXIT 6
