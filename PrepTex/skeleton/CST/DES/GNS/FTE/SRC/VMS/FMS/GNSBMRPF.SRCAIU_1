       FIN-VAL-FRM-GLS-CONF.
           EXIT.
       
       VAL-FRM-GLS-UARC SECTION.
       INI-VAL-FRM-GLS-UARC.
           IF FRM-GLS-UARC IN RPF-FLD(FRM-IFLD) NOT > SPACES
              MOVE SPACES TO FRM-GLS-UARC IN RPF-FLD(FRM-IFLD).
       FIN-VAL-FRM-GLS-UARC.
           EXIT.
       
       VAL-CON-KEY-RPF SECTION.
       INI-VAL-CON-KEY-RPF.
           IF     RPF-COD-DPRO IN RPF-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-RPF.
       FIN-VAL-CON-KEY-RPF.
           EXIT.
       
       VAL-CON-IKY-RPF SECTION.
       INI-VAL-CON-IKY-RPF.
           IF     RPF-COD-DPRO IN RPF-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-RPF.
       FIN-VAL-CON-IKY-RPF.
           EXIT.
       
       VAL-CON-FLD-RPF SECTION.
       INI-VAL-CON-FLD-RPF.
      *<<<< VAL_CON_FLD_RPF
           IF SCR-CMND = 'COP'
               MOVE RPF-COD-DPRO IN RPF-FLD TO RPF-COD-DPRO IN RPF
               MOVE SPACES                  TO RPF-COD-TRYC IN RPF
               MOVE SCR-SISG                TO FIO-SIST
               MOVE FIO-GET-KEY             TO FIO-CMND
               PERFORM GNS-FIO-RPF
               IF FIO-STAT-OKS
                   MOVE 'GNS'          TO MSG-COD-SIST
                   MOVE 'KEYPRIMDUPLI' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC   TO FRM-MENS
                   MOVE FRM-SUAR-MAL   TO FRM-SUAR
                   GO TO FIN-VAL-CON-FLD-RPF.
           PERFORM SIN-CMND.
           IF FRM-SUAR = FRM-SUAR-MAL
               GO TO FIN-VAL-CON-FLD-RPF.
      *>>>>
       FIN-VAL-CON-FLD-RPF.
           EXIT.
       
       VAL-CON-RPF SECTION.
       INI-VAL-CON-RPF.
       FIN-VAL-CON-RPF.
           EXIT.
       
       VAL-NUM-KEY-RPF SECTION.
       INI-VAL-NUM-KEY-RPF.
           PERFORM VAL-NUM-IKY-RPF.
           PERFORM VAL-NUM-MKY-RPF.
           PERFORM VAL-NUM-AKY-RPF.
       FIN-VAL-NUM-KEY-RPF.
           EXIT.
       
       VAL-NUM-IKY-RPF SECTION.
       INI-VAL-NUM-IKY-RPF.
       FIN-VAL-NUM-IKY-RPF.
           EXIT.
       
       VAL-NUM-MKY-RPF SECTION.
       INI-VAL-NUM-MKY-RPF.
       FIN-VAL-NUM-MKY-RPF.
           EXIT.
       
       VAL-NUM-AKY-RPF SECTION.
       INI-VAL-NUM-AKY-RPF.
       FIN-VAL-NUM-AKY-RPF.
           EXIT.
       
       VAL-NUM-FLD-RPF SECTION.
       INI-VAL-NUM-FLD-RPF.
       FIN-VAL-NUM-FLD-RPF.
           EXIT.
