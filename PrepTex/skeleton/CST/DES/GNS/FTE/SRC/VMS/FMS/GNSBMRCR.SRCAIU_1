      * 23-OCT-91 07:44 PM
       PUT-ALL-RCR SECTION.
       INI-PUT-ALL-RCR.
           PERFORM PUT-RCR-RCR.
       FIN-PUT-ALL-RCR.
           EXIT.
       
       PUT-RCR-RCR SECTION.
       INI-PUT-RCR-RCR.
           IF PGM-STAT-RCR-NUL
               GO TO FIN-PUT-RCR-RCR.
           IF NOT PGM-STAT-RCR-OKS
               PERFORM CLR-RCR-RCR
               GO TO FIN-PUT-RCR-RCR.
           MOVE RCR-COD-RECO IN RCR TO RCR-COD-RECO IN RCR-FLD.
           MOVE RCR-COD-DOMI IN RCR TO RCR-COD-DOMI IN RCR-FLD.
           MOVE RCR-COD-SIST IN RCR TO RCR-COD-SIST IN RCR-FLD.
           MOVE RCR-COD-REGI IN RCR TO RCR-COD-REGI IN RCR-FLD.
           MOVE RCR-GLS-FIMP IN RCR TO RCR-GLS-FIMP IN RCR-FLD.
           MOVE RCR-GLS-VALI IN RCR TO RCR-GLS-VALI IN RCR-FLD.
           MOVE RCR-GLS-BUSQ IN RCR TO RCR-GLS-BUSQ IN RCR-FLD.
       FIN-PUT-RCR-RCR.
           EXIT.
       
       RET-ALL-RCR SECTION.
       INI-RET-ALL-RCR.
           PERFORM KEY-ALL-RCR.
           PERFORM RET-RCR-RCR.
       FIN-RET-ALL-RCR.
           EXIT.
       
       RET-RCR-RCR SECTION.
       INI-RET-RCR-RCR.
           MOVE RCR-COD-DOMI IN RCR-FLD TO RCR-COD-DOMI IN RCR.
           MOVE RCR-COD-SIST IN RCR-FLD TO RCR-COD-SIST IN RCR.
           MOVE RCR-COD-REGI IN RCR-FLD TO RCR-COD-REGI IN RCR.
           MOVE RCR-GLS-FIMP IN RCR-FLD TO RCR-GLS-FIMP IN RCR.
           MOVE RCR-GLS-VALI IN RCR-FLD TO RCR-GLS-VALI IN RCR.
           MOVE RCR-GLS-BUSQ IN RCR-FLD TO RCR-GLS-BUSQ IN RCR.
       FIN-RET-RCR-RCR.
           EXIT.
       
       KEY-ALL-RCR SECTION.
       INI-KEY-ALL-RCR.
           PERFORM KEY-RCR-RCR.
       FIN-KEY-ALL-RCR.
           EXIT.
       
       KEY-RCR-RCR SECTION.
       INI-KEY-RCR-RCR.
           MOVE RCR-COD-RECO IN RCR-FLD TO RCR-COD-RECO IN RCR.
       FIN-KEY-RCR-RCR.
           EXIT.
       
       SCR-CLRA-RCR SECTION.
       INI-SCR-CLRA-RCR.
           MOVE FRM-CLR-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-CLRA-RCR.
           EXIT.
       
       CLR-KEY-RCR SECTION.
       INI-CLR-KEY-RCR.
           MOVE SPACES TO RCR-COD-RECO IN RCR-FLD.
       FIN-CLR-KEY-RCR.
           EXIT.
       
       CLR-FLD-RCR SECTION.
       INI-CLR-FLD-RCR.
           PERFORM CLR-RCR-RCR.
       FIN-CLR-FLD-RCR.
           EXIT.
       
       CLR-RCR-RCR SECTION.
       INI-CLR-RCR-RCR.
           MOVE SPACES TO RCR-COD-DOMI IN RCR-FLD.
           MOVE SPACES TO RCR-COD-SIST IN RCR-FLD.
           MOVE SPACES TO RCR-COD-REGI IN RCR-FLD.
           MOVE SPACES TO RCR-GLS-FIMP IN RCR-FLD.
           MOVE SPACES TO RCR-GLS-VALI IN RCR-FLD.
           MOVE SPACES TO RCR-GLS-BUSQ IN RCR-FLD.
       FIN-CLR-RCR-RCR.
           EXIT.
       
       FST-KEY-RCR SECTION.
       INI-FST-KEY-RCR.
           MOVE -1 TO RCR-COD-RECO-LEN IN RCR-FLD.
       FIN-FST-KEY-RCR.
           EXIT.
       
       FST-FLD-RCR SECTION.
       INI-FST-FLD-RCR.
           MOVE -1 TO RCR-COD-DOMI-LEN IN RCR-FLD.
       FIN-FST-FLD-RCR.
           EXIT.
       
       PRO-KEY-RCR SECTION.
       INI-PRO-KEY-RCR.
           PERFORM PRO-IKY-RCR.
           PERFORM PRO-AKY-RCR.
           PERFORM PRO-MKY-RCR.
       FIN-PRO-KEY-RCR.
           EXIT.
       
       PRO-IKY-RCR SECTION.
       INI-PRO-IKY-RCR.
           MOVE FRM-CPIM-AEY TO RCR-COD-RECO-ATR IN RCR-FLD.
       FIN-PRO-IKY-RCR.
           EXIT.
       
       PRO-MKY-RCR SECTION.
       INI-PRO-MKY-RCR.
       FIN-PRO-MKY-RCR.
           EXIT.
       
       PRO-AKY-RCR SECTION.
       INI-PRO-AKY-RCR.
       FIN-PRO-AKY-RCR.
           EXIT.
       
       PRO-FLD-RCR SECTION.
       INI-PRO-FLD-RCR.
           PERFORM PRO-RCR-RCR.
       FIN-PRO-FLD-RCR.
           EXIT.
       
       PRO-RCR-RCR SECTION.
       INI-PRO-RCR-RCR.
           MOVE FRM-CPIM-AEY TO RCR-COD-DOMI-ATR IN RCR-FLD.
           MOVE FRM-CPIM-AEY TO RCR-COD-SIST-ATR IN RCR-FLD.
           MOVE FRM-CPIM-AEY TO RCR-COD-REGI-ATR IN RCR-FLD.
           MOVE FRM-CPIM-AEY TO RCR-GLS-FIMP-ATR IN RCR-FLD.
           MOVE FRM-CPIM-AEY TO RCR-GLS-VALI-ATR IN RCR-FLD.
           MOVE FRM-CPIM-AEY TO RCR-GLS-BUSQ-ATR IN RCR-FLD.
       FIN-PRO-RCR-RCR.
           EXIT.
       
       UNP-KEY-RCR SECTION.
       INI-UNP-KEY-RCR.
           PERFORM UNP-IKY-RCR.
           PERFORM UNP-AKY-RCR.
           PERFORM UNP-MKY-RCR.
       FIN-UNP-KEY-RCR.
           EXIT.
       
       UNP-IKY-RCR SECTION.
       INI-UNP-IKY-RCR.
           MOVE FRM-CPIM-UBY TO RCR-COD-RECO-ATR.
       FIN-UNP-IKY-RCR.
           EXIT.
       
       UNP-MKY-RCR SECTION.
       INI-UNP-MKY-RCR.
       FIN-UNP-MKY-RCR.
           EXIT.
       
       UNP-AKY-RCR SECTION.
       INI-UNP-AKY-RCR.
       FIN-UNP-AKY-RCR.
           EXIT.
       
       UNP-FLD-RCR SECTION.
       INI-UNP-FLD-RCR.
           PERFORM UNP-RCR-RCR.
       FIN-UNP-FLD-RCR.
           EXIT.
       
       UNP-RCR-RCR SECTION.
       INI-UNP-RCR-RCR.
           MOVE FRM-CPIM-UBY TO RCR-COD-DOMI-ATR IN RCR-FLD.
           MOVE FRM-CPIM-UBY TO RCR-COD-SIST-ATR IN RCR-FLD.
           MOVE FRM-CPIM-UBY TO RCR-COD-REGI-ATR IN RCR-FLD.
           MOVE FRM-CPIM-UBY TO RCR-GLS-FIMP-ATR IN RCR-FLD.
           MOVE FRM-CPIM-UBY TO RCR-GLS-VALI-ATR IN RCR-FLD.
           MOVE FRM-CPIM-UBY TO RCR-GLS-BUSQ-ATR IN RCR-FLD.
       FIN-UNP-RCR-RCR.
           EXIT.
       
       VAL-KEY-RCR SECTION.
       INI-VAL-KEY-RCR.
           PERFORM VAL-IKY-RCR.
           PERFORM VAL-MKY-RCR.
           PERFORM VAL-AKY-RCR.
       FIN-VAL-KEY-RCR.
           EXIT.
       
       VAL-IKY-RCR SECTION.
       INI-VAL-IKY-RCR.
           PERFORM VAL-RCR-COD-RECO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RCR-COD-RECO-LEN IN RCR-FLD
                GO TO FIN-VAL-IKY-RCR.
       FIN-VAL-IKY-RCR.
           EXIT.
       
       VAL-MKY-RCR SECTION.
       INI-VAL-MKY-RCR.
       FIN-VAL-MKY-RCR.
           EXIT.
       
       VAL-AKY-RCR SECTION.
       INI-VAL-AKY-RCR.
       FIN-VAL-AKY-RCR.
           EXIT.
       
       VAL-FLD-RCR SECTION.
       INI-VAL-FLD-RCR.
           PERFORM VAL-RCR-COD-DOMI.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RCR-COD-DOMI-LEN IN RCR-FLD
                GO TO FIN-VAL-FLD-RCR.
           PERFORM VAL-RCR-COD-SIST.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RCR-COD-SIST-LEN IN RCR-FLD
                GO TO FIN-VAL-FLD-RCR.
           PERFORM VAL-RCR-COD-REGI.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RCR-COD-REGI-LEN IN RCR-FLD
                GO TO FIN-VAL-FLD-RCR.
           PERFORM VAL-RCR-GLS-FIMP.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RCR-GLS-FIMP-LEN IN RCR-FLD
                GO TO FIN-VAL-FLD-RCR.
           PERFORM VAL-RCR-GLS-VALI.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RCR-GLS-VALI-LEN IN RCR-FLD
                GO TO FIN-VAL-FLD-RCR.
           PERFORM VAL-RCR-GLS-BUSQ.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO RCR-GLS-BUSQ-LEN IN RCR-FLD
                GO TO FIN-VAL-FLD-RCR.
       FIN-VAL-FLD-RCR.
           EXIT.
       
       VAL-RCR-COD-RECO SECTION.
       INI-VAL-RCR-COD-RECO.
           IF RCR-COD-RECO IN RCR-FLD NOT > SPACES
              MOVE SPACES TO RCR-COD-RECO IN RCR-FLD.
       FIN-VAL-RCR-COD-RECO.
           EXIT.
       
       VAL-RCR-COD-DOMI SECTION.
       INI-VAL-RCR-COD-DOMI.
           IF RCR-COD-DOMI IN RCR-FLD NOT > SPACES
              MOVE SPACES TO RCR-COD-DOMI IN RCR-FLD.
       FIN-VAL-RCR-COD-DOMI.
           EXIT.
       
       VAL-RCR-COD-SIST SECTION.
       INI-VAL-RCR-COD-SIST.
           IF RCR-COD-SIST IN RCR-FLD NOT > SPACES
              MOVE SPACES TO RCR-COD-SIST IN RCR-FLD.
       FIN-VAL-RCR-COD-SIST.
           EXIT.
       
       VAL-RCR-COD-REGI SECTION.
       INI-VAL-RCR-COD-REGI.
           IF RCR-COD-REGI IN RCR-FLD NOT > SPACES
              MOVE SPACES TO RCR-COD-REGI IN RCR-FLD.
       FIN-VAL-RCR-COD-REGI.
           EXIT.
       
       VAL-RCR-GLS-FIMP SECTION.
       INI-VAL-RCR-GLS-FIMP.
           IF RCR-GLS-FIMP IN RCR-FLD NOT > SPACES
              MOVE SPACES TO RCR-GLS-FIMP IN RCR-FLD.
       FIN-VAL-RCR-GLS-FIMP.
           EXIT.
       
       VAL-RCR-GLS-VALI SECTION.
       INI-VAL-RCR-GLS-VALI.
           IF RCR-GLS-VALI IN RCR-FLD NOT > SPACES
              MOVE SPACES TO RCR-GLS-VALI IN RCR-FLD.
       FIN-VAL-RCR-GLS-VALI.
           EXIT.
       
       VAL-RCR-GLS-BUSQ SECTION.
       INI-VAL-RCR-GLS-BUSQ.
           IF RCR-GLS-BUSQ IN RCR-FLD NOT > SPACES
              MOVE SPACES TO RCR-GLS-BUSQ IN RCR-FLD.
       FIN-VAL-RCR-GLS-BUSQ.
           EXIT.
       
       VAL-CON-KEY-RCR SECTION.
       INI-VAL-CON-KEY-RCR.
           IF     RCR-COD-RECO IN RCR-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-RCR.
       FIN-VAL-CON-KEY-RCR.
           EXIT.
       
       VAL-CON-IKY-RCR SECTION.
       INI-VAL-CON-IKY-RCR.
           IF     RCR-COD-RECO IN RCR-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-RCR.
       FIN-VAL-CON-IKY-RCR.
           EXIT.
       
       VAL-CON-FLD-RCR SECTION.
       INI-VAL-CON-FLD-RCR.
       FIN-VAL-CON-FLD-RCR.
           EXIT.
       
       VAL-CON-RCR SECTION.
       INI-VAL-CON-RCR.
       FIN-VAL-CON-RCR.
           EXIT.
       
       VAL-NUM-KEY-RCR SECTION.
       INI-VAL-NUM-KEY-RCR.
           PERFORM VAL-NUM-IKY-RCR.
           PERFORM VAL-NUM-MKY-RCR.
           PERFORM VAL-NUM-AKY-RCR.
       FIN-VAL-NUM-KEY-RCR.
           EXIT.
       
       VAL-NUM-IKY-RCR SECTION.
       INI-VAL-NUM-IKY-RCR.
       FIN-VAL-NUM-IKY-RCR.
           EXIT.
       
       VAL-NUM-MKY-RCR SECTION.
       INI-VAL-NUM-MKY-RCR.
       FIN-VAL-NUM-MKY-RCR.
           EXIT.
       
       VAL-NUM-AKY-RCR SECTION.
       INI-VAL-NUM-AKY-RCR.
       FIN-VAL-NUM-AKY-RCR.
           EXIT.
       
       VAL-NUM-FLD-RCR SECTION.
       INI-VAL-NUM-FLD-RCR.
       FIN-VAL-NUM-FLD-RCR.
           EXIT.
