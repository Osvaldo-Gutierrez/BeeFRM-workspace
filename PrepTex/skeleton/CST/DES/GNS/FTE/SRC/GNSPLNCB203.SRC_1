       COPY GNSBGDTC.
*% IF PGM_V31
       COPY GNSBGIDD.
*% ELSE
       COPY GNSBGIFD.
*% END
       COPY GNSBTABT.
*% END
       COPY GNSBBSYS.
*% IF NOT NO_INC_TAB
       COPY GNSBFTAB.
*% END
*% IF NOT ELI_MSG
       COPY GNSBFMSG.
*% END
       COPY GNSBGEND.
*% IF NOT NO_INC_GNSPLHDR
       COPY GNSBGIRH.
*% END
       COPY ^srp^RE^rep^.
^ctl^
