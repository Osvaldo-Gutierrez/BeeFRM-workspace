*%!              WICHIBCI005.SRCAIU
*%! PARA TIPO FEC
*%!
*%!     SIGLA    FORMATO             D M S A
*%!     FEC      D M S A             X X X X
*%!     DMA      D M A               X X   X
*%!     AMD      D M A               X X   X
*%!     MSA      M S A                 X X X
*%!     SAM      M S A                 X X X
*%!     FMA      M A                   X   X
*%!     FAM      M A                   X   X
*%!     FSA      S A                     X X
*%!     FAS      S A                     X X
*%!     FDM      D M                 X X
*%!     FMD      D M                 X X
*%!     FFA      A                         X
*%!
*% TIPO_{TIPO} = TRUE
*% IF MSKIDC THEN
*% 	SVRF1 = "         "
*% 	SVRF2 = "             "
*% 	SVRFF = "                   "
*% ELSE
*% 	SVRF1 = "     "
*% 	SVRF2 = "         "
*% 	SVRFF = "                   "
*% END IF
*%!
*%! TIPO VRF
*%!
*%! TIPO IDC
*%!
*% IF TIPO_VRF THEN
{PADR}{RREG}-NUM-{NOMB}{IDX}      9( 8, 0,N)  (08)  "{DESCMTI01}" !!! ( Ptx Mti 04 )
{PADR}{RREG}-VRF-{NOMB}{IDX}                  (01)  "{DESCMTI02}" !!! ( Ptx Mti 06 )
*% ELSE
*% IF TIPO_IDC THEN
{PADR}{RREG}-IND-{NOMB}{IDX}                  (01)  "{DESCMTI01}" !!! ( Ptx Mti 07 )
{PADR}{RREG}-GLS-{NOMB}{IDX}                  (03)  "{DESCMTI02}" !!! ( Ptx Mti 08 )
*% ELSE
*%! TIPO FEC
*%!
*% IF TIPO_FEC
{PADR}{RREG}-FEC-{NOMB}{IDX}                  (08)  "{DESCMTI01}" !!! ( Ptx Mti 09 )
*% ELSE
*% IF TIPO_DMA OR TIPO_AMD OR TIPO_MSA OR TIPO_SAM OR TIPO_FMA OR TIPO_FAM OR TIPO_FSA OR TIPO_FAS OR TIPO_FDM OR TIPO_FMD OR TIPO_FFA THEN
{PADR}{CAMP}{IDX}.
*% IF TIPO_DMA OR TIPO_AMD OR TIPO_FDM OR TIPO_FMD THEN
{NIVFEC}{RFRM}-NUM-D{PFEC}{IDX}                PIC 9(02).!!! ( Ptx Mti 10 )
*% END IF
*% IF NOT ( TIPO_FSA OR TIPO_FAS OR TIPO_FFA ) THEN
{NIVFEC}{RFRM}-NUM-M{PFEC}{IDX}                PIC 9(02).!!! ( Ptx Mti 11 )
*% END IF
*% IF TIPO_MSA OR TIPO_SAM OR TIPO_FSA OR TIPO_FAS THEN
{NIVFEC}{RFRM}-NUM-S{PFEC}{IDX}                PIC 9(02).!!! ( Ptx Mti 12 )
*% END IF
*% IF NOT ( TIPO_FDM OR TIPO_FMD ) THEN
{NIVFEC}{RFRM}-NUM-A{PFEC}{IDX}                PIC 9(02).!!! ( Ptx Mti 13 )
*% END IF
*% ELSE
*%! TIPO OFI
*%!
*% IF TIPO_OFI
{PADR}{RREG}-COD-{NOMB}{IDX}-ATR               PIC X(01).!!! ( Ptx Mti 14 )
*% ELSE
*%! TIPO IDG
*%!
*% IF TIPO_IDG
{PADR}{RREG}-TIP-{NOMB}{IDX}                   PIC X(03).!!! ( Ptx Mti 15 )
      *
*% END IF
*% END IF
*% END IF
*% END IF
*% END IF
*% END IF
*% NORMA_CST = FALSE
*% TIPO_{TIPO} = FALSE
*% GEN_KEY_LAF = FALSE
