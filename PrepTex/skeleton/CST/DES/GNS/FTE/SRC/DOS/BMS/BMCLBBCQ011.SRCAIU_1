%!              BMCLBBCQ011.SRCAIU EDICION NUMERICA
% TIPO_{TIPO} = TRUE
% IF TIPO_VRF THEN
% IF NOT TIENE_IDC_{RREG}_IDC_{NOMB} THEN
      * IDC-INI --> BMCLBBCQ011.SRCAIU EDICION NUMERICA
% IF TIDX > 0 THEN
% FOR I = 1 TO {TIDX}
           IF {RREG}-NUM-{NOMB}-LEN IN {FRM}-FLD({I}) = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO {RREG}-NUM-{NOMB}-LEN IN {FRM}-FLD({I})
                GO TO FIN-EDT-{TFLD}-{FRM}.
           MOVE {RREG}-NUM-{NOMB} IN {FRM}-FLD({I}) TO
                {RREG}-NUM-{NOMB}-EDT IN {FRM}-EDT.
           MOVE {RREG}-NUM-{NOMB}-EDT IN {FRM}-EDT TO
                {RREG}-NUM-{NOMB}-ALF IN {FRM}-FLD({I}).
% NEXT I
% ELSE
           IF {RREG}-NUM-{NOMB}-LEN IN {FRM}-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO {RREG}-NUM-{NOMB}-LEN IN {FRM}-FLD
                GO TO FIN-EDT-{TFLD}-{FRM}.
           MOVE {RREG}-NUM-{NOMB} IN {FRM}-FLD TO
                {RREG}-NUM-{NOMB}-EDT IN {FRM}-EDT.
           MOVE {RREG}-NUM-{NOMB}-EDT IN {FRM}-EDT TO
                {RREG}-NUM-{NOMB}-ALF IN {FRM}-FLD.
% END IF
      * IDC-FIN --> BMCLBBCQ011.SRCAIU EDICION NUMERICA
% END IF
% END IF
