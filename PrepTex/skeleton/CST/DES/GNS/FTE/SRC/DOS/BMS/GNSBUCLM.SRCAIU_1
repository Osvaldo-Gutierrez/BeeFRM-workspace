      *
      * GNSBUCLM
      * Version 3.0.1 Nov-2005
      * 06-Nov-13 02:26 PM
      *
       SET-TAG-GNSBUCLM SECTION.
       INI-SET-TAG-GNSBUCLM.
           MOVE
           '<IDNMOD NOM=$GNSBUCLM$ VSN=$13.09.24$ VGM=$3.0.1$
      -    'GEN=$06-Nov-13 02:26 PM$ FNC=$LOGMTI$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-GNSBUCLM.
           EXIT.
           
       RET-ALL-CLM SECTION.
       INI-RET-ALL-CLM.
           PERFORM KEY-ALL-CLM.
           PERFORM RET-TAB-CLM.
       FIN-RET-ALL-CLM.
           EXIT.
       
       RET-TAB-CLM SECTION.
       INI-RET-TAB-CLM.
           MOVE TAB-CIC-CTAB IN CLM-FLD TO TAB-CIC-CTAB IN TAB.
           MOVE TAB-EXT-CTAB IN CLM-FLD TO TAB-EXT-CTAB IN TAB.
           MOVE TAB-GLS-DESC IN CLM-FLD TO TAB-GLS-DESC IN TAB.
           MOVE TAB-GLS-ABRV IN CLM-FLD TO TAB-GLS-ABRV IN TAB.
           MOVE TAB-GLS-DCOR IN CLM-FLD TO TAB-GLS-DCOR IN TAB.
           MOVE TAB-IND-VIGE IN CLM-FLD TO TAB-IND-VIGE IN TAB.
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE TAB-NUM-DTRN IN CLM-FLD TO TAB-NUM-DTRN IN TAB.
           MOVE TAB-NUM-MTRN IN CLM-FLD TO TAB-NUM-MTRN IN TAB.
           MOVE TAB-NUM-STRN IN CLM-FLD TO TAB-NUM-STRN IN TAB.
           MOVE TAB-NUM-ATRN IN CLM-FLD TO TAB-NUM-ATRN IN TAB.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
      *<<<< FIN_RET_TAB_CLM
           MOVE FRM-COD-LGMM IN CLM-FLD TO WSS-CLM-FRM-COD-LGMM.
           MOVE FRM-COD-LGMG IN CLM-FLD TO WSS-CLM-FRM-COD-LGMG.
           MOVE FRM-COD-LGMV IN CLM-FLD TO WSS-CLM-FRM-COD-LGMV.
           MOVE WSS-CLM-GLS-DATA TO TAB-GLS-DATA IN TAB.
      *>>>> FIN_RET_TAB_CLM
       FIN-RET-TAB-CLM.
           EXIT.
       
       KEY-ALL-CLM SECTION.
       INI-KEY-ALL-CLM.
           PERFORM KEY-TAB-CLM.
       FIN-KEY-ALL-CLM.
           EXIT.
       
       KEY-TAB-CLM SECTION.
       INI-KEY-TAB-CLM.
           MOVE TAB-COD-TTAB IN CLM-FLD TO TAB-COD-TTAB IN TAB.
           MOVE TAB-COD-CTAB IN CLM-FLD TO TAB-COD-CTAB IN TAB.
       FIN-KEY-TAB-CLM.
           EXIT.
       
       SCR-CLRA-CLM SECTION.
       INI-SCR-CLRA-CLM.
           PERFORM CLR-KEY-CLM.
           PERFORM CLR-FLD-CLM.
       FIN-SCR-CLRA-CLM.
           EXIT.
       
       CLR-KEY-CLM SECTION.
       INI-CLR-KEY-CLM.
           MOVE SPACES TO FRM-COD-SISG IN CLM-FLD.
           MOVE SPACES TO FRM-COD-CMND IN CLM-FLD.
           MOVE SPACES TO TAB-COD-TTAB IN CLM-FLD.
           MOVE SPACES TO TAB-COD-CTAB IN CLM-FLD.
      *<<<< FIN_CLR_KEY_CLM
           MOVE WSS-CLM-COD-TTAB-VDS TO
                    TAB-COD-TTAB IN CLM-FLD.
      *>>>> FIN_CLR_KEY_CLM
       FIN-CLR-KEY-CLM.
           EXIT.
       
       CLR-FLD-CLM SECTION.
       INI-CLR-FLD-CLM.
           MOVE SPACES TO FRM-COD-LGMM IN CLM-FLD.
           MOVE SPACES TO FRM-COD-LGMG IN CLM-FLD.
           MOVE SPACES TO FRM-COD-LGMV IN CLM-FLD.
           PERFORM CLR-TAB-CLM.
       FIN-CLR-FLD-CLM.
           EXIT.
       
       CLR-TAB-CLM SECTION.
       INI-CLR-TAB-CLM.
           MOVE SPACES TO TAB-CIC-CTAB IN CLM-FLD.
           MOVE SPACES TO TAB-EXT-CTAB IN CLM-FLD.
           MOVE SPACES TO TAB-GLS-DESC IN CLM-FLD.
           MOVE SPACES TO TAB-GLS-ABRV IN CLM-FLD.
           MOVE SPACES TO TAB-GLS-DCOR IN CLM-FLD.
           MOVE SPACES TO TAB-IND-VIGE IN CLM-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO TAB-FEC-FTRN IN CLM-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
       FIN-CLR-TAB-CLM.
           EXIT.
       
       FST-KEY-CLM SECTION.
       INI-FST-KEY-CLM.
           MOVE -1 TO FRM-COD-SISG-LEN IN CLM-FLD.
       FIN-FST-KEY-CLM.
           EXIT.
       
       FST-FLD-CLM SECTION.
       INI-FST-FLD-CLM.
           MOVE -1 TO TAB-CIC-CTAB-LEN IN CLM-FLD.
       FIN-FST-FLD-CLM.
           EXIT.
       
       PRO-KEY-CLM SECTION.
       INI-PRO-KEY-CLM.
           MOVE FRM-CPIM-AEY TO FRM-COD-SISG-ATR IN CLM-FLD.
           MOVE FRM-CPIM-AEY TO FRM-COD-CMND-ATR IN CLM-FLD.
           PERFORM PRO-MKY-CLM.
       FIN-PRO-KEY-CLM.
           EXIT.
       
       PRO-MKY-CLM SECTION.
       INI-PRO-MKY-CLM.
       FIN-PRO-MKY-CLM.
           EXIT.
       
       PRO-FLD-CLM SECTION.
       INI-PRO-FLD-CLM.
           MOVE FRM-CPIM-AEY TO FRM-COD-LGMM-ATR IN CLM-FLD.
           MOVE FRM-CPIM-AEY TO FRM-COD-LGMG-ATR IN CLM-FLD.
           MOVE FRM-CPIM-AEY TO FRM-COD-LGMV-ATR IN CLM-FLD.
           PERFORM PRO-TAB-CLM.
       FIN-PRO-FLD-CLM.
           EXIT.
       
       PRO-TAB-CLM SECTION.
       INI-PRO-TAB-CLM.
           MOVE FRM-CPIM-AEY TO TAB-CIC-CTAB-ATR IN CLM-FLD.
           MOVE FRM-CPIM-AEY TO TAB-EXT-CTAB-ATR IN CLM-FLD.
           MOVE FRM-CPIM-AEY TO TAB-GLS-DESC-ATR IN CLM-FLD.
           MOVE FRM-CPIM-AEY TO TAB-GLS-ABRV-ATR IN CLM-FLD.
           MOVE FRM-CPIM-AEY TO TAB-GLS-DCOR-ATR IN CLM-FLD.
           MOVE FRM-CPIM-AEY TO TAB-IND-VIGE-ATR IN CLM-FLD.
       FIN-PRO-TAB-CLM.
           EXIT.
       
       UNP-KEY-CLM SECTION.
       INI-UNP-KEY-CLM.
           MOVE FRM-CPIM-UBY TO FRM-COD-SISG-ATR IN CLM-FLD.
           MOVE FRM-CPIM-UBY TO FRM-COD-CMND-ATR IN CLM-FLD.
           PERFORM UNP-IKY-CLM.
           PERFORM UNP-AKY-CLM.
           PERFORM UNP-MKY-CLM.
       FIN-UNP-KEY-CLM.
           EXIT.
       
       UNP-IKY-CLM SECTION.
       INI-UNP-IKY-CLM.
           MOVE FRM-CPIM-UBY TO FRM-COD-SISG-ATR.
           MOVE FRM-CPIM-UBY TO FRM-COD-CMND-ATR.
           MOVE FRM-CPIM-UBY TO TAB-COD-TTAB-ATR.
           MOVE FRM-CPIM-UBY TO TAB-COD-CTAB-ATR.
      *<<<< FIN_UNP_IKY_CLM
           MOVE FRM-CPIM-AEY TO TAB-COD-TTAB-ATR IN CLM-FLD.
      *>>>> FIN_UNP_IKY_CLM
       FIN-UNP-IKY-CLM.
           EXIT.
       
       UNP-MKY-CLM SECTION.
       INI-UNP-MKY-CLM.
       FIN-UNP-MKY-CLM.
           EXIT.
       
       UNP-AKY-CLM SECTION.
       INI-UNP-AKY-CLM.
       FIN-UNP-AKY-CLM.
           EXIT.
       
       UNP-FLD-CLM SECTION.
       INI-UNP-FLD-CLM.
           MOVE FRM-CPIM-UBY TO FRM-COD-LGMM-ATR IN CLM-FLD.
           MOVE FRM-CPIM-UBY TO FRM-COD-LGMG-ATR IN CLM-FLD.
           MOVE FRM-CPIM-UBY TO FRM-COD-LGMV-ATR IN CLM-FLD.
           PERFORM UNP-TAB-CLM.
      *<<<< FIN_UNP_FLD_CLM
           MOVE FRM-CPIM-AEY TO FRM-COD-LGMM-ATR IN CLM-FLD.
      *>>>> FIN_UNP_FLD_CLM
       FIN-UNP-FLD-CLM.
           EXIT.
       
       UNP-TAB-CLM SECTION.
       INI-UNP-TAB-CLM.
           MOVE FRM-CPIM-UBY TO TAB-CIC-CTAB-ATR IN CLM-FLD.
           MOVE FRM-CPIM-UBY TO TAB-EXT-CTAB-ATR IN CLM-FLD.
           MOVE FRM-CPIM-UBY TO TAB-GLS-DESC-ATR IN CLM-FLD.
           MOVE FRM-CPIM-UBY TO TAB-GLS-ABRV-ATR IN CLM-FLD.
           MOVE FRM-CPIM-UBY TO TAB-GLS-DCOR-ATR IN CLM-FLD.
           MOVE FRM-CPIM-UBY TO TAB-IND-VIGE-ATR IN CLM-FLD.
       FIN-UNP-TAB-CLM.
           EXIT.
       
       VAL-MKY-CLM SECTION.
       INI-VAL-MKY-CLM.
       FIN-VAL-MKY-CLM.
           EXIT.
       
       VAL-FLD-CLM SECTION.
       INI-VAL-FLD-CLM.
           PERFORM VAL-TAB-CIC-CTAB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAB-CIC-CTAB-LEN IN CLM-FLD
                GO TO FIN-VAL-FLD-CLM.
           PERFORM VAL-TAB-EXT-CTAB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAB-EXT-CTAB-LEN IN CLM-FLD
                GO TO FIN-VAL-FLD-CLM.
           PERFORM VAL-TAB-GLS-DESC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAB-GLS-DESC-LEN IN CLM-FLD
                GO TO FIN-VAL-FLD-CLM.
           PERFORM VAL-TAB-GLS-ABRV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAB-GLS-ABRV-LEN IN CLM-FLD
                GO TO FIN-VAL-FLD-CLM.
           PERFORM VAL-TAB-GLS-DCOR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAB-GLS-DCOR-LEN IN CLM-FLD
                GO TO FIN-VAL-FLD-CLM.
           PERFORM VAL-TAB-IND-VIGE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAB-IND-VIGE-LEN IN CLM-FLD
                GO TO FIN-VAL-FLD-CLM.
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-TAB-FEC-FTRN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                TAB-FEC-FTRN-LEN IN CLM-FLD
                GO TO FIN-VAL-FLD-CLM.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-FRM-COD-LGMM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-LGMM-LEN IN CLM-FLD
                GO TO FIN-VAL-FLD-CLM.
           PERFORM VAL-FRM-COD-LGMG.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-LGMG-LEN IN CLM-FLD
                GO TO FIN-VAL-FLD-CLM.
           PERFORM VAL-FRM-COD-LGMV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-LGMV-LEN IN CLM-FLD
                GO TO FIN-VAL-FLD-CLM.
       FIN-VAL-FLD-CLM.
           EXIT.
       
       VAL-TAB-CIC-CTAB SECTION.
       INI-VAL-TAB-CIC-CTAB.
           IF TAB-CIC-CTAB IN CLM-FLD NOT > SPACES
              MOVE SPACES TO TAB-CIC-CTAB IN CLM-FLD.
      *<<<< TAB_CIC_CTAB
           IF TAB-CIC-CTAB IN CLM-FLD NOT > SPACES
               MOVE TAB-COD-CTAB IN CLM-FLD TO TAB-CIC-CTAB IN CLM-FLD.
      *>>>>
       FIN-VAL-TAB-CIC-CTAB.
           EXIT.
       
       VAL-TAB-EXT-CTAB SECTION.
       INI-VAL-TAB-EXT-CTAB.
           IF TAB-EXT-CTAB IN CLM-FLD NOT > SPACES
              MOVE SPACES TO TAB-EXT-CTAB IN CLM-FLD.
      *<<<< TAB_EXT_CTAB
           IF TAB-EXT-CTAB IN CLM-FLD NOT > SPACES                      
              MOVE TAB-COD-CTAB IN CLM-FLD TO TAB-EXT-CTAB IN CLM-FLD.  
      *>>>>
       FIN-VAL-TAB-EXT-CTAB.
           EXIT.
       
       VAL-TAB-GLS-DESC SECTION.
       INI-VAL-TAB-GLS-DESC.
           IF TAB-GLS-DESC IN CLM-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-TAB-GLS-DESC.
       FIN-VAL-TAB-GLS-DESC.
           EXIT.
       
       VAL-TAB-GLS-ABRV SECTION.
       INI-VAL-TAB-GLS-ABRV.
           IF TAB-GLS-ABRV IN CLM-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-TAB-GLS-ABRV.
       FIN-VAL-TAB-GLS-ABRV.
           EXIT.
       
       VAL-TAB-GLS-DCOR SECTION.
       INI-VAL-TAB-GLS-DCOR.
           IF TAB-GLS-DCOR IN CLM-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-TAB-GLS-DCOR.
       FIN-VAL-TAB-GLS-DCOR.
           EXIT.
       
       VAL-TAB-IND-VIGE SECTION.
       INI-VAL-TAB-IND-VIGE.
      *<<<< INI_TAB_IND_VIGE
      *>>>>
           IF TAB-IND-VIGE IN CLM-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-TAB-IND-VIGE.
      *VAL-IND Valida miscelaneo de tabla
           MOVE 'GNS' TO MSC-COD-SIST.
           MOVE 'VIGE' TO MSC-COD-TMSC IN MSC.
           MOVE TAB-IND-VIGE IN CLM-FLD TO MSC-COD-CMSC IN MSC.
           PERFORM VAL-IND.
       FIN-VAL-TAB-IND-VIGE.
           EXIT.
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-TAB-FEC-FTRN SECTION.
       INI-VAL-TAB-FEC-FTRN.
           IF TAB-FEC-FTRN IN CLM-FLD  NOT > ZEROES
              MOVE ZEROES TO TAB-FEC-FTRN IN CLM-FLD.
       FIN-VAL-TAB-FEC-FTRN.
           EXIT.
      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-FRM-COD-LGMM SECTION.
       INI-VAL-FRM-COD-LGMM.
           IF FRM-COD-LGMM IN CLM-FLD NOT > SPACES
              MOVE SPACES TO FRM-COD-LGMM IN CLM-FLD.
      *VAL-COD Valida codigo de tabla
           MOVE 'GNS' TO TAB-COD-SIST.
           MOVE 'LMM ' TO TAB-COD-TTAB IN TAB.
           MOVE FRM-COD-LGMM IN CLM-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
       FIN-VAL-FRM-COD-LGMM.
           EXIT.
       
       VAL-FRM-COD-LGMG SECTION.
       INI-VAL-FRM-COD-LGMG.
           IF FRM-COD-LGMG IN CLM-FLD NOT > SPACES
              MOVE SPACES TO FRM-COD-LGMG IN CLM-FLD.
      *VAL-COD Valida codigo de tabla
           MOVE 'GNS' TO TAB-COD-SIST.
           MOVE 'LMG ' TO TAB-COD-TTAB IN TAB.
           MOVE FRM-COD-LGMG IN CLM-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
       FIN-VAL-FRM-COD-LGMG.
           EXIT.
       
       VAL-FRM-COD-LGMV SECTION.
       INI-VAL-FRM-COD-LGMV.
           IF FRM-COD-LGMV IN CLM-FLD NOT > SPACES
              MOVE SPACES TO FRM-COD-LGMV IN CLM-FLD.
      *VAL-COD Valida codigo de tabla
           MOVE 'GNS' TO TAB-COD-SIST.
           MOVE 'LMV ' TO TAB-COD-TTAB IN TAB.
           MOVE FRM-COD-LGMV IN CLM-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
       FIN-VAL-FRM-COD-LGMV.
           EXIT.
       
       VAL-CON-FLD-CLM SECTION.
       INI-VAL-CON-FLD-CLM.
       FIN-VAL-CON-FLD-CLM.
           EXIT.
       
       VAL-CON-CLM SECTION.
       INI-VAL-CON-CLM.
       FIN-VAL-CON-CLM.
           EXIT.
       
       VAL-NUM-KEY-CLM SECTION.
       INI-VAL-NUM-KEY-CLM.
           PERFORM VAL-NUM-IKY-CLM.
           PERFORM VAL-NUM-MKY-CLM.
           PERFORM VAL-NUM-AKY-CLM.
       FIN-VAL-NUM-KEY-CLM.
           EXIT.
       
       VAL-NUM-IKY-CLM SECTION.
       INI-VAL-NUM-IKY-CLM.
       FIN-VAL-NUM-IKY-CLM.
           EXIT.
       
       VAL-NUM-MKY-CLM SECTION.
       INI-VAL-NUM-MKY-CLM.
       FIN-VAL-NUM-MKY-CLM.
           EXIT.
       
       VAL-NUM-AKY-CLM SECTION.
       INI-VAL-NUM-AKY-CLM.
       FIN-VAL-NUM-AKY-CLM.
           EXIT.
       
       VAL-NUM-FLD-CLM SECTION.
       INI-VAL-NUM-FLD-CLM.
       FIN-VAL-NUM-FLD-CLM.
           EXIT.
       
       EDT-KEY-CLM SECTION.
       INI-EDT-KEY-CLM.
           PERFORM EDT-IKY-CLM.
           PERFORM EDT-MKY-CLM.
           PERFORM EDT-AKY-CLM.
       FIN-EDT-KEY-CLM.
           EXIT.
       
       EDT-IKY-CLM SECTION.
       INI-EDT-IKY-CLM.
       FIN-EDT-IKY-CLM.
           EXIT.
       
       EDT-MKY-CLM SECTION.
       INI-EDT-MKY-CLM.
       FIN-EDT-MKY-CLM.
           EXIT.
       
       EDT-AKY-CLM SECTION.
       INI-EDT-AKY-CLM.
       FIN-EDT-AKY-CLM.
           EXIT.
       
       EDT-FLD-CLM SECTION.
       INI-EDT-FLD-CLM.
       FIN-EDT-FLD-CLM.
           EXIT.
