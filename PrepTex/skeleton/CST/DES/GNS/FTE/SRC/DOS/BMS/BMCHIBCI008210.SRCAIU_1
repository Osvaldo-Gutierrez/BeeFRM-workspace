%!              BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
%! TIPO VRF
% IF TIDX > 0 THEN
% FRM_IFLD = "(FRM-IFLD)"
*% FRM_IFLD = "(FRM-IFLD)"
% ELSE
% FRM_IFLD = ""
*% FRM_IFLD = ""
% END
      * {TIPO}-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
% TIPO_{TIPO} = TRUE
% IF TIPO_VRF THEN
% IF NOT TIENE_IDC_{RREG}_IDC_{NOMB} THEN
       VAL-{RREG}-NUM-{NOMB} SECTION.
       INI-VAL-{RREG}-NUM-{NOMB}.
*% INCLUDE INI_{RREG}_NUM_{NOMB} {ARCA}
*% INCLUDE {RREG}_NUM_{NOMB} {ARCA}
*% INCLUDE FIN_{RREG}_NUM_{NOMB} {ARCA}
       FIN-VAL-{RREG}-NUM-{NOMB}.
           EXIT.

       VAL-{RREG}-VRF-{NOMB} SECTION.
       INI-VAL-{RREG}-VRF-{NOMB}.
*% INCLUDE INI_{RREG}_VRF_{NOMB} {ARCA}
% IF OBLI THEN
% IF NOT DISP THEN
           IF {RREG}-VRF-{NOMB} IN {FRM}-FLD{FRM_IFLD} NOT > SPACES
              PERFORM ERR-GLS.
% END IF
% ELSE
           IF {RREG}-VRF-{NOMB} IN {FRM}-FLD{FRM_IFLD} NOT > SPACES
              MOVE SPACES TO {RREG}-VRF-{NOMB} IN {FRM}-FLD{FRM_IFLD}.
% END IF
% IF NOT DISP THEN
*% INCLUDE 'GNSSRC:SETVRF202.SRC'
% END IF
% IF NOT DISP THEN
*%! INCLUDE 'PROVRF'
*% LBL = ""
% END IF
*% INCLUDE {RREG}_VRF_{NOMB} {ARCA}
*% INCLUDE FIN_{RREG}_VRF_{NOMB} {ARCA}
       FIN-VAL-{RREG}-VRF-{NOMB}.
           EXIT.

% END IF
% ELSE
%! TIPO IDC
% IF TIPO_IDC THEN

       VAL-{RREG}-NUM-{NOMB} SECTION.
       INI-VAL-{RREG}-NUM-{NOMB}.
*% INCLUDE INI_{RREG}_NUM_{NOMB} {ARCA}
*% INCLUDE {RREG}_NUM_{NOMB} {ARCA}
*% INCLUDE FIN_{RREG}_NUM_{NOMB} {ARCA}
       FIN-VAL-{RREG}-NUM-{NOMB}.
           EXIT.

       VAL-{RREG}-VRF-{NOMB} SECTION.
       INI-VAL-{RREG}-VRF-{NOMB}.
*% INCLUDE INI_{RREG}_VRF_{NOMB} {ARCA}
% IF OBLI THEN
% IF NOT DISP THEN
           IF {RREG}-VRF-{NOMB} IN {FRM}-FLD{FRM_IFLD} NOT > SPACES
              PERFORM ERR-GLS.
% END IF
% ELSE
           IF {RREG}-VRF-{NOMB} IN {FRM}-FLD{FRM_IFLD} NOT > SPACES
              MOVE SPACES TO {RREG}-VRF-{NOMB} IN {FRM}-FLD{FRM_IFLD}.
% END IF
% IF NOT DISP THEN
*% INCLUDE 'GNSSRC:SETVRF202.SRC'
% END IF
% IF NOT DISP THEN
*%! INCLUDE 'PROVRF'
% END IF
*% INCLUDE {RREG}_VRF_{NOMB} {ARCA}
*% INCLUDE FIN_{RREG}_VRF_{NOMB} {ARCA}
*% LBL = ""
       FIN-VAL-{RREG}-VRF-{NOMB}.
           EXIT.
       VAL-{RREG}-IND-{NOMB} SECTION.
       INI-VAL-{RREG}-IND-{NOMB}.
*% INCLUDE INI_{RREG}_IND_{NOMB} {ARCA}
           IF {RREG}-IND-{NOMB} IN {FRM}-FLD{FRM_IFLD} NOT > SPACES
              MOVE SPACES TO {RREG}-IND-{NOMB} IN {FRM}-FLD{FRM_IFLD}.
% IF NOT DISP THEN
*% INCLUDE 'GNSSRC:VALIDC202.SRC'
% END IF
*% INCLUDE {RREG}_IND_{NOMB} {ARCA}
*% INCLUDE FIN_{RREG}_IND_{NOMB} {ARCA}
       FIN-VAL-{RREG}-IND-{NOMB}.
           EXIT.

       VAL-{RREG}-GLS-{NOMB} SECTION.
       INI-VAL-{RREG}-GLS-{NOMB}.
*% LBL = "{RREG}_GLS_{NOMB} {ARCA}"
*% INCLUDE INI_{RREG}_GLS_{NOMB} {ARCA}
% IF OBLI THEN
% IF NOT DISP THEN
           IF {RREG}-GLS-{NOMB} IN {FRM}-FLD{FRM_IFLD} NOT > IDC-INIC-GLS
              PERFORM ERR-GLS.
% END IF
% ELSE
           IF {RREG}-GLS-{NOMB} IN {FRM}-FLD{FRM_IFLD} NOT > IDC-INIC-GLS 
              MOVE IDC-INIC-GLS TO {RREG}-GLS-{NOMB} IN {FRM}-FLD{FRM_IFLD}.
% END IF
      *VAL-GLS para IDC valida glosa ( extension ) de identificador
           MOVE {RREG}-GLS-{NOMB} IN {FRM}-FLD{FRM_IFLD} TO IDC-GLS-RUTV.
           PERFORM VAL-GLS.
           MOVE IDC-GLS-RUTV TO {RREG}-GLS-{NOMB} IN {FRM}-FLD{FRM_IFLD}.
*% INCLUDE {RREG}_GLS_{NOMB} {ARCA}
*% INCLUDE FIN_{RREG}_GLS_{NOMB} {ARCA}
       FIN-VAL-{RREG}-GLS-{NOMB}.
           EXIT.

% ELSE
%! TIPO FEC Y SUS DERIVADOS
% IF TIPO_FEC OR TIPO_DMA OR TIPO_AMD OR TIPO_MSA OR TIPO_SAM OR TIPO_FMA OR TIPO_FAM OR TIPO_FSA OR TIPO_FAS OR TIPO_FDM OR TIPO_FMD OR TIPO_FFA THEN
       VAL-{CAMP} SECTION.
       INI-VAL-{CAMP}.
% IF OBLI THEN
% IF NOT DISP THEN
           IF {CAMP} IN {FRM}-FLD{FRM_IFLD}  NOT > ZEROES
              PERFORM ERR-GLS.
% END IF
% ELSE
           IF {CAMP} IN {FRM}-FLD{FRM_IFLD}  NOT > ZEROES
              MOVE ZEROES TO {CAMP} IN {FRM}-FLD{FRM_IFLD}.
% END IF
% IF NOT DISP THEN
*% INCLUDE 'SETFEC'
% END IF
*% INCLUDE INI_{LBL}
% IF NOT DISP THEN
*% INCLUDE 'PROFEC'
% END IF
*% INCLUDE {LBL}
*% INCLUDE FIN_{LBL}
       FIN-VAL-{CAMP}.
           EXIT.
% ELSE
% IF TIPO_OFI THEN
       VAL-{RREG}-COD-{NOMB} SECTION.
       INI-VAL-{RREG}-COD-{NOMB}.
*% LBL = "{RREG}_COD_{NOMB} {ARCA}"
*% INCLUDE INI_{RREG}_COD_{NOMB} {ARCA}
%! IF NOT DISP THEN
% IF DMN_REC THEN
*% INCLUDE 'VALOFI'
% END IF
%! END IF
% IF OBLI THEN
% IF NOT DISP THEN
           IF {RREG}-COD-{NOMB} IN {FRM}-FLD{FRM_IFLD} NOT > SPACES
              PERFORM ERR-GLS.
% END IF
% ELSE
           IF {RREG}-COD-{NOMB} IN {FRM}-FLD{FRM_IFLD} NOT > SPACES
              MOVE SPACES TO {RREG}-COD-{NOMB} IN {FRM}-FLD{FRM_IFLD}.
% END IF
*% INCLUDE {RREG}_COD_{NOMB} {ARCA}
*% INCLUDE FIN_{RREG}_COD_{NOMB} {ARCA}
       FIN-VAL-{RREG}-COD-{NOMB}.
           EXIT.
% ELSE
% IF TIPO_IDG THEN
       VAL-{RREG}-TIP-{NOMB} SECTION.
       INI-VAL-{RREG}-TIP-{NOMB}.
*% LBL = "{RREG}_TIP_{NOMB} {ARCA}"
*% INCLUDE INI_{RREG}_TIP_{NOMB} {ARCA}
%! IF NOT DISP THEN
% IF DMN_REC THEN
*% INCLUDE 'VALCOD'
% END IF
%! END IF
% IF OBLI THEN
% IF NOT DISP THEN
           IF {RREG}-TIP-{NOMB} IN {FRM}-FLD{FRM_IFLD} NOT > SPACES
              PERFORM ERR-GLS.
% END IF
% ELSE
           IF {RREG}-TIP-{NOMB} IN {FRM}-FLD{FRM_IFLD} NOT > SPACES
              MOVE SPACES TO {RREG}-TIP-{NOMB} IN {FRM}-FLD{FRM_IFLD}.
% END IF
*% INCLUDE {RREG}_TIP_{NOMB} {ARCA}
*% INCLUDE FIN_{RREG}_TIP_{NOMB} {ARCA}
       FIN-VAL-{RREG}-TIP-{NOMB}.
           EXIT.

       VAL-{RREG}-STP-{NOMB} SECTION.
       INI-VAL-{RREG}-STP-{NOMB}.
*% LBL = "{RREG}_STP_{NOMB} {ARCA}"
*% INCLUDE INI_{RREG}_STP_{NOMB} {ARCA}
%! IF NOT DISP THEN
% IF DMN_REC THEN
*% INCLUDE 'VALCOD'
% END IF
%! END IF
% IF OBLI THEN
% IF NOT DISP THEN
           IF {RREG}-STP-{NOMB} IN {FRM}-FLD{FRM_IFLD} NOT > SPACES
              PERFORM ERR-GLS.
% END IF
% ELSE
           IF {RREG}-STP-{NOMB} IN {FRM}-FLD{FRM_IFLD} NOT > SPACES
              MOVE SPACES TO {RREG}-STP-{NOMB} IN {FRM}-FLD{FRM_IFLD}.
% END IF
*% INCLUDE {RREG}_STP_{NOMB} {ARCA}
*% INCLUDE FIN_{RREG}_STP_{NOMB} {ARCA}
       FIN-VAL-{RREG}-STP-{NOMB}.
           EXIT.
% END IF
% END IF
% END IF
% END IF
% END IF
      * {TIPO}-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
% DMN_REC = FALSE
% TIPO_{TIPO} = FALSE
