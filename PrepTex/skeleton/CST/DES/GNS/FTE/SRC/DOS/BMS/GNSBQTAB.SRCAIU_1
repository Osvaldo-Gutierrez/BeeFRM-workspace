       PUT-ALL-TAB SECTION.
       INI-PUT-ALL-TAB.
           PERFORM PUT-TAB-TAB.
       FIN-PUT-ALL-TAB.
           EXIT.
       
       PUT-TAB-TAB SECTION.
       INI-PUT-TAB-TAB.
           IF PGM-STAT-TAB-NUL
               GO TO FIN-PUT-TAB-TAB.
           IF NOT PGM-STAT-TAB-OKS
               PERFORM CLR-TAB-TAB
               GO TO FIN-PUT-TAB-TAB.
           MOVE TAB-COD-TTAB IN TAB TO TAB-COD-TTAB IN TAB-FLD.
           MOVE TAB-COD-CTAB IN TAB TO TAB-COD-CTAB IN TAB-FLD.
           MOVE TAB-CIC-CTAB IN TAB TO TAB-CIC-CTAB IN TAB-FLD.
           MOVE TAB-EXT-CTAB IN TAB TO TAB-EXT-CTAB IN TAB-FLD.
           MOVE TAB-GLS-DESC IN TAB TO TAB-GLS-DESC IN TAB-FLD.
           MOVE TAB-GLS-ABRV IN TAB TO TAB-GLS-ABRV IN TAB-FLD.
           MOVE TAB-GLS-DCOR IN TAB TO TAB-GLS-DCOR IN TAB-FLD.
           MOVE TAB-IND-VIGE IN TAB TO TAB-IND-VIGE IN TAB-FLD.
           MOVE TAB-NUM-DTRN IN TAB TO TAB-NUM-DTRN IN TAB-FLD.
           MOVE TAB-NUM-MTRN IN TAB TO TAB-NUM-MTRN IN TAB-FLD.
           MOVE TAB-NUM-STRN IN TAB TO TAB-NUM-STRN IN TAB-FLD.
           MOVE TAB-NUM-ATRN IN TAB TO TAB-NUM-ATRN IN TAB-FLD.
           MOVE TAB-GLS-DATA IN TAB TO TAB-GLS-DATA IN TAB-FLD.
       FIN-PUT-TAB-TAB.
           EXIT.
       
       KEY-ALL-TAB SECTION.
       INI-KEY-ALL-TAB.
           PERFORM KEY-TAB-TAB.
       FIN-KEY-ALL-TAB.
           EXIT.
       
       KEY-TAB-TAB SECTION.
       INI-KEY-TAB-TAB.
           MOVE TAB-COD-TTAB IN TAB-FLD TO TAB-COD-TTAB IN TAB.
           MOVE TAB-COD-CTAB IN TAB-FLD TO TAB-COD-CTAB IN TAB.
       FIN-KEY-TAB-TAB.
           EXIT.
       
       SCR-CLRA-TAB SECTION.
       INI-SCR-CLRA-TAB.
           PERFORM CLR-KEY-TAB.
           PERFORM CLR-FLD-TAB.
       FIN-SCR-CLRA-TAB.
           EXIT.
       
       CLR-KEY-TAB SECTION.
       INI-CLR-KEY-TAB.
           MOVE SPACES TO TAB-COD-TTAB IN TAB-FLD.
           MOVE SPACES TO TAB-COD-CTAB IN TAB-FLD.
       FIN-CLR-KEY-TAB.
           EXIT.
       
       CLR-FLD-TAB SECTION.
       INI-CLR-FLD-TAB.
           PERFORM CLR-TAB-TAB.
       FIN-CLR-FLD-TAB.
           EXIT.
       
       CLR-TAB-TAB SECTION.
       INI-CLR-TAB-TAB.
           MOVE SPACES TO TAB-CIC-CTAB IN TAB-FLD.
           MOVE SPACES TO TAB-EXT-CTAB IN TAB-FLD.
           MOVE SPACES TO TAB-GLS-DESC IN TAB-FLD.
           MOVE SPACES TO TAB-GLS-ABRV IN TAB-FLD.
           MOVE SPACES TO TAB-GLS-DCOR IN TAB-FLD.
           MOVE SPACES TO TAB-IND-VIGE IN TAB-FLD.
           MOVE ZEROES TO TAB-FEC-FTRN IN TAB-FLD.
           MOVE SPACES TO TAB-GLS-DATA IN TAB-FLD.
       FIN-CLR-TAB-TAB.
           EXIT.
       
       FST-KEY-TAB SECTION.
       INI-FST-KEY-TAB.
           MOVE -1 TO TAB-COD-TTAB-LEN IN TAB-FLD.
       FIN-FST-KEY-TAB.
           EXIT.
       
       FST-FLD-TAB SECTION.
       INI-FST-FLD-TAB.
           MOVE -1 TO TAB-CIC-CTAB-LEN IN TAB-FLD.
       FIN-FST-FLD-TAB.
           EXIT.
       
       PRO-KEY-TAB SECTION.
       INI-PRO-KEY-TAB.
           PERFORM PRO-IKY-TAB.
           PERFORM PRO-AKY-TAB.
           PERFORM PRO-MKY-TAB.
       FIN-PRO-KEY-TAB.
           EXIT.
       
       PRO-IKY-TAB SECTION.
       INI-PRO-IKY-TAB.
           MOVE FRM-CPIM-AEY TO TAB-COD-TTAB-ATR IN TAB-FLD.
           MOVE FRM-CPIM-AEY TO TAB-COD-CTAB-ATR IN TAB-FLD.
       FIN-PRO-IKY-TAB.
           EXIT.
       
       PRO-MKY-TAB SECTION.
       INI-PRO-MKY-TAB.
       FIN-PRO-MKY-TAB.
           EXIT.
       
       PRO-AKY-TAB SECTION.
       INI-PRO-AKY-TAB.
       FIN-PRO-AKY-TAB.
           EXIT.
       
       UNP-KEY-TAB SECTION.
       INI-UNP-KEY-TAB.
           PERFORM UNP-IKY-TAB.
           PERFORM UNP-AKY-TAB.
           PERFORM UNP-MKY-TAB.
       FIN-UNP-KEY-TAB.
           EXIT.
       
       UNP-IKY-TAB SECTION.
       INI-UNP-IKY-TAB.
           MOVE FRM-CPIM-UBY TO TAB-COD-TTAB-ATR.
           MOVE FRM-CPIM-UBY TO TAB-COD-CTAB-ATR.
       FIN-UNP-IKY-TAB.
           EXIT.
       
       UNP-MKY-TAB SECTION.
       INI-UNP-MKY-TAB.
       FIN-UNP-MKY-TAB.
           EXIT.
       
       UNP-AKY-TAB SECTION.
       INI-UNP-AKY-TAB.
       FIN-UNP-AKY-TAB.
           EXIT.
       
       UNP-FLD-TAB SECTION.
       INI-UNP-FLD-TAB.
           PERFORM UNP-TAB-TAB.
       FIN-UNP-FLD-TAB.
           EXIT.
       
       UNP-TAB-TAB SECTION.
       INI-UNP-TAB-TAB.
           MOVE FRM-CPIM-UBY TO TAB-CIC-CTAB-ATR IN TAB-FLD.
           MOVE FRM-CPIM-UBY TO TAB-EXT-CTAB-ATR IN TAB-FLD.
           MOVE FRM-CPIM-UBY TO TAB-GLS-DESC-ATR IN TAB-FLD.
           MOVE FRM-CPIM-UBY TO TAB-GLS-ABRV-ATR IN TAB-FLD.
           MOVE FRM-CPIM-UBY TO TAB-GLS-DCOR-ATR IN TAB-FLD.
           MOVE FRM-CPIM-UBY TO TAB-IND-VIGE-ATR IN TAB-FLD.
           MOVE FRM-CPIM-UBY TO TAB-GLS-DATA-ATR IN TAB-FLD.
       FIN-UNP-TAB-TAB.
           EXIT.
       
       VAL-KEY-TAB SECTION.
       INI-VAL-KEY-TAB.
           PERFORM VAL-IKY-TAB.
           PERFORM VAL-MKY-TAB.
           PERFORM VAL-AKY-TAB.
       FIN-VAL-KEY-TAB.
           EXIT.
       
       VAL-IKY-TAB SECTION.
       INI-VAL-IKY-TAB.
           PERFORM VAL-TAB-COD-TTAB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO TAB-COD-TTAB-LEN IN TAB-FLD
                GO TO FIN-VAL-IKY-TAB.
           PERFORM VAL-TAB-COD-CTAB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO TAB-COD-CTAB-LEN IN TAB-FLD
                GO TO FIN-VAL-IKY-TAB.
       FIN-VAL-IKY-TAB.
           EXIT.
       
       VAL-MKY-TAB SECTION.
       INI-VAL-MKY-TAB.
       FIN-VAL-MKY-TAB.
           EXIT.
       
       VAL-AKY-TAB SECTION.
       INI-VAL-AKY-TAB.
       FIN-VAL-AKY-TAB.
           EXIT.
       
       VAL-TAB-COD-TTAB SECTION.
       INI-VAL-TAB-COD-TTAB.
           IF TAB-COD-TTAB IN TAB-FLD NOT > SPACES
              MOVE SPACES TO TAB-COD-TTAB IN TAB-FLD.
      *<<<< TAB_COD_TTAB
      *VAL-COD Valida codigo de tabla
           IF TAB-COD-TTAB IN TAB-FLD NOT > SPACES AND
              NOT ( FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8 )
                PERFORM ERR-GLS
                GO TO FIN-VAL-TAB-COD-TTAB.

           IF TAB-COD-TTAB IN TAB-FLD NOT = 'TBL' AND
              NOT ( FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8 )
               MOVE 'TBL' TO TAB-COD-TTAB IN TAB
               MOVE TAB-COD-TTAB IN TAB-FLD TO TAB-COD-CTAB IN TAB
               MOVE SCR-SISG TO FIO-SIST
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM GNS-FIO-TAB
               IF NOT FIO-STAT-OKS
                   MOVE 'GNS'        TO MSG-COD-SIST
                   MOVE 'TBL    NEX' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
               ELSE
               IF TAB-MSC-STAT IN TAB = 'N'
                   MOVE 'GNS'        TO MSG-COD-SIST
                   MOVE 'TBL    NVG' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR.
                  
      *>>>>
       FIN-VAL-TAB-COD-TTAB.
           EXIT.
       
       VAL-TAB-COD-CTAB SECTION.
       INI-VAL-TAB-COD-CTAB.
           IF TAB-COD-CTAB IN TAB-FLD NOT > SPACES
              MOVE SPACES TO TAB-COD-CTAB IN TAB-FLD.
      *<<<< TAB_COD_CTAB
           IF TAB-COD-CTAB IN TAB-FLD NOT > SPACES
               MOVE SPACES TO TAB-COD-CTAB IN TAB-FLD
               GO TO FIN-VAL-TAB-COD-CTAB.
           MOVE TAB-COD-TTAB IN TAB-FLD TO TAB-COD-TTAB IN TAB.
           MOVE SCR-SISG TO FIO-SIST.
           MOVE SPACES TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
               MOVE 'TAB    NEX' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>>
       FIN-VAL-TAB-COD-CTAB.
           EXIT.
       
       VAL-CON-KEY-TAB SECTION.
       INI-VAL-CON-KEY-TAB.
      *<<<< INI_VAL_CON_KEY_TAB
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8
               GO TO FIN-VAL-CON-KEY-TAB.
      *>>>>
           IF     TAB-COD-TTAB IN TAB-FLD NOT > SPACES 
              AND TAB-COD-CTAB IN TAB-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-TAB.
       FIN-VAL-CON-KEY-TAB.
           EXIT.
       
       VAL-CON-IKY-TAB SECTION.
       INI-VAL-CON-IKY-TAB.
           IF     TAB-COD-TTAB IN TAB-FLD NOT > SPACES 
              AND TAB-COD-CTAB IN TAB-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-TAB.
       FIN-VAL-CON-IKY-TAB.
           EXIT.
       
       VAL-NUM-KEY-TAB SECTION.
       INI-VAL-NUM-KEY-TAB.
           PERFORM VAL-NUM-IKY-TAB.
           PERFORM VAL-NUM-MKY-TAB.
           PERFORM VAL-NUM-AKY-TAB.
       FIN-VAL-NUM-KEY-TAB.
           EXIT.
       
       VAL-NUM-IKY-TAB SECTION.
       INI-VAL-NUM-IKY-TAB.
       FIN-VAL-NUM-IKY-TAB.
           EXIT.
       
       VAL-NUM-MKY-TAB SECTION.
       INI-VAL-NUM-MKY-TAB.
       FIN-VAL-NUM-MKY-TAB.
           EXIT.
       
       VAL-NUM-AKY-TAB SECTION.
       INI-VAL-NUM-AKY-TAB.
       FIN-VAL-NUM-AKY-TAB.
           EXIT.
       
       VAL-NUM-FLD-TAB SECTION.
       INI-VAL-NUM-FLD-TAB.
       FIN-VAL-NUM-FLD-TAB.
           EXIT.
       
       EDT-KEY-TAB SECTION.
       INI-EDT-KEY-TAB.
           PERFORM EDT-IKY-TAB.
           PERFORM EDT-MKY-TAB.
           PERFORM EDT-AKY-TAB.
       FIN-EDT-KEY-TAB.
           EXIT.
       
       EDT-IKY-TAB SECTION.
       INI-EDT-IKY-TAB.
       FIN-EDT-IKY-TAB.
           EXIT.
       
       EDT-MKY-TAB SECTION.
       INI-EDT-MKY-TAB.
       FIN-EDT-MKY-TAB.
           EXIT.
       
       EDT-AKY-TAB SECTION.
       INI-EDT-AKY-TAB.
       FIN-EDT-AKY-TAB.
           EXIT.
       
       EDT-FLD-TAB SECTION.
       INI-EDT-FLD-TAB.
       FIN-EDT-FLD-TAB.
           EXIT.
