%!              BMCHIBCI009.SRCAIU VAL-CON
% TIPO_{TIPO} = TRUE
% IF NOT PUSO_IF THEN
% ABPA = "IF    "
% ELSE
% ABPA = "   AND"
% END IF
%! TIPO VRF
% IF TIPO_VRF THEN
% IF NOT TIENE_IDC_{RREG}_IDC_{NOMB} THEN
           {ABPA} {RREG}-NUM-{NOMB} IN {FRM}-FLD NOT > ZEROES
              AND {RREG}-VRF-{NOMB} IN {FRM}-FLD NOT > SPACES
% PUSO_IF = TRUE
% END IF
% END IF
%! TIPO IDC
% IF TIPO_IDC THEN
      * IDC-INI --> BMCHIBCI009.SRCAIU VAL-CON
           {ABPA} {RREG}-NUM-{NOMB} IN {FRM}-FLD NOT > ZEROES
              AND {RREG}-VRF-{NOMB} IN {FRM}-FLD NOT > SPACES
              AND {RREG}-IND-{NOMB} IN {FRM}-FLD NOT > SPACES
              AND {RREG}-GLS-{NOMB} IN {FRM}-FLD NOT > IDC-INIC-GLS
% PUSO_IF = TRUE
      * IDC-FIN --> BMCHIBCI009.SRCAIU VAL-CON
% END IF
%! TIPO FEC Y SUS DERIVADOS
% IF TIPO_FEC OR TIPO_DMA OR TIPO_AMD OR TIPO_MSA OR TIPO_SAM OR TIPO_FMA OR TIPO_FAM OR TIPO_FSA OR TIPO_FAS OR TIPO_FDM OR TIPO_FMD OR TIPO_FFA THEN
% IF TIDX > 0 THEN
% FOR I = 1 TO {TIDX}
           {ABPA} {RREG}-{TIPO}-{NOMB} IN {FRM}-FLD({I}) NOT > ZEROES
% NEXT I
% ELSE
           {ABPA} {RREG}-{TIPO}-{NOMB} IN {FRM}-FLD NOT > ZEROES
% END IF
% END IF
% TIPO_{TIPO} = FALSE
