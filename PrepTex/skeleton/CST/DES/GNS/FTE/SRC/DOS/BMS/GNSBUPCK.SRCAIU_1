      *
      * GNSBUPCK
      * Version 3.0.1 Nov-2005
      * 13-Apr-07 11:04 PM
      *
       SET-TAG-GNSBUPCK SECTION.
       INI-SET-TAG-GNSBUPCK.
           MOVE
           '<IDNMOD NOM=$GNSBUPCK$ VSN=$07.02.07$ VGM=$3.0.1$
      -    'GEN=$13-Apr-07 11:04 PM$ FNC=$CVTWEB$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-GNSBUPCK.
           EXIT.
           
       RET-ALL-PCK SECTION.
       INI-RET-ALL-PCK.
           PERFORM KEY-ALL-PCK.
           PERFORM RET-PCK-PCK.
       FIN-RET-ALL-PCK.
           EXIT.
       
       RET-PCK-PCK SECTION.
       INI-RET-PCK-PCK.
           MOVE PCK-MSC-STAT IN PCK-FLD TO PCK-MSC-STAT IN PCK.
           MOVE PCK-GLS-DESC IN PCK-FLD TO PCK-GLS-DESC IN PCK.
           MOVE PCK-COD-IPYC IN PCK-FLD TO PCK-COD-IPYC IN PCK.
           MOVE PCK-COD-ISDI IN PCK-FLD TO PCK-COD-ISDI IN PCK.
       FIN-RET-PCK-PCK.
           EXIT.
       
       KEY-ALL-PCK SECTION.
       INI-KEY-ALL-PCK.
           PERFORM KEY-PCK-PCK.
       FIN-KEY-ALL-PCK.
           EXIT.
       
       KEY-PCK-PCK SECTION.
       INI-KEY-PCK-PCK.
           MOVE PCK-COD-PAMB IN PCK-FLD TO PCK-COD-PAMB IN PCK.
           MOVE PCK-COD-PACK IN PCK-FLD TO PCK-COD-PACK IN PCK.
           MOVE PCK-NUM-PACK IN PCK-FLD TO PCK-NUM-PACK IN PCK.
           MOVE PCK-COD-PFNC IN PCK-FLD TO PCK-COD-PFNC IN PCK.
           MOVE PCK-COD-APCK IN PCK-FLD TO PCK-COD-APCK IN PCK.
       FIN-KEY-PCK-PCK.
           EXIT.
       
       SCR-CLRA-PCK SECTION.
       INI-SCR-CLRA-PCK.
           PERFORM CLR-KEY-PCK.
           PERFORM CLR-FLD-PCK.
       FIN-SCR-CLRA-PCK.
           EXIT.
       
       CLR-KEY-PCK SECTION.
       INI-CLR-KEY-PCK.
           MOVE SPACES TO PCK-COD-PAMB IN PCK-FLD.
           MOVE SPACES TO PCK-COD-PACK IN PCK-FLD.
           MOVE ZEROES TO PCK-NUM-PACK IN PCK-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-FEC-FING IN PCK-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO FRM-HRA-HRNG IN PCK-FLD.
           MOVE SPACES TO PCK-COD-PFNC IN PCK-FLD.
           MOVE SPACES TO PCK-COD-APCK IN PCK-FLD.
       FIN-CLR-KEY-PCK.
           EXIT.
       
       CLR-FLD-PCK SECTION.
       INI-CLR-FLD-PCK.
           MOVE SPACES TO FRM-GLS-PAMB IN PCK-FLD.
           MOVE SPACES TO FRM-GLS-STAT IN PCK-FLD.
           MOVE SPACES TO FRM-GLS-PFNC IN PCK-FLD.
           MOVE SPACES TO FRM-GLS-APCK IN PCK-FLD.
           PERFORM CLR-PCK-PCK.
       FIN-CLR-FLD-PCK.
           EXIT.
       
       CLR-PCK-PCK SECTION.
       INI-CLR-PCK-PCK.
           MOVE SPACES TO PCK-MSC-STAT IN PCK-FLD.
           MOVE SPACES TO PCK-GLS-DESC IN PCK-FLD.
           MOVE SPACES TO PCK-COD-IPYC IN PCK-FLD.
           MOVE SPACES TO PCK-COD-ISDI IN PCK-FLD.
       FIN-CLR-PCK-PCK.
           EXIT.
       
       FST-KEY-PCK SECTION.
       INI-FST-KEY-PCK.
           MOVE -1 TO PCK-COD-PAMB-LEN IN PCK-FLD.
       FIN-FST-KEY-PCK.
           EXIT.
       
       FST-FLD-PCK SECTION.
       INI-FST-FLD-PCK.
           MOVE -1 TO PCK-GLS-DESC-LEN IN PCK-FLD.
       FIN-FST-FLD-PCK.
           EXIT.
       
       PRO-KEY-PCK SECTION.
       INI-PRO-KEY-PCK.
           MOVE FRM-CPIM-AEY TO FRM-FEC-FING-ATR IN PCK-FLD.
           MOVE FRM-CPIM-AEY TO FRM-HRA-HRNG-ATR IN PCK-FLD.
           PERFORM PRO-MKY-PCK.
       FIN-PRO-KEY-PCK.
           EXIT.
       
       PRO-MKY-PCK SECTION.
       INI-PRO-MKY-PCK.
       FIN-PRO-MKY-PCK.
           EXIT.
       
       PRO-FLD-PCK SECTION.
       INI-PRO-FLD-PCK.
           PERFORM PRO-PCK-PCK.
       FIN-PRO-FLD-PCK.
           EXIT.
       
       PRO-PCK-PCK SECTION.
       INI-PRO-PCK-PCK.
           MOVE FRM-CPIM-AEY TO PCK-GLS-DESC-ATR IN PCK-FLD.
           MOVE FRM-CPIM-AEY TO PCK-COD-IPYC-ATR IN PCK-FLD.
           MOVE FRM-CPIM-AEY TO PCK-COD-ISDI-ATR IN PCK-FLD.
       FIN-PRO-PCK-PCK.
           EXIT.
       
       UNP-KEY-PCK SECTION.
       INI-UNP-KEY-PCK.
           MOVE FRM-CPIM-UBY TO FRM-FEC-FING-ATR IN PCK-FLD.
           MOVE FRM-CPIM-UBY TO FRM-HRA-HRNG-ATR IN PCK-FLD.
           PERFORM UNP-IKY-PCK.
           PERFORM UNP-AKY-PCK.
           PERFORM UNP-MKY-PCK.
       FIN-UNP-KEY-PCK.
           EXIT.
       
       UNP-IKY-PCK SECTION.
       INI-UNP-IKY-PCK.
           MOVE FRM-CPIM-UBY TO PCK-COD-PAMB-ATR.
           MOVE FRM-CPIM-UBY TO PCK-COD-PACK-ATR.
           MOVE FRM-CPIM-UBY TO PCK-NUM-PACK-ATR.
           MOVE FRM-CPIM-UBY TO FRM-FEC-FING-ATR IN PCK-FLD.
           MOVE FRM-CPIM-UBY TO FRM-HRA-HRNG-ATR.
           MOVE FRM-CPIM-UBY TO PCK-COD-APCK-ATR.
       FIN-UNP-IKY-PCK.
           EXIT.
       
       UNP-MKY-PCK SECTION.
       INI-UNP-MKY-PCK.
       FIN-UNP-MKY-PCK.
           EXIT.
       
       UNP-AKY-PCK SECTION.
       INI-UNP-AKY-PCK.
       FIN-UNP-AKY-PCK.
           EXIT.
       
       UNP-FLD-PCK SECTION.
       INI-UNP-FLD-PCK.
           PERFORM UNP-PCK-PCK.
       FIN-UNP-FLD-PCK.
           EXIT.
       
       UNP-PCK-PCK SECTION.
       INI-UNP-PCK-PCK.
           MOVE FRM-CPIM-UBY TO PCK-GLS-DESC-ATR IN PCK-FLD.
           MOVE FRM-CPIM-UBY TO PCK-COD-IPYC-ATR IN PCK-FLD.
           MOVE FRM-CPIM-UBY TO PCK-COD-ISDI-ATR IN PCK-FLD.
       FIN-UNP-PCK-PCK.
           EXIT.
       
       VAL-MKY-PCK SECTION.
       INI-VAL-MKY-PCK.
       FIN-VAL-MKY-PCK.
           EXIT.
       
       VAL-FLD-PCK SECTION.
       INI-VAL-FLD-PCK.
           PERFORM VAL-FRM-GLS-PAMB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-PAMB-LEN IN PCK-FLD
                GO TO FIN-VAL-FLD-PCK.
           PERFORM VAL-PCK-MSC-STAT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PCK-MSC-STAT-LEN IN PCK-FLD
                GO TO FIN-VAL-FLD-PCK.
           PERFORM VAL-FRM-GLS-STAT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-STAT-LEN IN PCK-FLD
                GO TO FIN-VAL-FLD-PCK.
           PERFORM VAL-FRM-GLS-PFNC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-PFNC-LEN IN PCK-FLD
                GO TO FIN-VAL-FLD-PCK.
           PERFORM VAL-FRM-GLS-APCK.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-APCK-LEN IN PCK-FLD
                GO TO FIN-VAL-FLD-PCK.
           PERFORM VAL-PCK-GLS-DESC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PCK-GLS-DESC-LEN IN PCK-FLD
                GO TO FIN-VAL-FLD-PCK.
           PERFORM VAL-PCK-COD-IPYC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PCK-COD-IPYC-LEN IN PCK-FLD
                GO TO FIN-VAL-FLD-PCK.
           PERFORM VAL-PCK-COD-ISDI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO PCK-COD-ISDI-LEN IN PCK-FLD
                GO TO FIN-VAL-FLD-PCK.
       FIN-VAL-FLD-PCK.
           EXIT.
       
       VAL-FRM-GLS-PAMB SECTION.
       INI-VAL-FRM-GLS-PAMB.
       FIN-VAL-FRM-GLS-PAMB.
           EXIT.
       
       VAL-PCK-MSC-STAT SECTION.
       INI-VAL-PCK-MSC-STAT.
       FIN-VAL-PCK-MSC-STAT.
           EXIT.
       
       VAL-FRM-GLS-STAT SECTION.
       INI-VAL-FRM-GLS-STAT.
       FIN-VAL-FRM-GLS-STAT.
           EXIT.
       
       VAL-FRM-GLS-PFNC SECTION.
       INI-VAL-FRM-GLS-PFNC.
       FIN-VAL-FRM-GLS-PFNC.
           EXIT.
       
       VAL-FRM-GLS-APCK SECTION.
       INI-VAL-FRM-GLS-APCK.
       FIN-VAL-FRM-GLS-APCK.
           EXIT.
       
       VAL-PCK-GLS-DESC SECTION.
       INI-VAL-PCK-GLS-DESC.
           IF PCK-GLS-DESC IN PCK-FLD NOT > SPACES
              MOVE SPACES TO PCK-GLS-DESC IN PCK-FLD.
       FIN-VAL-PCK-GLS-DESC.
           EXIT.
       
       VAL-PCK-COD-IPYC SECTION.
       INI-VAL-PCK-COD-IPYC.
           IF PCK-COD-IPYC IN PCK-FLD NOT > SPACES
              MOVE SPACES TO PCK-COD-IPYC IN PCK-FLD.
       FIN-VAL-PCK-COD-IPYC.
           EXIT.
       
       VAL-PCK-COD-ISDI SECTION.
       INI-VAL-PCK-COD-ISDI.
           IF PCK-COD-ISDI IN PCK-FLD NOT > SPACES
              MOVE SPACES TO PCK-COD-ISDI IN PCK-FLD.
       FIN-VAL-PCK-COD-ISDI.
           EXIT.
       
       VAL-CON-FLD-PCK SECTION.
       INI-VAL-CON-FLD-PCK.
       FIN-VAL-CON-FLD-PCK.
           EXIT.
       
       VAL-CON-PCK SECTION.
       INI-VAL-CON-PCK.
       FIN-VAL-CON-PCK.
           EXIT.
       
       VAL-NUM-KEY-PCK SECTION.
       INI-VAL-NUM-KEY-PCK.
           PERFORM VAL-NUM-IKY-PCK.
           PERFORM VAL-NUM-MKY-PCK.
           PERFORM VAL-NUM-AKY-PCK.
       FIN-VAL-NUM-KEY-PCK.
           EXIT.
       
       VAL-NUM-IKY-PCK SECTION.
       INI-VAL-NUM-IKY-PCK.
       VAL-NUM-PCK-NUM-PACK.
           MOVE PCK-NUM-PACK-ALF IN PCK-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 5 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO PCK-NUM-PACK IN PCK-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO PCK-NUM-PACK-LEN IN PCK-FLD
                GO TO FIN-VAL-NUM-IKY-PCK.
       VAL-NUM-FRM-HRA-HRNG.
           MOVE FRM-HRA-HRNG-ALF IN PCK-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 6 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO FRM-HRA-HRNG IN PCK-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO FRM-HRA-HRNG-LEN IN PCK-FLD
                GO TO FIN-VAL-NUM-IKY-PCK.
       FIN-VAL-NUM-IKY-PCK.
           EXIT.
       
       VAL-NUM-MKY-PCK SECTION.
       INI-VAL-NUM-MKY-PCK.
       FIN-VAL-NUM-MKY-PCK.
           EXIT.
       
       VAL-NUM-AKY-PCK SECTION.
       INI-VAL-NUM-AKY-PCK.
       FIN-VAL-NUM-AKY-PCK.
           EXIT.
       
       VAL-NUM-FLD-PCK SECTION.
       INI-VAL-NUM-FLD-PCK.
       FIN-VAL-NUM-FLD-PCK.
           EXIT.
       
       EDT-KEY-PCK SECTION.
       INI-EDT-KEY-PCK.
           PERFORM EDT-IKY-PCK.
           PERFORM EDT-MKY-PCK.
           PERFORM EDT-AKY-PCK.
       FIN-EDT-KEY-PCK.
           EXIT.
       
       EDT-IKY-PCK SECTION.
       INI-EDT-IKY-PCK.
           IF PCK-NUM-PACK-LEN IN PCK-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO PCK-NUM-PACK-LEN IN PCK-FLD
                GO TO FIN-EDT-IKY-PCK.
           MOVE PCK-NUM-PACK IN PCK-FLD TO PCK-NUM-PACK-EDT IN PCK-EDT.
           MOVE PCK-NUM-PACK-EDT IN PCK-EDT TO 
                PCK-NUM-PACK-ALF IN PCK-FLD.
           IF FRM-HRA-HRNG-LEN IN PCK-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO FRM-HRA-HRNG-LEN IN PCK-FLD
                GO TO FIN-EDT-IKY-PCK.
           MOVE FRM-HRA-HRNG IN PCK-FLD TO FRM-HRA-HRNG-EDT IN PCK-EDT.
           MOVE FRM-HRA-HRNG-EDT IN PCK-EDT TO 
                FRM-HRA-HRNG-ALF IN PCK-FLD.
       FIN-EDT-IKY-PCK.
           EXIT.
       
       EDT-MKY-PCK SECTION.
       INI-EDT-MKY-PCK.
       FIN-EDT-MKY-PCK.
           EXIT.
       
       EDT-AKY-PCK SECTION.
       INI-EDT-AKY-PCK.
       FIN-EDT-AKY-PCK.
           EXIT.
       
       EDT-FLD-PCK SECTION.
       INI-EDT-FLD-PCK.
       FIN-EDT-FLD-PCK.
           EXIT.
