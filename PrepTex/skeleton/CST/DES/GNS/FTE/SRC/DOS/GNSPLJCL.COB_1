       FD  RPTJCL LABEL RECORD OMITTED
           REPORT IS RPT-JCL
           .

       WORKING-STORAGE SECTION.
      *-----------------------
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-BAT-REQA==.
       COPY GNSBRBAT.
       COPY GNSRRJCL.
       COPY GNSWGRQA.
       COPY GNSWGUIB.
       COPY GNSWGELS.
       COPY GNSWGIFD.
       COPY GNSWCFIO.
       COPY GNSWVFIO.
       COPY GNSWGRPT.
       COPY GNSRWJCL.
       COPY GNSWGSYS.
      *<<<< WSS
       COPY GNSBRMSG.
       COPY GNSBRTAB.
       COPY TABBROFI.
       01  WSS-VARI.
           03  WSS-IBOF               VALUE SPACES PIC X(01).
               88  WSS-BUSCA-OFICINA  VALUE 'O'.
           03  WSS-IEOF               VALUE 'N'    PIC X(01).
               88  WSS-IEOF-BAT-YES   VALUE 'S'.
           03 WSS-VEC-ILEE.
              05 WSS-ILEE         OCCURS 20        PIC  X(01).
           03 WSS-VEC-ORDE.
              05 WSS-ORDE         OCCURS 20        PIC  9(02).
           03 WSS-COD-CTAB.
              05 WSS-COD-CBAT                      PIC  X(07).
              05 WSS-NUM-CORR                      PIC  9(02).
              05 FILLER                            PIC  X(03).
           03 WSS-VEC-DATO        VALUE SPACES.
              05 WSS-GLS-DATO     OCCURS 20        PIC  X(20).
           03 WSS-SIND.
              05 WSS-I                             PIC  9(02).
              05 WSS-J                             PIC  9(02).
           03 WSS-STRN.
              05 WSS-SCHR         OCCURS 80        PIC  X(01).
           03 WSS-ISDL            VALUE SPACES     PIC  X(01).
              88 WSS-ISDL-YES     VALUE 'S'.

       01  QUE-VARI.
           03 QUE-ITEM                             PIC  X(80).
           03 QUE-CMND                             PIC  X(03).
           03 QUE-PUT             VALUE 'PUT'      PIC  X(03).

       01  SCR-SISG                                PIC  X(03).
      *>>>>
      *<<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSG-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-TAB-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OFI-REQA==.
       COPY GNSWGBIB.
       COPY GNSWGEDT.
      *>>>>

       REPORT SECTION.
      *--------------
       COPY GNSRFJCL.
           .
       COPY GNSRTJCL.

       PROCEDURE DIVISION.
      *==================
       DECLARATIVES.
       DEC-CF-JCL-SRT-COD-PROC SECTION.
           USE BEFORE REPORTING CF-JCL-SRT-COD-PROC.
       INI-CF-JCL-SRT-COD-PROC.
       FIN-CF-JCL-SRT-COD-PROC.
           EXIT.
       DEC-PH-JCL SECTION.
           USE BEFORE REPORTING PH-JCL.
       INI-PH-JCL.
       FIN-PH-JCL.
           EXIT.
       END DECLARATIVES.

       MAIN SECTION.
      *------------
       INI-MAIN.
       COPY GNSBGEDB.
           MOVE 'GNSPLJCL' TO FIO-PROG.
           PERFORM BUS-FSIS.
      *<<<< INI
           DISPLAY 'INGRESE SISTEMA : '.
           ACCEPT SCR-SISG.
           DISPLAY 'SISTEMA INGRESADO : ' SCR-SISG.

           DISPLAY 'SUBSTITUYO DELIMITADORES (S/*) ? : '.
           ACCEPT WSS-ISDL.
           DISPLAY WSS-ISDL.

           MOVE FIO-INP TO FIO-CMND.
           MOVE 'GNS'   TO FIO-SIST.
           PERFORM GNS-FIO-TAB.

           MOVE 'GNS'  TO TAB-COD-SIST FIO-SIST.
           MOVE 'DST'  TO TAB-COD-TTAB IN TAB.
           MOVE SPACES TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF FIO-STAT-OKS
               MOVE TAB-COD-DAT1 IN TAB( 1 ) TO WSS-IBOF.

           PERFORM BUS-GDAT.
      *>>>>
       INI-INP-SRT.
           CALL 'GNSPLHDR' USING RPT-VARI.
           MOVE FIO-INP TO FIO-CMND.
           PERFORM GNS-FIO-BAT.
       FST-INP-SRT.
           MOVE FIO-GET-FST TO FIO-CMND.
           PERFORM GNS-FIO-BAT.
       INI-OUT-SRT.
           OPEN OUTPUT RPTJCL.
           INITIATE RPT-JCL.
       LUP-MAIN.
       LUP-INP-SRT.
           IF NOT FIO-STAT-OKS
                GO TO FIN-MAIN.
      *<<<< LUP_INP
       CON-LUP-INP.
           IF BAT-COD-TREG IN BAT = ' '
               MOVE SPACES TO WSS-JCL
               MOVE BAT-COD-PROC IN BAT TO SRT-COD-PROC IN SRT
               MOVE BAT-COD-CLVE IN BAT TO WSS-COD-CLVE IN WSS-JCL
               MOVE BAT-IND-IGIA IN BAT TO WSS-IND-IGIA IN WSS-JCL
               MOVE BAT-NUM-NEJM IN BAT TO WSS-NUM-NEJM IN WSS-JCL
               MOVE BAT-IND-IDEL IN BAT TO WSS-IND-IDEL IN WSS-JCL
               MOVE BAT-COD-DEST IN BAT TO WSS-COD-DEST IN WSS-JCL
               MOVE BAT-COD-CLSE IN BAT TO WSS-COD-CLSE IN WSS-JCL
               MOVE BAT-COD-FORM IN BAT TO WSS-COD-FORM IN WSS-JCL
               MOVE BAT-VEC-ILEE IN BAT TO WSS-VEC-ILEE IN WSS-VARI
           ELSE
           IF BAT-COD-PROC IN BAT = SRT-COD-PROC IN SRT AND
              BAT-COD-TREG IN BAT = 'B'
               MOVE BAT-GLS-GLSA IN BAT TO WSS-GLS-GLSA IN WSS-JCL
               PERFORM BUS-GLS-TAB
           ELSE
           IF BAT-COD-PROC IN BAT = SRT-COD-PROC IN SRT AND
              BAT-COD-TREG IN BAT = 'C'
               PERFORM GEN-CARD
               IF WSS-IEOF-BAT-YES
                   GO TO FIN-INP-SRT
               ELSE
                   GO TO CON-LUP-INP.
           GO TO NXT-OUT-SRT.
      *>>>>
       MOV-INP-SRT.
       COPY GNSRMJCL.
       LUP-OUT-SRT.
           GENERATE DL-JCL.
       NXT-INP-SRT.
       NXT-OUT-SRT.
           MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM GNS-FIO-BAT.
           GO TO LUP-MAIN.
       FIN-MAIN.
       FIN-INP-SRT.
           MOVE FIO-CLO TO FIO-CMND.
           PERFORM GNS-FIO-BAT.
       FIN-OUT-SRT.
           TERMINATE RPT-JCL.
           CLOSE RPTJCL.
       COPY GNSBGGBK.

      *JSS COPY GNSBFBAT.
       COPY GNSIOBAT.
      *
       COPY GNSBGDTC.
       COPY GNSBGIFD.
       COPY GNSBTABT.
       COPY GNSBBSYS.

      *<<<< EOF
       BUS-GLS-TAB SECTION.
       INI-BUS-GLS-TAB.
      *BUS-TAB busca glosa de codigo en tabla
           IF WSS-COD-DEST IN WSS-JCL > SPACES
               MOVE WSS-COD-DEST IN WSS-JCL TO BAT-COD-DEST IN BAT
               IF BAT-COD-IDST IN BAT = 'O' AND WSS-BUSCA-OFICINA
                   MOVE BAT-COD-OFIC IN BAT TO OFI-COD-OFIC IN OFI
                   PERFORM BUS-OFI
                   MOVE OFI-GLS-DESC IN OFI TO WSS-GLS-DEST IN WSS-JCL
               ELSE
                   MOVE 'GNS'                   TO TAB-COD-SIST
                   MOVE 'DST'                   TO TAB-COD-TTAB IN TAB
                   MOVE WSS-COD-DEST IN WSS-JCL TO TAB-COD-CTAB IN TAB
                   PERFORM BUS-TAB
                   MOVE TAB-GLS-DESC IN TAB TO WSS-GLS-DEST IN WSS-JCL.

      *BUS-TAB busca glosa de codigo en tabla
           IF WSS-COD-CLSE IN WSS-JCL > SPACES
               MOVE 'GNS'                   TO TAB-COD-SIST
               MOVE 'CLS'                   TO TAB-COD-TTAB IN TAB
               MOVE WSS-COD-CLSE IN WSS-JCL TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB     TO WSS-GLS-CLSE IN WSS-JCL.

      *BUS-TAB busca glosa de codigo en tabla
           IF WSS-COD-FORM IN WSS-JCL > SPACES
               MOVE 'GNS'                   TO TAB-COD-SIST
               MOVE 'FML'                   TO TAB-COD-TTAB IN TAB
               MOVE WSS-COD-FORM IN WSS-JCL TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB     TO WSS-GLS-FORM IN WSS-JCL.
       FIN-BUS-GLS-TAB.
           EXIT.

       COPY GNSBFMSG.
       COPY GNSBFTAB.
       COPY TABBFOFI.
       COPY GNSBBMSG.
       COPY GNSBBTAB.
       COPY TABBBOFI.

       BUS-GDAT SECTION.
       INI-BUS-GDAT.
           MOVE SPACES TO WSS-COD-CTAB.
           MOVE 'BAT'  TO WSS-COD-CBAT.
           MOVE 01     TO WSS-NUM-CORR.
           MOVE 'FRM'        TO TAB-COD-TTAB IN TAB.
           MOVE WSS-COD-CTAB TO TAB-COD-CTAB IN TAB.
           MOVE SCR-SISG     TO TAB-COD-SIST FIO-SIST.
           MOVE FIO-GET-NLS TO FIO-CMND.
       LUP-BUS-GDAT.
           PERFORM GNS-FIO-TAB.
           MOVE TAB-COD-CTAB IN TAB TO WSS-COD-CTAB IN WSS-VARI.
           IF FIO-STAT-OKS AND
              TAB-COD-TTAB IN TAB      = 'FRM' AND
              WSS-COD-CBAT IN WSS-VARI = 'BAT'
               MOVE TAB-GLS-DESC IN TAB TO WSS-GLS-DATO(WSS-NUM-CORR)
               MOVE FIO-GET-NXT TO FIO-CMND
               GO TO LUP-BUS-GDAT.

           MOVE SCR-SISG    TO FIO-SIST.
      *JSS MOVE FIO-END-BRW TO FIO-CMND.
      *    PERFORM GNS-FIO-TAB.
       FIN-BUS-GDAT.
           EXIT.

       GEN-CARD SECTION.
       INI-GEN-CARD.
           MOVE 'BATHDOS' TO BIB-BIBL.
           MOVE 'GNS'     TO BIB-SIST.
           PERFORM GEN-BIB.

           MOVE 'BATHCMD' TO BIB-BIBL.
           MOVE 'GNS'     TO BIB-SIST.
           PERFORM GEN-BIB.

           PERFORM PUT-CMN.
           MOVE SCR-SISG    TO FIO-SIST.
      *JSS MOVE FIO-END-BRW TO FIO-CMND.
      *    PERFORM GNS-FIO-BAT.

           MOVE 'BATTCMD' TO BIB-BIBL.
           MOVE 'GNS'     TO BIB-SIST.
           PERFORM GEN-BIB.

           MOVE 'BATTDOS' TO BIB-BIBL.
           MOVE 'GNS'     TO BIB-SIST.
           PERFORM GEN-BIB.
       FIN-GEN-CARD.
           EXIT.

       PUT-CMN SECTION.
       INI-PUT-CMN.
           IF BAT-GLS-CMND IN BAT NOT > SPACES
               GO TO NXT-PUT-CMN.

           MOVE BAT-GLS-CMND TO EDT-STRN.
           PERFORM EDT-CARD.
           MOVE EDT-STRN TO QUE-ITEM.

           IF QUE-ITEM NOT > SPACES
               GO TO NXT-PUT-CMN.

           IF BAT-IND-TCMN IN BAT = 'G'
               MOVE QUE-ITEM TO BIB-BIBL
               MOVE 'GNS'    TO BIB-SIST
               PERFORM GEN-BIB
               GO TO NXT-PUT-CMN.

           IF BAT-IND-TCMN IN BAT = 'B'
               MOVE QUE-ITEM TO BIB-BIBL
               MOVE SCR-SISG TO BIB-SIST
               PERFORM GEN-BIB
               GO TO NXT-PUT-CMN.

           MOVE QUE-PUT TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

           IF BAT-IND-TCMN IN BAT = 'D' OR 'M'
              PERFORM PUT-DATA.
       NXT-PUT-CMN.
           MOVE SCR-SISG    TO FIO-SIST.
           MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM GNS-FIO-BAT.
           IF FIO-STAT-OKS AND
              BAT-COD-PROC IN BAT = SRT-COD-PROC IN SRT AND
              BAT-COD-TREG IN BAT = 'C'
               GO TO INI-PUT-CMN.
           IF NOT FIO-STAT-OKS
               MOVE 'S' TO WSS-IEOF.
       FIN-PUT-CMN.
           EXIT.

       GNS-PRO-QUE SECTION.
       INI-GNS-PRO-QUE.
           IF QUE-CMND = QUE-PUT
               MOVE QUE-ITEM TO WSS-GLS-CARD IN WSS-JCL
               GENERATE DL-JCL.
       FIN-GNS-PRO-QUE.
           EXIT.

       EDT-CARD SECTION.
       INI-EDT-CARD.
           MOVE 1 TO WSS-I.
       LUP-EDT-CARD.
           IF ( EDT-SCHR(WSS-I) = '{' OR '}' ) AND WSS-ISDL-YES
               MOVE '@' TO WSS-SCHR(WSS-I)
           ELSE
               MOVE EDT-SCHR(WSS-I) TO WSS-SCHR(WSS-I).
           ADD 1 TO WSS-I.
           IF WSS-I NOT > 80
               GO TO LUP-EDT-CARD.
           MOVE WSS-STRN TO EDT-STRN.
       FIN-EDT-CARD.
           EXIT.

       PUT-DATA SECTION.
       INI-PUT-DATA.
           MOVE ZEROES TO WSS-VEC-ORDE.
           PERFORM VER-ORD VARYING WSS-I FROM 1 BY 1 UNTIL WSS-I > 20.
           PERFORM MOV-ORD VARYING WSS-J FROM 1 BY 1 UNTIL WSS-J > 20.
       FIN-PUT-DATA.
           EXIT.

       VER-ORD SECTION.
       INI-VER-ORD.
           IF WSS-ILEE(WSS-I) NOT = '0' AND WSS-ILEE(WSS-I) > SPACES
               PERFORM CON-NUM
               MOVE WSS-I TO WSS-ORDE(WSS-J).
       FIN-VER-ORD.
           EXIT.

       CON-NUM SECTION.
       INI-CON-NUM.
           IF WSS-ILEE(WSS-I) IS NUMERIC
               MOVE WSS-ILEE(WSS-I) TO WSS-J
               GO TO FIN-CON-NUM.
           IF WSS-ILEE(WSS-I) = 'A' MOVE 10 TO WSS-J
           ELSE
           IF WSS-ILEE(WSS-I) = 'B' MOVE 11 TO WSS-J
           ELSE
           IF WSS-ILEE(WSS-I) = 'C' MOVE 12 TO WSS-J
           ELSE
           IF WSS-ILEE(WSS-I) = 'D' MOVE 13 TO WSS-J
           ELSE
           IF WSS-ILEE(WSS-I) = 'E' MOVE 14 TO WSS-J
           ELSE
           IF WSS-ILEE(WSS-I) = 'F' MOVE 15 TO WSS-J
           ELSE
           IF WSS-ILEE(WSS-I) = 'G' MOVE 16 TO WSS-J
           ELSE
           IF WSS-ILEE(WSS-I) = 'H' MOVE 17 TO WSS-J
           ELSE
           IF WSS-ILEE(WSS-I) = 'I' MOVE 18 TO WSS-J
           ELSE
           IF WSS-ILEE(WSS-I) = 'J' MOVE 19 TO WSS-J
           ELSE
           IF WSS-ILEE(WSS-I) = 'K' MOVE 20 TO WSS-J.
       FIN-CON-NUM.
           EXIT.

       MOV-ORD SECTION.
       INI-MOV-ORD.
           IF WSS-ORDE(WSS-J) NOT = 0
               MOVE WSS-ORDE(WSS-J) TO WSS-I
               IF WSS-ILEE(WSS-I) NOT = '0' AND WSS-ILEE(WSS-I) > SPACES
                   MOVE WSS-GLS-DATO(WSS-I) TO QUE-ITEM
                   MOVE QUE-PUT TO QUE-CMND
                   PERFORM GNS-PRO-QUE.
       FIN-MOV-ORD.
           EXIT.

       COPY GNSBGBIB.
      *>>>>
