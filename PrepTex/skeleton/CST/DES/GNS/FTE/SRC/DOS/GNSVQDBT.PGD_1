       PROCEDURE DIVISION.
      *===================

       MAIN SECTION.
       INI-MAIN.
           PERFORM INI.
       LUP-MAIN.
           IF SCR-INST = SCR-INST-HCM
                 PERFORM SCR-COM.
           IF SCR-CCPP = 'MOD'
                 PERFORM SCR-MOD
           ELSE
           IF SCR-CCPP = 'ING'
                 PERFORM SCR-ING
           ELSE
           IF SCR-CCPP = 'ACC'
                 PERFORM SCR-ACC
           ELSE
           IF SCR-CCPP = 'ELI'
                 PERFORM SCR-ELI
           ELSE
           IF SCR-CCPP = 'ACT'
                 PERFORM SCR-ACT
           ELSE
           IF SCR-CCPP = 'PGM'
                 PERFORM SCR-PGM
           ELSE
                 MOVE SCR-INST-HCM TO SCR-INST.
           GO TO LUP-MAIN.
       FIN-MAIN.
           EXIT.

       INI SECTION.
       INI-INI.
       COPY GNSBGEIB.
       COPY GNSBGEDB.
       COPY GNSBGCMA.
      *JSS-VRS Se debe cargar de tabla ( NO de MNU por la operadora )
           MOVE 'GNSAVRS' TO SCR-NLIB.
           MOVE 'GNSFVRS' TO SCR-NMAP.
      *
           MOVE 'GNSVQDBT' TO FIO-PROG.
       FIN-INI.
           EXIT.

       SCR-COM SECTION.
       INI-SCR-COM.
           IF SCR-TFUN = 'RET'
               GO TO CON-SCR-COM.
           PERFORM PRO-ALL.
           PERFORM SCR-CLRA-DBT.
           PERFORM EDT-KEY-DBT.
           PERFORM EDT-FLD-DBT.
           PERFORM SCR-PUTA.
       CON-SCR-COM.
           MOVE SCR-INST-HCM TO SCR-INST.
           MOVE SCR-CMNP     TO FRM-PROG.
           MOVE SCR-VARI     TO FRM-CMMA.
           MOVE SCR-TCMA     TO FRM-TCMA.
           MOVE FRM-XCT      TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       XCT-ERR.
           MOVE 'XCTERR    ' TO MSG-COD-MENS.
           MOVE 'GNS'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MEN1.
           MOVE FRM-PROG     TO FRM-MEN2.
           MOVE FRM-ABT      TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       COPY GNSBGGBK.
       FIN-SCR-COM.
           EXIT.

       SCR-MOD SECTION.
       INI-SCR-MOD.
           IF SCR-INST = SCR-INST-HKY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM HBL-KEY
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-MOD
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON.
           PERFORM DESSCRTRL.
       MSG-SCR-MOD.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'MODNOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-MOD
               IF FIO-STAT-OKS
                   IF FRM-MENS NOT > SPACES
                       MOVE 'MODYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-MOD.
           EXIT.

       SCR-ING SECTION.
       INI-SCR-ING.
           IF SCR-INST = SCR-INST-HKY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM HBL-IKY
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-RST
                PERFORM GET-IKY
                IF NOT FRM-STAT-ABT
                        MOVE SPACES TO FRM-MENS
                        PERFORM FND-KEY
                        IF NOT FIO-STAT-OKS AND
                           FIO-STAT NOT = FIO-STAT-NEX
      *JSS                  PERFORM CLR-FLD-DBT
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR-PGM
                            PERFORM SCR-TRL
                        ELSE
                        MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR-PGM
                        IF FRM-MENS NOT > SPACES
                            MOVE 'REGEXI    ' TO MSG-COD-MENS
                            MOVE 'GNS' TO MSG-COD-SIST
                            PERFORM GET-MSG
                            MOVE MSG-GLS-DESC TO FRM-MENS
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-ING
                        ELSE
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-ING
                ELSE
                        MOVE SPACES TO FRM-MENS
                        PERFORM SCR-MSSG
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ING
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON.
           PERFORM DESSCRTRL.
       MSG-SCR-ING.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'INGNOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-ING
               IF FIO-STAT-OKS
                   IF FRM-MENS NOT > SPACES
                       MOVE 'INGYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ING.
           EXIT.

       SCR-ACC SECTION.
       INI-SCR-ACC.
           IF SCR-INST = SCR-INST-HKY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM HBL-KEY
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
      *JSS-VRS          PERFORM SCR-TRL
                        PERFORM EDT-FLD-DBT
                        PERFORM GNS-PUT-OUF
                        PERFORM DESSCRTRL
                        MOVE 'S' TO PGM-IPUT
                        GO TO FIN-SCR-ACC
      *
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ACC
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON.
           PERFORM DESSCRTRL.
           MOVE SPACES TO FRM-MENS.
           IF SCR-STAT NOT = SCR-STAT-CON
               PERFORM CMN-ACC.
           PERFORM SCR-MSSG.
       FIN-SCR-ACC.
           EXIT.

       SCR-ELI SECTION.
       INI-SCR-ELI.
           IF SCR-INST = SCR-INST-HKY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM HBL-KEY
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ELI
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON.
           PERFORM DESSCRTRL.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'ELINOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-ELI
               IF FIO-STAT-OKS
                   IF FRM-MENS NOT > SPACES
                       MOVE 'ELIYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ELI.
           EXIT.

       SCR-ACT SECTION.
       INI-SCR-ACT.
           IF SCR-INST = SCR-INST-HKY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM HBL-KEY
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-RST
                PERFORM GET-KEY
                IF NOT FRM-STAT-ABT
                        MOVE SPACES TO FRM-MENS
                        PERFORM FND-KEY
                        IF FIO-STAT-OKS
                            MOVE SCR-ACTU-MOD TO SCR-ACTU
                            PERFORM PUT-SCR
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR-PGM
                            PERFORM SCR-TRL
                        ELSE
                        MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR-PGM
                        IF FIO-STAT-EOF
                            IF FRM-MENS NOT > SPACES
                                MOVE 'REGEOF    ' TO MSG-COD-MENS
                                MOVE 'GNS' TO MSG-COD-SIST
                                PERFORM GET-MSG
                                MOVE MSG-GLS-DESC TO FRM-MENS
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                            ELSE
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                        ELSE
                        IF FIO-STAT = FIO-STAT-NEX
                            IF FRM-MENS NOT > SPACES
                                MOVE 'REGNEX    ' TO MSG-COD-MENS
                                MOVE 'GNS' TO MSG-COD-SIST
                                PERFORM GET-MSG
                                MOVE MSG-GLS-DESC TO FRM-MENS
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                            ELSE
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                        ELSE
                            MOVE SCR-ACTU-ING TO SCR-ACTU
      *JSS                  PERFORM CLR-FLD-DBT
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR-PGM
                            PERFORM SCR-TRL
                ELSE
                        MOVE SPACES TO FRM-MENS
                        PERFORM SCR-MSSG
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ACT
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON.
           PERFORM DESSCRTRL.
           IF SCR-ACTU = SCR-ACTU-MOD
                  PERFORM MSG-SCR-MOD
           ELSE
                  PERFORM MSG-SCR-ING.
       FIN-SCR-ACT.
           EXIT.

       SCR-PGM SECTION.
       INI-SCR-PGM.
           IF SCR-INST = SCR-INST-HKY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM HBL-KEY
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-RST
                PERFORM GET-KEY
                IF FRM-STAT-ABT
                        MOVE SPACES TO FRM-MENS
                        PERFORM SCR-MSSG
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-PGM.
           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
      *JSS IF NOT FIO-STAT-OKS
      *        PERFORM CLR-FLD-DBT.
           IF FIO-STAT = FIO-STAT-NEX
               IF FRM-MENS NOT > SPACES
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   PERFORM SCR-MSSG
                   MOVE SCR-STAT-ABT TO SCR-STAT
                   PERFORM DESSCRTRL
                   GO TO FIN-SCR-PGM
               ELSE
                   PERFORM SCR-MSSG
                   MOVE SCR-STAT-ABT TO SCR-STAT
                   PERFORM DESSCRTRL
                   GO TO FIN-SCR-PGM.
       TRL-SCR-PGM.
           PERFORM DESSCRTRL.
           MOVE SPACES TO FRM-MENS.
           IF SCR-STAT NOT = SCR-STAT-CON
               PERFORM CMN-PGM.
           PERFORM SCR-MSSG.
       FIN-SCR-PGM.
           EXIT.

       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR-USR NOT > SPACES
               MOVE PGM-ICLR-PGM TO PGM-ICLR-USR.
           MOVE SPACES TO PGM-ICLR-PGM.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-DBT
               PERFORM EDT-KEY-DBT
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-CAMPOS
               PERFORM CLR-FLD-DBT
               PERFORM EDT-FLD-DBT
           ELSE
               PERFORM SCR-CLRA-DBT
               PERFORM EDT-KEY-DBT
               PERFORM EDT-FLD-DBT.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

       SCR-KEY SECTION.
       INI-SCR-KEY.
           PERFORM SCR-RST.
           PERFORM GET-KEY.
           IF FRM-STAT-ABT
                   MOVE SPACES TO FRM-MENS
                   PERFORM SCR-MSSG
                   GO TO FIN-SCR-KEY.
       FND-SCR-KEY.
           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
           IF FIO-STAT-OKS
               PERFORM PUT-SCR
               MOVE PGM-LIMPIAR-TODO TO PGM-ICLR-PGM
               GO TO FIN-SCR-KEY.
      *JSS ELSE
      *        PERFORM CLR-FLD-DBT.
       ERR-SCR-KEY.
           MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR-PGM.
           IF FRM-MENS NOT > SPACES
               IF FIO-STAT-EOF
                   MOVE 'REGEOF    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
               IF FIO-STAT = FIO-STAT-NEX
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
           MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-SCR-KEY.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES           TO PGM-ICLR-USR.
           MOVE PGM-LIMPIAR-TODO TO PGM-ICLR-PGM.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO DBT-FLD.
       FIN-SCR-RST.
           EXIT.

       GET-KEY SECTION.
       INI-GET-KEY.
           PERFORM SCR-GKEY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-KEY.
           EXIT.

       GET-IKY SECTION.
       INI-GET-IKY.
           PERFORM SCR-GIKY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-IKY.
           EXIT.

       FND-KEY SECTION.
       INI-FND-KEY.
           PERFORM KEY-ALL-DBT.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-DML.
           MOVE FIO-STAT TO PGM-STAT-DML.
      *<<<< FIN_FND_KEY
           MOVE SCR-LDOC TO WSS-NDOC IN WSS-VARI.
           IF WSS-IPRO NOT = 'F'
               GO TO FIN-FND-KEY.
           IF WSS-IFAX IN WSS-VARI > SPACES
               MOVE WSS-IFAX IN WSS-VARI TO WSS-GLS-NFAX IN WSS-VARI
           ELSE
           IF DML-COD-IOF1 IN DBT-FLD     > ZEROES AND
              DML-COD-IOF2 IN DBT-FLD NOT > ZEROES 
               MOVE DML-COD-IOF1 IN DBT-FLD TO WSS-GLS-NFAX IN WSS-VARI
               MOVE 9999999999              TO DML-COD-IOF1 IN DBT-FLD
           ELSE
           IF DML-COD-IOF2 IN DBT-FLD     > ZEROES AND
              DML-COD-IOF3 IN DBT-FLD NOT > ZEROES 
               MOVE DML-COD-IOF2 IN DBT-FLD TO WSS-GLS-NFAX IN WSS-VARI
               MOVE 9999999999              TO DML-COD-IOF2 IN DBT-FLD
           ELSE
           IF DML-COD-IOF3 IN DBT-FLD     > ZEROES AND
              DML-COD-IOF4 IN DBT-FLD NOT > ZEROES 
               MOVE DML-COD-IOF3 IN DBT-FLD TO WSS-GLS-NFAX IN WSS-VARI
               MOVE 9999999999              TO DML-COD-IOF3 IN DBT-FLD
           ELSE
           IF DML-COD-IOF4 IN DBT-FLD     > ZEROES AND
              DML-COD-IOF5 IN DBT-FLD NOT > ZEROES 
               MOVE DML-COD-IOF4 IN DBT-FLD TO WSS-GLS-NFAX IN WSS-VARI
               MOVE 9999999999              TO DML-COD-IOF4 IN DBT-FLD 
           ELSE
               MOVE DML-COD-IOF5 IN DBT-FLD TO WSS-GLS-NFAX IN WSS-VARI
               MOVE 9999999999              TO DML-COD-IOF5 IN DBT-FLD.
           MOVE '00' TO FIO-STAT.
           MOVE DML-COD-IOF1 IN DBT-FLD TO WSS-IOF1 IN WSS-VARI.
           MOVE DML-COD-IOF2 IN DBT-FLD TO WSS-IOF2 IN WSS-VARI.
           MOVE DML-COD-IOF3 IN DBT-FLD TO WSS-IOF3 IN WSS-VARI.
           MOVE DML-COD-IOF4 IN DBT-FLD TO WSS-IOF4 IN WSS-VARI.
           MOVE DML-COD-IOF5 IN DBT-FLD TO WSS-IOF5 IN WSS-VARI.
           MOVE DML-NUM-DDML IN DBT-FLD TO WSS-DOF6 IN WSS-VARI.
           MOVE DML-NUM-MDML IN DBT-FLD TO WSS-MOF6 IN WSS-VARI.
           MOVE DML-NUM-SDML IN DBT-FLD TO WSS-SOF6 IN WSS-VARI.
           MOVE DML-NUM-ADML IN DBT-FLD TO WSS-AOF6 IN WSS-VARI.
           MOVE SCR-RFBY                TO WSS-NUM-RFBY IN WSS-VARI.
      *Para setear Temporary Storage de Retorno
           PERFORM GNS-SET-RET.
      *Hace X-control al programa que llena pantallas para FAX
           MOVE WSS-KEYS   TO FRM-CMMA.
           MOVE +69        TO FRM-TCMA.
           MOVE 'GNSPQMCT' TO FRM-PROG.
           MOVE FRM-XCT    TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
      *>>>>
       FIN-FND-KEY.
           EXIT.

       PUT-SCR SECTION.
       INI-PUT-SCR.
      *JSS-VRS
      *    PERFORM GNS-PUT-OUF.
      *
           PERFORM PUT-ALL-DBT.
       FIN-PUT-SCR.
           EXIT.

       GET-FLD SECTION.
       INI-GET-FLD.
           PERFORM SCR-GFLD.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-FLD.
           EXIT.

       CMN-MOD SECTION.
       INI-CMN-MOD.
           PERFORM KEY-ALL-DBT.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM GNS-FIO-DML.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-MOD.
           PERFORM RET-ALL-DBT.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM GNS-FIO-DML.
       FIN-CMN-MOD.
           EXIT.

       CMN-ING SECTION.
       INI-CMN-ING.
           PERFORM RET-ALL-DBT.
           MOVE FIO-PUT TO FIO-CMND.
           PERFORM GNS-FIO-DML.
       FIN-CMN-ING.
           EXIT.

       CMN-ACC SECTION.
       INI-CMN-ACC.
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'ACCION NO REALIZADA' TO FRM-MENS
               GO TO FIN-CMN-ACC.
           PERFORM KEY-ALL-DBT.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM GNS-FIO-DML.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ACC.
           PERFORM RET-ALL-DBT.
       FIN-CMN-ACC.
           EXIT.

       CMN-ELI SECTION.
       INI-CMN-ELI.
           PERFORM KEY-ALL-DBT.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM GNS-FIO-DML.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ELI.
           MOVE FIO-DEL TO FIO-CMND.
           PERFORM GNS-FIO-DML.
       FIN-CMN-ELI.
           EXIT.

       CMN-PGM SECTION.
       INI-CMN-PGM.
           PERFORM RET-ALL-DBT.
       FIN-CMN-PGM.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO FRM-NTRN.
           MOVE SCR-VARI TO FRM-CMMA.
           MOVE SCR-TCMA TO FRM-TCMA.
           MOVE FRM-EXT  TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
      *JSS-VRS
           MOVE LOW-VALUES TO DBT-VRS-MENS IN DBT-FLD-RED1.
           MOVE SCR-RFBY TO DBT-VRS-RFBY IN DBT-FLD-RED1.
           IF DBT-VRS-RFBY IN DBT-FLD-RED1 = '1'
               MOVE FRM-CPIM-UBY TO DBT-VRS-RFBY-ATR IN DBT-FLD-RED1
               MOVE '0'          TO DBT-VRS-RFBY     IN DBT-FLD-RED1
           ELSE
               MOVE FRM-CPIM-UBY TO DBT-VRS-RFBY-ATR IN DBT-FLD-RED1
               MOVE '1'          TO DBT-VRS-RFBY     IN DBT-FLD-RED1.
           MOVE DBT-VRS-RFBY IN DBT-FLD-RED1 TO SCR-RFBY.
      *
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE DBT-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO DBT-GLS-MENS.
      *JSS-VRS
           IF FRM-ABRV > SPACES
               MOVE FRM-ABRV TO DBT-VRS-OMSG
           ELSE
               MOVE SPACES   TO DBT-VRS-OMSG.
      *
       FIN-SCR-MSSG.
           EXIT.

       SCR-GKEY SECTION.
       INI-SCR-GKEY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-DBT
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO DBT-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GKEY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-DBT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-DBT
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           PERFORM VAL-CON-KEY-DBT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               PERFORM FST-KEY-DBT
               GO TO ERR-SCR-GKEY.
      *JSS
           PERFORM CLR-FLD-DBT.
           PERFORM VAL-IKY-DBT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-MKY-DBT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-AKY-DBT.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GKEY.
       ERR-SCR-GKEY.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-DBT.
      *JSS-VRS SOLO SE DEBE RECUPERAR EL PROMPT DEL DATO QUE SALIO MALO
           PERFORM GNS-ERR-IOF.
      *
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GKEY.
           PERFORM SCR-MSSG.

       SCR-GIKY SECTION.
       INI-SCR-GIKY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-DBT
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO DBT-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GIKY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-IKY-DBT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               PERFORM SCR-MSSG
               PERFORM EDT-IKY-DBT
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           PERFORM VAL-CON-IKY-DBT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               PERFORM FST-KEY-DBT
               GO TO ERR-SCR-GIKY.
      *JSS
           PERFORM CLR-FLD-DBT.
           PERFORM VAL-IKY-DBT.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GIKY.
       ERR-SCR-GIKY.
           PERFORM SCR-MSSG.
           PERFORM EDT-IKY-DBT.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GIKY.
           PERFORM SCR-MSSG.

       SCR-GFLD SECTION.
       INI-SCR-GFLD.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-FLD-DBT
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO DBT-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GFLD.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-DBT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-DBT
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           PERFORM VAL-NUM-FLD-DBT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-DBT
               PERFORM EDT-FLD-DBT
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           PERFORM VAL-MKY-DBT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GFLD.
           PERFORM VAL-FLD-DBT.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GFLD.
           PERFORM VAL-CON-FLD-DBT.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GFLD.
           PERFORM FST-FLD-DBT.
       ERR-SCR-GFLD.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-DBT.
           PERFORM EDT-FLD-DBT.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GFLD.
           MOVE SCR-STAT-MOD TO SCR-STAT.

       GET-ACC SECTION.
       INI-GET-ACC.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC          TO DBT-GLS-MEN2
               MOVE SCR-GLS-PRMT( SCR-I ) TO DBT-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO DBT-FLD.
           IF NOT ( ( FRM-FFLD = FRM-FFLD-PF12 OR FRM-FFLD-PF3 ) OR
                      SCR-GLS-CONF( SCR-I ) > SPACES )
               PERFORM VAL-NUM-KEY-DBT
               PERFORM VAL-NUM-FLD-DBT.
       FIN-GET-ACC.
           MOVE SCR-STAT-SON TO SCR-STAT.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC          TO DBT-GLS-MEN2
               MOVE SCR-GLS-PRMT( SCR-I ) TO DBT-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO DBT-FLD.
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO DBT-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
           IF SCR-DESDE-CMN
               MOVE DBT-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO DBT-FLD
               MOVE FRM-MENS TO DBT-GLS-MENS.
      *JSS ANALIZAR PARA VRS
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-FLD-DBT.
           PERFORM PRO-MKY-DBT.
           PERFORM PRO-AKY-DBT.
           PERFORM UNP-IKY-DBT.
           PERFORM FST-KEY-DBT.
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
           IF SCR-DESDE-CMN
               MOVE DBT-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO DBT-FLD
               MOVE FRM-MENS TO DBT-GLS-MENS.
      *JSS ANALIZAR PARA VRS
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-FLD-DBT.
           PERFORM UNP-KEY-DBT.
           PERFORM FST-KEY-DBT.
      *JSS-VRS
           PERFORM GNS-PUT-IOF.
      *ESTO ES NECESARIO HACERLO UNA VEZ..........
           PERFORM GNS-PUT-DCR.
           MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR-PGM.
      *
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
           PERFORM PRO-IKY-DBT.
           PERFORM PRO-AKY-DBT.
           PERFORM UNP-MKY-DBT.
           PERFORM UNP-FLD-DBT.
           PERFORM FST-FLD-DBT.
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO DBT-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-DBT.
           PERFORM PRO-FLD-DBT.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-GLS-PRMT( SCR-I ) TO FRM-MEN1.
           IF SCR-STAT = SCR-STAT-MOD
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-DBT
               PERFORM EDT-FLD-DBT
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-DBT
               PERFORM EDT-FLD-DBT
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
           MOVE SCR-INST-GAB TO SCR-INST.
      *JSS ANALIZAR PARA VRS
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-DBT.
           PERFORM EDT-FLD-DBT.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           IF SCR-INST = SCR-INST-GCF
                  PERFORM SCR-GCNF
                  GO TO FIN-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
           IF SCR-STAT = SCR-STAT-ABT OR SCR-STAT-CON
                  GO TO FIN-DESSCRTRL.
           IF SCR-FFLD-RET OR SCR-FFLD-PF10
                  MOVE SCR-VFLD-YES TO SCR-VFLD
                  PERFORM PRO-CONF.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

       PRO-CONF SECTION.
       INI-PRO-CONF.
           IF FRM-MENS NOT > SPACES
               IF SCR-GLS-CONF( SCR-I ) NOT > SPACES
                   GO TO FIN-PRO-CONF
               ELSE
                   MOVE SCR-GLS-CONF( SCR-I ) TO FRM-MENS
           ELSE
               IF FRM-MEN1 NOT > SPACES
                   MOVE SCR-GLS-CONF( SCR-I ) TO FRM-MEN1.
           MOVE SCR-INST-GCF TO SCR-INST.
           PERFORM SCR-MSSG.
           PERFORM PRO-KEY-DBT.
           PERFORM PRO-FLD-DBT.
           PERFORM FST-MSG.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM EDT-KEY-DBT
               PERFORM EDT-FLD-DBT.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-PRO-CONF.
           EXIT.

       SCR-GCNF SECTION.
       INI-SCR-GCNF.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3  OR
                               FRM-FFLD-PF4 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC          TO DBT-GLS-MEN2
               MOVE SCR-GLS-CONF( SCR-I ) TO DBT-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO DBT-FLD.
           MOVE FRM-FFLD TO SCR-FFLD.
           IF FRM-FFLD = FRM-FFLD-PF3
               MOVE SCR-VFLD-NOT TO SCR-VFLD
           ELSE
           IF FRM-FFLD = FRM-FFLD-RET OR FRM-FFLD-PF10 OR FRM-FFLD-PF4
               MOVE SCR-VFLD-YES TO SCR-VFLD
               PERFORM VAL-NUM-KEY-DBT
               PERFORM VAL-NUM-FLD-DBT
           ELSE
           IF FRM-FFLD = FRM-FFLD-PF12
               MOVE SCR-STPP TO SCR-STAT
               IF SCR-STAT = SCR-STAT-MOD
                   PERFORM VAL-NUM-KEY-DBT
                   PERFORM VAL-NUM-FLD-DBT
                   PERFORM SCR-TRL
               ELSE
                   MOVE SCR-VFLD-NOT TO SCR-VFLD.
       FIN-SCR-GCNF.
           EXIT.

       COPY GNSBMDBT.
       COPY GNSBFDML.
       COPY GNSBFMSG.
       COPY GNSBECIC.
       COPY GNSBGSYS.
       COPY GNSBGVSM.
       COPY GNSBGDTC.
       COPY GNSBGFRM.
      *JSS-VRS
       COPY GNSBGMSV.
      *
       COPY GNSBEKEY.
       COPY GNSBIABT.
       COPY GNSBFTAB.
       COPY GNSBGFEC.
       COPY GNSBGHOY.

      *JSS-VRS
       GNS-PUT-IOF SECTION.
       INI-GNS-PUT-IOF.
           IF PGM-IPUT-YES
               MOVE 'A' TO DBT-VRS-VFLD IN DBT-FLD-RED1(1)
               GO TO FIN-GNS-PUT-IOF.

           MOVE SCR-SIST TO FIO-SIST.
           MOVE 'VRP'    TO TAB-COD-TTAB IN TAB.
      *    MOVE SCR-PROG TO PGM-PVRP.
           MOVE SCR-NEMO TO PGM-PVRP.
           MOVE 'IO'     TO PGM-FVRP.
           MOVE PGM-CTVR TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
               GO TO FIN-GNS-PUT-IOF.
           MOVE TAB-GLS-DESC IN TAB TO PGM-DESC.
           MOVE TAB-GLS-DATA IN TAB TO PGM-DATA.

           MOVE PGM-STRN TO UNS-STRN.
           MOVE ','      TO UNS-DLMT.
           MOVE 0 TO FRM-IFLD.
       LUP-GNS-PUT-IOF.
           MOVE FRM-IFLD TO UNS-NELE.
           ADD 1 TO FRM-IFLD.
           IF FRM-IFLD > DBT-VRS-INOU
               GO TO FIN-GNS-PUT-IOF.
           PERFORM GNS-UNS-STR.
           MOVE FRM-CPIM-UBY TO DBT-VRS-IFLD-ATR(FRM-IFLD).
           IF UNS-ELEM = UNS-DLMT
               MOVE SPACES   TO DBT-VRS-IFLD(FRM-IFLD)
           ELSE
               MOVE UNS-ELEM TO DBT-VRS-IFLD(FRM-IFLD).
           GO TO LUP-GNS-PUT-IOF.
       FIN-GNS-PUT-IOF.
           EXIT.

       GNS-PUT-OUF SECTION.
       INI-GNS-PUT-OUF.
           MOVE SCR-SIST TO FIO-SIST.
           MOVE 'VRP'    TO TAB-COD-TTAB IN TAB.
      *    MOVE SCR-PROG TO PGM-PVRP.
           MOVE SCR-NEMO TO PGM-PVRP.
           MOVE 'OP'     TO PGM-FVRP.
           MOVE PGM-CTVR TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
               GO TO FIN-GNS-PUT-OUF.
           MOVE TAB-GLS-DESC IN TAB TO PGM-DESC.
           MOVE TAB-GLS-DATA IN TAB TO PGM-DATA.

           MOVE 0 TO FRM-IFLD.
       LUP-GNS-PUT-OUF.
           MOVE PGM-STRN TO UNS-STRN.
           MOVE '/'      TO UNS-DLMT.
           MOVE FRM-IFLD TO UNS-NELE.
           ADD 1 TO FRM-IFLD.
           IF FRM-IFLD > DBT-VRS-OUTP
               GO TO FIN-GNS-PUT-OUF.
           IF DBT-VRS-OFLD-ATR(FRM-IFLD) = FRM-CPIM-ANY
               MOVE 'M'   TO DBT-VRS-OCOD(FRM-IFLD)
               MOVE '270' TO DBT-VRS-ODAT(FRM-IFLD)
               GO TO LUP-GNS-PUT-OUF.
           PERFORM GNS-UNS-STR.
           IF UNS-ELEM = UNS-DLMT
               GO TO FIN-GNS-PUT-OUF.

           MOVE UNS-ELEM TO UNS-STRN.
           MOVE ','      TO UNS-DLMT.
           MOVE 0        TO UNS-NELE.
           PERFORM GNS-UNS-STR.
           IF UNS-ELEM = UNS-DLMT
               GO TO LUP-GNS-PUT-OUF.
           MOVE UNS-ELEM TO DBT-VRS-OPRM(FRM-IFLD).
           IF DBT-VRS-OFLD-ATR(FRM-IFLD) = FRM-CPIM-UNY
               MOVE 'M'   TO DBT-VRS-OCOD(FRM-IFLD)
               MOVE '271' TO DBT-VRS-ODAT(FRM-IFLD)
               GO TO LUP-GNS-PUT-OUF.
           MOVE FRM-CPIM-UBY TO DBT-VRS-OFLD-ATR(FRM-IFLD).

           MOVE 1        TO UNS-NELE.
           PERFORM GNS-UNS-STR.
           IF UNS-ELEM = UNS-DLMT
               GO TO LUP-GNS-PUT-OUF.
           MOVE UNS-ELEM TO DBT-VRS-OCOD(FRM-IFLD).
           GO TO LUP-GNS-PUT-OUF.
       FIN-GNS-PUT-OUF.
           EXIT.

       GNS-PUT-DCR SECTION.
       INI-GNS-PUT-DCR.
           MOVE SCR-SIST TO FIO-SIST.
           MOVE 'VRP'    TO TAB-COD-TTAB IN TAB.
      *    MOVE SCR-PROG TO PGM-PVRP.
           MOVE SCR-NEMO TO PGM-PVRP.
           MOVE 'DCR'    TO PGM-FVRP.
           MOVE PGM-CTVR TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF FIO-STAT-OKS
               MOVE TAB-GLS-DESC IN TAB TO PGM-DESC
               MOVE TAB-GLS-DATA IN TAB TO PGM-DATA
               GO TO CON-GNS-PUT-DCR.

           MOVE 'GNS'   TO FIO-SIST.
           MOVE 'VRP'   TO TAB-COD-TTAB IN TAB.
           MOVE '*/DCR' TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF FIO-STAT-OKS
               MOVE TAB-GLS-DESC IN TAB TO PGM-DESC
               MOVE TAB-GLS-DATA IN TAB TO PGM-DATA
           ELSE
               MOVE SPACES TO PGM-DESC
               MOVE SPACES TO PGM-DATA.

       CON-GNS-PUT-DCR.
           MOVE PGM-PBXE TO DBT-VRS-PBXE IN DBT-FLD-RED1.
           MOVE PGM-FVMU TO DBT-VRS-FVMU IN DBT-FLD-RED1.
           MOVE PGM-ASDT TO DBT-VRS-ASDT IN DBT-FLD-RED1.
           MOVE PGM-NTRI TO DBT-VRS-NTRI IN DBT-FLD-RED1.
           MOVE PGM-TFKY TO DBT-VRS-TFKY IN DBT-FLD-RED1.
           MOVE PGM-FKY5 TO DBT-VRS-FKY5 IN DBT-FLD-RED1.
           MOVE PGM-FKY6 TO DBT-VRS-FKY6 IN DBT-FLD-RED1.
           MOVE PGM-EPFK TO DBT-VRS-EPFK IN DBT-FLD-RED1.
           MOVE PGM-RPFK TO DBT-VRS-RPFK IN DBT-FLD-RED1.
           IF PGM-SOFL NOT > SPACES
               MOVE DBT-VRS-LIOF IN DBT-FLD-VARI TO
                    DBT-VRS-SOFL IN DBT-FLD-RED1
           ELSE
               MOVE PGM-SOFL TO DBT-VRS-SOFL IN DBT-FLD-RED1.
           MOVE PGM-HMSG TO DBT-VRS-HMSG IN DBT-FLD-RED1.
           MOVE PGM-LFLG TO DBT-VRS-LFLG IN DBT-FLD-RED1.
           MOVE PGM-FKY9 TO DBT-VRS-FKY9 IN DBT-FLD-RED1.
           MOVE PGM-FKY0 TO DBT-VRS-FKY0 IN DBT-FLD-RED1.
           MOVE PGM-TOLN TO DBT-VRS-TOLN IN DBT-FLD-RED1.
           MOVE PGM-MRLN TO DBT-VRS-MRLN IN DBT-FLD-RED1.
           MOVE PGM-MTLN TO DBT-VRS-MTLN IN DBT-FLD-RED1.
           MOVE PGM-DFEF TO DBT-VRS-DFEF IN DBT-FLD-RED1.
           IF NOT ( DBT-VRS-RFBY IN DBT-FLD-RED1 = '0' OR '1' )
               MOVE PGM-RFBY TO DBT-VRS-RFBY IN DBT-FLD-RED1.
       FIN-GNS-PUT-DCR.
           EXIT.

       GNS-ERR-IOF SECTION.
       INI-GNS-ERR-IOF.
            IF PGM-SUAR > SPACES
                MOVE PGM-SUAR TO DBT-VRS-IFLD(1)
                MOVE -1       TO DBT-VRS-IFLD-LEN(1)
                GO TO FIN-GNS-ERR-IOF.
      *
           MOVE 0 TO FRM-IFLD.
       LUP-GNS-ERR-IOF.
           ADD 1 TO FRM-IFLD.
           IF FRM-IFLD > DBT-VRS-INOU
               MOVE 0 TO FRM-IFLD
               GO TO FIN-GNS-ERR-IOF.
           IF NOT ( DBT-VRS-IFLD-LEN(FRM-IFLD) = -1 OR -9 )
               GO TO LUP-GNS-ERR-IOF.
       CON-GNS-ERR-IOF.
           MOVE SCR-SIST TO FIO-SIST.
           MOVE 'VRP'    TO TAB-COD-TTAB IN TAB.
      *    MOVE SCR-PROG TO PGM-PVRP.
           MOVE SCR-NEMO TO PGM-PVRP.
           MOVE 'IO'     TO PGM-FVRP.
           MOVE PGM-CTVR TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
               GO TO FIN-GNS-ERR-IOF.
           MOVE TAB-GLS-DESC IN TAB TO PGM-DESC.
           MOVE TAB-GLS-DATA IN TAB TO PGM-DATA.

           MOVE PGM-STRN TO UNS-STRN.
           MOVE ','      TO UNS-DLMT.
           MOVE FRM-IFLD TO PGM-IFML.
           SUBTRACT 1 FROM FRM-IFLD.
       LU2-GNS-ERR-IOF.
           MOVE FRM-IFLD TO UNS-NELE.
           ADD 1 TO FRM-IFLD.
           IF FRM-IFLD > DBT-VRS-INOU
               GO TO FIN-GNS-ERR-IOF.
           PERFORM GNS-UNS-STR.
           MOVE FRM-CPIM-UBY TO DBT-VRS-IFLD-ATR(FRM-IFLD).
           IF UNS-ELEM = UNS-DLMT
               MOVE SPACES   TO DBT-VRS-IFLD(FRM-IFLD)
           ELSE
               MOVE UNS-ELEM TO DBT-VRS-IFLD(FRM-IFLD)
               IF ( UNS-ELEM = 'E' ) AND ( FRM-IFLD > PGM-IFML )
                   MOVE 0 TO PGM-IFML.
           IF PGM-IFML = 0
               GO TO LU2-GNS-ERR-IOF.
           GO TO LU2-GNS-ERR-IOF.
       FIN-GNS-ERR-IOF.
           EXIT.

       COPY GNSBGUNS.
      *<<<< EOF
       COPY GNSBGQUE.
       GNS-SET-RET SECTION.
       INI-GNS-SET-RET.
           PERFORM GNS-ERR-QUE.
           MOVE EIBTRMID TO QUE-TERM.
           MOVE 'GRET'   TO QUE-TYPE.
           MOVE 1        TO QUE-NITM.
           MOVE QUE-GET  TO QUE-CMND.
           MOVE +30      TO QUE-LITM.
           PERFORM GNS-PRO-QUE.
           IF QUE-STAT NOT = QUE-STAT-OKS
               GO TO CON-GNS-SET-RET.
           MOVE QUE-DEL TO QUE-CMND.
           PERFORM GNS-PRO-QUE.

       CON-GNS-SET-RET.
           MOVE 'GNSVPMNU' TO WSS-GPRO.
           MOVE 'GNS'      TO WSS-GSIS.
           MOVE SPACES     TO WSS-GSSG.
           MOVE SCR-FRET   TO WSS-GFUN.
           MOVE SPACES     TO WSS-GCMD.

           MOVE EIBTRMID TO QUE-TERM.
           MOVE 'GRET'   TO QUE-TYPE.
           MOVE 1        TO QUE-NITM.
           MOVE WSS-GRET TO QUE-ITEM.
           MOVE QUE-PUT  TO QUE-CMND.
           MOVE +30      TO QUE-LITM.
           PERFORM GNS-PRO-QUE.
       FIN-GNS-SET-RET.
           EXIT.
      *>>>>
