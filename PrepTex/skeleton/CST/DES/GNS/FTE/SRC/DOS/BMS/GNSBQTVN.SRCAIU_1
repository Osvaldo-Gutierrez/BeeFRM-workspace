      *
      * GNSBQTVN
      * Version 3.0.2 Oct-2013 VALVNM
      * 05-Nov-13 11:36 AM
      *
       SET-TAG-GNSBQTVN SECTION.
       INI-SET-TAG-GNSBQTVN.
           MOVE
           '<IDNMOD NOM=$GNSBQTVN$ VSN=$13.11.04$ VGM=$3.0.2$
      -    'GEN=$05-Nov-13 11:36 AM$ FNC=$131012$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-GNSBQTVN.
           EXIT.
           
       PUT-ALL-TVN SECTION.
       INI-PUT-ALL-TVN.
           PERFORM PUT-TVN-TVN.
       FIN-PUT-ALL-TVN.
           EXIT.
       
       PUT-TVN-TVN SECTION.
       INI-PUT-TVN-TVN.
           IF PGM-STAT-TVN-NUL
               GO TO FIN-PUT-TVN-TVN.
           IF NOT PGM-STAT-TVN-OKS
               PERFORM CLR-TVN-TVN
               GO TO FIN-PUT-TVN-TVN.
           MOVE TVN-NUM-DVDO IN TVN TO TVN-NUM-DVDO IN TVN-FLD.
           MOVE TVN-VAL-PDEI IN TVN TO TVN-VAL-PDEI IN TVN-FLD.
           MOVE TVN-NUM-DSOR IN TVN TO TVN-NUM-DSOR IN TVN-FLD.
           MOVE TVN-COD-STAT IN TVN TO TVN-COD-STAT IN TVN-FLD.
           MOVE TVN-COD-CMEN IN TVN TO TVN-COD-CMEN IN TVN-FLD.
           MOVE TVN-GLS-CMEN IN TVN TO TVN-GLS-CMEN IN TVN-FLD.
           MOVE TVN-NUM-COCI IN TVN TO TVN-NUM-COCI IN TVN-FLD.
           MOVE TVN-NUM-CXDE IN TVN TO TVN-NUM-CXDE IN TVN-FLD.
           MOVE TVN-NUM-REST IN TVN TO TVN-NUM-REST IN TVN-FLD.
           MOVE TVN-NUM-CXDR IN TVN TO TVN-NUM-CXDR IN TVN-FLD.
           MOVE TVN-VAL-PDEO IN TVN TO TVN-VAL-PDEO IN TVN-FLD.
       FIN-PUT-TVN-TVN.
           EXIT.
       
       KEY-ALL-TVN SECTION.
       INI-KEY-ALL-TVN.
           PERFORM KEY-TVN-TVN.
       FIN-KEY-ALL-TVN.
           EXIT.
       
       KEY-TVN-TVN SECTION.
       INI-KEY-TVN-TVN.
           MOVE TVN-NUM-DVDO IN TVN-FLD TO TVN-NUM-DVDO IN TVN.
           MOVE TVN-VAL-PDEI IN TVN-FLD TO TVN-VAL-PDEI IN TVN.
           MOVE TVN-NUM-DSOR IN TVN-FLD TO TVN-NUM-DSOR IN TVN.
       FIN-KEY-TVN-TVN.
           EXIT.
       
       SCR-CLRA-TVN SECTION.
       INI-SCR-CLRA-TVN.
           PERFORM CLR-KEY-TVN.
           PERFORM CLR-FLD-TVN.
       FIN-SCR-CLRA-TVN.
           EXIT.
       
       CLR-KEY-TVN SECTION.
       INI-CLR-KEY-TVN.
           MOVE ZEROES TO TVN-NUM-DVDO IN TVN-FLD.
           MOVE ZEROES TO TVN-VAL-PDEI IN TVN-FLD.
           MOVE ZEROES TO TVN-NUM-DSOR IN TVN-FLD.
       FIN-CLR-KEY-TVN.
           EXIT.
       
       CLR-FLD-TVN SECTION.
       INI-CLR-FLD-TVN.
           PERFORM CLR-TVN-TVN.
       FIN-CLR-FLD-TVN.
           EXIT.
       
       CLR-TVN-TVN SECTION.
       INI-CLR-TVN-TVN.
           MOVE SPACES TO TVN-COD-STAT IN TVN-FLD.
           MOVE SPACES TO TVN-COD-CMEN IN TVN-FLD.
           MOVE SPACES TO TVN-GLS-CMEN IN TVN-FLD.
           MOVE ZEROES TO TVN-NUM-COCI IN TVN-FLD.
           MOVE ZEROES TO TVN-NUM-CXDE IN TVN-FLD.
           MOVE ZEROES TO TVN-NUM-REST IN TVN-FLD.
           MOVE ZEROES TO TVN-NUM-CXDR IN TVN-FLD.
           MOVE ZEROES TO TVN-VAL-PDEO IN TVN-FLD.
       FIN-CLR-TVN-TVN.
           EXIT.
       
       FST-KEY-TVN SECTION.
       INI-FST-KEY-TVN.
           MOVE -1 TO TVN-NUM-DVDO-LEN IN TVN-FLD.
       FIN-FST-KEY-TVN.
           EXIT.
       
       FST-FLD-TVN SECTION.
       INI-FST-FLD-TVN.
           MOVE -1 TO TVN-COD-STAT-LEN IN TVN-FLD.
       FIN-FST-FLD-TVN.
           EXIT.
       
       PRO-KEY-TVN SECTION.
       INI-PRO-KEY-TVN.
           PERFORM PRO-IKY-TVN.
           PERFORM PRO-AKY-TVN.
           PERFORM PRO-MKY-TVN.
       FIN-PRO-KEY-TVN.
           EXIT.
       
       PRO-IKY-TVN SECTION.
       INI-PRO-IKY-TVN.
           MOVE FRM-CPIM-AEY TO TVN-NUM-DVDO-ATR IN TVN-FLD.
           MOVE FRM-CPIM-AEY TO TVN-VAL-PDEI-ATR IN TVN-FLD.
           MOVE FRM-CPIM-AEY TO TVN-NUM-DSOR-ATR IN TVN-FLD.
       FIN-PRO-IKY-TVN.
           EXIT.
       
       PRO-MKY-TVN SECTION.
       INI-PRO-MKY-TVN.
       FIN-PRO-MKY-TVN.
           EXIT.
       
       PRO-AKY-TVN SECTION.
       INI-PRO-AKY-TVN.
       FIN-PRO-AKY-TVN.
           EXIT.
       
       UNP-KEY-TVN SECTION.
       INI-UNP-KEY-TVN.
           PERFORM UNP-IKY-TVN.
           PERFORM UNP-AKY-TVN.
           PERFORM UNP-MKY-TVN.
       FIN-UNP-KEY-TVN.
           EXIT.
       
       UNP-IKY-TVN SECTION.
       INI-UNP-IKY-TVN.
           MOVE FRM-CPIM-UBY TO TVN-NUM-DVDO-ATR.
           MOVE FRM-CPIM-UBY TO TVN-VAL-PDEI-ATR.
           MOVE FRM-CPIM-UBY TO TVN-NUM-DSOR-ATR.
       FIN-UNP-IKY-TVN.
           EXIT.
       
       UNP-MKY-TVN SECTION.
       INI-UNP-MKY-TVN.
       FIN-UNP-MKY-TVN.
           EXIT.
       
       UNP-AKY-TVN SECTION.
       INI-UNP-AKY-TVN.
       FIN-UNP-AKY-TVN.
           EXIT.
       
       UNP-FLD-TVN SECTION.
       INI-UNP-FLD-TVN.
           PERFORM UNP-TVN-TVN.
       FIN-UNP-FLD-TVN.
           EXIT.
       
       UNP-TVN-TVN SECTION.
       INI-UNP-TVN-TVN.
           MOVE FRM-CPIM-UBY TO TVN-COD-STAT-ATR IN TVN-FLD.
           MOVE FRM-CPIM-UBY TO TVN-COD-CMEN-ATR IN TVN-FLD.
           MOVE FRM-CPIM-UBY TO TVN-GLS-CMEN-ATR IN TVN-FLD.
           MOVE FRM-CPIM-UBY TO TVN-NUM-COCI-ATR IN TVN-FLD.
           MOVE FRM-CPIM-UBY TO TVN-NUM-CXDE-ATR IN TVN-FLD.
           MOVE FRM-CPIM-UBY TO TVN-NUM-REST-ATR IN TVN-FLD.
           MOVE FRM-CPIM-UBY TO TVN-NUM-CXDR-ATR IN TVN-FLD.
           MOVE FRM-CPIM-UBY TO TVN-VAL-PDEO-ATR IN TVN-FLD.
       FIN-UNP-TVN-TVN.
           EXIT.
       
       VAL-KEY-TVN SECTION.
       INI-VAL-KEY-TVN.
           PERFORM VAL-IKY-TVN.
           PERFORM VAL-MKY-TVN.
           PERFORM VAL-AKY-TVN.
       FIN-VAL-KEY-TVN.
           EXIT.
       
       VAL-IKY-TVN SECTION.
       INI-VAL-IKY-TVN.
           PERFORM VAL-TVN-NUM-DVDO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TVN-NUM-DVDO-LEN IN TVN-FLD
                GO TO FIN-VAL-IKY-TVN.
           PERFORM VAL-TVN-VAL-PDEI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TVN-VAL-PDEI-LEN IN TVN-FLD
                GO TO FIN-VAL-IKY-TVN.
           PERFORM VAL-TVN-NUM-DSOR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TVN-NUM-DSOR-LEN IN TVN-FLD
                GO TO FIN-VAL-IKY-TVN.
       FIN-VAL-IKY-TVN.
           EXIT.
       
       VAL-MKY-TVN SECTION.
       INI-VAL-MKY-TVN.
       FIN-VAL-MKY-TVN.
           EXIT.
       
       VAL-AKY-TVN SECTION.
       INI-VAL-AKY-TVN.
       FIN-VAL-AKY-TVN.
           EXIT.
       
       VAL-TVN-NUM-DVDO SECTION.
       INI-VAL-TVN-NUM-DVDO.
       FIN-VAL-TVN-NUM-DVDO.
           EXIT.
       
       VAL-TVN-VAL-PDEI SECTION.
       INI-VAL-TVN-VAL-PDEI.
       FIN-VAL-TVN-VAL-PDEI.
           EXIT.
       
       VAL-TVN-NUM-DSOR SECTION.
       INI-VAL-TVN-NUM-DSOR.
       FIN-VAL-TVN-NUM-DSOR.
           EXIT.
       
       VAL-CON-KEY-TVN SECTION.
       INI-VAL-CON-KEY-TVN.
           IF     TVN-NUM-DVDO IN TVN-FLD NOT > ZEROES 
              AND TVN-VAL-PDEI IN TVN-FLD NOT > ZEROES 
              AND TVN-NUM-DSOR IN TVN-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-TVN.
       FIN-VAL-CON-KEY-TVN.
           EXIT.
       
       VAL-CON-IKY-TVN SECTION.
       INI-VAL-CON-IKY-TVN.
           IF     TVN-NUM-DVDO IN TVN-FLD NOT > ZEROES 
              AND TVN-VAL-PDEI IN TVN-FLD NOT > ZEROES 
              AND TVN-NUM-DSOR IN TVN-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-TVN.
       FIN-VAL-CON-IKY-TVN.
           EXIT.
       
       VAL-NUM-KEY-TVN SECTION.
       INI-VAL-NUM-KEY-TVN.
           PERFORM VAL-NUM-IKY-TVN.
           PERFORM VAL-NUM-MKY-TVN.
           PERFORM VAL-NUM-AKY-TVN.
       FIN-VAL-NUM-KEY-TVN.
           EXIT.
       
       VAL-NUM-IKY-TVN SECTION.
       INI-VAL-NUM-IKY-TVN.
       VAL-NUM-TVN-NUM-DVDO.
           MOVE TVN-NUM-DVDO-ALF IN TVN-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 15 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE VNM-1503 TO TVN-NUM-DVDO IN TVN-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO TVN-NUM-DVDO-LEN IN TVN-FLD
                GO TO FIN-VAL-NUM-IKY-TVN.
       VAL-NUM-TVN-VAL-PDEI.
           MOVE TVN-VAL-PDEI-ALF IN TVN-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 1 TO NUM-NENT.
           MOVE 5 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE VNM-0117 TO TVN-VAL-PDEI IN TVN-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO TVN-VAL-PDEI-LEN IN TVN-FLD
                GO TO FIN-VAL-NUM-IKY-TVN.
       VAL-NUM-TVN-NUM-DSOR.
           MOVE TVN-NUM-DSOR-ALF IN TVN-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 15 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE VNM-1503 TO TVN-NUM-DSOR IN TVN-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO TVN-NUM-DSOR-LEN IN TVN-FLD
                GO TO FIN-VAL-NUM-IKY-TVN.
       FIN-VAL-NUM-IKY-TVN.
           EXIT.
       
       VAL-NUM-MKY-TVN SECTION.
       INI-VAL-NUM-MKY-TVN.
       FIN-VAL-NUM-MKY-TVN.
           EXIT.
       
       VAL-NUM-AKY-TVN SECTION.
       INI-VAL-NUM-AKY-TVN.
       FIN-VAL-NUM-AKY-TVN.
           EXIT.
       
       VAL-NUM-FLD-TVN SECTION.
       INI-VAL-NUM-FLD-TVN.
       VAL-NUM-TVN-NUM-COCI.
           MOVE TVN-NUM-COCI-ALF IN TVN-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 15 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE VNM-1503 TO TVN-NUM-COCI IN TVN-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO TVN-NUM-COCI-LEN IN TVN-FLD
                GO TO FIN-VAL-NUM-FLD-TVN.
       VAL-NUM-TVN-NUM-CXDE.
           MOVE TVN-NUM-CXDE-ALF IN TVN-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 15 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE VNM-1503 TO TVN-NUM-CXDE IN TVN-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO TVN-NUM-CXDE-LEN IN TVN-FLD
                GO TO FIN-VAL-NUM-FLD-TVN.
       VAL-NUM-TVN-NUM-REST.
           MOVE TVN-NUM-REST-ALF IN TVN-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 15 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE VNM-1503 TO TVN-NUM-REST IN TVN-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO TVN-NUM-REST-LEN IN TVN-FLD
                GO TO FIN-VAL-NUM-FLD-TVN.
       VAL-NUM-TVN-NUM-CXDR.
           MOVE TVN-NUM-CXDR-ALF IN TVN-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 15 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE VNM-1503 TO TVN-NUM-CXDR IN TVN-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO TVN-NUM-CXDR-LEN IN TVN-FLD
                GO TO FIN-VAL-NUM-FLD-TVN.
       VAL-NUM-TVN-VAL-PDEO.
           MOVE TVN-VAL-PDEO-ALF IN TVN-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 1 TO NUM-NENT.
           MOVE 5 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE VNM-0117 TO TVN-VAL-PDEO IN TVN-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO TVN-VAL-PDEO-LEN IN TVN-FLD
                GO TO FIN-VAL-NUM-FLD-TVN.
       FIN-VAL-NUM-FLD-TVN.
           EXIT.
       
       EDT-KEY-TVN SECTION.
       INI-EDT-KEY-TVN.
           PERFORM EDT-IKY-TVN.
           PERFORM EDT-MKY-TVN.
           PERFORM EDT-AKY-TVN.
       FIN-EDT-KEY-TVN.
           EXIT.
       
       EDT-IKY-TVN SECTION.
       INI-EDT-IKY-TVN.
           IF TVN-NUM-DVDO-LEN IN TVN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO TVN-NUM-DVDO-LEN IN TVN-FLD
                GO TO FIN-EDT-IKY-TVN.
           MOVE TVN-NUM-DVDO IN TVN-FLD TO TVN-NUM-DVDO-EDT IN TVN-EDT.
           MOVE TVN-NUM-DVDO-EDT IN TVN-EDT TO 
                TVN-NUM-DVDO-ALF IN TVN-FLD.
           IF TVN-VAL-PDEI-LEN IN TVN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO TVN-VAL-PDEI-LEN IN TVN-FLD
                GO TO FIN-EDT-IKY-TVN.
           MOVE TVN-VAL-PDEI IN TVN-FLD TO TVN-VAL-PDEI-EDT IN TVN-EDT.
           MOVE TVN-VAL-PDEI-EDT IN TVN-EDT TO 
                TVN-VAL-PDEI-ALF IN TVN-FLD.
           IF TVN-NUM-DSOR-LEN IN TVN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO TVN-NUM-DSOR-LEN IN TVN-FLD
                GO TO FIN-EDT-IKY-TVN.
           MOVE TVN-NUM-DSOR IN TVN-FLD TO TVN-NUM-DSOR-EDT IN TVN-EDT.
           MOVE TVN-NUM-DSOR-EDT IN TVN-EDT TO 
                TVN-NUM-DSOR-ALF IN TVN-FLD.
       FIN-EDT-IKY-TVN.
           EXIT.
       
       EDT-MKY-TVN SECTION.
       INI-EDT-MKY-TVN.
       FIN-EDT-MKY-TVN.
           EXIT.
       
       EDT-AKY-TVN SECTION.
       INI-EDT-AKY-TVN.
       FIN-EDT-AKY-TVN.
           EXIT.
       
       EDT-FLD-TVN SECTION.
       INI-EDT-FLD-TVN.
           IF TVN-NUM-COCI-LEN IN TVN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO TVN-NUM-COCI-LEN IN TVN-FLD
                GO TO FIN-EDT-FLD-TVN.
           MOVE TVN-NUM-COCI IN TVN-FLD TO TVN-NUM-COCI-EDT IN TVN-EDT.
           MOVE TVN-NUM-COCI-EDT IN TVN-EDT TO 
                TVN-NUM-COCI-ALF IN TVN-FLD.
           IF TVN-NUM-CXDE-LEN IN TVN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO TVN-NUM-CXDE-LEN IN TVN-FLD
                GO TO FIN-EDT-FLD-TVN.
           MOVE TVN-NUM-CXDE IN TVN-FLD TO TVN-NUM-CXDE-EDT IN TVN-EDT.
           MOVE TVN-NUM-CXDE-EDT IN TVN-EDT TO 
                TVN-NUM-CXDE-ALF IN TVN-FLD.
           IF TVN-NUM-REST-LEN IN TVN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO TVN-NUM-REST-LEN IN TVN-FLD
                GO TO FIN-EDT-FLD-TVN.
           MOVE TVN-NUM-REST IN TVN-FLD TO TVN-NUM-REST-EDT IN TVN-EDT.
           MOVE TVN-NUM-REST-EDT IN TVN-EDT TO 
                TVN-NUM-REST-ALF IN TVN-FLD.
           IF TVN-NUM-CXDR-LEN IN TVN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO TVN-NUM-CXDR-LEN IN TVN-FLD
                GO TO FIN-EDT-FLD-TVN.
           MOVE TVN-NUM-CXDR IN TVN-FLD TO TVN-NUM-CXDR-EDT IN TVN-EDT.
           MOVE TVN-NUM-CXDR-EDT IN TVN-EDT TO 
                TVN-NUM-CXDR-ALF IN TVN-FLD.
           IF TVN-VAL-PDEO-LEN IN TVN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO TVN-VAL-PDEO-LEN IN TVN-FLD
                GO TO FIN-EDT-FLD-TVN.
           MOVE TVN-VAL-PDEO IN TVN-FLD TO TVN-VAL-PDEO-EDT IN TVN-EDT.
           MOVE TVN-VAL-PDEO-EDT IN TVN-EDT TO 
                TVN-VAL-PDEO-ALF IN TVN-FLD.
       FIN-EDT-FLD-TVN.
           EXIT.
