% TIPO_{TIPO} = TRUE
% IF MSKIDC THEN
% {MSK} = "MSK-"
% ELSE
% MSK = ""
% END
%! TIPO VRF
%!
% IF TIPO_VRF THEN
% IF NOT TIENE_IDC_{RREG}_IDC_{NOMB} THEN
      * VRF BANCOQUIA
      *
           05 FILLER                        PIC X(04) VALUE @277F{FCHEX_VRF_{RREG}_{NOMB}}@.
% IF NOT DISP THEN
           05 FILLER                        PIC X(01) VALUE @1C@.
% END
           05  {RREG}-NUM-{NOMB}-ATR              PIC X(01).
           05  {RREG}-NUM-{NOMB}-ALF             PIC  X(11).
           05  {RREG}-NUM-{NOMB}     REDEFINES
           05  {RREG}-NUM-{NOMB}-ALF             PIC  9(11).
% IF NOT DISP THEN
           05 FILLER                        PIC X(01) VALUE @1E@.
% END

           05 FILLER                        PIC X(01)
              VALUE "-".

% IF NOT DISP THEN
           05 FILLER                        PIC X(01) VALUE @1C@.
% END
          05 {RREG}-VRF-{NOMB}-ATR              PIC  X(01).
          05 {RREG}-VRF-{NOMB}                  PIC  X(01).
% IF NOT DISP THEN
           05 FILLER                        PIC X(01) VALUE @1E@.
% END
      * FIN VRF BANCOQUIA
% END
% ELSE
%! TIPO IDC
%!
% IF TIPO_IDC THEN
      * IDC BANCOQUIA
      *
           05 FILLER                        PIC X(04) VALUE @277F{FCHEX_VRF_{RREG}_{NOMB}}@.
% IF NOT DISP THEN
           05 FILLER                        PIC X(01) VALUE @1C@.
% END
           05 {MSK}{RREG}-NUM-{NOMB}-ATR              PIC X(01).
           05 {MSK}{RREG}-NUM-{NOMB}-ALF             PIC  X(11).
           05 {MSK}{RREG}-NUM-{NOMB}     REDEFINES
           05 {MSK}{RREG}-NUM-{NOMB}-ALF             PIC  9(11).
% IF NOT DISP THEN
           05 FILLER                        PIC X(01) VALUE @1E@.
% END

           05 FILLER                        PIC X(01)
              VALUE "-".

% IF NOT DISP THEN
           05 FILLER                        PIC X(01) VALUE @1C@.
% END
           05 {MSK}{RREG}-VRF-{NOMB}-ATR              PIC  X(01).
           05 {MSK}{RREG}-VRF-{NOMB}                  PIC  X(01).
% IF NOT DISP THEN
           05 FILLER                                PIC X(01) VALUE @1E@.
% END

           05 FILLER                                PIC X(01)
              VALUE SPACES.

           05 FILLER                        PIC X(04) VALUE @277F{FCHEX_IDC_{RREG}_{NOMB}}@.
% IF NOT DISP THEN
           05 FILLER                        PIC X(01) VALUE @1C@.
% END
          05 {MSK}{RREG}-IND-{NOMB}-ATR              PIC  X(01).
          05 {MSK}{RREG}-IND-{NOMB}                  PIC  X(01).
% IF NOT DISP THEN
           05 FILLER                        PIC X(01) VALUE @1E@.
% END

           05 FILLER                        PIC X(01)
              VALUE "-".

% IF NOT DISP THEN
           05 FILLER                        PIC X(01) VALUE @1C@.
% END
          05 {MSK}{RREG}-GLS-{NOMB}-ATR              PIC  X(01).
          05 {MSK}{RREG}-GLS-{NOMB}{IDX}             PIC X(03)
              VALUE SPACES.
% IF NOT DISP THEN
           05 FILLER                        PIC X(01) VALUE @1E@.
% END
      * FIN IDC BANCOQUIA
% ELSE
%! TIPO FEC
%!
% IF TIPO_FEC
      *    FECHA FEC BANCOQUIA
           05 {MSK}{CAMP}{IDX}            PIC X(08)
              VALUE SPACES.
% ELSE
% IF TIPO_DMA OR TIPO_AMD OR TIPO_MSA OR TIPO_SAM THEN
           05 {MSK}{CAMP}{IDX}            PIC X(06)
              VALUE SPACES.
% ELSE
% IF TIPO_FMA OR TIPO_FAM OR TIPO_FSA OR TIPO_FAS OR TIPO_FDM OR TIPO_FMD THEN
           05 {MSK}{CAMP}{IDX}            PIC X(04)
              VALUE SPACES.
% ELSE
% IF TIPO_FFA THEN
           05 {MSK}{CAMP}{IDX}            PIC X(02)
              VALUE SPACES.
% END IF
% END
% END
% END
% END
% END
% NORMA_CST = FALSE
% TIPO_{TIPO} = FALSE
