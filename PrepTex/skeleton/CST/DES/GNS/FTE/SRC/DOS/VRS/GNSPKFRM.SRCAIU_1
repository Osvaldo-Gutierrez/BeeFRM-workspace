*% END IF

       PROCEDURE DIVISION.
      *===================

       MAIN SECTION.
       INI-MAIN.
           PERFORM INI.
       LUP-MAIN.
           IF SCR-INST = SCR-INST-HCM
                 PERFORM SCR-COM.
% IF CPP_MOD OR CPP_ALL
           IF SCR-CCPP = 'MOD'
                 PERFORM SCR-MOD
           ELSE
% END
% IF CPP_ING OR CPP_ALL
           IF SCR-CCPP = 'ING'
                 PERFORM SCR-ING
           ELSE
% END
% IF CPP_ACC OR CPP_ALL
           IF SCR-CCPP = 'ACC'
                 PERFORM SCR-ACC
           ELSE
% END
% IF CPP_ELI OR CPP_ALL
           IF SCR-CCPP = 'ELI'
                 PERFORM SCR-ELI
           ELSE
% END
% IF CPP_ACT OR CPP_ALL
           IF SCR-CCPP = 'ACT'
                 PERFORM SCR-ACT
           ELSE
% END
% IF CPP_PGM OR CPP_ALL
           IF SCR-CCPP = 'PGM'
                 PERFORM SCR-PGM
           ELSE
% END
                 MOVE SCR-INST-HCM TO SCR-INST.
*% INCLUDE FIN_LUP_MAIN OF '{cda}.TXT'
*%! INCLUDE FIN_LUP_MAIN OF '{cda}.GEN'
           GO TO LUP-MAIN.
       FIN-MAIN.
           EXIT.

       INI SECTION.
       INI-INI.
% IF NOT NOCOMMAREA
       COPY GNSBGEIB.
% ELSE
*% INCLUDE PGM_INIT OF '{cda}.TXT'
*%! INCLUDE PGM_INIT OF '{cda}.GEN'
% END
*% INCLUDE PGM_INI OF '{cda}.TXT'
*%! INCLUDE PGM_INI OF '{cda}.GEN'
% IF PGM_DTC
       COPY GNSBGEDB.
% END
           PERFORM GNS-PRO-RCK.
% IF NOCOMMAREA
           PERFORM GNS-BUS-VTS.
% ELSE
           PERFORM GNS-BUS-VCA.
% END
           PERFORM GNS-HDL-SYS.
           PERFORM GNS-HDL-FRM.
% IF PGM_VSI
           PERFORM GNS-HDL-VSM.
% END
           MOVE '{pgm}' TO FIO-PROG.
           PERFORM GNS-BUS-IDD.
      *JSS-VRS Se debe cargar de tabla ( NO de MNU por la operadora )
           MOVE 'GNSAVRS' TO SCR-NLIB.
           MOVE 'GNSFVRS' TO SCR-NMAP.
*% INCLUDE INI_FIO_IBM OF '{cda}.TXT'
*%! INCLUDE INI_FIO_IBM OF '{cda}.GEN'
*% INCLUDE INI_FIO OF '{cda}.TXT'
*%! INCLUDE INI_FIO OF '{cda}.GEN'
       FIN-INI.
           EXIT.

       COPY GNSBHSYS.
       COPY GNSBHFRM.
% IF PGM_VSI
       COPY GNSBHVSM.
% END
       COPY GNSBPRCK.
% IF NOCOMMAREA
       COPY GNSBPVTS.
% ELSE
       COPY GNSBBVCA.
% END
       COPY GNSBBIDD.

       SCR-COM SECTION.
       INI-SCR-COM.
           MOVE SCR-INST-HCM TO SCR-INST.
           MOVE SCR-CMNP     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
           PERFORM GNS-PRO-SYS.
       XCT-ERR.
           MOVE 'XCTERR    ' TO MSG-COD-MENS.
           MOVE 'GNS'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO SYS-MEN1.
           MOVE SYS-PROG     TO SYS-MEN2.
           MOVE SYS-ABOR     TO SYS-CMND.
           PERFORM GNS-PRO-SYS.
       COPY GNSBGGBK.
       FIN-SCR-COM.
           EXIT.

% IF CPP_MOD OR CPP_ALL
       SCR-MOD SECTION.
       INI-SCR-MOD.
*% INCLUDE INI_SCR_MOD OF '{cda}.TXT'
*%! INCLUDE INI_SCR_MOD OF '{cda}.GEN'
           IF SCR-INST = SCR-INST-HKY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM HBL-KEY
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-MOD
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON.
           PERFORM DESSCRTRL.
*% INCLUDE PFS_SCR_MOD OF '{cda}.TXT'
*%! INCLUDE PFS_SCR_MOD OF '{cda}.GEN'
% END
% IF CPP_MOD OR CPP_ACT OR CPP_ALL
       MSG-SCR-MOD.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'MODNOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-MOD
               IF FIO-STAT-OKS
                   IF FRM-MENS NOT > SPACES
                       MOVE 'MODYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-MOD.
*% INCLUDE FIN_SCR_MOD OF '{cda}.TXT'
*%! INCLUDE FIN_SCR_MOD OF '{cda}.GEN'
           EXIT.
% END

% IF CPP_ING OR CPP_ALL
       SCR-ING SECTION.
       INI-SCR-ING.
*% INCLUDE INI_SCR_ING OF '{cda}.TXT'
*%! INCLUDE INI_SCR_ING OF '{cda}.GEN'
           IF SCR-INST = SCR-INST-HKY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM HBL-IKY
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-RST
                PERFORM GET-IKY
                IF NOT FRM-STAT-ABT
                        MOVE SPACES TO FRM-MENS
                        PERFORM FND-KEY
                        IF NOT FIO-STAT-OKS AND
                           FIO-STAT NOT = FIO-STAT-NEX
      *JSS                  PERFORM CLR-FLD-{frm}
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR-PGM
                            PERFORM SCR-TRL
                        ELSE
                        MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR-PGM
                        IF FRM-MENS NOT > SPACES
                            MOVE 'REGEXI    ' TO MSG-COD-MENS
                            MOVE 'GNS' TO MSG-COD-SIST
                            PERFORM GET-MSG
                            MOVE MSG-GLS-DESC TO FRM-MENS
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-ING
                        ELSE
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-ING
                ELSE
                        MOVE SPACES TO FRM-MENS
                        PERFORM SCR-MSSG
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ING
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON.
           PERFORM DESSCRTRL.
*% INCLUDE PFS_SCR_ING OF '{cda}.TXT'
*%! INCLUDE PFS_SCR_ING OF '{cda}.GEN'
% END
% IF CPP_ING OR CPP_ACT OR CPP_ALL
       MSG-SCR-ING.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'INGNOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-ING
               IF FIO-STAT-OKS
                   IF FRM-MENS NOT > SPACES
                       MOVE 'INGYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ING.
*% INCLUDE FIN_SCR_ING OF '{cda}.TXT'
*%! INCLUDE FIN_SCR_ING OF '{cda}.GEN'
           EXIT.
% END

% IF CPP_ACC OR CPP_ALL
       SCR-ACC SECTION.
       INI-SCR-ACC.
*% INCLUDE INI_SCR_ACC OF '{cda}.TXT'
*%! INCLUDE INI_SCR_ACC OF '{cda}.GEN'
           IF SCR-INST = SCR-INST-HKY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM HBL-KEY
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
      *JSS-VRS          PERFORM SCR-TRL
                        PERFORM EDT-FLD-{frm}
                        PERFORM GNS-PUT-OUF
                        PERFORM DESSCRTRL
                        MOVE 'S' TO PGM-IPUT
                        GO TO FIN-SCR-ACC
      *
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ACC
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON.
           PERFORM DESSCRTRL.
*% INCLUDE PFS_SCR_ACC OF '{cda}.TXT'
*%! INCLUDE PFS_SCR_ACC OF '{cda}.GEN'
           MOVE SPACES TO FRM-MENS.
           IF SCR-STAT NOT = SCR-STAT-CON
               PERFORM CMN-ACC.
           PERFORM SCR-MSSG.
       FIN-SCR-ACC.
*% INCLUDE FIN_SCR_ACC OF '{cda}.TXT'
*%! INCLUDE FIN_SCR_ACC OF '{cda}.GEN'
           EXIT.
% END

% IF CPP_ELI OR CPP_ALL
       SCR-ELI SECTION.
       INI-SCR-ELI.
*% INCLUDE INI_SCR_ELI OF '{cda}.TXT'
*%! INCLUDE INI_SCR_ELI OF '{cda}.GEN'
           IF SCR-INST = SCR-INST-HKY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM HBL-KEY
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ELI
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON.
           PERFORM DESSCRTRL.
*% INCLUDE PFS_SCR_ELI OF '{cda}.TXT'
*%! INCLUDE PFS_SCR_ELI OF '{cda}.GEN'
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'ELINOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-ELI
               IF FIO-STAT-OKS
                   IF FRM-MENS NOT > SPACES
                       MOVE 'ELIYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MENS
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ELI.
*% INCLUDE FIN_SCR_ELI OF '{cda}.TXT'
*%! INCLUDE FIN_SCR_ELI OF '{cda}.GEN'
           EXIT.
% END

% IF CPP_ACT OR CPP_ALL
       SCR-ACT SECTION.
       INI-SCR-ACT.
*% INCLUDE INI_SCR_ACT OF '{cda}.TXT'
*%! INCLUDE INI_SCR_ACT OF '{cda}.GEN'
           IF SCR-INST = SCR-INST-HKY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM HBL-KEY
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-RST
                PERFORM GET-KEY
                IF NOT FRM-STAT-ABT
                        MOVE SPACES TO FRM-MENS
                        PERFORM FND-KEY
                        IF FIO-STAT-OKS
                            MOVE SCR-ACTU-MOD TO SCR-ACTU
                            PERFORM PUT-SCR
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR-PGM
                            PERFORM SCR-TRL
                        ELSE
                        MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR-PGM
                        IF FIO-STAT-EOF
                            IF FRM-MENS NOT > SPACES
                                MOVE 'REGEOF    ' TO MSG-COD-MENS
                                MOVE 'GNS' TO MSG-COD-SIST
                                PERFORM GET-MSG
                                MOVE MSG-GLS-DESC TO FRM-MENS
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                            ELSE
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                        ELSE
                        IF FIO-STAT = FIO-STAT-NEX
                            IF FRM-MENS NOT > SPACES
                                MOVE 'REGNEX    ' TO MSG-COD-MENS
                                MOVE 'GNS' TO MSG-COD-SIST
                                PERFORM GET-MSG
                                MOVE MSG-GLS-DESC TO FRM-MENS
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                            ELSE
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                        ELSE
                            MOVE SCR-ACTU-ING TO SCR-ACTU
      *JSS                  PERFORM CLR-FLD-{frm}
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR-PGM
                            PERFORM SCR-TRL
                ELSE
                        MOVE SPACES TO FRM-MENS
                        PERFORM SCR-MSSG
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ACT
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON.
           PERFORM DESSCRTRL.
*% INCLUDE PFS_SCR_ACT OF '{cda}.TXT'
*%! INCLUDE PFS_SCR_ACT OF '{cda}.GEN'
           IF SCR-ACTU = SCR-ACTU-MOD
                  PERFORM MSG-SCR-MOD
           ELSE
                  PERFORM MSG-SCR-ING.
       FIN-SCR-ACT.
*% INCLUDE FIN_SCR_ACT OF '{cda}.TXT'
*%! INCLUDE FIN_SCR_ACT OF '{cda}.GEN'
           EXIT.
% END

% IF CPP_PGM OR CPP_ALL
       SCR-PGM SECTION.
       INI-SCR-PGM.
*% INCLUDE INI_SCR_PGM OF '{cda}.TXT'
*%! INCLUDE INI_SCR_PGM OF '{cda}.GEN'
           IF SCR-INST = SCR-INST-HKY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM HBL-KEY
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-RST
                PERFORM GET-KEY
                IF FRM-STAT-ABT
                        MOVE SPACES TO FRM-MENS
                        PERFORM SCR-MSSG
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-PGM.
           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
      *JSS IF NOT FIO-STAT-OKS
      *        PERFORM CLR-FLD-{frm}.
           IF FIO-STAT = FIO-STAT-NEX
               IF FRM-MENS NOT > SPACES
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   PERFORM SCR-MSSG
                   MOVE SCR-STAT-ABT TO SCR-STAT
                   PERFORM DESSCRTRL
                   GO TO FIN-SCR-PGM
               ELSE
                   PERFORM SCR-MSSG
                   MOVE SCR-STAT-ABT TO SCR-STAT
                   PERFORM DESSCRTRL
                   GO TO FIN-SCR-PGM.
*% INCLUDE SCR_PGM OF '{cda}.TXT'
*%! INCLUDE SCR_PGM OF '{cda}.GEN'
       TRL-SCR-PGM.
           PERFORM DESSCRTRL.
*% INCLUDE PFS_SCR_PGM OF '{cda}.TXT'
*%! INCLUDE PFS_SCR_PGM OF '{cda}.GEN'
           MOVE SPACES TO FRM-MENS.
           IF SCR-STAT NOT = SCR-STAT-CON
               PERFORM CMN-PGM.
           PERFORM SCR-MSSG.
       FIN-SCR-PGM.
*% INCLUDE FIN_SCR_PGM OF '{cda}.TXT'
*%! INCLUDE FIN_SCR_PGM OF '{cda}.GEN'
           EXIT.
% END

       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR-USR NOT > SPACES
               MOVE PGM-ICLR-PGM TO PGM-ICLR-USR.
           MOVE SPACES TO PGM-ICLR-PGM.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-{frm}
               PERFORM EDT-KEY-{frm}
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-CAMPOS
               PERFORM CLR-FLD-{frm}
               PERFORM EDT-FLD-{frm}
           ELSE
               PERFORM SCR-CLRA-{frm}
               PERFORM EDT-KEY-{frm}
               PERFORM EDT-FLD-{frm}.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

       SCR-KEY SECTION.
       INI-SCR-KEY.
           PERFORM SCR-RST.
           PERFORM GET-KEY.
           IF FRM-STAT-ABT
                   MOVE SPACES TO FRM-MENS
                   PERFORM SCR-MSSG
                   GO TO FIN-SCR-KEY.
       FND-SCR-KEY.
           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
           IF FIO-STAT-OKS
               PERFORM PUT-SCR
               MOVE PGM-LIMPIAR-TODO TO PGM-ICLR-PGM
               GO TO FIN-SCR-KEY.
      *JSS ELSE
      *        PERFORM CLR-FLD-{frm}.
       ERR-SCR-KEY.
           MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR-PGM.
           IF FRM-MENS NOT > SPACES
               IF FIO-STAT-EOF
                   MOVE 'REGEOF    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
               IF FIO-STAT = FIO-STAT-NEX
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
           MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-SCR-KEY.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES           TO PGM-ICLR-USR.
           MOVE PGM-LIMPIAR-TODO TO PGM-ICLR-PGM.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO {frm}-FLD.
*% INCLUDE SCR_RST OF '{cda}.TXT'
*%! INCLUDE SCR_RST OF '{cda}.GEN'
       FIN-SCR-RST.
           EXIT.

       GET-KEY SECTION.
       INI-GET-KEY.
*% INCLUDE GET_KEY OF '{cda}.TXT'
*%! INCLUDE GET_KEY OF '{cda}.GEN'
           PERFORM SCR-GKEY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-KEY.
           EXIT.

       GET-IKY SECTION.
       INI-GET-IKY.
*% INCLUDE GET_IKY OF '{cda}.TXT'
*%! INCLUDE GET_IKY OF '{cda}.GEN'
           PERFORM SCR-GIKY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-IKY.
           EXIT.

       FND-KEY SECTION.
       INI-FND-KEY.
*% INCLUDE INI_FND_KEY OF '{cda}.TXT'
*%! INCLUDE INI_FND_KEY OF '{cda}.GEN'
           PERFORM KEY-ALL-{frm}.
*% INCLUDE FND_KEY OF '{cda}.TXT'
*%! INCLUDE FND_KEY OF '{cda}.GEN'
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM {sio}-FIO-{reg}.
% IF TRG > 0
           MOVE FIO-STAT TO PGM-STAT-{reg}.
% END
*% INCLUDE FIN_FND_KEY OF '{cda}.TXT'
*%! INCLUDE FIN_FND_KEY OF '{cda}.GEN'
       FIN-FND-KEY.
           EXIT.

       PUT-SCR SECTION.
       INI-PUT-SCR.
      *JSS-VRS
      *    PERFORM GNS-PUT-OUF.
      *
*% INCLUDE PUT_SCR OF '{cda}.TXT'
*%! INCLUDE PUT_SCR OF '{cda}.GEN'
           PERFORM PUT-ALL-{frm}.
       FIN-PUT-SCR.
           EXIT.

       GET-FLD SECTION.
       INI-GET-FLD.
*% INCLUDE GET_FLD OF '{cda}.TXT'
*%! INCLUDE GET_FLD OF '{cda}.GEN'
           PERFORM SCR-GFLD.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-FLD.
           EXIT.

% IF CPP_MOD OR CPP_ACT OR CPP_ALL
       CMN-MOD SECTION.
       INI-CMN-MOD.
*% INCLUDE INI_CMN_MOD OF '{cda}.TXT'
*%! INCLUDE INI_CMN_MOD OF '{cda}.GEN'
           PERFORM KEY-ALL-{frm}.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM {sio}-FIO-{reg}.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-MOD.
*% INCLUDE CMN_MOD_RET OF '{cda}.TXT'
*%! INCLUDE CMN_MOD_RET OF '{cda}.GEN'
           PERFORM RET-ALL-{frm}.
*% INCLUDE CMN_MOD OF '{cda}.TXT'
*%! INCLUDE CMN_MOD OF '{cda}.GEN'
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM {sio}-FIO-{reg}.
*% INCLUDE FIN_CMN_MOD OF '{cda}.TXT'
*%! INCLUDE FIN_CMN_MOD OF '{cda}.GEN'
       FIN-CMN-MOD.
           EXIT.
% END

% IF CPP_ING OR CPP_ACT OR CPP_ALL
       CMN-ING SECTION.
       INI-CMN-ING.
*% INCLUDE INI_CMN_ING OF '{cda}.TXT'
*%! INCLUDE INI_CMN_ING OF '{cda}.GEN'
           PERFORM RET-ALL-{frm}.
*% INCLUDE CMN_ING OF '{cda}.TXT'
*%! INCLUDE CMN_ING OF '{cda}.GEN'
           MOVE FIO-PUT TO FIO-CMND.
           PERFORM {sio}-FIO-{reg}.
*% INCLUDE FIN_CMN_ING OF '{cda}.TXT'
*%! INCLUDE FIN_CMN_ING OF '{cda}.GEN'
       FIN-CMN-ING.
           EXIT.
% END

% IF CPP_ACC OR CPP_ALL
       CMN-ACC SECTION.
       INI-CMN-ACC.
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'ACCION NO REALIZADA' TO FRM-MENS
*% INCLUDE ABT_CMN_ACC OF '{cda}.TXT'
*%! INCLUDE ABT_CMN_ACC OF '{cda}.GEN'
               GO TO FIN-CMN-ACC.
*% INCLUDE INI_CMN_ACC OF '{cda}.TXT'
*%! INCLUDE INI_CMN_ACC OF '{cda}.GEN'
           PERFORM KEY-ALL-{frm}.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM {sio}-FIO-{reg}.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ACC.
*% INCLUDE CMN_ACC_RET OF '{cda}.TXT'
*%! INCLUDE CMN_ACC_RET OF '{cda}.GEN'
           PERFORM RET-ALL-{frm}.
*% INCLUDE CMN_ACC OF '{cda}.TXT'
*%! INCLUDE CMN_ACC OF '{cda}.GEN'
       FIN-CMN-ACC.
           EXIT.
% END

% IF CPP_ELI OR CPP_ALL
       CMN-ELI SECTION.
       INI-CMN-ELI.
*% INCLUDE INI_CMN_ELI OF '{cda}.TXT'
*%! INCLUDE INI_CMN_ELI OF '{cda}.GEN'
           PERFORM KEY-ALL-{frm}.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM {sio}-FIO-{reg}.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ELI.
*% INCLUDE CMN_ELI OF '{cda}.TXT'
*%! INCLUDE CMN_ELI OF '{cda}.GEN'
           MOVE FIO-DEL TO FIO-CMND.
           PERFORM {sio}-FIO-{reg}.
*% INCLUDE FIN_CMN_ELI OF '{cda}.TXT'
*%! INCLUDE FIN_CMN_ELI OF '{cda}.GEN'
       FIN-CMN-ELI.
           EXIT.
% END

% IF CPP_PGM OR CPP_ALL
       CMN-PGM SECTION.
       INI-CMN-PGM.
*% INCLUDE INI_CMN_PGM OF '{cda}.TXT'
*%! INCLUDE INI_CMN_PGM OF '{cda}.GEN'
           PERFORM RET-ALL-{frm}.
*% INCLUDE CMN_PGM OF '{cda}.TXT'
*%! INCLUDE CMN_PGM OF '{cda}.GEN'
       FIN-CMN-PGM.
           EXIT.
% END

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE 'P'      TO SCR-ICMN.
% IF NOCOMMAREA
           PERFORM GNS-MOD-VTS.
           MOVE SCR-NTRN TO SYS-NTRN.
           MOVE SPACES   TO SYS-CMMA.
           MOVE 0        TO SYS-TCMA.
*% INCLUDE PGM_EXIT OF '{cda}.TXT'
*%! INCLUDE PGM_EXIT OF '{cda}.GEN'
           MOVE SYS-RTRN TO SYS-CMND.
           PERFORM GNS-PRO-SYS.
% ELSE
           MOVE SCR-NTRN TO SYS-NTRN.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
           PERFORM GNS-PRO-SYS.
% END
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
      *JSS-VRS
           MOVE LOW-VALUES TO {frm}-VRS-MENS IN {frm}-FLD-RED1.
           MOVE SCR-RFBY TO {frm}-VRS-RFBY IN {frm}-FLD-RED1.
           IF {frm}-VRS-RFBY IN {frm}-FLD-RED1 = '1'
               MOVE FRM-CPIM-UBY TO {frm}-VRS-RFBY-ATR IN {frm}-FLD-RED1
               MOVE '0'          TO {frm}-VRS-RFBY     IN {frm}-FLD-RED1
           ELSE
               MOVE FRM-CPIM-UBY TO {frm}-VRS-RFBY-ATR IN {frm}-FLD-RED1
               MOVE '1'          TO {frm}-VRS-RFBY     IN {frm}-FLD-RED1.
           MOVE {frm}-VRS-RFBY IN {frm}-FLD-RED1 TO SCR-RFBY.
      *
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE {frm}-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO {frm}-GLS-MENS.
      *JSS-VRS
           IF FRM-ABRV > SPACES
               MOVE FRM-ABRV TO {frm}-VRS-OMSG
           ELSE
               MOVE SPACES   TO {frm}-VRS-OMSG.
      *
       FIN-SCR-MSSG.
           EXIT.

       SCR-GKEY SECTION.
       INI-SCR-GKEY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
*% INCLUDE PFS_SCR_GKEY OF '{cda}.TXT'
*%! INCLUDE PFS_SCR_GKEY OF '{cda}.GEN'
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-{frm}
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO {frm}-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GKEY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-{frm}.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-{frm}
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-{frm}.
*% INCLUDE SCR_GKEY_VAL_IKY OF '{cda}.TXT'
*%! INCLUDE SCR_GKEY_VAL_IKY OF '{cda}.GEN'
           PERFORM VAL-IKY-{frm}.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
*% INCLUDE SCR_GKEY_VAL_MKY OF '{cda}.TXT'
*%! INCLUDE SCR_GKEY_VAL_MKY OF '{cda}.GEN'
           PERFORM VAL-MKY-{frm}.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
*% INCLUDE SCR_GKEY_VAL_AKY OF '{cda}.TXT'
*%! INCLUDE SCR_GKEY_VAL_AKY OF '{cda}.GEN'
           PERFORM VAL-AKY-{frm}.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
*% INCLUDE SCR_GKEY_VAL_CON_KEY OF '{cda}.TXT'
*%! INCLUDE SCR_GKEY_VAL_CON_KEY OF '{cda}.GEN'
           PERFORM VAL-CON-KEY-{frm}.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GKEY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-{frm}.
       ERR-SCR-GKEY.
*% INCLUDE ERR_SCR_GKEY OF '{cda}.TXT'
*%! INCLUDE ERR_SCR_GKEY OF '{cda}.GEN'
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-{frm}.
      *JSS-VRS SOLO SE DEBE RECUPERAR EL PROMPT DEL DATO QUE SALIO MALO
           PERFORM GNS-ERR-IOF.
      *
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GKEY.
*% INCLUDE FIN_SCR_GKEY OF '{cda}.TXT'
*%! INCLUDE FIN_SCR_GKEY OF '{cda}.GEN'
           PERFORM SCR-MSSG.

       SCR-GIKY SECTION.
       INI-SCR-GIKY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
*% INCLUDE PFS_SCR_GIKY OF '{cda}.TXT'
*%! INCLUDE PFS_SCR_GIKY OF '{cda}.GEN'
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-{frm}
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO {frm}-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GIKY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-IKY-{frm}.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               PERFORM SCR-MSSG
               PERFORM EDT-IKY-{frm}
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-{frm}.
*% INCLUDE SCR_GIKY_VAL_IKY OF '{cda}.TXT'
*%! INCLUDE SCR_GIKY_VAL_IKY OF '{cda}.GEN'
           PERFORM VAL-IKY-{frm}.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GIKY.
*% INCLUDE SCR_GIKY_VAL_CON_IKY OF '{cda}.TXT'
*%! INCLUDE SCR_GIKY_VAL_CON_IKY OF '{cda}.GEN'
           PERFORM VAL-CON-IKY-{frm}.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GIKY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-{frm}.
       ERR-SCR-GIKY.
*% INCLUDE ERR_SCR_GIKY OF '{cda}.TXT'
*%! INCLUDE ERR_SCR_GIKY OF '{cda}.GEN'
           PERFORM SCR-MSSG.
           PERFORM EDT-IKY-{frm}.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GIKY.
*% INCLUDE FIN_SCR_GIKY OF '{cda}.TXT'
*%! INCLUDE FIN_SCR_GIKY OF '{cda}.GEN'
           PERFORM SCR-MSSG.

       SCR-GFLD SECTION.
       INI-SCR-GFLD.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
*% INCLUDE PFS_SCR_GFLD OF '{cda}.TXT'
*%! INCLUDE PFS_SCR_GFLD OF '{cda}.GEN'
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-FLD-{frm}
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO {frm}-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GFLD.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-{frm}.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-{frm}
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           PERFORM VAL-NUM-FLD-{frm}.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-{frm}
               PERFORM EDT-FLD-{frm}
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
*% INCLUDE SCR_GFLD_VAL_MKY OF '{cda}.TXT'
*%! INCLUDE SCR_GFLD_VAL_MKY OF '{cda}.GEN'
           PERFORM VAL-MKY-{frm}.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GFLD.
*% INCLUDE SCR_GFLD_VAL_FLD OF '{cda}.TXT'
*%! INCLUDE SCR_GFLD_VAL_FLD OF '{cda}.GEN'
           PERFORM VAL-FLD-{frm}.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GFLD.
*% INCLUDE SCR_GFLD_VAL_CON_FLD OF '{cda}.TXT'
*%! INCLUDE SCR_GFLD_VAL_CON_FLD OF '{cda}.GEN'
           PERFORM VAL-CON-FLD-{frm}.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GFLD.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-FLD-{frm}.
       ERR-SCR-GFLD.
*% INCLUDE ERR_SCR_GFLD OF '{cda}.TXT'
*%! INCLUDE ERR_SCR_GFLD OF '{cda}.GEN'
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-{frm}.
           PERFORM EDT-FLD-{frm}.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GFLD.
*% INCLUDE FIN_SCR_GFLD OF '{cda}.TXT'
*%! INCLUDE FIN_SCR_GFLD OF '{cda}.GEN'
           MOVE SCR-STAT-MOD TO SCR-STAT.

       GET-ACC SECTION.
       INI-GET-ACC.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
*% INCLUDE PFS_GET_ACC OF '{cda}.TXT'
*%! INCLUDE PFS_GET_ACC OF '{cda}.GEN'
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO {frm}-GLS-MEN2
               MOVE SCR-PRMT     TO {frm}-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO {frm}-FLD.
           IF NOT ( ( FRM-FFLD = FRM-FFLD-PF12 OR FRM-FFLD-PF3 ) OR
                      SCR-CONF > SPACES )
               PERFORM VAL-NUM-KEY-{frm}
               PERFORM VAL-NUM-FLD-{frm}.
*% INCLUDE FIN_GET_ACC OF '{cda}.TXT'
*%! INCLUDE FIN_GET_ACC OF '{cda}.GEN'
       FIN-GET-ACC.
           MOVE SCR-STAT-SON TO SCR-STAT.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
*% INCLUDE PFS_GET_CON OF '{cda}.TXT'
*%! INCLUDE PFS_GET_CON OF '{cda}.GEN'
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO {frm}-GLS-MEN2
               MOVE SCR-PRMT     TO {frm}-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO {frm}-FLD.
*% INCLUDE FIN_GET_CON OF '{cda}.TXT'
*%! INCLUDE FIN_GET_CON OF '{cda}.GEN'
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO {frm}-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
*% INCLUDE INI_HBL_IKY OF '{cda}.TXT'
*%! INCLUDE INI_HBL_IKY OF '{cda}.GEN'
           IF SCR-DESDE-CMN
               MOVE {frm}-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO {frm}-FLD
               MOVE FRM-MENS TO {frm}-GLS-MENS.
      *JSS ANALIZAR PARA VRS
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-FLD-{frm}.
           PERFORM PRO-MKY-{frm}.
           PERFORM PRO-AKY-{frm}.
           PERFORM UNP-IKY-{frm}.
           PERFORM FST-KEY-{frm}.
*% INCLUDE FIN_HBL_IKY OF '{cda}.TXT'
*%! INCLUDE FIN_HBL_IKY OF '{cda}.GEN'
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
*% INCLUDE INI_HBL_KEY OF '{cda}.TXT'
*%! INCLUDE INI_HBL_KEY OF '{cda}.GEN'
           IF SCR-DESDE-CMN
               MOVE {frm}-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO {frm}-FLD
               MOVE FRM-MENS TO {frm}-GLS-MENS.
      *JSS ANALIZAR PARA VRS
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-FLD-{frm}.
           PERFORM UNP-KEY-{frm}.
           PERFORM FST-KEY-{frm}.
      *JSS-VRS
           PERFORM GNS-PUT-IOF.
      *ESTO ES NECESARIO HACERLO UNA VEZ..........
           PERFORM GNS-PUT-DCR.
           MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR-PGM.
      *
*% INCLUDE FIN_HBL_KEY OF '{cda}.TXT'
*%! INCLUDE FIN_HBL_KEY OF '{cda}.GEN'
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
*% INCLUDE INI_HBL_FLD OF '{cda}.TXT'
*%! INCLUDE INI_HBL_FLD OF '{cda}.GEN'
           PERFORM PRO-IKY-{frm}.
           PERFORM PRO-AKY-{frm}.
           PERFORM UNP-MKY-{frm}.
           PERFORM UNP-FLD-{frm}.
           PERFORM FST-FLD-{frm}.
*% INCLUDE FIN_HBL_FLD OF '{cda}.TXT'
*%! INCLUDE FIN_HBL_FLD OF '{cda}.GEN'
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO {frm}-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-{frm}.
           PERFORM PRO-FLD-{frm}.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
*% INCLUDE INI_SCR_TRL OF '{cda}.TXT'
*%! INCLUDE INI_SCR_TRL OF '{cda}.GEN'
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
*% INCLUDE FIN_SCR_TRL OF '{cda}.TXT'
*%! INCLUDE FIN_SCR_TRL OF '{cda}.GEN'
           IF SCR-STAT = SCR-STAT-MOD
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-{frm}
               PERFORM EDT-FLD-{frm}
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-{frm}
               PERFORM EDT-FLD-{frm}
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
           MOVE SCR-INST-GAB TO SCR-INST.
      *JSS ANALIZAR PARA VRS
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-{frm}.
           PERFORM EDT-FLD-{frm}.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           IF SCR-INST = SCR-INST-GCF
                  PERFORM SCR-GCNF
                  GO TO FIN-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
           IF SCR-STAT = SCR-STAT-ABT OR SCR-STAT-CON
                  GO TO FIN-DESSCRTRL.
           IF SCR-FFLD-RET OR SCR-FFLD-PF10
                  MOVE SCR-VFLD-YES TO SCR-VFLD
                  PERFORM PRO-CONF.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

       PRO-CONF SECTION.
       INI-PRO-CONF.
           IF FRM-MENS NOT > SPACES
               IF SCR-CONF NOT > SPACES
                   GO TO FIN-PRO-CONF
               ELSE
                   MOVE SCR-CONF TO FRM-MENS
           ELSE
               IF FRM-MEN1 NOT > SPACES
                   MOVE SCR-CONF TO FRM-MEN1.
           MOVE SCR-INST-GCF TO SCR-INST.
           PERFORM SCR-MSSG.
           PERFORM PRO-KEY-{frm}.
           PERFORM PRO-FLD-{frm}.
           PERFORM FST-MSG.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM EDT-KEY-{frm}
               PERFORM EDT-FLD-{frm}.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-PRO-CONF.
           EXIT.

       SCR-GCNF SECTION.
       INI-SCR-GCNF.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3  OR
                               FRM-FFLD-PF4 )
*% INCLUDE PFS_SCR_GCNF OF '{cda}.TXT'
*%! INCLUDE PFS_SCR_GCNF OF '{cda}.GEN'
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO {frm}-GLS-MEN2
               MOVE SCR-CONF     TO {frm}-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO {frm}-FLD.
           MOVE FRM-FFLD TO SCR-FFLD.
           IF FRM-FFLD = FRM-FFLD-PF3
               MOVE SCR-VFLD-NOT TO SCR-VFLD
           ELSE
           IF FRM-FFLD = FRM-FFLD-RET OR FRM-FFLD-PF10 OR FRM-FFLD-PF4
               MOVE SCR-VFLD-YES TO SCR-VFLD
               PERFORM VAL-NUM-KEY-{frm}
               PERFORM VAL-NUM-FLD-{frm}
           ELSE
           IF FRM-FFLD = FRM-FFLD-PF12
               MOVE SCR-STPP TO SCR-STAT
               IF SCR-STAT = SCR-STAT-MOD
                   PERFORM VAL-NUM-KEY-{frm}
                   PERFORM VAL-NUM-FLD-{frm}
                   PERFORM SCR-TRL
               ELSE
                   MOVE SCR-VFLD-NOT TO SCR-VFLD.
*% INCLUDE FIN_SCR_GCNF OF '{cda}.TXT'
*%! INCLUDE FIN_SCR_GCNF OF '{cda}.GEN'
       FIN-SCR-GCNF.
           EXIT.

       COPY {sfr}BM{frm}.
       COPY {sio}BF{reg}.
       COPY GNSBFMSG.
       COPY GNSBGSYS.
% IF PGM_VSI
       COPY GNSBGVSM.
% END
% IF PGM_DTC
       COPY GNSBGDTC.
% END
       COPY GNSBGFRM.
      *JSS-VRS
       COPY GNSBGMSV.
      *
       COPY GNSBEKEY.
       COPY GNSBIABT.
% IF BUSTAB
       COPY GNSBBTAB.
% END
% IF VALCOD
       COPY GNSBVCOD.
       COPY GNSBVTAB.
% END
%! IF BUSTAB OR VALCOD OR FCH
       COPY GNSBFTAB.
%! END
% IF BUSMSC
       COPY GNSBBMSC.
% END
% IF VALIND OR IDC
       COPY GNSBVIND.
       COPY GNSBVMSC.
% END
% IF BUSMSC OR VALIND OR IDC
       COPY GNSBFMSC.
% END
% IF IDC
       COPY GNSBVIDC.
% END
% IF VRF
       COPY GNSBPVRF.
% END
% IF ERRGLS
       COPY GNSBEGLS.
% END
% IF FCH
       COPY GNSBGFEC.
       COPY GNSBGHOY.
% END
% IF BUSOFI
       COPY TABBBOFI.
% END
% IF VALOFI
       COPY TABBVOFI.
% END
% IF BUSOFI OR VALOFI
       COPY TABBFOFI.
% END
% IF BUSUSR
       COPY TABBBUSR.
% END
% IF VALUSR
       COPY TABBVUSR.
% END
% IF VALTDU
       COPY TABBVTDU.
% END
% IF BUSUSR OR VALUSR OR VALTDU
       COPY TABBFUSR.
% END

      *JSS-VRS
       GNS-PUT-IOF SECTION.
       INI-GNS-PUT-IOF.
           IF PGM-IPUT-YES
      *PGV
               ADD 1 TO VRS-IFLD
               MOVE 'A' TO {frm}-VRS-VFLD IN {frm}-FLD-RED1(VRS-IFLD)
               GO TO FIN-GNS-PUT-IOF.

           MOVE SCR-SIST TO FIO-SIST.
           MOVE 'VRP'    TO TAB-COD-TTAB IN TAB.
           MOVE SCR-PROG TO PGM-PVRP.
           MOVE '/'      TO PGM-SVRP.
           MOVE 'IO'     TO PGM-FVRP.
           MOVE PGM-CTVR TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
               GO TO FIN-GNS-PUT-IOF.
           MOVE TAB-GLS-DESC IN TAB TO PGM-DESC.
           MOVE TAB-GLS-DATA IN TAB TO PGM-DATA.

           MOVE PGM-STRN TO UNS-STRN.
           MOVE ','      TO UNS-DLMT.
           MOVE 0 TO FRM-IFLD.
       LUP-GNS-PUT-IOF.
           MOVE FRM-IFLD TO UNS-NELE.
           ADD 1 TO FRM-IFLD.
           IF FRM-IFLD > {frm}-VRS-INOU
               GO TO FIN-GNS-PUT-IOF.
           PERFORM GNS-UNS-STR.
           MOVE FRM-CPIM-UBY TO {frm}-VRS-IFLD-ATR(FRM-IFLD).
           IF UNS-ELEM = UNS-DLMT
               MOVE SPACES   TO {frm}-VRS-IFLD(FRM-IFLD)
           ELSE
               MOVE UNS-ELEM TO {frm}-VRS-IFLD(FRM-IFLD).
           GO TO LUP-GNS-PUT-IOF.
       FIN-GNS-PUT-IOF.
           EXIT.

       GNS-PUT-OUF SECTION.
       INI-GNS-PUT-OUF.
           MOVE SCR-SIST TO FIO-SIST.
           MOVE 'VRP'    TO TAB-COD-TTAB IN TAB.
           IF PGM-CTVR NOT > SPACES
               MOVE SCR-PROG TO PGM-PVRP
               MOVE '/'      TO PGM-SVRP
               MOVE 'OP'     TO PGM-FVRP.
           MOVE PGM-CTVR TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
               GO TO FIN-GNS-PUT-OUF.
           MOVE TAB-GLS-DESC IN TAB TO PGM-DESC.
           MOVE TAB-GLS-DATA IN TAB TO PGM-DATA.

           MOVE 0 TO FRM-IFLD.
      *PGV
           MOVE 0 TO VRS-IFLD.
           MOVE 0 TO VRS-NELE.
       LUP-GNS-PUT-OUF.
           MOVE PGM-STRN TO UNS-STRN.
           MOVE '/'      TO UNS-DLMT.
      *PGV
           MOVE VRS-NELE TO UNS-NELE.
           ADD 1 TO VRS-NELE.
           ADD 1 TO FRM-IFLD.
           IF FRM-IFLD > {frm}-VRS-OUTP
               GO TO FIN-GNS-PUT-OUF.
           IF {frm}-VRS-OFLD-ATR(FRM-IFLD) = FRM-CPIM-ANY
               MOVE 'M'   TO {frm}-VRS-OCOD(FRM-IFLD)
               MOVE '270' TO {frm}-VRS-ODAT(FRM-IFLD)
               GO TO LUP-GNS-PUT-OUF.
           PERFORM GNS-UNS-STR.
           IF UNS-ELEM = UNS-DLMT
               GO TO FIN-GNS-PUT-OUF.

           MOVE UNS-ELEM TO UNS-STRN.
           MOVE ','      TO UNS-DLMT.
           MOVE 0        TO UNS-NELE.
           PERFORM GNS-UNS-STR.
           IF UNS-ELEM = UNS-DLMT
               GO TO LUP-GNS-PUT-OUF.
           IF {frm}-VRS-OPRM(FRM-IFLD) NOT > SPACES
               MOVE UNS-ELEM TO {frm}-VRS-OPRM(FRM-IFLD).
           IF {frm}-VRS-OFLD-ATR(FRM-IFLD) = FRM-CPIM-UNY
               MOVE 'M'   TO {frm}-VRS-OCOD(FRM-IFLD)
               MOVE '271' TO {frm}-VRS-ODAT(FRM-IFLD)
               GO TO LUP-GNS-PUT-OUF.
           MOVE FRM-CPIM-UBY TO {frm}-VRS-OFLD-ATR(FRM-IFLD).

           MOVE 1        TO UNS-NELE.
           PERFORM GNS-UNS-STR.
           IF UNS-ELEM = UNS-DLMT
               GO TO LUP-GNS-PUT-OUF.
      *PGV
           IF UNS-ECHR(2) > SPACES
               MOVE {frm}-VRS-ODAT(FRM-IFLD) TO VRS-VAUX
               MOVE VRS-VAUX                 TO {frm}-VRS-ODA1(FRM-IFLD)
               MOVE UNS-ELEM                 TO {frm}-VRS-OCO1(FRM-IFLD)
               ADD 1                         TO VRS-IFLD
      *NO CONTROLA QUE SE EXCEDAN LOS IO FIELDS
               MOVE {frm}-VRS-OFLD(FRM-IFLD) TO
                    {frm}-VRS-VFLD IN {frm}-FLD-RED1(VRS-IFLD)
               MOVE SPACES                   TO {frm}-VRS-OFLD(FRM-IFLD)
               SUBTRACT 1 FROM FRM-IFLD
           ELSE
               MOVE UNS-ELEM TO {frm}-VRS-OCOD(FRM-IFLD).
           GO TO LUP-GNS-PUT-OUF.
       FIN-GNS-PUT-OUF.
           MOVE SPACES TO PGM-CTVR.
       EXT-GNS-PUT-OUF.
           EXIT.

       GNS-PUT-DCR SECTION.
       INI-GNS-PUT-DCR.
           MOVE SCR-SIST TO FIO-SIST.
           MOVE 'VRP'    TO TAB-COD-TTAB IN TAB.
           MOVE SCR-PROG TO PGM-PVRP.
           MOVE '/'      TO PGM-SVRP.
           MOVE 'DCR'    TO PGM-FVRP.
           MOVE PGM-CTVR TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF FIO-STAT-OKS
               MOVE TAB-GLS-DESC IN TAB TO PGM-DESC
               MOVE TAB-GLS-DATA IN TAB TO PGM-DATA
               GO TO CON-GNS-PUT-DCR.

           MOVE 'GNS'   TO FIO-SIST.
           MOVE 'VRP'   TO TAB-COD-TTAB IN TAB.
           MOVE '*/DCR' TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF FIO-STAT-OKS
               MOVE TAB-GLS-DESC IN TAB TO PGM-DESC
               MOVE TAB-GLS-DATA IN TAB TO PGM-DATA
           ELSE
               MOVE SPACES TO PGM-DESC
               MOVE SPACES TO PGM-DATA.

       CON-GNS-PUT-DCR.
           MOVE PGM-PBXE TO {frm}-VRS-PBXE IN {frm}-FLD-RED1.
           MOVE PGM-FVMU TO {frm}-VRS-FVMU IN {frm}-FLD-RED1.
           MOVE PGM-ASDT TO {frm}-VRS-ASDT IN {frm}-FLD-RED1.
           MOVE PGM-NTRI TO {frm}-VRS-NTRI IN {frm}-FLD-RED1.
           MOVE PGM-TFKY TO {frm}-VRS-TFKY IN {frm}-FLD-RED1.
           MOVE PGM-FKY5 TO {frm}-VRS-FKY5 IN {frm}-FLD-RED1.
           MOVE PGM-FKY6 TO {frm}-VRS-FKY6 IN {frm}-FLD-RED1.
           MOVE PGM-EPFK TO {frm}-VRS-EPFK IN {frm}-FLD-RED1.
           MOVE PGM-RPFK TO {frm}-VRS-RPFK IN {frm}-FLD-RED1.
           IF PGM-SOFL NOT > SPACES
               MOVE {frm}-VRS-LIOF IN {frm}-FLD-VARI TO
                    {frm}-VRS-SOFL IN {frm}-FLD-RED1
           ELSE
               MOVE PGM-SOFL TO {frm}-VRS-SOFL IN {frm}-FLD-RED1.
           MOVE PGM-HMSG TO {frm}-VRS-HMSG IN {frm}-FLD-RED1.
           MOVE PGM-LFLG TO {frm}-VRS-LFLG IN {frm}-FLD-RED1.
           MOVE PGM-FKY9 TO {frm}-VRS-FKY9 IN {frm}-FLD-RED1.
           MOVE PGM-FKY0 TO {frm}-VRS-FKY0 IN {frm}-FLD-RED1.
           MOVE PGM-TOLN TO {frm}-VRS-TOLN IN {frm}-FLD-RED1.
           MOVE PGM-MRLN TO {frm}-VRS-MRLN IN {frm}-FLD-RED1.
           MOVE PGM-MTLN TO {frm}-VRS-MTLN IN {frm}-FLD-RED1.
           MOVE PGM-DFEF TO {frm}-VRS-DFEF IN {frm}-FLD-RED1.
           IF NOT ( {frm}-VRS-RFBY IN {frm}-FLD-RED1 = '0' OR '1' )
               MOVE PGM-RFBY TO {frm}-VRS-RFBY IN {frm}-FLD-RED1.
       FIN-GNS-PUT-DCR.
           EXIT.

       GNS-ERR-IOF SECTION.
       INI-GNS-ERR-IOF.
            IF PGM-SUAR > SPACES
                MOVE PGM-SUAR TO {frm}-VRS-IFLD(1)
                MOVE -1       TO {frm}-VRS-IFLD-LEN(1)
                GO TO FIN-GNS-ERR-IOF.
      *
           MOVE 0 TO FRM-IFLD.
       LUP-GNS-ERR-IOF.
           ADD 1 TO FRM-IFLD.
           IF FRM-IFLD > {frm}-VRS-INOU
               MOVE 0 TO FRM-IFLD
               GO TO FIN-GNS-ERR-IOF.
           IF NOT ( {frm}-VRS-IFLD-LEN(FRM-IFLD) = -1 OR -9 )
               GO TO LUP-GNS-ERR-IOF.
       CON-GNS-ERR-IOF.
           MOVE SCR-SIST TO FIO-SIST.
           MOVE 'VRP'    TO TAB-COD-TTAB IN TAB.
           MOVE SCR-PROG TO PGM-PVRP.
           MOVE '/'      TO PGM-SVRP.
           MOVE 'IO'     TO PGM-FVRP.
           MOVE PGM-CTVR TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
               GO TO FIN-GNS-ERR-IOF.
           MOVE TAB-GLS-DESC IN TAB TO PGM-DESC.
           MOVE TAB-GLS-DATA IN TAB TO PGM-DATA.

           MOVE PGM-STRN TO UNS-STRN.
           MOVE ','      TO UNS-DLMT.
           MOVE FRM-IFLD TO PGM-IFML.
           SUBTRACT 1 FROM FRM-IFLD.
       LU2-GNS-ERR-IOF.
           MOVE FRM-IFLD TO UNS-NELE.
           ADD 1 TO FRM-IFLD.
           IF FRM-IFLD > {frm}-VRS-INOU
               GO TO FIN-GNS-ERR-IOF.
           PERFORM GNS-UNS-STR.
           MOVE FRM-CPIM-UBY TO {frm}-VRS-IFLD-ATR(FRM-IFLD).
           IF UNS-ELEM = UNS-DLMT
               MOVE SPACES   TO {frm}-VRS-IFLD(FRM-IFLD)
           ELSE
               MOVE UNS-ELEM TO {frm}-VRS-IFLD(FRM-IFLD)
               IF ( UNS-ELEM = 'E' ) AND ( FRM-IFLD > PGM-IFML )
                   MOVE 0 TO PGM-IFML.
           IF PGM-IFML = 0
               GO TO LU2-GNS-ERR-IOF.
*% INCLUDE FIN_LUP_ERR_IOF OF '{cda}.TXT'
*%! INCLUDE FIN_LUP_ERR_IOF OF '{cda}.GEN'
           GO TO LU2-GNS-ERR-IOF.
       FIN-GNS-ERR-IOF.
           EXIT.

       COPY GNSBGUNS.

