%!              BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
% TIPO_{TIPO} = TRUE
%! TIPO VRF
% IF TIPO_VRF THEN
% IF NOT TIENE_IDC_{RREG}_IDC_{NOMB} THEN
% IF TIDX > 0 THEN
% FOR I = 1 TO {TIDX}
           MOVE FRM-CPIM-UBY TO {RREG}-NUM-{NOMB}-ATR IN {FRM}-FLD({I}).
           MOVE FRM-CPIM-UBY TO {RREG}-VRF-{NOMB}-ATR IN {FRM}-FLD({I}).
% NEXT I
% ELSE
           MOVE FRM-CPIM-UBY TO {RREG}-NUM-{NOMB}-ATR IN {FRM}-FLD.
           MOVE FRM-CPIM-UBY TO {RREG}-VRF-{NOMB}-ATR IN {FRM}-FLD.
% END IF
% END IF
% ELSE
%! TIPO IDC
% IF TIPO_IDC THEN
      * IDC-INI --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
% IF TIDX > 0 THEN
% FOR I = 1 TO {TIDX}
           MOVE FRM-CPIM-UBY TO {RREG}-NUM-{NOMB}-ATR IN {FRM}-FLD({I}).
           MOVE FRM-CPIM-UBY TO {RREG}-VRF-{NOMB}-ATR IN {FRM}-FLD({I}).
           MOVE FRM-CPIM-UBY TO {RREG}-IND-{NOMB}-ATR IN {FRM}-FLD({I}).
           MOVE FRM-CPIM-UBY TO {RREG}-GLS-{NOMB}-ATR IN {FRM}-FLD({I}).
% NEXT I
% ELSE
           MOVE FRM-CPIM-UBY TO {RREG}-NUM-{NOMB}-ATR IN {FRM}-FLD.
           MOVE FRM-CPIM-UBY TO {RREG}-VRF-{NOMB}-ATR IN {FRM}-FLD.
           MOVE FRM-CPIM-UBY TO {RREG}-IND-{NOMB}-ATR IN {FRM}-FLD.
           MOVE FRM-CPIM-UBY TO {RREG}-GLS-{NOMB}-ATR IN {FRM}-FLD.
% END IF
      * IDC-FIN --> BMCHIBCI006.SRCAIU UNP ( KEY Y FLD )
% ELSE
%! TIPO FEC Y SUS DERIVADOS
% IF TIPO_FEC OR TIPO_DMA OR TIPO_AMD OR TIPO_MSA OR TIPO_SAM OR TIPO_FMA OR TIPO_FAM OR TIPO_FSA OR TIPO_FAS OR TIPO_FDM OR TIPO_FMD OR TIPO_FFA THEN
% IF TIDX > 0 THEN
% FOR I = 1 TO {TIDX}
           MOVE FRM-CPIM-UBY TO {CAMP}-ATR IN {FRM}-FLD({I}).
% NEXT I
% ELSE
           MOVE FRM-CPIM-UBY TO {CAMP}-ATR IN {FRM}-FLD.
% END IF
% END IF
% END IF
% END IF
% TIPO_{TIPO} = FALSE
