           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-IND-RRPT-LEN IN JCL-FLD
                GO TO FIN-VAL-FLD-JCL.
           PERFORM VAL-FRM-COD-DEST.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-COD-DEST-LEN IN JCL-FLD
                GO TO FIN-VAL-FLD-JCL.
           PERFORM VAL-FRM-COD-CLSE.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-COD-CLSE-LEN IN JCL-FLD
                GO TO FIN-VAL-FLD-JCL.
           PERFORM VAL-FRM-COD-FORM.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-COD-FORM-LEN IN JCL-FLD
                GO TO FIN-VAL-FLD-JCL.
           PERFORM VAL-FRM-GLS-DEST.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-DEST-LEN IN JCL-FLD
                GO TO FIN-VAL-FLD-JCL.
           PERFORM VAL-FRM-GLS-CLSE.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-CLSE-LEN IN JCL-FLD
                GO TO FIN-VAL-FLD-JCL.
           PERFORM VAL-FRM-GLS-FORM.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-FORM-LEN IN JCL-FLD
                GO TO FIN-VAL-FLD-JCL.
           PERFORM VAL-FRM-CIC-IGIA.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-CIC-IGIA-LEN IN JCL-FLD
                GO TO FIN-VAL-FLD-JCL.
           PERFORM VAL-FRM-CIC-IDEL.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-CIC-IDEL-LEN IN JCL-FLD
                GO TO FIN-VAL-FLD-JCL.
           PERFORM VAL-FRM-CIC-RRPT.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-CIC-RRPT-LEN IN JCL-FLD
                GO TO FIN-VAL-FLD-JCL.
           PERFORM VAL-FRM-CIC-DEST.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-CIC-DEST-LEN IN JCL-FLD
                GO TO FIN-VAL-FLD-JCL.
           PERFORM VAL-FRM-CIC-CLSE.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-CIC-CLSE-LEN IN JCL-FLD
                GO TO FIN-VAL-FLD-JCL.
           PERFORM VAL-FRM-CIC-FORM.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-CIC-FORM-LEN IN JCL-FLD
                GO TO FIN-VAL-FLD-JCL.
           PERFORM VAL-FRM-GLS-NRPT.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-NRPT-LEN IN JCL-FLD
                GO TO FIN-VAL-FLD-JCL.
           PERFORM VAL-FRM-IND-ILEE
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 20
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE -1 TO FRM-IND-ILEE-LEN IN JCL-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-JCL.
           PERFORM VAL-FRM-IND-PLNT
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 8
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE -1 TO FRM-IND-PLNT-LEN IN JCL-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-JCL.
           PERFORM VAL-FRM-GLS-DATO
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 20
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE -1 TO FRM-GLS-DATO-LEN IN JCL-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-JCL.
           PERFORM VAL-FRM-DAT-DATO
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 20
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE -1 TO FRM-DAT-DATO-LEN IN JCL-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-JCL.
       FIN-VAL-FLD-JCL.
           EXIT.
       
       VAL-BAT-COD-PROC SECTION.
       INI-VAL-BAT-COD-PROC.
           IF BAT-COD-PROC IN JCL-FLD NOT > SPACES
              MOVE SPACES TO BAT-COD-PROC IN JCL-FLD.
      *<<<< BAT_COD_PROC
      *>>>>
       FIN-VAL-BAT-COD-PROC.
