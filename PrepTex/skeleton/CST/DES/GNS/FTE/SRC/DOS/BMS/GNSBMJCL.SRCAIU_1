      *<<<< FRM_NUM_NEJM
           IF FRM-IND-PLNT IN JCL-FLD(4) = 'N'
               GO TO FIN-VAL-FRM-NUM-NEJM.
           IF FRM-NUM-NEJM IN JCL-FLD NOT > ZEROES AND
              FRM-IND-PLNT IN JCL-FLD( 4 ) = 'O'
               PERFORM ERR-GLS.
      *>>>>
       FIN-VAL-FRM-NUM-NEJM.
           EXIT.
       
       VAL-FRM-IND-IDEL SECTION.
       INI-VAL-FRM-IND-IDEL.
           IF FRM-IND-IDEL IN JCL-FLD NOT > SPACES
              MOVE SPACES TO FRM-IND-IDEL IN JCL-FLD.
      *<<<< FRM_IND_IDEL
           IF FRM-IND-PLNT IN JCL-FLD(5) = 'N'
               GO TO FIN-VAL-FRM-IND-IDEL.
           IF FRM-IND-IDEL IN JCL-FLD NOT > SPACES AND
              FRM-IND-PLNT IN JCL-FLD( 5 ) = 'O'
               PERFORM ERR-GLS
               GO TO FIN-VAL-FRM-IND-IDEL.
      *VAL-COD Valida codigo de tabla
           MOVE 'GNS' TO TAB-COD-SIST.
           MOVE 'IDL' TO TAB-COD-TTAB IN TAB.
           MOVE FRM-IND-IDEL IN JCL-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT = 'COD    NEX' AND
              FRM-IND-IDEL IN JCL-FLD > SPACES
               MOVE TAB-CIC-CTAB IN TAB TO FRM-CIC-IDEL IN JCL-FLD
           ELSE
               MOVE SPACES TO FRM-CIC-IDEL IN JCL-FLD.
      *>>>>
       FIN-VAL-FRM-IND-IDEL.
           EXIT.
       
       VAL-FRM-IND-RRPT SECTION.
       INI-VAL-FRM-IND-RRPT.
           IF FRM-IND-RRPT IN JCL-FLD NOT > SPACES
              MOVE SPACES TO FRM-IND-RRPT IN JCL-FLD.
      *<<<< FRM_IND_RRPT
      *VAL-COD Valida codigo de tabla
           MOVE 'GNS' TO TAB-COD-SIST.
           MOVE 'RRP' TO TAB-COD-TTAB IN TAB.
           MOVE FRM-IND-RRPT IN JCL-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT = 'COD    NEX' AND
              FRM-IND-RRPT IN JCL-FLD > SPACES
               MOVE TAB-CIC-CTAB IN TAB TO FRM-CIC-RRPT IN JCL-FLD
           ELSE
               MOVE SPACES TO FRM-CIC-RRPT IN JCL-FLD.
      *>>>>
       FIN-VAL-FRM-IND-RRPT.
           EXIT.
       
       VAL-FRM-COD-DEST SECTION.
       INI-VAL-FRM-COD-DEST.
           IF FRM-COD-DEST IN JCL-FLD NOT > SPACES
              MOVE SPACES TO FRM-COD-DEST IN JCL-FLD.
      *<<<< FRM_COD_DEST
           IF FRM-IND-PLNT IN JCL-FLD(6) = 'N'
               GO TO FIN-VAL-FRM-COD-DEST.
           IF FRM-COD-DEST IN JCL-FLD NOT > SPACES AND
              FRM-IND-PLNT IN JCL-FLD( 6 ) = 'O'
               PERFORM ERR-GLS
               GO TO FIN-VAL-FRM-COD-DEST.
           MOVE FRM-COD-DEST IN JCL-FLD TO BAT-COD-DEST IN BAT.
           IF BAT-COD-IDST NOT = 'O'
               MOVE 'GNS' TO TAB-COD-SIST
               MOVE 'DST' TO TAB-COD-TTAB IN TAB
               MOVE FRM-COD-DEST IN JCL-FLD TO TAB-COD-CTAB IN TAB
               PERFORM VAL-COD
               IF MSG-COD-MENS NOT = 'COD    NEX' AND
                  FRM-COD-DEST IN JCL-FLD > SPACES
                   MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-DEST IN JCL-FLD
                   MOVE TAB-CIC-CTAB IN TAB TO FRM-CIC-DEST IN JCL-FLD
                   GO TO FIN-VAL-FRM-COD-DEST
               ELSE
                   MOVE SPACES TO FRM-GLS-DEST IN JCL-FLD
                   MOVE SPACES TO FRM-CIC-DEST IN JCL-FLD
                   GO TO FIN-VAL-FRM-COD-DEST.

           MOVE 'GNS'  TO TAB-COD-SIST FIO-SIST.
           MOVE 'DST'  TO TAB-COD-TTAB IN TAB.
           MOVE SPACES TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT ( FIO-STAT-OKS AND TAB-COD-DAT1 IN TAB( 1 ) = 'O' )
               MOVE 'GNS' TO TAB-COD-SIST
               MOVE 'DST' TO TAB-COD-TTAB IN TAB
               MOVE FRM-COD-DEST IN JCL-FLD TO TAB-COD-CTAB IN TAB
               PERFORM VAL-COD
               IF MSG-COD-MENS NOT = 'COD    NEX' AND
                  FRM-COD-DEST IN JCL-FLD > SPACES
                   MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-DEST IN JCL-FLD
                   MOVE TAB-CIC-CTAB IN TAB TO FRM-CIC-DEST IN JCL-FLD
                   GO TO FIN-VAL-FRM-COD-DEST
               ELSE
                   MOVE SPACES TO FRM-GLS-DEST IN JCL-FLD
                   MOVE SPACES TO FRM-CIC-DEST IN JCL-FLD
                   GO TO FIN-VAL-FRM-COD-DEST.

      *VAL-OFI Valida codigo de Oficina en TABLAS
           MOVE BAT-COD-OFIC IN BAT TO OFI-COD-OFIC IN OFI.
           PERFORM VAL-OFI.
           IF MSG-COD-MENS NOT = SPACES
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
           IF MSG-COD-MENS NOT = 'COD    NEX'
               MOVE OFI-GLS-DESC IN OFI TO FRM-GLS-DEST IN JCL-FLD
               MOVE OFI-CIC-OFIC IN OFI TO FRM-CIC-DEST IN JCL-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-DEST IN JCL-FLD
               MOVE SPACES TO FRM-CIC-DEST IN JCL-FLD.
      *>>>>
       FIN-VAL-FRM-COD-DEST.
           EXIT.
       
       VAL-FRM-COD-CLSE SECTION.
       INI-VAL-FRM-COD-CLSE.
           IF FRM-COD-CLSE IN JCL-FLD NOT > SPACES
              MOVE SPACES TO FRM-COD-CLSE IN JCL-FLD.
      *<<<< FRM_COD_CLSE
           IF FRM-IND-PLNT IN JCL-FLD(7) = 'N'
               GO TO FIN-VAL-FRM-COD-CLSE.
           IF FRM-COD-CLSE IN JCL-FLD NOT > SPACES AND
              FRM-IND-PLNT IN JCL-FLD( 7 ) = 'O'
               PERFORM ERR-GLS
               GO TO FIN-VAL-FRM-COD-CLSE.
      *VAL-COD Valida codigo de tabla
           MOVE 'GNS' TO TAB-COD-SIST.
           MOVE 'CLS' TO TAB-COD-TTAB IN TAB.
           MOVE FRM-COD-CLSE IN JCL-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT = 'COD    NEX' AND
              FRM-COD-CLSE IN JCL-FLD > SPACES
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-CLSE IN JCL-FLD
               MOVE TAB-CIC-CTAB IN TAB TO FRM-CIC-CLSE IN JCL-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-CLSE IN JCL-FLD
               MOVE SPACES TO FRM-CIC-CLSE IN JCL-FLD.
      *>>>>
       FIN-VAL-FRM-COD-CLSE.
           EXIT.
       
       VAL-FRM-COD-FORM SECTION.
       INI-VAL-FRM-COD-FORM.
           IF FRM-COD-FORM IN JCL-FLD NOT > SPACES
