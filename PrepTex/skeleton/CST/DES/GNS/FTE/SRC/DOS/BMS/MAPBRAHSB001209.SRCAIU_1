%!              MAPBRAHSB001.SRCAIU
%!* {TIPO}-INI --> MAPBRAHSB001.SRCAIU
% TIPO_{TIPO} = TRUE
% ECHO = ",NORM"
% DISP = DONLY
% IF DISP THEN
% BRT = ",BRT"
% ELSE
% BRT = ""
% END
% IF KEY THEN
% DELI = "{"
% DELD = "}"
% ELSE
% DELI = "<"
% DELD = ">"
% END IF
% IF DARK THEN
% ECHO = ",DRK"
% ELSE
% ECHO = ""
% END IF
%! VRF
% IF TIPO_VRF THEN
% IF NOT DISP THEN
         DFHMDF POS=({FIL},{COLMENOS2}),ATTRB=(ASKIP,NORM{ECHO}),LENGTH=1,INITIAL='{DELI}'
% END IF
NUM{TIDC}  DFHMDF POS=({FIL},{COL}),ATTRB=(ASKIP{ECHO},NORM,FSET),LENGTH=12
         DFHMDF POS=({FIL},{COLMAS13}),ATTRB=(PROT,BRT{ECHO}),LENGTH=1,INITIAL='-'
VRF{TIDC}  DFHMDF POS=({FIL},{COLMAS15}),ATTRB=(ASKIP,NORM{ECHO},FSET),LENGTH=2
% IF NOT DISP THEN
         DFHMDF POS=({FIL},{COLMAS18}),ATTRB=(PROT),LENGTH=1,INITIAL='{DELD}'
% END IF
% ELSE
%! IDC
% IF TIPO_IDC THEN
% IF NOT DISP THEN
         DFHMDF POS=({FIL},{COLMAS3}),ATTRB=(ASKIP,NORM{ECHO}),LENGTH=1,INITIAL='{DELI}'
% END IF
IND{TIDC}  DFHMDF POS=({FIL},{COLMAS5}),ATTRB=(ASKIP,NORM{ECHO},FSET),LENGTH=1
         DFHMDF POS=({FIL},{COLMAS7}),ATTRB=(PROT{ECHO}),LENGTH=1,INITIAL='-'
GLS{TIDC}  DFHMDF POS=({FIL},{COLMAS9}),ATTRB=(ASKIP,NORM,FSET),LENGTH=3
% IF NOT DISP THEN
         DFHMDF POS=({FIL},{COLMAS13}),ATTRB=(PROT),LENGTH=1,INITIAL='{DELD}'
% END IF
% ELSE
%! FEC
% IF TIPO_FEC OR TIPO_DMA OR TIPO_MSA OR TIPO_FMA OR TIPO_FSA OR TIPO_FDM OR TIPO_FFA THEN
% IF NOT DISP THEN
         DFHMDF POS=({FIL},{COLMENOS2}),ATTRB=(ASKIP,NORM),LENGTH=1,INITIAL='{DELI}'
% END IF
{CORR}  DFHMDF POS=({FIL},{COLMAS2}),ATTRB=(ASKIP,NORM{ECHO}{BRT},FSET),LENGTH={LARG}
% IF NOT DISP THEN
         DFHMDF POS=({FIL},{COLMAS11}),ATTRB=(PROT),LENGTH=1,INITIAL='{DELD}'
% END IF
% ELSE
% IF TIPO_OFI THEN
% IF NOT DISP THEN
         DFHMDF POS=({FIL},{COLMENOS2}),ATTRB=(ASKIP,NORM{ECHO}),LENGTH=1,INITIAL='{DELI}'
% END IF
COD{TIDC}  DFHMDF POS=({FIL},{COL}),ATTRB=(ASKIP{ECHO},NORM,FSET),LENGTH=6
% IF NOT DISP THEN
         DFHMDF POS=({FIL},{COLMAS7}),ATTRB=(PROT),LENGTH=1,INITIAL='{DELD}'
% END IF
% ELSE
% IF TIPO_IDG THEN
% IF NOT DISP THEN
         DFHMDF POS=({FIL},{COLMENOS2}),ATTRB=(ASKIP,NORM{ECHO}),LENGTH=1,INITIAL='{DELI}'
TIP{NOMB}  DFHMDF POS=({FIL},{COL}),ATTRB=(ASKIP{ECHO},NORM,FSET),LENGTH=3
         DFHMDF POS=({FIL},{COLMAS4}),ATTRB=(PROT),LENGTH=1,INITIAL='{DELD}'
% ELSE
TIP{NOMB}  DFHMDF POS=({FIL},{COL}),ATTRB=(ASKIP{ECHO},NORM,FSET),LENGTH=3
% END IF
% IF NOT DISP THEN
         DFHMDF POS=({FIL},{COLMAS6}),ATTRB=(ASKIP,NORM{ECHO}),LENGTH=1,INITIAL='{DELI}'
STP{NOMB}  DFHMDF POS=({FIL},{COLMAS8}),ATTRB=(ASKIP{ECHO},NORM,FSET),LENGTH=3
         DFHMDF POS=({FIL},{COLMAS12}),ATTRB=(PROT),LENGTH=1,INITIAL='{DELD}'
% ELSE
STP{NOMB}  DFHMDF POS=({FIL},{COLMAS5}),ATTRB=(ASKIP{ECHO},NORM,FSET),LENGTH=3
% END IF
% END IF
% END IF
% END IF
% END IF
