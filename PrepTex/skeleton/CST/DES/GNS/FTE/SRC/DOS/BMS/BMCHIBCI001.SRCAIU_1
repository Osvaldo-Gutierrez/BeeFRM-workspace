%!              BMCHIBCI001.SRCAIU
% TIPO_{TIPO} = TRUE
%! TIPO VRF
%!
% IF TIPO_VRF THEN
% IF NOT TIENE_IDC_{RREG}_IDC_{NOMB} THEN
      * VRF-INI --> BMCHIBCI001.SRCAIU 
% IF TIDX > 0 THEN
% FOR I = 1 TO {TIDX}
           MOVE {RREG}-NUM-{NOMB} IN {RFRM}({I}) TO {FFRM}-NUM-{NOMB} IN {FRM}-FLD({I}).
           MOVE {RREG}-VRF-{NOMB} IN {RFRM}({I}) TO {RFRM}-VRF-{NOMB} IN {FRM}-FLD({I}).
% NEXT I
% ELSE
           MOVE {RREG}-NUM-{NOMB} IN {RFRM} TO {RFRM}-NUM-{NOMB} IN {FRM}-FLD.
           MOVE {RREG}-VRF-{NOMB} IN {RFRM} TO {RFRM}-VRF-{NOMB} IN {FRM}-FLD.
% END IF
      * VRF-FIN --> BMCHIBCI001.SRCAIU 
% END IF
% ELSE
%! TIPO IDC
%!
% IF TIPO_IDC THEN
      * IDC-INI --> BMCHIBCI001.SRCAIU 
% IF TIDX > 0 THEN
% FOR I = 1 TO {TIDX}
           MOVE {RREG}-NUM-{NOMB} IN {RFRM}({I}) TO {FFRM}-NUM-{NOMB} IN {FRM}-FLD({I}).
           MOVE {RREG}-VRF-{NOMB} IN {RFRM}({I}) TO {RFRM}-VRF-{NOMB} IN {FRM}-FLD({I}).
           MOVE {RREG}-IND-{NOMB} IN {RFRM}({I}) TO {FFRM}-IND-{NOMB} IN {FRM}-FLD({I}).
           MOVE {RREG}-GLS-{NOMB} IN {RFRM}({I}) TO {RFRM}-GLS-{NOMB} IN {FRM}-FLD({I}).
% NEXT I
% ELSE
           MOVE {RREG}-NUM-{NOMB} IN {RFRM} TO {RFRM}-NUM-{NOMB} IN {FRM}-FLD.
           MOVE {RREG}-VRF-{NOMB} IN {RFRM} TO {RFRM}-VRF-{NOMB} IN {FRM}-FLD.
           MOVE {RREG}-IND-{NOMB} IN {RFRM} TO {RFRM}-IND-{NOMB} IN {FRM}-FLD.
           MOVE {RREG}-GLS-{NOMB} IN {RFRM} TO {RFRM}-GLS-{NOMB} IN {FRM}-FLD.
% END IF
      * IDC-FIN --> BMCHIBCI001.SRCAIU 
% ELSE
%! TIPO FEC Y SUS DERIVADOS
%! EL TIPO FEC TIENE UN TRATAMIENTO ESPECIFICO, PUES SU APARICION ES MUCHO MAS FRECUENTE QUE EL RESTO
% IF TIPO_FEC
      * FEC-INI --> BMCHIBCI001.SRCAIU
% IF TIDX > 0 THEN
% FOR I = 1 TO {TIDX}
% IF NORMA_CST THEN
           MOVE {RREG}-NUM-D{PFEC} IN {RFRM}({I}) TO {RFRM}-NUM-D{PFEC} IN {FRM}-FLD({I}).
           MOVE {RREG}-NUM-M{PFEC} IN {RFRM}({I}) TO {RFRM}-NUM-M{PFEC} IN {FRM}-FLD({I}).
           MOVE {RREG}-NUM-S{PFEC} IN {RFRM}({I}) TO {RFRM}-NUM-S{PFEC} IN {FRM}-FLD({I}).
           MOVE {RREG}-NUM-A{PFEC} IN {RFRM}({I}) TO {RFRM}-NUM-A{PFEC} IN {FRM}-FLD({I}).
% ELSE
            MOVE {RREG}-FD-{NOMB} IN {RFRM}({I}) TO {RFRM}-FD-{NOMB} IN {FRM}-FLD({I}).
            MOVE {RREG}-FM-{NOMB} IN {RFRM}({I}) TO {RFRM}-FM-{NOMB} IN {FRM}-FLD({I}).
            MOVE {RREG}-FA-{NOMB} IN {RFRM}({I}) TO {RFRM}-FA-{NOMB} IN {FRM}-FLD({I}).
% END IF
% NEXT I
% ELSE
% IF NORMA_CST THEN
           MOVE {RREG}-NUM-D{PFEC} IN {RFRM} TO {RFRM}-NUM-D{PFEC} IN {FRM}-FLD.
           MOVE {RREG}-NUM-M{PFEC} IN {RFRM} TO {RFRM}-NUM-M{PFEC} IN {FRM}-FLD.
           MOVE {RREG}-NUM-S{PFEC} IN {RFRM} TO {RFRM}-NUM-S{PFEC} IN {FRM}-FLD.
           MOVE {RREG}-NUM-A{PFEC} IN {RFRM} TO {RFRM}-NUM-A{PFEC} IN {FRM}-FLD.
% ELSE
           MOVE {RREG}-FD-{NOMB} IN {RFRM} TO {RFRM}-FD-{NOMB} IN {FRM}-FLD.
           MOVE {RREG}-FM-{NOMB} IN {RFRM} TO {RFRM}-FM-{NOMB} IN {FRM}-FLD.
           MOVE {RREG}-FA-{NOMB} IN {RFRM} TO {RFRM}-FA-{NOMB} IN {FRM}-FLD.
% END IF
% END IF
      * FEC-FIN --> BMCHIBCI001.SRCAIU 
% ELSE
% IF TIPO_DMA OR TIPO_AMD OR TIPO_MSA OR TIPO_SAM OR TIPO_FMA OR TIPO_FAM OR TIPO_FSA OR TIPO_FAS OR TIPO_FDM OR TIPO_FMD OR TIPO_FFA THEN
