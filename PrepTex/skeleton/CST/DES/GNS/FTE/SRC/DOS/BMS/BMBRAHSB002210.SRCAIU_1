%! TIPO VRF
%!
% IF TIPO_VRF THEN
% IF NOT TIENE_IDC_{RREG}_IDC_{NOMB} THEN
      * VRF-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
% IF TIDX > 0 THEN
% FOR I = 1 TO {TIDX}
           MOVE {FFRM}-NUM-{NOMB} IN {FRM}-FLD({I}) TO {RREG}-NUM-{NOMB} IN {RFRM}({I}).
           MOVE {RFRM}-VRF-{NOMB} IN {FRM}-FLD({I}) TO {RREG}-VRF-{NOMB} IN {RFRM}({I}).
% NEXT I
% ELSE
           MOVE {RFRM}-NUM-{NOMB} IN {FRM}-FLD TO {RREG}-NUM-{NOMB} IN {RFRM}.
           MOVE {RFRM}-VRF-{NOMB} IN {FRM}-FLD TO {RREG}-VRF-{NOMB} IN {RFRM}.
% END IF
      * VRF-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
% END IF
% ELSE
%! TIPO IDC
%!
% IF TIPO_IDC THEN
      * IDC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
% IF TIDX > 0 THEN
% FOR I = 1 TO {TIDX}
           MOVE {FFRM}-NUM-{NOMB} IN {FRM}-FLD({I}) TO {RREG}-NUM-{NOMB} IN {RFRM}({I}).
           MOVE {RFRM}-VRF-{NOMB} IN {FRM}-FLD({I}) TO {RREG}-VRF-{NOMB} IN {RFRM}({I}).
           MOVE {FFRM}-IND-{NOMB} IN {FRM}-FLD({I}) TO {RREG}-IND-{NOMB} IN {RFRM}({I}).
           MOVE {RFRM}-GLS-{NOMB} IN {FRM}-FLD({I}) TO {RREG}-GLS-{NOMB} IN {RFRM}({I}).
% NEXT I
% ELSE
           MOVE {RFRM}-NUM-{NOMB} IN {FRM}-FLD TO {RREG}-NUM-{NOMB} IN {RFRM}.
           MOVE {RFRM}-VRF-{NOMB} IN {FRM}-FLD TO {RREG}-VRF-{NOMB} IN {RFRM}.
           MOVE {RFRM}-IND-{NOMB} IN {FRM}-FLD TO {RREG}-IND-{NOMB} IN {RFRM}.
           MOVE {RFRM}-GLS-{NOMB} IN {FRM}-FLD TO {RREG}-GLS-{NOMB} IN {RFRM}.
% END IF
      * IDC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
% ELSE
%! TIPO FEC Y SUS DERIVADOS
%! EL TIPO FEC TIENE UN TRATAMIENTO ESPECIFICO, PUES SU APARICION ES MUCHO MAS FRECUENTE QUE EL RESTO
