*%!              WIBRAHSB005.SRCAIU
      * WIBRAHSB005.SRCAIU
*%! PARA TIPO FEC
*%!
*%!     SIGLA    FORMATO             D M S A
*%!     FEC      D M S A             X X X X
*%!     DMA      D M A               X X   X
*%!     AMD      D M A               X X   X
*%!     MSA      M S A                 X X X
*%!     SAM      M S A                 X X X
*%!     FMA      M A                   X   X
*%!     FAM      M A                   X   X
*%!     FSA      S A                     X X
*%!     FAS      S A                     X X
*%!     FDM      D M                 X X
*%!     FMD      D M                 X X
*%!     FFA      A                         X
*%!
*% TIPO_{TIPO} = TRUE
*% IF MSKIDC THEN
*% 	SVRF1 = "         "
*% 	SVRF2 = "             "
*% 	SVRFF = "                   "
*% ELSE
*% 	SVRF1 = "     "
*% 	SVRF2 = "         "
*% 	SVRFF = "                   "
*% END IF
*%!
*%! TIPO VRF
*%!
*% IF TIPO_VRF THEN
*% IF NOT TIENE_IDC_{RREG}_IDC_{NOMB} THEN

{PADR}{RREG}-NUM-{NOMB}{IDX}-LEN        COMP   PIC S9(04).
{PADR}{RREG}-NUM-{NOMB}{IDX}-ATR       PIC X(01).
{PADR}{RREG}-NUM-{NOMB}{IDX}           PIC X(12).

{PADR}{RREG}-VRF-{NOMB}{IDX}-LEN        COMP   PIC S9(04).
{PADR}{RREG}-VRF-{NOMB}{IDX}-ATR       PIC X(01).
{PADR}{RREG}-VRF-{NOMB}{IDX}           PIC X(02).
*% END IF
*% ELSE
*%! TIPO IDC
*%!
*% IF TIPO_IDC THEN
      * IDC-INI --> WIBRAHSB005.SRCAIU ( -IDC )
      *
{PADR}{RREG}-NUM-{NOMB}{IDX}-LEN        COMP   PIC S9(04).
{PADR}{RREG}-NUM-{NOMB}{IDX}-ATR       PIC X(01).
*% IF GEN_CLI_BNM
{PADR}{RREG}-IDC-{NOMB}{IDX}-ATR       
{SPAC} REDEFINES {RREG}-NUM-{NOMB}{IDX}-ATR PIC X(01).
*% END IF
{PADR}{RREG}-NUM-{NOMB}{IDX}           PIC X(12).
*% IF GEN_CLI_BNM
{PADR}{RREG}-IDC-{NOMB}{IDX}      REDEFINES
{SPAC}{RREG}-NUM-{NOMB}{IDX}          PIC X(12).
*% END IF

{PADR}{RREG}-VRF-{NOMB}{IDX}-LEN        COMP   PIC S9(04).
{PADR}{RREG}-VRF-{NOMB}{IDX}-ATR       PIC X(01).
{PADR}{RREG}-VRF-{NOMB}{IDX}           PIC X(02).

{PADR}{RREG}-IND-{NOMB}{IDX}-LEN        COMP   PIC S9(04).
{PADR}{RREG}-IND-{NOMB}{IDX}-ATR       PIC X(01).
{PADR}{RREG}-IND-{NOMB}{IDX}           PIC X(01).

{PADR}{RREG}-GLS-{NOMB}{IDX}-LEN        COMP   PIC S9(04).
{PADR}{RREG}-GLS-{NOMB}{IDX}-ATR       PIC X(01).
{PADR}{RREG}-GLS-{NOMB}{IDX}           PIC X(03).

      * IDC-FIN --> WIBRAHSB005.SRCAIU 
*% ELSE
*%! TIPO FEC
*%!
*% IF TIPO_FEC
      *    FECHA FEC BCI
{PADR}{CAMP}{IDX}.
{NIVFEC}{RFRM}-NUM-D{PFEC}{IDX}           PIC 9(02).
{NIVFEC}{RFRM}-NUM-M{PFEC}{IDX}           PIC 9(02).
{NIVFEC}{RFRM}-NUM-S{PFEC}{IDX}           PIC 9(02).
{NIVFEC}{RFRM}-NUM-A{PFEC}{IDX}           PIC 9(02).

*% ELSE
*% IF TIPO_DMA OR TIPO_AMD OR TIPO_MSA OR TIPO_SAM OR TIPO_FMA OR TIPO_FAM OR TIPO_FSA OR TIPO_FAS OR TIPO_FDM OR TIPO_FMD OR TIPO_FFA THEN
      *    FECHA FEC BCI
{PADR}{CAMP}{IDX}.
*% IF TIPO_DMA OR TIPO_AMD OR TIPO_FDM OR TIPO_FMD THEN
{NIVFEC}{RFRM}-NUM-D{PFEC}{IDX}                PIC 9(02).
*% END IF
*% IF NOT ( TIPO_FSA OR TIPO_FAS OR TIPO_FFA ) THEN
{NIVFEC}{RFRM}-NUM-M{PFEC}{IDX}                PIC 9(02).
*% END IF
*% IF TIPO_MSA OR TIPO_SAM OR TIPO_FSA OR TIPO_FAS THEN
{NIVFEC}{RFRM}-NUM-S{PFEC}{IDX}                PIC 9(02).
*% END IF
*% IF NOT ( TIPO_FDM OR TIPO_FMD ) THEN
{NIVFEC}{RFRM}-NUM-A{PFEC}{IDX}                PIC 9(02).
*% END IF


*% ELSE
*%! TIPO OFI
*%!
*% IF TIPO_OFI
      *    OFICINA BCI
{PADR}{RREG}-COD-{NOMB}{IDX}-LEN        COMP   PIC S9(04).
{PADR}{RREG}-COD-{NOMB}{IDX}-ATR               PIC X(01).
{PADR}{RREG}-COD-{NOMB}{IDX}                   PIC X(06).
*% ELSE
*%! TIPO IDG
*%!
*% IF TIPO_IDG
      *    TIPO Y SUBTIPO GARANTIA BCI
{PADR}{RREG}-TIP-{NOMB}{IDX}-LEN        COMP   PIC S9(04).
{PADR}{RREG}-TIP-{NOMB}{IDX}-ATR               PIC X(01).
{PADR}{RREG}-TIP-{NOMB}{IDX}                   PIC X(03).
      *
{PADR}{RREG}-STP-{NOMB}{IDX}-LEN        COMP   PIC S9(04).
{PADR}{RREG}-STP-{NOMB}{IDX}-ATR               PIC X(01).
{PADR}{RREG}-STP-{NOMB}{IDX}                   PIC X(03).
*% END IF
*% END IF
*% END IF
*% END IF
*% END IF
*% END IF
*% NORMA_CST = FALSE
*% TIPO_{TIPO} = FALSE
*% GEN_KEY_LAF = FALSE
