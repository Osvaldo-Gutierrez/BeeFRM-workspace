           MOVE 0 TO NUM-NDCM.
           CALL 'GNSPVNUM' USING NUM-VARI.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO DML-COD-IOF5 IN DBT-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO DML-COD-IOF5-LEN IN DBT-FLD
                GO TO FIN-VAL-NUM-IKY-DBT.
       FIN-VAL-NUM-IKY-DBT.
           EXIT.
       
       VAL-NUM-MKY-DBT SECTION.
       INI-VAL-NUM-MKY-DBT.
       FIN-VAL-NUM-MKY-DBT.
           EXIT.
       
       VAL-NUM-AKY-DBT SECTION.
       INI-VAL-NUM-AKY-DBT.
       FIN-VAL-NUM-AKY-DBT.
           EXIT.
       
       VAL-NUM-FLD-DBT SECTION.
       INI-VAL-NUM-FLD-DBT.
       FIN-VAL-NUM-FLD-DBT.
           EXIT.
       
       EDT-KEY-DBT SECTION.
       INI-EDT-KEY-DBT.
           PERFORM EDT-IKY-DBT.
           PERFORM EDT-MKY-DBT.
           PERFORM EDT-AKY-DBT.
       FIN-EDT-KEY-DBT.
           EXIT.
       
       EDT-IKY-DBT SECTION.
       INI-EDT-IKY-DBT.
           IF DML-COD-IOF1-LEN IN DBT-FLD = -9
                MOVE -1 TO DML-COD-IOF1-LEN IN DBT-FLD
                GO TO FIN-EDT-IKY-DBT.
           MOVE DML-COD-IOF1 IN DBT-FLD TO DML-COD-IOF1-EDT IN DBT-FLD.
           IF DML-COD-IOF2-LEN IN DBT-FLD = -9
                MOVE -1 TO DML-COD-IOF2-LEN IN DBT-FLD
                GO TO FIN-EDT-IKY-DBT.
           MOVE DML-COD-IOF2 IN DBT-FLD TO DML-COD-IOF2-EDT IN DBT-FLD.
           IF DML-COD-IOF3-LEN IN DBT-FLD = -9
                MOVE -1 TO DML-COD-IOF3-LEN IN DBT-FLD
                GO TO FIN-EDT-IKY-DBT.
           MOVE DML-COD-IOF3 IN DBT-FLD TO DML-COD-IOF3-EDT IN DBT-FLD.
           IF DML-COD-IOF4-LEN IN DBT-FLD = -9
                MOVE -1 TO DML-COD-IOF4-LEN IN DBT-FLD
                GO TO FIN-EDT-IKY-DBT.
           MOVE DML-COD-IOF4 IN DBT-FLD TO DML-COD-IOF4-EDT IN DBT-FLD.
           IF DML-COD-IOF5-LEN IN DBT-FLD = -9
                MOVE -1 TO DML-COD-IOF5-LEN IN DBT-FLD
                GO TO FIN-EDT-IKY-DBT.
           MOVE DML-COD-IOF5 IN DBT-FLD TO DML-COD-IOF5-EDT IN DBT-FLD.
       FIN-EDT-IKY-DBT.
           EXIT.
       
       EDT-MKY-DBT SECTION.
       INI-EDT-MKY-DBT.
       FIN-EDT-MKY-DBT.
           EXIT.
       
       EDT-AKY-DBT SECTION.
       INI-EDT-AKY-DBT.
       FIN-EDT-AKY-DBT.
           EXIT.
       
       EDT-FLD-DBT SECTION.
       INI-EDT-FLD-DBT.
       FIN-EDT-FLD-DBT.
           EXIT.
