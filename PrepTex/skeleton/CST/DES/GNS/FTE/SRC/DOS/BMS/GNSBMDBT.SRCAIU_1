       VAL-KEY-DBT SECTION.
       INI-VAL-KEY-DBT.
           PERFORM VAL-IKY-DBT.
           PERFORM VAL-MKY-DBT.
           PERFORM VAL-AKY-DBT.
       FIN-VAL-KEY-DBT.
           EXIT.
       
       VAL-IKY-DBT SECTION.
       INI-VAL-IKY-DBT.
           PERFORM VAL-DML-COD-IOF1.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DML-COD-IOF1-LEN IN DBT-FLD
                GO TO FIN-VAL-IKY-DBT.
           PERFORM VAL-DML-COD-IOF2.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DML-COD-IOF2-LEN IN DBT-FLD
                GO TO FIN-VAL-IKY-DBT.
           PERFORM VAL-DML-COD-IOF3.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DML-COD-IOF3-LEN IN DBT-FLD
                GO TO FIN-VAL-IKY-DBT.
           PERFORM VAL-DML-COD-IOF4.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DML-COD-IOF4-LEN IN DBT-FLD
                GO TO FIN-VAL-IKY-DBT.
           PERFORM VAL-DML-COD-IOF5.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DML-COD-IOF5-LEN IN DBT-FLD
                GO TO FIN-VAL-IKY-DBT.
           PERFORM VAL-DML-FEC-FDML.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO DML-FEC-FDML-LEN IN DBT-FLD
                GO TO FIN-VAL-IKY-DBT.
       FIN-VAL-IKY-DBT.
           EXIT.
       
       VAL-MKY-DBT SECTION.
       INI-VAL-MKY-DBT.
       FIN-VAL-MKY-DBT.
           EXIT.
       
       VAL-AKY-DBT SECTION.
       INI-VAL-AKY-DBT.
       FIN-VAL-AKY-DBT.
           EXIT.
       
       VAL-FLD-DBT SECTION.
       INI-VAL-FLD-DBT.
           PERFORM VAL-DML-VAL-OUFL
            VARYING FRM-IFLD FROM 1 BY 1 UNTIL FRM-IFLD > 10
                OR FRM-SUAR = FRM-SUAR-MAL
                OR FRM-SUAR = FRM-SUAR-CLR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                SUBTRACT 1 FROM FRM-IFLD
                MOVE -1 TO DML-VAL-OUFL-LEN IN DBT-FLD(FRM-IFLD)
                GO TO FIN-VAL-FLD-DBT.
       FIN-VAL-FLD-DBT.
           EXIT.
       
       VAL-DML-COD-IOF1 SECTION.
