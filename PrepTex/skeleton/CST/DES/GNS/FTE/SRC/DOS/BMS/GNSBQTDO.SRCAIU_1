      * 10-Aug-95 02:10 PM
       PUT-ALL-TDO SECTION.
       INI-PUT-ALL-TDO.
           PERFORM PUT-TDO-TDO.
           MOVE TDO-GLS-DESC IN TDO TO WSS-TDO.
           MOVE WSS-FRM-GLS-FL01 TO FRM-GLS-FL01.
           MOVE WSS-FRM-GLS-FL02 TO FRM-GLS-FL02.
           MOVE WSS-FRM-GLS-FL03 TO FRM-GLS-FL03.
           MOVE WSS-FRM-GLS-FL04 TO FRM-GLS-FL04.
           MOVE WSS-FRM-GLS-FL05 TO FRM-GLS-FL05.
           MOVE WSS-FRM-GLS-FL06 TO FRM-GLS-FL06.
           MOVE WSS-FRM-GLS-FL07 TO FRM-GLS-FL07.
           MOVE WSS-FRM-GLS-FL08 TO FRM-GLS-FL08.
           MOVE WSS-FRM-GLS-FL09 TO FRM-GLS-FL09.
           MOVE WSS-FRM-GLS-FL10 TO FRM-GLS-FL10.
           MOVE WSS-FRM-GLS-FL11 TO FRM-GLS-FL11.
           MOVE WSS-FRM-GLS-FL12 TO FRM-GLS-FL12.
           MOVE WSS-FRM-GLS-FL13 TO FRM-GLS-FL13.
           MOVE WSS-FRM-GLS-FL14 TO FRM-GLS-FL14.
           MOVE WSS-FRM-GLS-FL15 TO FRM-GLS-FL15.
           MOVE WSS-FRM-GLS-FL16 TO FRM-GLS-FL16.
           MOVE WSS-FRM-GLS-FL17 TO FRM-GLS-FL17.
       FIN-PUT-ALL-TDO.
           EXIT.
       
       PUT-TDO-TDO SECTION.
       INI-PUT-TDO-TDO.
           IF PGM-STAT-TDO-NUL
               GO TO FIN-PUT-TDO-TDO.
           IF NOT PGM-STAT-TDO-OKS
               PERFORM CLR-TDO-TDO
               GO TO FIN-PUT-TDO-TDO.
           MOVE TDO-NUM-RECO IN TDO TO TDO-NUM-RECO IN TDO-FLD.
           MOVE TDO-GLS-HDR IN TDO TO TDO-GLS-HDR IN TDO-FLD.
       FIN-PUT-TDO-TDO.
           EXIT.
       
       KEY-ALL-TDO SECTION.
       INI-KEY-ALL-TDO.
           PERFORM KEY-TDO-TDO.
       FIN-KEY-ALL-TDO.
           EXIT.
       
       KEY-TDO-TDO SECTION.
       INI-KEY-TDO-TDO.
           MOVE TDO-NUM-RECO IN TDO-FLD TO TDO-NUM-RECO IN TDO.
       FIN-KEY-TDO-TDO.
           EXIT.
       
       SCR-CLRA-TDO SECTION.
       INI-SCR-CLRA-TDO.
           PERFORM CLR-KEY-TDO.
           PERFORM CLR-FLD-TDO.
       FIN-SCR-CLRA-TDO.
           EXIT.
       
       CLR-KEY-TDO SECTION.
       INI-CLR-KEY-TDO.
           MOVE ZEROES TO TDO-NUM-RECO IN TDO-FLD.
       FIN-CLR-KEY-TDO.
           EXIT.
       
       CLR-FLD-TDO SECTION.
       INI-CLR-FLD-TDO.
           MOVE SPACES TO FRM-GLS-FL01 IN TDO-FLD.
           MOVE SPACES TO FRM-GLS-FL02 IN TDO-FLD.
           MOVE SPACES TO FRM-GLS-FL03 IN TDO-FLD.
           MOVE SPACES TO FRM-GLS-FL04 IN TDO-FLD.
           MOVE SPACES TO FRM-GLS-FL05 IN TDO-FLD.
           MOVE SPACES TO FRM-GLS-FL06 IN TDO-FLD.
           MOVE SPACES TO FRM-GLS-FL07 IN TDO-FLD.
           MOVE SPACES TO FRM-GLS-FL08 IN TDO-FLD.
           MOVE SPACES TO FRM-GLS-FL09 IN TDO-FLD.
           MOVE SPACES TO FRM-GLS-FL10 IN TDO-FLD.
           MOVE SPACES TO FRM-GLS-FL11 IN TDO-FLD.
           MOVE SPACES TO FRM-GLS-FL12 IN TDO-FLD.
           MOVE SPACES TO FRM-GLS-FL13 IN TDO-FLD.
           MOVE SPACES TO FRM-GLS-FL14 IN TDO-FLD.
           MOVE SPACES TO FRM-GLS-FL15 IN TDO-FLD.
           MOVE SPACES TO FRM-GLS-FL16 IN TDO-FLD.
           MOVE SPACES TO FRM-GLS-FL17 IN TDO-FLD.
           PERFORM CLR-TDO-TDO.
       FIN-CLR-FLD-TDO.
           EXIT.
       
       CLR-TDO-TDO SECTION.
       INI-CLR-TDO-TDO.
           MOVE SPACES TO TDO-GLS-HDR IN TDO-FLD.
       FIN-CLR-TDO-TDO.
           EXIT.
       
       FST-KEY-TDO SECTION.
       INI-FST-KEY-TDO.
           MOVE -1 TO TDO-NUM-RECO-LEN IN TDO-FLD.
       FIN-FST-KEY-TDO.
           EXIT.
       
       FST-FLD-TDO SECTION.
       INI-FST-FLD-TDO.
           MOVE -1 TO TDO-GLS-HDR-LEN IN TDO-FLD.
       FIN-FST-FLD-TDO.
           EXIT.
       
       PRO-KEY-TDO SECTION.
       INI-PRO-KEY-TDO.
           PERFORM PRO-IKY-TDO.
           PERFORM PRO-AKY-TDO.
           PERFORM PRO-MKY-TDO.
       FIN-PRO-KEY-TDO.
           EXIT.
       
       PRO-IKY-TDO SECTION.
       INI-PRO-IKY-TDO.
           MOVE FRM-CPIM-AEY TO TDO-NUM-RECO-ATR IN TDO-FLD.
       FIN-PRO-IKY-TDO.
           EXIT.
       
       PRO-MKY-TDO SECTION.
       INI-PRO-MKY-TDO.
       FIN-PRO-MKY-TDO.
           EXIT.
       
       PRO-AKY-TDO SECTION.
       INI-PRO-AKY-TDO.
       FIN-PRO-AKY-TDO.
           EXIT.
       
       UNP-KEY-TDO SECTION.
       INI-UNP-KEY-TDO.
           PERFORM UNP-IKY-TDO.
           PERFORM UNP-AKY-TDO.
           PERFORM UNP-MKY-TDO.
       FIN-UNP-KEY-TDO.
           EXIT.
       
       UNP-IKY-TDO SECTION.
       INI-UNP-IKY-TDO.
           MOVE FRM-CPIM-UBY TO TDO-NUM-RECO-ATR.
       FIN-UNP-IKY-TDO.
           EXIT.
       
       UNP-MKY-TDO SECTION.
       INI-UNP-MKY-TDO.
       FIN-UNP-MKY-TDO.
           EXIT.
       
       UNP-AKY-TDO SECTION.
       INI-UNP-AKY-TDO.
       FIN-UNP-AKY-TDO.
           EXIT.
       
       UNP-FLD-TDO SECTION.
       INI-UNP-FLD-TDO.
           MOVE FRM-CPIM-UBY TO FRM-GLS-FL01-ATR IN TDO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-FL02-ATR IN TDO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-FL03-ATR IN TDO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-FL04-ATR IN TDO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-FL05-ATR IN TDO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-FL06-ATR IN TDO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-FL07-ATR IN TDO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-FL08-ATR IN TDO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-FL09-ATR IN TDO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-FL10-ATR IN TDO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-FL11-ATR IN TDO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-FL12-ATR IN TDO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-FL13-ATR IN TDO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-FL14-ATR IN TDO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-FL15-ATR IN TDO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-FL16-ATR IN TDO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-GLS-FL17-ATR IN TDO-FLD.
           PERFORM UNP-TDO-TDO.
       FIN-UNP-FLD-TDO.
           EXIT.
       
       UNP-TDO-TDO SECTION.
       INI-UNP-TDO-TDO.
           MOVE FRM-CPIM-UBY TO TDO-GLS-HDR-ATR IN TDO-FLD.
       FIN-UNP-TDO-TDO.
           EXIT.
       
       VAL-KEY-TDO SECTION.
       INI-VAL-KEY-TDO.
           PERFORM VAL-IKY-TDO.
           PERFORM VAL-MKY-TDO.
           PERFORM VAL-AKY-TDO.
       FIN-VAL-KEY-TDO.
           EXIT.
       
       VAL-IKY-TDO SECTION.
       INI-VAL-IKY-TDO.
           PERFORM VAL-TDO-NUM-RECO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO TDO-NUM-RECO-LEN IN TDO-FLD
                GO TO FIN-VAL-IKY-TDO.
       FIN-VAL-IKY-TDO.
           EXIT.
       
       VAL-MKY-TDO SECTION.
       INI-VAL-MKY-TDO.
       FIN-VAL-MKY-TDO.
           EXIT.
       
       VAL-AKY-TDO SECTION.
       INI-VAL-AKY-TDO.
       FIN-VAL-AKY-TDO.
           EXIT.
       
       VAL-TDO-NUM-RECO SECTION.
       INI-VAL-TDO-NUM-RECO.
       FIN-VAL-TDO-NUM-RECO.
           EXIT.
       
       VAL-CON-KEY-TDO SECTION.
       INI-VAL-CON-KEY-TDO.
           IF     TDO-NUM-RECO IN TDO-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-TDO.
       FIN-VAL-CON-KEY-TDO.
           EXIT.
       
       VAL-CON-IKY-TDO SECTION.
       INI-VAL-CON-IKY-TDO.
           IF     TDO-NUM-RECO IN TDO-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-TDO.
       FIN-VAL-CON-IKY-TDO.
           EXIT.
       
       VAL-NUM-KEY-TDO SECTION.
       INI-VAL-NUM-KEY-TDO.
           PERFORM VAL-NUM-IKY-TDO.
           PERFORM VAL-NUM-MKY-TDO.
           PERFORM VAL-NUM-AKY-TDO.
       FIN-VAL-NUM-KEY-TDO.
           EXIT.
       
       VAL-NUM-IKY-TDO SECTION.
       INI-VAL-NUM-IKY-TDO.
       VAL-NUM-TDO-NUM-RECO.
           MOVE TDO-NUM-RECO-ALF IN TDO-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO TDO-NUM-RECO IN TDO-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO TDO-NUM-RECO-LEN IN TDO-FLD
                GO TO FIN-VAL-NUM-IKY-TDO.
       FIN-VAL-NUM-IKY-TDO.
           EXIT.
       
       VAL-NUM-MKY-TDO SECTION.
       INI-VAL-NUM-MKY-TDO.
       FIN-VAL-NUM-MKY-TDO.
           EXIT.
       
       VAL-NUM-AKY-TDO SECTION.
       INI-VAL-NUM-AKY-TDO.
       FIN-VAL-NUM-AKY-TDO.
           EXIT.
       
       VAL-NUM-FLD-TDO SECTION.
       INI-VAL-NUM-FLD-TDO.
       FIN-VAL-NUM-FLD-TDO.
           EXIT.
       
       EDT-KEY-TDO SECTION.
       INI-EDT-KEY-TDO.
           PERFORM EDT-IKY-TDO.
           PERFORM EDT-MKY-TDO.
           PERFORM EDT-AKY-TDO.
       FIN-EDT-KEY-TDO.
           EXIT.
       
       EDT-IKY-TDO SECTION.
       INI-EDT-IKY-TDO.
           IF TDO-NUM-RECO-LEN IN TDO-FLD = -9
                MOVE -1 TO TDO-NUM-RECO-LEN IN TDO-FLD
                GO TO FIN-EDT-IKY-TDO.
           MOVE TDO-NUM-RECO IN TDO-FLD TO TDO-NUM-RECO-EDT IN TDO-EDT.
           MOVE TDO-NUM-RECO-EDT IN TDO-EDT TO 
                TDO-NUM-RECO-ALF IN TDO-FLD.
       FIN-EDT-IKY-TDO.
           EXIT.
       
       EDT-MKY-TDO SECTION.
       INI-EDT-MKY-TDO.
       FIN-EDT-MKY-TDO.
           EXIT.
       
       EDT-AKY-TDO SECTION.
       INI-EDT-AKY-TDO.
       FIN-EDT-AKY-TDO.
           EXIT.
       
       EDT-FLD-TDO SECTION.
       INI-EDT-FLD-TDO.
       FIN-EDT-FLD-TDO.
           EXIT.
