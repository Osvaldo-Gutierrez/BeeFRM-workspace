      * 04-Mar-93 01:03 PM
       RET-ALL-USR SECTION.
       INI-RET-ALL-USR.
           PERFORM KEY-ALL-USR.
           PERFORM RET-USR-USR.
       FIN-RET-ALL-USR.
           EXIT.

       RET-USR-USR SECTION.
       INI-RET-USR-USR.
           MOVE USR-EXT-CTAB IN USR-FLD TO USR-EXT-CTAB IN USR.
           MOVE USR-GLS-DESC IN USR-FLD TO USR-GLS-DESC IN USR.
           MOVE USR-GLS-ABRV IN USR-FLD TO USR-GLS-ABRV IN USR.
           MOVE USR-GLS-DCOR IN USR-FLD TO USR-GLS-DCOR IN USR.
           MOVE USR-IND-VIGE IN USR-FLD TO USR-IND-VIGE IN USR.
           MOVE USR-NUM-DTRN IN USR-FLD TO USR-NUM-DTRN IN USR.
           MOVE USR-NUM-MTRN IN USR-FLD TO USR-NUM-MTRN IN USR.
           MOVE USR-NUM-STRN IN USR-FLD TO USR-NUM-STRN IN USR.
           MOVE USR-NUM-ATRN IN USR-FLD TO USR-NUM-ATRN IN USR.
       FIN-RET-USR-USR.
           EXIT.

       KEY-ALL-USR SECTION.
       INI-KEY-ALL-USR.
           PERFORM KEY-USR-USR.
       FIN-KEY-ALL-USR.
           EXIT.

       KEY-USR-USR SECTION.
       INI-KEY-USR-USR.
           MOVE USR-COD-TTAB IN USR-FLD TO USR-COD-TTAB IN USR.
           MOVE USR-COD-USER IN USR-FLD TO USR-COD-USER IN USR.
       FIN-KEY-USR-USR.
           EXIT.

       SCR-CLRA-USR SECTION.
       INI-SCR-CLRA-USR.
           PERFORM CLR-KEY-USR.
           PERFORM CLR-FLD-USR.
       FIN-SCR-CLRA-USR.
           EXIT.

       CLR-KEY-USR SECTION.
       INI-CLR-KEY-USR.
           MOVE SPACES TO USR-COD-TTAB IN USR-FLD.
           MOVE SPACES TO USR-COD-USER IN USR-FLD.
       FIN-CLR-KEY-USR.
           EXIT.

       CLR-FLD-USR SECTION.
       INI-CLR-FLD-USR.
           MOVE SPACES TO FRM-GLS-TTAB IN USR-FLD.
           PERFORM CLR-USR-USR.
       FIN-CLR-FLD-USR.
           EXIT.

       CLR-USR-USR SECTION.
       INI-CLR-USR-USR.
           MOVE SPACES TO USR-EXT-CTAB IN USR-FLD.
           MOVE SPACES TO USR-GLS-DESC IN USR-FLD.
           MOVE SPACES TO USR-GLS-ABRV IN USR-FLD.
           MOVE SPACES TO USR-GLS-DCOR IN USR-FLD.
           MOVE SPACES TO USR-IND-VIGE IN USR-FLD.
           MOVE SPACES TO USR-FEC-FTRN IN USR-FLD.
       FIN-CLR-USR-USR.
           EXIT.

       FST-KEY-USR SECTION.
       INI-FST-KEY-USR.
           MOVE -1 TO USR-COD-TTAB-LEN IN USR-FLD.
       FIN-FST-KEY-USR.
           EXIT.

       FST-FLD-USR SECTION.
       INI-FST-FLD-USR.
           MOVE -1 TO USR-EXT-CTAB-LEN IN USR-FLD.
       FIN-FST-FLD-USR.
           EXIT.

       PRO-KEY-USR SECTION.
       INI-PRO-KEY-USR.
           PERFORM PRO-MKY-USR.
       FIN-PRO-KEY-USR.
           EXIT.

       PRO-MKY-USR SECTION.
       INI-PRO-MKY-USR.
       FIN-PRO-MKY-USR.
           EXIT.

       PRO-FLD-USR SECTION.
       INI-PRO-FLD-USR.
           PERFORM PRO-USR-USR.
       FIN-PRO-FLD-USR.
           EXIT.

       PRO-USR-USR SECTION.
       INI-PRO-USR-USR.
           MOVE FRM-CPIM-AEY TO USR-EXT-CTAB-ATR IN USR-FLD.
           MOVE FRM-CPIM-AEY TO USR-GLS-DESC-ATR IN USR-FLD.
           MOVE FRM-CPIM-AEY TO USR-GLS-ABRV-ATR IN USR-FLD.
           MOVE FRM-CPIM-AEY TO USR-GLS-DCOR-ATR IN USR-FLD.
           MOVE FRM-CPIM-AEY TO USR-IND-VIGE-ATR IN USR-FLD.
       FIN-PRO-USR-USR.
           EXIT.

       UNP-KEY-USR SECTION.
       INI-UNP-KEY-USR.
           PERFORM UNP-IKY-USR.
           PERFORM UNP-AKY-USR.
           PERFORM UNP-MKY-USR.
       FIN-UNP-KEY-USR.
           EXIT.

       UNP-IKY-USR SECTION.
       INI-UNP-IKY-USR.
           MOVE FRM-CPIM-UBY TO USR-COD-TTAB-ATR.
           MOVE FRM-CPIM-UBY TO USR-COD-USER-ATR.
       FIN-UNP-IKY-USR.
           EXIT.

       UNP-MKY-USR SECTION.
       INI-UNP-MKY-USR.
       FIN-UNP-MKY-USR.
           EXIT.

       UNP-AKY-USR SECTION.
       INI-UNP-AKY-USR.
       FIN-UNP-AKY-USR.
           EXIT.

       UNP-FLD-USR SECTION.
       INI-UNP-FLD-USR.
           PERFORM UNP-USR-USR.
       FIN-UNP-FLD-USR.
           EXIT.

       UNP-USR-USR SECTION.
       INI-UNP-USR-USR.
           MOVE FRM-CPIM-UBY TO USR-EXT-CTAB-ATR IN USR-FLD.
           MOVE FRM-CPIM-UBY TO USR-GLS-DESC-ATR IN USR-FLD.
           MOVE FRM-CPIM-UBY TO USR-GLS-ABRV-ATR IN USR-FLD.
           MOVE FRM-CPIM-UBY TO USR-GLS-DCOR-ATR IN USR-FLD.
           MOVE FRM-CPIM-UBY TO USR-IND-VIGE-ATR IN USR-FLD.
       FIN-UNP-USR-USR.
           EXIT.

       VAL-MKY-USR SECTION.
       INI-VAL-MKY-USR.
       FIN-VAL-MKY-USR.
           EXIT.

       VAL-FLD-USR SECTION.
       INI-VAL-FLD-USR.
           PERFORM VAL-FRM-GLS-TTAB.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-TTAB-LEN IN USR-FLD
                GO TO FIN-VAL-FLD-USR.
           PERFORM VAL-USR-EXT-CTAB.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO USR-EXT-CTAB-LEN IN USR-FLD
                GO TO FIN-VAL-FLD-USR.
           PERFORM VAL-USR-GLS-DESC.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO USR-GLS-DESC-LEN IN USR-FLD
                GO TO FIN-VAL-FLD-USR.
           PERFORM VAL-USR-GLS-ABRV.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO USR-GLS-ABRV-LEN IN USR-FLD
                GO TO FIN-VAL-FLD-USR.
           PERFORM VAL-USR-GLS-DCOR.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO USR-GLS-DCOR-LEN IN USR-FLD
                GO TO FIN-VAL-FLD-USR.
           PERFORM VAL-USR-IND-VIGE.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO USR-IND-VIGE-LEN IN USR-FLD
                GO TO FIN-VAL-FLD-USR.
           PERFORM VAL-USR-FEC-FTRN.
           IF FRM-SUAR =  FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO USR-FEC-FTRN-LEN IN USR-FLD
                GO TO FIN-VAL-FLD-USR.
       FIN-VAL-FLD-USR.
           EXIT.

       VAL-FRM-GLS-TTAB SECTION.
       INI-VAL-FRM-GLS-TTAB.
       FIN-VAL-FRM-GLS-TTAB.
           EXIT.

       VAL-USR-EXT-CTAB SECTION.
       INI-VAL-USR-EXT-CTAB.
           IF USR-EXT-CTAB IN USR-FLD NOT > SPACES
              MOVE SPACES TO USR-EXT-CTAB IN USR-FLD.
       FIN-VAL-USR-EXT-CTAB.
           EXIT.

       VAL-USR-GLS-DESC SECTION.
       INI-VAL-USR-GLS-DESC.
           IF USR-GLS-DESC IN USR-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-USR-GLS-DESC.
       FIN-VAL-USR-GLS-DESC.
           EXIT.

       VAL-USR-GLS-ABRV SECTION.
       INI-VAL-USR-GLS-ABRV.
           IF USR-GLS-ABRV IN USR-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-USR-GLS-ABRV.
       FIN-VAL-USR-GLS-ABRV.
           EXIT.

