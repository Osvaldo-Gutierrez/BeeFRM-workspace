%!              BMBRAHSB004.SRCAIU FST-KEY Y FST-FLD
% TIPO_{TIPO} = TRUE
%! VRF
% IF TIPO_VRF THEN
% IF NOT TIENE_IDC_{RREG}_IDC_{NOMB} THEN
      * VRF-INI --> BMBRAHSB004.SRCAIU ( FST-KEY Y FST-FLD )
% IF TIDX > 0 THEN
           MOVE FRM-POS-CURS TO {RREG}-NUM-{NOMB}-LEN IN {FRM}-FLD(1).
% ELSE
           MOVE FRM-POS-CURS TO {RREG}-NUM-{NOMB}-LEN IN {FRM}-FLD.
% END IF
% ELSE
% IF TIDX > 0 THEN
           MOVE FRM-POS-CURS TO {RREG}-NUM-{NOMB}-LEN IN {FRM}-FLD(1).
% ELSE
           MOVE FRM-POS-CURS TO {RREG}-NUM-{NOMB}-LEN IN {FRM}-FLD.
% END IF
      * VRF-FIN --> BMBRAHSB004.SRCAIU ( FST-KEY Y FST-FLD )
% END IF
% END IF
%! NO EXISTE TIPO IDC COMO FST-KEY O FST-FLD
%! FEC
% IF TIPO_FEC OR TIPO_DMA OR TIPO_AMD OR TIPO_MSA OR TIPO_SAM OR TIPO_FMA OR TIPO_FAM OR TIPO_FSA OR TIPO_FAS OR TIPO_FDM OR TIPO_FMD OR TIPO_FFA THEN
% IF TIDX > 0 THEN
           MOVE FRM-POS-CURS TO {CAMP}-LEN IN {FRM}-FLD(1).
% ELSE
           MOVE FRM-POS-CURS TO {CAMP}-LEN IN {FRM}-FLD.
% END IF
% ELSE
% IF TIPO_OFI THEN
% IF TIDX > 0 THEN
      * OFI-INI --> BMBRAHSB004.SRCAIU ( FST-KEY Y FST-FLD )
           MOVE FRM-POS-CURS TO {RREG}-COD-{NOMB}-LEN IN {FRM}-FLD(1).
      * OFI-FIN --> BMBRAHSB004.SRCAIU ( FST-KEY Y FST-FLD )
% ELSE
      * OFI-INI --> BMBRAHSB004.SRCAIU ( FST-KEY Y FST-FLD )
           MOVE FRM-POS-CURS TO {RREG}-COD-{NOMB}-LEN IN {FRM}-FLD.
      * OFI-FIN --> BMBRAHSB004.SRCAIU ( FST-KEY Y FST-FLD )
% END IF
% ELSE
% IF TIPO_IDG THEN
% IF TIDX > 0 THEN
      * IDG-INI --> BMBRAHSB004.SRCAIU ( FST-KEY Y FST-FLD )
           MOVE FRM-POS-CURS TO {RREG}-TIP-{NOMB}-LEN IN {FRM}-FLD(1).
           MOVE FRM-POS-CURS TO {RREG}-STP-{NOMB}-LEN IN {FRM}-FLD(1).
      * IDG-FIN --> BMBRAHSB004.SRCAIU ( FST-KEY Y FST-FLD )
% ELSE
      * IDG-INI --> BMBRAHSB004.SRCAIU ( FST-KEY Y FST-FLD )
           MOVE FRM-POS-CURS TO {RREG}-TIP-{NOMB}-LEN IN {FRM}-FLD.
           MOVE FRM-POS-CURS TO {RREG}-STP-{NOMB}-LEN IN {FRM}-FLD.
      * IDG-FIN --> BMBRAHSB004.SRCAIU ( FST-KEY Y FST-FLD )
% END IF
% END IF
% END IF
% END IF
% TIPO_{TIPO} = FALSE
