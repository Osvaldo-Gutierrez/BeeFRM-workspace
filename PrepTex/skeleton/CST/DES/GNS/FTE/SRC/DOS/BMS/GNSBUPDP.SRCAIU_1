      * 24-Mar-94 09:02 AM
       RET-ALL-PDP SECTION.
       INI-RET-ALL-PDP.
           PERFORM KEY-ALL-PDP.
           PERFORM RET-PDP-PDP.
       FIN-RET-ALL-PDP.
           EXIT.
       
       RET-PDP-PDP SECTION.
       INI-RET-PDP-PDP.
           MOVE PDP-COD-SIST IN PDP-FLD TO PDP-COD-SIST IN PDP.
           MOVE PDP-COD-NEMO IN PDP-FLD TO PDP-COD-NEMO IN PDP.
           MOVE PDP-COD-CMND IN PDP-FLD TO PDP-COD-CMND IN PDP.
           MOVE PDP-COD-PROC IN PDP-FLD TO PDP-COD-PROC IN PDP.
           MOVE PDP-IND-RESP IN PDP-FLD TO PDP-IND-RESP IN PDP.
       FIN-RET-PDP-PDP.
           EXIT.
       
       KEY-ALL-PDP SECTION.
       INI-KEY-ALL-PDP.
           PERFORM KEY-PDP-PDP.
       FIN-KEY-ALL-PDP.
           EXIT.
       
       KEY-PDP-PDP SECTION.
       INI-KEY-PDP-PDP.
           MOVE PDP-COD-GRPO IN PDP-FLD TO PDP-COD-GRPO IN PDP.
           MOVE PDP-COD-TDPU IN PDP-FLD TO PDP-COD-TDPU IN PDP.
           MOVE PDP-NUM-SECU IN PDP-FLD TO PDP-NUM-SECU IN PDP.
       FIN-KEY-PDP-PDP.
           EXIT.
       
       SCR-CLRA-PDP SECTION.
       INI-SCR-CLRA-PDP.
           PERFORM CLR-KEY-PDP.
           PERFORM CLR-FLD-PDP.
       FIN-SCR-CLRA-PDP.
           EXIT.
       
       CLR-KEY-PDP SECTION.
       INI-CLR-KEY-PDP.
           MOVE SPACES TO PDP-COD-GRPO IN PDP-FLD.
           MOVE SPACES TO PDP-COD-TDPU IN PDP-FLD.
           MOVE ZEROES TO PDP-NUM-SECU IN PDP-FLD.
       FIN-CLR-KEY-PDP.
           EXIT.
       
       CLR-FLD-PDP SECTION.
       INI-CLR-FLD-PDP.
           MOVE SPACES TO FRM-GLS-GRPO IN PDP-FLD.
           MOVE SPACES TO FRM-GLS-TDPU IN PDP-FLD.
           MOVE SPACES TO FRM-GLS-SIST IN PDP-FLD.
           MOVE SPACES TO FRM-GLS-CMND IN PDP-FLD.
           PERFORM CLR-PDP-PDP.
       FIN-CLR-FLD-PDP.
           EXIT.
       
       CLR-PDP-PDP SECTION.
       INI-CLR-PDP-PDP.
           MOVE SPACES TO PDP-COD-SIST IN PDP-FLD.
           MOVE SPACES TO PDP-COD-NEMO IN PDP-FLD.
           MOVE SPACES TO PDP-COD-CMND IN PDP-FLD.
           MOVE SPACES TO PDP-COD-PROC IN PDP-FLD.
           MOVE SPACES TO PDP-IND-RESP IN PDP-FLD.
       FIN-CLR-PDP-PDP.
           EXIT.
       
       FST-KEY-PDP SECTION.
       INI-FST-KEY-PDP.
           MOVE -1 TO PDP-COD-GRPO-LEN IN PDP-FLD.
       FIN-FST-KEY-PDP.
           EXIT.
       
       FST-FLD-PDP SECTION.
       INI-FST-FLD-PDP.
           MOVE -1 TO PDP-COD-SIST-LEN IN PDP-FLD.
       FIN-FST-FLD-PDP.
           EXIT.
       
       PRO-KEY-PDP SECTION.
       INI-PRO-KEY-PDP.
           PERFORM PRO-MKY-PDP.
       FIN-PRO-KEY-PDP.
           EXIT.
       
       PRO-MKY-PDP SECTION.
       INI-PRO-MKY-PDP.
       FIN-PRO-MKY-PDP.
           EXIT.
       
       PRO-FLD-PDP SECTION.
       INI-PRO-FLD-PDP.
           PERFORM PRO-PDP-PDP.
       FIN-PRO-FLD-PDP.
           EXIT.
       
       PRO-PDP-PDP SECTION.
       INI-PRO-PDP-PDP.
           MOVE FRM-CPIM-AEY TO PDP-COD-SIST-ATR IN PDP-FLD.
           MOVE FRM-CPIM-AEY TO PDP-COD-NEMO-ATR IN PDP-FLD.
           MOVE FRM-CPIM-AEY TO PDP-COD-CMND-ATR IN PDP-FLD.
           MOVE FRM-CPIM-AEY TO PDP-COD-PROC-ATR IN PDP-FLD.
           MOVE FRM-CPIM-AEY TO PDP-IND-RESP-ATR IN PDP-FLD.
       FIN-PRO-PDP-PDP.
           EXIT.
       
       UNP-KEY-PDP SECTION.
       INI-UNP-KEY-PDP.
           PERFORM UNP-IKY-PDP.
           PERFORM UNP-AKY-PDP.
           PERFORM UNP-MKY-PDP.
       FIN-UNP-KEY-PDP.
           EXIT.
       
       UNP-IKY-PDP SECTION.
       INI-UNP-IKY-PDP.
           MOVE FRM-CPIM-UBY TO PDP-COD-GRPO-ATR.
           MOVE FRM-CPIM-UBY TO PDP-COD-TDPU-ATR.
           MOVE FRM-CPIM-UBY TO PDP-NUM-SECU-ATR.
       FIN-UNP-IKY-PDP.
           EXIT.
       
       UNP-MKY-PDP SECTION.
       INI-UNP-MKY-PDP.
       FIN-UNP-MKY-PDP.
           EXIT.
       
       UNP-AKY-PDP SECTION.
       INI-UNP-AKY-PDP.
       FIN-UNP-AKY-PDP.
           EXIT.
       
       UNP-FLD-PDP SECTION.
       INI-UNP-FLD-PDP.
           PERFORM UNP-PDP-PDP.
       FIN-UNP-FLD-PDP.
           EXIT.
       
       UNP-PDP-PDP SECTION.
       INI-UNP-PDP-PDP.
           MOVE FRM-CPIM-UBY TO PDP-COD-SIST-ATR IN PDP-FLD.
           MOVE FRM-CPIM-UBY TO PDP-COD-NEMO-ATR IN PDP-FLD.
           MOVE FRM-CPIM-UBY TO PDP-COD-CMND-ATR IN PDP-FLD.
           MOVE FRM-CPIM-UBY TO PDP-COD-PROC-ATR IN PDP-FLD.
           MOVE FRM-CPIM-UBY TO PDP-IND-RESP-ATR IN PDP-FLD.
       FIN-UNP-PDP-PDP.
           EXIT.
       
       VAL-MKY-PDP SECTION.
       INI-VAL-MKY-PDP.
       FIN-VAL-MKY-PDP.
           EXIT.
       
       VAL-FLD-PDP SECTION.
       INI-VAL-FLD-PDP.
           PERFORM VAL-FRM-GLS-GRPO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-GRPO-LEN IN PDP-FLD
                GO TO FIN-VAL-FLD-PDP.
           PERFORM VAL-FRM-GLS-TDPU.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-TDPU-LEN IN PDP-FLD
                GO TO FIN-VAL-FLD-PDP.
           PERFORM VAL-PDP-COD-SIST.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO PDP-COD-SIST-LEN IN PDP-FLD
                GO TO FIN-VAL-FLD-PDP.
           PERFORM VAL-FRM-GLS-SIST.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-SIST-LEN IN PDP-FLD
                GO TO FIN-VAL-FLD-PDP.
           PERFORM VAL-PDP-COD-NEMO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO PDP-COD-NEMO-LEN IN PDP-FLD
                GO TO FIN-VAL-FLD-PDP.
           PERFORM VAL-PDP-COD-CMND.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO PDP-COD-CMND-LEN IN PDP-FLD
                GO TO FIN-VAL-FLD-PDP.
           PERFORM VAL-FRM-GLS-CMND.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO FRM-GLS-CMND-LEN IN PDP-FLD
                GO TO FIN-VAL-FLD-PDP.
           PERFORM VAL-PDP-COD-PROC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO PDP-COD-PROC-LEN IN PDP-FLD
                GO TO FIN-VAL-FLD-PDP.
           PERFORM VAL-PDP-IND-RESP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO PDP-IND-RESP-LEN IN PDP-FLD
                GO TO FIN-VAL-FLD-PDP.
       FIN-VAL-FLD-PDP.
           EXIT.
       
       VAL-FRM-GLS-GRPO SECTION.
       INI-VAL-FRM-GLS-GRPO.
       FIN-VAL-FRM-GLS-GRPO.
           EXIT.
       
       VAL-FRM-GLS-TDPU SECTION.
       INI-VAL-FRM-GLS-TDPU.
       FIN-VAL-FRM-GLS-TDPU.
           EXIT.
       
       VAL-PDP-COD-SIST SECTION.
       INI-VAL-PDP-COD-SIST.
           IF PDP-COD-SIST IN PDP-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-PDP-COD-SIST.
      *<<<< INI_PDP_COD_SIST
           IF PDP-COD-SIST IN PDP-FLD = '* '
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'TODOS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-GLS-SIST IN PDP-FLD
               GO TO FIN-VAL-PDP-COD-SIST.

      *>>>> INI_PDP_COD_SIST 
      *VAL-COD Valida codigo de tabla
           MOVE 'GNS' TO TAB-COD-SIST.
           MOVE 'SIS ' TO TAB-COD-TTAB IN TAB.
           MOVE PDP-COD-SIST IN PDP-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              PDP-COD-SIST IN PDP-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-SIST IN PDP-FLD
           ELSE
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-SIST IN PDP-FLD.
       FIN-VAL-PDP-COD-SIST.
           EXIT.
       
       VAL-FRM-GLS-SIST SECTION.
       INI-VAL-FRM-GLS-SIST.
       FIN-VAL-FRM-GLS-SIST.
           EXIT.
       
       VAL-PDP-COD-NEMO SECTION.
       INI-VAL-PDP-COD-NEMO.
           IF PDP-COD-NEMO IN PDP-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-PDP-COD-NEMO.
       FIN-VAL-PDP-COD-NEMO.
           EXIT.
       
       VAL-PDP-COD-CMND SECTION.
       INI-VAL-PDP-COD-CMND.
           IF PDP-COD-CMND IN PDP-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-PDP-COD-CMND.
      *<<<< INI_PDP_COD_CMND
           IF PDP-COD-CMND IN PDP-FLD = '* '
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'TODOS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-GLS-CMND IN PDP-FLD
               GO TO FIN-VAL-PDP-COD-CMND.

      *>>>> INI_PDP_COD_CMND 
      *VAL-COD Valida codigo de tabla
           MOVE 'GNS' TO TAB-COD-SIST.
           MOVE 'CMN ' TO TAB-COD-TTAB IN TAB.
           MOVE PDP-COD-CMND IN PDP-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              PDP-COD-CMND IN PDP-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-CMND IN PDP-FLD
           ELSE
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-CMND IN PDP-FLD.
       FIN-VAL-PDP-COD-CMND.
           EXIT.
       
       VAL-FRM-GLS-CMND SECTION.
       INI-VAL-FRM-GLS-CMND.
       FIN-VAL-FRM-GLS-CMND.
           EXIT.
       
       VAL-PDP-COD-PROC SECTION.
       INI-VAL-PDP-COD-PROC.
           IF PDP-COD-PROC IN PDP-FLD NOT > SPACES
              MOVE SPACES TO PDP-COD-PROC IN PDP-FLD.
       FIN-VAL-PDP-COD-PROC.
           EXIT.
       
       VAL-PDP-IND-RESP SECTION.
       INI-VAL-PDP-IND-RESP.
           IF PDP-IND-RESP IN PDP-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-PDP-IND-RESP.
      *VAL-COD Valida codigo de tabla
           MOVE 'GNS' TO TAB-COD-SIST.
           MOVE 'SON ' TO TAB-COD-TTAB IN TAB.
           MOVE PDP-IND-RESP IN PDP-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
       FIN-VAL-PDP-IND-RESP.
           EXIT.
       
       VAL-CON-FLD-PDP SECTION.
       INI-VAL-CON-FLD-PDP.
      *<<<< VAL_CON_FLD_PDP
           IF SCR-CIC-TDPU = 'GI' AND
              PDP-COD-PROC IN PDP-FLD > SPACES              
                MOVE 'GNS' TO MSG-COD-SIST
                MOVE 'TDPUPROCBLK' TO MSG-COD-MENS
                PERFORM GET-MSG
                MOVE MSG-GLS-DESC TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
           ELSE
           IF SCR-CIC-TDPU = 'GB' AND
              PDP-COD-PROC IN PDP-FLD NOT > SPACES              
                MOVE 'GNS' TO MSG-COD-SIST
                MOVE 'TDPUPROCOBL' TO MSG-COD-MENS
                PERFORM GET-MSG
                MOVE MSG-GLS-DESC TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>> VAL_CON_FLD_PDP
       FIN-VAL-CON-FLD-PDP.
           EXIT.
       
       VAL-CON-PDP SECTION.
       INI-VAL-CON-PDP.
       FIN-VAL-CON-PDP.
           EXIT.
       
       VAL-NUM-KEY-PDP SECTION.
       INI-VAL-NUM-KEY-PDP.
           PERFORM VAL-NUM-IKY-PDP.
           PERFORM VAL-NUM-MKY-PDP.
           PERFORM VAL-NUM-AKY-PDP.
       FIN-VAL-NUM-KEY-PDP.
           EXIT.
       
       VAL-NUM-IKY-PDP SECTION.
       INI-VAL-NUM-IKY-PDP.
       VAL-NUM-PDP-NUM-SECU.
           MOVE PDP-NUM-SECU-ALF IN PDP-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 4 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO PDP-NUM-SECU IN PDP-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO PDP-NUM-SECU-LEN IN PDP-FLD
                GO TO FIN-VAL-NUM-IKY-PDP.
       FIN-VAL-NUM-IKY-PDP.
           EXIT.
       
       VAL-NUM-MKY-PDP SECTION.
       INI-VAL-NUM-MKY-PDP.
       FIN-VAL-NUM-MKY-PDP.
           EXIT.
       
       VAL-NUM-AKY-PDP SECTION.
       INI-VAL-NUM-AKY-PDP.
       FIN-VAL-NUM-AKY-PDP.
           EXIT.
       
       VAL-NUM-FLD-PDP SECTION.
       INI-VAL-NUM-FLD-PDP.
       FIN-VAL-NUM-FLD-PDP.
           EXIT.
       
       EDT-KEY-PDP SECTION.
       INI-EDT-KEY-PDP.
           PERFORM EDT-IKY-PDP.
           PERFORM EDT-MKY-PDP.
           PERFORM EDT-AKY-PDP.
       FIN-EDT-KEY-PDP.
           EXIT.
       
       EDT-IKY-PDP SECTION.
       INI-EDT-IKY-PDP.
           IF PDP-NUM-SECU-LEN IN PDP-FLD = -9
                MOVE -1 TO PDP-NUM-SECU-LEN IN PDP-FLD
                GO TO FIN-EDT-IKY-PDP.
           MOVE PDP-NUM-SECU IN PDP-FLD TO PDP-NUM-SECU-EDT IN PDP-EDT.
           MOVE PDP-NUM-SECU-EDT IN PDP-EDT TO 
                PDP-NUM-SECU-ALF IN PDP-FLD.
       FIN-EDT-IKY-PDP.
           EXIT.
       
       EDT-MKY-PDP SECTION.
       INI-EDT-MKY-PDP.
       FIN-EDT-MKY-PDP.
           EXIT.
       
       EDT-AKY-PDP SECTION.
       INI-EDT-AKY-PDP.
       FIN-EDT-AKY-PDP.
           EXIT.
       
       EDT-FLD-PDP SECTION.
       INI-EDT-FLD-PDP.
       FIN-EDT-FLD-PDP.
           EXIT.
