$	FILE = F$ELEMENT(0,"/","''P1'")
$	LETT = F$ELEMENT(0,"/","''P1'")
$	EPCF = "''P2'"
$	WIDT = "''P3'"
$	IF EPCF .EQS. "132" .OR. EPCF .EQS. "80" THEN WIDT = EPCF
$	IF EPCF .EQS. "132" .OR. EPCF .EQS. "80" THEN EPCF = ""
$	I    = 0
$!	ENF = Impresion enfatizada.
$!	LET = Impresion con calidad de carta (letter quality)(solo FX-80).
$!	LIT = Impresion condensada (imprime 132 columnas en ancho 80).
$!	ITA = Impresion con caracteres italicos.
$!	ITE = impresion con caracteres italicos y enfatizados.
$
$	SET TERMINAL/FORM
$	SET TERMINAL/NOTAB
$!	IF LETT .EQS. "ENF" THEN WRITE sys$output  "[5iE[4i"
$!	IF LETT .EQS. "LET" THEN WRITE sys$output  "[5iEG[4i"
$!	IF LETT .EQS. "LIT" THEN WRITE SYS$OUTPUT  "[5i[4i"
$!	IF LETT .EQS. "ITA" THEN WRITE SYS$OUTPUT  "[5i4[4i"
$!	IF LETT .EQS. "ITE" THEN WRITE SYS$OUTPUT  "[5i4E[4i"
$	I132 = ""
$	F132 = ""
$ MAIN:
$	IF FILE .EQS. "" THEN GOTO LUP
$	IF F$SEARCH(FILE) .NES. "" THEN GOTO PR1
$	WRITE SYS$OUTPUT "		No existe Archivo ''FILE'"
$	EXIT
$ PR1:
$	WRITE SYS$OUTPUT " [5i "
$	IF WIDT .EQS. "132" THEN SET TERMINAL/WIDTH=132
$	IF WIDT .EQS. "132" THEN WRITE SYS$OUTPUT I132
$!	TYPE 'FILE'
$	GOSUB PFL
$	IF WIDT .EQS. "132" THEN WRITE SYS$OUTPUT F132
$	IF NL .NE. 0 THEN WRITE SYS$OUTPUT ""
$	IF EPCF .EQS. "LAST" THEN WRITE SYS$OUTPUT ""
$	WRITE SYS$OUTPUT " [4i "
$	EXIT
$ LUP:
$	INQUIRE FILE "Archivo : "
$	IF FILE .EQS. "" THEN GOTO FIN
$	IF F$SEARCH(FILE) .NES. "" THEN GOTO PRT
$	WRITE SYS$OUTPUT "		No existe Archivo ''FILE'"
$	GOTO LUP
$ PRT:
$	I = I + 1
$	WRITE SYS$OUTPUT " [5i "
$	IF WIDT .EQS. "132" THEN SET TERMINAL/WIDTH=132
$	IF WIDT .EQS. "132" THEN WRITE SYS$OUTPUT ""
$!	TYPE 'FILE'
$	GOSUB PFL
$	IF WIDT .EQS. "132" THEN WRITE SYS$OUTPUT ""
$	IF NL .NE. 0 THEN WRITE SYS$OUTPUT ""
$	WRITE SYS$OUTPUT " [4i "
$	GOTO LUP
$ FIN:
$	IF I .EQS. 0 THEN EXIT
$	WRITE SYS$OUTPUT " [5i "
$	WRITE SYS$OUTPUT ""
$	WRITE SYS$OUTPUT " [4i "
$	EXIT
$ PFL:
$	ON CONTROL_Y THEN GOTO CLO
$	OPEN/READ/SHARE FILE_INPUT 'FILE'
$ NL0:
$	NL = 0
$ LEE:
$	READ/END_OF_FILE=EOF FILE_INPUT REG
$	NL = NL + 1
$	WRITE SYS$OUTPUT REG
$	IF F$LOCATE("",REG) .NE. F$LENGTH(REG) THEN NL = 0
$	IF NL .LT. 60 THEN GOTO LEE
$	WRITE SYS$OUTPUT ""
$	GOTO NL0
$ EOF:
$	CLOSE FILE_INPUT
$	RETURN
$ CLO:
$	CLOSE FILE_INPUT
$	WRITE SYS$OUTPUT " [4i "
$	EXIT
