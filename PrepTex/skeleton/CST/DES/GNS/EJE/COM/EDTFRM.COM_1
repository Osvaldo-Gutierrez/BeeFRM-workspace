$	PURGE/NOLOG/NOCONFIRM	'P2'FRM'P1'.TEM, 'P2'FRM'P1'.COB
$	EXIT
$ ERR:
$	SET NOCONTROL=Y
$	SET NOON
$	DELETE/NOLOG/NOCONFIRM	FRM'P1'.IFS.*, FRM'P1'.VAL.*
$	DELETE/NOLOG/NOCONFIRM	FRM'P1'.EDT.*, FRM'P1'.MOV.*, FRM'P1'.CPY.*
$	DELETE/NOLOG/NOCONFIRM	FRM%%%'P1'.GEN.*
