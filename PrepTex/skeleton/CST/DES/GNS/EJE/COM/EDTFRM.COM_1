$	@DESCOM:DESLIM FRMDAT'P1' GEN
$	LIBINS GEN FRM%%%'P1'
$	COPY DESCOB:DESFRMFRM.COB 'P2'FRM'P1'.TEM
$	EDIT/COMMAND=FRM'P1'.EDT 'P2'FRM'P1'.TEM
$	GENCOB 'P2'FRM'P1' SWITCH
$	PTH = "''P2'FRM''P1'.TEM"
$	TPR = "F"
$!	RUN DESEXE:DESDOCPRO
$ FIN:
