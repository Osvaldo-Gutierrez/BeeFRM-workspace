$!	OPEN/WRITE FILE_OUTPTX 'GNS$FIL'.PTX
$	PTX 'GNS$PTH' 'GNS$FIL'.'EXT'/CONTROL="*%"/UNDEFINED=ZERO/COMMAND=(PGM_'ORG'=TRUE,PRV_'P3'=TRUE,PAI_'GNS$PAI'=TRUE,REALIA='REALIA',PGM_'GNS$PAI'=TRUE,PGM_'GNS$CLI'=TRUE,CLI_'GNS$CLI'=TRUE)/NOSHOW
