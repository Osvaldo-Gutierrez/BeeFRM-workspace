$!	INQUIRE P1 "Archivo ? "
$!	GOTO P1
$!	P1 := "''GNS$DSKGNS'[CST.DES.GNS.FTE.SRC]GNSPPMEN.PTX"
$ P2:
$	IF P2 .NES. "" THEN GOTO P3
$	IF F$MODE() .EQS. "BATCH" THEN EXIT
$	INQUIRE P2 "Organizacion (VSM/DTC) ? "
$	GOTO P2
