$	@GNSCOM:MNUSYSLNM.COM
$! P1 : PROYECTO
$! P2 : JEFE DE PROYECTO
$	IF F$MODE() .EQS. "BATCH" THEN 'BAT'
$ P1:
$	IF P1 .NES. "" THEN GOTO P2
$	IF F$MODE() .EQS. "BATCH" THEN EXIT
$	INQUIRE P1 "Proyecto ? "
$	IF P1 .EQS. "" THEN EXIT
$ P2:
$	IF P2 .NES. "" THEN GOTO MAIN
$	IF F$MODE() .EQS. "BATCH" THEN EXIT
$	INQUIRE P2 "Jefe de proyecto (si No desea asignar solo digite RETURN) ? "
$	IF P2 .EQS. "" THEN EXIT
$ MAIN:
$	write sys$output "[1mASSJDPPRY : ASIGNACION DE JEFE DE PROYECTO[0m"
$	write sys$output "[1mProyecto          : ''p1'[0m"
$	write sys$output "[1mJefe de Proyecto  : ''p2'[0m"
$
$	NOD =	F$PARSE("[]",,,"NODE")
$	DEV =	F$PARSE("[]",,,"DEVICE")
$	DIR =	F$PARSE("[]",,,"DIRECTORY")
$	ON CONTROL_Y THEN GOTO FIN
$	UAF :== "$SYS$SYSTEM:AUTHORIZE"
$	SET DEF SYS$SYSTEM
$
$	ON WARNING THEN CONTINUE
$	UAF GRANT/IDENT 'P1'_DES 'P2' /ATTR=(NODYN,RES)
$
$	ON WARNING THEN CONTINUE
$	UAF GRANT/IDENT 'P1'_WRK 'P2' /ATTR=(NODYN,RES)
$
$	ON WARNING THEN CONTINUE
$	UAF GRANT/IDENT 'P1'_JDP 'P2' /ATTR=(NODYN,NORES)
$
$	ON WARNING THEN CONTINUE
$	UAF GRANT/IDENT 'P1'_EXP 'P2' /ATTR=(NODYN,RES)
$
$	ON WARNING THEN CONTINUE
$	UAF GRANT/IDENT 'P1'_FIL 'P2' /ATTR=(NODYN,RES)
$
$	ON WARNING THEN CONTINUE
$	UAF GRANT/IDENT 'P1'_OPR 'P2' /ATTR=(NODYN,NORES)
$
$	ON WARNING THEN CONTINUE
$	UAF GRANT/IDENT  GNS_OPR 'P2' /ATTR=(NODYN,NORES)
$
$!	ON WARNING THEN CONTINUE
$!	UAF GRANT/IDENT  GNS_PGM 'P2' /ATTR=(NODYN,NORES)
$ FIN:
$	SET DEF 'NOD''DEV''DIR'
