$ SET NOVER
$ P1:
$ IF P1 .NES. "" THEN GOTO MAIN
$ IF F$MODE() .EQS. "BATCH" THEN EXIT
$ INQUIRE P1 "_File: "
$ GOTO P1
$ MAIN:
$ DEFINE/NOLOG SYS$INPUT SYS$COMMAND
$ ASS EDIT.LOG SYS$OUTPUT
$ ON WARNING   THEN GOTO FIN
$ ON CONTROL_Y THEN GOTO FIN
$ EDITT/COMMAND=GNSCOM:EDTGNSINI.COM 'P1''P2''P3''P4''P5''P6''P7''P8'
$ FIN:
$ DEASS SYS$OUTPUT
$ DELETE/NOLOG/NOCONF EDIT.LOG;
