$ 'BAT'
$ P1 = "GNSPPPK1"
$ P1:
$	IF P1 .NES. "" THEN GOTO P2
$	IF F$MODE() .EQS. "BATCH" THEN EXIT
$	INQUIRE P1 "Modulo ? "
$	GOTO P1
$ P2:
$	IF P2 .NES. "" THEN GOTO MAIN
$	IF F$MODE() .EQS. "BATCH" THEN EXIT
$	INQUIRE P2 "Realia ? ( TRUE / FALSE )"
$	GOTO P2
$ MAIN:
$	IF F$MODE() .EQS. "BATCH" THEN 'BAT'
$	WRITE SYS$OUTPUT "[5m[1m",-
		"			Generando...[0m"
$ ASS:	
$	ASS SRC'P1'.LOG SYS$OUTPUT
$	ASS SRC'P1'.LOG SYS$ERROR
$ AIU:
$	AIU = F$SEARCH("''P1'.SRC")
$	IF AIU .NES. "" THEN GOTO QLF
$ NAIU:
$	WRITE SYS$OUTPUT "[1m[A[K",-
	"		ERROR : NO Existe Archivo ''P1'.SRC[0m"
$	GOTO ERR
$ QLF:
$	IF P2 .EQS. "" THEN P2 = "/CDA=" + P1
$	QLF == "$GNSEXE:PROQLF"
$	QLF /CDA=* 'P2'
$ PROC:
$	ON WARNING THEN GOTO ERR
$	P3 = "(PGM_''GNS$SOP'=TRUE,PGM_''GNS$LNG'=TRUE,PGM_''GNS$AIO'=TRUE,PGM_''GNS$AIU'=TRUE,SW_REALIA=''P2')"
$	if gns$aio .EQS. "DTC" then GENPTX 'P1'.SRC 'P1'.PGD 'CDA'.TXT 'CDA'.GEN 'P3'
$	if gns$aio .EQS. "VSI" then GENPTX 'P1'.SRC 'P1'.PGI 'CDA'.TXT 'CDA'.GEN 'P3'
$!	IF AIU .NES. "" THEN PUT 'P1'.AIU
$!	IF AIU .EQS. "" THEN DELETE/NOLOG/NOCONFIRM 'P1'.AIU.*
$	DEASS SYS$OUTPUT
$	DEASS SYS$ERROR
$	DELETE/NOLOG/NOCONFIRM SRC'P1'.LOG.*
$	if gns$aio .EQS. "VSI" then WRITE SYS$OUTPUT "[1m[A[K			''P1'.PGI Generado[0m"
$	if gns$aio .EQS. "DTC" then WRITE SYS$OUTPUT "[1m[A[K			''P1'.PGD Generado[0m"
$	EXIT
$ ERR:
$	DEASS SYS$OUTPUT
$	DEASS SYS$ERROR
$	SET CONTROL=Y
$ EMS:
$	IF F$MODE() .EQS. "BATCH" THEN EXIT
$	WRITE SYS$COMMAND "[1m[A[K ERROR[0m"
$	TYPE SRC'P1'.LOG
$	EXIT
