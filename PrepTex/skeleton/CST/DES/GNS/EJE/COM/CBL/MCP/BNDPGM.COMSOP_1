$ P1:
$	PFM$SIS = P2
$	ON ERROR     THEN CONTINUE
$	ON WARNING   THEN CONTINUE
$	ASS BNDFIOPGM.LOG SYS$OUTPUT
$	ASS BNDFIOPGM.LOG SYS$ERROR
$	DEASSIGN SYS$OUTPUT
$	DEASSIGN SYS$ERROR
$	DEL/NOLOG/NOCONF BNDFIOPGM.LOG.*
$	IF P1 .NES. "" THEN GOTO MAIN
$	IF F$MODE() .EQS. "BATCH" THEN EXIT
$	INQUIRE P1 "Archivo entrada ? "
$	GOTO P1
$ MAIN:
$	IF F$MODE() .EQS. "BATCH" THEN 'BAT'
$	ON WARNING   THEN EXIT
$	ON CONTROL_Y THEN EXIT
$	WRITE SYS$OUTPUT "[5m[1m		Generando...[0m"
$	@GNSCOM:FIL 'P1'
$	IF F$MODE() .EQS. "BATCH" THEN SET NOVER
$	IF GNS$EXT .EQS. "" THEN GNS$EXT = "PGI"
$	SEARCH 'GNS$FIL'.'GNS$EXT' "   COPY"/OUT='GNS$FIL'.BNDCPY
$	SEARCH 'GNS$FIL'.BNDCPY "COLBPCA2","GNSBVNUM","GNSBPVRF","GNSBGQUE","GNSBGCIC","BF"/OUT='GNS$FIL'.BNDFIO
$	DEL/NOLOG/NOCONF 'GNS$FIL'.BNDCPY.
$	RUN GNSEXE:BNDPGM
$!	RUN [CST.RECHEVER.GNS.BND]BNDPGMRC
$	DEL/NOLOG/NOCONF 'GNS$FIL'.BNDFIO.
$ FIN:
$	WRITE SYS$OUTPUT "[A[2K"
$	WRITE SYS$OUTPUT "[1m		",-
	"BNDPGMRC :  Archivo ''GNS$FIL'.PGO Generados [0m"
$ CLI:
$	DEASS SYS$OUTPUT
