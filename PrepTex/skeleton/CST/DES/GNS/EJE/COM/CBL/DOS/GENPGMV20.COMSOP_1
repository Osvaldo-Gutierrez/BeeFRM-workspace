$	SET NOVER
$ P1:
$	IF P1 .NES. "" THEN GOTO MAIN
$	IF F$MODE() .EQS. "BATCH" THEN EXIT
$	INQUIRE P1 "Programa ? "
$	GOTO P1
$ MAIN:
$	ON WARNING   THEN GOTO ERR
$	ON CONTROL_Y THEN GOTO ERR
$	IF F$MODE() .EQS. "BATCH" THEN 'BAT'
$	WRITE SYS$OUTPUT "[5m[1m",-
		"			Generando...[0m"
$	QLF == "$GNSEXE:PROQLF"
$	QLF /CPP=ING,MOD,ACT,ELI,ACC,PGM,DEF,ALL/OUT=*/CDA=* 'P2'
$	IF CPP .EQS. "" THEN CPP = "ALL"
$	IF OUT .EQS. "" THEN OUT = P1
$	IF CDA .EQS. "" THEN CDA = P1
$
$	FIL = P1
$	SKL = "GNSSRCSOP:GNSPAFRM.SRCSOP"
$	RUN GNSEXE:GENPGM
$
$	BIF = "''BIF.AIU"
$	NMB = F$ELEMENT(0,".",BIF)
$	EXT = F$ELEMENT(1,".",BIF)
$	MOD = F$SEARCH("''NMB'.''EXT'")
$	IF MOD .NES. "" THEN GOTO PROC
$	ARC = F$SEARCH("''EXT':''NMB'.''EXT'")
$	IF ARC .EQS. "" THEN ARC = F$SEARCH("''GNS$PRY'''EXT':''NMB'.''EXT'")
$	IF ARC .NES. "" THEN COPY/NOLOG  'ARC' []
$	IF ARC .EQS. "" THEN CREATE 'NMB'.'EXT'
$ PROC:
$	P3 = "(PGM_''GNS$SOP'=TRUE,PGM_''GNS$LNG'=TRUE,PGM_''GNS$AIO'=TRUE,PGM_''GNS$AIU'=TRUE)"
$	PTX PGM'OUT'.PTX 'OUT'.TEM/NOSHOW/UNDEFINED=ZERO/COMMAND='P3'
$	IF GNS$AIO .EQS. "DTC" THEN -
		GENPTX 'OUT'.TEM 'OUT'.PGD 'CDA'.TXT 'CDA'.GEN 'P3'
$	IF GNS$AIO .EQS. "VSI" THEN -
		GENPTX 'OUT'.TEM 'OUT'.PGI 'CDA'.TXT 'CDA'.GEN 'P3'
$	IF MOD .NES. "" THEN PUT 'NMB'.'EXT'
$ FIN:
$	SET NOON
$	DELETE/NOLOG/NOCONFIRM	PGM'OUT'.PTX.*
$	IF MOD .EQS. "" THEN DELETE/NOLOG/NOCONFIRM 'NMB'.'EXT'.*
$	IF GNS$AIO .EQS. "DTC" THEN TEX = "PGD"
$	IF GNS$AIO .EQS. "VSI" THEN TEX = "PGI"
$	WRITE SYS$OUTPUT "[1m[A[K", -
		"			''OUT'.''TEX' Generado[0m"
$	EXIT
$ ERR:
$	SET NOCONTROL=Y
$	SET NOON
$	DELETE/NOLOG/NOCONFIRM	PGM'OUT'.PTX.*
$	WRITE SYS$OUTPUT "[1m[A[K ERROR[0m"
$	SET CONTROL=Y
$	IF MOD .EQS. "" THEN DELETE/NOLOG/NOCONFIRM 'NMB'.'EXT'.*
$	EXIT
