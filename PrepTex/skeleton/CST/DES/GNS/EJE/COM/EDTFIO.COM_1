$ MAIN:
$	ON WARNING   THEN GOTO ERR
$	ON CONTROL_Y THEN GOTO ERR
$	COPY/NOLOG DESCOB:DESFIO'P2'.COB 'P3'FIO'P4'.COB
$	EDIT/COMMAND= FIO'P1'.EDT 'P3'FIO'P4'.COB
$	COPY/NOLOG DESCOB:DESPRU'P2'.COB 'P3'PRU'P4'.COB
$	EDIT/COMMAND= PRU'P1'.EDT 'P3'PRU'P4'.COB
$	PTH = "''P3'FIO''P4'.COB"
$	TPR = "A"
$	RUN DESEXE:DESDOCPRO
$ FIN:
$	SET NOON
$	DELETE/NOLOG/NOCONFIRM FIO'P1'.EDT.*, PRU'P1'.EDT.*, FIO'P1'.ALT.*
$	DELETE/NOLOG/NOCONFIRM FIO'P1'.IFS.*, FIO'P1'.FND.*, FIO'P1'.MOV.*
$	PURGE/NOLOG/NOCONFIRM 'P3'FIO'P4'.COB, 'P3'PRU'P4'.COB
$	IF F$MODE() .EQS. "BATCH" THEN EXIT
$	WRITE SYS$COMMAND "[A[2K"
$	WRITE SYS$COMMAND "[1m			",-
		P3,"FIO",P4,".COB Generado[0m"
$	WRITE SYS$COMMAND "[1m			",-
		P3,"PRU",P4,".COB Generado[0m"
$	EXIT
$ ERR:
$	SET NOCONTROL=Y
$	SET NOON
$	DELETE/NOLOG/NOCONFIRM FIO'P1'.EDT.*, PRU'P1'.EDT.*, FIO'P1'.ALT.*
$	DELETE/NOLOG/NOCONFIRM FIO'P1'.IFS.*, FIO'P1'.FND.*, FIO'P1'.MOV.*
$	DELETE/NOLOG/NOCONFIRM 'P3'FIO'P4'.COB.*, 'P3'PRU'P4'.COB.*
$	MALO
$	EXIT
