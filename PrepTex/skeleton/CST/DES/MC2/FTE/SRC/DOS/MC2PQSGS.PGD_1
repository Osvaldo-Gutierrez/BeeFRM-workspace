           MOVE SGS-NUM-EDAT IN SGS TO WSS-NUM-EDAT-V(WSS-J).           
           MOVE SGS-IND-EDRA IN SGS TO WSS-IND-EDRA-V(WSS-J).           
           ADD  SGS-VAL-TOPE IN SGS TO WSS-VAL-TOPE-V(WSS-J).           
                                                                        
           MOVE SGS-COD-VTOP IN SGS TO WSS-COD-VTOP-V(WSS-J).           
           MOVE SGS-COD-CPTT IN SGS TO WSS-COD-CPTT-V(WSS-J).           
           ADD  SGS-VAL-VMIN IN SGS TO WSS-VAL-VMIN-V(WSS-J).           
                                                                        
           ADD  SGS-VAL-VMAX IN SGS TO WSS-VAL-VMAX-V(WSS-J).           
                                                                        
           MOVE SGS-COD-VMYM IN SGS TO WSS-COD-VMYM-V(WSS-J).           
           MOVE SGS-IND-SOBL IN SGS TO WSS-IND-SOBL-V(WSS-J).           
           MOVE SGS-COD-IPVC IN SGS TO WSS-COD-IPVC-V(WSS-J).           
                                                                        
           MOVE 'TAB'                   TO TAB-COD-SIST.                
           MOVE 'SIS'                   TO TAB-COD-TTAB IN TAB.         
           MOVE SGS-COD-SIST IN SGS     TO TAB-COD-CTAB IN TAB.         
           PERFORM BUS-TAB.                                             
           MOVE TAB-GLS-DESC IN TAB     TO WSS-GLS-SIST-SGS-V(WSS-J).   
                                                                        
           MOVE 'MC2'                   TO TAB-COD-SIST.                
           MOVE 'EVT'                   TO TAB-COD-TTAB IN TAB.         
           MOVE SGS-COD-SIST IN SGS     TO WSS-COD-SIST IN WSS-VARI.    
           MOVE SGS-COD-EVEN IN SGS     TO WSS-COD-EVTO IN WSS-VARI.    
           MOVE WSS-COD-EVEN IN WSS-VARI TO TAB-COD-CTAB IN TAB.        
           PERFORM BUS-TAB.                                             
           MOVE TAB-GLS-DESC IN TAB     TO WSS-GLS-EVEN-SGS-V(WSS-J).   
                                                                        
           MOVE 'MC2'                   TO TAB-COD-SIST FIO-SIST.
           MOVE 'DPL'                   TO TAB-COD-TTAB IN TAB.         
           MOVE SGS-CIC-PLAN IN SGS     TO TAB-COD-CTAB IN TAB.         
           PERFORM BUS-TAB.                                             
           MOVE TAB-GLS-DESC            TO WSS-GLS-DES1-SGS-V(WSS-J).   
           MOVE TAB-GLS-DATA            TO WSS-GLS-DES2-SGS-V(WSS-J).   
                                                                        
           IF SGS-COD-TSEG IN SGS > SPACES                              
              MOVE 'MC2'                   TO TAB-COD-SIST              
              MOVE 'TSE'                   TO TAB-COD-TTAB IN TAB       
              MOVE SGS-COD-TSEG IN SGS     TO TAB-COD-CTAB IN TAB       
              PERFORM BUS-TAB                                           
              MOVE TAB-GLS-DCOR IN TAB     TO WSS-GLS-TSEG-V(WSS-J)     
           ELSE                                                         
               MOVE SPACES                 TO WSS-GLS-TSEG-V(WSS-J).    
                                                                        
           IF SGS-COD-SEGU IN SGS > SPACES                              
              MOVE 'MC2'                   TO TAB-COD-SIST              
              MOVE 'SGS'                   TO TAB-COD-TTAB IN TAB       
              MOVE SGS-COD-SEGU IN SGS     TO TAB-COD-CTAB IN TAB       
              PERFORM BUS-TAB                                           
              MOVE TAB-GLS-DCOR IN TAB     TO WSS-GLS-SEGU-V(WSS-J)     
           ELSE                                                         
              MOVE SPACES                  TO WSS-GLS-SEGU-V(WSS-J).    
                                                                        
           IF SGS-COD-OCOB IN SGS > SPACES                              
              MOVE 'MC2'                   TO TAB-COD-SIST              
              MOVE 'OCS'                   TO TAB-COD-TTAB IN TAB       
              MOVE SGS-COD-OCOB IN SGS     TO TAB-COD-CTAB IN TAB       
              PERFORM BUS-TAB                                           
              MOVE TAB-GLS-DCOR IN TAB     TO WSS-GLS-OCOB-V(WSS-J)     
           ELSE                                                         
              MOVE SPACES                  TO WSS-GLS-OCOB-V(WSS-J).    
                                                                        
           IF SGS-MSC-BCTI IN SGS > SPACES                              
              MOVE 'COL'                   TO MSC-COD-SIST              
              MOVE 'BCTI'                  TO MSC-COD-TMSC IN MSC       
              MOVE SGS-MSC-BCTI IN SGS     TO MSC-COD-CMSC IN MSC       
              PERFORM BUS-MSC                                           
              MOVE MSC-GLS-DCOR IN MSC     TO WSS-GLS-BCTI-V(WSS-J)     
           ELSE                                                         
               MOVE SPACES                 TO WSS-GLS-BCTI-V(WSS-J).    
                                                                        
           IF SGS-MSC-PBTI IN SGS > SPACES                              
              MOVE 'COL'                   TO MSC-COD-SIST              
              MOVE 'PBTI'                  TO MSC-COD-TMSC IN MSC       
              MOVE SGS-MSC-PBTI IN SGS     TO MSC-COD-CMSC IN MSC       
              PERFORM BUS-MSC                                           
              MOVE MSC-GLS-DCOR IN MSC     TO WSS-GLS-PBTI-V(WSS-J)     
           ELSE                                                         
              MOVE SPACES                  TO WSS-GLS-PBTI-V(WSS-J).    
                                                                        
           IF SGS-MSC-BPBT IN SGS > SPACES                              
              MOVE 'COL'                   TO MSC-COD-SIST              
              MOVE 'MCTI'                  TO MSC-COD-TMSC IN MSC       
              MOVE SGS-MSC-BPBT IN SGS     TO MSC-COD-CMSC IN MSC       
              PERFORM BUS-MSC                                           
              MOVE MSC-GLS-DESC IN MSC     TO WSS-GLS-BPBT-V(WSS-J)     
           ELSE                                                         
              MOVE SPACES                  TO WSS-GLS-BPBT-V(WSS-J).    
                                                                        
           IF SGS-MSC-SPBT IN SGS > SPACES                              
              MOVE 'COL'                   TO MSC-COD-SIST              
              MOVE 'MCTI'                  TO MSC-COD-TMSC IN MSC       
              MOVE SGS-MSC-SPBT IN SGS     TO MSC-COD-CMSC IN MSC       
              PERFORM BUS-MSC                                           
              MOVE MSC-GLS-DESC IN MSC     TO WSS-GLS-SPBT-V(WSS-J)     
           ELSE                                                         
              MOVE SPACES                  TO WSS-GLS-SPBT-V(WSS-J).    
                                                                        
           IF SGS-MSC-TFPR IN SGS > SPACES                              
              MOVE 'COL'                   TO MSC-COD-SIST              
              MOVE 'TFPR'                  TO MSC-COD-TMSC IN MSC       
              MOVE SGS-MSC-TFPR IN SGS     TO MSC-COD-CMSC IN MSC       
              PERFORM BUS-MSC                                           
              MOVE MSC-GLS-DESC IN MSC     TO WSS-GLS-TFPR-V(WSS-J)     
           ELSE                                                         
              MOVE SPACES                  TO WSS-GLS-TFPR-V(WSS-J).    
                                                                        
           IF SGS-MSC-UTPR IN SGS > SPACES                              
              MOVE 'TAB'                   TO MSC-COD-SIST              
              MOVE 'IPER'                  TO MSC-COD-TMSC IN MSC       
              MOVE SGS-MSC-UTPR IN SGS     TO MSC-COD-CMSC IN MSC       
              PERFORM BUS-MSC                                           
              MOVE MSC-GLS-DCOR IN MSC     TO WSS-GLS-UTPR-V(WSS-J)     
           ELSE                                                         
              MOVE SPACES                  TO WSS-GLS-UTPR-V(WSS-J).    
                                                                        
           IF SGS-IND-UTPZ IN SGS > SPACES                              
              MOVE 'TAB'                   TO MSC-COD-SIST              
              MOVE 'IPER'                  TO MSC-COD-TMSC IN MSC       
              MOVE SGS-IND-UTPZ IN SGS     TO MSC-COD-CMSC IN MSC       
              PERFORM BUS-MSC                                           
               MOVE MSC-GLS-DCOR IN MSC    TO WSS-GLS-UTPZ-V(WSS-J)     
           ELSE                                                         
               MOVE SPACES                 TO WSS-GLS-UTPZ-V(WSS-J).    
                                                                        
           IF SGS-IND-EDRA IN SGS > SPACES                              
              MOVE 'MC2'                   TO TAB-COD-SIST              
              MOVE 'ERA'                   TO TAB-COD-TTAB IN TAB       
              MOVE SGS-IND-EDRA IN SGS     TO TAB-COD-CTAB IN TAB       
              PERFORM BUS-TAB                                           
               MOVE TAB-GLS-DCOR IN TAB    TO WSS-GLS-EDRA-V(WSS-J)     
           ELSE                                                         
               MOVE SPACES                 TO WSS-GLS-EDRA-V(WSS-J).    
       FIN-CRG-VAL-VEC.                                                 
           EXIT.                                                        
                                                                        
                                                                        
       SCRL-TAB-SGS SECTION.                                            
       INI-SCRL-TAB-SGS.                                                
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF5                   
               PERFORM SCRL-TAB-SGS-PF7                                 
               GO TO FIN-SCRL-TAB-SGS.                                  
           IF FRM-FFLD = FRM-FFLD-PF8  OR FRM-FFLD-PF6                  
               PERFORM SCRL-TAB-SGS-PF8                                 
               GO TO FIN-SCRL-TAB-SGS.                                  
       FIN-SCRL-TAB-SGS.                                                
           EXIT.                                                        
                                                                        
       SCRL-TAB-SGS-PF7 SECTION.                                        
       INI-SCRL-TAB-SGS-PF7.                                            
           MOVE SCR-SGS-KEY-ISGS TO SGS-KEY-ISGS IN SGS.                
           MOVE FIO-GET-LEQ TO FIO-CMND.                                
           PERFORM MC2-FIO-SGS.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    SGS-KEY-ISGS IN SGS = SCR-SGS-KEY-ISGS)             
      *        SGS-KEY-ISGS IN SGS = SCR-SGS-KEY-ISGS )                 
               MOVE 'TABCURRNEX' TO MSG-COD-MENS                        
               GO TO MSG-SCRL-TAB-SGS-PF7.                              
       PRV-SCRL-TAB-SGS-PF8.                                            
           MOVE FIO-GET-PRV TO FIO-CMND.                                
           PERFORM MC2-FIO-SGS.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    SGS-CIC-PLAN IN SGS = SCR-SGS-CIC-PLAN AND          
                    SGS-COD-SIST IN SGS = SCR-SGS-COD-SIST AND          
                    SGS-COD-EVEN IN SGS = SCR-SGS-COD-EVEN AND          
                    SGS-IND-TPLT IN SGS = SCR-SGS-IND-TPLT AND          
                    SGS-FEC-PLAN IN SGS = SCR-SGS-FEC-PLAN AND          
                    SGS-HRA-PLAN IN SGS = SCR-SGS-HRA-PLAN )            
               MOVE 'TAB    NOPRV' TO MSG-COD-MENS                      
               GO TO MSG-SCRL-TAB-SGS-PF7.                              
           MOVE FIO-STAT TO PGM-STAT-SGS.                               
           PERFORM SCR-CLRA-SGS.                                        
           PERFORM PUT-SCR.                                             
           GO TO FIN-SCRL-TAB-SGS-PF7.                                  
       MSG-SCRL-TAB-SGS-PF7.                                            
           MOVE 'MC2' TO MSG-COD-SIST.                                  
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MEN2.                               
       FIN-SCRL-TAB-SGS-PF7.                                            
           EXIT.                                                        
                                                                        
       SCRL-TAB-SGS-PF8 SECTION.                                        
       INI-SCRL-TAB-SGS-PF8.                                            
           MOVE SCR-SGS-KEY-ISGS TO SGS-KEY-ISGS IN SGS.                
           MOVE FIO-GET-NLS TO FIO-CMND.                                
           PERFORM MC2-FIO-SGS.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    SGS-KEY-ISGS IN SGS = SCR-SGS-KEY-ISGS)             
               MOVE 'TABCURRNEX' TO MSG-COD-MENS                        
               GO TO MSG-SCRL-TAB-SGS-PF8.                              
       NXT-SCRL-TAB-SGS-PF8.                                            
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           PERFORM MC2-FIO-SGS.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    SGS-CIC-PLAN IN SGS = SCR-SGS-CIC-PLAN AND          
                    SGS-COD-SIST IN SGS = SCR-SGS-COD-SIST AND          
                    SGS-COD-EVEN IN SGS = SCR-SGS-COD-EVEN AND          
                    SGS-IND-TPLT IN SGS = SCR-SGS-IND-TPLT AND          
                    SGS-FEC-PLAN IN SGS = SCR-SGS-FEC-PLAN AND          
                    SGS-HRA-PLAN IN SGS = SCR-SGS-HRA-PLAN )            
               MOVE 'TAB    NONXT' TO MSG-COD-MENS                      
               GO TO MSG-SCRL-TAB-SGS-PF8.                              
           MOVE FIO-STAT TO PGM-STAT-SGS.                               
           PERFORM SCR-CLRA-SGS.                                        
           PERFORM PUT-SCR.                                             
           GO TO FIN-SCRL-TAB-SGS-PF8.                                  
       MSG-SCRL-TAB-SGS-PF8.                                            
           MOVE 'MC2' TO MSG-COD-SIST.                                  
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MEN2.                               
       FIN-SCRL-TAB-SGS-PF8.                                            
           EXIT.                                                        
