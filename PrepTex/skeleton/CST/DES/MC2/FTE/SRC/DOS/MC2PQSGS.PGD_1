       COPY GNSBGTSK.
      *PTC-FIN

      *<<<<
       PGM-EBR-ALL SECTION.                                             
       INI-PGM-EBR-ALL.                                                 
           MOVE 'GNS' TO FIO-SIST.                                      
           MOVE FIO-EBR-ALL TO FIO-CMND.                                
           PERFORM GNS-FIO-TAB.                                         
       FIN-PGM-EBR-ALL.                                                 
           EXIT.                                                        
      *>>>>
      *<<<< EOF_PQ                                                      
       COPY GNSBFMSC.                                                   
       COPY GNSBVCOD.                                                   
       COPY GNSBVTAB.                                                   
       COPY GNSBBTAB.                                                   
       COPY GNSBBMSC.                                                   
       COPY GNSBGVSM.                                                   
       BUS-COR-SGS SECTION.                                             
       INI-BUS-COR-SGS.                                                 
           PERFORM GET-FHOY.                                            
           IF SGS-FEC-PLAN IN SGS-FLD NOT > ZEROES                      
              MOVE HOY-SHOY            TO SGS-NUM-SLAN IN SGS           
              MOVE HOY-AHOY            TO SGS-NUM-ALAN IN SGS           
              MOVE HOY-MHOY            TO SGS-NUM-MLAN IN SGS           
              MOVE HOY-DHOY            TO SGS-NUM-DLAN IN SGS           
              MOVE SGS-NUM-SLAN IN SGS TO SGS-NUM-SLAN IN SGS-FLD       
              MOVE SGS-NUM-ALAN IN SGS TO SGS-NUM-ALAN IN SGS-FLD       
              MOVE SGS-NUM-MLAN IN SGS TO SGS-NUM-MLAN IN SGS-FLD       
              MOVE SGS-NUM-DLAN IN SGS TO SGS-NUM-DLAN IN SGS-FLD.      
                                                                        
           IF SGS-HRA-PLAN IN SGS-FLD NOT > ZEROES                      
              MOVE HOY-HHHY TO SGS-NUM-HHPL IN SGS                      
              MOVE HOY-MMHY TO SGS-NUM-MMPL IN SGS                      
              MOVE HOY-SSHY TO SGS-NUM-SSPL IN SGS                      
              MOVE SGS-HRA-PLAN IN SGS TO SGS-HRA-PLAN IN SGS-FLD.      
                                                                        
           MOVE SGS-NUM-SLAN IN SGS TO WSS-NUM-SLAN IN WSS-VARI.        
           MOVE SGS-NUM-ALAN IN SGS TO WSS-NUM-ALAN IN WSS-VARI.        
           MOVE SGS-NUM-MLAN IN SGS TO WSS-NUM-MLAN IN WSS-VARI.        
           MOVE SGS-NUM-DLAN IN SGS TO WSS-NUM-DLAN IN WSS-VARI.        
                                                                        
           MOVE 999         TO SGS-NUM-ISGS IN SGS.                     
           MOVE FIO-GET-LEQ TO FIO-CMND.                                
           PERFORM MC2-FIO-SGS.                                         
           MOVE FIO-STAT TO PGM-STAT-SGS.                               
           IF SCR-CMND = 'CON'                                          
              IF NOT ( FIO-STAT-OKS AND                                 
                 SGS-CAI-PLAN IN SGS-FLD  = SGS-CAI-PLAN IN SGS AND     
                 SGS-IIC-PLAN IN SGS-FLD  = SGS-IIC-PLAN IN SGS AND     
                 SGS-COD-SIST IN SGS-FLD  = SGS-COD-SIST IN SGS AND     
                 SGS-COD-EVEN IN SGS-FLD  = SGS-COD-EVEN IN SGS AND     
                 SGS-IND-TPLT IN SGS-FLD  = SGS-IND-TPLT IN SGS )       
                 PERFORM KEY-ALL-SGS                                    
                 MOVE ZEROES TO SGS-NUM-ISGS IN SGS                     
              ELSE                                                      
                 MOVE FIO-END-BRW TO FIO-CMND                           
                 PERFORM MC2-FIO-SGS                                    
           ELSE                                                         
           IF NOT ( FIO-STAT-OKS AND                                    
              SGS-CAI-PLAN IN SGS-FLD  = SGS-CAI-PLAN IN SGS AND        
              SGS-IIC-PLAN IN SGS-FLD  = SGS-IIC-PLAN IN SGS AND        
              SGS-COD-SIST IN SGS-FLD  = SGS-COD-SIST IN SGS AND        
              SGS-COD-EVEN IN SGS-FLD  = SGS-COD-EVEN IN SGS AND        
              SGS-IND-TPLT IN SGS-FLD  = SGS-IND-TPLT IN SGS AND        
              WSS-FEC-PLAN IN WSS-VARI = SGS-FEC-PLAN IN SGS AND        
              SGS-HRA-PLAN IN SGS-FLD  = SGS-HRA-PLAN IN SGS )          
              PERFORM KEY-ALL-SGS                                       
              MOVE ZEROES TO SGS-NUM-ISGS IN SGS.                       
                                                                        
           IF SCR-CCPP = 'ING' OR 'VRF'                                 
               ADD 1 TO SGS-NUM-ISGS IN SGS                             
               MOVE SGS-NUM-ISGS IN SGS TO SGS-NUM-ISGS IN SGS-FLD.     
           MOVE 1 TO WSS-ICOR.                                          
                                                                        
      *PVG INI                                                          
      *    IF SCR-CMND = 'ING' AND SGS-NUM-ISGS IN SGS-FLD > 1          
           IF SGS-NUM-ISGS IN SGS-FLD > 1                               
      *PVG FIN                                                          
               MOVE 'MC2'               TO FIO-SIST                     
               MOVE 'DPL'               TO TAB-COD-TTAB IN TAB          
               MOVE SGS-CIC-PLAN IN SGS TO TAB-COD-CTAB IN TAB          
               MOVE FIO-GET-KEY TO FIO-CMND                             
               PERFORM GNS-FIO-TAB                                      
               MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-DES1 IN SGS-FLD      
               MOVE TAB-GLS-DATA IN TAB TO FRM-GLS-DES2 IN SGS-FLD.     
       FIN-BUS-COR-SGS.                                                 
           EXIT.                                                        
                                                                        
       CRG-VEC-SGS SECTION.                                             
       INI-CRG-VEC-SGS.                                                 
           MOVE SGS-NOM-TSSGS TO QUE-COLA.                              
           MOVE QUE-DEL       TO QUE-CMND.                              
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE SGS-NOM-TSSGS-MAS TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE SPACES TO WSS-IND-VIGE-V(2).                            
           MOVE ZEROES TO WSS-J.                                        
           MOVE ZEROES TO WSS-NITM IN WSS-VARI.                         
           IF FRM-IND-SGTE IN SGS-FLD = 'S'                             
              MOVE FIO-GET-NLS  TO FIO-CMND                             
           ELSE                                                         
              MOVE FIO-GET-KEY  TO FIO-CMND.                            
       LUP-CRG-VEC-SGS.                                                 
           PERFORM MC2-FIO-SGS.                                         
           IF NOT (FIO-STAT-OKS AND                                     
                  SGS-CAI-PLAN IN SGS-FLD = SGS-CAI-PLAN IN SGS AND     
                  SGS-IIC-PLAN IN SGS-FLD = SGS-IIC-PLAN IN SGS AND     
                  SGS-COD-SIST IN SGS-FLD = SGS-COD-SIST IN SGS AND     
                  SGS-COD-EVEN IN SGS-FLD = SGS-COD-EVEN IN SGS AND     
                  SGS-IND-TPLT IN SGS-FLD = SGS-IND-TPLT IN SGS AND     
                  SGS-NUM-DLAN IN SGS-FLD = SGS-NUM-DLAN IN SGS AND     
                  SGS-NUM-MLAN IN SGS-FLD = SGS-NUM-MLAN IN SGS AND     
                  SGS-NUM-SLAN IN SGS-FLD = SGS-NUM-SLAN IN SGS AND     
                  SGS-NUM-ALAN IN SGS-FLD = SGS-NUM-ALAN IN SGS AND     
                  SGS-HRA-PLAN IN SGS-FLD = SGS-HRA-PLAN IN SGS )       
              GO TO EXT-CRG-VEC-SGS.                                    
                                                                        
           ADD 1 TO WSS-J.                                              
           IF WSS-J IN WSS-VARI > WSS-JMAX IN WSS-VARI                  
               GO TO EXT-CRG-VEC-SGS.                                   
                                                                        
           PERFORM CLR-SGS-VEC.                                         
           PERFORM CRG-VAL-VEC.                                         
       NXT-CRG-VEC-SGS.                                                 
           IF FRM-IND-SGTE IN SGS-FLD = 'S'                             
              MOVE FIO-GET-NXT TO FIO-CMND                              
              GO TO LUP-CRG-VEC-SGS.                                    
                                                                        
       EXT-CRG-VEC-SGS.                                                 
           IF FIO-STAT-OKS AND FRM-IND-SGTE IN SGS-FLD = 'S'            
              MOVE FIO-END-BRW TO FIO-CMND                              
              PERFORM MC2-FIO-SGS.                                      
                                                                        
           MOVE ZEROES TO WSS-F.                                        
           MOVE ZEROES TO WSS-NITM.                                     
                                                                        
       LUP-CRG-TAB.                                                     
           ADD 1 TO WSS-F IN WSS-VARI.                                  
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI                  
               GO TO GRB-TS-MAS.                                        
           IF WSS-IND-VIGE-V(WSS-F) > SPACES                            
              PERFORM PUT-TAB-SGS                                       
              PERFORM PUT-SGS-QUE                                       
              GO TO LUP-CRG-TAB.                                        
                                                                        
       GRB-TS-MAS.                                                      
