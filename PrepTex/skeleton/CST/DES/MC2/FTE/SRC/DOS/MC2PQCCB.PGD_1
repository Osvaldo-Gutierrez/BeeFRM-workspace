      * OPCION                            CON
      * ARCHIVO BASE                      MC2RCC
      * PANTALLA                          MC2CCB
      * INP/OUT                           DATACOM
      * TAMANO COMMAREA                   250


       ENVIRONMENT DIVISION.
      *=====================
       CONFIGURATION SECTION.
      *----------------------
       SPECIAL-NAMES.
               DECIMAL-POINT IS COMMA.
       DATA DIVISION.
      *==============

       WORKING-STORAGE SECTION.
      *------------------------
      * COPY GNSWGSCR.
       COPY GNSWGCVT.
       77  WSS-TAG-MC2PQCCB-INI PIC X(125) VALUE                        
           '<IDNPGM NOM=$MC2PQCCB$ SKL=$19.04.15$ VSN=$05.11.21.1$        
      -    'GEN=$10-May-19 05:45 PM$ FNC=$MC2-CONVENIO$/>'.    

       COPY GNSWCSCR.
      *PTC-INI
      *PTC-SUB
      * COPY GNSWVSCR.
       COPY GNSWV3CR.
      *PTC-FIN
      *<<<<
           03 SCR-NUM-PANT               PIC 9(0002).                   
           03 SCR-CCB-FLD                PIC X(0250).                   
      *>>>>
       COPY MC2WICCB.

       COPY MC2BRRCC.


       COPY GNSWVIDD.
       COPY GNSWGRQA.
       COPY GNSWGUIB.
       COPY GNSWGELS.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RCC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-TAB-REQA==.
       COPY GNSBRTAB.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSG-REQA==.
       COPY GNSBRMSG.
       COPY GNSWCFIO.
       COPY GNSWVFIO.
      *PTC-INI
      *PTC-SUB
      * ISP  10-MAY-2001 13:46:35 
      * COPY GNSWGFRM.
       COPY GNSWG3CF.
       COPY GNSWGTSK.
       COPY GNSWGQUE.
      *PTC-FIN
       COPY GNSWVNUM.
       COPY GNSWCNUM.
       COPY GNSWGSYS.

      *<<<< WSS                                                         
       COPY MC2WXCCB.                                                   
      *>>>>                                                             
       01  PGM-VARI.
           03 PGM-ICLR                  VALUE ' '       PIC X(01).
           03 PGM-LIMPIAR-TODO          VALUE ' '       PIC X(01).
           03 PGM-LIMPIAR-LLAVES        VALUE 'K'       PIC X(01).
           03 PGM-LIMPIAR-CAMPOS        VALUE 'F'       PIC X(01).
           03 PGM-NO-LIMPIAR-NADA       VALUE 'N'       PIC X(01).
           03 PGM-PROG                                  PIC X(08).
           03 PGM-NTRN                                  PIC X(04).
           03 PGM-STAT.
              05 PGM-STAT-RCC.
                 88 PGM-STAT-RCC-NUL    VALUE '  '.
                 07 PGM-STT1-RCC        VALUE '2' PIC X(01).
                    88 PGM-STAT-RCC-OKS VALUE '0'.
                 07 PGM-STT2-RCC        VALUE '3' PIC X(01).
           03 PGM-SRNF                  VALUE '23'      PIC X(02).
           03 PGM-SOKS                  VALUE '00'      PIC X(02).
           03 PGM-SNUL                  VALUE '  '      PIC X(02).
           03 PGM-SUAR                  VALUE SPACES    PIC X(15).
           03 PGM-CMMA.
              05 PGM-CMMA-CHR OCCURS 3000 PIC X(01).
      *INI-MFC TUXEDO ORACLE
      * LINKAGE SECTION.
       COPY GNSWHLKG.
      *FIN-MFC TUXEDO ORACLE
      *----------------
       01  DFHCOMMAREA.
           02 DFH-CMMA OCCURS 3000 PIC X(01).
       COPY GNSLGFIO.

       PROCEDURE DIVISION.
      *===================

       MAIN SECTION.
       INI-MAIN.
      *INI-MFC TUXEDO ORACLE
           PERFORM GNS-PRO-LIMPIAR-VARIABLES.
      *FIN-MFC TUXEDO ORACLE
           PERFORM INI.
       LUP-MAIN.
           IF SCR-INST = SCR-INST-HCM
                 PERFORM SCR-COM.
           IF SCR-CCPP = 'ACC'
                 PERFORM SCR-ACC
           ELSE
                 MOVE SCR-INST-HCM TO SCR-INST.
           GO TO LUP-MAIN.
       FIN-MAIN.
           EXIT.

       INI SECTION.
       INI-INI.
       COPY GNSBGEIB.
       COPY GNSBGEDB.
           MOVE LOW-VALUES TO CCB-FLD.
           MOVE LOW-VALUES TO CCB-EDT.
           PERFORM GNS-PRO-RCK.
           PERFORM GNS-BUS-VCA.
           MOVE +3000 TO SCR-TCMA.
           PERFORM GNS-HDL-SYS.
           PERFORM GNS-HDL-FRM.
           MOVE 'MC2PQCCB' TO FIO-PROG.
           PERFORM GNS-BUS-IDD.
      *PTC-INI
           PERFORM GNS-ERR-QUE.
           PERFORM GET-TSK-TERM.
      *PTC-FIN
      *<<<< INI_FIO                                                     
           IF SCR-OFRM-SRV                                              
                MOVE TSK-TERM-ALF TO CCB-COD-TERM                       
                MOVE TSK-TERM-ALF TO CCB-COD-TERM-MAS                   
           ELSE                                                         
                MOVE EIBTRMID     TO CCB-COD-TERM                       
                MOVE EIBTRMID     TO CCB-COD-TERM-MAS.                  
                                                                        
           MOVE +3859 TO SCR-TCMA.                                      
           PERFORM GNS-ERR-QUE.                                         
           IF ( FRM-FFLD = FRM-FFLD-PF5 ) AND ( SCR-NUM-PANT = 1 )      
               MOVE SCR-CCB-FLD TO CCB-FLD                              
               GO TO FIN-INI.                                           
                                                                        
           IF ( FRM-FFLD = FRM-FFLD-PF6 )                               
               MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM        
               ADD 1              TO QUE-NITM                           
               MOVE CCB-NOM-TSCCB TO QUE-COLA                           
               MOVE CCB-LEN-TSCCB TO QUE-LITM                           
               MOVE QUE-GET       TO QUE-CMND                           
               PERFORM GNS-PRO-QUE                                      
               IF QUE-STAT NOT = QUE-STAT-OKS                           
                  MOVE SCR-CCB-FLD TO CCB-FLD                           
                  GO TO FIN-INI.                                        
      *>>>>                                                             
       FIN-INI.
           EXIT.

       COPY GNSBHSYS.
       COPY GNSBHFRM.
       COPY GNSBPRCK.
       GNS-BUS-VCA SECTION.
       INI-GNS-BUS-VCA.
           MOVE 0 TO FRM-IFLD.
       LUP-GNS-BUS-VCA.
           ADD 1 TO FRM-IFLD.
           IF FRM-IFLD NOT > EIBCALEN AND
              FRM-IFLD NOT > 3000
                MOVE DFH-CMMA(FRM-IFLD) TO PGM-CMMA-CHR(FRM-IFLD)
                GO TO LUP-GNS-BUS-VCA.
           MOVE PGM-CMMA TO SCR-VARI.
       FIN-GNS-BUS-VCA.
           EXIT.
       COPY GNSBBIDD.

       SCR-COM SECTION.
       INI-SCR-COM.
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-CLRA-CCB.
           PERFORM EDT-KEY-CCB.
           PERFORM EDT-FLD-CCB.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-HCM TO SCR-INST.
           MOVE SCR-CMNP     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       XCT-ERR.
           MOVE 'XCTERR    ' TO MSG-COD-MENS.
           MOVE 'GNS'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO SYS-MEN1.
           MOVE SYS-PROG     TO SYS-MEN2.
           MOVE SYS-ABOR     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       COPY GNSBGGBK.
       FIN-SCR-COM.
           EXIT.



       SCR-ACC SECTION.
       INI-SCR-ACC.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ACC
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-ACC.
           PERFORM DESSCRTRL.
      *<<<< PFS_SCR_ACC                                                 
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6                   
               PERFORM VAL-NUM-KEY-CCB                                  
               PERFORM VAL-NUM-FLD-CCB                                  
               PERFORM SCRL-TAB-CCB                                     
               PERFORM SCR-TRL.                                         
      *>>>>                                                             
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ACC.
           EXIT.

       BRW-SCR-ACC SECTION.
       INI-BRW-SCR-ACC.
       FIN-BRW-SCR-ACC.
           EXIT.




       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               PERFORM SCR-CLRA-CCB
               PERFORM EDT-KEY-CCB
               PERFORM EDT-FLD-CCB
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-CCB
               PERFORM EDT-KEY-CCB
           ELSE
               PERFORM CLR-FLD-CCB
               PERFORM EDT-FLD-CCB.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

       SCR-KEY SECTION.
       INI-SCR-KEY.
           PERFORM SCR-RST.
           PERFORM GET-KEY.
           IF FRM-STAT-ABT
                   MOVE SPACES TO FRM-MENS
                   PERFORM SCR-MSSG
                   GO TO FIN-SCR-KEY.
       FND-SCR-KEY.
           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
           IF FIO-STAT-OKS
               PERFORM PUT-SCR
               MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
               GO TO FIN-SCR-KEY.
      *JSS ELSE
      *        PERFORM CLR-FLD-CCB.
       ERR-SCR-KEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR.
           IF FRM-MENS NOT > SPACES
               IF FIO-STAT-EOF
                   MOVE 'REGEOF    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
               IF FIO-STAT = FIO-STAT-NEX
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
           MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-SCR-KEY.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO CCB-FLD.
           MOVE LOW-VALUES TO CCB-EDT.
       FIN-SCR-RST.
           EXIT.

       GET-KEY SECTION.
       INI-GET-KEY.
           PERFORM SCR-GKEY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-KEY.
           EXIT.

       GET-IKY SECTION.
       INI-GET-IKY.
           PERFORM SCR-GIKY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-IKY.
           EXIT.

       FND-KEY SECTION.
       INI-FND-KEY.
      *<<<< FND_KEY                                                     
           PERFORM KEY-ALL-CCB.                                         
           GO TO CON-FND-KEY.                                           
      *>>>>                                                             
           PERFORM KEY-ALL-CCB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM MC2-FIO-RCC.
           MOVE FIO-STAT TO PGM-STAT-RCC.
      *<<<< FND_KEY                                                     
       CON-FND-KEY.                                                     
           PERFORM LEE-ALL-RCC.                                         
                                                                        
           PERFORM PUT-TAB.                                             
                                                                        
           MOVE '00' TO FIO-STAT.                                       
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           GO TO FIN-FND-KEY.                                           
       MSG-FND-KEY.                                                     
           MOVE 'TAB'        TO MSG-COD-SIST.                           
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MENS.                               
           MOVE '00'         TO FIO-STAT.                               
           MOVE SCR-STAT-CON TO SCR-STAT.                               
      *>>>>                                                             
       FIN-FND-KEY.
           IF FIO-STAT = FIO-STAT-NEX
               MOVE 'S' TO SCR-IERR.
       EXT-FND-KEY.
           EXIT.
 
       PUT-SCR SECTION.
       INI-PUT-SCR.
           PERFORM PUT-ALL-CCB.
       FIN-PUT-SCR.
           EXIT.

       STP-EXIT SECTION.
       INI-STP-EXIT.
           MOVE SCR-PROG TO PGM-PROG.
           MOVE SCR-NTRN TO PGM-NTRN.

           MOVE SCR-PROV TO SCR-PROG.
           MOVE SCR-NTRV TO SCR-NTRN.

           MOVE PGM-PROG TO SCR-PROV
           MOVE PGM-NTRN TO SCR-NTRV.
       FIN-STP-EXIT.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO SYS-NTRN.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
      *PTC-INI
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE CCB-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO CCB-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GKEY SECTION.
       INI-SCR-GKEY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-CCB
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO CCB-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GKEY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-CCB.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-CCB
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-CCB.
           PERFORM VAL-IKY-CCB.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-MKY-CCB.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-AKY-CCB.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-CON-KEY-CCB.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GKEY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-CCB.
       ERR-SCR-GKEY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-CCB.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GKEY.
           PERFORM SCR-MSSG.

       SCR-GIKY SECTION.
       INI-SCR-GIKY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-CCB
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO CCB-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GIKY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-CCB.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-IKY-CCB
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-CCB.
           PERFORM VAL-IKY-CCB.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GIKY.
           PERFORM VAL-CON-IKY-CCB.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GIKY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-CCB.
       ERR-SCR-GIKY.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-IKY-CCB.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GIKY.
           PERFORM SCR-MSSG.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
      *<<<< PFS_GET_CON                                                 
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )           
      *>>>>                                                             
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO CCB-GLS-MEN2
               MOVE SCR-PRMT     TO CCB-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO CCB-FLD.
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO CCB-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE CCB-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO CCB-FLD
      * FLB  7-NOV-1994
               PERFORM CLR-KEY-CCB

               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO CCB-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO CCB-GLS-MENS.
           PERFORM PRO-MKY-CCB.
           PERFORM PRO-AKY-CCB.
           PERFORM UNP-IKY-CCB.
           PERFORM FST-KEY-CCB.
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE CCB-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO CCB-FLD
               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO CCB-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO CCB-GLS-MENS.
           PERFORM UNP-KEY-CCB.
           PERFORM FST-KEY-CCB.
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
           PERFORM PRO-IKY-CCB.
           PERFORM PRO-AKY-CCB.
           PERFORM UNP-MKY-CCB.
           PERFORM UNP-FLD-CCB.
           PERFORM FST-FLD-CCB.
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO CCB-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-CCB.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
      *PTC-INI
           IF SCR-STAT NOT = SCR-STPP
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-CCB
               PERFORM EDT-FLD-CCB
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-CCB
               PERFORM EDT-FLD-CCB
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-CCB.
           PERFORM EDT-FLD-CCB.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

      * PGM-EBR-ALL SECTION.
      * INI-PGM-EBR-ALL.
      *     MOVE 'GNS' TO FIO-SIST.
      *     MOVE FIO-EBR-ALL TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
      * FIN-PGM-EBR-ALL.
      *     EXIT.

       COPY MC2BQCCB.
       COPY GNSBVNUM.
       COPY MC2BFRCC.
       COPY GNSBFMSG.
       COPY GNSBGSYS.
       COPY GNSBGDTC.
      *PTC-INI
      *PTC-SUB
      * ISP  10-MAY-2001 13:46:35 
      * COPY GNSBGFRM.
       COPY GNSBG3CF.
      *PTC-FIN
       COPY GNSBGMSG.
       COPY GNSBEKEY.
       COPY GNSBIABT.
       COPY GNSBFTAB.
      *PTC-INI
       COPY GNSBGQUE.
       COPY GNSBGTSK.
      *PTC-FIN

      *<<<<
       PGM-EBR-ALL SECTION.                                             
       INI-PGM-EBR-ALL.                                                 
           MOVE 'GNS' TO FIO-SIST.                                      
           MOVE FIO-EBR-ALL TO FIO-CMND.                                
           PERFORM GNS-FIO-TAB.                                         
       FIN-PGM-EBR-ALL.                                                 
           EXIT.                                                        
                                                                        
       COPY GNSBBTAB.                                                   
       COPY GNSBVTAB.                                                   
       COPY GNSBVCOD.                                                   
                                                                        
       LEE-ALL-RCC SECTION.                                             
       INI-LEE-ALL-RCC.                                                 
           MOVE SPACES      TO MSG-COD-MENS.                            
           MOVE SPACES      TO RCC-KEY-IREG IN RCC.                     
      *PVG INI 03-OCT-2011 RESPUESTA NO HABILITADA CI&P
           IF SCR-OFRM-SRV
              GO TO FIN-LEE-ALL-RCC.
      *PVG FIN 03-OCT-2011
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-LEE-ALL-RCC.                                                 
           PERFORM MC2-FIO-RCC.                                         
           IF NOT FIO-STAT-OKS                                          
              GO TO EXT-LEE-ALL-RCC.                                    
           IF RCC-COD-BANC IN CCB-FLD > SPACES AND                      
              RCC-COD-BANC IN RCC NOT = RCC-COD-BANC IN CCB-FLD         
              GO TO NXT-LEE-ALL-RCC.                                    
                                                                        
           IF RCC-MSC-STAT IN RCC NOT = 'A'                             
              GO TO NXT-LEE-ALL-RCC.                                    
                                                                        
           IF RCC-COD-BANC IN RCC NOT > SPACES                          
              MOVE 'SIN' TO RCC-COD-BANC IN RCC.                        
           PERFORM CRG-VEC-CCB.                                         
           IF RCC-COD-BANC IN RCC = 'SIN'                               
              MOVE SPACES TO RCC-COD-BANC IN RCC.                       
                                                                        
       NXT-LEE-ALL-RCC.                                                 
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-LEE-ALL-RCC.                                       
       EXT-LEE-ALL-RCC.                                                 
           IF FIO-STAT-OKS                                              
              MOVE FIO-END-BRW TO FIO-CMND                              
              PERFORM MC2-FIO-RCC.                                      
       FIN-LEE-ALL-RCC.                                                 
           EXIT.                                                        
                                                                        
       CRG-VEC-CCB SECTION.                                             
       INI-CRG-VEC-CCB.                                                 
           MOVE 1 TO WSS-F IN WSS-VARI.                                 
       LUP-CRG-VEC-CCB.                                                 
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI                  
              GO TO FIN-CRG-VEC-CCB.                                    
           IF WSS-COD-BANC-V(WSS-F) NOT > SPACES                        
              GO TO CON-CRG-VEC-CCB.                                    
           IF WSS-COD-BANC-V(WSS-F) = RCC-COD-BANC IN RCC               
              GO TO CON-CRG-VEC-CCB-2.                                  
           ADD 1 TO WSS-F.                                              
           GO TO LUP-CRG-VEC-CCB.                                       
       CON-CRG-VEC-CCB.                                                 
           PERFORM CLR-CCB-VEC.                                         
                                                                        
           MOVE RCC-COD-BANC IN RCC TO WSS-COD-BANC-V(WSS-F).           
                                                                        
       CON-CRG-VEC-CCB-2.                                               
           ADD 1 TO WSS-NUM-TBAN-V(WSS-F).                              
           ADD 1 TO WSS-NUM-TOTA IN WSS-VARI.                           
           IF RCC-COD-CPTO IN RCC = 'INT'                               
              ADD 1 TO WSS-NUM-CINT-V(WSS-F)                            
              ADD 1 TO WSS-NUM-TINT IN WSS-VARI                         
           ELSE                                                         
           IF RCC-COD-CPTO IN RCC = 'SGS'                               
              ADD 1 TO WSS-NUM-CSEG-V(WSS-F)                            
              ADD 1 TO WSS-NUM-TSEG IN WSS-VARI                         
           ELSE                                                         
           IF RCC-COD-CPTO IN RCC = 'COM'                               
              ADD 1 TO WSS-NUM-CCOM-V(WSS-F)                            
              ADD 1 TO WSS-NUM-TCOM IN WSS-VARI                         
           ELSE                                                         
           IF RCC-COD-CPTO IN RCC = 'GAS'                               
              ADD 1 TO WSS-NUM-CGAS-V(WSS-F)                            
              ADD 1 TO WSS-NUM-TGAS IN WSS-VARI                         
           ELSE                                                         
           IF RCC-COD-CPTO IN RCC = 'IMP'                               
              ADD 1 TO WSS-NUM-CIMP-V(WSS-F)                            
              ADD 1 TO WSS-NUM-TIMP IN WSS-VARI                         
           ELSE                                                         
              ADD 1 TO WSS-NUM-COTR-V(WSS-F)                            
              ADD 1 TO WSS-NUM-TOTR IN WSS-VARI.                        
       FIN-CRG-VEC-CCB.                                                 
           EXIT.                                                        
                                                                        
       CLR-CCB-VEC SECTION.                                             
       INI-CLR-CCB-VEC.                                                 
           MOVE SPACES TO WSS-COD-BANC-V(WSS-F).                        
           MOVE ZEROES TO WSS-NUM-CINT-V(WSS-F).                        
           MOVE ZEROES TO WSS-NUM-CSEG-V(WSS-F).                        
           MOVE ZEROES TO WSS-NUM-CCOM-V(WSS-F).                        
           MOVE ZEROES TO WSS-NUM-CGAS-V(WSS-F).                        
           MOVE ZEROES TO WSS-NUM-CIMP-V(WSS-F).                        
           MOVE ZEROES TO WSS-NUM-COTR-V(WSS-F).                        
           MOVE ZEROES TO WSS-NUM-TBAN-V(WSS-F).                        
       FIN-CLR-CCB-VEC.                                                 
           EXIT.                                                        
                                                                        
                                                                        
       PUT-TAB SECTION.                                                 
       INI-PUT-TAB.                                                     
           MOVE CCB-NOM-TSCCB TO QUE-COLA.                              
           MOVE QUE-DEL       TO QUE-CMND.                              
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE CCB-NOM-TSCCB-MAS TO QUE-COLA.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE 1          TO SCR-NUM-PANT.                             
           MOVE LOW-VALUES TO WSS-COD-BANC-IDX.                         
      *                                                                 
           MOVE 1       TO FRM-IFLD.                                    
           MOVE ZEROES  TO WSS-NITM IN WSS-VARI.                        
                                                                        
           MOVE 1 TO WSS-F IN WSS-VARI.                                 
       LUP-PUT-TAB.                                                     
           IF WSS-F IN WSS-VARI > WSS-FMAX IN WSS-VARI                  
              GO TO FIN-PUT-TAB.                                        
           IF WSS-COD-BANC-V(WSS-F) > SPACES                            
              PERFORM PUT-TAB-CCB                                       
              PERFORM PUT-CCB-QUE                                       
              PERFORM SET-FRM-IFLD                                      
           ELSE                                                         
           IF FRM-IFLD = 1 AND WSS-F = 1                                
              GO TO MSG-PUT-TAB                                         
           ELSE                                                         
              MOVE '00' TO FIO-STAT                                     
              GO TO FIN-PUT-TAB.                                        
                                                                        
           ADD 1 TO WSS-F IN WSS-VARI.                                  
           GO TO LUP-PUT-TAB.                                           
       MSG-PUT-TAB.                                                     
           MOVE 'TAB'        TO MSG-COD-SIST.                           
           MOVE 'CRC    NEX' TO MSG-COD-MENS.                           
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MENS.                               
           MOVE '00'         TO FIO-STAT.                               
      *    MOVE 'S'          TO SCR-IERR.                               
       FIN-PUT-TAB.                                                     
           MOVE 1                  TO QUE-NITM.                         
           MOVE CCB-LEN-TSCCB-MAS  TO QUE-LITM.                         
           MOVE CCB-NOM-TSCCB-MAS  TO QUE-COLA.                         
                                                                        
           IF WSS-NITM > WSS-NITMMAX                                    
              MOVE 'S'           TO CCB-MAS-IND-HMAS                    
           ELSE                                                         
              MOVE 'N'           TO CCB-MAS-IND-HMAS.                   
                                                                        
           MOVE WSS-NITM         TO WSS-NUM-ICUR.                       
           MOVE WSS-NUM-ICUR     TO WSS-NUM-ICUR-EDT.                   
           MOVE WSS-NUM-ICUR-EDT TO WSS-NUM-ICUR-ALF.                   
           MOVE WSS-NUM-ICUR-ALF TO CCB-MAS-NUM-TOCC.                   
           MOVE CCB-MAS-ITEM     TO QUE-ITEM.                           
           MOVE QUE-PUT          TO QUE-CMND.                           
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           IF WSS-NITM > WSS-IMAX                                       
               MOVE 'TAB'        TO MSG-COD-SIST                        
               MOVE 'CCB   DMAS' TO MSG-COD-MENS                        
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN2.                           
                                                                        
      *TOTALES                                                          
           MOVE WSS-NUM-TINT IN WSS-VARI TO FRM-NUM-TINT IN CCB-FLD.    
           MOVE WSS-NUM-TSEG IN WSS-VARI TO FRM-NUM-TSEG IN CCB-FLD.    
           MOVE WSS-NUM-TCOM IN WSS-VARI TO FRM-NUM-TCOM IN CCB-FLD.    
           MOVE WSS-NUM-TGAS IN WSS-VARI TO FRM-NUM-TGAS IN CCB-FLD.    
           MOVE WSS-NUM-TIMP IN WSS-VARI TO FRM-NUM-TIMP IN CCB-FLD.    
           MOVE WSS-NUM-TOTR IN WSS-VARI TO FRM-NUM-TOTR IN CCB-FLD.    
           MOVE WSS-NUM-TOTA IN WSS-VARI TO FRM-NUM-TOTA IN CCB-FLD.    
       EXT-PUT-TAB.                                                     
           EXIT.                                                        
                                                                        
       SET-FRM-IFLD SECTION.                                            
       INI-SET-FRM-IFLD.                                                
           IF SCR-OFRM-SRV                                              
                MOVE 1 TO FRM-IFLD                                      
           ELSE                                                         
                ADD 1 TO FRM-IFLD.                                      
       FIN-SET-FRM-IFLD.                                                
           EXIT.                                                        
      *--------------------------------------------                     
       SCRL-TAB-CCB SECTION.                                            
       INI-SCRL-TAB-CCB.                                                
           MOVE SPACES TO MSG-COD-MENS.                                 
                                                                        
           IF FRM-FFLD = FRM-FFLD-PF6                                   
               PERFORM PUT-TAB-ASC                                      
           ELSE                                                         
               PERFORM PUT-TAB-DSC.                                     
           IF MSG-COD-MENS > SPACES                                     
               MOVE 'TAB' TO MSG-COD-SIST                               
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN2.                           
       FIN-SCRL-TAB-CCB.                                                
           EXIT.                                                        
                                                                        
       PUT-TAB-CCB SECTION.                                             
       INI-PUT-TAB-CCB.                                                 
           PERFORM CLR-WSS.                                             
           MOVE WSS-COD-BANC-V(WSS-F) TO WSS-COD-BANC IN WSS-VARI.      
           MOVE WSS-NUM-CINT-V(WSS-F) TO WSS-NUM-CINT IN WSS-VARI.      
           MOVE WSS-NUM-CSEG-V(WSS-F) TO WSS-NUM-CSEG IN WSS-VARI.      
           MOVE WSS-NUM-CCOM-V(WSS-F) TO WSS-NUM-CCOM IN WSS-VARI.      
           MOVE WSS-NUM-CGAS-V(WSS-F) TO WSS-NUM-CGAS IN WSS-VARI.      
           MOVE WSS-NUM-CIMP-V(WSS-F) TO WSS-NUM-CIMP IN WSS-VARI.      
           MOVE WSS-NUM-COTR-V(WSS-F) TO WSS-NUM-COTR IN WSS-VARI.      
           MOVE WSS-NUM-TBAN-V(WSS-F) TO WSS-NUM-TBAN IN WSS-VARI.      
           IF WSS-COD-BANC IN WSS-VARI = 'SIN'                          
              MOVE 'SIN BANCA'              TO WSS-GLS-NBAN IN WSS-VARI 
           ELSE                                                         
              MOVE 'TAB'                    TO TAB-COD-SIST             
              MOVE 'BAN'                    TO TAB-COD-TTAB IN TAB      
              MOVE WSS-COD-BANC IN WSS-VARI TO TAB-COD-CTAB IN TAB      
              PERFORM BUS-TAB                                           
              MOVE TAB-GLS-DESC             TO WSS-GLS-NBAN IN WSS-VARI.
                                                                        
           IF FRM-IFLD > WSS-IMAX                                       
              GO TO FIN-PUT-TAB-CCB.                                    
                                                                        
           MOVE WSS-COD-BANC IN WSS-VARI TO                             
                FRM-COD-BANC IN CCB-FLD(FRM-IFLD).                      
           MOVE WSS-NUM-CINT IN WSS-VARI TO                             
                FRM-NUM-CINT IN CCB-FLD(FRM-IFLD).                      
           MOVE WSS-NUM-CSEG IN WSS-VARI TO                             
                FRM-NUM-CSEG IN CCB-FLD(FRM-IFLD).                      
           MOVE WSS-NUM-CCOM IN WSS-VARI TO                             
                FRM-NUM-CCOM IN CCB-FLD(FRM-IFLD).                      
           MOVE WSS-NUM-CGAS IN WSS-VARI TO                             
                FRM-NUM-CGAS IN CCB-FLD(FRM-IFLD).                      
           MOVE WSS-NUM-CIMP IN WSS-VARI TO                             
                FRM-NUM-CIMP IN CCB-FLD(FRM-IFLD).                      
           MOVE WSS-NUM-COTR IN WSS-VARI TO                             
                FRM-NUM-COTR IN CCB-FLD(FRM-IFLD).                      
           MOVE WSS-NUM-TBAN IN WSS-VARI TO                             
                FRM-NUM-TBAN IN CCB-FLD(FRM-IFLD).                      
                                                                        
           MOVE WSS-GLS-NBAN IN WSS-VARI TO                             
                FRM-GLS-NBAN IN CCB-FLD(FRM-IFLD).                      
       FIN-PUT-TAB-CCB.                                                 
           EXIT.                                                        
      *----------------------------------------------------             
       PUT-CCB-QUE SECTION.                                             
       INI-PUT-CCB-QUE.                                                 
           ADD 1                  TO WSS-NITM.                          
           MOVE CCB-NOM-TSCCB     TO QUE-COLA.                          
           MOVE CCB-LEN-TSCCB     TO QUE-LITM.                          
           MOVE WSS-NITM          TO QUE-NITM.                          
           MOVE SPACES            TO QUE-ITEM.                          
           MOVE WSS-COD-BANC-IDX  TO QUE-ITEM                           
           MOVE QUE-PUT           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
       FIN-PUT-CCB-QUE.                                                 
           EXIT.                                                        
      *----------------------------------------------------             
                                                                        
       PUT-TAB-ASC SECTION.                                             
       INI-PUT-TAB-ASC.                                                 
           MOVE SPACES TO MSG-COD-MENS.                                 
           MULTIPLY SCR-NUM-PANT BY WSS-IMAX GIVING QUE-NITM.           
           MOVE 0 TO FRM-IFLD.                                          
       LUP-PUT-TAB-ASC.                                                 
           ADD 1              TO QUE-NITM.                              
           MOVE CCB-NOM-TSCCB TO QUE-COLA.                              
           MOVE CCB-LEN-TSCCB TO QUE-LITM.                              
           MOVE QUE-GET       TO QUE-CMND.                              
           PERFORM GNS-PRO-QUE.                                         
           IF QUE-STAT NOT = QUE-STAT-OKS                               
               IF FRM-IFLD = 0                                          
                   MOVE 'CCB   NONXT' TO MSG-COD-MENS                   
                   GO TO FIN-PUT-TAB-ASC                                
               ELSE                                                     
                   GO TO FIN-PUT-TAB-ASC.                               
           ADD 1 TO FRM-IFLD.                                           
           MOVE QUE-ITEM TO FRM-COD-BANC-IDX(FRM-IFLD).                 
           IF FRM-IFLD < WSS-IMAX                                       
               GO TO LUP-PUT-TAB-ASC.                                   
       FIN-PUT-TAB-ASC.                                                 
           IF FRM-IFLD > 0 AND FRM-IFLD < WSS-IMAX                      
               ADD 1 FRM-IFLD GIVING WSS-I                              
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY 1            
                                 UNTIL WSS-I > WSS-IMAX.                
           IF FRM-IFLD > 0                                              
               ADD 1 TO SCR-NUM-PANT.                                   
           IF FRM-IFLD = WSS-IMAX                                       
               ADD 1              TO QUE-NITM                           
               MOVE CCB-NOM-TSCCB TO QUE-COLA                           
               MOVE CCB-LEN-TSCCB TO QUE-LITM                           
               MOVE QUE-GET       TO QUE-CMND                           
               PERFORM GNS-PRO-QUE                                      
               IF QUE-STAT = QUE-STAT-OKS                               
                   MOVE 'TAB'        TO MSG-COD-SIST                    
                   MOVE 'CCB   DMAS' TO MSG-COD-MENS.                   
       EXT-PUT-TAB-ASC.                                                 
           EXIT.                                                        
                                                                        
       PUT-TAB-DSC SECTION.                                             
       INI-PUT-TAB-DSC.                                                 
           MOVE SPACES TO MSG-COD-MENS.                                 
           IF SCR-NUM-PANT = 1                                          
               MOVE 'CCB   NOPRV' TO MSG-COD-MENS                       
               GO TO EXT-PUT-TAB-DSC.                                   
                                                                        
           SUBTRACT 1 FROM SCR-NUM-PANT GIVING QUE-NITM.                
           MULTIPLY QUE-NITM BY WSS-IMAX GIVING QUE-NITM.               
           ADD 1 TO QUE-NITM.                                           
           ADD 1 WSS-IMAX GIVING FRM-IFLD.                              
       LUP-PUT-TAB-DSC.                                                 
           SUBTRACT  1 FROM QUE-NITM.                                   
           MOVE CCB-NOM-TSCCB TO QUE-COLA                               
           MOVE CCB-LEN-TSCCB TO QUE-LITM                               
           MOVE QUE-GET       TO QUE-CMND.                              
           PERFORM GNS-PRO-QUE.                                         
           IF QUE-STAT NOT = QUE-STAT-OKS                               
               IF FRM-IFLD > WSS-IMAX                                   
                   MOVE 'CCB   NOPRV' TO MSG-COD-MENS                   
                   GO TO FIN-PUT-TAB-DSC                                
               ELSE                                                     
                   GO TO FIN-PUT-TAB-DSC.                               
           SUBTRACT 1 FROM FRM-IFLD.                                    
           MOVE QUE-ITEM TO FRM-COD-BANC-IDX(FRM-IFLD).                 
           IF FRM-IFLD > 1                                              
               GO TO LUP-PUT-TAB-DSC.                                   
       FIN-PUT-TAB-DSC.                                                 
                                                                        
           IF FRM-IFLD NOT > WSS-IMAX AND FRM-IFLD > 1                  
               SUBTRACT 1 FROM FRM-IFLD GIVING WSS-I                    
               PERFORM CLR-FRM VARYING WSS-I FROM WSS-I BY -1           
                                 UNTIL WSS-I < 1.                       
                                                                        
           IF FRM-IFLD NOT > WSS-IMAX                                   
               SUBTRACT 1 FROM SCR-NUM-PANT                             
               MOVE 'TAB'        TO MSG-COD-SIST                        
               MOVE 'CCB   DMAS' TO MSG-COD-MENS.                       
       EXT-PUT-TAB-DSC.                                                 
           EXIT.                                                        
                                                                        
      *------------------------------------------------------           
       CLR-FRM SECTION.                                                 
       INI-CLR-FRM.                                                     
           MOVE SPACES TO FRM-COD-BANC     IN CCB-FLD(WSS-I).           
           MOVE SPACES TO FRM-GLS-NBAN     IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-CINT     IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-CSEG     IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-CCOM     IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-CGAS     IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-CIMP     IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-COTR     IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-TBAN     IN CCB-FLD(WSS-I).           
                                                                        
           MOVE 'P'    TO FRM-NUM-CGAS-ATR IN CCB-FLD(WSS-I).           
           MOVE 'P'    TO FRM-NUM-CSEG-ATR IN CCB-FLD(WSS-I).           
           MOVE 'P'    TO FRM-NUM-COTR-ATR IN CCB-FLD(WSS-I).           
           MOVE 'P'    TO FRM-NUM-CINT-ATR IN CCB-FLD(WSS-I).           
           MOVE 'P'    TO FRM-GLS-NBAN-ATR IN CCB-FLD(WSS-I).           
           MOVE 'P'    TO FRM-COD-BANC-ATR IN CCB-FLD(WSS-I).           
           MOVE 'P'    TO FRM-NUM-CIMP-ATR IN CCB-FLD(WSS-I).           
           MOVE 'P'    TO FRM-NUM-CCOM-ATR IN CCB-FLD(WSS-I).           
           MOVE 'P'    TO FRM-NUM-TBAN-ATR IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-CGAS-LEN IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-CCOM-LEN IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-COTR-LEN IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-CSEG-LEN IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-CINT-LEN IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-GLS-NBAN-LEN IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-COD-BANC-LEN IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-CIMP-LEN IN CCB-FLD(WSS-I).           
           MOVE ZEROES TO FRM-NUM-TBAN-LEN IN CCB-FLD(WSS-I).           
       FIN-CLR-FRM.                                                     
           EXIT.                                                        
                                                                        
       CLR-WSS SECTION.                                                 
       INI-CLR-WSS.                                                     
           MOVE SPACES TO WSS-COD-BANC IN WSS-VARI.                     
           MOVE SPACES TO WSS-GLS-NBAN IN WSS-VARI.                     
           MOVE ZEROES TO WSS-NUM-CINT IN WSS-VARI.                     
           MOVE ZEROES TO WSS-NUM-CSEG IN WSS-VARI.                     
           MOVE ZEROES TO WSS-NUM-CCOM IN WSS-VARI.                     
           MOVE ZEROES TO WSS-NUM-CGAS IN WSS-VARI.                     
           MOVE ZEROES TO WSS-NUM-CIMP IN WSS-VARI.                     
           MOVE ZEROES TO WSS-NUM-COTR IN WSS-VARI.                     
           MOVE ZEROES TO WSS-NUM-TBAN IN WSS-VARI.                     
                                                                        
           MOVE 'P'    TO WSS-NUM-CGAS-ATR.                             
           MOVE 'P'    TO WSS-NUM-CSEG-ATR.                             
           MOVE 'P'    TO WSS-NUM-COTR-ATR.                             
           MOVE 'P'    TO WSS-NUM-CINT-ATR.                             
           MOVE 'P'    TO WSS-GLS-NBAN-ATR.                             
           MOVE 'P'    TO WSS-COD-BANC-ATR.                             
           MOVE 'P'    TO WSS-NUM-CIMP-ATR.                             
           MOVE 'P'    TO WSS-NUM-CCOM-ATR.                             
           MOVE 'P'    TO WSS-NUM-TBAN-ATR.                             
           MOVE ZEROES TO WSS-NUM-CGAS-LEN.                             
           MOVE ZEROES TO WSS-NUM-CCOM-LEN.                             
           MOVE ZEROES TO WSS-NUM-COTR-LEN.                             
           MOVE ZEROES TO WSS-NUM-CSEG-LEN.                             
           MOVE ZEROES TO WSS-NUM-CINT-LEN.                             
