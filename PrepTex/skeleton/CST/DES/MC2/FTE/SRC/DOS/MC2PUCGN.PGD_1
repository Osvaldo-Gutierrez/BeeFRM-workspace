       PROCEDURE DIVISION.
      *===================

       MAIN SECTION.
       INI-MAIN.
      *INI-MFC TUXEDO ORACLE
           PERFORM GNS-PRO-LIMPIAR-VARIABLES.
      *FIN-MFC TUXEDO ORACLE
           PERFORM INI.
       LUP-MAIN.
           IF SCR-INST = SCR-INST-HCM
                 PERFORM SCR-COM.
           IF SCR-CCPP = 'MOD'
                 PERFORM SCR-MOD
           ELSE
           IF SCR-CCPP = 'ING'
                 PERFORM SCR-ING
           ELSE
           IF SCR-CCPP = 'ACC'
                 PERFORM SCR-ACC
           ELSE
           IF SCR-CCPP = 'ELI'
                 PERFORM SCR-ELI
           ELSE
           IF SCR-CCPP = 'ACT'
                 PERFORM SCR-ACT
           ELSE
           IF SCR-CCPP = 'PGM'
                 PERFORM SCR-PGM
           ELSE
                 MOVE SCR-INST-HCM TO SCR-INST.
           GO TO LUP-MAIN.
       FIN-MAIN.
           EXIT.

       INI SECTION.
       INI-INI.
       COPY GNSBGEIB.
       COPY GNSBGEDB.
           MOVE LOW-VALUES TO CGN-FLD.
           MOVE LOW-VALUES TO CGN-EDT.
           PERFORM GNS-PRO-RCK.
           PERFORM GNS-BUS-VCA.
           MOVE +3000 TO SCR-TCMA.
           PERFORM GNS-HDL-SYS.
           PERFORM GNS-HDL-FRM.
           MOVE 'MC2PUCGN' TO FIO-PROG.
           PERFORM GNS-BUS-IDD.
      *PTC-INI
           PERFORM GNS-ERR-QUE.
           PERFORM GET-TSK-TERM.
      *PTC-FIN
      *<<<< INI_FIO
           IF SCR-OFRM-SRV
                MOVE TSK-TERM-ALF TO DGN-COD-TERM
                MOVE TSK-TERM-ALF TO DGN-COD-TERM-MAS
           ELSE
                MOVE EIBTRMID     TO DGN-COD-TERM
                MOVE EIBTRMID     TO DGN-COD-TERM-MAS.
      *>>>>
       FIN-INI.
           EXIT.

       COPY GNSBHSYS.
       COPY GNSBHFRM.
       COPY GNSBPRCK.
       COPY GNSBBVCA.
       COPY GNSBBIDD.

       SCR-COM SECTION.
       INI-SCR-COM.
           PERFORM STP-EXIT.
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-CLRA-CGN.
           PERFORM EDT-KEY-CGN.
           PERFORM EDT-FLD-CGN.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-HCM TO SCR-INST.
           MOVE SCR-CMNP     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       XCT-ERR.
           MOVE 'XCTERR    ' TO MSG-COD-MENS.
           MOVE 'GNS'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO SYS-MEN1.
           MOVE SYS-PROG     TO SYS-MEN2.
           MOVE SYS-ABOR     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       COPY GNSBGGBK.
       FIN-SCR-COM.
           EXIT.

       SCR-MOD SECTION.
       INI-SCR-MOD.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
               PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
               PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8
               PERFORM XCT-PRGP.
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
               GO TO MSG-SCR-MOD.
       MSG-SCR-MOD.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'MODNOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-MOD
               IF FIO-STAT-OKS
                   IF FRM-MEN1 NOT > SPACES
                       MOVE 'MODYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MEN1
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-MOD.
           EXIT.

       SCR-ING SECTION.
       INI-SCR-ING.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-IKY
               MOVE SCR-INST-GKY TO SCR-INST
      *PTC-INI
               IF NOT SCR-OFRM-SRV
                   PERFORM SCR-DMSG
               ELSE
                   PERFORM SCR-PUTA
      *PTC-FIN
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
       MSG-SCR-ING.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'INGNOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-ING
               IF FIO-STAT-OKS
                   IF FRM-MEN1 NOT > SPACES
                       MOVE 'INGYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MEN1
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ING.
           EXIT.

       SCR-ACC SECTION.
       INI-SCR-ACC.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8 OR
                         FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM XCT-PRGP.
           MOVE SPACES TO FRM-MENS.
           IF SCR-STAT NOT = SCR-STAT-CON
               PERFORM CMN-ACC.
           PERFORM SCR-MSSG.
       FIN-SCR-ACC.
           EXIT.

       SCR-ELI SECTION.
       INI-SCR-ELI.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8 OR
                         FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM XCT-PRGP.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'ELINOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-ELI
               IF FIO-STAT-OKS
                   IF FRM-MEN1 NOT > SPACES
                       MOVE 'ELIYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MEN1
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ELI.
           EXIT.

       SCR-ACT SECTION.
       INI-SCR-ACT.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF ( FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8  OR
                           FRM-FFLD-PF5 OR FRM-FFLD-PF6 ) AND
              SCR-ACTU = SCR-ACTU-MOD
               PERFORM XCT-PRGP.
           IF SCR-ACTU = SCR-ACTU-MOD
                  PERFORM MSG-SCR-MOD
           ELSE
                  PERFORM MSG-SCR-ING.
       FIN-SCR-ACT.
           EXIT.

       SCR-PGM SECTION.
       INI-SCR-PGM.
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
       TRL-SCR-PGM.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8 OR
                         FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM XCT-PRGP.
           MOVE SPACES TO FRM-MENS.
           IF SCR-STAT NOT = SCR-STAT-CON
               PERFORM CMN-PGM.
           PERFORM SCR-MSSG.
       FIN-SCR-PGM.
           EXIT.

       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               PERFORM SCR-CLRA-CGN
               PERFORM EDT-KEY-CGN
               PERFORM EDT-FLD-CGN
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-CGN
               PERFORM EDT-KEY-CGN
           ELSE
               PERFORM CLR-FLD-CGN
               PERFORM EDT-FLD-CGN.
           PERFORM STP-EXIT.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

       XCT-PRGP SECTION.
       INI-XCT-PRGP.
           MOVE SCR-INST-BRW TO SCR-INST.
           PERFORM STP-EXIT.
           MOVE FRM-FFLD   TO SCR-FFLD.
      *
           MOVE SCR-PROG     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
           PERFORM XCT-ERR.
       FIN-XCT-PRGP.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO CGN-FLD.
           MOVE LOW-VALUES TO CGN-EDT.
       FIN-SCR-RST.
           EXIT.

       GET-FLD SECTION.
       INI-GET-FLD.
           PERFORM SCR-GFLD.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-FLD.
           EXIT.

       CMN-MOD SECTION.
       INI-CMN-MOD.
      *<<<<
           GO TO NXT-CMN-MOD.
      *>>>>
           PERFORM KEY-ALL-CGN.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-CGN.
           IF FIO-STAT-OKS AND
              CGN-STP-ITRN IN CGN NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM MC2-FIO-CGN
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-CGN
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               GO TO FIN-CMN-MOD.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-MOD.
           PERFORM RET-STT-CGN.
           PERFORM RET-ALL-CGN.
           PERFORM RET-HDR-CGN.
      *<<<< CMN_MOD
       NXT-CMN-MOD.
           PERFORM KEY-ALL-CGN.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-CGN.
           IF FIO-STAT-OKS AND
              CGN-STP-ITRN IN CGN NOT = SCR-UMOD-CGN
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM MC2-FIO-CGN
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-CGN
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-MOD.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-MOD.
           PERFORM RET-STT-CGN.
           PERFORM RET-ALL-CGN.
           PERFORM RET-HDR-CGN.
           MOVE 'M' TO CGN-MSC-STAT IN CGN.
      *>>>>
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM MC2-FIO-CGN.
      *<<<< FIN_CMN_MOD
           IF NOT FIO-STAT-OKS
              GO TO FIN-CMN-MOD.
           MOVE CGN-CIC-ICGN IN CGN     TO DGN-CIC-IDGN IN DGN.
           MOVE DGN-NUM-IDGN IN CGN-FLD TO DGN-NUM-IDGN IN DGN.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-DGN.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-CGN
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-CGN
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE 'MC2' TO MSG-COD-SIST
              MOVE 'ERRUPD   DGN' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE '23' TO FIO-STAT PGM-STAT-DGN PGM-STAT-DGN
              GO TO FIN-CMN-MOD.

           PERFORM RET-STT-DGN.
           PERFORM RET-DGN-CGN.
           PERFORM RET-HDR-DGN.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM MC2-FIO-DGN.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-CGN
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-CGN
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE '23' TO FIO-STAT PGM-STAT-DGN PGM-STAT-DGN
              GO TO FIN-CMN-MOD.

           MOVE 'MC2'               TO FIO-SIST.
           MOVE 'DPL'               TO TAB-COD-TTAB IN TAB.
           MOVE CGN-CIC-ICGN IN CGN TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-MOD.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
              MOVE '23' TO FIO-STAT PGM-STAT-DGN PGM-STAT-DGN
              GO TO FIN-CMN-MOD.

           MOVE FRM-GLS-DES1 IN CGN-FLD TO TAB-GLS-DESC IN TAB.
           MOVE FRM-GLS-DES2 IN CGN-FLD TO TAB-GLS-DATA IN TAB.
           PERFORM GET-FHOY.
           MOVE HOY-FHOY                 TO TAB-FEC-FTRN IN TAB.
           MOVE HOY-HHOY                 TO TAB-HRA-HRTR IN TAB.
           MOVE SCR-OTRN                 TO TAB-COD-OTRN IN TAB.
           MOVE SCR-USER                 TO TAB-COD-ATRN IN TAB.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
              MOVE '23' TO FIO-STAT PGM-STAT-DGN PGM-STAT-DGN
              GO TO FIN-CMN-MOD.
      *>>>>
       FIN-CMN-MOD.
           EXIT.

       CMN-ING SECTION.
       INI-CMN-ING.
           MOVE SPACES TO CGN.
      *<<<< INI_CMN_ING
           IF DGN-NUM-IDGN IN CGN-FLD > 1
              MOVE '00' TO FIO-STAT
              GO TO CNT-CMN-ING.
           PERFORM OBT-CIC-CGN.
           IF CIC-STAT NOT = CIC-STAT-OKS
              GO TO FIN-CMN-ING.
           MOVE CIC-CAI-ICIC IN CIC TO CGN-CAI-ICGN IN CGN.
           MOVE CIC-IIC-ICIC IN CIC TO CGN-IIC-ICGN IN CGN.
           MOVE CIC-CIC-ICIC IN CIC TO CGN-CIC-ICGN IN CGN-FLD.
      *>>>>
           PERFORM RET-STT-CGN.
           PERFORM RET-ALL-CGN.
           PERFORM RET-HDR-CGN.
           MOVE FIO-PUT TO FIO-CMND.
           PERFORM MC2-FIO-CGN.
      *<<<< FIN_CMN_ING
       CNT-CMN-ING.
           IF NOT FIO-STAT-OKS
              GO TO FIN-CMN-ING.
           PERFORM RET-STT-DGN.
           PERFORM KEY-DGN-CGN.
           MOVE CGN-CIC-ICGN IN CGN-FLD TO DGN-CIC-IDGN IN DGN.
           PERFORM RET-DGN-CGN.
           PERFORM RET-HDR-DGN.
           MOVE FIO-PUT TO FIO-CMND.
           PERFORM MC2-FIO-DGN.
           IF NOT FIO-STAT-OKS
              MOVE FIO-STAT TO PGM-STAT-CGN PGM-STAT-DGN
              GO TO FIN-CMN-ING.

           IF DGN-NUM-IDGN IN CGN-FLD = 1
              MOVE 'MC2'                   TO FIO-SIST
              MOVE 'DPL'                   TO TAB-COD-TTAB IN TAB
              MOVE CGN-CIC-ICGN IN CGN     TO TAB-COD-CTAB IN TAB
              MOVE FRM-GLS-DES1 IN CGN-FLD TO TAB-GLS-DESC IN TAB
              MOVE FRM-GLS-DES2 IN CGN-FLD TO TAB-GLS-DATA IN TAB
              MOVE 'I'                     TO TAB-MSC-TACC IN TAB
              MOVE 'I'                     TO TAB-MSC-STAT IN TAB
              MOVE 'S'                     TO TAB-IND-VIGE IN TAB
              MOVE SPACES                  TO TAB-COD-OTRN IN TAB
              MOVE 'MC2PUCGN'              TO TAB-COD-ATRN IN TAB
              MOVE DGN-CAI-IDGN IN DGN     TO TAB-GLS-DCOR IN TAB
              MOVE DGN-IIC-IDGN IN DGN     TO TAB-GLS-ABRV IN TAB
              PERFORM GET-FHOY
              MOVE HOY-FHOY                 TO TAB-FEC-FTRN IN TAB
              MOVE HOY-HHOY                 TO TAB-HRA-HRTR IN TAB
              MOVE SCR-OTRN                 TO TAB-COD-OTRN IN TAB
              MOVE SCR-USER                 TO TAB-COD-ATRN IN TAB
              MOVE FIO-PUT TO FIO-CMND
              PERFORM GNS-FIO-TAB
              IF NOT FIO-STAT-OKS
                 MOVE 'MC2'               TO MSG-COD-SIST
                 MOVE 'DPL    NOGRB'      TO MSG-COD-MENS
                 PERFORM GET-MSG
                 MOVE MSG-GLS-DESC        TO FRM-MENS
                 GO TO FIN-CMN-ING.

      *MODIFICA ESTADO CGN
           PERFORM KEY-ALL-CGN.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-CGN.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-CGN
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-CGN
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE 'MC2' TO MSG-COD-SIST
              MOVE 'ERRUPDSTACGN' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              GO TO FIN-CMN-ING.
           PERFORM RET-STT-CGN.
           PERFORM RET-HDR-CGN.
           PERFORM RET-ALL-CGN.                                         
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM MC2-FIO-CGN.
      *>>>>
       FIN-CMN-ING.
           EXIT.

       CMN-ACC SECTION.
       INI-CMN-ACC.
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'ACCION NO REALIZADA' TO FRM-MENS
               GO TO FIN-CMN-ACC.
      *<<<< INI_CMN_ACC
           IF SCR-CMND = 'VRF'
               PERFORM PRO-CMN-VRF
               IF MSG-COD-MENS > SPACES
                  GO TO FIN-CMN-ACC.
           GO TO NXT-CMN-ACC.
      *>>>>
           PERFORM KEY-ALL-CGN.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-CGN.
           IF FIO-STAT-OKS AND
              CGN-STP-ITRN IN CGN NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM MC2-FIO-CGN
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-CGN
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-ACC.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ACC.
           PERFORM RET-ALL-CGN.
      *<<<< CMN_ACC
       NXT-CMN-ACC.
           PERFORM KEY-ALL-CGN.                                         
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.                            
           PERFORM MC2-FIO-CGN.                                         
           IF FIO-STAT-OKS AND                                          
              CGN-STP-ITRN IN CGN NOT = SCR-UMOD-CGN                    
               MOVE FIO-ULK-REC TO FIO-CMND                             
               PERFORM MC2-FIO-CGN                                      
               MOVE FIO-BCK-OUT TO FIO-CMND                             
               PERFORM MC2-FIO-CGN                                      
               MOVE 'GNS' TO MSG-COD-SIST                               
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS                      
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               GO TO FIN-CMN-ACC.                                       
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-CMN-ACC.                                       
      *    PERFORM RET-ALL-CGN.                                         
    
           IF SCR-CMND = 'VRF'
               MOVE 'V'          TO CGN-IND-VIGE IN CGN
               MOVE 'VRFYES    ' TO WSS-COD-MENY
               MOVE 'VRFNOT    ' TO WSS-COD-MENN
           ELSE
           IF SCR-CMND = 'ACA'
               MOVE 'A'          TO CGN-IND-VIGE IN CGN
               MOVE 'ACAYES    ' TO WSS-COD-MENY
               MOVE 'ACANOT    ' TO WSS-COD-MENN
           ELSE
           IF SCR-CMND = 'ANL'
               MOVE 'D'          TO CGN-IND-VIGE IN CGN
               MOVE 'ANLYES    ' TO WSS-COD-MENY
               MOVE 'ANLNOT    ' TO WSS-COD-MENN.

           MOVE FIO-MOD TO FIO-CMND.
           PERFORM MC2-FIO-CGN.
           IF NOT FIO-STAT-OKS
               MOVE 'S' TO SCR-IERR
               MOVE 'MC2' TO MSG-COD-SIST
               MOVE WSS-COD-MENN TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE WSS-COD-MENY  TO MSG-COD-MENS
               MOVE 'MC2' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN1.

           IF SCR-CMND NOT = 'ANL'                                      
              GO TO FIN-CMN-ACC.                                        
           MOVE 'MC2' TO FIO-SIST.                                      
           MOVE 'DPL' TO TAB-COD-TTAB IN TAB.                           
           MOVE CGN-CIC-ICGN IN CGN TO TAB-COD-CTAB IN TAB.             
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM GNS-FIO-TAB.                                         
           IF FIO-STAT-OKS                                              
              MOVE FIO-GET-KEY-UPD TO FIO-CMND                          
              PERFORM GNS-FIO-TAB                                       
              IF FIO-STAT-OKS                                           
                 PERFORM GET-FHOY                                       
                 MOVE HOY-FHOY            TO TAB-FEC-FTRN IN TAB        
                 MOVE HOY-HHOY            TO TAB-HRA-HRTR IN TAB        
                 MOVE SCR-OTRN            TO TAB-COD-OTRN IN TAB        
                 MOVE SCR-USER            TO TAB-COD-ATRN IN TAB        
                 MOVE 'N'      TO TAB-IND-VIGE IN TAB                   
                 MOVE FIO-MOD TO FIO-CMND                               
                 PERFORM GNS-FIO-TAB                                    
                 IF NOT FIO-STAT-OKS                                    
                    MOVE 'ERROR UPD DPL' TO FRM-MEN2                    
                 ELSE                                                   
                    NEXT SENTENCE                                       
              ELSE                                                      
                 MOVE 'ERROR UPD DPL' TO FRM-MEN2                       
           ELSE                                                         
              MOVE 'ERROR UPD DPL' TO FRM-MEN2.                         
      *>>>>
       FIN-CMN-ACC.
           EXIT.

       CMN-ELI SECTION.
       INI-CMN-ELI.
      *<<<< INI_CMN_ELI
           PERFORM KEY-ALL-CGN.
           GO TO CNT-CMN-ELI.
      *>>>>
           PERFORM KEY-ALL-CGN.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-CGN.
           IF FIO-STAT-OKS AND
              CGN-STP-ITRN IN CGN NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM MC2-FIO-CGN
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-CGN
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-ELI.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ELI.
           MOVE FIO-DEL TO FIO-CMND.
           PERFORM MC2-FIO-CGN.
      *<<<< FIN_CMN_ELI
           MOVE FIO-STAT TO PGM-STAT-CGN.
       CNT-CMN-ELI.
           IF FRM-IND-SGTE IN CGN-FLD = 'S'
              PERFORM ELI-ALL-CGN
              GO TO FIN-CMN-ELI.
           MOVE CGN-CIC-ICGN IN CGN-FLD TO DGN-CIC-IDGN IN DGN.
           MOVE DGN-NUM-IDGN IN CGN-FLD TO DGN-NUM-IDGN IN DGN.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-DGN.
           IF FIO-STAT-OKS AND
              DGN-STP-ITRN IN DGN NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM MC2-FIO-CGN
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-CGN
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE '23' TO FIO-STAT PGM-STAT-DGN PGM-STAT-DGN
               GO TO FIN-CMN-ELI.
           IF NOT FIO-STAT-OKS
              GO TO FIN-CMN-ELI.

           MOVE FIO-DEL TO FIO-CMND.
           PERFORM MC2-FIO-DGN.
           MOVE FIO-STAT TO PGM-STAT-DGN.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE '23' TO FIO-STAT PGM-STAT-DGN PGM-STAT-DGN
              GO TO FIN-CMN-ELI.
           MOVE CGN-CIC-ICGN IN CGN-FLD TO DGN-CIC-IDGN IN DGN.
           MOVE ZEROES                  TO DGN-NUM-IDGN IN DGN.
           MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM MC2-FIO-DGN.
           IF (FIO-STAT-OKS AND
               DGN-CIC-IDGN IN DGN = CGN-CIC-ICGN IN CGN-FLD)
              MOVE PGM-STAT-DGN TO FIO-STAT
              PERFORM MOD-STAT-CGN
              GO TO FIN-CMN-ELI.

           PERFORM KEY-ALL-CGN.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-CGN.
           IF FIO-STAT-OKS AND
              CGN-STP-ITRN IN CGN NOT = SCR-UMOD-CGN
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM MC2-FIO-CGN
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-CGN
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM MC2-FIO-DGN
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-DGN
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-ELI.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ELI.
           MOVE FIO-DEL TO FIO-CMND.
           PERFORM MC2-FIO-CGN.
           MOVE FIO-STAT TO PGM-STAT-CGN.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-CGN
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-CGN
              MOVE '23' TO FIO-STAT PGM-STAT-DGN PGM-STAT-DGN
              GO TO FIN-CMN-ELI.
           MOVE 'MC2'               TO FIO-SIST.
           MOVE 'DPL'               TO TAB-COD-TTAB IN TAB.
           MOVE CGN-CIC-ICGN IN CGN TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
              MOVE PGM-STAT-DGN TO FIO-STAT
              GO TO FIN-CMN-ELI.
           MOVE FIO-DEL TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
              MOVE PGM-STAT-DGN TO FIO-STAT
              GO TO FIN-CMN-ELI.
      *>>>>
       FIN-CMN-ELI.
           EXIT.

       CMN-PGM SECTION.
       INI-CMN-PGM.
           PERFORM RET-ALL-CGN.
       FIN-CMN-PGM.
           EXIT.

       STP-EXIT SECTION.
       INI-STP-EXIT.
           MOVE SCR-PROG TO PGM-PROG.
           MOVE SCR-NTRN TO PGM-NTRN.

           MOVE SCR-PROV TO SCR-PROG.
           MOVE SCR-NTRV TO SCR-NTRN.

           MOVE PGM-PROG TO SCR-PROV
           MOVE PGM-NTRN TO SCR-NTRV.
       FIN-STP-EXIT.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO SYS-NTRN.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
      *PTC-INI
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE CGN-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO CGN-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GFLD SECTION.
       INI-SCR-GFLD.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-FLD-CGN
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO CGN-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GFLD.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-CGN.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-CGN
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           PERFORM VAL-NUM-FLD-CGN.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-CGN
               PERFORM EDT-FLD-CGN
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           PERFORM VAL-MKY-CGN.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
               GO TO ERR-SCR-GFLD.
           PERFORM VAL-FLD-CGN.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GFLD.
           PERFORM VAL-CON-FLD-CGN.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GFLD.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-FLD-CGN.
       ERR-SCR-GFLD.
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-CGN.
           PERFORM EDT-FLD-CGN.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GFLD.
           MOVE SCR-STAT-MOD TO SCR-STAT.

       GET-ACC SECTION.
       INI-GET-ACC.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO CGN-GLS-MEN2
               MOVE SCR-PRMT     TO CGN-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO CGN-FLD.
           IF NOT ( ( FRM-FFLD = FRM-FFLD-PF12 OR FRM-FFLD-PF3 ) OR
                      SCR-CONF > SPACES )
               PERFORM VAL-NUM-KEY-CGN
               PERFORM VAL-NUM-FLD-CGN.
       FIN-GET-ACC.
           MOVE SCR-STAT-SON TO SCR-STAT.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF7 )
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 )
      *>>>>
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO CGN-GLS-MEN2
               MOVE SCR-PRMT     TO CGN-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO CGN-FLD.
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO CGN-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE CGN-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO CGN-FLD
      * FLB  7-NOV-1994
               PERFORM CLR-KEY-CGN

               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO CGN-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO CGN-GLS-MENS.
           PERFORM PRO-FLD-CGN.
           PERFORM PRO-MKY-CGN.
           PERFORM UNP-IKY-CGN.
           PERFORM FST-KEY-CGN.
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
           IF SCR-DESDE-CMN
               MOVE CGN-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO CGN-FLD
               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO CGN-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO CGN-GLS-MENS.
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-FLD-CGN.
           MOVE '1' TO FRM-CPIM-AEY.
           PERFORM UNP-KEY-CGN.
           PERFORM FST-KEY-CGN.
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
           PERFORM UNP-MKY-CGN.
           PERFORM UNP-FLD-CGN.
           PERFORM FST-FLD-CGN.
      *<<<< FIN_HBL_FLD
      *>>>>
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO CGN-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-CGN.
           PERFORM PRO-FLD-CGN.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
      * PTC-INI
           IF SCR-STAT NOT = SCR-STPP
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
      * PTC-FIN
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-CGN
               PERFORM EDT-FLD-CGN
               PERFORM SCR-PUTA
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-CGN
               PERFORM EDT-FLD-CGN
               PERFORM SCR-PUTA
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-CGN.
           PERFORM EDT-FLD-CGN.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           IF SCR-INST = SCR-INST-GCF
                  PERFORM SCR-GCNF
                  GO TO FIN-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
           IF SCR-STAT = SCR-STAT-ABT OR SCR-STAT-CON
                  GO TO FIN-DESSCRTRL.
           IF SCR-FFLD-RET OR SCR-FFLD-PF10
                  MOVE SCR-VFLD-YES TO SCR-VFLD
                  PERFORM PRO-CONF.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

       PRO-CONF SECTION.
       INI-PRO-CONF.
      *PTC-INI
           IF SCR-OFRM-SRV AND
              SCR-CONF NOT > SPACES
                MOVE SPACES TO FRM-MENS.
      *PTC-FIN
           IF FRM-MENS NOT > SPACES
               IF SCR-CONF NOT > SPACES
                   GO TO FIN-PRO-CONF
               ELSE
                   MOVE SCR-CONF TO FRM-MENS
           ELSE
               IF FRM-MEN1 NOT > SPACES
                   MOVE SCR-CONF TO FRM-MEN1.
           PERFORM PRO-KEY-CGN.
           PERFORM PRO-FLD-CGN.
           PERFORM FST-MSG.
           PERFORM SCR-MSSG.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM EDT-KEY-CGN
               PERFORM EDT-FLD-CGN.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GCF TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-PRO-CONF.
           EXIT.

       SCR-GCNF SECTION.
       INI-SCR-GCNF.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3  OR
                               FRM-FFLD-PF4 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO CGN-GLS-MEN2
               MOVE SCR-CONF     TO CGN-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO CGN-FLD.
           MOVE FRM-FFLD TO SCR-FFLD.
           IF FRM-FFLD = FRM-FFLD-PF3
               MOVE SCR-VFLD-NOT TO SCR-VFLD
           ELSE
           IF FRM-FFLD = FRM-FFLD-RET OR FRM-FFLD-PF10 OR FRM-FFLD-PF4
               MOVE SCR-VFLD-YES TO SCR-VFLD
               PERFORM VAL-NUM-KEY-CGN
               PERFORM VAL-NUM-FLD-CGN
           ELSE
           IF FRM-FFLD = FRM-FFLD-PF12
               MOVE SCR-STPP TO SCR-STAT
               IF SCR-STAT = SCR-STAT-MOD
                   PERFORM VAL-NUM-KEY-CGN
                   PERFORM VAL-NUM-FLD-CGN
                   PERFORM SCR-TRL
               ELSE
                   MOVE SCR-VFLD-NOT TO SCR-VFLD.
       FIN-SCR-GCNF.
           EXIT.

      * PGM-EBR-ALL SECTION.
      * INI-PGM-EBR-ALL.
      *     MOVE 'GNS' TO FIO-SIST.
      *     MOVE FIO-EBR-ALL TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
      * FIN-PGM-EBR-ALL.
      *     EXIT.

       RET-STT-CGN SECTION.
       INI-RET-STT-CGN.
           IF SCR-CCPP = 'ING'
               MOVE 'I' TO CGN-MSC-TACC IN CGN
               MOVE 'I' TO CGN-MSC-STAT IN CGN
           ELSE
           IF SCR-CCPP = 'MOD'
               MOVE 'M' TO CGN-MSC-TACC IN CGN
               MOVE 'M' TO CGN-MSC-STAT IN CGN.
       FIN-RET-STT-CGN.
           EXIT.

       RET-HDR-CGN SECTION.
       INI-RET-HDR-CGN.
           PERFORM GET-FHOY.
      *    Fecha Transaccion
           MOVE HOY-FHOY TO CGN-FEC-FTRN IN CGN.
      *    Hora Transaccion
           MOVE HOY-HHOY TO CGN-HRA-HRTR IN CGN.
      *    Codigo Origen transaccion
      *<<<< NOT PGM_VMS
           MOVE SCR-OTRN TO CGN-COD-OTRN IN CGN.
      *    Autor Transaccion
           MOVE SCR-USER TO CGN-COD-ATRN IN CGN.
      *>>>>
       FIN-RET-HDR-CGN.
           EXIT.

       COPY MC2BUCGN.
       COPY GNSBVNUM.
       COPY MC2BFCGN.
       COPY GNSBFMSG.
       COPY GNSBGSYS.
       COPY GNSBGDTC.
      *PTC-INI
      *PTC-SUB
      * ISP  10-MAY-2001 13:46:35 
      * COPY GNSBGFRM.
       COPY GNSBG3CF.
      *PTC-FIN
       COPY GNSBGMSG.
       COPY GNSBEKEY.
       COPY GNSBIABT.
       COPY GNSBFTAB.
       COPY GNSBEGLS.
       COPY GNSBGFEC.
       COPY GNSBGHOY.
      *PTC-INI
       COPY GNSBGQUE.
       COPY GNSBGTSK.
      *PTC-FIN

      *<<<< EOF
       PGM-EBR-ALL SECTION.                                             
       INI-PGM-EBR-ALL.                                                 
           MOVE 'GNS' TO FIO-SIST.                                      
           MOVE FIO-EBR-ALL TO FIO-CMND.                                
           PERFORM GNS-FIO-TAB.                                         
       FIN-PGM-EBR-ALL.                                                 
           EXIT.                                                        

       COPY MC2BFDGN.
       COPY GNSBGVSM.
       COPY GNSBVTAB.
       COPY GNSBVCOD.
       COPY GNSBBTAB.
       COPY GNSBVMSC.
       COPY GNSBBMSC.
       COPY GNSBFMSC.
      *<<<< EOF_PU
       COPY GNSBGCIC.
       COPY GNSBFCIC.
       COPY GNSBPVRF.
       COPY GNSBVIND.
       COPY TABBFCAM.

       OBT-CIC-CGN SECTION.
       INI-OBT-CIC-CGN.
           MOVE 'PCGN'  TO CIC-CAI-ICIC IN CIC.
           MOVE CIC-GET TO CIC-CMND.
           PERFORM PRO-CIC.
           IF CIC-STAT NOT = CIC-STAT-OKS
               MOVE CIC-MENS     TO FRM-MENS
               MOVE FIO-STAT-NEX TO FIO-STAT.
       FIN-OBT-CIC-CGN.
           EXIT.

       RET-STT-DGN SECTION.
       INI-RET-STT-DGN.
           IF SCR-CCPP = 'ING'
               MOVE 'I' TO DGN-MSC-TACC IN DGN
               MOVE 'I' TO DGN-MSC-STAT IN DGN
           ELSE
           IF SCR-CCPP = 'MOD'
               MOVE 'M' TO DGN-MSC-TACC IN DGN
               MOVE 'M' TO DGN-MSC-STAT IN DGN.
       FIN-RET-STT-DGN.
           EXIT.

       RET-HDR-DGN SECTION.
       INI-RET-HDR-DGN.
           PERFORM GET-FHOY.
      *    Fecha Transaccion
           MOVE HOY-FHOY TO DGN-FEC-FTRN IN DGN.
      *    Hora Transaccion
           MOVE HOY-HHOY TO DGN-HRA-HRTR IN DGN.
      *    Codigo Origen transaccion
      *<<<< NOT PGM_VMS
           MOVE SCR-OTRN TO DGN-COD-OTRN IN DGN.
      *    Autor Transaccion
           MOVE SCR-USER TO DGN-COD-ATRN IN DGN.
      *>>>>
       FIN-RET-HDR-DGN.
           EXIT.

       MOD-STAT-CGN SECTION.
       INI-MOD-STAT-CGN.
      *MODIFICA ESTADO CGN
           PERFORM KEY-ALL-CGN.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-CGN.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-CGN
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-CGN
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE 'MC2'          TO MSG-COD-SIST
              MOVE 'ERRUPDSTACGN' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              GO TO FIN-MOD-STAT-CGN.
           MOVE 'M' TO CGN-IND-VIGE IN CGN.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM MC2-FIO-CGN.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-CGN
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-CGN
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE 'MC2'          TO MSG-COD-SIST
              MOVE 'ERRUPDSTACGN' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              GO TO FIN-MOD-STAT-CGN.
      *PVG INI 13-DIC-2007 ACTUALIZA CORRELATIVO POST ELI UNITARIA      
           PERFORM PRO-ACT-NCOR.                                        
      *PVG FIN 13-DIC-2007                                              
       FIN-MOD-STAT-CGN.
           EXIT.

       PRO-CMN-VRF SECTION.
       INI-PRO-CMN-VRF.
           PERFORM OBT-COD-PESO.
           IF MSG-COD-MENS > SPACES
              GO TO MSG-PRO-CMN-VRF.
           MOVE SPACES TO MSG-COD-MENS.
           MOVE ZEROES TO WSS-K.
           MOVE CGN-CIC-ICGN IN CGN-FLD TO DGN-CIC-IDGN IN DGN.
           MOVE ZEROES                  TO DGN-NUM-IDGN IN DGN.
           MOVE FIO-GET-NLS TO FIO-CMND.
       LUP-PRO-CMN-VRF.
           PERFORM MC2-FIO-DGN.
           IF NOT (FIO-STAT-OKS AND
                   CGN-CIC-ICGN IN CGN-FLD = DGN-CIC-IDGN IN DGN)
              GO TO CNT-PRO-CMN-VRF.
           ADD 1 TO WSS-K.
           MOVE DGN-VAL-MTOP IN DGN TO VEC-DGN-VAL-MTOPP(WSS-K).
           IF  DGN-VAL-MTOP IN DGN > ZEROES AND                         
              (DGN-COD-VCMT IN DGN NOT = WSS-COD-PESO IN WSS-VARI)      
              PERFORM BUS-EQV-MNAC
              IF MSG-COD-MENS > SPACES
                 MOVE DGN-COD-VCMT IN DGN TO FRM-MEN2
                 GO TO MSG-PRO-CMN-VRF.
           PERFORM CRG-DGN-VEC.
      *MONTOS INDEFINIDOS                                               
           IF DGN-NUM-NDOC IN DGN NOT > ZEROES                          
              MOVE 99999               TO VEC-DGN-NUM-NDOCG(WSS-K)      
           ELSE                                                         
              MOVE DGN-NUM-NDOC IN DGN TO VEC-DGN-NUM-NDOCG(WSS-K).     
                                                                        
           IF DGN-VAL-MTOP IN DGN NOT > ZEROES                          
              MOVE 99999999999         TO VEC-DGN-VAL-MTOPP(WSS-K).     
                                                                        
           IF DGN-IND-UTPT IN DGN = 'D'
              MOVE DGN-NUM-PTOP IN DGN TO VEC-DGN-NUM-PTOPD(WSS-K)
           ELSE
           IF DGN-IND-UTPT IN DGN = 'M'
              MULTIPLY DGN-NUM-PTOP IN DGN BY 30
                                   GIVING VEC-DGN-NUM-PTOPD(WSS-K)
           ELSE
           IF DGN-IND-UTPT IN DGN = 'A'
              MULTIPLY DGN-NUM-PTOP IN DGN BY 365
                                   GIVING VEC-DGN-NUM-PTOPD(WSS-K).
           IF DGN-NUM-PTOP IN DGN NOT > ZEROES                          
              MOVE 99999               TO VEC-DGN-NUM-PTOPD(WSS-K).     
                                                                        
           MOVE FIO-GET-NXT TO FIO-CMND.
           GO TO LUP-PRO-CMN-VRF.
       CNT-PRO-CMN-VRF.
           PERFORM PRO-VEC-SORT.

           PERFORM ELI-ALL-DGN.
           IF MSG-COD-MENS > SPACES
              GO TO MSG-PRO-CMN-VRF.

           PERFORM PUT-NEW-DGN.
           IF MSG-COD-MENS > SPACES
               GO TO MSG-PRO-CMN-VRF.
           GO TO FIN-PRO-CMN-VRF.
       MSG-PRO-CMN-VRF.
           IF MSG-COD-SIST NOT > SPACES
              MOVE 'MC2' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MEN1.
           MOVE 'S' TO SCR-IERR.
           MOVE '00' TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-PRO-CMN-VRF.
       FIN-PRO-CMN-VRF.
           EXIT.

       ELI-ALL-DGN SECTION.
       INI-ELI-ALL-DGN.
           MOVE CGN-CIC-ICGN IN CGN-FLD TO DGN-CIC-IDGN IN DGN.
           MOVE ZEROES                  TO DGN-NUM-IDGN IN DGN.
           MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM MC2-FIO-DGN.
           IF NOT (FIO-STAT-OKS AND
                   CGN-CIC-ICGN IN CGN-FLD = DGN-CIC-IDGN IN DGN)
              GO TO FIN-ELI-ALL-DGN.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-DGN.
           IF NOT FIO-STAT-OKS
              MOVE 'DGNVRF NOUPD' TO MSG-COD-MENS
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DGN
              GO TO FIN-ELI-ALL-DGN.
           MOVE FIO-DEL TO FIO-CMND.
           PERFORM MC2-FIO-DGN.
           IF NOT FIO-STAT-OKS
              MOVE 'DGNVRF NODEL' TO MSG-COD-MENS
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DGN
              GO TO FIN-ELI-ALL-DGN.
           GO TO INI-ELI-ALL-DGN.
       FIN-ELI-ALL-DGN.
           EXIT.

       PUT-NEW-DGN SECTION.
       INI-PUT-NEW-DGN.
           MOVE ZEROES TO WSS-R.
           MOVE ZEROES TO WSS-RDGN.                                     
           MOVE WSS-K  TO WSS-L.
       LUP-PUT-NEW-DGN.
           IF WSS-L NOT > ZEROES
               GO TO FIN-PUT-NEW-DGN.
           ADD 1 TO WSS-R.
           PERFORM CRG-VEC-DGN.
           IF VEC-DGN-GLS-FLAG(WSS-R) = 'E'                             
              GO TO CNT-PUT-NEW-DGN.                                    
           MOVE FIO-PUT TO FIO-CMND.
           PERFORM MC2-FIO-DGN.
           IF NOT FIO-STAT-OKS
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-DGN
               MOVE 'DGNVRF NOPUT' TO MSG-COD-MENS
               GO TO FIN-PUT-NEW-DGN.
       CNT-PUT-NEW-DGN.                                                 
           SUBTRACT 1 FROM WSS-L.
           GO TO LUP-PUT-NEW-DGN.
       FIN-PUT-NEW-DGN.
           EXIT.

       OBT-COD-PESO SECTION.
       INI-OBT-COD-PESO.
      * OBTIENE CODIGO VALOR DE CAMBIO DE PESOS MEDIANTE CIC DE LA MONED
           MOVE 'TAB'          TO FIO-SIST.
           MOVE 'VLR'          TO TAB-CIC-TTAB IN TAB.
           MOVE '0999  '       TO TAB-CIC-CTAB IN TAB.
           MOVE 'TAB-CIC-TABL' TO FIO-AKEY.
           MOVE FIO-GET-KEY    TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
               MOVE 'MC2'          TO MSG-COD-SIST
               MOVE 'CIC0999  NEX' TO MSG-COD-MENS.
           MOVE TAB-COD-CTAB IN TAB TO WSS-COD-PESO IN WSS-VARI.
       FIN-OBT-COD-PESO.
           EXIT.

       CRG-DGN-VEC SECTION.
       INI-CRG-DGN-VEC.
           MOVE DGN-GLS-FLAG IN DGN TO VEC-DGN-GLS-FLAG(WSS-K).         
           MOVE DGN-KEY-IREG IN DGN TO VEC-DGN-KEY-IREG(WSS-K).         
           MOVE DGN-NUM-IDGN IN DGN TO VEC-DGN-NUM-IDGN(WSS-K).         
           MOVE DGN-STP-ITRN IN DGN TO VEC-DGN-STP-ITRN(WSS-K).         
           MOVE DGN-MSC-TACC IN DGN TO VEC-DGN-MSC-TACC(WSS-K).         
           MOVE DGN-MSC-STAT IN DGN TO VEC-DGN-MSC-STAT(WSS-K).         
           MOVE DGN-COD-OTRN IN DGN TO VEC-DGN-COD-OTRN(WSS-K).         
           MOVE DGN-COD-ATRN IN DGN TO VEC-DGN-COD-ATRN(WSS-K).         
           MOVE DGN-COD-TDOC IN DGN TO VEC-DGN-COD-TDOC(WSS-K).         
           MOVE DGN-COD-EDOC IN DGN TO VEC-DGN-COD-EDOC(WSS-K).         
           MOVE DGN-COD-OCOB IN DGN TO VEC-DGN-COD-OCOB(WSS-K).         
           MOVE DGN-IND-COBL IN DGN TO VEC-DGN-IND-COBL(WSS-K).         
           MOVE DGN-NUM-UCOB IN DGN TO VEC-DGN-NUM-UCOB(WSS-K).         
           MOVE DGN-NUM-NDOC IN DGN TO VEC-DGN-NUM-NDOC(WSS-K).         
           MOVE DGN-VAL-MTOP IN DGN TO VEC-DGN-VAL-MTOP(WSS-K).         
           MOVE DGN-COD-VCMT IN DGN TO VEC-DGN-COD-VCMT(WSS-K).         
           MOVE DGN-COD-CPMT IN DGN TO VEC-DGN-COD-CPMT(WSS-K).         
                                                                        
           MOVE DGN-NUM-PTOP IN DGN TO VEC-DGN-NUM-PTOP(WSS-K).         
           MOVE DGN-IND-UTPT IN DGN TO VEC-DGN-IND-UTPT(WSS-K).         
           MOVE DGN-SGV-TMBA IN DGN TO VEC-DGN-SGV-TMBA(WSS-K).         
           MOVE DGN-COD-VCBA IN DGN TO VEC-DGN-COD-VCBA(WSS-K).         
           MOVE DGN-COD-CPBA IN DGN TO VEC-DGN-COD-CPBA(WSS-K).         
           MOVE DGN-SGV-TMNO IN DGN TO VEC-DGN-SGV-TMNO(WSS-K).         
           MOVE DGN-COD-VCNO IN DGN TO VEC-DGN-COD-VCNO(WSS-K).         
           MOVE DGN-COD-CPNO IN DGN TO VEC-DGN-COD-CPNO(WSS-K).         
           MOVE DGN-MSC-BCTI IN DGN TO VEC-DGN-MSC-BCTI(WSS-K).         
           MOVE DGN-MSC-PBTI IN DGN TO VEC-DGN-MSC-PBTI(WSS-K).         
           MOVE DGN-MSC-BPBT IN DGN TO VEC-DGN-MSC-BPBT(WSS-K).         
           MOVE DGN-MSC-SPBT IN DGN TO VEC-DGN-MSC-SPBT(WSS-K).         
           MOVE DGN-COD-CVTI IN DGN TO VEC-DGN-COD-CVTI(WSS-K).         
           MOVE DGN-MSC-TFPR IN DGN TO VEC-DGN-MSC-TFPR(WSS-K).         
           MOVE DGN-NUM-PREP IN DGN TO VEC-DGN-NUM-PREP(WSS-K).         
           MOVE DGN-MSC-UTPR IN DGN TO VEC-DGN-MSC-UTPR(WSS-K).         
           MOVE DGN-VAL-VMIN IN DGN TO VEC-DGN-VAL-VMIN(WSS-K).         
           MOVE DGN-VAL-VMAX IN DGN TO VEC-DGN-VAL-VMAX(WSS-K).         
           MOVE DGN-COD-VMYM IN DGN TO VEC-DGN-COD-VMYM(WSS-K).         
           MOVE DGN-IND-MIVA IN DGN TO VEC-DGN-IND-MIVA(WSS-K).         
           MOVE DGN-NUM-VIGD IN DGN TO VEC-DGN-NUM-VIGD(WSS-K).         
           MOVE DGN-MSC-UTVD IN DGN TO VEC-DGN-MSC-UTVD(WSS-K).         
           MOVE DGN-IND-ACAL IN DGN TO VEC-DGN-IND-ACAL(WSS-K).         
           MOVE DGN-GLS-DISP IN DGN TO VEC-DGN-GLS-DISP(WSS-K).         
       FIN-CRG-DGN-VEC.
           EXIT.

       CRG-VEC-DGN SECTION.
       INI-CRG-VEC-DGN.
           IF VEC-DGN-GLS-FLAG(WSS-R) = 'E'                             
              GO TO FIN-CRG-VEC-DGN.                                    
           ADD 1 TO WSS-RDGN.                                           
                                                                        
           MOVE VEC-DGN-GLS-FLAG(WSS-R) TO DGN-GLS-FLAG IN DGN.         
           MOVE VEC-DGN-KEY-IREG(WSS-R) TO DGN-KEY-IREG IN DGN.         
           MOVE WSS-RDGN                TO DGN-NUM-IDGN IN DGN.         
           MOVE VEC-DGN-STP-ITRN(WSS-R) TO DGN-STP-ITRN IN DGN.         
           MOVE 'M'                     TO DGN-MSC-TACC IN DGN.         
           MOVE 'V'                     TO DGN-MSC-STAT IN DGN.         
           MOVE VEC-DGN-COD-OTRN(WSS-R) TO DGN-COD-OTRN IN DGN.         
           MOVE VEC-DGN-COD-ATRN(WSS-R) TO DGN-COD-ATRN IN DGN.         
           MOVE VEC-DGN-COD-TDOC(WSS-R) TO DGN-COD-TDOC IN DGN.         
           MOVE VEC-DGN-COD-EDOC(WSS-R) TO DGN-COD-EDOC IN DGN.         
           MOVE VEC-DGN-COD-OCOB(WSS-R) TO DGN-COD-OCOB IN DGN.         
           MOVE VEC-DGN-IND-COBL(WSS-R) TO DGN-IND-COBL IN DGN.         
           MOVE VEC-DGN-NUM-UCOB(WSS-R) TO DGN-NUM-UCOB IN DGN.         
           MOVE VEC-DGN-NUM-NDOC(WSS-R) TO DGN-NUM-NDOC IN DGN.         
           MOVE VEC-DGN-VAL-MTOP(WSS-R) TO DGN-VAL-MTOP IN DGN.         
           MOVE VEC-DGN-COD-VCMT(WSS-R) TO DGN-COD-VCMT IN DGN.         
           MOVE VEC-DGN-COD-CPMT(WSS-R) TO DGN-COD-CPMT IN DGN.         
                                                                        
           MOVE VEC-DGN-NUM-PTOP(WSS-R) TO DGN-NUM-PTOP IN DGN.         
           MOVE VEC-DGN-IND-UTPT(WSS-R) TO DGN-IND-UTPT IN DGN.         
           MOVE VEC-DGN-SGV-TMBA(WSS-R) TO DGN-SGV-TMBA IN DGN.         
           MOVE VEC-DGN-COD-VCBA(WSS-R) TO DGN-COD-VCBA IN DGN.         
           MOVE VEC-DGN-COD-CPBA(WSS-R) TO DGN-COD-CPBA IN DGN.         
           MOVE VEC-DGN-SGV-TMNO(WSS-R) TO DGN-SGV-TMNO IN DGN.         
           MOVE VEC-DGN-COD-VCNO(WSS-R) TO DGN-COD-VCNO IN DGN.         
           MOVE VEC-DGN-COD-CPNO(WSS-R) TO DGN-COD-CPNO IN DGN.         
                                                                        
           MOVE VEC-DGN-MSC-BCTI(WSS-R) TO DGN-MSC-BCTI IN DGN.         
           MOVE VEC-DGN-MSC-PBTI(WSS-R) TO DGN-MSC-PBTI IN DGN.         
           MOVE VEC-DGN-MSC-BPBT(WSS-R) TO DGN-MSC-BPBT IN DGN.         
           MOVE VEC-DGN-MSC-SPBT(WSS-R) TO DGN-MSC-SPBT IN DGN.         
           MOVE VEC-DGN-COD-CVTI(WSS-R) TO DGN-COD-CVTI IN DGN.         
           MOVE VEC-DGN-MSC-TFPR(WSS-R) TO DGN-MSC-TFPR IN DGN.         
           MOVE VEC-DGN-NUM-PREP(WSS-R) TO DGN-NUM-PREP IN DGN.         
           MOVE VEC-DGN-MSC-UTPR(WSS-R) TO DGN-MSC-UTPR IN DGN.         
                                                                        
           MOVE VEC-DGN-VAL-VMIN(WSS-R) TO DGN-VAL-VMIN IN DGN.         
           MOVE VEC-DGN-VAL-VMAX(WSS-R) TO DGN-VAL-VMAX IN DGN.         
           MOVE VEC-DGN-COD-VMYM(WSS-R) TO DGN-COD-VMYM IN DGN.         
                                                                        
           MOVE VEC-DGN-IND-MIVA(WSS-R) TO DGN-IND-MIVA IN DGN.         
           MOVE VEC-DGN-NUM-VIGD(WSS-R) TO DGN-NUM-VIGD IN DGN.         
           MOVE VEC-DGN-MSC-UTVD(WSS-R) TO DGN-MSC-UTVD IN DGN.         
           MOVE VEC-DGN-IND-ACAL(WSS-R) TO DGN-IND-ACAL IN DGN.         
                                                                        
           MOVE VEC-DGN-GLS-DISP(WSS-R) TO DGN-GLS-DISP IN DGN.         
       FIN-CRG-VEC-DGN.
           EXIT.

       PRO-VEC-SORT SECTION.
       INI-PRO-VEC-SORT.
           PERFORM ORD-UNO VARYING WSS-V
                           FROM WSS-K BY -1
                           UNTIL WSS-V < 1.
       FIN-PRO-VEC-SORT.
           EXIT.

       ORD-UNO SECTION.
       INI-ORD-UNO.
           COMPUTE WSS-V1 = WSS-V - 1.
           PERFORM ORD-UNO-A VARYING WSS-V2
                             FROM 1 BY 1
                             UNTIL WSS-V2 > WSS-V1.
       FIN-ORD-UNO.
           EXIT.

       ORD-UNO-A SECTION.
       INI-ORD-UNO-A.
           COMPUTE WSS-V3 = WSS-V2 + 1.
           IF VEC-DGN-KEY1(WSS-V2) = VEC-DGN-KEY1(WSS-V3)               
              MOVE 'E' TO VEC-DGN-GLS-FLAG(WSS-V2).                     
            IF VEC-DGN-KEY1(WSS-V2) > VEC-DGN-KEY1(WSS-V3)
                PERFORM SWP-VEC-DGN.
       FIN-ORD-UNO-A.
           EXIT.

       SWP-VEC-DGN SECTION.
       INI-SWP-VEC-DGN.
           MOVE VEC-DGN-KEY1(WSS-V2) TO TMP-DGN-KEY1.
           MOVE VEC-DGN-DATA(WSS-V2) TO TMP-DGN-DATA.

           MOVE VEC-DGN-KEY1(WSS-V3) TO VEC-DGN-KEY1(WSS-V2).
           MOVE VEC-DGN-DATA(WSS-V3) TO VEC-DGN-DATA(WSS-V2).

           MOVE TMP-DGN-KEY1         TO VEC-DGN-KEY1(WSS-V3).
           MOVE TMP-DGN-DATA         TO VEC-DGN-DATA(WSS-V3).
       FIN-SWP-VEC-DGN.
           EXIT.

       BUS-EQV-MNAC SECTION.
       INI-BUS-EQV-MNAC.
           MOVE ZEROES TO WSS-CGN-E IN CGN-VARI.
       LUP-BUS-EQV-MNAC.
           ADD 1 TO WSS-CGN-E IN CGN-VARI.
           IF WSS-CGN-E IN CGN-VARI > WSS-CGN-EMAX IN CGN-VARI OR
              WSS-CGN-COD-VCOC(WSS-CGN-E) NOT > SPACES
              GO TO LEE-BUS-EQV-MNAC.
           IF WSS-CGN-COD-VCOC(WSS-CGN-E) = DGN-COD-VCMT IN DGN
              MOVE WSS-CGN-SGV-VCAM(WSS-CGN-E) TO WSS-CGN-SGV-VEQV
              GO TO CNT-BUS-EQV-MNAC
           ELSE
              GO TO LUP-BUS-EQV-MNAC.
       LEE-BUS-EQV-MNAC.
           PERFORM GET-FHOY.
           MOVE DGN-COD-VCMT IN DGN      TO CAM-COD-VCAM IN CAM(1).
           MOVE WSS-COD-PESO IN WSS-VARI TO CAM-COD-VCAM IN CAM(2).
           MOVE HOY-FHOY    TO CAM-FEC-FCAM IN CAM(1).
           MOVE HOY-FHOY    TO CAM-FEC-FCAM IN CAM(2).
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM TAB-FIO-CAM.
           IF NOT (FIO-STAT-OKS AND CAM-MSC-STAT IN CAM = 'S')
              MOVE 'MC2'        TO MSG-COD-SIST
              MOVE 'CAM    NEX' TO MSG-COD-MENS
              GO TO FIN-BUS-EQV-MNAC.
           MOVE CAM-SGV-VCAM IN CAM TO WSS-CGN-SGV-VEQV IN CGN-VARI.
           IF WSS-CGN-E IN CGN-VARI NOT > WSS-CGN-EMAX IN CGN-VARI
              MOVE DGN-COD-VCMT IN DGN          TO
                                         WSS-CGN-COD-VCOC(WSS-CGN-E)
              MOVE WSS-CGN-SGV-VEQV IN CGN-VARI TO
                                         WSS-CGN-SGV-VCAM(WSS-CGN-E).
       CNT-BUS-EQV-MNAC.
           MULTIPLY DGN-VAL-MTOP IN DGN BY WSS-CGN-SGV-VEQV IN CGN-VARI
             GIVING VEC-DGN-VAL-MTOPP(WSS-K).
       FIN-BUS-EQV-MNAC.
           EXIT.

       ELI-ALL-CGN SECTION.
       INI-ELI-ALL-CGN.
           PERFORM ELI-ALL-DGN.
           IF MSG-COD-MENS > SPACES
              GO TO MSG-ELI-ALL-CGN.
      *ELIMINA CGN
           PERFORM KEY-ALL-CGN.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-CGN.
           IF FIO-STAT-OKS AND
              CGN-STP-ITRN IN CGN NOT = SCR-UMOD-CGN
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM MC2-FIO-CGN
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-CGN
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM MC2-FIO-DGN
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-DGN
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-ELI-ALL-CGN.
           IF NOT FIO-STAT-OKS
               GO TO FIN-ELI-ALL-CGN.
      *PVG INI 02-FEB-2012
           MOVE CGN-CIC-ICGN IN CGN TO CGN-CIC-ICGN IN CGN-FLD.
      *PVG FIN 02-FEB-2012
           MOVE FIO-DEL TO FIO-CMND.
           PERFORM MC2-FIO-CGN.
           MOVE FIO-STAT TO PGM-STAT-CGN.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-CGN
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-CGN
              MOVE '23' TO FIO-STAT PGM-STAT-DGN PGM-STAT-DGN
              GO TO FIN-ELI-ALL-CGN.

      *ELIMINA DPL
           MOVE 'MC2'                   TO FIO-SIST.
           MOVE 'DPL'                   TO TAB-COD-TTAB IN TAB.
           MOVE CGN-CIC-ICGN IN CGN-FLD TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
              MOVE PGM-STAT-DGN TO FIO-STAT
              GO TO FIN-ELI-ALL-CGN.
           MOVE FIO-DEL TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
              MOVE PGM-STAT-DGN TO FIO-STAT
              GO TO FIN-ELI-ALL-CGN.
           GO TO FIN-ELI-ALL-CGN.
       MSG-ELI-ALL-CGN.
           IF MSG-COD-SIST NOT > SPACES
              MOVE 'MC2' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MEN1.
           MOVE 'S' TO SCR-IERR.
       FIN-ELI-ALL-CGN.
           EXIT.
                                                                        
      *PVG INI 13-DIC-2007 ACTUALIZA CORRELATIVOS                       
       PRO-ACT-NCOR SECTION.                                            
       INI-PRO-ACT-NCOR.                                                
           MOVE SPACES TO MSG-COD-MENS.                                 
           MOVE ZEROES TO WSS-K.                                        
           MOVE CGN-CIC-ICGN IN CGN-FLD TO DGN-CIC-IDGN IN DGN.         
           MOVE ZEROES                  TO DGN-NUM-IDGN IN DGN.         
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-PRO-ACT-NCOR.                                                
           PERFORM MC2-FIO-DGN.                                         
           IF NOT FIO-STAT-OKS                                          
              GO TO CNT-PRO-ACT-NCOR.                                   
           IF NOT (CGN-CIC-ICGN IN CGN-FLD = DGN-CIC-IDGN IN DGN)       
              MOVE FIO-END-BRW TO FIO-CMND                              
              PERFORM MC2-FIO-DGN                                       
              GO TO CNT-PRO-ACT-NCOR.                                   
                                                                        
           ADD 1 TO WSS-K.                                              
           PERFORM CRG-DGN-VEC.                                         
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-PRO-ACT-NCOR.                                      
       CNT-PRO-ACT-NCOR.                                                
           PERFORM ELI-ALL-DGN.                                         
           IF MSG-COD-MENS > SPACES                                     
              GO TO MSG-PRO-ACT-NCOR.                                   
                                                                        
           PERFORM PUT-NEW-DGN.                                         
           IF MSG-COD-MENS > SPACES                                     
               GO TO MSG-PRO-ACT-NCOR.                                  
           GO TO FIN-PRO-ACT-NCOR.                                      
       MSG-PRO-ACT-NCOR.                                                
           IF MSG-COD-SIST NOT > SPACES                                 
              MOVE 'MC2' TO MSG-COD-SIST.                               
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MEN1.                               
           MOVE 'S'  TO SCR-IERR.                                       
           MOVE '00' TO FIO-STAT.                                       
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           GO TO FIN-PRO-ACT-NCOR.                                      
       FIN-PRO-ACT-NCOR.                                                
           EXIT.                                                        
      *PVG FIN 13-DIC-2007                                              
      *>>>>
      *INI-MFC TUXEDO ORACLE
       COPY MC2ZUCGN.
      *FIN-MFC TUXEDO ORACLE
