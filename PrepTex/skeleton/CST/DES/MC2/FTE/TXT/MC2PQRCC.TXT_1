*% IF INI_FIO                                                     
      *<<<< INI_FIO                                                     
           IF SCR-OFRM-SRV                                              
                MOVE TSK-TERM-ALF TO RCC-COD-TERM-CIC                   
           ELSE                                                         
                MOVE EIBTRMID     TO RCC-COD-TERM-CIC.                  
                                                                        
      * Procesamiento Multiple de Contextos (VRF-ACT-ANL)               
           MOVE 'N' TO WSS-IND-MULT.                                    
           MOVE 'TAB'          TO FIO-SIST.                             
           MOVE 'VSN'          TO TAB-COD-TTAB IN TAB.                  
           MOVE 'PROMULT  RCC' TO TAB-COD-CTAB IN TAB.                  
           MOVE FIO-GET-KEY    TO FIO-CMND.                             
           PERFORM GNS-FIO-TAB.                                         
           IF FIO-STAT-OKS AND TAB-MSC-STAT = 'S'                       
              MOVE 'S' TO WSS-IND-MULT.                                 
      *>>>>
*% END
*% IF FND_KEY                                                     
      *<<<< FND_KEY                                                     
           MOVE SPACES TO RCC.                                          
           PERFORM KEY-ALL-RCC.                                         
           IF SCR-CCPP = 'ING'                                          
               MOVE 'I'                     TO RCC-IND-VIGE IN RCC-FLD  
               MOVE 'GNS'                   TO TAB-COD-SIST             
               MOVE 'STT'                   TO TAB-COD-TTAB IN TAB      
               MOVE RCC-IND-VIGE IN RCC-FLD TO TAB-COD-CTAB IN TAB      
               PERFORM BUS-TAB                                          
               MOVE TAB-GLS-DCOR IN TAB TO FRM-GLS-VIGE IN RCC-FLD      
               PERFORM BUS-COR-RCC                                      
               GO TO CON-FND-KEY.                                       
           IF RCC-NUM-IRCC IN RCC NOT > ZEROES                          
               IF NOT SCR-OFRM-SRV                                      
                  PERFORM BUS-COR-RCC.                                  
       CON-FND-KEY.                                                     
           IF MSG-COD-MENS > SPACES                                     
              MOVE '23' TO FIO-STAT                                     
              GO TO FIN-FND-KEY.                                        
                                                                        
      *>>>>
*% END
*% IF FIN_FND_KEY                                                 
      *<<<< FIN_FND_KEY                                                 
           IF NOT FIO-STAT-OKS                                          
               MOVE '23' TO PGM-STAT-RCC                                
           ELSE                                                         
               MOVE '00' TO PGM-STAT-RCC.                               
      *>>>>
*% END
*% IF FIN_HBL_FLD                                                 
      *<<<< FIN_HBL_FLD                                                 
      *>>>>
*% END
*% IF EOF_PQ
      *<<<< EOF
       PGM-EBR-ALL SECTION.                                             
       INI-PGM-EBR-ALL.                                                 
           MOVE 'GNS' TO FIO-SIST.                                      
           MOVE FIO-EBR-ALL TO FIO-CMND.                                
           PERFORM GNS-FIO-TAB.                                         
       FIN-PGM-EBR-ALL.                                                 
           EXIT.                                                        

       BUS-COR-RCC SECTION.                                             
       INI-BUS-COR-RCC.                                                 
           MOVE 999 TO RCC-NUM-IRCC IN RCC.                             
           MOVE FIO-GET-LEQ TO FIO-CMND.                                
           MOVE RCC-KEY-IREG IN RCC TO WSS-KEY-IREG-DEL.                
           PERFORM MC2-FIO-RCC.                                         
           IF FIO-STAT-OKS                                              
      *        MOVE RCC-IND-VIGE IN RCC TO RCC-IND-VIGE IN RCC-FLD      
              MOVE RCC-KEY-IREG IN RCC TO WSS-KEY-IREG-ACT.             
                                                                        
           IF NOT ( FIO-STAT-OKS AND                                    
                    WSS-KEY-IRCC-ACT = WSS-KEY-IRCC-DEL )               
               PERFORM KEY-ALL-RCC                                      
               MOVE ZEROES TO RCC-NUM-IRCC IN RCC                       
               ADD 1                    TO RCC-NUM-IRCC IN RCC          
               MOVE RCC-NUM-IRCC IN RCC TO RCC-NUM-IRCC IN RCC-FLD      
               GO TO FIN-BUS-COR-RCC.                                   
                                                                        
           IF SCR-CCPP = 'ING' OR 'ACT'                                 
              ADD 1                    TO RCC-NUM-IRCC IN RCC           
              MOVE RCC-NUM-IRCC IN RCC TO RCC-NUM-IRCC IN RCC-FLD       
              IF RCC-IND-VIGE IN RCC = 'A'                              
                 MOVE 'TAB' TO MSG-COD-SIST                             
                 MOVE 'RCC    YAACA' TO MSG-COD-MENS                    
                 PERFORM GET-MSG                                        
                 MOVE MSG-GLS-DESC   TO FRM-MENS                        
              ELSE                                                      
              IF RCC-IND-VIGE IN RCC = 'D'                              
                 MOVE 'TAB' TO MSG-COD-SIST                             
                 MOVE 'RCC    YAANL' TO MSG-COD-MENS                    
                 PERFORM GET-MSG                                        
                 MOVE MSG-GLS-DESC   TO FRM-MENS.                       
                                                                        
           IF SCR-CMND = 'ING' AND RCC-NUM-IRCC IN RCC-FLD > 1          
               MOVE 'TAB'               TO FIO-SIST                     
               MOVE 'DPL'               TO TAB-COD-TTAB IN TAB          
               MOVE RCC-CIC-IRCC IN RCC TO TAB-COD-CTAB IN TAB          
               MOVE FIO-GET-KEY TO FIO-CMND                             
               PERFORM GNS-FIO-TAB                                      
               MOVE TAB-GLS-DESC TO FRM-GLS-DES1 IN RCC-FLD             
               MOVE TAB-GLS-DATA TO FRM-GLS-DES2 IN RCC-FLD.            
       FIN-BUS-COR-RCC.                                                 
           EXIT.                                                        
                                                                        
       COPY SGCBFDBC.                                                   
       COPY GNSBVCOD.                                                   
       COPY GNSBVTAB.                                                   
       COPY GNSBBTAB.                                                   
       COPY GNSBBMSC.                                                   
       COPY GNSBGVSM.                                                   
       COPY GNSBGCPT.                                                   
       COPY TABBFOFI.                                                   
       COPY TABBBOFI.                                                   
       COPY TABBVOFI.                                                   
      *PVG INI 19-ENE-2005 PRECIOS-PLN                                  
       COPY MC2BFPLN.                                                   
      *PVG FIN 19-ENE-2005                                              
      *>>>>                                                             
*% END
*% IF INI_CMN_MOD
      *<<<< INI_CMN_MOD                                                 
           IF RCC-IND-VIGE IN RCC-FLD = 'A'                             
               MOVE 'S' TO SCR-IERR                                     
               MOVE 'TAB' TO MSG-COD-SIST                               
               MOVE 'RCCMOD NOACA' TO MSG-COD-MENS                      
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               GO TO FIN-CMN-MOD.                                       
                                                                        
           IF RCC-IND-VIGE IN RCC-FLD = 'D'                             
               MOVE 'S' TO SCR-IERR                                     
               MOVE 'TAB' TO MSG-COD-SIST                               
               MOVE 'RCCMOD NOANL' TO MSG-COD-MENS                      
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               GO TO FIN-CMN-MOD.                                       
                                                                        
           MOVE 'I' TO RCC-IND-VIGE IN RCC-FLD.                         
      *>>>>                                                             
*% END
*% IF FIN_CMN_MOD
      *<<<<
           IF NOT FIO-STAT-OKS                                          
              GO TO FIN-CMN-MOD.                                        
                                                                        
           MOVE SPACES TO WSS-IND-VIGE.                                 
           PERFORM CHK-STAT-RCC.                                        
           MOVE 'I' TO WSS-IND-VIGE.                                    
           PERFORM MOD-STAT-RCC.                                        
           IF WSS-IND-MODR = 'N'                                        
               MOVE 'S' TO SCR-IERR                                     
               MOVE 'TAB' TO MSG-COD-SIST                               
               MOVE 'MODNOT    ' TO MSG-COD-MENS                        
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE WSS-BKP-IRCC TO FRM-MEN2                            
           ELSE                                                         
               MOVE 'MODYES    ' TO MSG-COD-MENS                        
               MOVE 'GNS' TO MSG-COD-SIST                               
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MEN1.                           
                                                                        
           MOVE RCC-CAI-IRCC IN RCC TO RCC-CAI-IRCC-TS.                 
           MOVE RCC-IIC-IRCC IN RCC TO RCC-IIC-IRCC-TS.                 
           MOVE RCC-NOM-TS-CICRCC   TO QUE-COLA.                        
           MOVE RCC-LEN-TS-CICRCC   TO QUE-LITM.                        
           MOVE RCC-ITEM-CICRCC     TO QUE-ITEM.                        
           MOVE QUE-PUT        TO QUE-CMND.                             
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE 'TAB' TO FIO-SIST.                                      
           MOVE 'DPL' TO TAB-COD-TTAB IN TAB.                           
           MOVE RCC-CIC-IRCC IN RCC TO TAB-COD-CTAB IN TAB.             
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM GNS-FIO-TAB.                                         
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-CMN-MOD.                                       
                                                                        
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.                            
           PERFORM GNS-FIO-TAB.                                         
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-CMN-MOD.                                       
                                                                        
           MOVE FRM-GLS-DES1 IN RCC-FLD TO TAB-GLS-DESC IN TAB.         
           MOVE FRM-GLS-DES2 IN RCC-FLD TO TAB-GLS-DATA IN TAB.         
           PERFORM GET-FHOY.                                            
           MOVE HOY-FHOY                TO TAB-FEC-FTRN IN TAB.         
           MOVE HOY-HHOY                TO TAB-HRA-HRTR IN TAB.         
           MOVE SCR-OTRN                TO TAB-COD-OTRN IN TAB.         
           MOVE SCR-USER                TO TAB-COD-ATRN IN TAB.         
           MOVE FIO-MOD TO FIO-CMND.                                    
           PERFORM GNS-FIO-TAB.                                         
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-CMN-MOD.                                       
      *>>>>
*% END
*% IF INI_CMN_ING
      *<<<< INI_CMN_ING                                                 
      *PVG INI 03-NOV-2004                                              
      *    IF RCC-NUM-IRCC IN RCC-FLD = 1                               
      *       GO TO CIC-CMN-ING.                                        
      *    PERFORM KEY-ALL-RCC.                                         
      *    MOVE 1 TO RCC-NUM-IRCC IN RCC.                               
      *    MOVE FIO-GET-KEY TO FIO-CMND.                                
      *    PERFORM MC2-FIO-RCC.                                         
      *    IF FIO-STAT-OKS                                              
      *       MOVE RCC-CAI-IRCC IN RCC TO RCC-CAI-IRCC IN RCC-FLD       
      *       MOVE RCC-IIC-IRCC IN RCC TO RCC-IIC-IRCC IN RCC-FLD       
      *    ELSE                                                         
      *       MOVE 'TAB'               TO MSG-COD-SIST                  
      *       MOVE 'RCCUNO   NEX'      TO MSG-COD-MENS                  
      *       PERFORM GET-MSG                                           
      *       MOVE MSG-GLS-DESC        TO FRM-MENS                      
      *       MOVE '23'                TO FIO-STAT                      
      *       GO TO FIN-CMN-ING.                                        
      *    GO TO CON-CMN-ING.                                           
      *PVG FIN 03-NOV-2004                                              
      *OBTIENE CIC PARA RCC (ENGANCHE A AGD)                            
       CIC-CMN-ING.                                                     
           PERFORM OBT-CIC-RCC.                                         
           IF CIC-STAT NOT = CIC-STAT-OKS                               
              GO TO FIN-CMN-ING.                                        
           MOVE CIC-CAI-ICIC IN CIC TO RCC-CAI-IRCC IN RCC.             
           MOVE CIC-IIC-ICIC IN CIC TO RCC-IIC-IRCC IN RCC.             
           MOVE CIC-CAI-ICIC IN CIC TO RCC-CAI-IRCC IN RCC-FLD.         
           MOVE CIC-IIC-ICIC IN CIC TO RCC-IIC-IRCC IN RCC-FLD.         
           MOVE 'TAB'                   TO FIO-SIST.                    
           MOVE 'DPL'                   TO TAB-COD-TTAB IN TAB          
           MOVE RCC-CIC-IRCC IN RCC     TO TAB-COD-CTAB IN TAB.         
           MOVE FRM-GLS-DES1 IN RCC-FLD TO TAB-GLS-DESC IN TAB.         
           MOVE FRM-GLS-DES2 IN RCC-FLD TO TAB-GLS-DATA IN TAB.         
           MOVE 'I'                     TO TAB-MSC-TACC IN TAB.         
           MOVE 'I'                     TO TAB-MSC-STAT IN TAB.         
           MOVE 'S'                     TO TAB-IND-VIGE IN TAB.         
           MOVE SPACES                  TO TAB-COD-OTRN IN TAB.         
           MOVE 'MC2PURCC'              TO TAB-COD-ATRN IN TAB.         
           MOVE RCC-CIC-IRCC IN RCC     TO TAB-GLS-DCOR IN TAB.         
           MOVE RCC-CAI-IRCC IN RCC     TO TAB-GLS-ABRV IN TAB.         
           PERFORM GET-FHOY.                                            
           MOVE HOY-FHOY                TO TAB-FEC-FTRN IN TAB.         
           MOVE HOY-HHOY                TO TAB-HRA-HRTR IN TAB.         
           MOVE SCR-OTRN                TO TAB-COD-OTRN IN TAB.         
           MOVE SCR-USER                TO TAB-COD-ATRN IN TAB.         
                                                                        
           MOVE FIO-PUT TO FIO-CMND.                                    
           PERFORM GNS-FIO-TAB.                                         
           IF NOT FIO-STAT-OKS                                          
              MOVE 'TAB'               TO MSG-COD-SIST                  
              MOVE 'DPL    NOGRB'      TO MSG-COD-MENS                  
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC        TO FRM-MEN2.                     
      *>>>>                                                             
*% END
*% IF FIN_CMN_ING
      *<<<< FIN_CMN_ING                                                 
           IF NOT (FIO-STAT-OKS AND SCR-OFRM-SRV)                       
                 GO TO FIN-CMN-ING.                                     
      *INFORMA CIC DE RCC (ENGANCHE AGD)                                
           MOVE RCC-CAI-IRCC IN RCC TO RCC-CAI-IRCC-TS.
           MOVE RCC-IIC-IRCC IN RCC TO RCC-IIC-IRCC-TS.                 
           MOVE RCC-NOM-TS-CICRCC   TO QUE-COLA.                        
           MOVE RCC-LEN-TS-CICRCC   TO QUE-LITM.                        
           MOVE RCC-ITEM-CICRCC     TO QUE-ITEM.                        
           MOVE QUE-PUT        TO QUE-CMND.                             
           PERFORM GNS-PRO-QUE.                                         
      *>>>>                                                             
*% END
*% IF INI_CMN_ACC
      *<<<<
           GO TO NXT-CMN-ACC.
      *>>>>
*% END
*% IF CMN_ACC
      *<<<<
       NXT-CMN-ACC.
           PERFORM KEY-ALL-RCC.                                         
      *     MOVE FIO-GET-KEY-UPD TO FIO-CMND.                           
      *     PERFORM MC2-FIO-RCC.                                        
      *     IF FIO-STAT-OKS AND       
      *        RCC-STP-ITRN IN RCC NOT = SCR-UMOD                       
      *         MOVE FIO-ULK-REC TO FIO-CMND
      *         PERFORM MC2-FIO-RCC                                     
      *         MOVE FIO-BCK-OUT TO FIO-CMND                            
      *         PERFORM MC2-FIO-RCC                                     
      *         MOVE 'GNS' TO MSG-COD-SIST                              
      *         MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS                     
      *         PERFORM GET-MSG                                         
      *         MOVE MSG-GLS-DESC TO FRM-MENS                           
      *         GO TO FIN-CMN-ACC.    
      *     IF NOT FIO-STAT-OKS                                         
      *         GO TO FIN-CMN-ACC.                                      
                                      
           PERFORM RET-ALL-RCC.                                         
      *PVG INI 02-NOV-2004                                              
           IF SCR-CMND = 'VRF'                                          
              PERFORM VAL-CON-CPTO    
              IF MSG-COD-MENS > SPACES                                  
                 MOVE 'S'           TO SCR-IERR                         
                 MOVE 'TAB'         TO MSG-COD-SIST
                 PERFORM GET-MSG                                        
                 MOVE MSG-GLS-DESC  TO FRM-MENS                         
                 GO TO FIN-CMN-ACC.
      *PVG FIN 02-NOV-2004                                              
                                                                        
           IF SCR-CMND = 'VRF'                                          
               PERFORM CHK-AGD-RCC                                    
               IF WSS-IND-CHKA = 'N'                                    
                   MOVE 'S' TO SCR-IERR                                 
                   MOVE 'TAB' TO MSG-COD-SIST                           
                   MOVE 'AGDTAGDDUPL' TO MSG-COD-MENS
                   PERFORM GET-MSG                                      
                   MOVE MSG-GLS-DESC TO FRM-MENS                        
               ELSE                                                     
                   MOVE 'I' TO WSS-IND-VIGE
                   PERFORM CHK-STAT-RCC                                 
                   IF WSS-IND-CHKR = 'N'                                
                      MOVE 'S' TO SCR-IERR                              
                      MOVE 'TAB' TO MSG-COD-SIST                        
                      MOVE 'RCCSTATNOING' TO MSG-COD-MENS               
                      PERFORM GET-MSG                                   
                      MOVE MSG-GLS-DESC   TO FRM-MEN1                   
                      MOVE WSS-BKP-IRCC TO FRM-MEN2                     
                   ELSE               
                      MOVE 'V' TO WSS-IND-VIGE                          
                      PERFORM MOD-STAT-RCC
                      IF WSS-IND-MODR = 'N'                             
                         MOVE 'S' TO SCR-IERR
                         MOVE 'TAB' TO MSG-COD-SIST                     
                         MOVE 'VRFNOT    ' TO MSG-COD-MENS              
                         PERFORM GET-MSG
                         MOVE MSG-GLS-DESC TO FRM-MENS                  
                         MOVE WSS-BKP-IRCC TO FRM-MEN2                  
                      ELSE                                              
                         MOVE 'GNS'        TO MSG-COD-SIST              
                         MOVE 'VRFYES    ' TO MSG-COD-MENS            
                         PERFORM GET-MSG                                
                         MOVE MSG-GLS-DESC TO FRM-MEN1.                 
                                      
           IF SCR-CMND = 'ACA'                                          
               MOVE 'V' TO WSS-IND-VIGE  
               PERFORM CHK-STAT-RCC 
               IF WSS-IND-CHKR = 'N'                                    
                  MOVE 'S' TO SCR-IERR    
                  MOVE 'TAB' TO MSG-COD-SIST                            
                  MOVE 'RCCSTATNOVRF' TO MSG-COD-MENS                   
                  PERFORM GET-MSG     
                  MOVE MSG-GLS-DESC   TO FRM-MEN1
                  MOVE WSS-BKP-IRCC TO FRM-MEN2                         
               ELSE                                                     
                  MOVE 'A' TO WSS-IND-VIGE                              
                  PERFORM MOD-STAT-RCC                                  
                  IF WSS-IND-MODR = 'N'                                 
                     MOVE 'S' TO SCR-IERR                               
                     MOVE 'TAB' TO MSG-COD-SIST                 
                     MOVE 'ACANOT    ' TO MSG-COD-MENS
                     PERFORM GET-MSG        
                     MOVE MSG-GLS-DESC TO FRM-MEN1                      
                     MOVE WSS-BKP-IRCC TO FRM-MEN2    
                  ELSE                    
                     MOVE 'GNS'        TO MSG-COD-SIST
                     MOVE 'ACAYES    ' TO MSG-COD-MENS
                     PERFORM GET-MSG         
                     MOVE MSG-GLS-DESC TO FRM-MEN1.                     
                                                                        
           IF SCR-CMND = 'ANL'                                          
               MOVE 'A' TO WSS-IND-VIGE                                 
               PERFORM CHK-STAT-RCC                                     
               IF WSS-IND-CHKR = 'N'                                    
                  MOVE 'S' TO SCR-IERR                                  
                  MOVE 'TAB' TO MSG-COD-SIST                            
                  MOVE 'RCCSTATNOACA' TO MSG-COD-MENS                   
                  PERFORM GET-MSG                                       
                  MOVE MSG-GLS-DESC   TO FRM-MEN1                       
                  MOVE WSS-BKP-IRCC TO FRM-MEN2                         
               ELSE                                                     
                  MOVE 'D' TO WSS-IND-VIGE                              
                  PERFORM MOD-STAT-RCC                                  
                  IF WSS-IND-MODR = 'N'                                 
                     MOVE 'S' TO SCR-IERR                               
                     MOVE 'TAB' TO MSG-COD-SIST                         
                     MOVE 'ANLNOT    ' TO MSG-COD-MENS                  
                     PERFORM GET-MSG                                    
                     MOVE MSG-GLS-DESC TO FRM-MEN1                      
                     MOVE WSS-BKP-IRCC TO FRM-MEN2                      
                  ELSE                                                  
                     MOVE 'GNS'        TO MSG-COD-SIST                  
                     MOVE 'ANLYES    ' TO MSG-COD-MENS                  
                     PERFORM GET-MSG                                    
                     MOVE MSG-GLS-DESC TO FRM-MEN1.                     
      *>>>>
*% END
*% IF INI_CMN_ELI
      *<<<< INI_CMN_ELI                                                 
           IF RCC-IND-VIGE IN RCC-FLD = 'A'                             
               MOVE 'S' TO SCR-IERR                                     
               MOVE 'TAB' TO MSG-COD-SIST                               
               MOVE 'RCCELI NOACA' TO MSG-COD-MENS                      
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               GO TO FIN-CMN-ELI.                                       
                                                                        
           IF RCC-IND-VIGE IN RCC-FLD = 'D'                             
               MOVE 'S' TO SCR-IERR                                     
               MOVE 'TAB' TO MSG-COD-SIST                               
               MOVE 'RCCELI NOANL' TO MSG-COD-MENS                      
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               GO TO FIN-CMN-ELI.                                       
      *>>>>                                                             
*% END
*% IF FIN_CMN_ELI
      *<<<<
           MOVE FIO-STAT TO PGM-STAT-RCC.                               
           PERFORM ELI-AGD.                                             
           MOVE PGM-STAT-RCC TO FIO-STAT.                               
      *>>>>
*% END
*% IF EOF_PU
      *<<<< EOF                                                         
       PGM-EBR-ALL SECTION.                                             
       INI-PGM-EBR-ALL.                                                 
           MOVE 'GNS' TO FIO-SIST.                                      
           MOVE FIO-EBR-ALL TO FIO-CMND.                                
           PERFORM GNS-FIO-TAB.                                         
       FIN-PGM-EBR-ALL.                                                 
           EXIT.                                                        
       COPY SGCBFDBC.                                                   
       COPY MC2BFAGD.                                                   
       COPY GNSBVCOD.                                                   
       COPY GNSBVTAB.                                                   
       COPY GNSBBTAB.                                                   
       COPY GNSBBMSC.                                                   
       COPY GNSBGVSM.                                                   
       COPY GNSBGCPT.                                                   
       COPY TABBFOFI.                                                   
       COPY TABBBOFI.                                                   
       COPY TABBVOFI.                                                   
                                                                        
       COPY GNSBGCIC.                                                   
       COPY GNSBFCIC.                                                   
                                                                        
       OBT-CIC-RCC SECTION.                                             
       INI-OBT-CIC-RCC.                                                 
           MOVE 'PRCC'  TO CIC-CAI-ICIC IN CIC.                         
           MOVE CIC-GET TO CIC-CMND.                                    
           PERFORM PRO-CIC.                                             
           IF CIC-STAT NOT = CIC-STAT-OKS                               
               MOVE CIC-MENS     TO FRM-MENS                            
               MOVE FIO-STAT-NEX TO FIO-STAT                            
           ELSE                                                         
               MOVE CIC-CAI-ICIC TO WSS-CPT-CAI                         
               MOVE CIC-IIC-ICIC TO WSS-CPT-IIC                         
               MOVE WSS-CIC-PRCC TO FRM-MEN1.                           
       FIN-OBT-CIC-RCC.                                                 
           EXIT.                                                        
                                                                        
       CHK-AGD-RCC SECTION.                                             
       INI-CHK-AGD-RCC.                                                 
           MOVE ZEROES TO WSS-AGD-IMAX                                  
           MOVE SPACES TO WSS-IND-CHKA.                                 
           MOVE RCC-CAI-IRCC IN RCC TO AGD-CAI-IAGD IN AGD.             
           MOVE RCC-IIC-IRCC IN RCC TO AGD-IIC-IAGD IN AGD.             
           MOVE ZEROES              TO AGD-NUM-IAGD IN AGD.             
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-CHK-AGD-RCC.                                                 
           PERFORM MC2-FIO-AGD.                                         
           IF NOT (FIO-STAT-OKS AND                                     
              AGD-CAI-IAGD IN AGD = RCC-CAI-IRCC IN RCC AND             
              AGD-IIC-IAGD IN AGD = RCC-IIC-IRCC IN RCC)                
               GO TO FIN-CHK-AGD-RCC.                                   
                                                                        
           PERFORM CHK-AGD VARYING WSS-AGD-I FROM 1 BY 1 UNTIL          
             WSS-AGD-I > WSS-AGD-IMAX OR                                
             AGD-COD-TAGD IN AGD = WSS-COD-TAGD(WSS-AGD-I).             
                                                                        
      * Encuentra la AGD Repetida en el arreglo                         
           IF WSS-AGD-I NOT > WSS-AGD-IMAX                              
               MOVE 'N' TO WSS-IND-CHKA                                 
               GO TO FIN-CHK-AGD-RCC.                                   
                                                                        
      * No encuentra la AGD  en el arreglo                              
           ADD 1 TO WSS-AGD-IMAX.                                       
           IF WSS-AGD-IMAX > WSS-AGD-MAX-IMAX                           
               MOVE 'TAB' TO MSG-COD-SIST                               
               MOVE 'MAXAGD OVRFL'      TO MSG-COD-MENS                 
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               GO TO FIN-CHK-AGD-RCC.                                   
                                                                        
           MOVE AGD-COD-TAGD IN AGD TO WSS-COD-TAGD(WSS-AGD-IMAX)       
                                                                        
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-CHK-AGD-RCC.                                       
                                                                        
       FIN-CHK-AGD-RCC.                                                 
           EXIT.                                                        
                                                                       
       CHK-AGD SECTION.                                                 
       INI-CHK-AGD.                                                     
       FIN-CHK-AGD.                                                     
           EXIT.                                                  

       ELI-AGD SECTION.                                               
       INI-ELI-AGD.                                                     
           MOVE RCC-CIC-IRCC IN RCC TO AGD-CIC-IAGD IN AGD.             
           MOVE ZEROES              TO WSS-NUM-ERCC.                    
           MOVE RCC-KEY-IREG        TO WSS-KEY-IREG-DEL.        
           MOVE ZEROES              TO RCC-NUM-IRCC IN RCC.             
           MOVE FIO-GET-NLS         TO FIO-CMND.                        
       LUP-ELI-AGD.                                                     
           PERFORM MC2-FIO-RCC.                                         
           IF NOT FIO-STAT-OKS                                          
              GO TO CNT-AGD-RCC.                                        
           MOVE RCC-KEY-IREG IN RCC TO WSS-KEY-IREG-ACT.                
                                                                        
           IF WSS-KEY-IRCC-ACT NOT = WSS-KEY-IRCC-DEL                   
              GO TO CNT-AGD-RCC.                                        
           IF WSS-ACT-IRCC NOT = WSS-DEL-IRCC                           
              ADD 1 TO WSS-NUM-ERCC.                                  

           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-ELI-AGD.                                           
                                                                        
       CNT-AGD-RCC.                                                     
           IF WSS-NUM-ERCC > ZEROES                                     
              GO TO FIN-ELI-AGD.      
           MOVE ZEROES TO WSS-NUM-ERCC.                                 
       LUP-AGD-RCC.                                                           
                                                            
           ADD 1             TO WSS-NUM-ERCC.                           
           MOVE WSS-NUM-ERCC TO AGD-NUM-IAGD IN AGD.                    
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.                            
           PERFORM MC2-FIO-AGD.                                         
           IF FIO-STAT-OKS                                              
              MOVE FIO-DEL TO FIO-CMND                                  
              PERFORM MC2-FIO-AGD                                       
              GO TO LUP-AGD-RCC.                                        
       FIN-ELI-AGD.                                                     
           EXIT.                                                        
                                                                     
       CHK-STAT-RCC SECTION.                                            
       INI-CHK-STAT-RCC.                                                
      *PVG INI 13-OCT-2004                                              
           IF WSS-IND-MULT = 'N'                                        
              IF RCC-IND-VIGE IN RCC NOT = WSS-IND-VIGE AND             
                 WSS-IND-VIGE > SPACES                                  
                 MOVE 'N' TO WSS-IND-CHKR                               
                 MOVE RCC-NUM-IRCC IN RCC TO WSS-BKP-IRCC.              
           GO TO FIN-CHK-STAT-RCC.                                      
      *PVG FIN 13-OCT-2004                                              

           PERFORM CLR-VEC-IRCC.                                        
           MOVE SPACES TO WSS-IND-CHKR.                                 
           MOVE RCC-KEY-IREG IN RCC TO WSS-KEY-IREG-DEL.                
           MOVE ZEROES TO RCC-NUM-IRCC IN RCC.                          
           MOVE ZEROES TO WSS-IRCC.                                     
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-CHK-STAT-RCC.                                                
           PERFORM MC2-FIO-RCC.                                         
           MOVE RCC-KEY-IREG IN RCC TO WSS-KEY-IREG-ACT.                
           IF NOT (FIO-STAT-OKS AND                                     
              WSS-KEY-IRCC-ACT = WSS-KEY-IRCC-DEL)                      
               GO TO EXT-CHK-STAT-RCC.                                  
                                                                        
           IF RCC-IND-VIGE IN RCC NOT = WSS-IND-VIGE AND                
              WSS-IND-VIGE > SPACES                                     
               MOVE 'N' TO WSS-IND-CHKR                                 
               MOVE RCC-NUM-IRCC IN RCC TO WSS-BKP-IRCC                 
               GO TO EXT-CHK-STAT-RCC.                                  
                                                                        
           ADD 1 TO WSS-IRCC.                                           
           MOVE RCC-NUM-IRCC IN RCC TO WSS-NUM-IRCC(WSS-IRCC).          
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-CHK-STAT-RCC.                                      
       EXT-CHK-STAT-RCC.                                                
           MOVE WSS-KEY-IREG-DEL TO RCC-KEY-IREG IN RCC.                
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM MC2-FIO-RCC.                                         
       FIN-CHK-STAT-RCC.                                                
           EXIT.                                                        
                                                                        
       MOD-STAT-RCC SECTION.                                            
       INI-MOD-STAT-RCC.                                                
           MOVE SPACES TO MSG-COD-MENS.                                 
           MOVE SPACES TO WSS-IND-MODR.                                 
           MOVE RCC-KEY-IREG IN RCC TO WSS-KEY-IREG-DEL.                
                                                                        
           IF WSS-IND-MULT = 'N'                                        
              MOVE 1                   TO WSS-IRCC                      
              MOVE RCC-NUM-IRCC IN RCC TO WSS-NUM-IRCC(WSS-IRCC)        
              PERFORM MOD-RCC                                           
              GO TO CNT-MOD-STAT-RCC.                                   
                                                                        
           PERFORM MOD-RCC VARYING WSS-IRCC FROM 1 BY 1 UNTIL           
             WSS-IRCC > WSS-IRCC-MAX OR                                 
             WSS-NUM-IRCC(WSS-IRCC) NOT > ZEROES OR                     
             MSG-COD-MENS > SPACES.                                     
                                                                        
           IF WSS-IRCC > WSS-IRCC-MAX                                   
              MOVE 'MAXVECRCCEXE' TO MSG-COD-MENS.                      
                                                                        
       CNT-MOD-STAT-RCC.                                                
           IF MSG-COD-MENS > SPACES                                     
              MOVE 'N' TO WSS-IND-MODR.                                 
       FIN-MOD-STAT-RCC.                                                
           EXIT.                                                        
                                                                        
       MOD-RCC SECTION.                                                 
       INI-MOD-RCC.                                                     
           MOVE WSS-KEY-IREG-DEL       TO RCC-KEY-IREG IN RCC.          
           MOVE WSS-NUM-IRCC(WSS-IRCC) TO RCC-NUM-IRCC IN RCC.          
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM MC2-FIO-RCC.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE 'RCC    NEX' TO MSG-COD-MENS                        
               MOVE WSS-NUM-IRCC(WSS-IRCC) TO WSS-BKP-IRCC              
               GO TO FIN-MOD-RCC.                                       
                                                                        
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.                            
           PERFORM MC2-FIO-RCC.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE 'RCC  NOUPD' TO MSG-COD-MENS                        
               MOVE WSS-NUM-IRCC(WSS-IRCC) TO WSS-BKP-IRCC              
               GO TO FIN-MOD-RCC.                                       
                                                                        
           MOVE WSS-IND-VIGE TO RCC-IND-VIGE IN RCC.                    
           MOVE FIO-MOD TO FIO-CMND.                                    
           PERFORM MC2-FIO-RCC.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE 'RCC  NOMOD' TO MSG-COD-MENS                        
               MOVE WSS-NUM-IRCC(WSS-IRCC) TO WSS-BKP-IRCC              
               GO TO FIN-MOD-RCC.                                       
                                                                        
       FIN-MOD-RCC.                                                     
           EXIT.                                                        
                                                                        
       CLR-VEC-IRCC SECTION.                                            
       INI-CLR-VEC-IRCC.                                                
           MOVE ZEROES TO WSS-IRCC.                                     
       LUP-CLR-VEC-IRCC.                                                
           ADD 1 TO WSS-IRCC.                                           
           IF WSS-IRCC > WSS-IRCC-MAX                                   
              GO TO FIN-CLR-VEC-IRCC.                                   
                                                                        
           MOVE ZEROES TO WSS-NUM-IRCC(WSS-IRCC).                       
           GO TO LUP-CLR-VEC-IRCC.                                      
                                                                        
       FIN-CLR-VEC-IRCC.                                                
           EXIT.                                                        
                                                                        
      *PVG INI 02-NOV-2004                                              
       VAL-CON-CPTO SECTION.                                            
       INI-VAL-CON-CPTO.                                                
      *CARGA VALORES RCC ACTUAL                                         
           MOVE RCC-KEY-IRCC IN RCC TO WSS-KEY-RCC-A  IN WSS-VARI.      
           MOVE RCC-COD-CPTO IN RCC TO WSS-COD-CPTO-A IN WSS-VARI.      
           MOVE RCC-IND-TPLT IN RCC TO WSS-IND-TPLT-A IN WSS-VARI.      
      *LEE AGD ASOCIADAS                                                
           MOVE SPACES              TO WSS-KEY-AGD-A IN WSS-VARI.       
           MOVE RCC-CIC-IRCC IN RCC TO AGD-CIC-IAGD IN AGD.             
           MOVE ZEROES              TO AGD-NUM-IAGD IN AGD.             
           MOVE FIO-GET-NLS         TO FIO-CMND.                        
       LUP-VAL-CON-CPTO-A.                                              
           PERFORM MC2-FIO-AGD.                                         
           MOVE FIO-STAT TO PGM-STAT-AGD.                               
           IF NOT (FIO-STAT-OKS AND                                     
               AGD-CIC-IAGD IN AGD = RCC-CIC-IRCC IN RCC)               
               GO TO FIN-VAL-CON-CPTO-A.                                
                                                                        
           IF AGD-COD-TAGD IN AGD = 'CNV'                               
              MOVE AGD-COD-CAGD IN AGD TO WSS-COD-CAGD-A(1)             
           ELSE                                                         
           IF AGD-COD-TAGD IN AGD = 'PAC'                               
              MOVE AGD-COD-CAGD IN AGD TO WSS-COD-CAGD-A(2)             
           ELSE                                                         
           IF AGD-COD-TAGD IN AGD = 'AGS'                               
              MOVE AGD-COD-CAGD IN AGD TO WSS-COD-CAGD-A(3)             
           ELSE                                                         
           IF AGD-COD-TAGD IN AGD = 'PLN'                               
              MOVE AGD-COD-CAGD IN AGD TO WSS-COD-CAGD-A(4)             
           ELSE                                                         
           IF AGD-COD-TAGD IN AGD = 'CRI'                               
              MOVE AGD-COD-CAGD IN AGD TO WSS-COD-CAGD-A(5)             
           ELSE                                                         
           IF AGD-COD-TAGD IN AGD = 'NED'                               
              MOVE AGD-COD-CAGD IN AGD TO WSS-COD-CAGD-A(6)             
           ELSE                                                         
           IF AGD-COD-TAGD IN AGD = 'SEX'                               
              MOVE AGD-COD-CAGD IN AGD TO WSS-COD-CAGD-A(7)             
           ELSE                                                         
           IF AGD-COD-TAGD IN AGD = 'TCL'                               
              MOVE AGD-COD-CAGD IN AGD TO WSS-COD-CAGD-A(8).            
                                                                        
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-VAL-CON-CPTO-A.                                    
       FIN-VAL-CON-CPTO-A.                                              
           MOVE PGM-STAT-AGD TO FIO-STAT.                               
           IF FIO-STAT-OKS                                              
              MOVE FIO-END-BRW TO FIO-CMND                              
              PERFORM MC2-FIO-AGD.                                      
                                                                        
      *BUSCA CONTEXTOS IDENTICOS ( RCC Y AGD )                          
           MOVE RCC-KEY-IRCC IN RCC TO WSS-KEY-RCC-A IN WSS-VARI.       
           MOVE ZEROES              TO RCC-NUM-IRCC IN RCC.             
           MOVE FIO-GET-NLS         TO FIO-CMND.                        
       LUP-VAL-CON-CPTO-B.                                              
           PERFORM MC2-FIO-RCC.                                         
           MOVE FIO-STAT            TO PGM-STAT-RCC.                    
           MOVE RCC-KEY-IRCC IN RCC TO WSS-KEY-RCC-B IN WSS-VARI.       
           IF NOT (FIO-STAT-OKS AND                                     
                   WSS-KEY-IRCC-B IN WSS-VARI =                         
                   WSS-KEY-IRCC-A IN WSS-VARI)                          
              GO TO FIN-VAL-CON-CPTO-B.                                 
           IF RCC-KEY-IRCC IN RCC = WSS-KEY-RCC-A IN WSS-VARI           
              GO TO NXT-VAL-CON-CPTO-B.                                 
           IF NOT (RCC-MSC-STAT IN RCC = 'V' OR 'A')                    
              GO TO NXT-VAL-CON-CPTO-B.                                 
                                                                        
           MOVE RCC-COD-CPTO IN RCC TO WSS-COD-CPTO-B IN WSS-VARI.      
           MOVE RCC-IND-TPLT IN RCC TO WSS-IND-TPLT-B IN WSS-VARI.      
                                                                        
           MOVE SPACES              TO WSS-KEY-AGD-B IN WSS-VARI.       
           MOVE RCC-CIC-IRCC IN RCC TO AGD-CIC-IAGD IN AGD.             
           MOVE ZEROES              TO AGD-NUM-IAGD IN AGD.             
           MOVE FIO-GET-NLS         TO FIO-CMND.                        
       LUP-VAL-CON-CPTO-B-AGD.                                          
           PERFORM MC2-FIO-AGD.                                         
           MOVE FIO-STAT TO PGM-STAT-AGD.                               
           IF NOT (FIO-STAT-OKS AND                                     
               AGD-CIC-IAGD IN AGD = RCC-CIC-IRCC IN RCC)               
               GO TO CMP-VAL-CON-CPTO-B.                                
                                                                        
           IF AGD-COD-TAGD IN AGD = 'CNV'                               
              MOVE AGD-COD-CAGD IN AGD TO WSS-COD-CAGD-B(1)             
           ELSE                                                         
           IF AGD-COD-TAGD IN AGD = 'PAC'                               
              MOVE AGD-COD-CAGD IN AGD TO WSS-COD-CAGD-B(2)             
           ELSE                                                         
           IF AGD-COD-TAGD IN AGD = 'AGS'                               
              MOVE AGD-COD-CAGD IN AGD TO WSS-COD-CAGD-B(3)             
           ELSE                                                         
           IF AGD-COD-TAGD IN AGD = 'PLN'                               
              MOVE AGD-COD-CAGD IN AGD TO WSS-COD-CAGD-B(4)             
           ELSE                                                         
           IF AGD-COD-TAGD IN AGD = 'CRI'                               
              MOVE AGD-COD-CAGD IN AGD TO WSS-COD-CAGD-B(5)             
           ELSE                                                         
           IF AGD-COD-TAGD IN AGD = 'NED'                               
              MOVE AGD-COD-CAGD IN AGD TO WSS-COD-CAGD-B(6)             
           ELSE                                                         
           IF AGD-COD-TAGD IN AGD = 'SEX'                               
              MOVE AGD-COD-CAGD IN AGD TO WSS-COD-CAGD-B(7)             
           ELSE                                                         
           IF AGD-COD-TAGD IN AGD = 'TCL'                               
              MOVE AGD-COD-CAGD IN AGD TO WSS-COD-CAGD-B(8).            
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-VAL-CON-CPTO-B-AGD.                                
                                                                        
       CMP-VAL-CON-CPTO-B.                                              
           MOVE PGM-STAT-AGD TO FIO-STAT.                               
           IF FIO-STAT-OKS                                              
              MOVE FIO-END-BRW TO FIO-CMND                              
              PERFORM MC2-FIO-AGD.                                      
           IF NOT ( WSS-KEY-AGD-A  IN WSS-VARI =                        
                    WSS-KEY-AGD-B  IN WSS-VARI AND                      
                    WSS-KEY-IRCC-A IN WSS-VARI =                        
                    WSS-KEY-IRCC-B IN WSS-VARI AND                      
                    WSS-COD-CPTO-A IN WSS-VARI =                        
                    WSS-COD-CPTO-B IN WSS-VARI)                         
              GO TO NXT-VAL-CON-CPTO-B.                                 
                                                                        
      *VALIDA TIPO DE CONCEPTO ASOCIADO                                 
      *ES VALIDO SOLO UN CONCEPTO DE PRECIOS                            
           IF WSS-IND-TPLT-A = 'P' AND WSS-IND-TPLT-B = 'P'             
              MOVE 'PLTP  INV' TO MSG-COD-MENS                          
              GO TO FIN-VAL-CON-CPTO-B.                                 
                                                                        
      *ES VALIDO SOLO CONCEPTOS RECARGOS O SOLO CONCEPTOS DE DESCUENTOS 
      *CASO EN QUE SE VERIFICA UN DESCUENTO                             
           IF ((WSS-IND-TPLT-A = '%' OR 'M') AND                        
               (WSS-IND-TPLT-B = 'X' OR 'Y') )                          
              MOVE 'YAEXIPLTR' TO MSG-COD-MENS                          
              GO TO FIN-VAL-CON-CPTO-B.                                 
                                                                        
      *CASO EN QUE SE VERIFICA UN RECARGO                               
           IF ((WSS-IND-TPLT-A = 'X' OR 'Y') AND                        
               (WSS-IND-TPLT-B = '%' OR 'M') )                          
              MOVE 'YAEXIPLTD' TO MSG-COD-MENS                          
              GO TO FIN-VAL-CON-CPTO-B.                                 
                                                                        
       NXT-VAL-CON-CPTO-B.                                              
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-VAL-CON-CPTO-B.                                    
                                                                        
       FIN-VAL-CON-CPTO-B.                                              
           MOVE PGM-STAT-RCC TO FIO-STAT.                               
           IF FIO-STAT-OKS                                              
              MOVE FIO-END-BRW TO FIO-CMND                              
              PERFORM MC2-FIO-RCC.                                      
                                                                        
           MOVE WSS-KEY-RCC-A IN WSS-VARI TO RCC-KEY-IRCC IN RCC.       
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM MC2-FIO-RCC.                                         
       EXT-VAL-CON-CPTO.                                                
           EXIT.                                                        
      *PVG FIN 02-NOV-2004                                              
      *>>>>
*% END                                                                        
