*%! Codigo adicional para archivo
*% IF SETTAG
*% VSNBQ = "05.11.21.1"
*% VSNBU = "05.11.21.1"
*% FNCBQ = "MC2-CONVENIO"
*% FNCBU = "MC2-CONVENIO"
*% END
*% IF FIN_FST_FLD_DPC
      *<<<< FIN_FST_FLD_DPC
      *>>>>
*% END
*% IF FIN_UNP_DPC_DPC
      *<<<< FIN_UNP_DPC_DPC
      *>>>>
*% END
*% IF FIN_KEY_DPC_DPC
      *<<<< FIN_KEY_DPC_DPC
           MOVE SCR-CAI-ICLI TO DPC-CAI-ICLI IN DPC.
           MOVE SCR-IIC-ICLI TO DPC-IIC-ICLI IN DPC.
           IF DPC-COD-SIST IN DPC-FLD NOT > SPACES
               MOVE FRM-COD-GRPO IN DPC-FLD TO DPC-COD-GRPO IN DPC.
      *>>>>
*% END
*% IF FIN_PUT_DPC_DPC
      *<<<< FIN_PUT_DPC_DPC
           IF DPC-COD-SIST IN DPC NOT > SPACES
               MOVE SPACES TO DPC-COD-VCOC IN DPC-FLD
               MOVE SPACES TO DPC-COD-TOOC IN DPC-FLD
               MOVE SPACES TO DPC-COD-AOOC IN DPC-FLD.
              
           MOVE 'MC2'               TO TAB-COD-SIST.                    
           MOVE 'DCV'               TO TAB-COD-TTAB IN TAB.             
           MOVE DPC-CIC-ICVN IN DPC TO TAB-COD-CTAB IN TAB.
           PERFORM BUS-TAB.
           IF MSG-COD-MENS NOT > SPACES
              MOVE TAB-GLS-DESC TO FRM-GLS-DSCN IN DPC-FLD
           ELSE
              MOVE SPACES TO MSG-COD-MENS
              MOVE 'SIN DESCRIPCION' TO FRM-GLS-DSCN IN DPC-FLD.

           IF DPC-COD-TCNC IN DPC-FLD > SPACES
              MOVE 'TAB'                   TO TAB-COD-SIST
              MOVE 'CNL'                   TO TAB-COD-TTAB IN TAB
              MOVE DPC-COD-TCNC IN DPC-FLD TO TAB-COD-CTAB IN TAB
              PERFORM BUS-TAB
              MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-TCNC IN DPC-FLD
           ELSE
              MOVE SPACES              TO FRM-GLS-TCNC IN DPC-FLD.

           IF DPC-COD-CNAC IN DPC-FLD = SPACES
              MOVE 'TODOS        ' TO FRM-GLS-CNAC IN DPC-FLD
              GO TO CNT-COD-CNAC.
           IF DPC-COD-TCNC IN DPC NOT = '010'
              MOVE 'TAB'                   TO TAB-COD-SIST
              MOVE DPC-COD-TCNC IN DPC     TO TAB-COD-TTAB IN TAB
              MOVE DPC-COD-CNAC IN DPC     TO TAB-COD-CTAB IN TAB
              PERFORM BUS-TAB
              MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-CNAC IN DPC-FLD
           ELSE
              MOVE DPC-COD-CNAC IN DPC     TO OFI-COD-OFIC IN OFI
              PERFORM BUS-OFI
              MOVE OFI-GLS-DESC IN OFI TO FRM-GLS-CNAC IN DPC-FLD.
       CNT-COD-CNAC.

      *MHM-INI 07-AGO-2006
      *     IF DPC-COD-FPAG IN DPC-FLD > SPACES
      **       MOVE 'MC2'                   TO TAB-COD-SIST
      *        MOVE 'TAB'                   TO TAB-COD-SIST
      *        MOVE 'FPG'                   TO TAB-COD-TTAB IN TAB
      *        MOVE DPC-COD-FPAG IN DPC-FLD TO TAB-COD-CTAB IN TAB
      *        PERFORM BUS-TAB
      *        MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-FPAG IN DPC-FLD
      *     ELSE
      *        MOVE SPACES              TO FRM-GLS-FPAG IN DPC-FLD.
      *
      *     IF DPC-COD-MNRM IN DPC-FLD > SPACES
      *        MOVE 'TAB'                   TO TAB-COD-SIST
      *        MOVE 'VLR'                   TO TAB-COD-TTAB IN TAB
      *        MOVE DPC-COD-MNRM IN DPC-FLD TO TAB-COD-CTAB IN TAB
      *        PERFORM BUS-TAB
      *        MOVE TAB-GLS-DCOR IN TAB TO FRM-GLS-MNRM IN DPC-FLD
      *     ELSE
      *        MOVE SPACES              TO FRM-GLS-MNRM IN DPC-FLD.
      *
      *     IF DPC-IND-TPLN IN DPC-FLD > SPACES
      **       MOVE 'MC2'                   TO TAB-COD-SIST
      *        MOVE 'TAB'                   TO TAB-COD-SIST
      *        MOVE 'TPL'                   TO TAB-COD-TTAB IN TAB
      *        MOVE DPC-IND-TPLN IN DPC-FLD TO TAB-COD-CTAB IN TAB
      *        PERFORM BUS-TAB
      *        MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-TPLN IN DPC-FLD
      *     ELSE
      *        MOVE SPACES              TO FRM-GLS-TPLN IN DPC-FLD.
      *
      *     IF DPC-COD-ENVO IN DPC-FLD > SPACES
      *       MOVE 'MC2'                   TO TAB-COD-SIST
      *        MOVE 'TAB'                   TO TAB-COD-SIST
      *        MOVE 'TNV'                   TO TAB-COD-TTAB IN TAB
      *        MOVE DPC-COD-ENVO IN DPC-FLD TO TAB-COD-CTAB IN TAB
      *        PERFORM BUS-TAB
      *        MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-ENVO IN DPC-FLD
      *     ELSE
      *        MOVE SPACES              TO FRM-GLS-ENVO IN DPC-FLD.
      *>>>>
*% END
*% IF DPC_COD_TCNC
      *<<<< DPC_COD_TCNC
           MOVE 'TAB'                   TO TAB-COD-SIST.
           MOVE 'CNL'                   TO TAB-COD-TTAB IN TAB.
           MOVE DPC-COD-TCNC IN DPC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT > SPACES
               MOVE TAB-GLS-DESC TO FRM-GLS-TCNC IN DPC-FLD
           ELSE
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-DPC-COD-TCNC.
      *>>>>
*% END
*% IF DPC_COD_CNAC
      *<<<< DPC_COD_CNAC
           IF DPC-COD-CNAC IN DPC-FLD = SPACES
               MOVE 'TODOS        ' TO FRM-GLS-CNAC IN DPC-FLD
               GO TO FIN-VAL-DPC-COD-CNAC.
           IF DPC-COD-TCNC IN DPC-FLD NOT = '010'
              MOVE 'TAB'                   TO TAB-COD-SIST
              MOVE DPC-COD-TCNC IN DPC-FLD TO TAB-COD-TTAB IN TAB
              MOVE DPC-COD-CNAC IN DPC-FLD TO TAB-COD-CTAB IN TAB
              PERFORM VAL-COD
              IF MSG-COD-MENS NOT > SPACES
                 MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-CNAC IN DPC-FLD
                 GO TO FIN-VAL-DPC-COD-CNAC
              ELSE
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 MOVE FRM-SUAR-MAL TO FRM-SUAR
                 GO TO FIN-VAL-DPC-COD-CNAC
           ELSE
              MOVE DPC-COD-CNAC IN DPC-FLD TO OFI-COD-OFIC IN OFI
              PERFORM VAL-OFI
              IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'
                 MOVE SPACES       TO FRM-GLS-CNAC IN DPC-FLD
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 MOVE FRM-SUAR-MAL TO FRM-SUAR
              ELSE
                 MOVE OFI-GLS-DESC IN OFI TO FRM-GLS-CNAC IN DPC-FLD.
      *>>>>
*% END
*% IF DPC_COD_FPAG
      *<<<< DPC_COD_FPAG
           MOVE 'COL'                   TO TAB-COD-SIST.                
           MOVE 'PAC'                   TO TAB-COD-TTAB IN TAB.         
      *AFRI-INI/SEP06                                                   
           MOVE DPC-COD-FPAG IN DPC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS > SPACES
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-DPC-COD-FPAG.
      *>>>>
*% END
*% IF DPC_COD_TOOC
      *<<<< DPC_COD_TOOC
           IF DPC-COD-TOOC IN DPC-FLD NOT > SPACES
              IF FRM-COD-GRPO IN DPC-FLD NOT > SPACES
                 MOVE 'TODOS        ' TO FRM-GLS-TOOC IN DPC-FLD
                 GO TO FIN-VAL-DPC-COD-TOOC
              ELSE
                 MOVE SPACES          TO FRM-GLS-TOOC IN DPC-FLD
                 GO TO FIN-VAL-DPC-COD-TOOC.

      *VAL-COD Valida codigo de tabla
           MOVE 'TAB'                   TO TAB-COD-SIST.
           MOVE 'TIO '                  TO TAB-COD-TTAB IN TAB.
           MOVE DPC-COD-TOOC IN DPC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'
               MOVE SPACES       TO FRM-GLS-TOOC IN DPC-FLD
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
           ELSE
               MOVE TAB-GLS-DCOR IN TAB TO FRM-GLS-TOOC IN DPC-FLD.
      *>>>>
*% END
*% IF DPC_COD_AOOC
      *<<<< DPC_COD_AOOC
           IF DPC-COD-AOOC IN DPC-FLD NOT > SPACES
              IF FRM-COD-GRPO IN DPC-FLD NOT > SPACES
                 MOVE 'TODOS        ' TO FRM-GLS-AOOC IN DPC-FLD
                 GO TO FIN-VAL-DPC-COD-AOOC
              ELSE
                 MOVE SPACES          TO FRM-GLS-AOOC IN DPC-FLD
                 GO TO FIN-VAL-DPC-COD-AOOC.

      *VAL-COD Valida codigo de tabla
           MOVE 'TAB'                    TO TAB-COD-SIST.
           MOVE 'AUX'                    TO TAB-COD-TTAB IN TAB.
           MOVE DPC-COD-TOOC IN DPC-FLD  TO WSS-COD-TOOC IN WSS-VARI.
           MOVE DPC-COD-AOOC IN DPC-FLD  TO WSS-COD-AOOC IN WSS-VARI.
           MOVE WSS-COD-CAUX IN WSS-VARI TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'
               MOVE SPACES       TO FRM-GLS-AOOC IN DPC-FLD
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
           ELSE
               MOVE TAB-GLS-DCOR IN TAB TO FRM-GLS-AOOC IN DPC-FLD.
      *>>>>
*% END
*% IF DPC_IIC_ICVN
      *<<<< DPC_IIC_ICVN
           MOVE DPC-CAI-ICVN IN DPC-FLD TO CVN-CAI-ICVN IN CVN.
           MOVE DPC-IIC-ICVN IN DPC-FLD TO CVN-IIC-ICVN IN CVN.
           MOVE 99999999                TO CVN-FEC-FCVN IN CVN.
           MOVE 999999                  TO CVN-HRA-HRCV IN CVN.
           MOVE FIO-GET-LEQ TO FIO-CMND.
           PERFORM MC2-FIO-CVN.
           IF NOT ( FIO-STAT-OKS AND 
              DPC-CAI-ICVN IN DPC-FLD = CVN-CAI-ICVN IN CVN AND
              DPC-IIC-ICVN IN DPC-FLD = CVN-IIC-ICVN IN CVN )
               MOVE 'MC2'        TO MSG-COD-SIST
               MOVE 'CVN    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-DPC-IIC-ICVN
           ELSE
               MOVE 'MC2'               TO TAB-COD-SIST                 
               MOVE 'DCV'               TO TAB-COD-TTAB IN TAB          
               MOVE CVN-CIC-ICVN IN CVN TO TAB-COD-CTAB IN TAB
               MOVE CVN-IND-SCVN IN CVN TO SCR-IND-SCVN
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC TO FRM-GLS-DSCN IN DPC-FLD
      *PVC-INI 26-MAY-2010  TIPO DXP
               MOVE CVN-COD-TIPO IN CVN TO SCR-COD-TIPO
               MOVE CVN-COD-STIP IN CVN TO SCR-COD-STIP.
      *PVC-FIN 26-MAY-2010
      *>>>>
*% END
*% IF DPC_COD_VCOC
      *<<<< DPC_COD_VCOC
           IF DPC-COD-VCOC IN DPC-FLD NOT > SPACES
              IF FRM-COD-GRPO IN DPC-FLD NOT > SPACES
                 MOVE 'TODAS        ' TO FRM-GLS-VCOC IN DPC-FLD
                 GO TO FIN-VAL-DPC-COD-VCOC
              ELSE
                 MOVE SPACES          TO FRM-GLS-VCOC IN DPC-FLD
                 GO TO FIN-VAL-DPC-COD-VCOC.

      *VAL-COD Valida codigo de tabla
           MOVE 'TAB'                   TO TAB-COD-SIST.
           MOVE 'VLR '                  TO TAB-COD-TTAB IN TAB.
           MOVE DPC-COD-VCOC IN DPC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'
              MOVE SPACES       TO FRM-GLS-VCOC IN DPC-FLD
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE FRM-SUAR-MAL TO FRM-SUAR
           ELSE
               MOVE TAB-GLS-DCOR IN TAB TO FRM-GLS-VCOC IN DPC-FLD.
      *>>>>
*% END
*% IF DPC_COD_SIST
      *<<<< DPC_COD_SIST
           IF DPC-COD-SIST IN DPC-FLD NOT > SPACES AND
              FRM-COD-GRPO IN DPC-FLD > SPACES
               GO TO FIN-VAL-DPC-COD-SIST.

      *VAL-COD Valida codigo de tabla
           MOVE 'TAB'                   TO TAB-COD-SIST.
           MOVE 'SIS'                   TO TAB-COD-TTAB IN TAB.
           MOVE DPC-COD-SIST IN DPC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'
              MOVE SPACES       TO FRM-GLS-SIST IN DPC-FLD
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE FRM-SUAR-MAL TO FRM-SUAR
           ELSE
               MOVE TAB-GLS-DCOR IN TAB TO FRM-GLS-SIST IN DPC-FLD.
      *>>>>
*% END
*% IF FRM_COD_GRPO
      *<<<< FRM_COD_GRPO
           IF FRM-COD-GRPO IN DPC-FLD > SPACES AND
            ( DPC-COD-SIST IN DPC-FLD > SPACES OR
              DPC-COD-VCOC IN DPC-FLD > SPACES OR
              DPC-COD-TOOC IN DPC-FLD > SPACES OR
              DPC-COD-AOOC IN DPC-FLD > SPACES )
               MOVE 'MC2'          TO MSG-COD-SIST
               MOVE 'GRPO   NOVAL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC   TO FRM-MENS
               MOVE FRM-SUAR-MAL   TO FRM-SUAR
               GO TO FIN-VAL-FRM-COD-GRPO.

           IF FRM-COD-GRPO IN DPC-FLD NOT > SPACES
              GO TO FIN-VAL-FRM-COD-GRPO.

           MOVE FRM-COD-GRPO IN DPC-FLD TO PLN-CIC-PLAN IN PLN.
           MOVE 99999999                TO PLN-FEC-FINI IN PLN.
           MOVE 999999                  TO PLN-HRA-HINI IN PLN.
           MOVE FIO-GET-LEQ TO FIO-CMND.
       LUP-VAL-FRM-COD-GRPO.
           PERFORM MC2-FIO-PLN.
           IF NOT (FIO-STAT-OKS AND
                   PLN-CIC-PLAN IN PLN = FRM-COD-GRPO IN DPC-FLD)
              MOVE 'MC2'        TO MSG-COD-SIST
              MOVE 'PLN    NEX' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE FRM-SUAR-MAL TO FRM-SUAR
              GO TO FIN-VAL-FRM-COD-GRPO.

           IF PLN-MSC-STAT IN PLN NOT = 'A'
              MOVE FIO-GET-PRV TO FIO-CMND
              GO TO LUP-VAL-FRM-COD-GRPO.

      *VAL-COD Valida codigo de Grupo
           MOVE 'MC2'               TO TAB-COD-SIST.
           MOVE 'DPN'               TO TAB-COD-TTAB IN TAB.
           MOVE PLN-CIC-IPRD IN PLN TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'
              MOVE SPACES       TO FRM-GLS-GRPO IN DPC-FLD
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE FRM-SUAR-MAL TO FRM-SUAR
           ELSE
              MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-GRPO IN DPC-FLD.           
      *>>>>
*% END
*% IF DPC_COD_MNRM
      *<<<< DPC_COD_MNRM
           IF DPC-COD-MNRM IN DPC-FLD > SPACES AND
              DPC-VAL-RNTM IN DPC-FLD NOT > ZEROES
               MOVE 'MC2'        TO MSG-COD-SIST
               MOVE 'MON  NOVLD' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-DPC-COD-MNRM.

           IF DPC-COD-MNRM IN DPC-FLD NOT > SPACES AND
              DPC-VAL-RNTM IN DPC-FLD NOT > ZEROES
               GO TO FIN-VAL-DPC-COD-MNRM.

      *VAL-COD Valida codigo de tabla
           MOVE 'TAB'                   TO TAB-COD-SIST.
           MOVE 'VLR '                  TO TAB-COD-TTAB IN TAB.
           MOVE DPC-COD-MNRM IN DPC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>>
*% END
*% IF DPC_COD_TREC
      *<<<< DPC_COD_TREC
      *VAL-COD Valida codigo de tabla
           MOVE 'SGC' TO TAB-COD-SIST.
           MOVE 'REC ' TO TAB-COD-TTAB IN TAB.
           MOVE DPC-COD-TREC IN DPC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT > SPACES
               MOVE TAB-GLS-DESC TO FRM-GLS-TREC IN DPC-FLD
           ELSE
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-DPC-COD-TREC.
      *>>>>
*% END
*% IF DBC_NUM_ICLI
      *<<<< DBC_NUM_ICLI
           IF DBC-NUM-ICLI IN DPC-FLD NOT > ZEROES
               PERFORM ERR-GLS
               GO TO FIN-VAL-DBC-NUM-ICLI.
      *>>>>
*% END
*% IF DBC_GLS_ICLI
      *<<<< DBC_GLS_ICLI
           IF DBC-NUM-ICLI IN DPC-FLD > ZEROES
              MOVE DBC-NUM-ICLI IN DPC-FLD TO DBC-NUM-ICLI IN DBC
              MOVE DBC-IND-ICLI IN DPC-FLD TO DBC-IND-ICLI IN DBC
              MOVE DBC-GLS-ICLI IN DPC-FLD TO DBC-GLS-ICLI IN DBC
              MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY
              MOVE FIO-GET-KEY TO FIO-CMND
              PERFORM SGC-FIO-DBC
              IF NOT FIO-STAT-OKS
                  MOVE 'SGC'        TO MSG-COD-SIST
                  MOVE 'CLI    NEX' TO MSG-COD-MENS
                  PERFORM GET-MSG
                  MOVE SPACES       TO FRM-GLS-RZNS IN DPC-FLD
                  MOVE MSG-GLS-DESC TO FRM-MENS
                  MOVE FRM-SUAR-MAL TO FRM-SUAR
                  GO TO FIN-VAL-DBC-GLS-ICLI
              ELSE
                  IF DBC-IND-TICL IN DBC NOT = 'E'
                     MOVE 'SGC'        TO MSG-COD-SIST
                     MOVE 'CLI    PER' TO MSG-COD-MENS
                     PERFORM GET-MSG
                     MOVE SPACES TO FRM-GLS-RZNS IN DPC-FLD
                     MOVE MSG-GLS-DESC TO FRM-MENS
                     MOVE FRM-SUAR-MAL TO FRM-SUAR
                     GO TO FIN-VAL-DBC-GLS-ICLI
                  ELSE
                     MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
                     PERFORM CPT-BLKS
                     MOVE CPT-STRN  TO FRM-GLS-RZNS IN DPC-FLD
                     MOVE DBC-CAI-ICLI IN DBC TO SCR-CAI-ICLI
                     MOVE DBC-IIC-ICLI IN DBC TO SCR-IIC-ICLI.
      *>>>>
*% END
*% IF INI_DPC_FEC_FIVC
      *<<<< INI_DPC_FEC_FIVC
           IF DPC-FEC-FIVC IN DPC-FLD NOT > ZEROES
               PERFORM GET-FHOY
               MOVE HOY-SHOY TO DPC-NUM-SIVC IN DPC-FLD
               MOVE HOY-AHOY TO DPC-NUM-AIVC IN DPC-FLD
               MOVE HOY-MHOY TO DPC-NUM-MIVC IN DPC-FLD
               MOVE HOY-DHOY TO DPC-NUM-DIVC IN DPC-FLD.
           MOVE DPC-FEC-FIVC IN DPC-FLD TO FEC-FECH.
           MOVE FEC-CHOY-N TO FEC-CHOY.
      *>>>>
*% END
*% IF INI_DPC_FEC_FTVC
      *<<<< INI_DPC_FEC_FTVC
           MOVE FEC-CHOY-N TO FEC-CHOY.
      *>>>>
*% END
*% IF DPC_FEC_FTVC
      *<<<< DPC_FEC_FTVC
           MOVE DPC-NUM-SIVC IN DPC-FLD TO DPC-NUM-SIVC IN DPC.
           MOVE DPC-NUM-AIVC IN DPC-FLD TO DPC-NUM-AIVC IN DPC.
           MOVE DPC-NUM-MIVC IN DPC-FLD TO DPC-NUM-MIVC IN DPC.
           MOVE DPC-NUM-DIVC IN DPC-FLD TO DPC-NUM-DIVC IN DPC.
           MOVE DPC-NUM-STVC IN DPC-FLD TO DPC-NUM-STVC IN DPC.
           MOVE DPC-NUM-ATVC IN DPC-FLD TO DPC-NUM-ATVC IN DPC.
           MOVE DPC-NUM-MTVC IN DPC-FLD TO DPC-NUM-MTVC IN DPC.
           MOVE DPC-NUM-DTVC IN DPC-FLD TO DPC-NUM-DTVC IN DPC.
           IF DPC-FEC-FTVC IN DPC NOT > DPC-FEC-FIVC IN DPC
               MOVE 'MC2'          TO MSG-COD-SIST
               MOVE 'FTVC<=FIVC'   TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC   TO FRM-MENS
               MOVE FRM-SUAR-MAL   TO FRM-SUAR.
      *>>>>
*% END
*% IF DPC_IND_TPLN
      *<<<< DPC_IND_TPLN
           MOVE 'MC2'                   TO TAB-COD-SIST.
           MOVE 'TPL'                   TO TAB-COD-TTAB IN TAB.
           MOVE DPC-IND-TPLN IN DPC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS > SPACES
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-DPC-IND-TPLN.
      *>>>>
*% END
*% IF DPC_NUM_DANP
      *<<<< DPC_NUM_DANP
      *PVC-INI 26-MAY-2010  TIPO DXP
           IF DPC-NUM-DANP IN DPC-FLD < 1 OR
              DPC-NUM-DANP IN DPC-FLD > 31
               MOVE 'MC2'        TO MSG-COD-SIST                        
               MOVE 'DIA  NOVLD' TO MSG-COD-MENS                        
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               MOVE FRM-SUAR-MAL TO FRM-SUAR                            
               GO TO FIN-VAL-DPC-NUM-DANP.

           IF  WSS-IND-IDXP IN WSS-VARI = 'S' AND
             ( DPC-NUM-DANP IN DPC-FLD < WSS-NUM-MDES IN WSS-VARI OR
               DPC-NUM-DANP IN DPC-FLD > WSS-NUM-MHAS IN WSS-VARI )
               MOVE 'MC2'        TO MSG-COD-SIST                        
               MOVE 'DIA  NOVLD' TO MSG-COD-MENS                        
