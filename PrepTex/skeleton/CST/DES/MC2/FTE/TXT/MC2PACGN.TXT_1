*%! Codigo adicional para archivo
*% GEN_MFC = TRUE
*% IF PGM_PQ
*% CLRMFC = "MC2ZQCGN"
*% ELSE
*% CLRMFC = "MC2ZUCGN"
*% END
*% PGM_PTC = TRUE
*% LARGO_TOTAL_COMMAREA = 3000
*% IF FIN_SCR_VARI
      *<<<< FIN_SCR_VARI
           03  SCR-VARI-PFS.
               05  SCR-DGN-KEY-IDGN.
                   07 SCR-DGN-CIC-IDGN              PIC X(12).
                   07 SCR-DGN-NUM-IDGN              PIC 9(03).
           03  SCR-IND-ELI                          PIC X(01).
           03  SCR-UMOD-CGN            VALUE SPACES PIC X(14).
      *>>>>
*% END
*% IF WSS
      *<<<< WSS
       COPY GNSWCVRF.
       COPY GNSWVVRF.
       COPY GNSBRCIC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CIC-REQA==.
       COPY GNSWGCIC.
       COPY MC2BRDGN.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DGN-REQA==.
       COPY GNSBRMSC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSC-REQA==.
       COPY TABBRCAM.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CAM-REQA==.

       COPY MC2WGDGN.
       COPY MC2WXDGN.
       01  WSS-VARI.
           03 WSS-COD-MENY        VALUE SPACES          PIC X(12).
           03 WSS-COD-MENN        VALUE SPACES          PIC X(12).
           03 WSS-COD-EVEN.
              05 WSS-COD-SIST                           PIC X(03).
              05 WSS-COD-EVTO                           PIC X(06).
           03 WSS-COD-VCAM.
              05 WSS-COD-TVTI                           PIC X(01).
              05 WSS-COD-CVTI                           PIC X(03).
              05 WSS-COD-TCTI                           PIC X(02).
*% IF PGM_PU
           03 WSS-GLS-TPLT.
              05 WSS-CIC-TPLT                           PIC X(01).
              05 WSS-GLS-DPLT                           PIC X(11).
*% END
      *
           03 WSS-COD-PESO        VALUE SPACES          PIC X(06).
           03 WSS-K               VALUE ZEROES          PIC 9(03).
           03 WSS-R               VALUE ZEROES          PIC 9(03).
*% IF PGM_PU
           03 WSS-RDGN            VALUE ZEROES          PIC 9(03).
*% END
           03 WSS-L               VALUE ZEROES          PIC 9(03).
           03 WSS-V               VALUE ZEROES          PIC 9(03).
           03 WSS-V1              VALUE ZEROES          PIC 9(03).
           03 WSS-V2              VALUE ZEROES          PIC 9(03).
           03 WSS-V3              VALUE ZEROES          PIC 9(03).
      *>>>>
*% END
*% IF PFS_SCR_ACC
      *<<<< PFS_SCR_ACC
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 OR
                         FRM-FFLD-PF7 OR FRM-FFLD-PF8
               PERFORM VAL-NUM-KEY-CGN
               PERFORM VAL-NUM-FLD-CGN
               PERFORM SCRL-TAB-CGN
               PERFORM SCR-TRL.
      *>>>>
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-OFRM-SRV
                MOVE TSK-TERM-ALF TO DGN-COD-TERM
*% IF PGM_PQ
      *PVG INI 29-DIC-2011
                MOVE TSK-TERM-ALF TO DGN-COD-TERM-K
      *PVG FIN 29-DIC-2011
*% END
                MOVE TSK-TERM-ALF TO DGN-COD-TERM-MAS
           ELSE
                MOVE EIBTRMID     TO DGN-COD-TERM
*% IF PGM_PQ
      *PVG INI 29-DIC-2011
                MOVE EIBTRMID     TO DGN-COD-TERM-K
      *PVG FIN 29-DIC-2011
*% END
                MOVE EIBTRMID     TO DGN-COD-TERM-MAS.
      *>>>>
*% END
*% IF FND_KEY
      *<<<< FND_KEY
           IF SCR-CCPP = 'ING'
              MOVE 'I' TO CGN-IND-VIGE IN CGN-FLD.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
           MOVE CGN-STP-ITRN IN CGN TO SCR-UMOD-CGN.
           IF NOT FIO-STAT-OKS
              GO TO DGN-FND-KEY.
           IF SCR-CMND = 'CON'
              GO TO DGN-FND-KEY.
      *                                                                 
           IF CGN-MSC-STAT IN CGN = 'D' AND SCR-CMND NOT = 'CON'        
              MOVE 'S' TO SCR-IERR
              MOVE 'CGN    ANLYA' TO MSG-COD-MENS
              GO TO MSG-FND-KEY.
           IF CGN-MSC-STAT IN CGN = 'A' AND SCR-CMND NOT = 'ANL'
              MOVE 'S' TO SCR-IERR
              MOVE 'CGN    ACAYA' TO MSG-COD-MENS
              GO TO MSG-FND-KEY.
           IF CGN-MSC-STAT IN CGN = 'V' AND SCR-CMND = 'ANL'
              MOVE 'S' TO SCR-IERR
              MOVE 'CGN    ACANO' TO MSG-COD-MENS
              GO TO MSG-FND-KEY.
           IF SCR-CMND = 'VRF'
              IF CGN-MSC-STAT IN CGN = 'I' OR 'M'
                 GO TO DGN-FND-KEY
              ELSE
                 MOVE 'S' TO SCR-IERR
                 MOVE 'CMN      INV' TO MSG-COD-MENS
                 GO TO MSG-FND-KEY.
           IF SCR-CMND = 'ACA'
              IF CGN-MSC-STAT IN CGN = 'V'
                 GO TO DGN-FND-KEY
              ELSE
                 MOVE 'S' TO SCR-IERR
                 MOVE 'CMN      INV' TO MSG-COD-MENS
                 GO TO MSG-FND-KEY.
           IF SCR-CMND = 'ANL'
              IF CGN-MSC-STAT IN CGN = 'A'
                 GO TO DGN-FND-KEY
              ELSE
                 MOVE 'S' TO SCR-IERR
                 MOVE 'CMN      INV' TO MSG-COD-MENS
                 GO TO MSG-FND-KEY.
           IF SCR-CMND = 'ELI'
              IF CGN-MSC-STAT IN CGN = 'I' OR 'M' OR 'V'
                 GO TO DGN-FND-KEY
              ELSE
                 MOVE 'S' TO SCR-IERR
                 MOVE 'CMN      INV' TO MSG-COD-MENS
                 GO TO MSG-FND-KEY.
           GO TO DGN-FND-KEY.
       MSG-FND-KEY.
           MOVE 'TAB'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE '00'         TO FIO-STAT.
           MOVE SCR-STAT-CON TO SCR-STAT.
           GO TO FIN-FND-KEY.
       DGN-FND-KEY.
           IF SCR-CCPP = 'ING'
               PERFORM BUS-COR-DGN
               GO TO CON-FND-KEY
           ELSE                                                         
              IF NOT FIO-STAT-OKS                                       
                 GO TO FIN-FND-KEY.                                     
      *PVG INI 16-NOV-2007                                              
            IF SCR-OFRM-SRV AND SCR-CMND = 'CON' AND                    
               DGN-NUM-IDGN IN CGN-FLD NOT > ZEROES                     
               MOVE 1 TO DGN-NUM-IDGN IN CGN-FLD.                       
      *PVG FIN 16-NOV-2007                                              
           IF DGN-NUM-IDGN IN CGN-FLD NOT > ZEROES
               PERFORM BUS-COR-DGN
           ELSE
              MOVE CGN-IND-DSCT IN CGN TO CGN-IND-DSCT IN CGN-FLD       
              MOVE CGN-IND-CAAB IN CGN TO CGN-IND-CAAB IN CGN-FLD       
              MOVE CGN-COD-CTAN IN CGN TO CGN-COD-CTAN IN CGN-FLD       
              MOVE CGN-EXT-CTAN IN CGN TO CGN-EXT-CTAN IN CGN-FLD       
              MOVE CGN-IND-RBHB IN CGN TO CGN-IND-RBHB IN CGN-FLD       
                                                                        
              MOVE CGN-CIC-ICGN IN CGN     TO DGN-CIC-IDGN IN DGN
              MOVE DGN-NUM-IDGN IN CGN-FLD TO DGN-NUM-IDGN IN DGN
              MOVE FIO-GET-KEY TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE FIO-STAT TO PGM-STAT-DGN
              IF FIO-STAT-OKS AND SCR-CMND = 'ELI'
                 MOVE DGN-STP-ITRN IN DGN TO SCR-UMOD.

            IF PGM-STAT-DGN-OKS AND SCR-OFRM-SRV AND SCR-CMND = 'CON'
               PERFORM CRG-VEC-DGN.
            MOVE PGM-STAT-DGN TO FIO-STAT.
       CON-FND-KEY.
           IF MSG-COD-MENS > SPACES
              MOVE '23' TO FIO-STAT
              GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF PUT_SCR
      *<<<< PUT_SCR
           IF PGM-STAT-CGN-OKS
              MOVE DGN-KEY-IDGN IN DGN TO SCR-DGN-KEY-IDGN
              IF SCR-CMND = 'ELI'
               MOVE DGN-STP-ITRN IN DGN TO SCR-UMOD.
      *>>>>
*% END
*% IF SCR_GIKY_VAL_IKY
      *<<<< SCR_GIKY_VAL_IKY
      *>>>>
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF7 )
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 )
      *>>>>
*% END
*% IF CMN_MOD
      *<<<< CMN_MOD
       NXT-CMN-MOD.
           PERFORM KEY-ALL-CGN.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-CGN.
           IF FIO-STAT-OKS AND
              CGN-STP-ITRN IN CGN NOT = SCR-UMOD-CGN
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM MC2-FIO-CGN
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-CGN
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-MOD.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-MOD.
           PERFORM RET-STT-CGN.
           PERFORM RET-ALL-CGN.
           PERFORM RET-HDR-CGN.
           MOVE 'M' TO CGN-MSC-STAT IN CGN.
      *>>>>
*% END
*% IF FIN_CMN_MOD
      *<<<< FIN_CMN_MOD
           IF NOT FIO-STAT-OKS
              GO TO FIN-CMN-MOD.
           MOVE CGN-CIC-ICGN IN CGN     TO DGN-CIC-IDGN IN DGN.
           MOVE DGN-NUM-IDGN IN CGN-FLD TO DGN-NUM-IDGN IN DGN.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-DGN.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-CGN
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-CGN
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE 'MC2' TO MSG-COD-SIST
              MOVE 'ERRUPD   DGN' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              MOVE '23' TO FIO-STAT PGM-STAT-DGN PGM-STAT-DGN
              GO TO FIN-CMN-MOD.

           PERFORM RET-STT-DGN.
           PERFORM RET-DGN-CGN.
           PERFORM RET-HDR-DGN.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM MC2-FIO-DGN.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-CGN
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-CGN
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE '23' TO FIO-STAT PGM-STAT-DGN PGM-STAT-DGN
              GO TO FIN-CMN-MOD.

           MOVE 'MC2'               TO FIO-SIST.
           MOVE 'DPL'               TO TAB-COD-TTAB IN TAB.
           MOVE CGN-CIC-ICGN IN CGN TO TAB-COD-CTAB IN TAB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-MOD.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
              MOVE '23' TO FIO-STAT PGM-STAT-DGN PGM-STAT-DGN
              GO TO FIN-CMN-MOD.

           MOVE FRM-GLS-DES1 IN CGN-FLD TO TAB-GLS-DESC IN TAB.
           MOVE FRM-GLS-DES2 IN CGN-FLD TO TAB-GLS-DATA IN TAB.
           PERFORM GET-FHOY.
           MOVE HOY-FHOY                 TO TAB-FEC-FTRN IN TAB.
           MOVE HOY-HHOY                 TO TAB-HRA-HRTR IN TAB.
           MOVE SCR-OTRN                 TO TAB-COD-OTRN IN TAB.
           MOVE SCR-USER                 TO TAB-COD-ATRN IN TAB.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
              MOVE '23' TO FIO-STAT PGM-STAT-DGN PGM-STAT-DGN
              GO TO FIN-CMN-MOD.
      *>>>>
*% END
*% IF INI_CMN_ING
      *<<<< INI_CMN_ING
           IF DGN-NUM-IDGN IN CGN-FLD > 1
              MOVE '00' TO FIO-STAT
              GO TO CNT-CMN-ING.
           PERFORM OBT-CIC-CGN.
           IF CIC-STAT NOT = CIC-STAT-OKS
              GO TO FIN-CMN-ING.
           MOVE CIC-CAI-ICIC IN CIC TO CGN-CAI-ICGN IN CGN.
           MOVE CIC-IIC-ICIC IN CIC TO CGN-IIC-ICGN IN CGN.
           MOVE CIC-CIC-ICIC IN CIC TO CGN-CIC-ICGN IN CGN-FLD.
      *>>>>
*% END
*% IF FIN_CMN_ING
      *<<<< FIN_CMN_ING
       CNT-CMN-ING.
           IF NOT FIO-STAT-OKS
              GO TO FIN-CMN-ING.
           PERFORM RET-STT-DGN.
           PERFORM KEY-DGN-CGN.
           MOVE CGN-CIC-ICGN IN CGN-FLD TO DGN-CIC-IDGN IN DGN.
           PERFORM RET-DGN-CGN.
           PERFORM RET-HDR-DGN.
           MOVE FIO-PUT TO FIO-CMND.
           PERFORM MC2-FIO-DGN.
           IF NOT FIO-STAT-OKS
              MOVE FIO-STAT TO PGM-STAT-CGN PGM-STAT-DGN
              GO TO FIN-CMN-ING.

           IF DGN-NUM-IDGN IN CGN-FLD = 1
              MOVE 'MC2'                   TO FIO-SIST
              MOVE 'DPL'                   TO TAB-COD-TTAB IN TAB
              MOVE CGN-CIC-ICGN IN CGN     TO TAB-COD-CTAB IN TAB
              MOVE FRM-GLS-DES1 IN CGN-FLD TO TAB-GLS-DESC IN TAB
              MOVE FRM-GLS-DES2 IN CGN-FLD TO TAB-GLS-DATA IN TAB
              MOVE 'I'                     TO TAB-MSC-TACC IN TAB
              MOVE 'I'                     TO TAB-MSC-STAT IN TAB
              MOVE 'S'                     TO TAB-IND-VIGE IN TAB
              MOVE SPACES                  TO TAB-COD-OTRN IN TAB
              MOVE 'MC2PUCGN'              TO TAB-COD-ATRN IN TAB
              MOVE DGN-CAI-IDGN IN DGN     TO TAB-GLS-DCOR IN TAB
              MOVE DGN-IIC-IDGN IN DGN     TO TAB-GLS-ABRV IN TAB
              PERFORM GET-FHOY
              MOVE HOY-FHOY                 TO TAB-FEC-FTRN IN TAB
              MOVE HOY-HHOY                 TO TAB-HRA-HRTR IN TAB
              MOVE SCR-OTRN                 TO TAB-COD-OTRN IN TAB
              MOVE SCR-USER                 TO TAB-COD-ATRN IN TAB
              MOVE FIO-PUT TO FIO-CMND
              PERFORM GNS-FIO-TAB
              IF NOT FIO-STAT-OKS
                 MOVE 'MC2'               TO MSG-COD-SIST
                 MOVE 'DPL    NOGRB'      TO MSG-COD-MENS
                 PERFORM GET-MSG
                 MOVE MSG-GLS-DESC        TO FRM-MENS
                 GO TO FIN-CMN-ING.

      *MODIFICA ESTADO CGN
           PERFORM KEY-ALL-CGN.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-CGN.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-CGN
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-CGN
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE 'MC2' TO MSG-COD-SIST
              MOVE 'ERRUPDSTACGN' TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
              GO TO FIN-CMN-ING.
           PERFORM RET-STT-CGN.
           PERFORM RET-HDR-CGN.
           PERFORM RET-ALL-CGN.                                         
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM MC2-FIO-CGN.
      *>>>>
*% END
*% IF INI_CMN_ACC
      *<<<< INI_CMN_ACC
           IF SCR-CMND = 'VRF'
               PERFORM PRO-CMN-VRF
               IF MSG-COD-MENS > SPACES
                  GO TO FIN-CMN-ACC.
           GO TO NXT-CMN-ACC.
      *>>>>
*% END
*% IF CMN_ACC
      *<<<< CMN_ACC
       NXT-CMN-ACC.
           PERFORM KEY-ALL-CGN.                                         
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.                            
           PERFORM MC2-FIO-CGN.                                         
           IF FIO-STAT-OKS AND                                          
              CGN-STP-ITRN IN CGN NOT = SCR-UMOD-CGN                    
               MOVE FIO-ULK-REC TO FIO-CMND                             
               PERFORM MC2-FIO-CGN                                      
               MOVE FIO-BCK-OUT TO FIO-CMND                             
               PERFORM MC2-FIO-CGN                                      
               MOVE 'GNS' TO MSG-COD-SIST                               
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS                      
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               GO TO FIN-CMN-ACC.                                       
           IF NOT FIO-STAT-OKS                                          
               GO TO FIN-CMN-ACC.                                       
      *    PERFORM RET-ALL-CGN.                                         
    
           IF SCR-CMND = 'VRF'
               MOVE 'V'          TO CGN-IND-VIGE IN CGN
               MOVE 'VRFYES    ' TO WSS-COD-MENY
               MOVE 'VRFNOT    ' TO WSS-COD-MENN
           ELSE
           IF SCR-CMND = 'ACA'
               MOVE 'A'          TO CGN-IND-VIGE IN CGN
               MOVE 'ACAYES    ' TO WSS-COD-MENY
               MOVE 'ACANOT    ' TO WSS-COD-MENN
           ELSE
           IF SCR-CMND = 'ANL'
               MOVE 'D'          TO CGN-IND-VIGE IN CGN
               MOVE 'ANLYES    ' TO WSS-COD-MENY
               MOVE 'ANLNOT    ' TO WSS-COD-MENN.

           MOVE FIO-MOD TO FIO-CMND.
           PERFORM MC2-FIO-CGN.
           IF NOT FIO-STAT-OKS
               MOVE 'S' TO SCR-IERR
               MOVE 'MC2' TO MSG-COD-SIST
               MOVE WSS-COD-MENN TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE WSS-COD-MENY  TO MSG-COD-MENS
               MOVE 'MC2' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN1.

           IF SCR-CMND NOT = 'ANL'                                      
              GO TO FIN-CMN-ACC.                                        
           MOVE 'MC2' TO FIO-SIST.                                      
           MOVE 'DPL' TO TAB-COD-TTAB IN TAB.                           
           MOVE CGN-CIC-ICGN IN CGN TO TAB-COD-CTAB IN TAB.             
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM GNS-FIO-TAB.                                         
           IF FIO-STAT-OKS                                              
              MOVE FIO-GET-KEY-UPD TO FIO-CMND                          
              PERFORM GNS-FIO-TAB                                       
              IF FIO-STAT-OKS                                           
                 PERFORM GET-FHOY                                       
                 MOVE HOY-FHOY            TO TAB-FEC-FTRN IN TAB        
                 MOVE HOY-HHOY            TO TAB-HRA-HRTR IN TAB        
                 MOVE SCR-OTRN            TO TAB-COD-OTRN IN TAB        
                 MOVE SCR-USER            TO TAB-COD-ATRN IN TAB        
                 MOVE 'N'      TO TAB-IND-VIGE IN TAB                   
                 MOVE FIO-MOD TO FIO-CMND                               
                 PERFORM GNS-FIO-TAB                                    
                 IF NOT FIO-STAT-OKS                                    
                    MOVE 'ERROR UPD DPL' TO FRM-MEN2                    
                 ELSE                                                   
                    NEXT SENTENCE                                       
              ELSE                                                      
                 MOVE 'ERROR UPD DPL' TO FRM-MEN2                       
           ELSE                                                         
              MOVE 'ERROR UPD DPL' TO FRM-MEN2.                         
      *>>>>
*% END
*% IF INI_CMN_ELI
      *<<<< INI_CMN_ELI
           PERFORM KEY-ALL-CGN.
           GO TO CNT-CMN-ELI.
      *>>>>
*% END
*% IF FIN_CMN_ELI
      *<<<< FIN_CMN_ELI
           MOVE FIO-STAT TO PGM-STAT-CGN.
       CNT-CMN-ELI.
           IF FRM-IND-SGTE IN CGN-FLD = 'S'
              PERFORM ELI-ALL-CGN
              GO TO FIN-CMN-ELI.
           MOVE CGN-CIC-ICGN IN CGN-FLD TO DGN-CIC-IDGN IN DGN.
           MOVE DGN-NUM-IDGN IN CGN-FLD TO DGN-NUM-IDGN IN DGN.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-DGN.
           IF FIO-STAT-OKS AND
              DGN-STP-ITRN IN DGN NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM MC2-FIO-CGN
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-CGN
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE '23' TO FIO-STAT PGM-STAT-DGN PGM-STAT-DGN
               GO TO FIN-CMN-ELI.
           IF NOT FIO-STAT-OKS
              GO TO FIN-CMN-ELI.

           MOVE FIO-DEL TO FIO-CMND.
           PERFORM MC2-FIO-DGN.
           MOVE FIO-STAT TO PGM-STAT-DGN.
           IF NOT FIO-STAT-OKS
              MOVE FIO-ULK-REC TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE FIO-BCK-OUT TO FIO-CMND
              PERFORM MC2-FIO-DGN
              MOVE '23' TO FIO-STAT PGM-STAT-DGN PGM-STAT-DGN
              GO TO FIN-CMN-ELI.
           MOVE CGN-CIC-ICGN IN CGN-FLD TO DGN-CIC-IDGN IN DGN.
           MOVE ZEROES                  TO DGN-NUM-IDGN IN DGN.
           MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM MC2-FIO-DGN.
           IF (FIO-STAT-OKS AND
               DGN-CIC-IDGN IN DGN = CGN-CIC-ICGN IN CGN-FLD)
              MOVE PGM-STAT-DGN TO FIO-STAT
              PERFORM MOD-STAT-CGN
              GO TO FIN-CMN-ELI.

           PERFORM KEY-ALL-CGN.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM MC2-FIO-CGN.
           IF FIO-STAT-OKS AND
              CGN-STP-ITRN IN CGN NOT = SCR-UMOD-CGN
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM MC2-FIO-CGN
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-CGN
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM MC2-FIO-DGN
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM MC2-FIO-DGN
