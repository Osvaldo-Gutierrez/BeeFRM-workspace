*%! Codigo adicional
*% GEN_MFC = TRUE
*% IF PGM_PQ
*% CLRMFC = "MC2ZQAGD"
*% ELSE
*% CLRMFC = "MC2ZUAGD"
*% END
*% NO_TIENE_TIMESTAMP = TRUE
*% HDR_CST = FALSE
*% GNSTCA_ORIG = 3000
*% GNSTCA = 3000
*% LARGO_TOTAL_COMMAREA = 3000
*% PGM_PTC = TRUE
*% IF WSS
      *<<<< WSS
*% IF PGM_PQ
       COPY MC2BRRCC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RCC-REQA==.
       COPY GNSBRMSC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSC-REQA==.
       COPY MC2WXAGD.                                                   
      *MHM-INI 30-OCT-2007                                              
       COPY GNSBRCIC.                                                   
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CIC-REQA==.        
       COPY GNSWGCIC.                                                   
       COPY GNSWCVRF.                                                   
       COPY GNSWVVRF.                                                   
      *MHM-FIN 30-OCT-2007                                              
                                                                        
       01  WSS-VARI.                                                    
           03 WSS-IND-CHKA           VALUE SPACES       PIC X(01).      
           03 WSS-BKP-KEY-IAGD.                                         
              05 WSS-BKP-CIC-IAGD.                                      
                 07 WSS-BKP-CAI-IAGD                    PIC X(04).      
                 07 WSS-BKP-IIC-IAGD                    PIC X(08).      
              05 WSS-BKP-NUM-IAGD                       PIC 9(03).      

*% END
*% IF PGM_PU
       COPY GNSBRMSC.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSC-REQA==.
       01  WSS-VARI.                                                    
           03 WSS-IND-CHKA           VALUE SPACES       PIC X(01).      
           03 WSS-BKP-KEY-IAGD.                                         
              05 WSS-BKP-CIC-IAGD.                                      
                 07 WSS-BKP-CAI-IAGD                    PIC X(04).      
                 07 WSS-BKP-IIC-IAGD                    PIC X(08).      
              05 WSS-BKP-NUM-IAGD                       PIC 9(03).      
                                                                         
*% END
      *>>>>
*% END
*
*% IF INI_FIO
*% IF PGM_PQ
      *<<<< INI_FIO                                                     
           IF SCR-OFRM-SRV                                              
                MOVE TSK-TERM-ALF TO AGD-COD-TERM                       
                MOVE TSK-TERM-ALF TO AGD-COD-TERM-MAS                   
           ELSE                                                         
                MOVE EIBTRMID     TO AGD-COD-TERM                       
                MOVE EIBTRMID     TO AGD-COD-TERM-MAS.                  
*% END
      *>>>> 
*% END
*% IF INI_CMN_ING
      *<<<< INI_CMN_ING
           MOVE AGD-CAI-IAGD IN AGD-FLD TO WSS-BKP-CAI-IAGD.            
           MOVE AGD-IIC-IAGD IN AGD-FLD TO WSS-BKP-IIC-IAGD.            
           MOVE AGD-NUM-IAGD IN AGD-FLD TO WSS-BKP-NUM-IAGD.            
           PERFORM CHK-AGD-RCC                                          
           IF WSS-IND-CHKA = 'N'                                        
              MOVE 'S' TO SCR-IERR                                      
              MOVE 'MC2' TO MSG-COD-SIST                                
              MOVE 'AGDTAGDDUPL' TO MSG-COD-MENS                        
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC TO FRM-MENS                             
              MOVE '00'           TO FIO-STAT                           
              MOVE SCR-STAT-CON   TO SCR-STAT                           
              GO TO FIN-CMN-ING                                         
           ELSE                                                         
              MOVE WSS-BKP-CAI-IAGD TO AGD-CAI-IAGD IN AGD              
              MOVE WSS-BKP-IIC-IAGD TO AGD-IIC-IAGD IN AGD              
              MOVE WSS-BKP-NUM-IAGD TO AGD-NUM-IAGD IN AGD
              MOVE FIO-GET-KEY TO FIO-CMND                              
              PERFORM MC2-FIO-AGD.                                      
                                                                        
           MOVE SPACES TO AGD.
      *>>>> 
*% END
*% IF I
      *<<<< I

      *>>>> 
*% END
*% IF INI_SCR_COM  AND  PGM_PQ 
      *<<<< INI_SCR_COM                                                 
           MOVE AGD-NOM-TSAGD     TO QUE-COLA.                          
           MOVE AGD-LEN-TSAGD     TO QUE-LITM.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
                                                                        
           MOVE AGD-NOM-TSAGD-MAS TO QUE-COLA.                          
           MOVE AGD-LEN-TSAGD-MAS TO QUE-LITM.                          
           MOVE QUE-DEL           TO QUE-CMND.                          
           PERFORM GNS-PRO-QUE.                                         
      *>>>>                                                             
*% END
*% IF FND_KEY
      *<<<< FND_KEY                                                     
      *MHM-INI 30-OCT-2007                                              
           IF SCR-CCPP = 'ING' AND                                      
              AGD-CAI-IAGD IN AGD-FLD = 'PTRF' AND                      
              AGD-IIC-IAGD IN AGD-FLD NOT > SPACES                      
               PERFORM OBT-CIC-PTRF                                     
               IF FRM-MENS > SPACES                                     
                  MOVE SCR-STAT-CON   TO SCR-STAT                       
                  GO TO FIN-FND-KEY                                     
               ELSE                                                     
                  MOVE WSS-BKP-IIC-IAGD TO AGD-IIC-IAGD IN AGD-FLD      
                  MOVE WSS-BKP-IIC-IAGD TO AGD-IIC-IAGD IN AGD.         
      *MHM-FIN 30-OCT-2007                                              
           IF SCR-CCPP = 'ING'                                          
               PERFORM BUS-COR-AGD                                      
               GO TO CON-FND-KEY.                                       
           IF AGD-NUM-IAGD IN AGD NOT > ZEROES                          
              PERFORM BUS-COR-AGD.                                      
       CON-FND-KEY.                                                     
      *>>>>                                                             
*% END
*% IF FIN_FND_KEY 
      *<<<< FIN_FND_KEY                                                 
           IF NOT FIO-STAT-OKS                                          
               MOVE '23' TO PGM-STAT-AGD                                
           ELSE                                                         
               MOVE '00' TO PGM-STAT-AGD.                               
      *MHM-INI 30-OCT-2007                                              
           IF AGD-CAI-IAGD IN AGD-FLD = 'PTRF'                          
               MOVE 'I' TO RCC-MSC-STAT IN RCC                          
               GO TO CNT-SIN-RCC.                                       
      *MHM-FIN 30-OCT-2007                                              
           MOVE AGD-CAI-IAGD IN AGD TO RCC-CAI-IRCC IN RCC.             
           MOVE AGD-IIC-IAGD IN AGD TO RCC-IIC-IRCC IN RCC.             
           MOVE 'RCC-CIC-IRCC'      TO FIO-AKEY.                        
           MOVE FIO-GET-KEY         TO FIO-CMND.                        
           PERFORM MC2-FIO-RCC.                                         
           IF NOT FIO-STAT-OKS                                          
               MOVE 'MC2'          TO MSG-COD-SIST                      
               MOVE 'AGD    NORCC' TO MSG-COD-MENS                      
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC   TO FRM-MENS                          
               MOVE '23'           TO FIO-STAT                          
               MOVE SCR-STAT-CON   TO SCR-STAT                          
               GO TO FIN-FND-KEY.                                       
                                                                        
      *PVG INI 15-JUN-2004                                              
      *PVG INI 14-DIC-2006 INTEGRACION A CNC                            
      *    IF RCC-IND-OPCL IN RCC > SPACES                              
           IF (RCC-IND-OPCL IN RCC = 'O' OR                             
              (RCC-IND-OPCL IN RCC = 'C' AND                            
               RCC-COD-TREC IN RCC NOT > SPACES))                       
      *PVG FIN 14-DIC-2006                                              
              MOVE 'MC2'          TO MSG-COD-SIST                       
              MOVE 'AGD    NOVAL' TO MSG-COD-MENS                       
              PERFORM GET-MSG                                           
              MOVE MSG-GLS-DESC   TO FRM-MENS                           
              MOVE '23'           TO FIO-STAT                           
              MOVE SCR-STAT-CON   TO SCR-STAT                           
              GO TO FIN-FND-KEY.                                        
      *PVG FIN 15-JUN-2004                                              
                                                                        
           MOVE 'MC2'               TO TAB-COD-SIST.                    
           MOVE 'DPL'               TO TAB-COD-TTAB IN TAB.             
           MOVE RCC-CIC-IRCC IN RCC TO TAB-COD-CTAB IN TAB.             
           PERFORM BUS-TAB.                                             
           MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-DES1 IN AGD-FLD.         
           MOVE TAB-GLS-DATA IN TAB TO FRM-GLS-DES2 IN AGD-FLD.         
                                                                        
      *MHM-INI 30-OCT-2007                                              
       CNT-SIN-RCC.                                                     
      *MHM-FIN 30-OCT-2007                                              
           MOVE PGM-STAT-AGD TO FIO-STAT.                               
           IF SCR-CMND = 'CON'                                          
              IF SCR-OFRM-SRV                                           
                 PERFORM CRG-AGD-QUE                                    
                 GO TO FIN-FND-KEY                                      
              ELSE                                                      
                 GO TO FIN-FND-KEY                                      
           ELSE                                                         
              IF RCC-MSC-STAT = 'V'                                     
                 MOVE 'RCC    VRFYA' TO MSG-COD-MENS                    
              ELSE                                                      
              IF RCC-MSC-STAT = 'A'                                     
                 MOVE 'RCC    ACTYA' TO MSG-COD-MENS                    
              ELSE                                                      
              IF RCC-MSC-STAT = 'D'                                     
                 MOVE 'RCC    ANLYA' TO MSG-COD-MENS                    
              ELSE                                                      
                 GO TO FIN-FND-KEY.                                     
       SIG-FND-KEY.                                                     
           MOVE 'MC2'        TO MSG-COD-SIST.                           
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MENS                                
           MOVE '00'         TO FIO-STAT.                               
           MOVE SCR-STAT-CON TO SCR-STAT.                               
           GO TO FIN-FND-KEY.                                           
      *>>>>               
*% END
*% IF EOF
      *<<<< EOF
       PGM-EBR-ALL SECTION.
       INI-PGM-EBR-ALL.
           MOVE 'GNS' TO FIO-SIST.
           MOVE FIO-EBR-ALL TO FIO-CMND.
           PERFORM GNS-FIO-TAB.

       FIN-PGM-EBR-ALL.
           EXIT.
                                                                        
       COPY GNSBVTAB.                                                   
       COPY GNSBVMSC.                                                   
       COPY GNSBFMSC.                                                   
*% IF PGM_PQ
       COPY MC2BFRCC.                                                   
       COPY GNSBBTAB.                                                   

       GNS-BUS-VCA SECTION.
       INI-GNS-BUS-VCA.
           MOVE 0 TO FRM-IFLD.
       LUP-GNS-BUS-VCA.
           ADD 1 TO FRM-IFLD.
           IF FRM-IFLD NOT > EIBCALEN AND
              FRM-IFLD NOT > 3000
                MOVE DFH-CMMA(FRM-IFLD) TO PGM-CMMA-CHR(FRM-IFLD)
                GO TO LUP-GNS-BUS-VCA.
           MOVE PGM-CMMA TO SCR-VARI.
       FIN-GNS-BUS-VCA.
           EXIT.

      *MHM-INI 30-OCT-2007                                              
       COPY GNSBFCIC.                                                   
       COPY GNSBGCIC.                                                   
      *MHM-FIN 30-OCT-2007                                              
                                                                        
       BUS-COR-AGD SECTION.                                             
       INI-BUS-COR-AGD.                                                 
           MOVE 999 TO AGD-NUM-IAGD IN AGD.                             
           MOVE FIO-GET-LEQ TO FIO-CMND.                                
           PERFORM MC2-FIO-AGD.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    AGD-CAI-IAGD IN AGD-FLD = AGD-CAI-IAGD IN AGD AND
                    AGD-IIC-IAGD IN AGD-FLD = AGD-IIC-IAGD IN AGD )
               PERFORM KEY-ALL-AGD                                      
               MOVE ZEROES TO AGD-NUM-IAGD IN AGD.                      
           IF SCR-CCPP = 'ING' OR 'ACT'                                 
               ADD 1 TO AGD-NUM-IAGD IN AGD                             
              MOVE AGD-NUM-IAGD IN AGD TO AGD-NUM-IAGD IN AGD-FLD.
       FIN-BUS-COR-AGD.                                                 
           EXIT.                                                        
                                                                        
       CRG-AGD-QUE SECTION.                                             
       INI-CRG-AGD-QUE.                                                 
           IF NOT FIO-STAT-OKS                                          
              MOVE '00' TO FIO-STAT                                     
              GO TO FIN-CRG-AGD-QUE.                                    
                                                                        
           MOVE SPACES      TO AGD-ITEM                                 
           MOVE ZEROES      TO WSS-I WSS-NITM.                          
           MOVE ZEROES      TO AGD-NUM-IAGD IN AGD.                     
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-CRG-AGD-QUE.                                                 
           PERFORM MC2-FIO-AGD.                                         
           IF NOT ( FIO-STAT-OKS AND                                    
                    AGD-CAI-IAGD IN AGD = AGD-CAI-IAGD IN AGD-FLD AND   
                    AGD-IIC-IAGD IN AGD = AGD-IIC-IAGD IN AGD-FLD )     
              GO TO SLD-CRG-AGD-QUE.                                    
                                                                        
           MOVE AGD-COD-TAGD IN AGD TO AGD-FRM-COD-TAGD.                
           MOVE AGD-COD-CAGD IN AGD TO AGD-FRM-COD-CAGD.                
                                                                        
           MOVE 'MC2'               TO TAB-COD-SIST.                    
           MOVE 'AGD'               TO TAB-CIC-TTAB IN TAB.             
           MOVE AGD-COD-TAGD IN AGD TO TAB-CIC-CTAB IN TAB.             
           MOVE 'TAB-CIC-TABL'      TO FIO-AKEY.                        
           PERFORM VAL-TAB.                                             
           IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'               
              MOVE AGD-COD-TAGD IN AGD TO AGD-FRM-GLS-TAGD              
              MOVE AGD-COD-CAGD IN AGD TO AGD-FRM-GLS-CAGD              
           ELSE                                                         
              MOVE TAB-GLS-DCOR IN TAB     TO AGD-FRM-GLS-TAGD          
              IF TAB-COD-DAT3(1) > SPACES AND                           
                 TAB-COD-DAT3(1) NOT = 'SIS'                            
                 IF TAB-COD-DAT3(10) NOT > SPACES                       
                    MOVE TAB-COD-DAT3(1)     TO TAB-COD-SIST            
                    MOVE TAB-GLS-DAT1 IN TAB TO TAB-COD-TTAB IN TAB     
                    MOVE AGD-COD-CAGD IN AGD TO TAB-COD-CTAB IN TAB     
                    PERFORM VAL-TAB                                     
                    IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'      
                       MOVE AGD-COD-CAGD IN AGD TO AGD-FRM-GLS-CAGD     
                    ELSE                                                
                       MOVE TAB-GLS-DCOR IN TAB TO AGD-FRM-GLS-CAGD     
                 ELSE                                                   
                    MOVE TAB-COD-DAT3(1)     TO MSC-COD-SIST            
                    MOVE TAB-GLS-DAT1 IN TAB TO MSC-COD-TMSC IN MSC     
                    MOVE AGD-COD-CAGD IN AGD TO MSC-COD-CMSC IN MSC     
                    PERFORM VAL-MSC                                     
                    IF MSG-COD-MENS = 'COD    NEX' OR 'COD    NVG'      
                       MOVE SPACES       TO AGD-FRM-GLS-CAGD            
                       MOVE MSG-GLS-DESC TO FRM-MENS                    
                       MOVE FRM-SUAR-MAL TO FRM-SUAR                    
                    ELSE                                                
                       MOVE MSC-GLS-DCOR IN MSC TO AGD-FRM-GLS-CAGD     
              ELSE       
                 MOVE AGD-COD-CAGD IN AGD TO AGD-FRM-GLS-CAGD.

           PERFORM PUT-AGD-QUE.
                                                                        
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-CRG-AGD-QUE.                                       
       SLD-CRG-AGD-QUE.                                                 
           IF WSS-NITM > ZEROES                                         
                MOVE '00' TO FIO-STAT.                                  
       FIN-CRG-AGD-QUE.                                                 
           MOVE 1                 TO QUE-NITM.                          
           MOVE AGD-LEN-TSAGD-MAS TO QUE-LITM.                          
           MOVE AGD-NOM-TSAGD-MAS TO QUE-COLA.                          
           IF WSS-NITM > WSS-IMAX                                       
              MOVE 'S'            TO AGD-MAS-IND-HMAS                   
           ELSE                                                         
              MOVE 'N'            TO AGD-MAS-IND-HMAS.                  
           MOVE WSS-NITM          TO WSS-NUM-ICUR.                      
           MOVE WSS-NUM-ICUR      TO WSS-NUM-ICUR-EDT.                  
           MOVE WSS-NUM-ICUR-EDT  TO WSS-NUM-ICUR-ALF.                  
           MOVE WSS-NUM-ICUR-ALF  TO AGD-MAS-NUM-TOCC.                  
           MOVE AGD-MAS-ITEM      TO QUE-ITEM.                          
           MOVE QUE-PUT          TO QUE-CMND.                           
           PERFORM GNS-PRO-QUE.                                         
       EXT-CRG-AGD-QUE.                                                 
           EXIT.                                                        
                                                                        
       PUT-AGD-QUE SECTION.                                             
       INI-PUT-AGD-QUE.                                                 
           ADD 1                 TO WSS-NITM.                           
           MOVE AGD-NOM-TSAGD    TO QUE-COLA.                           
           MOVE AGD-LEN-TSAGD    TO QUE-LITM.                           
           MOVE WSS-NITM         TO QUE-NITM.                           
           MOVE SPACES           TO QUE-ITEM.                           
           MOVE AGD-ITEM         TO QUE-ITEM.                           
           MOVE QUE-PUT  TO QUE-CMND.                                   
           PERFORM GNS-PRO-QUE.                                         
       FIN-PUT-AGD-QUE.                                                 
           EXIT.                                                        
                                                                        
      *MHM-INI 30-OCT-2007  CONSULTA GENERICA TARIFARIO                 
       OBT-CIC-PTRF SECTION.                                            
       INI-OBT-CIC-PTRF.                                                
      *GET-CIC OBTENER CIC                                              
           MOVE 'PTRF' TO CIC-CAI-ICIC IN CIC.                          
           MOVE CIC-GET TO CIC-CMND.                                    
           PERFORM PRO-CIC.                                             
           IF CIC-STAT NOT = CIC-STAT-OKS                               
               MOVE CIC-MENS TO FRM-MENS                                
               GO TO FIN-OBT-CIC-PTRF.                                  
           MOVE CIC-CIC-ICIC TO WSS-BKP-CIC-IAGD.                       
       FIN-OBT-CIC-PTRF.                                                
           EXIT.                                                        
      *MHM-FIN 30-OCT-2007         
       COPY GNSBPVRF.
*% END
*% IF PGM_PU
       CHK-AGD-RCC SECTION.                                             
       INI-CHK-AGD-RCC.                                                 
           MOVE SPACES TO WSS-IND-CHKA.                                 
           MOVE AGD-CAI-IAGD IN AGD-FLD TO AGD-CAI-IAGD IN AGD.         
           MOVE AGD-IIC-IAGD IN AGD-FLD TO AGD-IIC-IAGD IN AGD.         
           MOVE ZEROES                  TO AGD-NUM-IAGD IN AGD.         
           MOVE FIO-GET-NLS TO FIO-CMND.                                
       LUP-CHK-AGD-RCC.                                                 
           PERFORM MC2-FIO-AGD.                                         
           IF NOT (FIO-STAT-OKS AND                                     
              AGD-CAI-IAGD IN AGD = AGD-CAI-IAGD IN AGD-FLD AND         
              AGD-IIC-IAGD IN AGD = AGD-IIC-IAGD IN AGD-FLD)            
               GO TO FIN-CHK-AGD-RCC.                                   
                                                                        
           IF AGD-COD-TAGD IN AGD-FLD = AGD-COD-TAGD IN AGD             
      *PVG INI 06-NOV-2008 NMN                                          
              AND                                                       
              AGD-COD-CAGD IN AGD-FLD = AGD-COD-CAGD IN AGD             
      *PVG FIN 06-NOV-2008                                              
               MOVE 'N' TO WSS-IND-CHKA                                 
               GO TO FIN-CHK-AGD-RCC.                                   
                                                                        
           MOVE FIO-GET-NXT TO FIO-CMND.                                
           GO TO LUP-CHK-AGD-RCC.                                       
                                                                        
       FIN-CHK-AGD-RCC.                                                 
           EXIT.                                                        
                                                                        
       CHK-AGD SECTION.                                                 
       INI-CHK-AGD.                                                     
       FIN-CHK-AGD.                                                     
           EXIT.                                                        
*% END
      *>>>>
*% END
