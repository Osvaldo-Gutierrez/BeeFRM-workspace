*%! Codigo adicional
*% IF SETTAG
*% VSNBQ = "06.03.20.1"
*% FNCBQ = "MC2-CONVENIO"
*% END
*% IF DBC_GLS_ICLI
      *<<<< DBC_GLS_ICLI
           IF DBC-NUM-ICLI IN QCC-FLD > ZEROES
              MOVE DBC-NUM-ICLI IN QCC-FLD TO DBC-NUM-ICLI IN DBC
              MOVE DBC-IND-ICLI IN QCC-FLD TO DBC-IND-ICLI IN DBC
              MOVE DBC-GLS-ICLI IN QCC-FLD TO DBC-GLS-ICLI IN DBC
              MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY
              MOVE FIO-GET-KEY TO FIO-CMND
              PERFORM SGC-FIO-DBC
              IF NOT FIO-STAT-OKS
                  MOVE 'SGC'        TO MSG-COD-SIST
                  MOVE 'CLI    NEX' TO MSG-COD-MENS
                  PERFORM GET-MSG
                  MOVE SPACES       TO FRM-GLS-NOMB IN QCC-FLD
                  MOVE MSG-GLS-DESC TO FRM-MENS
                  MOVE FRM-SUAR-MAL TO FRM-SUAR
                  GO TO FIN-VAL-DBC-GLS-ICLI
              ELSE
                  IF DBC-IND-TICL IN DBC NOT = 'P'                      
                     MOVE 'SGC'        TO MSG-COD-SIST                  
                     MOVE 'CLI    EMP' TO MSG-COD-MENS                  
                     PERFORM GET-MSG                                    
                     MOVE SPACES TO FRM-GLS-NOMB IN QCC-FLD             
                     MOVE MSG-GLS-DESC TO FRM-MENS                      
                     MOVE FRM-SUAR-MAL TO FRM-SUAR                      
                     GO TO FIN-VAL-DBC-GLS-ICLI                         
                  ELSE                                                  
                     MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
                     PERFORM CPT-BLKS
                     MOVE CPT-STRN  TO FRM-GLS-NOMB IN QCC-FLD
                     MOVE DBC-CAI-ICLI IN DBC TO SCR-CAI-ICLI
                     MOVE DBC-IIC-ICLI IN DBC TO SCR-IIC-ICLI.
      *>>>>
*% END
*% IF DBC_NUM_ICLI
      *<<<< DBC_NUM_ICLI
           IF DBC-NUM-ICLI IN QCC-FLD NOT > ZEROES
               PERFORM ERR-GLS
               GO TO FIN-VAL-DBC-NUM-ICLI.
      *>>>>
*% END
*% IF CVN_IND_SCVN
      *<<<< CVN_IND_SCVN
           IF CVN-IND-SCVN IN QCC-FLD NOT > SPACES
               GO TO FIN-VAL-CVN-IND-SCVN.

      *    MOVE 'TAB'                   TO TAB-COD-SIST.
           MOVE 'MC2'                   TO TAB-COD-SIST.
           MOVE 'SCV'                   TO TAB-COD-TTAB IN TAB.
           MOVE CVN-IND-SCVN IN QCC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT > SPACES
               MOVE TAB-GLS-DESC TO FRM-GLS-ESTA IN QCC-FLD
           ELSE
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CVN-IND-SCVN.

      *>>>>
*% END
*% IF DPC_COD_SIST
      *<<<< DPC_COD_SIST
           IF DPC-COD-SIST IN QCC-FLD NOT > SPACES
               GO TO FIN-VAL-DPC-COD-SIST.

           MOVE 'TAB'                   TO TAB-COD-SIST.
           MOVE 'SIS'                   TO TAB-COD-TTAB IN TAB.
           MOVE DPC-COD-SIST IN QCC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT > SPACES
               MOVE TAB-GLS-DESC TO FRM-GLS-SIST IN QCC-FLD
           ELSE
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-DPC-COD-SIST.
      *>>>>
*% END
*% IF DPC_COD_VCOC
      *<<<< DPC_COD_VCOC
           IF DPC-COD-VCOC IN QCC-FLD NOT > SPACES
               GO TO FIN-VAL-DPC-COD-VCOC.

           MOVE 'TAB'                   TO TAB-COD-SIST.
           MOVE 'VLR'                   TO TAB-COD-TTAB IN TAB.
           MOVE DPC-COD-VCOC IN QCC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT > SPACES
               MOVE TAB-GLS-DESC TO FRM-GLS-VCOC IN QCC-FLD
           ELSE
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-DPC-COD-VCOC.
      *>>>>
*% END
*% IF DPC_COD_TOOC
      *<<<< DPC_COD_TOOC
           IF DPC-COD-TOOC IN QCC-FLD NOT > SPACES
               GO TO FIN-VAL-DPC-COD-TOOC.

           MOVE 'TAB'                   TO TAB-COD-SIST.
           MOVE 'TIO'                   TO TAB-COD-TTAB IN TAB.
           MOVE DPC-COD-TOOC IN QCC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT > SPACES
               MOVE TAB-GLS-DESC TO FRM-GLS-TOOC IN QCC-FLD
           ELSE
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-DPC-COD-TOOC.
      *>>>>
*% END
*% IF DPC_COD_AOOC
      *<<<< DPC_COD_AOOC
           IF DPC-COD-AOOC IN QCC-FLD NOT > SPACES
               GO TO FIN-VAL-DPC-COD-AOOC.

           MOVE 'TAB'                   TO TAB-COD-SIST.
           MOVE 'AUX'                   TO TAB-COD-TTAB IN TAB.
           MOVE DPC-COD-TOOC IN QCC-FLD TO WSS-COD-TOOC IN WSS-VARI.
           MOVE DPC-COD-AOOC IN QCC-FLD TO WSS-COD-AOOC IN WSS-VARI.
           MOVE WSS-COD-CAUX IN WSS-VARI TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT > SPACES
               MOVE TAB-GLS-DESC TO FRM-GLS-AOOC IN QCC-FLD
           ELSE
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-DPC-COD-AOOC.
      *>>>>
*% END
*% IF CVN_COD_TIPO
      *<<<< CVN_COD_TIPO
           IF CVN-COD-TIPO IN QCC-FLD NOT > SPACES
               GO TO FIN-VAL-CVN-COD-TIPO.
           MOVE 'MC2'                   TO TAB-COD-SIST.
           MOVE 'TCV'                   TO TAB-COD-TTAB IN TAB.
           MOVE CVN-COD-TIPO IN QCC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS > SPACES
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CVN-COD-TIPO.
      *>>>>
*% END
*% IF CVN_COD_STIP
      *<<<< CVN_COD_STIP
           IF CVN-COD-STIP IN QCC-FLD NOT > SPACES
               GO TO FIN-VAL-CVN-COD-STIP.
           MOVE 'MC2'                   TO TAB-COD-SIST.
           MOVE 'STC'                   TO TAB-COD-TTAB IN TAB.
           MOVE CVN-COD-TIPO IN QCC-FLD TO WSS-COD-TIPO IN WSS-VARI
           MOVE CVN-COD-STIP IN QCC-FLD TO WSS-COD-STIP IN WSS-VARI
           MOVE WSS-COD-TIST            TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS > SPACES
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CVN-COD-STIP.
      *>>>>
*% END
*% IF FRM_COD_TSEG
      *<<<< FRM_COD_TSEG
           IF FRM-COD-TSEG IN QCC-FLD > SPACES
              MOVE 'SGC'  TO TAB-COD-SIST
              MOVE 'REC ' TO TAB-COD-TTAB IN TAB
              MOVE FRM-COD-TSEG IN QCC-FLD TO TAB-COD-CTAB IN TAB
              PERFORM VAL-COD
              IF MSG-COD-MENS > SPACES
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 MOVE FRM-SUAR-MAL TO FRM-SUAR
                 GO TO FIN-VAL-FRM-COD-TSEG.
      *>>>>
*% END
*% IF FRM_IND_VIGE
      *<<<< FRM_IND_VIGE
           IF FRM-IND-VIGE IN QCC-FLD > SPACES AND
              FRM-IND-VIGE IN QCC-FLD NOT = 'S'
               MOVE 'TAB'         TO MSG-COD-SIST
               MOVE 'CODVIGE INV' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-FRM-IND-VIGE.
      *>>>>
*% END
*% IF DPC_IND_TPLN
      *<<<< DPC_IND_TPLN
           IF DPC-IND-TPLN IN QCC-FLD > SPACES
              MOVE 'TAB'                   TO TAB-COD-SIST
              MOVE 'TPL'                   TO TAB-COD-TTAB IN TAB
              MOVE DPC-IND-TPLN IN QCC-FLD TO TAB-COD-CTAB IN TAB
              PERFORM VAL-COD
              IF MSG-COD-MENS > SPACES
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 MOVE FRM-SUAR-MAL TO FRM-SUAR
                 GO TO FIN-VAL-DPC-IND-TPLN.
      *>>>>
*% END
