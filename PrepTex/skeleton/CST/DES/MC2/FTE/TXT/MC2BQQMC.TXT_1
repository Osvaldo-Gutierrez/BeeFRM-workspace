*%! Codigo adicional
*% IF SETTAG
*% VSNBQ = "06.03.10.1"
*% FNCBQ = "MC2-CONVENIO"
*% END
*% IF FRM_GLS_ICLE
      *<<<< FRM_GLS_ICLE
           IF FRM-NUM-ICLE IN QMC-FLD > ZEROES
              MOVE FRM-NUM-ICLE IN QMC-FLD TO DBC-NUM-ICLI IN DBC
              MOVE FRM-IND-ICLE IN QMC-FLD TO DBC-IND-ICLI IN DBC
              MOVE FRM-GLS-ICLE IN QMC-FLD TO DBC-GLS-ICLI IN DBC
              MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY
              MOVE FIO-GET-KEY TO FIO-CMND
              PERFORM SGC-FIO-DBC
              IF NOT FIO-STAT-OKS
                  MOVE 'SGC'        TO MSG-COD-SIST
                  MOVE 'CLI    NEX' TO MSG-COD-MENS
                  PERFORM GET-MSG
                  MOVE SPACES       TO FRM-GLS-RZSL IN QMC-FLD
                  MOVE MSG-GLS-DESC TO FRM-MENS
                  MOVE FRM-SUAR-MAL TO FRM-SUAR
                  GO TO FIN-VAL-FRM-GLS-ICLE
              ELSE
                  IF DBC-IND-TICL IN DBC NOT = 'E'
                     MOVE 'SGC'        TO MSG-COD-SIST
                     MOVE 'CLI    PER' TO MSG-COD-MENS
                     PERFORM GET-MSG
                     MOVE SPACES TO FRM-GLS-RZSL IN QMC-FLD
                     MOVE MSG-GLS-DESC TO FRM-MENS
                     MOVE FRM-SUAR-MAL TO FRM-SUAR
                     GO TO FIN-VAL-FRM-GLS-ICLE
                  ELSE
                     MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
                     PERFORM CPT-BLKS
                     MOVE CPT-STRN  TO FRM-GLS-RZSL IN QMC-FLD
                     MOVE DBC-CAI-ICLI IN DBC TO SCR-CAI-ICLE
                     MOVE DBC-IIC-ICLI IN DBC TO SCR-IIC-ICLE.
      *>>>>
*% END
*% IF FRM_NUM_ICLE
      *<<<< FRM_NUM_ICLE
           IF FRM-NUM-ICLE IN QMC-FLD NOT > ZEROES
               PERFORM ERR-GLS
               GO TO FIN-VAL-FRM-NUM-ICLE.
      *>>>>
*% END
*% IF FIN_CVN_CAI_ICVN
      *<<<< CVN_CAI_ICVN                                                
      *>>>>                                                             
*% END
*% IF FIN_KEY_CNT_CNT
      *<<<< FIN_KEY_CNT_CNT
           MOVE SCR-CAI-ICLI TO CNT-CAI-ICLE IN CNT.
           MOVE SCR-IIC-ICLI TO CNT-IIC-ICLE IN CNT.
      *>>>>
*% END
*% IF CVN_IIC_ICVN
      *<<<< CVN_IIC_ICVN
           IF CVN-IIC-ICVN IN QMC-FLD NOT > ZEROES
               PERFORM ERR-GLS
               GO TO FIN-VAL-CVN-IIC-ICVN.

           MOVE CVN-CAI-ICVN IN QMC-FLD TO CVN-CAI-ICVN IN CVN.
           MOVE CVN-IIC-ICVN IN QMC-FLD TO CVN-IIC-ICVN IN CVN.
           MOVE 99999999                TO CVN-FEC-FCVN IN CVN.
           MOVE 999999                  TO CVN-HRA-HRCV IN CVN.
           MOVE FIO-GET-LEQ TO FIO-CMND.
           PERFORM MC2-FIO-CVN
           IF NOT ( FIO-STAT-OKS AND
              CVN-CAI-ICVN IN CVN = CVN-CAI-ICVN IN QMC-FLD AND
              CVN-IIC-ICVN IN CVN = CVN-IIC-ICVN IN QMC-FLD )
               MOVE 'MC2'        TO MSG-COD-SIST
      *        MOVE 'TAB'        TO MSG-COD-SIST
               MOVE 'CVN    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-CVN-IIC-ICVN
           ELSE
      *        MOVE 'TAB'               TO TAB-COD-SIST                 
               MOVE 'MC2'               TO TAB-COD-SIST                 
      *        MOVE 'DPL'               TO TAB-COD-TTAB IN TAB          
               MOVE 'DCV'               TO TAB-COD-TTAB IN TAB          
               MOVE CVN-CIC-ICVN IN CVN TO TAB-COD-CTAB IN TAB          
               PERFORM BUS-TAB                                          
               MOVE TAB-GLS-DESC TO FRM-GLS-ICVN IN QMC-FLD             
               GO TO FIN-VAL-CVN-IIC-ICVN.                              
      *>>>>
*% END
