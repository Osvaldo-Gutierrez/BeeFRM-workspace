*%! Codigo adicional para archivo
*% IF SETTAG
*% VSNBQ = "05.11.21.1"
*% VSNBU = "05.11.21.1"
*% FNCBQ = "MC2-CONVENIO"
*% FNCBU = "MC2-CONVENIO"
*% END
*% IF FIN_FST_FLD_DEC
      *<<<< FIN_FST_FLD_DEC
      *>>>>
*% END
*% IF FIN_KEY_DEC_DEC
      *<<<< FIN_KEY_DEC_DEC
           MOVE SCR-CAI-ICLI TO DEC-CAI-ICLI IN DEC.
           MOVE SCR-IIC-ICLI TO DEC-IIC-ICLI IN DEC.
      *>>>>
*% END
*% IF FIN_PUT_DEC_DEC
      *<<<< FIN_PUT_DEC_DEC
           IF DEC-IND-SEMP IN DEC-FLD > SPACES
              MOVE 'MC2'                    TO TAB-COD-SIST
      *       MOVE 'TAB'                    TO TAB-COD-SIST
              MOVE 'SEM'                    TO TAB-COD-TTAB IN TAB
              MOVE DEC-IND-SEMP IN DEC-FLD  TO TAB-COD-CTAB IN TAB
              PERFORM BUS-TAB
              MOVE TAB-GLS-DESC IN TAB      TO FRM-GLS-SEMP IN DEC-FLD
           ELSE
              MOVE SPACES                   TO FRM-GLS-SEMP IN DEC-FLD.

           IF DEC-IND-APRB IN DEC-FLD > SPACES
              MOVE 'MC2'                   TO TAB-COD-SIST
      *       MOVE 'TAB'                   TO TAB-COD-SIST
              MOVE 'APE'                   TO TAB-COD-TTAB IN TAB
              MOVE DEC-IND-APRB IN DEC-FLD TO TAB-COD-CTAB IN TAB
              PERFORM BUS-TAB
              MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-APRB IN DEC-FLD
           ELSE
              MOVE SPACES              TO FRM-GLS-APRB IN DEC-FLD.

           IF DEC-COD-FRTE IN DEC-FLD > SPACES
              MOVE 'MC2'                   TO TAB-COD-SIST
      *       MOVE 'TAB'                   TO TAB-COD-SIST
              MOVE 'FEN'                   TO TAB-COD-TTAB IN TAB
              MOVE DEC-COD-FRTE IN DEC-FLD TO TAB-COD-CTAB IN TAB
              PERFORM BUS-TAB
              MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-FRTE IN DEC-FLD
           ELSE
              MOVE SPACES              TO FRM-GLS-FRTE IN DEC-FLD.

           IF DEC-IND-CMEM IN DEC-FLD > SPACES
              MOVE 'MC2'                   TO TAB-COD-SIST
      *       MOVE 'TAB'                   TO TAB-COD-SIST
              MOVE 'CME'                   TO TAB-COD-TTAB IN TAB
              MOVE DEC-IND-CMEM IN DEC-FLD TO TAB-COD-CTAB IN TAB
              PERFORM BUS-TAB
              MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-CMEM IN DEC-FLD
           ELSE
              MOVE SPACES              TO FRM-GLS-CMEM IN DEC-FLD.
        
           MOVE DEC-CAI-ICLI IN DEC TO DBC-CAI-ICLI IN DBC.
           MOVE DEC-IIC-ICLI IN DEC TO DBC-IIC-ICLI IN DBC.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           IF NOT FIO-STAT-OKS
              MOVE 'SGC'          TO MSG-COD-SIST
              MOVE 'CLI    NEX'   TO MSG-COD-MENS
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-GLS-RZNS
           ELSE
              MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
              PERFORM CPT-BLKS
              MOVE CPT-STRN            TO FRM-GLS-RZNS IN DEC-FLD
              MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN DEC-FLD
              MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN DEC-FLD
              MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN DEC-FLD
              MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN DEC-FLD.

      *    MOVE 'TAB'               TO TAB-COD-SIST.                    
      *    MOVE 'DPL'               TO TAB-COD-TTAB IN TAB.             
           MOVE 'MC2'               TO TAB-COD-SIST.                    
           MOVE 'DCV'               TO TAB-COD-TTAB IN TAB.             
           MOVE DEC-CIC-ICVN IN DEC TO TAB-COD-CTAB IN TAB.
           PERFORM BUS-TAB.
           MOVE TAB-GLS-DESC TO FRM-GLS-DSCV IN DEC-FLD.              
      *>>>>
*% END
*% IF INI_DEC_FEC_PLAN
      *<<<< INI_DEC_FEC_PLAN
      *>>>>
*% END
*% IF VAL_CON_FLD_DEC
      *<<<< VAL_CON_FLD_DEC
      *>>>>
*% END
*% IF DEC_IND_SEMP
      *<<<< DEC_IND_SEMP
           MOVE 'MC2'                   TO TAB-COD-SIST.
      *    MOVE 'TAB'                   TO TAB-COD-SIST.
           MOVE 'SEM'                   TO TAB-COD-TTAB IN TAB.
           MOVE DEC-IND-SEMP IN DEC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT > SPACES
               MOVE TAB-GLS-DESC TO FRM-GLS-SEMP IN DEC-FLD
           ELSE
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-DEC-IND-SEMP.
      *>>>>
*% END
*% IF DEC_IND_APRB
      *<<<< DEC_IND_APRB
           MOVE 'MC2'                   TO TAB-COD-SIST.
      *    MOVE 'TAB'                   TO TAB-COD-SIST.
           MOVE 'APE'                   TO TAB-COD-TTAB IN TAB.
           MOVE DEC-IND-APRB IN DEC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT > SPACES
               MOVE TAB-GLS-DESC TO FRM-GLS-APRB IN DEC-FLD
           ELSE
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-DEC-IND-APRB.
      *>>>>
*% END
*% IF DEC_COD_FRTE
      *<<<< DEC_COD_FRTE
           MOVE 'MC2'                   TO TAB-COD-SIST.
      *    MOVE 'TAB'                   TO TAB-COD-SIST.
           MOVE 'FEN'                   TO TAB-COD-TTAB IN TAB.
           MOVE DEC-COD-FRTE IN DEC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT > SPACES
               MOVE TAB-GLS-DESC TO FRM-GLS-FRTE IN DEC-FLD
           ELSE
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-DEC-COD-FRTE.
      *>>>>
*% END
*% IF DEC_IND_CMEM
      *<<<< DEC_IND_CMEM
           MOVE 'MC2'                   TO TAB-COD-SIST.
      *    MOVE 'TAB'                   TO TAB-COD-SIST.
           MOVE 'CME'                   TO TAB-COD-TTAB IN TAB.
           MOVE DEC-IND-CMEM IN DEC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT > SPACES
               MOVE TAB-GLS-DESC TO FRM-GLS-CMEM IN DEC-FLD
           ELSE
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-DEC-IND-CMEM.
      *>>>>
*% END
*% IF DEC_COD_TREC
      *<<<< DEC_COD_TREC
      *VAL-COD Valida codigo de tabla
           MOVE 'SGC' TO TAB-COD-SIST.
           MOVE 'REC ' TO TAB-COD-TTAB IN TAB.
           MOVE DEC-COD-TREC IN DEC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS NOT > SPACES
               MOVE TAB-GLS-DESC TO FRM-GLS-TREC IN DEC-FLD
           ELSE
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-DEC-COD-TREC.
      *>>>>
*% END
*% IF DBC_GLS_ICLI
      *<<<< DBC_GLS_ICLI
           IF DBC-NUM-ICLI IN DEC-FLD > ZEROES
              MOVE DBC-NUM-ICLI IN DEC-FLD TO DBC-NUM-ICLI IN DBC
              MOVE DBC-IND-ICLI IN DEC-FLD TO DBC-IND-ICLI IN DBC
              MOVE DBC-GLS-ICLI IN DEC-FLD TO DBC-GLS-ICLI IN DBC
              MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY
              MOVE FIO-GET-KEY TO FIO-CMND
              PERFORM SGC-FIO-DBC
              IF NOT FIO-STAT-OKS
                  MOVE 'SGC'        TO MSG-COD-SIST
                  MOVE 'CLI    NEX' TO MSG-COD-MENS
                  PERFORM GET-MSG
                  MOVE SPACES       TO FRM-GLS-RZNS IN DEC-FLD
                  MOVE MSG-GLS-DESC TO FRM-MENS
                  MOVE FRM-SUAR-MAL TO FRM-SUAR
                  GO TO FIN-VAL-DBC-GLS-ICLI
              ELSE
                  IF DBC-IND-TICL IN DBC NOT = 'E'
                     MOVE 'SGC'        TO MSG-COD-SIST
                     MOVE 'CLI    PER' TO MSG-COD-MENS
                     PERFORM GET-MSG
                     MOVE SPACES TO FRM-GLS-RZNS IN DEC-FLD
                     MOVE MSG-GLS-DESC TO FRM-MENS
                     MOVE FRM-SUAR-MAL TO FRM-SUAR
                     GO TO FIN-VAL-DBC-GLS-ICLI
                  ELSE
                     MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
                     PERFORM CPT-BLKS
                     MOVE CPT-STRN  TO FRM-GLS-RZNS IN DEC-FLD
                     MOVE DBC-CAI-ICLI IN DBC TO SCR-CAI-ICLI
                     MOVE DBC-IIC-ICLI IN DBC TO SCR-IIC-ICLI.
      *>>>>
*% END
*% IF DBC_NUM_ICLI
      *<<<< DBC_NUM_ICLI
           IF DBC-NUM-ICLI IN DEC-FLD NOT > ZEROES
               PERFORM ERR-GLS
               GO TO FIN-VAL-DBC-NUM-ICLI.
      *>>>>
*% END
*% IF DEC_CAI_ICVN
      *<<<< DEC_CAI_ICVN                                                
           IF SCR-CMND NOT = 'ING' AND                                  
               DEC-CAI-ICVN IN DEC-FLD NOT > SPACES                     
               PERFORM ERR-GLS                                          
               GO TO FIN-VAL-DEC-CAI-ICVN.                              
      *>>>>                                                             
*% END
*% IF DEC_IIC_ICVN
      *<<<< DEC_IIC_ICVN
      *MHM-INI 27-JUN-2008                                              
      *    IF SCR-CMND NOT = 'ING' AND                                  
      *        DEC-IIC-ICVN IN DEC-FLD NOT > ZEROES                     
           IF DEC-IIC-ICVN IN DEC-FLD NOT > ZEROES                      
      *MHM-INI 27-JUN-2008                                              
               PERFORM ERR-GLS
               GO TO FIN-VAL-DEC-IIC-ICVN.
 
      *    IF NOT SCR-OFRM-SRV
           IF DEC-IIC-ICVN IN DEC-FLD > ZEROES
               MOVE DEC-CAI-ICVN IN DEC-FLD TO CVN-CAI-ICVN IN CVN
               MOVE DEC-IIC-ICVN IN DEC-FLD TO CVN-IIC-ICVN IN CVN
               MOVE 99999999                TO CVN-FEC-FCVN IN CVN
               MOVE 999999                  TO CVN-HRA-HRCV IN CVN
               MOVE FIO-GET-LEQ TO FIO-CMND
               PERFORM MC2-FIO-CVN
               IF NOT ( FIO-STAT-OKS AND
                  CVN-CAI-ICVN IN CVN = DEC-CAI-ICVN IN DEC-FLD AND
                  CVN-IIC-ICVN IN CVN = DEC-IIC-ICVN IN DEC-FLD )
                   MOVE 'MC2'        TO MSG-COD-SIST
      *            MOVE 'TAB'        TO MSG-COD-SIST
                   MOVE 'CVN    NEX' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
                   GO TO FIN-VAL-DEC-IIC-ICVN
               ELSE
      *            MOVE 'TAB'               TO TAB-COD-SIST
      *            MOVE 'DPL'               TO TAB-COD-TTAB IN TAB
                   MOVE 'MC2'               TO TAB-COD-SIST             
                   MOVE 'DCV'               TO TAB-COD-TTAB IN TAB      
                   MOVE CVN-CIC-ICVN IN CVN TO TAB-COD-CTAB IN TAB
                   MOVE CVN-IND-SCVN IN CVN TO SCR-IND-SCVN
                   PERFORM BUS-TAB
                   MOVE TAB-GLS-DESC TO FRM-GLS-DSCV IN DEC-FLD            
                   GO TO FIN-VAL-DEC-IIC-ICVN.

      *MHM-INI 27-JUN-2008                                              
      *    IF (SCR-CMND = 'ING' AND                                     
      *        DEC-CAI-ICVN IN DEC-FLD NOT > SPACES AND                 
      *        DEC-IIC-ICVN IN DEC-FLD NOT > ZEROES)                    
      *         PERFORM OBT-CIC-CVN                                     
      *         IF CIC-STAT NOT = CIC-STAT-OKS                          
      *            MOVE FRM-SUAR-MAL TO FRM-SUAR                        
      *            MOVE CIC-MENS     TO FRM-MENS                        
      *         ELSE                                                    
      *            MOVE CIC-CAI-ICIC IN CIC TO DEC-CAI-ICVN IN DEC-FLD  
      *            MOVE CIC-IIC-ICIC IN CIC TO DEC-IIC-ICVN IN DEC-FLD. 
      *MHM-FIN 27-JUN-2008                                              
      *>>>>
*% END
*% IF EOF_BQ
      *<<<< EOF_BQ
       OBT-CIC-CVN SECTION.
       INI-OBT-CIC-CVN.
           MOVE 'PCNV'  TO CIC-CAI-ICIC IN CIC.
           MOVE CIC-GET TO CIC-CMND.
           PERFORM PRO-CIC.
           IF CIC-STAT NOT = CIC-STAT-OKS
               MOVE CIC-MENS     TO FRM-MENS
               MOVE FIO-STAT-NEX TO FIO-STAT.
       FIN-OBT-CIC-CVN.
             EXIT.
      *>>>>
*% END
