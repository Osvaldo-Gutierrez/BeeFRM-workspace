      *
      * DAPBQBW4
      * Version 3.0.1 Nov-2005
      * 26-Aug-09 01:15 PM
      *
       SET-TAG-DAPBQBW4 SECTION.
       INI-SET-TAG-DAPBQBW4.
           MOVE
           '<IDNMOD NOM=$DAPBQBW4$ VSN=$08.08.18$ VGM=$3.0.1$
      -    'GEN=$26-Aug-09 01:15 PM$ FNC=$080816$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-DAPBQBW4.
           EXIT.
           
       PUT-ALL-BW4 SECTION.
       INI-PUT-ALL-BW4.
           PERFORM PUT-BCW-BW4.
       FIN-PUT-ALL-BW4.
           EXIT.
       
       PUT-BCW-BW4 SECTION.
       INI-PUT-BCW-BW4.
           IF PGM-STAT-BCW-NUL
               GO TO FIN-PUT-BCW-BW4.
           IF NOT PGM-STAT-BCW-OKS
               PERFORM CLR-BCW-BW4
               GO TO FIN-PUT-BCW-BW4.
           MOVE BCW-COD-SBTP IN BCW TO BCW-COD-SBTP IN BW4-FLD.
           MOVE BCW-COD-STAX IN BCW TO BCW-COD-STAX IN BW4-FLD.
           MOVE BCW-COD-EJEC IN BCW TO BCW-COD-EJEC IN BW4-FLD.
           MOVE BCW-COD-REGI IN BCW TO BCW-COD-REGI IN BW4-FLD.
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE BCW-NUM-DCRE IN BCW TO BCW-NUM-DCRE IN BW4-FLD.
           MOVE BCW-NUM-MCRE IN BCW TO BCW-NUM-MCRE IN BW4-FLD.
           MOVE BCW-NUM-SCRE IN BCW TO BCW-NUM-SCRE IN BW4-FLD.
           MOVE BCW-NUM-ACRE IN BCW TO BCW-NUM-ACRE IN BW4-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE BCW-GLS-DISP IN BCW TO BCW-GLS-DISP IN BW4-FLD.
           MOVE BCW-COD-REFN IN BCW TO BCW-COD-REFN IN BW4-FLD.
           MOVE BCW-NUM-INTP IN BCW TO BCW-NUM-INTP IN BW4-FLD.
           MOVE BCW-NUM-INDV IN BCW TO BCW-NUM-INDV IN BW4-FLD.
           MOVE BCW-NUM-RDEV IN BCW TO BCW-NUM-RDEV IN BW4-FLD.
           MOVE BCW-NUM-RPAG IN BCW TO BCW-NUM-RPAG IN BW4-FLD.
           MOVE BCW-COD-CRED IN BCW TO BCW-COD-CRED IN BW4-FLD.
           MOVE BCW-COD-CNTA IN BCW TO BCW-COD-CNTA IN BW4-FLD.
           MOVE BCW-NUM-RUTT IN BCW TO BCW-NUM-RUTT IN BW4-FLD.
           MOVE BCW-NUM-DIGV IN BCW TO BCW-NUM-DIGV IN BW4-FLD.
           MOVE BCW-NUM-CLIE IN BCW TO BCW-NUM-CLIE IN BW4-FLD.
           MOVE BCW-COD-OFIC IN BCW TO BCW-COD-OFIC IN BW4-FLD.
           MOVE BCW-COD-BNCA IN BCW TO BCW-COD-BNCA IN BW4-FLD.
       FIN-PUT-BCW-BW4.
           EXIT.
       
       KEY-ALL-BW4 SECTION.
       INI-KEY-ALL-BW4.
           PERFORM KEY-BCW-BW4.
       FIN-KEY-ALL-BW4.
           EXIT.
       
       KEY-BCW-BW4 SECTION.
       INI-KEY-BCW-BW4.
           MOVE BCW-COD-REFN IN BW4-FLD TO BCW-COD-REFN IN BCW.
       FIN-KEY-BCW-BW4.
           EXIT.
       
       SCR-CLRA-BW4 SECTION.
       INI-SCR-CLRA-BW4.
           PERFORM CLR-KEY-BW4.
           PERFORM CLR-FLD-BW4.
       FIN-SCR-CLRA-BW4.
           EXIT.
       
       CLR-KEY-BW4 SECTION.
       INI-CLR-KEY-BW4.
           MOVE SPACES TO BCW-COD-REFN IN BW4-FLD.
       FIN-CLR-KEY-BW4.
           EXIT.
       
       CLR-FLD-BW4 SECTION.
       INI-CLR-FLD-BW4.
           PERFORM CLR-BCW-BW4.
       FIN-CLR-FLD-BW4.
           EXIT.
       
       CLR-BCW-BW4 SECTION.
       INI-CLR-BCW-BW4.
           MOVE SPACES TO BCW-COD-SBTP IN BW4-FLD.
           MOVE SPACES TO BCW-COD-STAX IN BW4-FLD.
           MOVE SPACES TO BCW-COD-EJEC IN BW4-FLD.
           MOVE SPACES TO BCW-COD-REGI IN BW4-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO BCW-FEC-FCRE IN BW4-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO BCW-GLS-DISP IN BW4-FLD.
           MOVE ZEROES TO BCW-NUM-INTP IN BW4-FLD.
           MOVE ZEROES TO BCW-NUM-INDV IN BW4-FLD.
           MOVE ZEROES TO BCW-NUM-RDEV IN BW4-FLD.
           MOVE ZEROES TO BCW-NUM-RPAG IN BW4-FLD.
           MOVE SPACES TO BCW-COD-CRED IN BW4-FLD.
           MOVE SPACES TO BCW-COD-CNTA IN BW4-FLD.
           MOVE ZEROES TO BCW-NUM-RUTT IN BW4-FLD.
           MOVE SPACES TO BCW-NUM-DIGV IN BW4-FLD.
           MOVE ZEROES TO BCW-NUM-CLIE IN BW4-FLD.
           MOVE SPACES TO BCW-COD-OFIC IN BW4-FLD.
           MOVE SPACES TO BCW-COD-BNCA IN BW4-FLD.
       FIN-CLR-BCW-BW4.
           EXIT.
       
       FST-KEY-BW4 SECTION.
       INI-FST-KEY-BW4.
           MOVE -1 TO BCW-COD-REFN-LEN IN BW4-FLD.
       FIN-FST-KEY-BW4.
           EXIT.
       
       FST-FLD-BW4 SECTION.
       INI-FST-FLD-BW4.
           MOVE -1 TO BCW-COD-SBTP-LEN IN BW4-FLD.
       FIN-FST-FLD-BW4.
           EXIT.
       
       PRO-KEY-BW4 SECTION.
       INI-PRO-KEY-BW4.
           PERFORM PRO-IKY-BW4.
           PERFORM PRO-AKY-BW4.
           PERFORM PRO-MKY-BW4.
       FIN-PRO-KEY-BW4.
           EXIT.
       
       PRO-IKY-BW4 SECTION.
       INI-PRO-IKY-BW4.
           MOVE FRM-CPIM-AEY TO BCW-COD-REFN-ATR IN BW4-FLD.
       FIN-PRO-IKY-BW4.
           EXIT.
       
       PRO-MKY-BW4 SECTION.
       INI-PRO-MKY-BW4.
       FIN-PRO-MKY-BW4.
           EXIT.
       
       PRO-AKY-BW4 SECTION.
       INI-PRO-AKY-BW4.
       FIN-PRO-AKY-BW4.
           EXIT.
       
       UNP-KEY-BW4 SECTION.
       INI-UNP-KEY-BW4.
           PERFORM UNP-IKY-BW4.
           PERFORM UNP-AKY-BW4.
           PERFORM UNP-MKY-BW4.
       FIN-UNP-KEY-BW4.
           EXIT.
       
       UNP-IKY-BW4 SECTION.
       INI-UNP-IKY-BW4.
           MOVE FRM-CPIM-UBY TO BCW-COD-REFN-ATR.
      *<<<< FIN_UNP_IKY_BW4
      *>>>>
       FIN-UNP-IKY-BW4.
           EXIT.
       
       UNP-MKY-BW4 SECTION.
       INI-UNP-MKY-BW4.
       FIN-UNP-MKY-BW4.
           EXIT.
       
       UNP-AKY-BW4 SECTION.
       INI-UNP-AKY-BW4.
       FIN-UNP-AKY-BW4.
           EXIT.
       
       UNP-FLD-BW4 SECTION.
       INI-UNP-FLD-BW4.
           PERFORM UNP-BCW-BW4.
       FIN-UNP-FLD-BW4.
           EXIT.
       
       UNP-BCW-BW4 SECTION.
       INI-UNP-BCW-BW4.
           MOVE FRM-CPIM-UBY TO BCW-COD-SBTP-ATR IN BW4-FLD.
           MOVE FRM-CPIM-UBY TO BCW-COD-STAX-ATR IN BW4-FLD.
           MOVE FRM-CPIM-UBY TO BCW-COD-EJEC-ATR IN BW4-FLD.
           MOVE FRM-CPIM-UBY TO BCW-COD-REGI-ATR IN BW4-FLD.
           MOVE FRM-CPIM-UBY TO BCW-FEC-FCRE-ATR IN BW4-FLD.
           MOVE FRM-CPIM-UBY TO BCW-GLS-DISP-ATR IN BW4-FLD.
           MOVE FRM-CPIM-UBY TO BCW-NUM-INTP-ATR IN BW4-FLD.
           MOVE FRM-CPIM-UBY TO BCW-NUM-INDV-ATR IN BW4-FLD.
           MOVE FRM-CPIM-UBY TO BCW-NUM-RDEV-ATR IN BW4-FLD.
           MOVE FRM-CPIM-UBY TO BCW-NUM-RPAG-ATR IN BW4-FLD.
           MOVE FRM-CPIM-UBY TO BCW-COD-CRED-ATR IN BW4-FLD.
           MOVE FRM-CPIM-UBY TO BCW-COD-CNTA-ATR IN BW4-FLD.
           MOVE FRM-CPIM-UBY TO BCW-NUM-RUTT-ATR IN BW4-FLD.
           MOVE FRM-CPIM-UBY TO BCW-NUM-DIGV-ATR IN BW4-FLD.
           MOVE FRM-CPIM-UBY TO BCW-NUM-CLIE-ATR IN BW4-FLD.
           MOVE FRM-CPIM-UBY TO BCW-COD-OFIC-ATR IN BW4-FLD.
           MOVE FRM-CPIM-UBY TO BCW-COD-BNCA-ATR IN BW4-FLD.
       FIN-UNP-BCW-BW4.
           EXIT.
       
       VAL-KEY-BW4 SECTION.
       INI-VAL-KEY-BW4.
           PERFORM VAL-IKY-BW4.
           PERFORM VAL-MKY-BW4.
           PERFORM VAL-AKY-BW4.
       FIN-VAL-KEY-BW4.
           EXIT.
       
       VAL-IKY-BW4 SECTION.
       INI-VAL-IKY-BW4.
           PERFORM VAL-BCW-COD-REFN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO BCW-COD-REFN-LEN IN BW4-FLD
                GO TO FIN-VAL-IKY-BW4.
       FIN-VAL-IKY-BW4.
           EXIT.
       
       VAL-MKY-BW4 SECTION.
       INI-VAL-MKY-BW4.
       FIN-VAL-MKY-BW4.
           EXIT.
       
       VAL-AKY-BW4 SECTION.
       INI-VAL-AKY-BW4.
       FIN-VAL-AKY-BW4.
           EXIT.
       
       VAL-BCW-COD-REFN SECTION.
       INI-VAL-BCW-COD-REFN.
           IF BCW-COD-REFN IN BW4-FLD NOT > SPACES
              MOVE SPACES TO BCW-COD-REFN IN BW4-FLD.
       FIN-VAL-BCW-COD-REFN.
           EXIT.
       
       VAL-CON-KEY-BW4 SECTION.
       INI-VAL-CON-KEY-BW4.
           IF     BCW-COD-REFN IN BW4-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-BW4.
       FIN-VAL-CON-KEY-BW4.
           EXIT.
       
       VAL-CON-IKY-BW4 SECTION.
       INI-VAL-CON-IKY-BW4.
           IF     BCW-COD-REFN IN BW4-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-BW4.
       FIN-VAL-CON-IKY-BW4.
           EXIT.
       
       VAL-NUM-KEY-BW4 SECTION.
       INI-VAL-NUM-KEY-BW4.
           PERFORM VAL-NUM-IKY-BW4.
           PERFORM VAL-NUM-MKY-BW4.
           PERFORM VAL-NUM-AKY-BW4.
       FIN-VAL-NUM-KEY-BW4.
           EXIT.
       
       VAL-NUM-IKY-BW4 SECTION.
       INI-VAL-NUM-IKY-BW4.
       FIN-VAL-NUM-IKY-BW4.
           EXIT.
       
       VAL-NUM-MKY-BW4 SECTION.
       INI-VAL-NUM-MKY-BW4.
       FIN-VAL-NUM-MKY-BW4.
           EXIT.
       
       VAL-NUM-AKY-BW4 SECTION.
       INI-VAL-NUM-AKY-BW4.
       FIN-VAL-NUM-AKY-BW4.
           EXIT.
       
       VAL-NUM-FLD-BW4 SECTION.
       INI-VAL-NUM-FLD-BW4.
       VAL-NUM-BCW-NUM-INTP.
           MOVE BCW-NUM-INTP-ALF IN BW4-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO BCW-NUM-INTP IN BW4-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO BCW-NUM-INTP-LEN IN BW4-FLD
                GO TO FIN-VAL-NUM-FLD-BW4.
       VAL-NUM-BCW-NUM-INDV.
           MOVE BCW-NUM-INDV-ALF IN BW4-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO BCW-NUM-INDV IN BW4-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO BCW-NUM-INDV-LEN IN BW4-FLD
                GO TO FIN-VAL-NUM-FLD-BW4.
       VAL-NUM-BCW-NUM-RDEV.
           MOVE BCW-NUM-RDEV-ALF IN BW4-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO BCW-NUM-RDEV IN BW4-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO BCW-NUM-RDEV-LEN IN BW4-FLD
                GO TO FIN-VAL-NUM-FLD-BW4.
       VAL-NUM-BCW-NUM-RPAG.
           MOVE BCW-NUM-RPAG-ALF IN BW4-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO BCW-NUM-RPAG IN BW4-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO BCW-NUM-RPAG-LEN IN BW4-FLD
                GO TO FIN-VAL-NUM-FLD-BW4.
       VAL-NUM-BCW-NUM-RUTT.
           MOVE BCW-NUM-RUTT-ALF IN BW4-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 10 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO BCW-NUM-RUTT IN BW4-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO BCW-NUM-RUTT-LEN IN BW4-FLD
                GO TO FIN-VAL-NUM-FLD-BW4.
       VAL-NUM-BCW-NUM-CLIE.
           MOVE BCW-NUM-CLIE-ALF IN BW4-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 15 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NS18 TO BCW-NUM-CLIE IN BW4-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO BCW-NUM-CLIE-LEN IN BW4-FLD
                GO TO FIN-VAL-NUM-FLD-BW4.
       FIN-VAL-NUM-FLD-BW4.
           EXIT.
       
       EDT-KEY-BW4 SECTION.
       INI-EDT-KEY-BW4.
           PERFORM EDT-IKY-BW4.
           PERFORM EDT-MKY-BW4.
           PERFORM EDT-AKY-BW4.
       FIN-EDT-KEY-BW4.
           EXIT.
       
       EDT-IKY-BW4 SECTION.
       INI-EDT-IKY-BW4.
       FIN-EDT-IKY-BW4.
           EXIT.
       
       EDT-MKY-BW4 SECTION.
       INI-EDT-MKY-BW4.
       FIN-EDT-MKY-BW4.
           EXIT.
       
       EDT-AKY-BW4 SECTION.
       INI-EDT-AKY-BW4.
       FIN-EDT-AKY-BW4.
           EXIT.
       
       EDT-FLD-BW4 SECTION.
       INI-EDT-FLD-BW4.
           IF BCW-NUM-INTP-LEN IN BW4-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO BCW-NUM-INTP-LEN IN BW4-FLD
                GO TO FIN-EDT-FLD-BW4.
           MOVE BCW-NUM-INTP IN BW4-FLD TO BCW-NUM-INTP-EDT IN BW4-EDT.
           MOVE BCW-NUM-INTP-EDT IN BW4-EDT TO 
                BCW-NUM-INTP-ALF IN BW4-FLD.
           IF BCW-NUM-INDV-LEN IN BW4-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO BCW-NUM-INDV-LEN IN BW4-FLD
                GO TO FIN-EDT-FLD-BW4.
           MOVE BCW-NUM-INDV IN BW4-FLD TO BCW-NUM-INDV-EDT IN BW4-EDT.
           MOVE BCW-NUM-INDV-EDT IN BW4-EDT TO 
                BCW-NUM-INDV-ALF IN BW4-FLD.
           IF BCW-NUM-RDEV-LEN IN BW4-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO BCW-NUM-RDEV-LEN IN BW4-FLD
                GO TO FIN-EDT-FLD-BW4.
           MOVE BCW-NUM-RDEV IN BW4-FLD TO BCW-NUM-RDEV-EDT IN BW4-EDT.
           MOVE BCW-NUM-RDEV-EDT IN BW4-EDT TO 
                BCW-NUM-RDEV-ALF IN BW4-FLD.
           IF BCW-NUM-RPAG-LEN IN BW4-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO BCW-NUM-RPAG-LEN IN BW4-FLD
                GO TO FIN-EDT-FLD-BW4.
           MOVE BCW-NUM-RPAG IN BW4-FLD TO BCW-NUM-RPAG-EDT IN BW4-EDT.
           MOVE BCW-NUM-RPAG-EDT IN BW4-EDT TO 
                BCW-NUM-RPAG-ALF IN BW4-FLD.
           IF BCW-NUM-RUTT-LEN IN BW4-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO BCW-NUM-RUTT-LEN IN BW4-FLD
                GO TO FIN-EDT-FLD-BW4.
           MOVE BCW-NUM-RUTT IN BW4-FLD TO BCW-NUM-RUTT-EDT IN BW4-EDT.
           MOVE BCW-NUM-RUTT-EDT IN BW4-EDT TO 
                BCW-NUM-RUTT-ALF IN BW4-FLD.
           IF BCW-NUM-CLIE-LEN IN BW4-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO BCW-NUM-CLIE-LEN IN BW4-FLD
                GO TO FIN-EDT-FLD-BW4.
           MOVE BCW-NUM-CLIE IN BW4-FLD TO BCW-NUM-CLIE-EDT IN BW4-EDT.
           MOVE BCW-NUM-CLIE-EDT IN BW4-EDT TO 
                BCW-NUM-CLIE-ALF IN BW4-FLD.
       FIN-EDT-FLD-BW4.
           EXIT.
      *<<<< EOF_BQ
      *>>>> EOF_BQ
      *<<<< EOF
      *>>>> EOF
