           PERFORM GNS-PRO-QUE.
       FIN-GNX-PRO-QUE.
           EXIT.
      
       SET-KEY-LAD SECTION.
       INI-SET-KEY-LAD.
      
           MOVE WSS-COD-ENTI TO LAD-COD-ENTF IN LAD
                                LAD-COD-ENTC IN LAD
                                LAD-COD-ENTA IN LAD
                                LAD-COD-ENTI IN LAD.
      
           IF FRM-COD-CREB IN CLV-FLD = 'OPE'
               MOVE 'LAD-KEY-OLAD' TO FIO-AKEY
           ELSE
           IF FRM-COD-CREB IN CLV-FLD = 'CLI'
               MOVE 'LAD-KEY-CLAD' TO FIO-AKEY
           ELSE
           IF FRM-COD-CREB IN CLV-FLD = 'AUT'
               MOVE 'LAD-KEY-ALAD' TO FIO-AKEY.
       FIN-SET-KEY-LAD.
           EXIT.
      
       OBTENER-NOMBRE-DBC  SECTION.
      *----------------------------
       INI-OBTENER-NOMBRE-DBC.
      
           IF WSS-LEE-DBC-DESDE-LAD = 'S'
               MOVE LAD-CIC-ICLI IN LAD TO DBC-CIC-ICLI IN DBC.
      
           IF FRM-COD-CREB IN CLV-FLD = 'OPE' AND
              NOT PGM-STAT-DBC-OKS
                MOVE LAD-CIC-IOPD IN LAD TO RCC-CIC-IOPD IN RCC
                PERFORM GET-CUR-RCC
                IF PGM-STAT-RCC-OKS AND
                   LAD-CIC-IOPD IN LAD  =  RCC-CIC-IOPD IN RCC
                     MOVE RCC-CIC-ICLI IN RCC TO DBC-CIC-ICLI IN DBC
                ELSE
                   MOVE '23'      TO FIO-STAT
                   MOVE '23'      TO PGM-STAT-DBC
                   GO TO FIN-OBTENER-NOMBRE-DBC.
      
           IF FRM-COD-CREB IN CLV-FLD = 'CLI' AND
              NOT PGM-STAT-DBC-OKS
                MOVE 'DBC-IDC-ICLI' TO FIO-AKEY.
      
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           MOVE FIO-STAT TO PGM-STAT-DBC.
      
           IF FIO-STAT-OKS
      *  ISP INI 27-04-2009
                MOVE DBC-CIC-ICLI IN DBC TO
                     LAD-CIC-ICLI IN LAD
      *  FIN INI 27-04-2009
                PERFORM CPT-NOMB
           ELSE
                MOVE SPACES TO DBC-GLS-NOMC IN DBC
                MOVE SPACES TO DBC-VRF-ICLI IN DBC
                MOVE SPACES TO DBC-IDC-ICLI IN DBC
                MOVE '00'   TO PGM-STAT-DBC.
      
       FIN-OBTENER-NOMBRE-DBC.
           EXIT.
      *
       GET-CUR-RCC SECTION.
       INI-GET-CUR-RCC.
           MOVE   'RCC-KEY-IOPD'           TO FIO-AKEY.
           MOVE    FIO-GET-NLS             TO FIO-CMND.
           PERFORM DAP-FIO-RCC.
           IF NOT ( FIO-STAT-OKS     AND
                    LAD-CIC-IOPD IN LAD = RCC-CIC-IOPD IN RCC )
                    MOVE '23' TO FIO-STAT.
           MOVE   FIO-STAT  TO PGM-STAT-RCC.
       FIN-GET-CUR-RCC.
           EXIT.
      
      
       COPY GNSBBTAB.
       COPY DAPBFVEN.
       COPY DAPBFRCC.
       COPY SGCBFDBC.
       COPY GNSBGCPT.
       COPY GNSBGSTR.
       CPT-NOMB SECTION.
       INI-CPT-NOMB.
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.
           PERFORM CPT-BLKS.
           MOVE CPT-STRN            TO DBC-GLS-NOMC IN DBC.
       FIN-CPT-NOMB.
           EXIT.
      
       PRO-PF7 SECTION.
       INI-PRO-PF7.
           PERFORM SET-KEY-LAD.
           MOVE FIO-GET-PRV TO FIO-CMND.
           PERFORM DAP-FIO-LAD.
           MOVE FIO-STAT TO PGM-STAT-LAD.
           IF NOT FIO-STAT-OKS
                    MOVE '23'  TO FIO-STAT
                    MOVE FIO-STAT TO PGM-STAT-LAD
           ELSE
           IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI
                    MOVE '23'  TO FIO-STAT
                    MOVE FIO-STAT TO PGM-STAT-LAD.
           IF NOT FIO-STAT-OKS
                    OR
              (
                   FIO-STAT-OKS
                       AND
               ( FRM-COD-CREB IN CLV-FLD = 'OPE'
                         AND
                  (
                     LAD-CAI-IOPD IN CLV-FLD NOT =
                     LAD-CAI-IOPD IN LAD
                             OR
                     LAD-IIC-IOPD IN CLV-FLD NOT =
                     LAD-IIC-IOPD IN LAD
                   )
                )
               )
                    OR
              (
                   FIO-STAT-OKS
                       AND
               ( FRM-COD-CREB IN CLV-FLD = 'AUT'
                         AND
                  (
                     LAD-COD-AUTO IN CLV-FLD NOT =
                     LAD-COD-AUTO IN LAD
                   )
                )
               )
                    OR
              (
                   FIO-STAT-OKS
                       AND
               ( FRM-COD-CREB IN CLV-FLD = 'CLI'
                         AND
                  (
                     LAD-CIC-ICLI IN LAD NOT =
                     WSS-CIC-ICLI
                   )
                )
               )
                  IF NOT FIO-STAT-OKS
                         PERFORM EDT-FLD-CLV
                         MOVE 'CLVNOPRV' TO MSG-COD-MENS
                         MOVE 'DAP' TO MSG-COD-SIST
                         PERFORM GET-MSG
                         MOVE MSG-GLS-DESC TO FRM-MENS
                  ELSE
                  IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI
                          PERFORM SET-KEY-LAD
                          MOVE FIO-GET-NXT TO FIO-CMND
                          PERFORM DAP-FIO-LAD
                          PERFORM EDT-FLD-CLV
                          MOVE 'CLVNOPRV' TO MSG-COD-MENS
                          MOVE 'DAP' TO MSG-COD-SIST
                          PERFORM GET-MSG
                          MOVE MSG-GLS-DESC TO FRM-MENS.
       FIN-PRO-PF7.
           EXIT.
      
       PRO-PF8 SECTION.
       INI-PRO-PF8.
           PERFORM SET-KEY-LAD.
           MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM DAP-FIO-LAD.
           MOVE FIO-STAT TO PGM-STAT-LAD.
      
           IF NOT FIO-STAT-OKS
                     MOVE '23'  TO FIO-STAT
                     MOVE FIO-STAT TO PGM-STAT-LAD
           ELSE
           IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI
                     MOVE '23'  TO FIO-STAT
                     MOVE FIO-STAT TO PGM-STAT-LAD.
           IF NOT FIO-STAT-OKS
                    OR
              (
                   FIO-STAT-OKS
                       AND
               ( FRM-COD-CREB IN CLV-FLD = 'OPE'
                         AND
                  (
                     LAD-CAI-IOPD IN CLV-FLD NOT =
                     LAD-CAI-IOPD IN LAD
                             OR
                     LAD-IIC-IOPD IN CLV-FLD NOT =
                     LAD-IIC-IOPD IN LAD
                   )
                )
               )
                    OR
              (
                   FIO-STAT-OKS
                       AND
               ( FRM-COD-CREB IN CLV-FLD = 'AUT'
                         AND
                  (
                     LAD-COD-AUTO IN CLV-FLD NOT =
                     LAD-COD-AUTO IN LAD
                   )
                )
               )
                    OR
              (
                   FIO-STAT-OKS
                       AND
               ( FRM-COD-CREB IN CLV-FLD = 'CLI'
                         AND
                  (
                     LAD-CIC-ICLI IN LAD NOT =
                     WSS-CIC-ICLI
                   )
                )
               )
                  IF NOT FIO-STAT-OKS
                     PERFORM EDT-FLD-CLV
                     MOVE 'CLVNONXT' TO MSG-COD-MENS
                     MOVE 'DAP' TO MSG-COD-SIST
                     PERFORM GET-MSG
                     MOVE MSG-GLS-DESC TO FRM-MENS
                  ELSE
                  IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI
                      PERFORM SET-KEY-LAD
                      MOVE FIO-GET-PRV TO FIO-CMND
                      PERFORM DAP-FIO-LAD
                      PERFORM EDT-FLD-CLV
                      MOVE 'CLVNONXT' TO MSG-COD-MENS
                      MOVE 'DAP' TO MSG-COD-SIST
                      PERFORM GET-MSG
                      MOVE MSG-GLS-DESC TO FRM-MENS.
       FIN-PRO-PF8.
           EXIT.
      *>>>> EOF
