      *
      * DAPBUDCD
      * Version 3.0.1 Nov-2005
      * 08-May-19 12:16 PM
      *
       SET-TAG-DAPBUDCD SECTION.
       INI-SET-TAG-DAPBUDCD.
           MOVE
           '<IDNMOD NOM=$DAPBUDCD$ VSN=$07.11.22$ VGM=$3.0.1$
      -    'GEN=$08-May-19 12:16 PM$ FNC=$DAPNVA$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-DAPBUDCD.
           EXIT.
           
       RET-ALL-DCD SECTION.
       INI-RET-ALL-DCD.
           PERFORM KEY-ALL-DCD.
           PERFORM RET-OPD-DCD.
           PERFORM RET-DCD-DCD.
       FIN-RET-ALL-DCD.
           EXIT.
       
       RET-OPD-DCD SECTION.
       INI-RET-OPD-DCD.
           MOVE OPD-COD-VCAM IN DCD-FLD TO OPD-COD-VCAM IN OPD.
           MOVE OPD-COD-TOPD IN DCD-FLD TO OPD-COD-TOPD IN OPD.
           MOVE OPD-COD-OFIC IN DCD-FLD TO OPD-COD-OFIC IN OPD.
           MOVE OPD-COD-AOPD IN DCD-FLD TO OPD-COD-AOPD IN OPD.
       FIN-RET-OPD-DCD.
           EXIT.
       
       RET-DCD-DCD SECTION.
       INI-RET-DCD-DCD.
           MOVE DCD-COD-A57B IN DCD-FLD TO DCD-COD-A57B IN DCD.
           MOVE DCD-COD-DOEF IN DCD-FLD TO DCD-COD-DOEF IN DCD.
       FIN-RET-DCD-DCD.
           EXIT.
       
       KEY-ALL-DCD SECTION.
       INI-KEY-ALL-DCD.
           PERFORM KEY-OPD-DCD.
           PERFORM KEY-DCD-DCD.
       FIN-KEY-ALL-DCD.
           EXIT.
       
       KEY-OPD-DCD SECTION.
       INI-KEY-OPD-DCD.
           MOVE OPD-CAI-ISER IN DCD-FLD TO OPD-CAI-ISER IN OPD.
           MOVE OPD-IIC-ISER IN DCD-FLD TO OPD-IIC-ISER IN OPD.
       FIN-KEY-OPD-DCD.
           EXIT.
       
       KEY-DCD-DCD SECTION.
       INI-KEY-DCD-DCD.
           MOVE DCD-CAI-IOPD IN DCD-FLD TO DCD-CAI-IOPD IN DCD.
           MOVE DCD-IIC-IOPD IN DCD-FLD TO DCD-IIC-IOPD IN DCD.
           MOVE DCD-KEY-NOV1 IN DCD-FLD TO DCD-KEY-NOV1 IN DCD.
           MOVE DCD-KEY-NOV2 IN DCD-FLD TO DCD-KEY-NOV2 IN DCD.
       FIN-KEY-DCD-DCD.
           EXIT.
       
       SCR-CLRA-DCD SECTION.
       INI-SCR-CLRA-DCD.
           PERFORM CLR-KEY-DCD.
           PERFORM CLR-FLD-DCD.
       FIN-SCR-CLRA-DCD.
           EXIT.
       
       CLR-KEY-DCD SECTION.
       INI-CLR-KEY-DCD.
           MOVE SPACES TO DCD-CAI-IOPD IN DCD-FLD.
           MOVE ZEROES TO DCD-IIC-IOPD IN DCD-FLD.
           MOVE SPACES TO OPD-CAI-ISER IN DCD-FLD.
           MOVE ZEROES TO OPD-IIC-ISER IN DCD-FLD.
           MOVE SPACES TO DCD-KEY-NOV1 IN DCD-FLD.
           MOVE SPACES TO DCD-KEY-NOV2 IN DCD-FLD.
       FIN-CLR-KEY-DCD.
           EXIT.
       
       CLR-FLD-DCD SECTION.
       INI-CLR-FLD-DCD.
           MOVE SPACES TO FRM-GLS-VCAM IN DCD-FLD.
           MOVE SPACES TO FRM-GLS-TOPD IN DCD-FLD.
           MOVE SPACES TO FRM-GLS-OFIC IN DCD-FLD.
           MOVE SPACES TO FRM-GLS-A57B IN DCD-FLD.
           MOVE SPACES TO FRM-GLS-DOEF IN DCD-FLD.
           MOVE SPACES TO FRM-GLS-AOPD IN DCD-FLD.
           PERFORM CLR-OPD-DCD.
           PERFORM CLR-DCD-DCD.
       FIN-CLR-FLD-DCD.
           EXIT.
       
       CLR-OPD-DCD SECTION.
       INI-CLR-OPD-DCD.
           MOVE SPACES TO OPD-COD-VCAM IN DCD-FLD.
           MOVE SPACES TO OPD-COD-TOPD IN DCD-FLD.
           MOVE SPACES TO OPD-COD-OFIC IN DCD-FLD.
           MOVE SPACES TO OPD-COD-AOPD IN DCD-FLD.
       FIN-CLR-OPD-DCD.
           EXIT.
       
       CLR-DCD-DCD SECTION.
       INI-CLR-DCD-DCD.
           MOVE SPACES TO DCD-COD-A57B IN DCD-FLD.
           MOVE SPACES TO DCD-COD-DOEF IN DCD-FLD.
       FIN-CLR-DCD-DCD.
           EXIT.
       
       FST-KEY-DCD SECTION.
       INI-FST-KEY-DCD.
           MOVE -1 TO DCD-CAI-IOPD-LEN IN DCD-FLD.
       FIN-FST-KEY-DCD.
           EXIT.
       
       FST-FLD-DCD SECTION.
       INI-FST-FLD-DCD.
      *<<<< INI_FST_FLD_DCD
           IF SCR-CMND = 'A57' OR
              SCR-CCPP = 'ING'
               MOVE FRM-POS-CURS TO 
                    DCD-COD-A57B-LEN IN DCD-FLD
               GO TO FIN-FST-FLD-DCD.
           IF SCR-CMND = 'DDE'
               PERFORM FST-MSG    
               GO TO FIN-FST-FLD-DCD.
      *>>>> INI_FST_KEY_OPD 
           MOVE -1 TO DCD-COD-A57B-LEN IN DCD-FLD.
       FIN-FST-FLD-DCD.
           EXIT.
       
       PRO-KEY-DCD SECTION.
       INI-PRO-KEY-DCD.
           PERFORM PRO-MKY-DCD.
       FIN-PRO-KEY-DCD.
           EXIT.
       
       PRO-MKY-DCD SECTION.
       INI-PRO-MKY-DCD.
       FIN-PRO-MKY-DCD.
           EXIT.
       
       PRO-FLD-DCD SECTION.
       INI-PRO-FLD-DCD.
           PERFORM PRO-OPD-DCD.
           PERFORM PRO-DCD-DCD.
       FIN-PRO-FLD-DCD.
           EXIT.
       
       PRO-OPD-DCD SECTION.
       INI-PRO-OPD-DCD.
       FIN-PRO-OPD-DCD.
           EXIT.
       
       PRO-DCD-DCD SECTION.
       INI-PRO-DCD-DCD.
           MOVE FRM-CPIM-AEY TO DCD-COD-A57B-ATR IN DCD-FLD.
           MOVE FRM-CPIM-AEY TO DCD-COD-DOEF-ATR IN DCD-FLD.
       FIN-PRO-DCD-DCD.
           EXIT.
       
       UNP-KEY-DCD SECTION.
       INI-UNP-KEY-DCD.
           PERFORM UNP-IKY-DCD.
           PERFORM UNP-AKY-DCD.
           PERFORM UNP-MKY-DCD.
       FIN-UNP-KEY-DCD.
           EXIT.
       
       UNP-IKY-DCD SECTION.
       INI-UNP-IKY-DCD.
           MOVE FRM-CPIM-UBY TO DCD-CAI-IOPD-ATR.
           MOVE FRM-CPIM-UBY TO DCD-IIC-IOPD-ATR.
           MOVE FRM-CPIM-UBY TO OPD-CAI-ISER-ATR.
           MOVE FRM-CPIM-UBY TO OPD-IIC-ISER-ATR.
           MOVE FRM-CPIM-UBY TO DCD-KEY-NOV1-ATR.
           MOVE FRM-CPIM-UBY TO DCD-KEY-NOV2-ATR.
       FIN-UNP-IKY-DCD.
           EXIT.
       
       UNP-MKY-DCD SECTION.
       INI-UNP-MKY-DCD.
       FIN-UNP-MKY-DCD.
           EXIT.
       
       UNP-AKY-DCD SECTION.
       INI-UNP-AKY-DCD.
       FIN-UNP-AKY-DCD.
           EXIT.
       
       UNP-FLD-DCD SECTION.
       INI-UNP-FLD-DCD.
           PERFORM UNP-OPD-DCD.
           PERFORM UNP-DCD-DCD.
       FIN-UNP-FLD-DCD.
           EXIT.
       
       UNP-OPD-DCD SECTION.
       INI-UNP-OPD-DCD.
       FIN-UNP-OPD-DCD.
           EXIT.
       
       UNP-DCD-DCD SECTION.
       INI-UNP-DCD-DCD.
           MOVE FRM-CPIM-UBY TO DCD-COD-A57B-ATR IN DCD-FLD.
           MOVE FRM-CPIM-UBY TO DCD-COD-DOEF-ATR IN DCD-FLD.
       FIN-UNP-DCD-DCD.
           EXIT.
       
       VAL-MKY-DCD SECTION.
       INI-VAL-MKY-DCD.
       FIN-VAL-MKY-DCD.
           EXIT.
       
       VAL-FLD-DCD SECTION.
       INI-VAL-FLD-DCD.
           PERFORM VAL-OPD-COD-VCAM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPD-COD-VCAM-LEN IN DCD-FLD
                GO TO FIN-VAL-FLD-DCD.
           PERFORM VAL-OPD-COD-TOPD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPD-COD-TOPD-LEN IN DCD-FLD
                GO TO FIN-VAL-FLD-DCD.
           PERFORM VAL-FRM-GLS-VCAM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-VCAM-LEN IN DCD-FLD
                GO TO FIN-VAL-FLD-DCD.
           PERFORM VAL-FRM-GLS-TOPD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TOPD-LEN IN DCD-FLD
                GO TO FIN-VAL-FLD-DCD.
           PERFORM VAL-OPD-COD-OFIC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPD-COD-OFIC-LEN IN DCD-FLD
                GO TO FIN-VAL-FLD-DCD.
           PERFORM VAL-FRM-GLS-OFIC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-OFIC-LEN IN DCD-FLD
                GO TO FIN-VAL-FLD-DCD.
           PERFORM VAL-DCD-COD-A57B.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCD-COD-A57B-LEN IN DCD-FLD
                GO TO FIN-VAL-FLD-DCD.
           PERFORM VAL-FRM-GLS-A57B.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-A57B-LEN IN DCD-FLD
                GO TO FIN-VAL-FLD-DCD.
           PERFORM VAL-DCD-COD-DOEF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCD-COD-DOEF-LEN IN DCD-FLD
                GO TO FIN-VAL-FLD-DCD.
           PERFORM VAL-FRM-GLS-DOEF.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-DOEF-LEN IN DCD-FLD
                GO TO FIN-VAL-FLD-DCD.
           PERFORM VAL-OPD-COD-AOPD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO OPD-COD-AOPD-LEN IN DCD-FLD
                GO TO FIN-VAL-FLD-DCD.
           PERFORM VAL-FRM-GLS-AOPD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-AOPD-LEN IN DCD-FLD
                GO TO FIN-VAL-FLD-DCD.
       FIN-VAL-FLD-DCD.
           EXIT.
       
       VAL-OPD-COD-VCAM SECTION.
       INI-VAL-OPD-COD-VCAM.
       FIN-VAL-OPD-COD-VCAM.
           EXIT.
       
       VAL-OPD-COD-TOPD SECTION.
       INI-VAL-OPD-COD-TOPD.
       FIN-VAL-OPD-COD-TOPD.
           EXIT.
       
       VAL-FRM-GLS-VCAM SECTION.
       INI-VAL-FRM-GLS-VCAM.
       FIN-VAL-FRM-GLS-VCAM.
           EXIT.
       
       VAL-FRM-GLS-TOPD SECTION.
       INI-VAL-FRM-GLS-TOPD.
       FIN-VAL-FRM-GLS-TOPD.
           EXIT.
       
       VAL-OPD-COD-OFIC SECTION.
       INI-VAL-OPD-COD-OFIC.
       FIN-VAL-OPD-COD-OFIC.
           EXIT.
       
       VAL-FRM-GLS-OFIC SECTION.
       INI-VAL-FRM-GLS-OFIC.
       FIN-VAL-FRM-GLS-OFIC.
           EXIT.
       
       VAL-DCD-COD-A57B SECTION.
       INI-VAL-DCD-COD-A57B.
           IF DCD-COD-A57B IN DCD-FLD NOT > SPACES
              MOVE SPACES TO DCD-COD-A57B IN DCD-FLD.
      *VAL-COD Valida codigo de tabla
           MOVE 'DAP' TO TAB-COD-SIST.
           MOVE 'A57 ' TO TAB-COD-TTAB IN TAB.
           MOVE DCD-COD-A57B IN DCD-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              DCD-COD-A57B IN DCD-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-A57B IN DCD-FLD
           ELSE
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-A57B IN DCD-FLD.
       FIN-VAL-DCD-COD-A57B.
           EXIT.
       
       VAL-FRM-GLS-A57B SECTION.
       INI-VAL-FRM-GLS-A57B.
       FIN-VAL-FRM-GLS-A57B.
           EXIT.
       
       VAL-DCD-COD-DOEF SECTION.
       INI-VAL-DCD-COD-DOEF.
           IF DCD-COD-DOEF IN DCD-FLD NOT > SPACES
              MOVE SPACES TO DCD-COD-DOEF IN DCD-FLD.
      *VAL-COD Valida codigo de tabla
           MOVE 'DAP' TO TAB-COD-SIST.
           MOVE 'DOE ' TO TAB-COD-TTAB IN TAB.
           MOVE DCD-COD-DOEF IN DCD-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
           IF MSG-COD-MENS = 'COD    NEX' OR
              DCD-COD-DOEF IN DCD-FLD NOT > SPACES
               MOVE SPACES TO FRM-GLS-DOEF IN DCD-FLD
           ELSE
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-DOEF IN DCD-FLD.
       FIN-VAL-DCD-COD-DOEF.
           EXIT.
       
       VAL-FRM-GLS-DOEF SECTION.
       INI-VAL-FRM-GLS-DOEF.
       FIN-VAL-FRM-GLS-DOEF.
           EXIT.
       
       VAL-OPD-COD-AOPD SECTION.
       INI-VAL-OPD-COD-AOPD.
       FIN-VAL-OPD-COD-AOPD.
           EXIT.
       
       VAL-FRM-GLS-AOPD SECTION.
       INI-VAL-FRM-GLS-AOPD.
       FIN-VAL-FRM-GLS-AOPD.
           EXIT.
       
       VAL-CON-FLD-DCD SECTION.
       INI-VAL-CON-FLD-DCD.
       FIN-VAL-CON-FLD-DCD.
           EXIT.
       
       VAL-CON-DCD SECTION.
       INI-VAL-CON-DCD.
       FIN-VAL-CON-DCD.
           EXIT.
       
       VAL-NUM-KEY-DCD SECTION.
       INI-VAL-NUM-KEY-DCD.
           PERFORM VAL-NUM-IKY-DCD.
           PERFORM VAL-NUM-MKY-DCD.
           PERFORM VAL-NUM-AKY-DCD.
       FIN-VAL-NUM-KEY-DCD.
           EXIT.
       
       VAL-NUM-IKY-DCD SECTION.
       INI-VAL-NUM-IKY-DCD.
       VAL-NUM-DCD-IIC-IOPD.
           MOVE DCD-IIC-IOPD-ALF IN DCD-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO DCD-IIC-IOPD IN DCD-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO DCD-IIC-IOPD-LEN IN DCD-FLD
                GO TO FIN-VAL-NUM-IKY-DCD.
       VAL-NUM-OPD-IIC-ISER.
           MOVE OPD-IIC-ISER-ALF IN DCD-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO OPD-IIC-ISER IN DCD-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO OPD-IIC-ISER-LEN IN DCD-FLD
                GO TO FIN-VAL-NUM-IKY-DCD.
       FIN-VAL-NUM-IKY-DCD.
           EXIT.
       
       VAL-NUM-MKY-DCD SECTION.
       INI-VAL-NUM-MKY-DCD.
       FIN-VAL-NUM-MKY-DCD.
           EXIT.
       
       VAL-NUM-AKY-DCD SECTION.
       INI-VAL-NUM-AKY-DCD.
       FIN-VAL-NUM-AKY-DCD.
           EXIT.
       
       VAL-NUM-FLD-DCD SECTION.
       INI-VAL-NUM-FLD-DCD.
       FIN-VAL-NUM-FLD-DCD.
           EXIT.
       
       EDT-KEY-DCD SECTION.
       INI-EDT-KEY-DCD.
           PERFORM EDT-IKY-DCD.
           PERFORM EDT-MKY-DCD.
           PERFORM EDT-AKY-DCD.
       FIN-EDT-KEY-DCD.
           EXIT.
       
       EDT-IKY-DCD SECTION.
       INI-EDT-IKY-DCD.
           IF DCD-IIC-IOPD-LEN IN DCD-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DCD-IIC-IOPD-LEN IN DCD-FLD
                GO TO FIN-EDT-IKY-DCD.
           MOVE DCD-IIC-IOPD IN DCD-FLD TO DCD-IIC-IOPD-EDT IN DCD-EDT.
           MOVE DCD-IIC-IOPD-EDT IN DCD-EDT TO 
                DCD-IIC-IOPD-ALF IN DCD-FLD.
           IF OPD-IIC-ISER-LEN IN DCD-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO OPD-IIC-ISER-LEN IN DCD-FLD
                GO TO FIN-EDT-IKY-DCD.
           MOVE OPD-IIC-ISER IN DCD-FLD TO OPD-IIC-ISER-EDT IN DCD-EDT.
           MOVE OPD-IIC-ISER-EDT IN DCD-EDT TO 
                OPD-IIC-ISER-ALF IN DCD-FLD.
       FIN-EDT-IKY-DCD.
           EXIT.
       
       EDT-MKY-DCD SECTION.
       INI-EDT-MKY-DCD.
       FIN-EDT-MKY-DCD.
           EXIT.
       
       EDT-AKY-DCD SECTION.
       INI-EDT-AKY-DCD.
       FIN-EDT-AKY-DCD.
           EXIT.
       
       EDT-FLD-DCD SECTION.
       INI-EDT-FLD-DCD.
       FIN-EDT-FLD-DCD.
           EXIT.
