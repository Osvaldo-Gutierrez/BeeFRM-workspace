      *
      * DAPBQCCO
      * Version 3.0.1 Nov-2005
      * 27-Aug-09 04:50 PM
      *
       SET-TAG-DAPBQCCO SECTION.
       INI-SET-TAG-DAPBQCCO.
           MOVE
           '<IDNMOD NOM=$DAPBQCCO$ VSN=$08.10.20$ VGM=$3.0.1$
      -    'GEN=$27-Aug-09 04:50 PM$ FNC=$080905$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-DAPBQCCO.
           EXIT.
           
       PUT-ALL-CCO SECTION.
       INI-PUT-ALL-CCO.
           PERFORM PUT-CCO-CCO.
       FIN-PUT-ALL-CCO.
           EXIT.
       
       PUT-CCO-CCO SECTION.
       INI-PUT-CCO-CCO.
           IF PGM-STAT-CCO-NUL
               GO TO FIN-PUT-CCO-CCO.
           IF NOT PGM-STAT-CCO-OKS
               PERFORM CLR-CCO-CCO
               GO TO FIN-PUT-CCO-CCO.
           MOVE CCO-NUM-OFIC IN CCO TO CCO-NUM-OFIC IN CCO-FLD.
           MOVE CCO-NUM-NRUT IN CCO TO CCO-NUM-NRUT IN CCO-FLD.
           MOVE CCO-COD-NRUT IN CCO TO CCO-COD-NRUT IN CCO-FLD.
           MOVE CCO-GLS-NMBR IN CCO TO CCO-GLS-NMBR IN CCO-FLD.
           MOVE CCO-GLS-DIRE IN CCO TO CCO-GLS-DIRE IN CCO-FLD.
           MOVE CCO-NUM-CDAD IN CCO TO CCO-NUM-CDAD IN CCO-FLD.
           MOVE CCO-GLS-CDAD IN CCO TO CCO-GLS-CDAD IN CCO-FLD.
           MOVE CCO-IND-EMIS IN CCO TO CCO-IND-EMIS IN CCO-FLD.
           MOVE CCO-NUM-FOLI IN CCO TO CCO-NUM-FOLI IN CCO-FLD.
           MOVE CCO-GLS-DISP IN CCO TO CCO-GLS-DISP IN CCO-FLD.
      *<<<< FIN_PUT_CCO_CCO
      *>>>> FIN_PUT_CCO_CCO
       FIN-PUT-CCO-CCO.
           EXIT.
       
       KEY-ALL-CCO SECTION.
       INI-KEY-ALL-CCO.
           PERFORM KEY-CCO-CCO.
       FIN-KEY-ALL-CCO.
           EXIT.
       
       KEY-CCO-CCO SECTION.
       INI-KEY-CCO-CCO.
           MOVE CCO-NUM-OFIC IN CCO-FLD TO CCO-NUM-OFIC IN CCO.
           MOVE CCO-NUM-NRUT IN CCO-FLD TO CCO-NUM-NRUT IN CCO.
           MOVE CCO-COD-NRUT IN CCO-FLD TO CCO-COD-NRUT IN CCO.
       FIN-KEY-CCO-CCO.
           EXIT.
       
       SCR-CLRA-CCO SECTION.
       INI-SCR-CLRA-CCO.
           PERFORM CLR-KEY-CCO.
           PERFORM CLR-FLD-CCO.
       FIN-SCR-CLRA-CCO.
           EXIT.
       
       CLR-KEY-CCO SECTION.
       INI-CLR-KEY-CCO.
           MOVE SPACES TO FRM-COD-VOID IN CCO-FLD.
           MOVE ZEROES TO CCO-NUM-OFIC IN CCO-FLD.
           MOVE SPACES TO FRM-IND-NEXT IN CCO-FLD.
           MOVE ZEROES TO CCO-NUM-NRUT IN CCO-FLD.
           MOVE SPACES TO CCO-COD-NRUT IN CCO-FLD.
       FIN-CLR-KEY-CCO.
           EXIT.
       
       CLR-FLD-CCO SECTION.
       INI-CLR-FLD-CCO.
           PERFORM CLR-CCO-CCO.
       FIN-CLR-FLD-CCO.
           EXIT.
       
       CLR-CCO-CCO SECTION.
       INI-CLR-CCO-CCO.
           MOVE SPACES TO CCO-GLS-NMBR IN CCO-FLD.
           MOVE SPACES TO CCO-GLS-DIRE IN CCO-FLD.
           MOVE ZEROES TO CCO-NUM-CDAD IN CCO-FLD.
           MOVE SPACES TO CCO-GLS-CDAD IN CCO-FLD.
           MOVE ZEROES TO CCO-IND-EMIS IN CCO-FLD.
           MOVE ZEROES TO CCO-NUM-FOLI IN CCO-FLD.
           MOVE SPACES TO CCO-GLS-DISP IN CCO-FLD.
       FIN-CLR-CCO-CCO.
           EXIT.
       
       FST-KEY-CCO SECTION.
       INI-FST-KEY-CCO.
           MOVE -1 TO FRM-COD-VOID-LEN IN CCO-FLD.
       FIN-FST-KEY-CCO.
           EXIT.
       
       FST-FLD-CCO SECTION.
       INI-FST-FLD-CCO.
           MOVE -1 TO CCO-GLS-NMBR-LEN IN CCO-FLD.
       FIN-FST-FLD-CCO.
           EXIT.
       
       PRO-KEY-CCO SECTION.
       INI-PRO-KEY-CCO.
           MOVE FRM-CPIM-AEY TO FRM-COD-VOID-ATR IN CCO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-IND-NEXT-ATR IN CCO-FLD.
           PERFORM PRO-IKY-CCO.
           PERFORM PRO-AKY-CCO.
           PERFORM PRO-MKY-CCO.
       FIN-PRO-KEY-CCO.
           EXIT.
       
       PRO-IKY-CCO SECTION.
       INI-PRO-IKY-CCO.
           MOVE FRM-CPIM-AEY TO CCO-NUM-OFIC-ATR IN CCO-FLD.
           MOVE FRM-CPIM-AEY TO CCO-NUM-NRUT-ATR IN CCO-FLD.
           MOVE FRM-CPIM-AEY TO CCO-COD-NRUT-ATR IN CCO-FLD.
       FIN-PRO-IKY-CCO.
           EXIT.
       
       PRO-MKY-CCO SECTION.
       INI-PRO-MKY-CCO.
       FIN-PRO-MKY-CCO.
           EXIT.
       
       PRO-AKY-CCO SECTION.
       INI-PRO-AKY-CCO.
       FIN-PRO-AKY-CCO.
           EXIT.
       
       UNP-KEY-CCO SECTION.
       INI-UNP-KEY-CCO.
           MOVE FRM-CPIM-UBY TO FRM-COD-VOID-ATR IN CCO-FLD.
           MOVE FRM-CPIM-UBY TO FRM-IND-NEXT-ATR IN CCO-FLD.
           PERFORM UNP-IKY-CCO.
           PERFORM UNP-AKY-CCO.
           PERFORM UNP-MKY-CCO.
       FIN-UNP-KEY-CCO.
           EXIT.
       
       UNP-IKY-CCO SECTION.
       INI-UNP-IKY-CCO.
           MOVE FRM-CPIM-UBY TO FRM-COD-VOID-ATR.
           MOVE FRM-CPIM-UBY TO CCO-NUM-OFIC-ATR.
           MOVE FRM-CPIM-UBY TO FRM-IND-NEXT-ATR.
           MOVE FRM-CPIM-UBY TO CCO-NUM-NRUT-ATR.
           MOVE FRM-CPIM-UBY TO CCO-COD-NRUT-ATR.
       FIN-UNP-IKY-CCO.
           EXIT.
       
       UNP-MKY-CCO SECTION.
       INI-UNP-MKY-CCO.
       FIN-UNP-MKY-CCO.
           EXIT.
       
       UNP-AKY-CCO SECTION.
       INI-UNP-AKY-CCO.
       FIN-UNP-AKY-CCO.
           EXIT.
       
       UNP-FLD-CCO SECTION.
       INI-UNP-FLD-CCO.
           PERFORM UNP-CCO-CCO.
       FIN-UNP-FLD-CCO.
           EXIT.
       
       UNP-CCO-CCO SECTION.
       INI-UNP-CCO-CCO.
           MOVE FRM-CPIM-UBY TO CCO-GLS-NMBR-ATR IN CCO-FLD.
           MOVE FRM-CPIM-UBY TO CCO-GLS-DIRE-ATR IN CCO-FLD.
           MOVE FRM-CPIM-UBY TO CCO-NUM-CDAD-ATR IN CCO-FLD.
           MOVE FRM-CPIM-UBY TO CCO-GLS-CDAD-ATR IN CCO-FLD.
           MOVE FRM-CPIM-UBY TO CCO-IND-EMIS-ATR IN CCO-FLD.
           MOVE FRM-CPIM-UBY TO CCO-NUM-FOLI-ATR IN CCO-FLD.
           MOVE FRM-CPIM-UBY TO CCO-GLS-DISP-ATR IN CCO-FLD.
       FIN-UNP-CCO-CCO.
           EXIT.
       
       VAL-KEY-CCO SECTION.
       INI-VAL-KEY-CCO.
           PERFORM VAL-IKY-CCO.
           PERFORM VAL-MKY-CCO.
           PERFORM VAL-AKY-CCO.
       FIN-VAL-KEY-CCO.
           EXIT.
       
       VAL-IKY-CCO SECTION.
       INI-VAL-IKY-CCO.
           PERFORM VAL-FRM-COD-VOID.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-COD-VOID-LEN IN CCO-FLD
                GO TO FIN-VAL-IKY-CCO.
           PERFORM VAL-CCO-NUM-OFIC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CCO-NUM-OFIC-LEN IN CCO-FLD
                GO TO FIN-VAL-IKY-CCO.
           PERFORM VAL-FRM-IND-NEXT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-IND-NEXT-LEN IN CCO-FLD
                GO TO FIN-VAL-IKY-CCO.
           PERFORM VAL-CCO-NUM-NRUT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CCO-NUM-NRUT-LEN IN CCO-FLD
                GO TO FIN-VAL-IKY-CCO.
           PERFORM VAL-CCO-COD-NRUT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CCO-COD-NRUT-LEN IN CCO-FLD
                GO TO FIN-VAL-IKY-CCO.
       FIN-VAL-IKY-CCO.
           EXIT.
       
       VAL-MKY-CCO SECTION.
       INI-VAL-MKY-CCO.
       FIN-VAL-MKY-CCO.
           EXIT.
       
       VAL-AKY-CCO SECTION.
       INI-VAL-AKY-CCO.
       FIN-VAL-AKY-CCO.
           EXIT.
       
       VAL-FRM-COD-VOID SECTION.
       INI-VAL-FRM-COD-VOID.
           IF FRM-COD-VOID IN CCO-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-FRM-COD-VOID.
       FIN-VAL-FRM-COD-VOID.
           EXIT.
       
       VAL-CCO-NUM-OFIC SECTION.
       INI-VAL-CCO-NUM-OFIC.
       FIN-VAL-CCO-NUM-OFIC.
           EXIT.
       
       VAL-FRM-IND-NEXT SECTION.
       INI-VAL-FRM-IND-NEXT.
           IF FRM-IND-NEXT IN CCO-FLD NOT > SPACES
              MOVE SPACES TO FRM-IND-NEXT IN CCO-FLD.
       FIN-VAL-FRM-IND-NEXT.
           EXIT.
       
       VAL-CCO-NUM-NRUT SECTION.
       INI-VAL-CCO-NUM-NRUT.
       FIN-VAL-CCO-NUM-NRUT.
           EXIT.
       
       VAL-CCO-COD-NRUT SECTION.
       INI-VAL-CCO-COD-NRUT.
           IF CCO-COD-NRUT IN CCO-FLD NOT > SPACES
              MOVE SPACES TO CCO-COD-NRUT IN CCO-FLD.
       FIN-VAL-CCO-COD-NRUT.
           EXIT.
       
       VAL-CON-KEY-CCO SECTION.
       INI-VAL-CON-KEY-CCO.
           IF     FRM-COD-VOID IN CCO-FLD NOT > SPACES 
              AND CCO-NUM-OFIC IN CCO-FLD NOT > ZEROES 
              AND FRM-IND-NEXT IN CCO-FLD NOT > SPACES 
              AND CCO-NUM-NRUT IN CCO-FLD NOT > ZEROES 
              AND CCO-COD-NRUT IN CCO-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CCO.
       FIN-VAL-CON-KEY-CCO.
           EXIT.
       
       VAL-CON-IKY-CCO SECTION.
       INI-VAL-CON-IKY-CCO.
           IF     FRM-COD-VOID IN CCO-FLD NOT > SPACES 
              AND CCO-NUM-OFIC IN CCO-FLD NOT > ZEROES 
              AND FRM-IND-NEXT IN CCO-FLD NOT > SPACES 
              AND CCO-NUM-NRUT IN CCO-FLD NOT > ZEROES 
              AND CCO-COD-NRUT IN CCO-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CCO.
       FIN-VAL-CON-IKY-CCO.
           EXIT.
       
       VAL-NUM-KEY-CCO SECTION.
       INI-VAL-NUM-KEY-CCO.
           PERFORM VAL-NUM-IKY-CCO.
           PERFORM VAL-NUM-MKY-CCO.
           PERFORM VAL-NUM-AKY-CCO.
       FIN-VAL-NUM-KEY-CCO.
           EXIT.
       
       VAL-NUM-IKY-CCO SECTION.
       INI-VAL-NUM-IKY-CCO.
       VAL-NUM-CCO-NUM-OFIC.
           MOVE CCO-NUM-OFIC-ALF IN CCO-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CCO-NUM-OFIC IN CCO-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CCO-NUM-OFIC-LEN IN CCO-FLD
                GO TO FIN-VAL-NUM-IKY-CCO.
       VAL-NUM-CCO-NUM-NRUT.
           MOVE CCO-NUM-NRUT-ALF IN CCO-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 9 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CCO-NUM-NRUT IN CCO-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CCO-NUM-NRUT-LEN IN CCO-FLD
                GO TO FIN-VAL-NUM-IKY-CCO.
       FIN-VAL-NUM-IKY-CCO.
           EXIT.
       
       VAL-NUM-MKY-CCO SECTION.
       INI-VAL-NUM-MKY-CCO.
       FIN-VAL-NUM-MKY-CCO.
           EXIT.
       
       VAL-NUM-AKY-CCO SECTION.
       INI-VAL-NUM-AKY-CCO.
       FIN-VAL-NUM-AKY-CCO.
           EXIT.
       
       VAL-NUM-FLD-CCO SECTION.
       INI-VAL-NUM-FLD-CCO.
       VAL-NUM-CCO-NUM-CDAD.
           MOVE CCO-NUM-CDAD-ALF IN CCO-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 4 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CCO-NUM-CDAD IN CCO-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CCO-NUM-CDAD-LEN IN CCO-FLD
                GO TO FIN-VAL-NUM-FLD-CCO.
       VAL-NUM-CCO-IND-EMIS.
           MOVE CCO-IND-EMIS-ALF IN CCO-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 1 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CCO-IND-EMIS IN CCO-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CCO-IND-EMIS-LEN IN CCO-FLD
                GO TO FIN-VAL-NUM-FLD-CCO.
       VAL-NUM-CCO-NUM-FOLI.
           MOVE CCO-NUM-FOLI-ALF IN CCO-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 6 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CCO-NUM-FOLI IN CCO-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CCO-NUM-FOLI-LEN IN CCO-FLD
                GO TO FIN-VAL-NUM-FLD-CCO.
       FIN-VAL-NUM-FLD-CCO.
           EXIT.
       
       EDT-KEY-CCO SECTION.
       INI-EDT-KEY-CCO.
           PERFORM EDT-IKY-CCO.
           PERFORM EDT-MKY-CCO.
           PERFORM EDT-AKY-CCO.
       FIN-EDT-KEY-CCO.
           EXIT.
       
       EDT-IKY-CCO SECTION.
       INI-EDT-IKY-CCO.
           IF CCO-NUM-OFIC-LEN IN CCO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CCO-NUM-OFIC-LEN IN CCO-FLD
                GO TO FIN-EDT-IKY-CCO.
           MOVE CCO-NUM-OFIC IN CCO-FLD TO CCO-NUM-OFIC-EDT IN CCO-EDT.
           MOVE CCO-NUM-OFIC-EDT IN CCO-EDT TO 
                CCO-NUM-OFIC-ALF IN CCO-FLD.
           IF CCO-NUM-NRUT-LEN IN CCO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CCO-NUM-NRUT-LEN IN CCO-FLD
                GO TO FIN-EDT-IKY-CCO.
           MOVE CCO-NUM-NRUT IN CCO-FLD TO CCO-NUM-NRUT-EDT IN CCO-EDT.
           MOVE CCO-NUM-NRUT-EDT IN CCO-EDT TO 
                CCO-NUM-NRUT-ALF IN CCO-FLD.
       FIN-EDT-IKY-CCO.
           EXIT.
       
       EDT-MKY-CCO SECTION.
       INI-EDT-MKY-CCO.
       FIN-EDT-MKY-CCO.
           EXIT.
       
       EDT-AKY-CCO SECTION.
       INI-EDT-AKY-CCO.
       FIN-EDT-AKY-CCO.
           EXIT.
       
       EDT-FLD-CCO SECTION.
       INI-EDT-FLD-CCO.
           IF CCO-NUM-CDAD-LEN IN CCO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CCO-NUM-CDAD-LEN IN CCO-FLD
                GO TO FIN-EDT-FLD-CCO.
           MOVE CCO-NUM-CDAD IN CCO-FLD TO CCO-NUM-CDAD-EDT IN CCO-EDT.
           MOVE CCO-NUM-CDAD-EDT IN CCO-EDT TO 
                CCO-NUM-CDAD-ALF IN CCO-FLD.
           IF CCO-IND-EMIS-LEN IN CCO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CCO-IND-EMIS-LEN IN CCO-FLD
                GO TO FIN-EDT-FLD-CCO.
           MOVE CCO-IND-EMIS IN CCO-FLD TO CCO-IND-EMIS-EDT IN CCO-EDT.
           MOVE CCO-IND-EMIS-EDT IN CCO-EDT TO 
                CCO-IND-EMIS-ALF IN CCO-FLD.
           IF CCO-NUM-FOLI-LEN IN CCO-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CCO-NUM-FOLI-LEN IN CCO-FLD
                GO TO FIN-EDT-FLD-CCO.
           MOVE CCO-NUM-FOLI IN CCO-FLD TO CCO-NUM-FOLI-EDT IN CCO-EDT.
           MOVE CCO-NUM-FOLI-EDT IN CCO-EDT TO 
                CCO-NUM-FOLI-ALF IN CCO-FLD.
       FIN-EDT-FLD-CCO.
           EXIT.
