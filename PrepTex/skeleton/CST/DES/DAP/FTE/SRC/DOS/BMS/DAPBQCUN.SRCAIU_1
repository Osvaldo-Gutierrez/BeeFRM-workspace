      *
      * DAPBQCUN
      * Version 3.0.1 Nov-2005
      * 01-Sep-09 06:39 PM
      *
       SET-TAG-DAPBQCUN SECTION.
       INI-SET-TAG-DAPBQCUN.
           MOVE
           '<IDNMOD NOM=$DAPBQCUN$ VSN=$06.08.30$ VGM=$3.0.1$
      -    'GEN=$01-Sep-09 06:39 PM$ FNC=$DAPNVA$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-DAPBQCUN.
           EXIT.
           
       PUT-ALL-CUN SECTION.
       INI-PUT-ALL-CUN.
           PERFORM PUT-CUN-CUN.
       FIN-PUT-ALL-CUN.
           EXIT.
       
       PUT-CUN-CUN SECTION.
       INI-PUT-CUN-CUN.
           IF PGM-STAT-CUN-NUL
               GO TO FIN-PUT-CUN-CUN.
           IF NOT PGM-STAT-CUN-OKS
               PERFORM CLR-CUN-CUN
               GO TO FIN-PUT-CUN-CUN.
           MOVE CUN-COD-OFIC IN CUN TO CUN-COD-OFIC IN CUN-FLD.
           MOVE CUN-COD-MONE IN CUN TO CUN-COD-MONE IN CUN-FLD.
           MOVE CUN-COD-PROD IN CUN TO CUN-COD-PROD IN CUN-FLD.
           MOVE CUN-COD-CORR IN CUN TO CUN-COD-CORR IN CUN-FLD.
           MOVE CUN-COD-VPRO IN CUN TO CUN-COD-VPRO IN CUN-FLD.
           MOVE CUN-NUM-NCUO IN CUN TO CUN-NUM-NCUO IN CUN-FLD.
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CUN-NUM-DING IN CUN TO CUN-NUM-DING IN CUN-FLD.
           MOVE CUN-NUM-MING IN CUN TO CUN-NUM-MING IN CUN-FLD.
           MOVE CUN-NUM-SING IN CUN TO CUN-NUM-SING IN CUN-FLD.
           MOVE CUN-NUM-AING IN CUN TO CUN-NUM-AING IN CUN-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE CUN-COD-EJEI IN CUN TO CUN-COD-EJEI IN CUN-FLD.
      * FEC-INI --> BMCHIBCI001210.SRCAIU 
           MOVE CUN-NUM-DOUT IN CUN TO CUN-NUM-DOUT IN CUN-FLD.
           MOVE CUN-NUM-MOUT IN CUN TO CUN-NUM-MOUT IN CUN-FLD.
           MOVE CUN-NUM-SOUT IN CUN TO CUN-NUM-SOUT IN CUN-FLD.
           MOVE CUN-NUM-AOUT IN CUN TO CUN-NUM-AOUT IN CUN-FLD.
      * FEC-FIN --> BMCHIBCI001210.SRCAIU 
           MOVE CUN-COD-EJEO IN CUN TO CUN-COD-EJEO IN CUN-FLD.
       FIN-PUT-CUN-CUN.
           EXIT.
       
       KEY-ALL-CUN SECTION.
       INI-KEY-ALL-CUN.
           PERFORM KEY-CUN-CUN.
       FIN-KEY-ALL-CUN.
           EXIT.
       
       KEY-CUN-CUN SECTION.
       INI-KEY-CUN-CUN.
           MOVE CUN-COD-OFIC IN CUN-FLD TO CUN-COD-OFIC IN CUN.
           MOVE CUN-COD-MONE IN CUN-FLD TO CUN-COD-MONE IN CUN.
           MOVE CUN-COD-PROD IN CUN-FLD TO CUN-COD-PROD IN CUN.
           MOVE CUN-COD-CORR IN CUN-FLD TO CUN-COD-CORR IN CUN.
           MOVE CUN-COD-VPRO IN CUN-FLD TO CUN-COD-VPRO IN CUN.
           MOVE CUN-NUM-NCUO IN CUN-FLD TO CUN-NUM-NCUO IN CUN.
       FIN-KEY-CUN-CUN.
           EXIT.
       
       SCR-CLRA-CUN SECTION.
       INI-SCR-CLRA-CUN.
           PERFORM CLR-KEY-CUN.
           PERFORM CLR-FLD-CUN.
       FIN-SCR-CLRA-CUN.
           EXIT.
       
       CLR-KEY-CUN SECTION.
       INI-CLR-KEY-CUN.
           MOVE SPACES TO CUN-COD-OFIC IN CUN-FLD.
           MOVE SPACES TO CUN-COD-MONE IN CUN-FLD.
           MOVE SPACES TO CUN-COD-PROD IN CUN-FLD.
           MOVE SPACES TO CUN-COD-CORR IN CUN-FLD.
           MOVE SPACES TO CUN-COD-VPRO IN CUN-FLD.
           MOVE ZEROES TO CUN-NUM-NCUO IN CUN-FLD.
       FIN-CLR-KEY-CUN.
           EXIT.
       
       CLR-FLD-CUN SECTION.
       INI-CLR-FLD-CUN.
           PERFORM CLR-CUN-CUN.
       FIN-CLR-FLD-CUN.
           EXIT.
       
       CLR-CUN-CUN SECTION.
       INI-CLR-CUN-CUN.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CUN-FEC-FING IN CUN-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO CUN-COD-EJEI IN CUN-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO CUN-FEC-FOUT IN CUN-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO CUN-COD-EJEO IN CUN-FLD.
       FIN-CLR-CUN-CUN.
           EXIT.
       
       FST-KEY-CUN SECTION.
       INI-FST-KEY-CUN.
           MOVE -1 TO CUN-COD-OFIC-LEN IN CUN-FLD.
       FIN-FST-KEY-CUN.
           EXIT.
       
       FST-FLD-CUN SECTION.
       INI-FST-FLD-CUN.
      *<<<< INI_FST_FLD_CUN
      *>>>> INI_FST_KEY_OPD 
           MOVE FRM-POS-CURS TO CUN-FEC-FING-LEN IN CUN-FLD.
       FIN-FST-FLD-CUN.
           EXIT.
       
       PRO-KEY-CUN SECTION.
       INI-PRO-KEY-CUN.
           PERFORM PRO-IKY-CUN.
           PERFORM PRO-AKY-CUN.
           PERFORM PRO-MKY-CUN.
       FIN-PRO-KEY-CUN.
           EXIT.
       
       PRO-IKY-CUN SECTION.
       INI-PRO-IKY-CUN.
           MOVE FRM-CPIM-AEY TO CUN-COD-OFIC-ATR IN CUN-FLD.
           MOVE FRM-CPIM-AEY TO CUN-COD-MONE-ATR IN CUN-FLD.
           MOVE FRM-CPIM-AEY TO CUN-COD-PROD-ATR IN CUN-FLD.
           MOVE FRM-CPIM-AEY TO CUN-COD-CORR-ATR IN CUN-FLD.
           MOVE FRM-CPIM-AEY TO CUN-COD-VPRO-ATR IN CUN-FLD.
           MOVE FRM-CPIM-AEY TO CUN-NUM-NCUO-ATR IN CUN-FLD.
       FIN-PRO-IKY-CUN.
           EXIT.
       
       PRO-MKY-CUN SECTION.
       INI-PRO-MKY-CUN.
       FIN-PRO-MKY-CUN.
           EXIT.
       
       PRO-AKY-CUN SECTION.
       INI-PRO-AKY-CUN.
       FIN-PRO-AKY-CUN.
           EXIT.
       
       UNP-KEY-CUN SECTION.
       INI-UNP-KEY-CUN.
           PERFORM UNP-IKY-CUN.
           PERFORM UNP-AKY-CUN.
           PERFORM UNP-MKY-CUN.
       FIN-UNP-KEY-CUN.
           EXIT.
       
       UNP-IKY-CUN SECTION.
       INI-UNP-IKY-CUN.
           MOVE FRM-CPIM-UBY TO CUN-COD-OFIC-ATR.
           MOVE FRM-CPIM-UBY TO CUN-COD-MONE-ATR.
           MOVE FRM-CPIM-UBY TO CUN-COD-PROD-ATR.
           MOVE FRM-CPIM-UBY TO CUN-COD-CORR-ATR.
           MOVE FRM-CPIM-UBY TO CUN-COD-VPRO-ATR.
           MOVE FRM-CPIM-UBY TO CUN-NUM-NCUO-ATR.
       FIN-UNP-IKY-CUN.
           EXIT.
       
       UNP-MKY-CUN SECTION.
       INI-UNP-MKY-CUN.
       FIN-UNP-MKY-CUN.
           EXIT.
       
       UNP-AKY-CUN SECTION.
       INI-UNP-AKY-CUN.
       FIN-UNP-AKY-CUN.
           EXIT.
       
       UNP-FLD-CUN SECTION.
       INI-UNP-FLD-CUN.
           PERFORM UNP-CUN-CUN.
       FIN-UNP-FLD-CUN.
           EXIT.
       
       UNP-CUN-CUN SECTION.
       INI-UNP-CUN-CUN.
           MOVE FRM-CPIM-UBY TO CUN-FEC-FING-ATR IN CUN-FLD.
           MOVE FRM-CPIM-UBY TO CUN-COD-EJEI-ATR IN CUN-FLD.
           MOVE FRM-CPIM-UBY TO CUN-FEC-FOUT-ATR IN CUN-FLD.
           MOVE FRM-CPIM-UBY TO CUN-COD-EJEO-ATR IN CUN-FLD.
       FIN-UNP-CUN-CUN.
           EXIT.
       
       VAL-KEY-CUN SECTION.
       INI-VAL-KEY-CUN.
           PERFORM VAL-IKY-CUN.
           PERFORM VAL-MKY-CUN.
           PERFORM VAL-AKY-CUN.
       FIN-VAL-KEY-CUN.
           EXIT.
       
       VAL-IKY-CUN SECTION.
       INI-VAL-IKY-CUN.
           PERFORM VAL-CUN-COD-OFIC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CUN-COD-OFIC-LEN IN CUN-FLD
                GO TO FIN-VAL-IKY-CUN.
           PERFORM VAL-CUN-COD-MONE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CUN-COD-MONE-LEN IN CUN-FLD
                GO TO FIN-VAL-IKY-CUN.
           PERFORM VAL-CUN-COD-PROD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CUN-COD-PROD-LEN IN CUN-FLD
                GO TO FIN-VAL-IKY-CUN.
           PERFORM VAL-CUN-COD-CORR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CUN-COD-CORR-LEN IN CUN-FLD
                GO TO FIN-VAL-IKY-CUN.
           PERFORM VAL-CUN-COD-VPRO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CUN-COD-VPRO-LEN IN CUN-FLD
                GO TO FIN-VAL-IKY-CUN.
           PERFORM VAL-CUN-NUM-NCUO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CUN-NUM-NCUO-LEN IN CUN-FLD
                GO TO FIN-VAL-IKY-CUN.
       FIN-VAL-IKY-CUN.
           EXIT.
       
       VAL-MKY-CUN SECTION.
       INI-VAL-MKY-CUN.
       FIN-VAL-MKY-CUN.
           EXIT.
       
       VAL-AKY-CUN SECTION.
       INI-VAL-AKY-CUN.
       FIN-VAL-AKY-CUN.
           EXIT.
       
       VAL-CUN-COD-OFIC SECTION.
       INI-VAL-CUN-COD-OFIC.
           IF CUN-COD-OFIC IN CUN-FLD NOT > SPACES
              MOVE SPACES TO CUN-COD-OFIC IN CUN-FLD.
       FIN-VAL-CUN-COD-OFIC.
           EXIT.
       
       VAL-CUN-COD-MONE SECTION.
       INI-VAL-CUN-COD-MONE.
           IF CUN-COD-MONE IN CUN-FLD NOT > SPACES
              MOVE SPACES TO CUN-COD-MONE IN CUN-FLD.
       FIN-VAL-CUN-COD-MONE.
           EXIT.
       
       VAL-CUN-COD-PROD SECTION.
       INI-VAL-CUN-COD-PROD.
           IF CUN-COD-PROD IN CUN-FLD NOT > SPACES
              MOVE SPACES TO CUN-COD-PROD IN CUN-FLD.
       FIN-VAL-CUN-COD-PROD.
           EXIT.
       
       VAL-CUN-COD-CORR SECTION.
       INI-VAL-CUN-COD-CORR.
           IF CUN-COD-CORR IN CUN-FLD NOT > SPACES
              MOVE SPACES TO CUN-COD-CORR IN CUN-FLD.
       FIN-VAL-CUN-COD-CORR.
           EXIT.
       
       VAL-CUN-COD-VPRO SECTION.
       INI-VAL-CUN-COD-VPRO.
           IF CUN-COD-VPRO IN CUN-FLD NOT > SPACES
              MOVE SPACES TO CUN-COD-VPRO IN CUN-FLD.
       FIN-VAL-CUN-COD-VPRO.
           EXIT.
       
       VAL-CUN-NUM-NCUO SECTION.
       INI-VAL-CUN-NUM-NCUO.
       FIN-VAL-CUN-NUM-NCUO.
           EXIT.
       
       VAL-CON-KEY-CUN SECTION.
       INI-VAL-CON-KEY-CUN.
           IF     CUN-COD-OFIC IN CUN-FLD NOT > SPACES 
              AND CUN-COD-MONE IN CUN-FLD NOT > SPACES 
              AND CUN-COD-PROD IN CUN-FLD NOT > SPACES 
              AND CUN-COD-CORR IN CUN-FLD NOT > SPACES 
              AND CUN-COD-VPRO IN CUN-FLD NOT > SPACES 
              AND CUN-NUM-NCUO IN CUN-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CUN.
       FIN-VAL-CON-KEY-CUN.
           EXIT.
       
       VAL-CON-IKY-CUN SECTION.
       INI-VAL-CON-IKY-CUN.
           IF     CUN-COD-OFIC IN CUN-FLD NOT > SPACES 
              AND CUN-COD-MONE IN CUN-FLD NOT > SPACES 
              AND CUN-COD-PROD IN CUN-FLD NOT > SPACES 
              AND CUN-COD-CORR IN CUN-FLD NOT > SPACES 
              AND CUN-COD-VPRO IN CUN-FLD NOT > SPACES 
              AND CUN-NUM-NCUO IN CUN-FLD NOT > ZEROES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-CUN.
       FIN-VAL-CON-IKY-CUN.
           EXIT.
       
       VAL-NUM-KEY-CUN SECTION.
       INI-VAL-NUM-KEY-CUN.
           PERFORM VAL-NUM-IKY-CUN.
           PERFORM VAL-NUM-MKY-CUN.
           PERFORM VAL-NUM-AKY-CUN.
       FIN-VAL-NUM-KEY-CUN.
           EXIT.
       
       VAL-NUM-IKY-CUN SECTION.
       INI-VAL-NUM-IKY-CUN.
       VAL-NUM-CUN-NUM-NCUO.
           MOVE CUN-NUM-NCUO-ALF IN CUN-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 2 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CUN-NUM-NCUO IN CUN-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CUN-NUM-NCUO-LEN IN CUN-FLD
                GO TO FIN-VAL-NUM-IKY-CUN.
       FIN-VAL-NUM-IKY-CUN.
           EXIT.
       
       VAL-NUM-MKY-CUN SECTION.
       INI-VAL-NUM-MKY-CUN.
       FIN-VAL-NUM-MKY-CUN.
           EXIT.
       
       VAL-NUM-AKY-CUN SECTION.
       INI-VAL-NUM-AKY-CUN.
       FIN-VAL-NUM-AKY-CUN.
           EXIT.
       
       VAL-NUM-FLD-CUN SECTION.
       INI-VAL-NUM-FLD-CUN.
       FIN-VAL-NUM-FLD-CUN.
           EXIT.
       
       EDT-KEY-CUN SECTION.
       INI-EDT-KEY-CUN.
           PERFORM EDT-IKY-CUN.
           PERFORM EDT-MKY-CUN.
           PERFORM EDT-AKY-CUN.
       FIN-EDT-KEY-CUN.
           EXIT.
       
       EDT-IKY-CUN SECTION.
       INI-EDT-IKY-CUN.
           IF CUN-NUM-NCUO-LEN IN CUN-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CUN-NUM-NCUO-LEN IN CUN-FLD
                GO TO FIN-EDT-IKY-CUN.
           MOVE CUN-NUM-NCUO IN CUN-FLD TO CUN-NUM-NCUO-EDT IN CUN-EDT.
           MOVE CUN-NUM-NCUO-EDT IN CUN-EDT TO 
                CUN-NUM-NCUO-ALF IN CUN-FLD.
       FIN-EDT-IKY-CUN.
           EXIT.
       
       EDT-MKY-CUN SECTION.
       INI-EDT-MKY-CUN.
       FIN-EDT-MKY-CUN.
           EXIT.
       
       EDT-AKY-CUN SECTION.
       INI-EDT-AKY-CUN.
       FIN-EDT-AKY-CUN.
           EXIT.
       
       EDT-FLD-CUN SECTION.
       INI-EDT-FLD-CUN.
       FIN-EDT-FLD-CUN.
           EXIT.
