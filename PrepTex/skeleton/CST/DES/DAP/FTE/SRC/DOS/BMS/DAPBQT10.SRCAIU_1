      * 27-Dec-96 02:18 PM
       PUT-ALL-T10 SECTION.
       INI-PUT-ALL-T10.
           PERFORM PUT-T10-T10.
       FIN-PUT-ALL-T10.
           EXIT.
       
       PUT-T10-T10 SECTION.
       INI-PUT-T10-T10.
           IF PGM-STAT-T10-NUL
               GO TO FIN-PUT-T10-T10.
           IF NOT PGM-STAT-T10-OKS
               PERFORM CLR-T10-T10
               GO TO FIN-PUT-T10-T10.
           MOVE T10-COD-CMON IN T10 TO T10-COD-CMON IN T10-FLD.
           MOVE T10-COD-REGI IN T10 TO T10-COD-REGI IN T10-FLD.
           MOVE T10-CIC-VCAM IN T10 TO T10-CIC-VCAM IN T10-FLD.
           MOVE T10-VAL-MONT IN T10 TO T10-VAL-MONT IN T10-FLD.
           MOVE T10-VAL-NUME IN T10 TO T10-VAL-NUME IN T10-FLD.
           MOVE T10-GLS-DISP IN T10 TO T10-GLS-DISP IN T10-FLD.
       FIN-PUT-T10-T10.
           EXIT.
       
       KEY-ALL-T10 SECTION.
       INI-KEY-ALL-T10.
           PERFORM KEY-T10-T10.
       FIN-KEY-ALL-T10.
           EXIT.
       
       KEY-T10-T10 SECTION.
       INI-KEY-T10-T10.
           MOVE T10-COD-CMON IN T10-FLD TO T10-COD-CMON IN T10.
           MOVE T10-COD-REGI IN T10-FLD TO T10-COD-REGI IN T10.
       FIN-KEY-T10-T10.
           EXIT.
       
       SCR-CLRA-T10 SECTION.
       INI-SCR-CLRA-T10.
           PERFORM CLR-KEY-T10.
           PERFORM CLR-FLD-T10.
       FIN-SCR-CLRA-T10.
           EXIT.
       
       CLR-KEY-T10 SECTION.
       INI-CLR-KEY-T10.
           MOVE SPACES TO T10-COD-CMON IN T10-FLD.
           MOVE SPACES TO T10-COD-REGI IN T10-FLD.
       FIN-CLR-KEY-T10.
           EXIT.
       
       CLR-FLD-T10 SECTION.
       INI-CLR-FLD-T10.
           PERFORM CLR-T10-T10.
       FIN-CLR-FLD-T10.
           EXIT.
       
       CLR-T10-T10 SECTION.
       INI-CLR-T10-T10.
           MOVE SPACES TO T10-CIC-VCAM IN T10-FLD.
           MOVE ZEROES TO T10-VAL-MONT IN T10-FLD.
           MOVE ZEROES TO T10-VAL-NUME IN T10-FLD.
           MOVE SPACES TO T10-GLS-DISP IN T10-FLD.
       FIN-CLR-T10-T10.
           EXIT.
       
       FST-KEY-T10 SECTION.
       INI-FST-KEY-T10.
           MOVE -1 TO T10-COD-CMON-LEN IN T10-FLD.
       FIN-FST-KEY-T10.
           EXIT.
       
       FST-FLD-T10 SECTION.
       INI-FST-FLD-T10.
           MOVE -1 TO T10-CIC-VCAM-LEN IN T10-FLD.
       FIN-FST-FLD-T10.
           EXIT.
       
       PRO-KEY-T10 SECTION.
       INI-PRO-KEY-T10.
           PERFORM PRO-IKY-T10.
           PERFORM PRO-AKY-T10.
           PERFORM PRO-MKY-T10.
       FIN-PRO-KEY-T10.
           EXIT.
       
       PRO-IKY-T10 SECTION.
       INI-PRO-IKY-T10.
           MOVE FRM-CPIM-AEY TO T10-COD-CMON-ATR IN T10-FLD.
           MOVE FRM-CPIM-AEY TO T10-COD-REGI-ATR IN T10-FLD.
       FIN-PRO-IKY-T10.
           EXIT.
       
       PRO-MKY-T10 SECTION.
       INI-PRO-MKY-T10.
       FIN-PRO-MKY-T10.
           EXIT.
       
       PRO-AKY-T10 SECTION.
       INI-PRO-AKY-T10.
       FIN-PRO-AKY-T10.
           EXIT.
       
       UNP-KEY-T10 SECTION.
       INI-UNP-KEY-T10.
           PERFORM UNP-IKY-T10.
           PERFORM UNP-AKY-T10.
           PERFORM UNP-MKY-T10.
       FIN-UNP-KEY-T10.
           EXIT.
       
       UNP-IKY-T10 SECTION.
       INI-UNP-IKY-T10.
           MOVE FRM-CPIM-UBY TO T10-COD-CMON-ATR.
           MOVE FRM-CPIM-UBY TO T10-COD-REGI-ATR.
       FIN-UNP-IKY-T10.
           EXIT.
       
       UNP-MKY-T10 SECTION.
       INI-UNP-MKY-T10.
       FIN-UNP-MKY-T10.
           EXIT.
       
       UNP-AKY-T10 SECTION.
       INI-UNP-AKY-T10.
       FIN-UNP-AKY-T10.
           EXIT.
       
       UNP-FLD-T10 SECTION.
       INI-UNP-FLD-T10.
           PERFORM UNP-T10-T10.
       FIN-UNP-FLD-T10.
           EXIT.
       
       UNP-T10-T10 SECTION.
       INI-UNP-T10-T10.
           MOVE FRM-CPIM-UBY TO T10-CIC-VCAM-ATR IN T10-FLD.
           MOVE FRM-CPIM-UBY TO T10-VAL-MONT-ATR IN T10-FLD.
           MOVE FRM-CPIM-UBY TO T10-VAL-NUME-ATR IN T10-FLD.
           MOVE FRM-CPIM-UBY TO T10-GLS-DISP-ATR IN T10-FLD.
       FIN-UNP-T10-T10.
           EXIT.
       
       VAL-KEY-T10 SECTION.
       INI-VAL-KEY-T10.
           PERFORM VAL-IKY-T10.
           PERFORM VAL-MKY-T10.
           PERFORM VAL-AKY-T10.
       FIN-VAL-KEY-T10.
           EXIT.
       
       VAL-IKY-T10 SECTION.
       INI-VAL-IKY-T10.
           PERFORM VAL-T10-COD-CMON.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO T10-COD-CMON-LEN IN T10-FLD
                GO TO FIN-VAL-IKY-T10.
           PERFORM VAL-T10-COD-REGI.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE -1 TO T10-COD-REGI-LEN IN T10-FLD
                GO TO FIN-VAL-IKY-T10.
       FIN-VAL-IKY-T10.
           EXIT.
       
       VAL-MKY-T10 SECTION.
       INI-VAL-MKY-T10.
       FIN-VAL-MKY-T10.
           EXIT.
       
       VAL-AKY-T10 SECTION.
       INI-VAL-AKY-T10.
       FIN-VAL-AKY-T10.
           EXIT.
       
       VAL-T10-COD-CMON SECTION.
       INI-VAL-T10-COD-CMON.
           IF T10-COD-CMON IN T10-FLD NOT > SPACES
              MOVE SPACES TO T10-COD-CMON IN T10-FLD.
       FIN-VAL-T10-COD-CMON.
           EXIT.
       
       VAL-T10-COD-REGI SECTION.
       INI-VAL-T10-COD-REGI.
           IF T10-COD-REGI IN T10-FLD NOT > SPACES
              MOVE SPACES TO T10-COD-REGI IN T10-FLD.
       FIN-VAL-T10-COD-REGI.
           EXIT.
       
       VAL-CON-KEY-T10 SECTION.
       INI-VAL-CON-KEY-T10.
           IF     T10-COD-CMON IN T10-FLD NOT > SPACES 
              AND T10-COD-REGI IN T10-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-T10.
       FIN-VAL-CON-KEY-T10.
           EXIT.
       
       VAL-CON-IKY-T10 SECTION.
       INI-VAL-CON-IKY-T10.
           IF     T10-COD-CMON IN T10-FLD NOT > SPACES 
              AND T10-COD-REGI IN T10-FLD NOT > SPACES 
                PERFORM ERR-KEY
                PERFORM FST-KEY-T10.
       FIN-VAL-CON-IKY-T10.
           EXIT.
       
       VAL-NUM-KEY-T10 SECTION.
       INI-VAL-NUM-KEY-T10.
           PERFORM VAL-NUM-IKY-T10.
           PERFORM VAL-NUM-MKY-T10.
           PERFORM VAL-NUM-AKY-T10.
       FIN-VAL-NUM-KEY-T10.
           EXIT.
       
       VAL-NUM-IKY-T10 SECTION.
       INI-VAL-NUM-IKY-T10.
       FIN-VAL-NUM-IKY-T10.
           EXIT.
       
       VAL-NUM-MKY-T10 SECTION.
       INI-VAL-NUM-MKY-T10.
       FIN-VAL-NUM-MKY-T10.
           EXIT.
       
       VAL-NUM-AKY-T10 SECTION.
       INI-VAL-NUM-AKY-T10.
       FIN-VAL-NUM-AKY-T10.
           EXIT.
       
       VAL-NUM-FLD-T10 SECTION.
       INI-VAL-NUM-FLD-T10.
       VAL-NUM-T10-VAL-MONT.
           MOVE T10-VAL-MONT-ALF IN T10-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 12 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO T10-VAL-MONT IN T10-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO T10-VAL-MONT-LEN IN T10-FLD
                GO TO FIN-VAL-NUM-FLD-T10.
       VAL-NUM-T10-VAL-NUME.
           MOVE T10-VAL-NUME-ALF IN T10-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 7 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO T10-VAL-NUME IN T10-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO T10-VAL-NUME-LEN IN T10-FLD
                GO TO FIN-VAL-NUM-FLD-T10.
       FIN-VAL-NUM-FLD-T10.
           EXIT.
       
       EDT-KEY-T10 SECTION.
       INI-EDT-KEY-T10.
           PERFORM EDT-IKY-T10.
           PERFORM EDT-MKY-T10.
           PERFORM EDT-AKY-T10.
       FIN-EDT-KEY-T10.
           EXIT.
       
       EDT-IKY-T10 SECTION.
       INI-EDT-IKY-T10.
       FIN-EDT-IKY-T10.
           EXIT.
       
       EDT-MKY-T10 SECTION.
       INI-EDT-MKY-T10.
       FIN-EDT-MKY-T10.
           EXIT.
       
       EDT-AKY-T10 SECTION.
       INI-EDT-AKY-T10.
       FIN-EDT-AKY-T10.
           EXIT.
       
       EDT-FLD-T10 SECTION.
       INI-EDT-FLD-T10.
           IF T10-VAL-MONT-LEN IN T10-FLD = -9
                MOVE -1 TO T10-VAL-MONT-LEN IN T10-FLD
                GO TO FIN-EDT-FLD-T10.
           MOVE T10-VAL-MONT IN T10-FLD TO T10-VAL-MONT-EDT IN T10-EDT.
           MOVE T10-VAL-MONT-EDT IN T10-EDT TO 
                T10-VAL-MONT-ALF IN T10-FLD.
           IF T10-VAL-NUME-LEN IN T10-FLD = -9
                MOVE -1 TO T10-VAL-NUME-LEN IN T10-FLD
                GO TO FIN-EDT-FLD-T10.
           MOVE T10-VAL-NUME IN T10-FLD TO T10-VAL-NUME-EDT IN T10-EDT.
           MOVE T10-VAL-NUME-EDT IN T10-EDT TO 
                T10-VAL-NUME-ALF IN T10-FLD.
       FIN-EDT-FLD-T10.
           EXIT.
