//JVALDES  JOB ,,CLASS=A,MSGCLASS=X,USER=JVALDES,PASSWORD=JVLDIC,
//            REGION=4M
/*ROUTE PRINT MVS
//STEP1  EXEC PGM=AFOLIBR
//STEPLIB  DD DSN=ADR.LIB.V38.LOAD,DISP=SHR
//MASTER   DD DSN=DAPCL.FUENTES,DISP=SHR
//OSJOB    DD DUMMY
//SYSPRINT DD SYSOUT=X
//SYSIN    DD *
-ADD DAPPCIPA,PSWD=CONS,ARC
     DUMMY
-EMOD
-SEL DAPPCIPA,CONS
-REP ALL
       IDENTIFICATION DIVISION.
      *=======================
       PROGRAM-ID.      DAPPCIPA.
       AUTHOR.          CONSIST.
       DATE-WRITTEN.    10-Jul-91 12:53 PM.

      * Programa que Limpia archivo VSAM.

       ENVIRONMENT DIVISION.
      *====================
       CONFIGURATION SECTION.
      *---------------------
       SPECIAL-NAMES.
            DECIMAL-POINT IS COMMA.
       INPUT-OUTPUT SECTION.
      *--------------------
       FILE-CONTROL.

       DATA DIVISION.
      *=============
       FILE SECTION.
      *------------

       WORKING-STORAGE SECTION.
      *-----------------------
       COPY DAPBRIPA.

       COPY GNSWGRQA.
       COPY GNSWGUIB.
       COPY GNSWGELS.
       COPY GNSWGIFD.
       COPY GNSWCFIO.
       COPY GNSWVFIO.
      * COPY GNSWVIDD.
       COPY GNSBRTAB.
       COPY GNSBRMSG.

       COPY GNSWGSYS.
      *<<<< WSS
      *Entidades de GNS

       01  WSS-VARI.
           03  WSS-LEI-IPA            VALUE ZEROES   PIC 9(05).
           03  WSS-ELI-IPA            VALUE ZEROES   PIC 9(05).
      *>>>>
      *<<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-TAB-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-MSG-REQA==.
      *>>>>

       REPORT SECTION.
      *--------------

       PROCEDURE DIVISION.
      *==================
       DECLARATIVES.
       END DECLARATIVES.

       MAIN SECTION.
      *------------
       INI-MAIN.
       COPY GNSBGEDB.
           MOVE 'DAPPCIPA' TO FIO-PROG.
           PERFORM BUS-FSIS.
       INI-INP-SRT.
      *<<<< INI_INP
           MOVE FIO-UPD TO FIO-CMND.
           PERFORM DAP-FIO-IPA.
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR FIO-UPD IPA, FIO-STAT : ' FIO-STAT
               GO TO FIN-MAIN.

       LUP-MAIN.
           IF NOT FIO-STAT-OKS
                GO TO FIN-MAIN.
           PERFORM PRO-IPA.
       FIN-MAIN.

           DISPLAY 'TOTALES DE CONTROL DAPPCIPA '.
           DISPLAY '--------------------------- '.
           DISPLAY '     '.
           DISPLAY 'TOTAL REGISTROS LEIDOS IPA   : ' WSS-LEI-IPA.
           DISPLAY 'TOTAL REGISTROS ELIMIN IPA   : ' WSS-ELI-IPA.

           MOVE FIO-CLO TO FIO-CMND.
           PERFORM DAP-FIO-IPA.

           PERFORM GNS-PRO-END.

       COPY DAPBFIPA.
       COPY GNSBFTAB.
       COPY GNSBFMSG.
       COPY GNSBGDTC.
       COPY GNSBGIFD.
       COPY GNSBTABT.
       COPY GNSBBSYS.
       COPY GNSBGEND.

      *<<<< EOF
       PRO-IPA SECTION.
       INI-PRO-IPA.
           MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM DAP-FIO-IPA.
           IF NOT FIO-STAT-OKS
               DISPLAY 'ERROR AL ACCESAR REGISTRO IPA '
                       FIO-STAT
               GO TO FIN-PRO-IPA.

       LUP-PRO-IPA.
           ADD 1 TO WSS-LEI-IPA.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM DAP-FIO-IPA.
           IF NOT FIO-STAT-OKS
              DISPLAY 'IPA INEXISTENTE ' IPA 
              GO TO FIN-PRO-IPA.

           MOVE FIO-DEL TO FIO-CMND.
           PERFORM DAP-FIO-IPA.
           IF NOT FIO-STAT-OKS
              DISPLAY 'ERROR AL ELIMINAR IPA ' FIO-STAT
              GO TO FIN-PRO-IPA.
 
           ADD 1 TO WSS-ELI-IPA IN WSS-VARI.

       NXT-PRO-IPA.
           MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM DAP-FIO-IPA.
           IF NOT FIO-STAT-OKS
               GO TO FIN-PRO-IPA
           ELSE
               GO TO LUP-PRO-IPA.
       FIN-PRO-IPA.
           EXIT.
      *>>>>
-END
