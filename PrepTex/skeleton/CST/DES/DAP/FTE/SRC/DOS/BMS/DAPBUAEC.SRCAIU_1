       SET-TAG-DAPBUAEC SECTION.
       INI-SET-TAG-DAPBUAEC.
           MOVE
           '<IDNMOD NOM=$DAPBUAEC$ VSN=$08.09.03$ VGM=$3.0.1$
      -    'GEN=$04-Sep-08 12:29 PM$ FNC=$080311$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-DAPBUAEC.
           EXIT.
           
       RET-ALL-AEC SECTION.
       INI-RET-ALL-AEC.
           PERFORM KEY-ALL-AEC.
           PERFORM RET-CAD-AEC.
       FIN-RET-ALL-AEC.
           EXIT.
       
       RET-CAD-AEC SECTION.
       INI-RET-CAD-AEC.
           MOVE CAD-GLS-FLAG IN AEC-FLD TO CAD-GLS-FLAG IN CAD.
           MOVE CAD-STP-ITRN IN AEC-FLD TO CAD-STP-ITRN IN CAD.
           MOVE CAD-COD-TACC IN AEC-FLD TO CAD-COD-TACC IN CAD.
           MOVE CAD-COD-STAT IN AEC-FLD TO CAD-COD-STAT IN CAD.
           MOVE CAD-COD-OTRN IN AEC-FLD TO CAD-COD-OTRN IN CAD.
           MOVE CAD-COD-ATRN IN AEC-FLD TO CAD-COD-ATRN IN CAD.
           MOVE CAD-IND-TCAD IN AEC-FLD TO CAD-IND-TCAD IN CAD.
           MOVE CAD-CAI-ICAD IN AEC-FLD TO CAD-CAI-ICAD IN CAD.
           MOVE CAD-IIC-ICAD IN AEC-FLD TO CAD-IIC-ICAD IN CAD.
           MOVE CAD-SGV-LIQU IN AEC-FLD TO CAD-SGV-LIQU IN CAD.
           MOVE CAD-IND-MAUT IN AEC-FLD TO CAD-IND-MAUT IN CAD.
           MOVE CAD-COD-OFIC IN AEC-FLD TO CAD-COD-OFIC IN CAD.
           MOVE CAD-GLS-DISP IN AEC-FLD TO CAD-GLS-DISP IN CAD.
       FIN-RET-CAD-AEC.
           EXIT.
       
       KEY-ALL-AEC SECTION.
       INI-KEY-ALL-AEC.
           PERFORM KEY-CAD-AEC.
       FIN-KEY-ALL-AEC.
           EXIT.
       
       KEY-CAD-AEC SECTION.
       INI-KEY-CAD-AEC.
           MOVE CAD-CAI-IOPD IN AEC-FLD TO CAD-CAI-IOPD IN CAD.
           MOVE CAD-IIC-IOPD IN AEC-FLD TO CAD-IIC-IOPD IN CAD.
           MOVE CAD-NUM-ILIQ IN AEC-FLD TO CAD-NUM-ILIQ IN CAD.
           MOVE CAD-NUM-ICAD IN AEC-FLD TO CAD-NUM-ICAD IN CAD.
       FIN-KEY-CAD-AEC.
           EXIT.
       
       SCR-CLRA-AEC SECTION.
       INI-SCR-CLRA-AEC.
           PERFORM CLR-KEY-AEC.
           PERFORM CLR-FLD-AEC.
       FIN-SCR-CLRA-AEC.
           EXIT.
       
       CLR-KEY-AEC SECTION.
       INI-CLR-KEY-AEC.
           MOVE SPACES TO CAD-CAI-IOPD IN AEC-FLD.
           MOVE ZEROES TO CAD-IIC-IOPD IN AEC-FLD.
           MOVE SPACES TO CAD-NUM-ILIQ IN AEC-FLD.
           MOVE ZEROES TO CAD-NUM-ICAD IN AEC-FLD.
       FIN-CLR-KEY-AEC.
           EXIT.
       
       CLR-FLD-AEC SECTION.
       INI-CLR-FLD-AEC.
           MOVE SPACES TO FRM-GLS-GAEC IN AEC-FLD.
           PERFORM CLR-CAD-AEC.
       FIN-CLR-FLD-AEC.
           EXIT.
       
       CLR-CAD-AEC SECTION.
       INI-CLR-CAD-AEC.
           MOVE SPACES TO CAD-GLS-FLAG IN AEC-FLD.
           MOVE SPACES TO CAD-STP-ITRN IN AEC-FLD.
           MOVE SPACES TO CAD-COD-TACC IN AEC-FLD.
           MOVE SPACES TO CAD-COD-STAT IN AEC-FLD.
           MOVE SPACES TO CAD-COD-OTRN IN AEC-FLD.
           MOVE SPACES TO CAD-COD-ATRN IN AEC-FLD.
           MOVE SPACES TO CAD-IND-TCAD IN AEC-FLD.
           MOVE SPACES TO CAD-CAI-ICAD IN AEC-FLD.
           MOVE ZEROES TO CAD-IIC-ICAD IN AEC-FLD.
           MOVE ZEROES TO CAD-SGV-LIQU IN AEC-FLD.
           MOVE SPACES TO CAD-IND-MAUT IN AEC-FLD.
           MOVE SPACES TO CAD-COD-OFIC IN AEC-FLD.
           MOVE SPACES TO CAD-GLS-DISP IN AEC-FLD.
       FIN-CLR-CAD-AEC.
           EXIT.
       
       FST-KEY-AEC SECTION.
       INI-FST-KEY-AEC.
           MOVE -1 TO CAD-CAI-IOPD-LEN IN AEC-FLD.
       FIN-FST-KEY-AEC.
           EXIT.
       
       FST-FLD-AEC SECTION.
       INI-FST-FLD-AEC.
           MOVE -1 TO CAD-GLS-FLAG-LEN IN AEC-FLD.
       FIN-FST-FLD-AEC.
           EXIT.
       
       PRO-KEY-AEC SECTION.
       INI-PRO-KEY-AEC.
           PERFORM PRO-MKY-AEC.
       FIN-PRO-KEY-AEC.
           EXIT.
       
       PRO-MKY-AEC SECTION.
       INI-PRO-MKY-AEC.
       FIN-PRO-MKY-AEC.
           EXIT.
       
       PRO-FLD-AEC SECTION.
       INI-PRO-FLD-AEC.
           PERFORM PRO-CAD-AEC.
       FIN-PRO-FLD-AEC.
           EXIT.
       
       PRO-CAD-AEC SECTION.
       INI-PRO-CAD-AEC.
           MOVE FRM-CPIM-AEY TO CAD-GLS-FLAG-ATR IN AEC-FLD.
           MOVE FRM-CPIM-AEY TO CAD-STP-ITRN-ATR IN AEC-FLD.
           MOVE FRM-CPIM-AEY TO CAD-COD-TACC-ATR IN AEC-FLD.
           MOVE FRM-CPIM-AEY TO CAD-COD-STAT-ATR IN AEC-FLD.
           MOVE FRM-CPIM-AEY TO CAD-COD-OTRN-ATR IN AEC-FLD.
           MOVE FRM-CPIM-AEY TO CAD-COD-ATRN-ATR IN AEC-FLD.
           MOVE FRM-CPIM-AEY TO CAD-IND-TCAD-ATR IN AEC-FLD.
           MOVE FRM-CPIM-AEY TO CAD-CAI-ICAD-ATR IN AEC-FLD.
           MOVE FRM-CPIM-AEY TO CAD-IIC-ICAD-ATR IN AEC-FLD.
           MOVE FRM-CPIM-AEY TO CAD-SGV-LIQU-ATR IN AEC-FLD.
           MOVE FRM-CPIM-AEY TO CAD-IND-MAUT-ATR IN AEC-FLD.
           MOVE FRM-CPIM-AEY TO CAD-COD-OFIC-ATR IN AEC-FLD.
           MOVE FRM-CPIM-AEY TO CAD-GLS-DISP-ATR IN AEC-FLD.
       FIN-PRO-CAD-AEC.
           EXIT.
       
       UNP-KEY-AEC SECTION.
       INI-UNP-KEY-AEC.
           PERFORM UNP-IKY-AEC.
           PERFORM UNP-AKY-AEC.
           PERFORM UNP-MKY-AEC.
       FIN-UNP-KEY-AEC.
           EXIT.
       
       UNP-IKY-AEC SECTION.
       INI-UNP-IKY-AEC.
           MOVE FRM-CPIM-UBY TO CAD-CAI-IOPD-ATR.
           MOVE FRM-CPIM-UBY TO CAD-IIC-IOPD-ATR.
           MOVE FRM-CPIM-UNY TO CAD-NUM-ILIQ-ATR.
           MOVE FRM-CPIM-UBY TO CAD-NUM-ICAD-ATR.
       FIN-UNP-IKY-AEC.
           EXIT.
       
       UNP-MKY-AEC SECTION.
       INI-UNP-MKY-AEC.
       FIN-UNP-MKY-AEC.
           EXIT.
       
       UNP-AKY-AEC SECTION.
       INI-UNP-AKY-AEC.
       FIN-UNP-AKY-AEC.
           EXIT.
       
       UNP-FLD-AEC SECTION.
       INI-UNP-FLD-AEC.
           PERFORM UNP-CAD-AEC.
       FIN-UNP-FLD-AEC.
           EXIT.
       
       UNP-CAD-AEC SECTION.
       INI-UNP-CAD-AEC.
           MOVE FRM-CPIM-UBY TO CAD-GLS-FLAG-ATR IN AEC-FLD.
           MOVE FRM-CPIM-UBY TO CAD-STP-ITRN-ATR IN AEC-FLD.
           MOVE FRM-CPIM-UBY TO CAD-COD-TACC-ATR IN AEC-FLD.
           MOVE FRM-CPIM-UBY TO CAD-COD-STAT-ATR IN AEC-FLD.
           MOVE FRM-CPIM-UBY TO CAD-COD-OTRN-ATR IN AEC-FLD.
           MOVE FRM-CPIM-UBY TO CAD-COD-ATRN-ATR IN AEC-FLD.
           MOVE FRM-CPIM-UBY TO CAD-IND-TCAD-ATR IN AEC-FLD.
           MOVE FRM-CPIM-UBY TO CAD-CAI-ICAD-ATR IN AEC-FLD.
           MOVE FRM-CPIM-UBY TO CAD-IIC-ICAD-ATR IN AEC-FLD.
           MOVE FRM-CPIM-UBY TO CAD-SGV-LIQU-ATR IN AEC-FLD.
           MOVE FRM-CPIM-UBY TO CAD-IND-MAUT-ATR IN AEC-FLD.
           MOVE FRM-CPIM-UBY TO CAD-COD-OFIC-ATR IN AEC-FLD.
           MOVE FRM-CPIM-UBY TO CAD-GLS-DISP-ATR IN AEC-FLD.
       FIN-UNP-CAD-AEC.
           EXIT.
       
       VAL-MKY-AEC SECTION.
       INI-VAL-MKY-AEC.
       FIN-VAL-MKY-AEC.
           EXIT.
       
       VAL-FLD-AEC SECTION.
       INI-VAL-FLD-AEC.
           PERFORM VAL-FRM-GLS-GAEC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-GAEC-LEN IN AEC-FLD
                GO TO FIN-VAL-FLD-AEC.
           PERFORM VAL-CAD-GLS-FLAG.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-GLS-FLAG-LEN IN AEC-FLD
                GO TO FIN-VAL-FLD-AEC.
           PERFORM VAL-CAD-STP-ITRN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-STP-ITRN-LEN IN AEC-FLD
                GO TO FIN-VAL-FLD-AEC.
           PERFORM VAL-CAD-COD-TACC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-COD-TACC-LEN IN AEC-FLD
                GO TO FIN-VAL-FLD-AEC.
           PERFORM VAL-CAD-COD-STAT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-COD-STAT-LEN IN AEC-FLD
                GO TO FIN-VAL-FLD-AEC.
           PERFORM VAL-CAD-COD-OTRN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-COD-OTRN-LEN IN AEC-FLD
                GO TO FIN-VAL-FLD-AEC.
           PERFORM VAL-CAD-COD-ATRN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-COD-ATRN-LEN IN AEC-FLD
                GO TO FIN-VAL-FLD-AEC.
           PERFORM VAL-CAD-IND-TCAD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-IND-TCAD-LEN IN AEC-FLD
                GO TO FIN-VAL-FLD-AEC.
           PERFORM VAL-CAD-CAI-ICAD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-CAI-ICAD-LEN IN AEC-FLD
                GO TO FIN-VAL-FLD-AEC.
           PERFORM VAL-CAD-IIC-ICAD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-IIC-ICAD-LEN IN AEC-FLD
                GO TO FIN-VAL-FLD-AEC.
           PERFORM VAL-CAD-SGV-LIQU.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-SGV-LIQU-LEN IN AEC-FLD
                GO TO FIN-VAL-FLD-AEC.
           PERFORM VAL-CAD-IND-MAUT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-IND-MAUT-LEN IN AEC-FLD
                GO TO FIN-VAL-FLD-AEC.
           PERFORM VAL-CAD-COD-OFIC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-COD-OFIC-LEN IN AEC-FLD
                GO TO FIN-VAL-FLD-AEC.
           PERFORM VAL-CAD-GLS-DISP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO CAD-GLS-DISP-LEN IN AEC-FLD
                GO TO FIN-VAL-FLD-AEC.
       FIN-VAL-FLD-AEC.
           EXIT.
       
       VAL-FRM-GLS-GAEC SECTION.
       INI-VAL-FRM-GLS-GAEC.
       FIN-VAL-FRM-GLS-GAEC.
           EXIT.
       
       VAL-CAD-GLS-FLAG SECTION.
       INI-VAL-CAD-GLS-FLAG.
           IF CAD-GLS-FLAG IN AEC-FLD NOT > SPACES
              MOVE SPACES TO CAD-GLS-FLAG IN AEC-FLD.
       FIN-VAL-CAD-GLS-FLAG.
           EXIT.
       
       VAL-CAD-STP-ITRN SECTION.
       INI-VAL-CAD-STP-ITRN.
           IF CAD-STP-ITRN IN AEC-FLD NOT > SPACES
              MOVE SPACES TO CAD-STP-ITRN IN AEC-FLD.
       FIN-VAL-CAD-STP-ITRN.
           EXIT.
       
       VAL-CAD-COD-TACC SECTION.
       INI-VAL-CAD-COD-TACC.
           IF CAD-COD-TACC IN AEC-FLD NOT > SPACES
              MOVE SPACES TO CAD-COD-TACC IN AEC-FLD.
       FIN-VAL-CAD-COD-TACC.
           EXIT.
       
       VAL-CAD-COD-STAT SECTION.
       INI-VAL-CAD-COD-STAT.
           IF CAD-COD-STAT IN AEC-FLD NOT > SPACES
              MOVE SPACES TO CAD-COD-STAT IN AEC-FLD.
       FIN-VAL-CAD-COD-STAT.
           EXIT.
       
       VAL-CAD-COD-OTRN SECTION.
       INI-VAL-CAD-COD-OTRN.
           IF CAD-COD-OTRN IN AEC-FLD NOT > SPACES
              MOVE SPACES TO CAD-COD-OTRN IN AEC-FLD.
       FIN-VAL-CAD-COD-OTRN.
           EXIT.
       
       VAL-CAD-COD-ATRN SECTION.
       INI-VAL-CAD-COD-ATRN.
           IF CAD-COD-ATRN IN AEC-FLD NOT > SPACES
              MOVE SPACES TO CAD-COD-ATRN IN AEC-FLD.
       FIN-VAL-CAD-COD-ATRN.
           EXIT.
       
       VAL-CAD-IND-TCAD SECTION.
       INI-VAL-CAD-IND-TCAD.
           IF CAD-IND-TCAD IN AEC-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-CAD-IND-TCAD.
      *VAL-IND Valida miscelaneo de tabla
           MOVE 'TAB' TO MSC-COD-SIST.
           MOVE 'TCYA' TO MSC-COD-TMSC IN MSC.
           MOVE CAD-IND-TCAD IN AEC-FLD TO MSC-COD-CMSC IN MSC.
           PERFORM VAL-IND.
       FIN-VAL-CAD-IND-TCAD.
           EXIT.
       
       VAL-CAD-CAI-ICAD SECTION.
       INI-VAL-CAD-CAI-ICAD.
           IF CAD-CAI-ICAD IN AEC-FLD NOT > SPACES
              MOVE SPACES TO CAD-CAI-ICAD IN AEC-FLD.
       FIN-VAL-CAD-CAI-ICAD.
           EXIT.
       
       VAL-CAD-IIC-ICAD SECTION.
       INI-VAL-CAD-IIC-ICAD.
       FIN-VAL-CAD-IIC-ICAD.
           EXIT.
       
       VAL-CAD-SGV-LIQU SECTION.
       INI-VAL-CAD-SGV-LIQU.
       FIN-VAL-CAD-SGV-LIQU.
           EXIT.
       
       VAL-CAD-IND-MAUT SECTION.
       INI-VAL-CAD-IND-MAUT.
           IF CAD-IND-MAUT IN AEC-FLD NOT > SPACES
              MOVE SPACES TO CAD-IND-MAUT IN AEC-FLD.
       FIN-VAL-CAD-IND-MAUT.
           EXIT.
       
       VAL-CAD-COD-OFIC SECTION.
       INI-VAL-CAD-COD-OFIC.
           IF CAD-COD-OFIC IN AEC-FLD NOT > SPACES
              MOVE SPACES TO CAD-COD-OFIC IN AEC-FLD.
       FIN-VAL-CAD-COD-OFIC.
           EXIT.
       
       VAL-CAD-GLS-DISP SECTION.
       INI-VAL-CAD-GLS-DISP.
           IF CAD-GLS-DISP IN AEC-FLD NOT > SPACES
              MOVE SPACES TO CAD-GLS-DISP IN AEC-FLD.
       FIN-VAL-CAD-GLS-DISP.
           EXIT.
       
       VAL-CON-FLD-AEC SECTION.
       INI-VAL-CON-FLD-AEC.
       FIN-VAL-CON-FLD-AEC.
           EXIT.
       
       VAL-CON-AEC SECTION.
       INI-VAL-CON-AEC.
       FIN-VAL-CON-AEC.
           EXIT.
       
       VAL-NUM-KEY-AEC SECTION.
       INI-VAL-NUM-KEY-AEC.
           PERFORM VAL-NUM-IKY-AEC.
           PERFORM VAL-NUM-MKY-AEC.
           PERFORM VAL-NUM-AKY-AEC.
       FIN-VAL-NUM-KEY-AEC.
           EXIT.
       
       VAL-NUM-IKY-AEC SECTION.
       INI-VAL-NUM-IKY-AEC.
       VAL-NUM-CAD-IIC-IOPD.
           MOVE CAD-IIC-IOPD-ALF IN AEC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CAD-IIC-IOPD IN AEC-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CAD-IIC-IOPD-LEN IN AEC-FLD
                GO TO FIN-VAL-NUM-IKY-AEC.
       VAL-NUM-CAD-NUM-ICAD.
           MOVE CAD-NUM-ICAD-ALF IN AEC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 1 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CAD-NUM-ICAD IN AEC-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CAD-NUM-ICAD-LEN IN AEC-FLD
                GO TO FIN-VAL-NUM-IKY-AEC.
       FIN-VAL-NUM-IKY-AEC.
           EXIT.
       
       VAL-NUM-MKY-AEC SECTION.
       INI-VAL-NUM-MKY-AEC.
       FIN-VAL-NUM-MKY-AEC.
           EXIT.
       
       VAL-NUM-AKY-AEC SECTION.
       INI-VAL-NUM-AKY-AEC.
       FIN-VAL-NUM-AKY-AEC.
           EXIT.
       
       VAL-NUM-FLD-AEC SECTION.
       INI-VAL-NUM-FLD-AEC.
       VAL-NUM-CAD-IIC-ICAD.
           MOVE CAD-IIC-ICAD-ALF IN AEC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO CAD-IIC-ICAD IN AEC-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CAD-IIC-ICAD-LEN IN AEC-FLD
                GO TO FIN-VAL-NUM-FLD-AEC.
       VAL-NUM-CAD-SGV-LIQU.
           MOVE CAD-SGV-LIQU-ALF IN AEC-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9V TO CAD-SGV-LIQU IN AEC-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO CAD-SGV-LIQU-LEN IN AEC-FLD
                GO TO FIN-VAL-NUM-FLD-AEC.
       FIN-VAL-NUM-FLD-AEC.
           EXIT.
       
       EDT-KEY-AEC SECTION.
       INI-EDT-KEY-AEC.
           PERFORM EDT-IKY-AEC.
           PERFORM EDT-MKY-AEC.
           PERFORM EDT-AKY-AEC.
       FIN-EDT-KEY-AEC.
           EXIT.
       
       EDT-IKY-AEC SECTION.
       INI-EDT-IKY-AEC.
           IF CAD-IIC-IOPD-LEN IN AEC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CAD-IIC-IOPD-LEN IN AEC-FLD
                GO TO FIN-EDT-IKY-AEC.
           MOVE CAD-IIC-IOPD IN AEC-FLD TO CAD-IIC-IOPD-EDT IN AEC-EDT.
           MOVE CAD-IIC-IOPD-EDT IN AEC-EDT TO 
                CAD-IIC-IOPD-ALF IN AEC-FLD.
           IF CAD-NUM-ICAD-LEN IN AEC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CAD-NUM-ICAD-LEN IN AEC-FLD
                GO TO FIN-EDT-IKY-AEC.
           MOVE CAD-NUM-ICAD IN AEC-FLD TO CAD-NUM-ICAD-EDT IN AEC-EDT.
           MOVE CAD-NUM-ICAD-EDT IN AEC-EDT TO 
                CAD-NUM-ICAD-ALF IN AEC-FLD.
       FIN-EDT-IKY-AEC.
           EXIT.
       
       EDT-MKY-AEC SECTION.
       INI-EDT-MKY-AEC.
       FIN-EDT-MKY-AEC.
           EXIT.
       
       EDT-AKY-AEC SECTION.
       INI-EDT-AKY-AEC.
       FIN-EDT-AKY-AEC.
           EXIT.
       
       EDT-FLD-AEC SECTION.
       INI-EDT-FLD-AEC.
           IF CAD-IIC-ICAD-LEN IN AEC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CAD-IIC-ICAD-LEN IN AEC-FLD
                GO TO FIN-EDT-FLD-AEC.
           MOVE CAD-IIC-ICAD IN AEC-FLD TO CAD-IIC-ICAD-EDT IN AEC-EDT.
           MOVE CAD-IIC-ICAD-EDT IN AEC-EDT TO 
                CAD-IIC-ICAD-ALF IN AEC-FLD.
           IF CAD-SGV-LIQU-LEN IN AEC-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO CAD-SGV-LIQU-LEN IN AEC-FLD
                GO TO FIN-EDT-FLD-AEC.
           MOVE CAD-SGV-LIQU IN AEC-FLD TO CAD-SGV-LIQU-EDT IN AEC-EDT.
           MOVE CAD-SGV-LIQU-EDT IN AEC-EDT TO 
                CAD-SGV-LIQU-ALF IN AEC-FLD.
       FIN-EDT-FLD-AEC.
           EXIT.
