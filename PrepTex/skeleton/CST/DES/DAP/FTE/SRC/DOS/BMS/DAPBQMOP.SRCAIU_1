           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9V TO MOP-TAS IN MOP-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO MOP-TAS-LEN IN MOP-FLD
                GO TO FIN-VAL-NUM-FLD-MOP.
       VAL-NUM-MOP-VAL-1.
           MOVE MOP-VAL-1-ALF IN MOP-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 12 TO NUM-NENT.
           MOVE 3 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9V TO MOP-VAL-1 IN MOP-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               MOVE -9 TO MOP-VAL-1-LEN IN MOP-FLD
                GO TO FIN-VAL-NUM-FLD-MOP.
       FIN-VAL-NUM-FLD-MOP.
           EXIT.
       
