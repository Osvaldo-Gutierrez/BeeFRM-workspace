      *
      * DAPBUDCC
      * Version 3.0.1 Nov-2005
      * 08-May-19 12:09 PM
      *
       SET-TAG-DAPBUDCC SECTION.
       INI-SET-TAG-DAPBUDCC.
           MOVE
           '<IDNMOD NOM=$DAPBUDCC$ VSN=${VSNBU}$ VGM=$3.0.1$
      -    'GEN=$08-May-19 12:09 PM$ FNC=${FNCBU}$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-DAPBUDCC.
           EXIT.
           
       RET-ALL-DCC SECTION.
       INI-RET-ALL-DCC.
           PERFORM KEY-ALL-DCC.
           PERFORM RET-DCC-DCC.
           PERFORM RET-TAB-DCC.
       FIN-RET-ALL-DCC.
           EXIT.
       
       RET-DCC-DCC SECTION.
       INI-RET-DCC-DCC.
           MOVE DCC-CIC-CCTB IN DCC-FLD TO DCC-CIC-CCTB IN DCC.
           MOVE DCC-EXT-CTAB IN DCC-FLD TO DCC-EXT-CTAB IN DCC.
           MOVE DCC-GLS-CCSB IN DCC-FLD TO DCC-GLS-CCSB IN DCC.
           MOVE DCC-COD-VIGE IN DCC-FLD TO DCC-COD-VIGE IN DCC.
      * FEC-INI --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DCC-NUM-DTRN IN DCC-FLD TO DCC-NUM-DTRN IN DCC.
           MOVE DCC-NUM-MTRN IN DCC-FLD TO DCC-NUM-MTRN IN DCC.
           MOVE DCC-NUM-STRN IN DCC-FLD TO DCC-NUM-STRN IN DCC.
           MOVE DCC-NUM-ATRN IN DCC-FLD TO DCC-NUM-ATRN IN DCC.
      * FEC-FIN --> BMCHIBCI002210.SRCAIU RET-ALL Y KEY-ALL
           MOVE DCC-GLS-DISP IN DCC-FLD TO DCC-GLS-DISP IN DCC.
       FIN-RET-DCC-DCC.
           EXIT.
       
       RET-TAB-DCC SECTION.
       INI-RET-TAB-DCC.
           MOVE TAB-CIC-CTAB IN DCC-FLD TO TAB-CIC-CTAB IN TAB.
           MOVE TAB-GLS-DESC IN DCC-FLD TO TAB-GLS-DESC IN TAB.
           MOVE TAB-GLS-DATA IN DCC-FLD TO TAB-GLS-DATA IN TAB.
           MOVE TAB-GLS-ABRV IN DCC-FLD TO TAB-GLS-ABRV IN TAB.
           MOVE TAB-GLS-DCOR IN DCC-FLD TO TAB-GLS-DCOR IN TAB.
       FIN-RET-TAB-DCC.
           EXIT.
       
       KEY-ALL-DCC SECTION.
       INI-KEY-ALL-DCC.
           PERFORM KEY-DCC-DCC.
           PERFORM KEY-TAB-DCC.
       FIN-KEY-ALL-DCC.
           EXIT.
       
       KEY-DCC-DCC SECTION.
       INI-KEY-DCC-DCC.
           MOVE DCC-COD-TCCT IN DCC-FLD TO DCC-COD-TCCT IN DCC.
           MOVE DCC-COD-VCAM IN DCC-FLD TO DCC-COD-VCAM IN DCC.
           MOVE DCC-COD-COPD IN DCC-FLD TO DCC-COD-COPD IN DCC.
           MOVE DCC-COD-PLZO IN DCC-FLD TO DCC-COD-PLZO IN DCC.
           MOVE DCC-COD-SCTB IN DCC-FLD TO DCC-COD-SCTB IN DCC.
      *<<<< FIN_KEY_DCC_DCC
           MOVE 'DCC' TO DCC-COD-TTAB IN DCC.
      *>>>>
       FIN-KEY-DCC-DCC.
           EXIT.
       
       KEY-TAB-DCC SECTION.
       INI-KEY-TAB-DCC.
       FIN-KEY-TAB-DCC.
           EXIT.
       
       SCR-CLRA-DCC SECTION.
       INI-SCR-CLRA-DCC.
           PERFORM CLR-KEY-DCC.
           PERFORM CLR-FLD-DCC.
       FIN-SCR-CLRA-DCC.
           EXIT.
       
       CLR-KEY-DCC SECTION.
       INI-CLR-KEY-DCC.
           MOVE SPACES TO DCC-COD-TCCT IN DCC-FLD.
           MOVE SPACES TO DCC-COD-VCAM IN DCC-FLD.
           MOVE SPACES TO DCC-COD-COPD IN DCC-FLD.
           MOVE SPACES TO DCC-COD-PLZO IN DCC-FLD.
           MOVE SPACES TO DCC-COD-SCTB IN DCC-FLD.
       FIN-CLR-KEY-DCC.
           EXIT.
       
       CLR-FLD-DCC SECTION.
       INI-CLR-FLD-DCC.
           MOVE SPACES TO FRM-GLS-TCCT IN DCC-FLD.
           MOVE SPACES TO FRM-GLS-VCAM IN DCC-FLD.
           MOVE SPACES TO FRM-GLS-COPD IN DCC-FLD.
           MOVE SPACES TO FRM-GLS-PLZO IN DCC-FLD.
           MOVE SPACES TO FRM-GLS-SCTB IN DCC-FLD.
           PERFORM CLR-DCC-DCC.
           PERFORM CLR-TAB-DCC.
       FIN-CLR-FLD-DCC.
           EXIT.
       
       CLR-DCC-DCC SECTION.
       INI-CLR-DCC-DCC.
      *<<<<
           MOVE SPACES TO DCC-CIC-CCTB IN DCC-FLD.                      
           MOVE SPACES TO DCC-EXT-CTAB IN DCC-FLD.                      
           MOVE SPACES TO DCC-GLS-CCSB IN DCC-FLD.                      
           MOVE SPACES TO DCC-COD-VIGE IN DCC-FLD.                      
           MOVE SPACES TO DCC-FEC-FTRN IN DCC-FLD.                      
           MOVE SPACES TO DCC-GLS-DISP IN DCC-FLD.                      
           GO TO FIN-CLR-DCC-DCC.
      *>>>>
           MOVE SPACES TO DCC-CIC-CCTB IN DCC-FLD.
           MOVE SPACES TO DCC-EXT-CTAB IN DCC-FLD.
           MOVE SPACES TO DCC-GLS-CCSB IN DCC-FLD.
           MOVE SPACES TO DCC-COD-VIGE IN DCC-FLD.
      * FEC-INI --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE ZEROES TO DCC-FEC-FTRN IN DCC-FLD.
      * FEC-FIN --> BMCHIBCI003.SRCAIU CLR ( KEY Y FLD )
           MOVE SPACES TO DCC-GLS-DISP IN DCC-FLD.
       FIN-CLR-DCC-DCC.
           EXIT.
       
       CLR-TAB-DCC SECTION.
       INI-CLR-TAB-DCC.
           MOVE SPACES TO TAB-CIC-CTAB IN DCC-FLD.
           MOVE SPACES TO TAB-GLS-DESC IN DCC-FLD.
           MOVE SPACES TO TAB-GLS-DATA IN DCC-FLD.
           MOVE SPACES TO TAB-GLS-ABRV IN DCC-FLD.
           MOVE SPACES TO TAB-GLS-DCOR IN DCC-FLD.
       FIN-CLR-TAB-DCC.
           EXIT.
       
       FST-KEY-DCC SECTION.
       INI-FST-KEY-DCC.
           MOVE -1 TO DCC-COD-TCCT-LEN IN DCC-FLD.
      *<<<< FIN_FST_KEY_DCC
      *OGB-INI 31-MAY-2018 CONSULTA Y ELIMINACION MASIVA DE DCC
           IF SCR-CMND = 'COM' OR 'ELM'
               MOVE ZEROES TO DCC-COD-TCCT-LEN IN DCC-FLD
               MOVE -1 TO DCC-COD-VCAM-LEN IN DCC-FLD.
      *OGB-FIN 31-MAY-2018 CONSULTA Y ELIMINACION MASIVA DE DCC
      *>>>>
       FIN-FST-KEY-DCC.
           EXIT.
       
       FST-FLD-DCC SECTION.
       INI-FST-FLD-DCC.
           MOVE -1 TO DCC-CIC-CCTB-LEN IN DCC-FLD.
       FIN-FST-FLD-DCC.
           EXIT.
       
       PRO-KEY-DCC SECTION.
       INI-PRO-KEY-DCC.
           PERFORM PRO-MKY-DCC.
       FIN-PRO-KEY-DCC.
           EXIT.
       
       PRO-MKY-DCC SECTION.
       INI-PRO-MKY-DCC.
       FIN-PRO-MKY-DCC.
           EXIT.
       
       PRO-FLD-DCC SECTION.
       INI-PRO-FLD-DCC.
           PERFORM PRO-DCC-DCC.
           PERFORM PRO-TAB-DCC.
       FIN-PRO-FLD-DCC.
           EXIT.
       
       PRO-DCC-DCC SECTION.
       INI-PRO-DCC-DCC.
           MOVE FRM-CPIM-AEY TO DCC-CIC-CCTB-ATR IN DCC-FLD.
           MOVE FRM-CPIM-AEY TO DCC-EXT-CTAB-ATR IN DCC-FLD.
           MOVE FRM-CPIM-AEY TO DCC-GLS-CCSB-ATR IN DCC-FLD.
           MOVE FRM-CPIM-AEY TO DCC-COD-VIGE-ATR IN DCC-FLD.
           MOVE FRM-CPIM-AEY TO DCC-GLS-DISP-ATR IN DCC-FLD.
       FIN-PRO-DCC-DCC.
           EXIT.
       
       PRO-TAB-DCC SECTION.
       INI-PRO-TAB-DCC.
       FIN-PRO-TAB-DCC.
           EXIT.
       
       UNP-KEY-DCC SECTION.
       INI-UNP-KEY-DCC.
           PERFORM UNP-IKY-DCC.
           PERFORM UNP-AKY-DCC.
           PERFORM UNP-MKY-DCC.
       FIN-UNP-KEY-DCC.
           EXIT.
       
       UNP-IKY-DCC SECTION.
       INI-UNP-IKY-DCC.
           MOVE FRM-CPIM-UBY TO DCC-COD-TCCT-ATR.
           MOVE FRM-CPIM-UBY TO DCC-COD-VCAM-ATR.
           MOVE FRM-CPIM-UBY TO DCC-COD-COPD-ATR.
           MOVE FRM-CPIM-UBY TO DCC-COD-PLZO-ATR.
           MOVE FRM-CPIM-UBY TO DCC-COD-SCTB-ATR.
      *<<<< FIN_UNP_IKY_DCC
            MOVE FRM-CPIM-AEY TO DCC-COD-SCTB-ATR IN DCC-FLD.
      *OGB-INI 31-MAY-2018 CONSULTA Y ELIMINACION MASIVA DE DCC
           IF SCR-CMND = 'COM' OR 'ELM'
              MOVE FRM-CPIM-AEY TO DCC-COD-TCCT-ATR
              MOVE FRM-CPIM-AEY TO DCC-COD-PLZO-ATR.
      *OGB-FIN 31-MAY-2018 CONSULTA Y ELIMINACION MASIVA DE DCC
      *>>>> END
       FIN-UNP-IKY-DCC.
           EXIT.
       
       UNP-MKY-DCC SECTION.
       INI-UNP-MKY-DCC.
       FIN-UNP-MKY-DCC.
           EXIT.
       
       UNP-AKY-DCC SECTION.
       INI-UNP-AKY-DCC.
       FIN-UNP-AKY-DCC.
           EXIT.
       
       UNP-FLD-DCC SECTION.
       INI-UNP-FLD-DCC.
           PERFORM UNP-DCC-DCC.
           PERFORM UNP-TAB-DCC.
       FIN-UNP-FLD-DCC.
           EXIT.
       
       UNP-DCC-DCC SECTION.
       INI-UNP-DCC-DCC.
           MOVE FRM-CPIM-UBY TO DCC-CIC-CCTB-ATR IN DCC-FLD.
           MOVE FRM-CPIM-UBY TO DCC-EXT-CTAB-ATR IN DCC-FLD.
           MOVE FRM-CPIM-UBY TO DCC-GLS-CCSB-ATR IN DCC-FLD.
           MOVE FRM-CPIM-UBY TO DCC-COD-VIGE-ATR IN DCC-FLD.
           MOVE FRM-CPIM-UBY TO DCC-GLS-DISP-ATR IN DCC-FLD.
       FIN-UNP-DCC-DCC.
           EXIT.
       
       UNP-TAB-DCC SECTION.
       INI-UNP-TAB-DCC.
       FIN-UNP-TAB-DCC.
           EXIT.
       
       VAL-MKY-DCC SECTION.
       INI-VAL-MKY-DCC.
       FIN-VAL-MKY-DCC.
           EXIT.
       
       VAL-FLD-DCC SECTION.
       INI-VAL-FLD-DCC.
           PERFORM VAL-FRM-GLS-TCCT.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-TCCT-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-FRM-GLS-VCAM.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-VCAM-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-FRM-GLS-COPD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-COPD-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-FRM-GLS-PLZO.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-PLZO-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-FRM-GLS-SCTB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO FRM-GLS-SCTB-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-DCC-CIC-CCTB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCC-CIC-CCTB-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-DCC-EXT-CTAB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCC-EXT-CTAB-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-DCC-GLS-CCSB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCC-GLS-CCSB-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-TAB-CIC-CTAB.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAB-CIC-CTAB-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-TAB-GLS-DESC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAB-GLS-DESC-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-TAB-GLS-DATA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAB-GLS-DATA-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-TAB-GLS-ABRV.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAB-GLS-ABRV-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-TAB-GLS-DCOR.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO TAB-GLS-DCOR-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
           PERFORM VAL-DCC-COD-VIGE.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCC-COD-VIGE-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
      * FEC-INI --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DCC-FEC-FTRN.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO
                DCC-FEC-FTRN-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
      * FEC-FIN --> BMCHIBCI007.SRCAIU PERFORM VAL-[FLD]
           PERFORM VAL-DCC-GLS-DISP.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCC-GLS-DISP-LEN IN DCC-FLD
                GO TO FIN-VAL-FLD-DCC.
       FIN-VAL-FLD-DCC.
           EXIT.
       
       VAL-FRM-GLS-TCCT SECTION.
       INI-VAL-FRM-GLS-TCCT.
       FIN-VAL-FRM-GLS-TCCT.
           EXIT.
       
       VAL-FRM-GLS-VCAM SECTION.
       INI-VAL-FRM-GLS-VCAM.
       FIN-VAL-FRM-GLS-VCAM.
           EXIT.
       
       VAL-FRM-GLS-COPD SECTION.
       INI-VAL-FRM-GLS-COPD.
       FIN-VAL-FRM-GLS-COPD.
           EXIT.
       
       VAL-FRM-GLS-PLZO SECTION.
       INI-VAL-FRM-GLS-PLZO.
       FIN-VAL-FRM-GLS-PLZO.
           EXIT.
       
       VAL-FRM-GLS-SCTB SECTION.
       INI-VAL-FRM-GLS-SCTB.
       FIN-VAL-FRM-GLS-SCTB.
           EXIT.
       
       VAL-DCC-CIC-CCTB SECTION.
       INI-VAL-DCC-CIC-CCTB.
           IF DCC-CIC-CCTB IN DCC-FLD NOT > SPACES
              MOVE SPACES TO DCC-CIC-CCTB IN DCC-FLD.
      *<<<< DCC_CIC_CCTB
      * ISP:
      * DEBE GENERAR UN NUEVO REGISTRO EN COMANDO 'COP'
           IF SCR-CMND = 'COP' 
              PERFORM VAL-KEY-DCC
              PERFORM KEY-ALL-DCC
              MOVE FIO-GET-KEY TO FIO-CMND
              PERFORM CHK-DUPL
              IF FRM-SUAR = FRM-SUAR-MAL 
                  GO TO FIN-VAL-DCC-CIC-CCTB.

      *>>>> END
       FIN-VAL-DCC-CIC-CCTB.
           EXIT.
       
       VAL-DCC-EXT-CTAB SECTION.
       INI-VAL-DCC-EXT-CTAB.
           IF DCC-EXT-CTAB IN DCC-FLD NOT > SPACES
              MOVE SPACES TO DCC-EXT-CTAB IN DCC-FLD.
       FIN-VAL-DCC-EXT-CTAB.
           EXIT.
       
       VAL-DCC-GLS-CCSB SECTION.
       INI-VAL-DCC-GLS-CCSB.
           IF DCC-GLS-CCSB IN DCC-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-DCC-GLS-CCSB.
      *<<<< DCC_GLS_CCSB
      * Busca datos en registro TAB.
           IF DCC-GLS-CCSB IN DCC-FLD NOT > SPACES
               PERFORM CLR-TAB-DCC
               GO TO FIN-VAL-DCC-GLS-CCSB.
           MOVE 'PCC' TO TAB-COD-TTAB IN TAB.
           MOVE DCC-GLS-CCSB IN DCC-FLD TO TAB-COD-CTAB IN TAB.
           MOVE 'TAB'       TO FIO-SIST.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           MOVE FIO-STAT TO PGM-STAT-TAB.
           IF FIO-STAT-OKS
               PERFORM PUT-TAB-DCC
           ELSE
               MOVE 'DAP' TO MSG-COD-SIST
               MOVE 'GLSCCTBNEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>>
       FIN-VAL-DCC-GLS-CCSB.
           EXIT.
       
       VAL-TAB-CIC-CTAB SECTION.
       INI-VAL-TAB-CIC-CTAB.
       FIN-VAL-TAB-CIC-CTAB.
           EXIT.
       
       VAL-TAB-GLS-DESC SECTION.
       INI-VAL-TAB-GLS-DESC.
       FIN-VAL-TAB-GLS-DESC.
           EXIT.
       
       VAL-TAB-GLS-DATA SECTION.
       INI-VAL-TAB-GLS-DATA.
       FIN-VAL-TAB-GLS-DATA.
           EXIT.
       
       VAL-TAB-GLS-ABRV SECTION.
       INI-VAL-TAB-GLS-ABRV.
       FIN-VAL-TAB-GLS-ABRV.
           EXIT.
       
       VAL-TAB-GLS-DCOR SECTION.
       INI-VAL-TAB-GLS-DCOR.
       FIN-VAL-TAB-GLS-DCOR.
           EXIT.
       
       VAL-DCC-COD-VIGE SECTION.
       INI-VAL-DCC-COD-VIGE.
           IF DCC-COD-VIGE IN DCC-FLD NOT > SPACES
                PERFORM ERR-GLS
                GO TO FIN-VAL-DCC-COD-VIGE.
      *VAL-COD Valida codigo de tabla
           MOVE 'GNS' TO TAB-COD-SIST.
           MOVE 'SON ' TO TAB-COD-TTAB IN TAB.
           MOVE DCC-COD-VIGE IN DCC-FLD TO TAB-COD-CTAB IN TAB.
           PERFORM VAL-COD.
       FIN-VAL-DCC-COD-VIGE.
           EXIT.
      * FEC-INI --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       VAL-DCC-FEC-FTRN SECTION.
       INI-VAL-DCC-FEC-FTRN.
           IF DCC-FEC-FTRN IN DCC-FLD  NOT > ZEROES
              MOVE ZEROES TO DCC-FEC-FTRN IN DCC-FLD.
       FIN-VAL-DCC-FEC-FTRN.
           EXIT.
      * FEC-FIN --> BMCHIBCI008.SRCAIU VAL-[FLD] SECTION
       
       VAL-DCC-GLS-DISP SECTION.
       INI-VAL-DCC-GLS-DISP.
           IF DCC-GLS-DISP IN DCC-FLD NOT > SPACES
              MOVE SPACES TO DCC-GLS-DISP IN DCC-FLD.
       FIN-VAL-DCC-GLS-DISP.
           EXIT.
       
       VAL-CON-FLD-DCC SECTION.
       INI-VAL-CON-FLD-DCC.
       FIN-VAL-CON-FLD-DCC.
           EXIT.
       
       VAL-CON-DCC SECTION.
       INI-VAL-CON-DCC.
       FIN-VAL-CON-DCC.
           EXIT.
       
       VAL-NUM-KEY-DCC SECTION.
       INI-VAL-NUM-KEY-DCC.
           PERFORM VAL-NUM-IKY-DCC.
           PERFORM VAL-NUM-MKY-DCC.
           PERFORM VAL-NUM-AKY-DCC.
       FIN-VAL-NUM-KEY-DCC.
           EXIT.
       
       VAL-NUM-IKY-DCC SECTION.
       INI-VAL-NUM-IKY-DCC.
       FIN-VAL-NUM-IKY-DCC.
           EXIT.
       
       VAL-NUM-MKY-DCC SECTION.
       INI-VAL-NUM-MKY-DCC.
       FIN-VAL-NUM-MKY-DCC.
           EXIT.
       
       VAL-NUM-AKY-DCC SECTION.
       INI-VAL-NUM-AKY-DCC.
       FIN-VAL-NUM-AKY-DCC.
           EXIT.
       
       VAL-NUM-FLD-DCC SECTION.
       INI-VAL-NUM-FLD-DCC.
       FIN-VAL-NUM-FLD-DCC.
           EXIT.
       
       EDT-KEY-DCC SECTION.
       INI-EDT-KEY-DCC.
           PERFORM EDT-IKY-DCC.
           PERFORM EDT-MKY-DCC.
           PERFORM EDT-AKY-DCC.
       FIN-EDT-KEY-DCC.
           EXIT.
       
       EDT-IKY-DCC SECTION.
       INI-EDT-IKY-DCC.
       FIN-EDT-IKY-DCC.
           EXIT.
       
       EDT-MKY-DCC SECTION.
       INI-EDT-MKY-DCC.
       FIN-EDT-MKY-DCC.
           EXIT.
       
       EDT-AKY-DCC SECTION.
       INI-EDT-AKY-DCC.
       FIN-EDT-AKY-DCC.
           EXIT.
       
       EDT-FLD-DCC SECTION.
       INI-EDT-FLD-DCC.
       FIN-EDT-FLD-DCC.
           EXIT.
      *<<<< EOF_BU

       PUT-TAB-DCC SECTION.
       INI-PUT-TAB-DCC.
           IF PGM-STAT-TAB-NUL
               GO TO FIN-PUT-TAB-DCC.
           IF NOT PGM-STAT-TAB-OKS
               PERFORM CLR-TAB-DCC
               GO TO FIN-PUT-TAB-DCC.
           MOVE TAB-CIC-CTAB IN TAB TO TAB-CIC-CTAB IN DCC-FLD.
           MOVE TAB-GLS-DESC IN TAB TO TAB-GLS-DESC IN DCC-FLD.
           MOVE TAB-GLS-DATA IN TAB TO TAB-GLS-DATA IN DCC-FLD.
           MOVE TAB-GLS-ABRV IN TAB TO TAB-GLS-ABRV IN DCC-FLD.
           MOVE TAB-GLS-DCOR IN TAB TO TAB-GLS-DCOR IN DCC-FLD.
       FIN-PUT-TAB-DCC.
           EXIT.
       
       CHK-DUPL SECTION.
       INI-CHK-DUPL.
           PERFORM DAP-FIO-DCC.
           IF FIO-STAT-OKS
              MOVE 'DAP' TO MSG-COD-SIST
