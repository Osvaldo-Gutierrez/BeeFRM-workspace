      *
      * DAPBUDCL
      * Version 3.0.1 Nov-2005
      * 08-May-19 12:22 PM
      *
       SET-TAG-DAPBUDCL SECTION.
       INI-SET-TAG-DAPBUDCL.
           MOVE
           '<IDNMOD NOM=$DAPBUDCL$ VSN=$09.01.14$ VGM=$3.0.1$
      -    'GEN=$08-May-19 12:22 PM$ FNC=$081102$/>'
               TO WSS-CVT-ITEM.
       FIN-SET-TAG-DAPBUDCL.
           EXIT.
           
       RET-ALL-DCL SECTION.
       INI-RET-ALL-DCL.
           PERFORM KEY-ALL-DCL.
           PERFORM RET-DCL-DCL.
       FIN-RET-ALL-DCL.
           EXIT.
       
       RET-DCL-DCL SECTION.
       INI-RET-DCL-DCL.
           MOVE DCL-SGV-RESC IN DCL-FLD TO DCL-SGV-RESC IN DCL.
           MOVE DCL-SGV-PEGA IN DCL-FLD TO DCL-SGV-PEGA IN DCL.
           MOVE DCL-SGV-KNEW IN DCL-FLD TO DCL-SGV-KNEW IN DCL.
           MOVE DCL-SGV-FNEW IN DCL-FLD TO DCL-SGV-FNEW IN DCL.
           MOVE DCL-SGV-INTD IN DCL-FLD TO DCL-SGV-INTD IN DCL.
           MOVE DCL-COD-ACED IN DCL-FLD TO DCL-COD-ACED IN DCL.
       FIN-RET-DCL-DCL.
           EXIT.
       
       KEY-ALL-DCL SECTION.
       INI-KEY-ALL-DCL.
           PERFORM KEY-DCL-DCL.
       FIN-KEY-ALL-DCL.
           EXIT.
       
       KEY-DCL-DCL SECTION.
       INI-KEY-DCL-DCL.
           MOVE DCL-CAI-IOPD IN DCL-FLD TO DCL-CAI-IOPD IN DCL.
           MOVE DCL-IIC-IOPD IN DCL-FLD TO DCL-IIC-IOPD IN DCL.
           MOVE DCL-NUM-ILIQ IN DCL-FLD TO DCL-NUM-ILIQ IN DCL.
       FIN-KEY-DCL-DCL.
           EXIT.
       
       SCR-CLRA-DCL SECTION.
       INI-SCR-CLRA-DCL.
           PERFORM CLR-KEY-DCL.
           PERFORM CLR-FLD-DCL.
       FIN-SCR-CLRA-DCL.
           EXIT.
       
       CLR-KEY-DCL SECTION.
       INI-CLR-KEY-DCL.
           MOVE SPACES TO DCL-CAI-IOPD IN DCL-FLD.
           MOVE ZEROES TO DCL-IIC-IOPD IN DCL-FLD.
           MOVE ZEROES TO DCL-NUM-ILIQ IN DCL-FLD.
       FIN-CLR-KEY-DCL.
           EXIT.
       
       CLR-FLD-DCL SECTION.
       INI-CLR-FLD-DCL.
           PERFORM CLR-DCL-DCL.
       FIN-CLR-FLD-DCL.
           EXIT.
       
       CLR-DCL-DCL SECTION.
       INI-CLR-DCL-DCL.
           MOVE ZEROES TO DCL-SGV-RESC IN DCL-FLD.
           MOVE ZEROES TO DCL-SGV-PEGA IN DCL-FLD.
           MOVE ZEROES TO DCL-SGV-KNEW IN DCL-FLD.
           MOVE ZEROES TO DCL-SGV-FNEW IN DCL-FLD.
           MOVE ZEROES TO DCL-SGV-INTD IN DCL-FLD.
           MOVE SPACES TO DCL-COD-ACED IN DCL-FLD.
       FIN-CLR-DCL-DCL.
           EXIT.
       
       FST-KEY-DCL SECTION.
       INI-FST-KEY-DCL.
           MOVE -1 TO DCL-CAI-IOPD-LEN IN DCL-FLD.
       FIN-FST-KEY-DCL.
           EXIT.
       
       FST-FLD-DCL SECTION.
       INI-FST-FLD-DCL.
           MOVE -1 TO DCL-SGV-RESC-LEN IN DCL-FLD.
       FIN-FST-FLD-DCL.
           EXIT.
       
       PRO-KEY-DCL SECTION.
       INI-PRO-KEY-DCL.
           PERFORM PRO-MKY-DCL.
       FIN-PRO-KEY-DCL.
           EXIT.
       
       PRO-MKY-DCL SECTION.
       INI-PRO-MKY-DCL.
       FIN-PRO-MKY-DCL.
           EXIT.
       
       PRO-FLD-DCL SECTION.
       INI-PRO-FLD-DCL.
           PERFORM PRO-DCL-DCL.
       FIN-PRO-FLD-DCL.
           EXIT.
       
       PRO-DCL-DCL SECTION.
       INI-PRO-DCL-DCL.
           MOVE FRM-CPIM-AEY TO DCL-SGV-RESC-ATR IN DCL-FLD.
           MOVE FRM-CPIM-AEY TO DCL-SGV-PEGA-ATR IN DCL-FLD.
           MOVE FRM-CPIM-AEY TO DCL-SGV-KNEW-ATR IN DCL-FLD.
           MOVE FRM-CPIM-AEY TO DCL-SGV-FNEW-ATR IN DCL-FLD.
           MOVE FRM-CPIM-AEY TO DCL-SGV-INTD-ATR IN DCL-FLD.
           MOVE FRM-CPIM-AEY TO DCL-COD-ACED-ATR IN DCL-FLD.
       FIN-PRO-DCL-DCL.
           EXIT.
       
       UNP-KEY-DCL SECTION.
       INI-UNP-KEY-DCL.
           PERFORM UNP-IKY-DCL.
           PERFORM UNP-AKY-DCL.
           PERFORM UNP-MKY-DCL.
       FIN-UNP-KEY-DCL.
           EXIT.
       
       UNP-IKY-DCL SECTION.
       INI-UNP-IKY-DCL.
           MOVE FRM-CPIM-UBY TO DCL-CAI-IOPD-ATR.
           MOVE FRM-CPIM-UBY TO DCL-IIC-IOPD-ATR.
           MOVE FRM-CPIM-UBY TO DCL-NUM-ILIQ-ATR.
       FIN-UNP-IKY-DCL.
           EXIT.
       
       UNP-MKY-DCL SECTION.
       INI-UNP-MKY-DCL.
       FIN-UNP-MKY-DCL.
           EXIT.
       
       UNP-AKY-DCL SECTION.
       INI-UNP-AKY-DCL.
       FIN-UNP-AKY-DCL.
           EXIT.
       
       UNP-FLD-DCL SECTION.
       INI-UNP-FLD-DCL.
           PERFORM UNP-DCL-DCL.
       FIN-UNP-FLD-DCL.
           EXIT.
       
       UNP-DCL-DCL SECTION.
       INI-UNP-DCL-DCL.
           MOVE FRM-CPIM-UBY TO DCL-SGV-RESC-ATR IN DCL-FLD.
           MOVE FRM-CPIM-UBY TO DCL-SGV-PEGA-ATR IN DCL-FLD.
           MOVE FRM-CPIM-UBY TO DCL-SGV-KNEW-ATR IN DCL-FLD.
           MOVE FRM-CPIM-UBY TO DCL-SGV-FNEW-ATR IN DCL-FLD.
           MOVE FRM-CPIM-UBY TO DCL-SGV-INTD-ATR IN DCL-FLD.
           MOVE FRM-CPIM-UBY TO DCL-COD-ACED-ATR IN DCL-FLD.
       FIN-UNP-DCL-DCL.
           EXIT.
       
       VAL-MKY-DCL SECTION.
       INI-VAL-MKY-DCL.
       FIN-VAL-MKY-DCL.
           EXIT.
       
       VAL-FLD-DCL SECTION.
       INI-VAL-FLD-DCL.
           PERFORM VAL-DCL-SGV-RESC.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCL-SGV-RESC-LEN IN DCL-FLD
                GO TO FIN-VAL-FLD-DCL.
           PERFORM VAL-DCL-SGV-PEGA.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCL-SGV-PEGA-LEN IN DCL-FLD
                GO TO FIN-VAL-FLD-DCL.
           PERFORM VAL-DCL-SGV-KNEW.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCL-SGV-KNEW-LEN IN DCL-FLD
                GO TO FIN-VAL-FLD-DCL.
           PERFORM VAL-DCL-SGV-FNEW.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCL-SGV-FNEW-LEN IN DCL-FLD
                GO TO FIN-VAL-FLD-DCL.
           PERFORM VAL-DCL-SGV-INTD.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCL-SGV-INTD-LEN IN DCL-FLD
                GO TO FIN-VAL-FLD-DCL.
           PERFORM VAL-DCL-COD-ACED.
           IF FRM-SUAR = FRM-SUAR-MAL OR FRM-SUAR = FRM-SUAR-CLR
                MOVE FRM-IND-ERRO TO DCL-COD-ACED-LEN IN DCL-FLD
                GO TO FIN-VAL-FLD-DCL.
       FIN-VAL-FLD-DCL.
           EXIT.
       
       VAL-DCL-SGV-RESC SECTION.
       INI-VAL-DCL-SGV-RESC.
       FIN-VAL-DCL-SGV-RESC.
           EXIT.
       
       VAL-DCL-SGV-PEGA SECTION.
       INI-VAL-DCL-SGV-PEGA.
       FIN-VAL-DCL-SGV-PEGA.
           EXIT.
       
       VAL-DCL-SGV-KNEW SECTION.
       INI-VAL-DCL-SGV-KNEW.
       FIN-VAL-DCL-SGV-KNEW.
           EXIT.
       
       VAL-DCL-SGV-FNEW SECTION.
       INI-VAL-DCL-SGV-FNEW.
       FIN-VAL-DCL-SGV-FNEW.
           EXIT.
       
       VAL-DCL-SGV-INTD SECTION.
       INI-VAL-DCL-SGV-INTD.
       FIN-VAL-DCL-SGV-INTD.
           EXIT.
       
       VAL-DCL-COD-ACED SECTION.
       INI-VAL-DCL-COD-ACED.
           IF DCL-COD-ACED IN DCL-FLD NOT > SPACES
              MOVE SPACES TO DCL-COD-ACED IN DCL-FLD.
       FIN-VAL-DCL-COD-ACED.
           EXIT.
       
       VAL-CON-FLD-DCL SECTION.
       INI-VAL-CON-FLD-DCL.
       FIN-VAL-CON-FLD-DCL.
           EXIT.
       
       VAL-CON-DCL SECTION.
       INI-VAL-CON-DCL.
       FIN-VAL-CON-DCL.
           EXIT.
       
       VAL-NUM-KEY-DCL SECTION.
       INI-VAL-NUM-KEY-DCL.
           PERFORM VAL-NUM-IKY-DCL.
           PERFORM VAL-NUM-MKY-DCL.
           PERFORM VAL-NUM-AKY-DCL.
       FIN-VAL-NUM-KEY-DCL.
           EXIT.
       
       VAL-NUM-IKY-DCL SECTION.
       INI-VAL-NUM-IKY-DCL.
       VAL-NUM-DCL-IIC-IOPD.
           MOVE DCL-IIC-IOPD-ALF IN DCL-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 8 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO DCL-IIC-IOPD IN DCL-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO DCL-IIC-IOPD-LEN IN DCL-FLD
                GO TO FIN-VAL-NUM-IKY-DCL.
       VAL-NUM-DCL-NUM-ILIQ.
           MOVE DCL-NUM-ILIQ-ALF IN DCL-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 3 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NN9N TO DCL-NUM-ILIQ IN DCL-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO DCL-NUM-ILIQ-LEN IN DCL-FLD
                GO TO FIN-VAL-NUM-IKY-DCL.
       FIN-VAL-NUM-IKY-DCL.
           EXIT.
       
       VAL-NUM-MKY-DCL SECTION.
       INI-VAL-NUM-MKY-DCL.
       FIN-VAL-NUM-MKY-DCL.
           EXIT.
       
       VAL-NUM-AKY-DCL SECTION.
       INI-VAL-NUM-AKY-DCL.
       FIN-VAL-NUM-AKY-DCL.
           EXIT.
       
       VAL-NUM-FLD-DCL SECTION.
       INI-VAL-NUM-FLD-DCL.
       VAL-NUM-DCL-SGV-RESC.
           MOVE DCL-SGV-RESC-ALF IN DCL-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NS9V TO DCL-SGV-RESC IN DCL-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO DCL-SGV-RESC-LEN IN DCL-FLD
                GO TO FIN-VAL-NUM-FLD-DCL.
       VAL-NUM-DCL-SGV-PEGA.
           MOVE DCL-SGV-PEGA-ALF IN DCL-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NS9V TO DCL-SGV-PEGA IN DCL-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO DCL-SGV-PEGA-LEN IN DCL-FLD
                GO TO FIN-VAL-NUM-FLD-DCL.
       VAL-NUM-DCL-SGV-KNEW.
           MOVE DCL-SGV-KNEW-ALF IN DCL-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NS9V TO DCL-SGV-KNEW IN DCL-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO DCL-SGV-KNEW-LEN IN DCL-FLD
                GO TO FIN-VAL-NUM-FLD-DCL.
       VAL-NUM-DCL-SGV-FNEW.
           MOVE DCL-SGV-FNEW-ALF IN DCL-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NS9V TO DCL-SGV-FNEW IN DCL-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO DCL-SGV-FNEW-LEN IN DCL-FLD
                GO TO FIN-VAL-NUM-FLD-DCL.
       VAL-NUM-DCL-SGV-INTD.
           MOVE DCL-SGV-INTD-ALF IN DCL-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-S TO NUM-SGNO.
           MOVE 11 TO NUM-NENT.
           MOVE 4 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
                MOVE NUM-NS9V TO DCL-SGV-INTD IN DCL-FLD
           ELSE
                MOVE NUM-MENS     TO FRM-MENS
                MOVE FRM-SUAR-MAL TO FRM-SUAR
                MOVE -9 TO DCL-SGV-INTD-LEN IN DCL-FLD
                GO TO FIN-VAL-NUM-FLD-DCL.
       FIN-VAL-NUM-FLD-DCL.
           EXIT.
       
       EDT-KEY-DCL SECTION.
       INI-EDT-KEY-DCL.
           PERFORM EDT-IKY-DCL.
           PERFORM EDT-MKY-DCL.
           PERFORM EDT-AKY-DCL.
       FIN-EDT-KEY-DCL.
           EXIT.
       
       EDT-IKY-DCL SECTION.
       INI-EDT-IKY-DCL.
           IF DCL-IIC-IOPD-LEN IN DCL-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DCL-IIC-IOPD-LEN IN DCL-FLD
                GO TO FIN-EDT-IKY-DCL.
           MOVE DCL-IIC-IOPD IN DCL-FLD TO DCL-IIC-IOPD-EDT IN DCL-EDT.
           MOVE DCL-IIC-IOPD-EDT IN DCL-EDT TO 
                DCL-IIC-IOPD-ALF IN DCL-FLD.
           IF DCL-NUM-ILIQ-LEN IN DCL-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DCL-NUM-ILIQ-LEN IN DCL-FLD
                GO TO FIN-EDT-IKY-DCL.
           MOVE DCL-NUM-ILIQ IN DCL-FLD TO DCL-NUM-ILIQ-EDT IN DCL-EDT.
           MOVE DCL-NUM-ILIQ-EDT IN DCL-EDT TO 
                DCL-NUM-ILIQ-ALF IN DCL-FLD.
       FIN-EDT-IKY-DCL.
           EXIT.
       
       EDT-MKY-DCL SECTION.
       INI-EDT-MKY-DCL.
       FIN-EDT-MKY-DCL.
           EXIT.
       
       EDT-AKY-DCL SECTION.
       INI-EDT-AKY-DCL.
       FIN-EDT-AKY-DCL.
           EXIT.
       
       EDT-FLD-DCL SECTION.
       INI-EDT-FLD-DCL.
           IF DCL-SGV-RESC-LEN IN DCL-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DCL-SGV-RESC-LEN IN DCL-FLD
                GO TO FIN-EDT-FLD-DCL.
           MOVE DCL-SGV-RESC IN DCL-FLD TO DCL-SGV-RESC-EDT IN DCL-EDT.
           MOVE DCL-SGV-RESC-EDT IN DCL-EDT TO 
                DCL-SGV-RESC-ALF IN DCL-FLD.
           IF DCL-SGV-PEGA-LEN IN DCL-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DCL-SGV-PEGA-LEN IN DCL-FLD
                GO TO FIN-EDT-FLD-DCL.
           MOVE DCL-SGV-PEGA IN DCL-FLD TO DCL-SGV-PEGA-EDT IN DCL-EDT.
           MOVE DCL-SGV-PEGA-EDT IN DCL-EDT TO 
                DCL-SGV-PEGA-ALF IN DCL-FLD.
           IF DCL-SGV-KNEW-LEN IN DCL-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DCL-SGV-KNEW-LEN IN DCL-FLD
                GO TO FIN-EDT-FLD-DCL.
           MOVE DCL-SGV-KNEW IN DCL-FLD TO DCL-SGV-KNEW-EDT IN DCL-EDT.
           MOVE DCL-SGV-KNEW-EDT IN DCL-EDT TO 
                DCL-SGV-KNEW-ALF IN DCL-FLD.
           IF DCL-SGV-FNEW-LEN IN DCL-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DCL-SGV-FNEW-LEN IN DCL-FLD
                GO TO FIN-EDT-FLD-DCL.
           MOVE DCL-SGV-FNEW IN DCL-FLD TO DCL-SGV-FNEW-EDT IN DCL-EDT.
           MOVE DCL-SGV-FNEW-EDT IN DCL-EDT TO 
                DCL-SGV-FNEW-ALF IN DCL-FLD.
           IF DCL-SGV-INTD-LEN IN DCL-FLD = FRM-ERR-NUME
                MOVE FRM-IND-ERRO TO DCL-SGV-INTD-LEN IN DCL-FLD
                GO TO FIN-EDT-FLD-DCL.
           MOVE DCL-SGV-INTD IN DCL-FLD TO DCL-SGV-INTD-EDT IN DCL-EDT.
           MOVE DCL-SGV-INTD-EDT IN DCL-EDT TO 
                DCL-SGV-INTD-ALF IN DCL-FLD.
       FIN-EDT-FLD-DCL.
           EXIT.
