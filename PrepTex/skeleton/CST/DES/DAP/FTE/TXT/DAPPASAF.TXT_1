*%! Codigo adicional para DAPPAOPD
*% IF INI_FIO
      *<<<< INI_FIO
           PERFORM GNS-HDL-VSM.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBGVSM.
       COPY GNSBHVSM.
      *>>>>
*% END
*% IF WSS
      *<<<< WSS
      *Entidades de TAB
       COPY TABBRCAM.
       COPY GNSWGPES.
       COPY GNSWGINT.


       01  WSS-VARI.

           03 WSS-VAL-IPES                        PIC 9(11)V9(04).
           03 WSS-VAL-CAPT                        PIC 9(11)V9(04).
           03 WSS-SGV-VCAM                        PIC 9(11)V9(04).
           03 WSS-CIC-VCAM.
              05 WSS-CIC-TVAL                     PIC X(01).
              05 WSS-CIC-CVAL                     PIC X(03).
              05 WSS-CIC-TCAM                     PIC X(08).

      *>>>> WSS
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
      *Entidades de TAB
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CAM-REQA==.
      *>>>>
*% END
*% IF CMN_MOD
      *<<<< CMN_MOD
           MOVE FRM-VAL-NCAJ IN SAF-FLD TO OPD-VAL-CAJA IN OPD.
           IF OPD-VAL-CAJA IN OPD = ZEROES
               MOVE 'A' TO OPD-COD-STAT IN OPD.
      *>>>>
*% END
*% IF FIN_HBL_FLD
      *<<<< FIN_HBL_FLD
           MOVE FRM-CPIM-AEY TO FRM-VAL-IPES-ATR IN SAF-FLD.
           MOVE FRM-CPIM-AEY TO OPD-VAL-CAJA-ATR IN SAF-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VAL-NPES-ATR IN SAF-FLD.
           MOVE FRM-CPIM-AEY TO FRM-VAL-NCAJ-ATR IN SAF-FLD.
      *>>>>
*% END
*% IF FIN_CMN_MOD
      *<<<< FIN_CMN_MOD
           IF FIO-STAT-OKS
               PERFORM BFR-RFSH.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
      *Entidades de TAB
       COPY TABBFCAM.

       COPY GNSBGCAM.
       COPY GNSBGPES.

       BFR-RFSH SECTION.
       INI-BFR-RFSH.

           MOVE 'OPD-CIC-ISER' TO FIO-AKEY.
           MOVE FIO-BFR-RFH    TO FIO-CMND.
           PERFORM DAP-FIO-OPD.

       FIN-BFR-RFSH.
           EXIT.
      *>>>>
*% END
