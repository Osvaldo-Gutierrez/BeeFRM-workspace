*%! Codigo adicional para programa CLV
*% IF SETTAG
*% VSNPQ = "09.07.26"
*% FNCPQ = "090205"
*% VSNPU = "09.07.26"
*% FNCPU = "090205"
*% END
*% NO_HDR_CST = TRUE
*% PGM_PTC = TRUE
*%
*%! IF PGM_PQ
*%!  FRM = "CLV"
*%!  PADRE = "LAD"
*%!  KEY_PADRE = "LAD-KEY-FLAD"
*%!  LARGO_KEY_PADRE = 20
*%!  FIO_PADRE = "DAP-FIO-LAD"
*%!  SIST_PADRE = "DAP"
*%!  KEY_BROWSE_PADRE = "LAD-KEY-FLAD"
*%!
*%!  ACEPTE_PF7 = TRUE
*%!  ACEPTE_PF8 = TRUE
*%! INCLUDE 'GNSSRC:PFSCONENT.SRC'
*%!
*%!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
*%! END
*% IF WSS
      *<<<< WSS
       77  WSS-LEE-DBC-DESDE-LAD  VALUE 'N'   PIC X(01).
       77  WSS-CIC-ICLI    VALUE SPACES PIC X(12).
       77  WSS-YA-LEI-REG-2 VALUE 'N' PIC X(01).
       77  WSS-COMPARA      VALUE 'N' PIC X(01).
      *          03 FRM-CPIM.
      * AEY = Protegido, normal
      *             05 FRM-CPIM-AEY         PIC X(01) VALUE '1'.
      * UBY = Desprotegido, brillante
      *             05 FRM-CPIM-UBY         PIC X(01) VALUE 'I'.
      * ANY = Protegido, non-display
      *             05 FRM-CPIM-ANY         PIC X(01) VALUE '_'.
      * UNY = Desprotegido, non-display
      *             05 FRM-CPIM-UNY         PIC X(01) VALUE '('.
      * ANY = Protegido, normal
      *             05 FRM-CPSF-AEY         PIC X(01) VALUE '-'.
      * ABY = Protegido, brillante
      *             05 FRM-CPIM-ABY         PIC X(01) VALUE '9'.
      * UST = Virtual para BMS, pues es campo STOPPER en UNISYS
      *             05 FRM-CPIM-UST         PIC X(01) VALUE SPACES.
       COPY DAPBRVEN.
       COPY DAPBRVEN REPLACING VEN BY VEN-CPR.
      *COPY DAPBROPD REPLACING ==01  OPD== BY ==01  OPD-CPR==.
       COPY DAPBRRCC.
       COPY SGCBRDBC.
       COPY GNSWGCPT.
       COPY GNSWGSTR.
       77  WSS-COD-ENTI         VALUE 'VEN'     PIC X(03).
       01  WSS-FEC-FAMD.
           03 WSS-NUM-SAMD                      PIC X(02).
           03 WSS-NUM-AAMD                      PIC X(02).
           03 WSS-NUM-MAMD                      PIC X(02).
           03 WSS-NUM-DAMD                      PIC X(02).
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
      *Entidades de SGC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-VEN-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RCC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
      *
       01   VAR-PROGRAMA.
            03 PGM-COD-CREB              VALUE ' '       PIC X(03).
            03 PGM-STAT-RCC.
               88 PGM-STAT-RCC-NUL       VALUE '  '.
               07 PGM-STT1-RCC           VALUE '2'       PIC X(01).
                  88 PGM-STAT-RCC-OKS    VALUE '0'.
               07 PGM-STT2-RCC           VALUE '3'       PIC X(01).
      *>>>>
*% END
*% IF INI_FIO_IBM
      *<<<< INI_FIO
           PERFORM GET-FHOY.
           IF SCR-INST = 'GAB' AND
              ( FRM-FFLD = FRM-FFLD-PF8 OR
                FRM-FFLD = FRM-FFLD-PF7 )
                  MOVE 'GKY' TO SCR-INST.
      *>>>> INI_FIO
*% END
*% IF FND_KEY_CMND
      *<<<< FND_KEY_CMND
           IF FRM-COD-CREB IN CLV-FLD = 'OPE' OR
              FRM-COD-CREB IN CLV-FLD = 'CLI'
               MOVE 'N' TO WSS-LEE-DBC-DESDE-LAD
               PERFORM OBTENER-NOMBRE-DBC.
           PERFORM SET-KEY-LAD.
      
           MOVE FIO-GET-NLS TO FIO-CMND.
      *>>>> FND_KEY_CMND
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
           IF NOT FIO-STAT-OKS
               GO TO FIN-FND-KEY.
      
           IF FIO-STAT-OKS
               IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI
                    MOVE '23'   TO FIO-STAT
                    MOVE FIO-STAT TO PGM-STAT-LAD
                    GO TO FIN-FND-KEY.
      
           IF NOT ( FRM-COD-CREB IN CLV-FLD = 'OPE' OR
                    FRM-COD-CREB IN CLV-FLD = 'CLI' )
                     MOVE 'S' TO WSS-LEE-DBC-DESDE-LAD
                     PERFORM OBTENER-NOMBRE-DBC.
      
           MOVE LAD-CIC-ICLI IN LAD TO WSS-CIC-ICLI.
      
           IF FRM-COD-CREB IN CLV-FLD = 'OPE'
               IF LAD-CAI-IOPD IN CLV-FLD NOT =
                  LAD-CAI-IOPD IN LAD    OR
                  LAD-IIC-IOPD IN CLV-FLD NOT =
                  LAD-IIC-IOPD IN LAD
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE '23' TO FIO-STAT
                    MOVE '23' TO PGM-STAT-LAD
                    MOVE 'CLVNONXTOIPE' TO MSG-COD-MENS
                    MOVE 'DAP' TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
      
           IF FRM-COD-CREB IN CLV-FLD = 'AUT'
               IF LAD-COD-AUTO IN CLV-FLD NOT =
                  LAD-COD-AUTO IN LAD
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE '23' TO FIO-STAT
                    MOVE '23' TO PGM-STAT-LAD
                    MOVE 'CLVNONXTOAUT' TO MSG-COD-MENS
                    MOVE 'DAP' TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
      
           IF FRM-COD-CREB IN CLV-FLD = 'CLI'
               IF LAD-CIC-ICLI IN LAD     NOT =
                  DBC-CIC-ICLI IN DBC
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE '23' TO FIO-STAT
                    MOVE '23' TO PGM-STAT-LAD
                    MOVE 'CLVNONXTOCLI' TO MSG-COD-MENS
                    MOVE 'DAP' TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
      
           IF SCR-CMND = 'CON'
              IF FRM-FFLD = FRM-FFLD-PF8
                    PERFORM SET-KEY-LAD
                    PERFORM PRO-PF8
              ELSE
                IF FRM-FFLD = FRM-FFLD-PF7
                    PERFORM SET-KEY-LAD
                    PERFORM PRO-PF7.
      
           IF PGM-STAT-LAD-OKS
           IF FRM-FFLD = FRM-FFLD-PF7 OR
              FRM-FFLD = FRM-FFLD-PF8
               MOVE 'S' TO WSS-LEE-DBC-DESDE-LAD
               PERFORM OBTENER-NOMBRE-DBC.
      
           IF PGM-STAT-LAD-OKS
               IF LAD-COD-ENTF IN LAD = WSS-COD-ENTI
                   MOVE '00'   TO FIO-STAT
                   MOVE LAD-COD-CONT IN LAD TO VEN
                   MOVE '00'   TO PGM-STAT-VEN.
      
      *>>>> FIN_FND_KEY
*% END
*% IF INI_SCR_DMSG
*% IF PGM_PQ
      *<<<< INI_SCR_DMSG
      * Con este codigo logramos que una vez terminada la consulta
      * las claves no se borren de la pantalla
           IF SCR-CMND = 'CON'
                AND
              SCR-INST = SCR-INST-GKY
                AND
              FRM-FFLD NOT = FRM-FFLD-RET
                  MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR.
      *>>>> INI_SCR_DMSG
*% END
*% END
*% IF FIN_SCR_TRL
      *<<<< FIN_SCR_TRL
      
           IF PGM-STAT-LAD-OKS
               MOVE 'CLVPF7PF8 ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
      
      *>>>> FIN_SCR_TRL
*% END
*% IF PFS_SCR_GKEY_PREV
      *<<<< PFS_SCR_GKEY_PREV
           IF NOT ( FRM-FFLD = FRM-FFLD-PF7 )
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 )
      *>>>> PFS_SCR_GKEY_PREV
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF7 )
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 )
      *>>>> PFS_GET_CON
*% END
*% IF POST_SCR_PUTA
      *<<<< POST_SCR_PUTA
           IF PGM-STAT-LAD-OKS
               IF LAD-COD-INST = 'ANT'
                    PERFORM SET-KEY-LAD
                    MOVE FIO-GET-NXT TO FIO-CMND
                    PERFORM DAP-FIO-LAD
                    IF FIO-STAT-OKS
                         IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI
                            MOVE '23'  TO FIO-STAT
                            MOVE FIO-STAT TO PGM-STAT-LAD
                         ELSE
                             MOVE LAD-COD-CONT IN LAD TO VEN-CPR
                             MOVE 'S' TO WSS-COMPARA
                             GO TO FIN-AT-DES.
      
           IF PGM-STAT-LAD-OKS
               IF LAD-COD-INST = 'DES'
                     PERFORM SET-KEY-LAD
                     MOVE FIO-GET-PRV TO FIO-CMND
                     PERFORM DAP-FIO-LAD
                     IF FIO-STAT-OKS
                          IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI
                               MOVE '23' TO FIO-STAT
                               MOVE FIO-STAT TO PGM-STAT-LAD
                          ELSE
                               MOVE LAD-COD-CONT IN LAD TO VEN-CPR
                               MOVE 'S' TO WSS-COMPARA.
       FIN-AT-DES.
      
      *     ==>>  01
      *      
           MOVE '01. F Transaccion' TO FRM-GLS-TX01     IN CLV-FLD.
           MOVE FRM-CPIM-AEY        TO FRM-GLS-TX01-ATR IN CLV-FLD.
      
           MOVE FRM-CPIM-AEY        TO FRM-FEC-FTRN-ATR IN CLV-FLD.
      
           IF WSS-COMPARA = 'S'
               IF VEN-FEC-FTRN IN VEN NOT =
                  VEN-FEC-FTRN IN VEN-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX01-ATR IN CLV-FLD
                   MOVE FRM-CPIM-ABY TO FRM-FEC-FTRN-ATR IN CLV-FLD.

      *     ==>>  02
      *      
           MOVE '02. Hora Transaccion' TO FRM-GLS-TX02     IN CLV-FLD.
           MOVE FRM-CPIM-AEY      TO FRM-GLS-TX02-ATR IN CLV-FLD.
      
           MOVE FRM-CPIM-AEY      TO VEN-HRA-HRTR-ATR IN CLV-FLD.
      
           IF WSS-COMPARA = 'S'
               IF VEN-HRA-HRTR IN VEN NOT =
                  VEN-HRA-HRTR IN VEN-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX02-ATR IN CLV-FLD
                   MOVE FRM-CPIM-ABY TO VEN-HRA-HRTR-ATR IN CLV-FLD.

      *     ==>>  03
      *      
           MOVE '03. Indicador Tipo Accion' TO FRM-GLS-TX03 IN CLV-FLD.
           MOVE FRM-CPIM-AEY    TO FRM-GLS-TX03-ATR IN CLV-FLD.
      
           MOVE FRM-CPIM-AEY    TO VEN-COD-TACC-ATR IN CLV-FLD.
      *     IF WSS-COMPARA = 'S'
               IF VEN-COD-TACC IN VEN NOT =
                  VEN-COD-TACC IN VEN-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX03-ATR IN CLV-FLD
                   MOVE FRM-CPIM-ABY TO VEN-COD-TACC-ATR IN CLV-FLD.

      *     ==>>  04
      *      
           MOVE '04. Valor Capital Vcto' TO FRM-GLS-TX04 IN CLV-FLD.
           MOVE FRM-CPIM-AEY    TO FRM-GLS-TX04-ATR IN CLV-FLD.
           MOVE FRM-CPIM-AEY    TO VEN-VAL-CAPI-ATR IN CLV-FLD.
           IF WSS-COMPARA = 'S'
               IF VEN-VAL-CAPI IN VEN NOT =
                  VEN-VAL-CAPI IN VEN-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX04-ATR IN CLV-FLD
                   MOVE FRM-CPIM-ABY TO VEN-VAL-CAPI-ATR IN CLV-FLD.

      *     ==>>  05
      *      
           MOVE '05. Indicador Status' TO FRM-GLS-TX05     IN CLV-FLD.
           MOVE FRM-CPIM-AEY      TO FRM-GLS-TX05-ATR IN CLV-FLD.
           MOVE FRM-CPIM-AEY      TO VEN-COD-STAT-ATR IN CLV-FLD.
           IF WSS-COMPARA = 'S'
               IF VEN-COD-STAT IN VEN NOT =
                  VEN-COD-STAT IN VEN-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX05-ATR IN CLV-FLD
                   MOVE FRM-CPIM-ABY TO VEN-COD-STAT-ATR IN CLV-FLD.

      *     ==>>  06
      *      
           MOVE '06. Saldo Valor Capit.' TO FRM-GLS-TX06     IN CLV-FLD.
           MOVE FRM-CPIM-AEY     TO FRM-GLS-TX06-ATR IN CLV-FLD.
           MOVE FRM-CPIM-AEY     TO VEN-VAL-SCAP-ATR IN CLV-FLD.
             IF WSS-COMPARA = 'S'
               IF VEN-VAL-SCAP IN VEN NOT =
                  VEN-VAL-SCAP IN VEN-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX06-ATR IN CLV-FLD
                   MOVE FRM-CPIM-ABY TO VEN-VAL-SCAP-ATR IN CLV-FLD.

      *     ==>>  07
      *      
           MOVE '07. Origen Transaccion' TO FRM-GLS-TX07  IN CLV-FLD.
           MOVE FRM-CPIM-AEY      TO FRM-GLS-TX07-ATR IN CLV-FLD.
           MOVE FRM-CPIM-AEY      TO VEN-COD-OTRN-ATR IN CLV-FLD.
           IF WSS-COMPARA = 'S'
               IF VEN-COD-OTRN IN VEN NOT =
                  VEN-COD-OTRN IN VEN-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX07-ATR IN CLV-FLD
                   MOVE FRM-CPIM-ABY TO VEN-COD-OTRN-ATR IN CLV-FLD.

      *     ==>>  08
      *      
           MOVE '08. Ints Dev No Liquid' TO FRM-GLS-TX08 IN CLV-FLD.
           MOVE FRM-CPIM-AEY      TO FRM-GLS-TX08-ATR IN CLV-FLD.
      
           MOVE FRM-CPIM-AEY    TO VEN-VAL-IDNI-ATR IN CLV-FLD.
      
           IF WSS-COMPARA = 'S'
               IF VEN-VAL-IDNI IN VEN NOT =
                  VEN-VAL-IDNI IN VEN-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX08-ATR IN CLV-FLD
                   MOVE FRM-CPIM-ABY TO VEN-VAL-IDNI-ATR IN CLV-FLD.

      *     ==>>  09
      *      
           MOVE '09. Autor' TO FRM-GLS-TX09     IN CLV-FLD.
           MOVE FRM-CPIM-AEY      TO FRM-GLS-TX09-ATR IN CLV-FLD.
      
           MOVE FRM-CPIM-AEY     TO VEN-COD-ATRN-ATR IN CLV-FLD.
      
           IF WSS-COMPARA = 'S'
               IF VEN-COD-ATRN IN VEN NOT =
                  VEN-COD-ATRN IN VEN-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX09-ATR IN CLV-FLD
                   MOVE FRM-CPIM-ABY TO VEN-COD-ATRN-ATR IN CLV-FLD.

      *     ==>>  10
      *      
           MOVE '10. Valor Final Vcto' TO FRM-GLS-TX10     IN CLV-FLD.
           MOVE FRM-CPIM-AEY           TO FRM-GLS-TX10-ATR IN CLV-FLD.
      
           MOVE FRM-CPIM-AEY    TO VEN-VAL-FINA-ATR IN CLV-FLD.
      
           IF WSS-COMPARA = 'S'
               IF VEN-VAL-FINA IN VEN NOT =
                  VEN-VAL-FINA IN VEN-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX10-ATR IN CLV-FLD
                   MOVE FRM-CPIM-ABY TO VEN-VAL-FINA-ATR IN CLV-FLD.

      *     ==>>  11
      *      
           MOVE '11. F Vencimiento' TO FRM-GLS-TX11     IN CLV-FLD.
           MOVE FRM-CPIM-AEY        TO FRM-GLS-TX11-ATR IN CLV-FLD.
      
           MOVE FRM-CPIM-AEY    TO VEN-FEC-FVEN-ATR IN CLV-FLD.
      
           IF WSS-COMPARA = 'S'
               IF VEN-FEC-FVEN IN VEN NOT =
                  VEN-FEC-FVEN IN VEN-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX11-ATR IN CLV-FLD
                   MOVE FRM-CPIM-ABY TO VEN-FEC-FVEN-ATR IN CLV-FLD.

      *     ==>>  12
      *      
           MOVE '12. Saldo Valor Final' TO FRM-GLS-TX12 IN CLV-FLD.
           MOVE FRM-CPIM-AEY    TO FRM-GLS-TX12-ATR IN CLV-FLD.
      
           MOVE FRM-CPIM-AEY    TO VEN-VAL-SFIN-ATR IN CLV-FLD.
      
           IF WSS-COMPARA = 'S'
               IF VEN-VAL-SFIN IN VEN NOT =
                  VEN-VAL-SFIN IN VEN-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX12-ATR IN CLV-FLD
                   MOVE FRM-CPIM-ABY TO VEN-VAL-SFIN-ATR IN CLV-FLD.

      *     ==>>  13
      *      
           MOVE '13. Situacion Contable' TO FRM-GLS-TX13 IN CLV-FLD.
           MOVE FRM-CPIM-AEY      TO FRM-GLS-TX13-ATR IN CLV-FLD.
      
           MOVE FRM-CPIM-AEY      TO VEN-COD-SCTB-ATR IN CLV-FLD.
      
           IF WSS-COMPARA = 'S'
               IF VEN-COD-SCTB IN VEN NOT =
                  VEN-COD-SCTB IN VEN-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX13-ATR IN CLV-FLD
                   MOVE FRM-CPIM-ABY TO VEN-COD-SCTB-ATR IN CLV-FLD.
      *     ==>>  14
      *      
           MOVE '14. Vfin Flex'   TO FRM-GLS-TX14     IN CLV-FLD.
           MOVE FRM-CPIM-AEY      TO FRM-GLS-TX14-ATR IN CLV-FLD.
      
           MOVE FRM-CPIM-AEY    TO VEN-VAL-FFLE-ATR IN CLV-FLD.

           IF VEN-VAL-FFLE IN VEN-CPR IS NOT NUMERIC
                 MOVE ZEROES TO VEN-VAL-FFLE IN VEN-CPR.
           IF VEN-VAL-FFLE IN VEN IS NOT NUMERIC
                 MOVE ZEROES TO VEN-VAL-FFLE IN VEN.

           IF WSS-COMPARA = 'S'
               IF VEN-VAL-FFLE IN VEN NOT =
                  VEN-VAL-FFLE IN VEN-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX14-ATR IN CLV-FLD
                   MOVE FRM-CPIM-ABY TO VEN-VAL-FFLE-ATR IN CLV-FLD.
      
           MOVE CLV-FLD TO FRM-DFLD.
      
      
      *>>>> POST_SCR_PUTA
*% END
*% IF EOF
      *<<<< EOF
       GNX-PRO-QUE SECTION.
       INI-GNX-PRO-QUE.
           PERFORM GNS-PRO-QUE.
       FIN-GNX-PRO-QUE.
           EXIT.
      
       SET-KEY-LAD SECTION.
       INI-SET-KEY-LAD.
      
           MOVE WSS-COD-ENTI TO LAD-COD-ENTF IN LAD
                                LAD-COD-ENTO IN LAD
                                LAD-COD-ENTC IN LAD
                                LAD-COD-ENTA IN LAD
                                LAD-COD-ENTI IN LAD.
      
           IF FRM-COD-CREB IN CLV-FLD = 'OPE'
               MOVE 'LAD-KEY-OLAD' TO FIO-AKEY
           ELSE
           IF FRM-COD-CREB IN CLV-FLD = 'CLI'
               MOVE 'LAD-KEY-CLAD' TO FIO-AKEY
           ELSE
           IF FRM-COD-CREB IN CLV-FLD = 'AUT'
               MOVE 'LAD-KEY-ALAD' TO FIO-AKEY.
       FIN-SET-KEY-LAD.
           EXIT.
      
       OBTENER-NOMBRE-DBC  SECTION.
      *----------------------------
       INI-OBTENER-NOMBRE-DBC.
      
           IF WSS-LEE-DBC-DESDE-LAD = 'S'
               MOVE LAD-CIC-ICLI IN LAD TO DBC-CIC-ICLI IN DBC.
      
           IF FRM-COD-CREB IN CLV-FLD = 'OPE' AND
              NOT PGM-STAT-DBC-OKS
                MOVE LAD-CIC-IOPD IN LAD TO RCC-CIC-IOPD IN RCC
                PERFORM GET-CUR-RCC
                IF PGM-STAT-RCC-OKS AND
                   LAD-CIC-IOPD IN LAD  =  RCC-CIC-IOPD IN RCC
                     MOVE RCC-CIC-ICLI IN RCC TO DBC-CIC-ICLI IN DBC
                ELSE
                   MOVE '23'      TO FIO-STAT
                   MOVE '23'      TO PGM-STAT-DBC
                   GO TO FIN-OBTENER-NOMBRE-DBC.
      
           IF FRM-COD-CREB IN CLV-FLD = 'CLI' AND
              NOT PGM-STAT-DBC-OKS
                MOVE 'DBC-IDC-ICLI' TO FIO-AKEY.
      
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           MOVE FIO-STAT TO PGM-STAT-DBC.
      
           IF FIO-STAT-OKS
      *  ISP INI 27-04-2009
                MOVE DBC-CIC-ICLI IN DBC TO
                     LAD-CIC-ICLI IN LAD
      *  FIN INI 27-04-2009
                PERFORM CPT-NOMB
           ELSE
                MOVE SPACES TO DBC-GLS-NOMC IN DBC
                MOVE SPACES TO DBC-VRF-ICLI IN DBC
                MOVE SPACES TO DBC-IDC-ICLI IN DBC
                MOVE '00'   TO PGM-STAT-DBC.
      
       FIN-OBTENER-NOMBRE-DBC.
           EXIT.
      *
       GET-CUR-RCC SECTION.
       INI-GET-CUR-RCC.
           MOVE   'RCC-KEY-IOPD'           TO FIO-AKEY.
           MOVE    FIO-GET-NLS             TO FIO-CMND.
           PERFORM DAP-FIO-RCC.
           IF NOT ( FIO-STAT-OKS     AND
                    LAD-CIC-IOPD IN LAD = RCC-CIC-IOPD IN RCC )
                    MOVE '23' TO FIO-STAT.
           MOVE   FIO-STAT  TO PGM-STAT-RCC.
       FIN-GET-CUR-RCC.
           EXIT.
      
      
       COPY GNSBBTAB.
       COPY DAPBFVEN.
       COPY DAPBFRCC.
       COPY SGCBFDBC.
       COPY GNSBGCPT.
       COPY GNSBGSTR.
       CPT-NOMB SECTION.
       INI-CPT-NOMB.
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.
           PERFORM CPT-BLKS.
           MOVE CPT-STRN            TO DBC-GLS-NOMC IN DBC.
       FIN-CPT-NOMB.
           EXIT.
      
       PRO-PF7 SECTION.
       INI-PRO-PF7.
           PERFORM SET-KEY-LAD.
           MOVE FIO-GET-PRV TO FIO-CMND.
           PERFORM DAP-FIO-LAD.
           MOVE FIO-STAT TO PGM-STAT-LAD.
      
           IF NOT FIO-STAT-OKS
                MOVE FIO-GET-KEY TO FIO-CMND
                PERFORM DAP-FIO-LAD
                PERFORM EDT-FLD-CLV
                MOVE 'CLVNOPRV' TO MSG-COD-MENS
                MOVE 'DAP' TO MSG-COD-SIST
                PERFORM GET-MSG
                MOVE MSG-GLS-DESC TO FRM-MENS
                GO TO FIN-PRO-PF7.
      
           IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI
