*% FRM = 'CCO'
*% FMT_CHI = TRUE
*% NO_HDR_CST = TRUE
*% NO_TIENE_TIMESTAMP = TRUE
*% IF SETTAG
*% FNCPQ = "080905"
*% VSNPQ = "08.10.20"
*% FNCPU = "080905"
*% VSNPU = "08.10.20"
*% END
*% IF WSS
      *<<<< WSS
       COPY GNSWGHOY.
       COPY GNSWGCPT.
      *>>>> WSS
*% END
*% IF INI_SCR_DMSG
*% IF PGM_PQ
      *<<<< INI_SCR_DMSG
           IF SCR-CMND = 'CON' AND
              SCR-INST = SCR-INST-GKY
                  MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR.   
      *>>>> INI_SCR_DMSG
*% END
*% END
*% IF FND_KEY_CMND
      *<<<< FND_KEY_CMND
           IF SCR-CMND NOT = 'ING'
               MOVE FIO-GET-NLS TO FIO-CMND.
      *>>>> FND_KEY_CMND
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
           IF SCR-CMND = 'CON' AND
              FRM-IND-NEXT IN CCO-FLD = 'S'
                 MOVE FIO-GET-NXT TO FIO-CMND
                 PERFORM DAP-FIO-CCO
                 MOVE FIO-STAT TO PGM-STAT-CCO.
      *>>>> FIN_FND_KEY
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBGVSM.
       COPY GNSBGCPT.
       COPY GNSBGHOY. 
       COPY GNSBEGLS.
      *>>>>
*% END
