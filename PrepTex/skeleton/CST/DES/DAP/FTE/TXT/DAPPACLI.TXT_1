*% IF WSS_DTC
      *<<<< WSS_DTC
      *Entidades de SGC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RCC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-IMP-REQA==.
      *
       01   VAR-PROGRAMA.
            03 PGM-COD-CREB              VALUE ' '       PIC X(03).
            03 PGM-STAT-RCC.
               88 PGM-STAT-RCC-NUL       VALUE '  '.
               07 PGM-STT1-RCC           VALUE '2'       PIC X(01).
                  88 PGM-STAT-RCC-OKS    VALUE '0'.
               07 PGM-STT2-RCC           VALUE '3'       PIC X(01).

      *>>>>
*% END
*% IF INI_FIO_IBM
      *<<<< INI_FIO
           PERFORM GET-FHOY.
           IF SCR-INST = WSS-GAB AND
              ( FRM-FFLD = FRM-FFLD-PF8 OR
                FRM-FFLD = FRM-FFLD-PF7 )
                  MOVE WSS-GKY TO SCR-INST.
      *>>>> INI_FIO
*% END
*% IF FND_KEY_CMND
      *<<<< FND_KEY_CMND
           IF FRM-COD-CREB IN CLI-FLD = WSS-OPE OR
              FRM-COD-CREB IN CLI-FLD = WSS-CLI
               MOVE WSS-N TO WSS-LEE-DBC-DESDE-LAD
               PERFORM OBTENER-NOMBRE-DBC.
           PERFORM SET-KEY-LAD.
      
           MOVE FIO-GET-NLS TO FIO-CMND.
      *>>>> FND_KEY_CMND
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
      *CORRECION NAVEGABILIDAD    
      *LECTURA NO OK
           IF NOT FIO-STAT-OKS AND FRM-COD-CREB IN CLI-FLD = WSS-FEC
               MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
               MOVE WSS-23         TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CLINOEXFECPK' TO MSG-COD-MENS
               MOVE 'DAP'          TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.

           IF NOT FIO-STAT-OKS AND FRM-COD-CREB IN CLI-FLD = WSS-OPE
               MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
               MOVE WSS-23         TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CLINONXTOIPE' TO MSG-COD-MENS
               MOVE 'DAP' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.      

           IF NOT FIO-STAT-OKS AND  FRM-COD-CREB IN CLI-FLD = WSS-AUT
               MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
               MOVE WSS-23         TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CLINONXTOAUT' TO MSG-COD-MENS
               MOVE 'DAP'          TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.

           IF NOT FIO-STAT-OKS AND  FRM-COD-CREB IN CLI-FLD = WSS-CLI
               MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
               MOVE WSS-23         TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CLINONXTOCLI' TO MSG-COD-MENS
               MOVE 'DAP'          TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.               
               
      *FIN CORRECION NAVEGABILIDAD
      
           IF NOT FIO-STAT-OKS
               GO TO FIN-FND-KEY.
      
      
      

           IF FRM-COD-CREB IN CLI-FLD = WSS-FEC
      *CORRECION NAVEGABILIDAD     
               IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI OR
                  LAD-NUM-DLAD IN CLI-FLD NOT =
                  LAD-NUM-DLAD IN LAD    OR
                  LAD-NUM-MLAD IN CLI-FLD NOT =
                  LAD-NUM-MLAD IN LAD    OR
                  LAD-NUM-SLAD IN CLI-FLD NOT =
                  LAD-NUM-SLAD IN LAD    OR
                  LAD-NUM-ALAD IN CLI-FLD NOT =
                  LAD-NUM-ALAD IN LAD
                    MOVE WSS-23 TO FIO-STAT
                    MOVE WSS-23 TO PGM-STAT-LAD
                    MOVE WSS-CLINOEXFECPK TO MSG-COD-MENS
                    MOVE WSS-DAP TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
           
           
           IF FRM-COD-CREB IN CLI-FLD = WSS-OPE
               IF LAD-COD-ENTO IN LAD NOT = WSS-COD-ENTI OR
                  LAD-CAI-IOPD IN CLI-FLD NOT =
                  LAD-CAI-IOPD IN LAD    OR
                  LAD-IIC-IOPD IN CLI-FLD NOT =
                  LAD-IIC-IOPD IN LAD
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE WSS-23 TO FIO-STAT
                    MOVE WSS-23 TO PGM-STAT-LAD
                    MOVE WSS-CLINONXTOIPE TO MSG-COD-MENS
                    MOVE WSS-DAP TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
      
           IF FRM-COD-CREB IN CLI-FLD = WSS-AUT
               IF LAD-COD-ENTA IN LAD NOT = WSS-COD-ENTI OR
                  LAD-COD-AUTO IN CLI-FLD NOT =
                  LAD-COD-AUTO IN LAD
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE WSS-23 TO FIO-STAT
                    MOVE WSS-23 TO PGM-STAT-LAD
                    MOVE WSS-CLINONXTOAUT TO MSG-COD-MENS
                    MOVE WSS-DAP TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
      
           IF FRM-COD-CREB IN CLI-FLD = WSS-CLI AND
              WSS-EXI-CLI NOT = ZEROES
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE WSS-23         TO FIO-STAT
                    MOVE WSS-23         TO PGM-STAT-LAD
                    MOVE WSS-CLINEX TO MSG-COD-MENS
                    MOVE WSS-SGC        TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
      
           IF FRM-COD-CREB IN CLI-FLD = WSS-CLI
               IF LAD-COD-ENTC IN LAD NOT = WSS-COD-ENTI OR
                  LAD-CIC-ICLI IN LAD     NOT =
                  DBC-CIC-ICLI IN DBC
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE WSS-23 TO FIO-STAT
                    MOVE WSS-23 TO PGM-STAT-LAD
                    MOVE WSS-CLINONXTOCLI TO MSG-COD-MENS
                    MOVE WSS-DAP TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.

           IF NOT ( FRM-COD-CREB IN CLI-FLD = WSS-OPE OR
                    FRM-COD-CREB IN CLI-FLD = WSS-CLI )
                     MOVE WSS-S TO WSS-LEE-DBC-DESDE-LAD
                     PERFORM OBTENER-NOMBRE-DBC.
      
           MOVE LAD-CIC-ICLI IN LAD TO WSS-CIC-ICLI.                    
      *FIN CORRECION NAVEGABILIDAD
           IF SCR-CMND = WSS-CON
              IF FRM-FFLD = FRM-FFLD-PF8
                    PERFORM SET-KEY-LAD
                    PERFORM PRO-PF8
              ELSE
                IF FRM-FFLD = FRM-FFLD-PF7
                    PERFORM SET-KEY-LAD
                    PERFORM PRO-PF7.
      
           IF PGM-STAT-LAD-OKS
           IF FRM-FFLD = FRM-FFLD-PF7 OR
              FRM-FFLD = FRM-FFLD-PF8
               MOVE WSS-S TO WSS-LEE-DBC-DESDE-LAD
               PERFORM OBTENER-NOMBRE-DBC.
      
           IF PGM-STAT-LAD-OKS
               IF LAD-COD-ENTF IN LAD = WSS-COD-ENTI
                   MOVE WSS-00   TO FIO-STAT

                   MOVE SPACES TO IMP

                   MOVE LAD-COD-CONT IN LAD TO IMP
                   MOVE WSS-00   TO PGM-STAT-DBC
                   MOVE WSS-00   TO PGM-STAT-IMP.
      
      *>>>> FIN_FND_KEY
*% END
*% IF INI_SCR_DMSG
*% IF PGM_PQ
      *<<<< INI_SCR_DMSG
      * Con este codigo logramos que una vez terminada la consulta
      * las claves no se borren de la pantalla
           IF SCR-CMND = WSS-CON
                AND
              SCR-INST = SCR-INST-GKY
                AND
              FRM-FFLD NOT = FRM-FFLD-RET
                  MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR.
      *>>>> INI_SCR_DMSG
*% END
*% END
*% IF FIN_SCR_TRL
      *<<<< FIN_SCR_TRL
      
           IF PGM-STAT-LAD-OKS
               MOVE WSS-CLIPF7PF8 TO MSG-COD-MENS
               MOVE WSS-GNS TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
      
      *>>>> FIN_SCR_TRL
*% END
*% IF PFS_SCR_GKEY_PREV
      *<<<< PFS_SCR_GKEY_PREV
           IF NOT ( FRM-FFLD = FRM-FFLD-PF7 )
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 )
      *>>>> PFS_SCR_GKEY_PREV
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF7 )
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 )
      *>>>> PFS_GET_CON
*% END
*% IF POST_SCR_PUTA
      *<<<< POST_SCR_PUTA
           IF PGM-STAT-LAD-OKS
               IF LAD-COD-INST = WSS-ANT
               
                    MOVE WSS-DAP TO TAB-COD-SIST
                    MOVE WSS-DAP TO FIO-SIST
                    PERFORM SET-KEY-LAD
                    MOVE FIO-GET-NXT TO FIO-CMND
                    PERFORM DAP-FIO-LAD
                    IF FIO-STAT-OKS
                         IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI
                            MOVE WSS-23  TO FIO-STAT
                            MOVE FIO-STAT TO PGM-STAT-LAD
                         ELSE


                             MOVE LAD-COD-CONT IN LAD TO IMP-CPR
                             MOVE WSS-S TO WSS-COMPARA
                             GO TO FIN-AT-DES.
      
           IF PGM-STAT-LAD-OKS
               IF LAD-COD-INST = WSS-DES

                     MOVE WSS-DAP TO TAB-COD-SIST
                     MOVE WSS-DAP TO FIO-SIST
                     PERFORM SET-KEY-LAD
                     MOVE FIO-GET-PRV TO FIO-CMND
                     PERFORM DAP-FIO-LAD
                     IF FIO-STAT-OKS
                          IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI
                               MOVE WSS-23 TO FIO-STAT
                               MOVE FIO-STAT TO PGM-STAT-LAD
                          ELSE

                               MOVE LAD-COD-CONT IN LAD TO IMP-CPR
                               MOVE WSS-S TO WSS-COMPARA.
       FIN-AT-DES.
      
      *     ==>>  01
      *      
           MOVE WSS-TX01            TO FRM-GLS-TX01     IN CLI-FLD.
           MOVE FRM-CPIM-AEY        TO FRM-GLS-TX01-ATR IN CLI-FLD.
           MOVE FRM-CPIM-AEY        TO IMP-GLS-DISP-ATR IN CLI-FLD.
           IF WSS-COMPARA = WSS-S
               IF IMP-GLS-DISP IN IMP NOT =
                  IMP-GLS-DISP IN IMP-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX01-ATR IN CLI-FLD
                   MOVE FRM-CPIM-ABY TO IMP-GLS-DISP-ATR IN CLI-FLD.
      

      *     ==>>  02
      *      
           MOVE WSS-TX02          TO FRM-GLS-TX02     IN CLI-FLD.
           MOVE FRM-CPIM-AEY      TO FRM-GLS-TX02-ATR IN CLI-FLD.
           MOVE FRM-CPIM-AEY      TO IMP-CAI-ISER-ATR IN CLI-FLD.
           MOVE FRM-CPIM-AEY      TO IMP-IIC-ISER-ATR IN CLI-FLD.
             IF WSS-COMPARA = WSS-S
               IF (IMP-CAI-ISER IN IMP NOT =
                  IMP-CAI-ISER IN IMP-CPR)                  
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX02-ATR IN CLI-FLD
                   MOVE FRM-CPIM-ABY TO IMP-IIC-ISER-ATR IN CLI-FLD
                   MOVE FRM-CPIM-ABY TO IMP-CAI-ISER-ATR IN CLI-FLD.
             IF WSS-COMPARA = WSS-S
               IF (IMP-IIC-ISER IN IMP NOT =
                  IMP-IIC-ISER IN IMP-CPR)                  
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX02-ATR IN CLI-FLD
                   MOVE FRM-CPIM-ABY TO IMP-IIC-ISER-ATR IN CLI-FLD
                   MOVE FRM-CPIM-ABY TO IMP-CAI-ISER-ATR IN CLI-FLD.


      *     ==>>  03
      *      
           MOVE WSS-TX03        TO FRM-GLS-TX03 IN CLI-FLD.
           MOVE FRM-CPIM-AEY    TO FRM-GLS-TX03-ATR IN CLI-FLD.
           MOVE FRM-CPIM-AEY    TO IMP-COD-STAT-ATR IN CLI-FLD.
           IF WSS-COMPARA = WSS-S
               IF IMP-COD-STAT IN IMP NOT =
                  IMP-COD-STAT IN IMP-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX03-ATR IN CLI-FLD
                   MOVE FRM-CPIM-ABY TO IMP-COD-STAT-ATR IN CLI-FLD.


      *     ==>>  04
      *      
           MOVE WSS-TX04        TO FRM-GLS-TX04 IN CLI-FLD.
           MOVE FRM-CPIM-AEY    TO FRM-GLS-TX04-ATR IN CLI-FLD.
           MOVE FRM-CPIM-AEY    TO IMP-STP-ITRN-ATR IN CLI-FLD.
           IF WSS-COMPARA = WSS-S
               IF IMP-STP-ITRN IN IMP NOT =
                  IMP-STP-ITRN IN IMP-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX04-ATR IN CLI-FLD
                   MOVE FRM-CPIM-ABY TO IMP-STP-ITRN-ATR IN CLI-FLD.

      *     ==>>  05
      *      
           MOVE WSS-TX05          TO FRM-GLS-TX05     IN CLI-FLD.
           MOVE FRM-CPIM-AEY      TO FRM-GLS-TX05-ATR IN CLI-FLD.
           MOVE FRM-CPIM-AEY      TO IMP-COD-TACC-ATR IN CLI-FLD.
           IF WSS-COMPARA = WSS-S
               IF IMP-COD-TACC IN IMP NOT =
                  IMP-COD-TACC IN IMP-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX05-ATR IN CLI-FLD
                   MOVE FRM-CPIM-ABY TO IMP-COD-TACC-ATR IN CLI-FLD.

      *     ==>>  06
      *      
           MOVE WSS-TX06         TO FRM-GLS-TX06     IN CLI-FLD.
           MOVE FRM-CPIM-AEY     TO FRM-GLS-TX06-ATR IN CLI-FLD.
           MOVE FRM-CPIM-AEY     TO IMP-COD-OTRN-ATR IN CLI-FLD.
             IF WSS-COMPARA = WSS-S
               IF IMP-COD-OTRN IN IMP NOT =
                  IMP-COD-OTRN IN IMP-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX06-ATR IN CLI-FLD
                   MOVE FRM-CPIM-ABY TO IMP-COD-OTRN-ATR IN CLI-FLD.

      *     ==>>  07
      *      
           MOVE WSS-TX07         TO FRM-GLS-TX07     IN CLI-FLD.
           MOVE FRM-CPIM-AEY     TO FRM-GLS-TX07-ATR IN CLI-FLD.
           MOVE FRM-CPIM-AEY     TO IMP-COD-ATRN-ATR IN CLI-FLD.

           
