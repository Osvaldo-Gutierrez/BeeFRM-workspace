*% IF INI_CMR_FEC_FCMR
      *<<<< INI_CMR_FEC_FCMR
           MOVE FEC-CHOY-N TO FEC-CHOY.
           MOVE FEC-VHBL-N TO FEC-VHBL.
      *>>>> INI_CMR_FEC_FCMR
*% END
*% IF INI_PUT_CMR_CMR
      *<<<< INI_PUT_CMR_CMR
           GO TO SIGE.
      *>>>> END
*% END
*% IF INI_VAL_CON_KEY_CMR
      *<<<<
           IF     CMR-FEC-FCMR IN CMR-FLD NOT > SPACES                  
              AND CMR-NUM-DCMR IN CMR-FLD NOT > SPACES                  
              AND CMR-NUM-MCMR IN CMR-FLD NOT > SPACES                  
              AND CMR-NUM-SCMR IN CMR-FLD NOT > SPACES                  
              AND CMR-NUM-ACMR IN CMR-FLD NOT > SPACES                  
              AND CMR-COD-VCAM IN CMR-FLD NOT > SPACES                  
                PERFORM ERR-KEY                                         
                PERFORM FST-KEY-CMR.                                    
            GO TO FIN-VAL-CON-KEY-CMR.
      *>>>>
*% END
*% IF INI_VAL_CON_IKY_CMR
      *<<<<
           IF     CMR-FEC-FCMR IN CMR-FLD NOT > SPACES                  
              AND CMR-COD-VCAM IN CMR-FLD NOT > SPACES                  
                PERFORM ERR-KEY                                         
                PERFORM FST-KEY-CMR.                                    
           GO TO FIN-VAL-CON-IKY-CMR.
      *>>>>
*% END
*% IF FIN_PUT_CMR_CMR
      *<<<< FIN_PUT_CMR_CMR
       SIGE.
           IF CMR-COD-VCAM IN CMR-FLD = '*'
               MOVE 'DAP' TO MSG-COD-SIST
               MOVE 'TODAS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-GLS-VCAM IN CMR-FLD
               GO TO FIN-PUT-CMR-CMR.

      *BUS-TAB busca glosa de codigo en tabla
           IF CMR-COD-VCAM IN CMR-FLD > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'TMS' TO TAB-COD-TTAB IN TAB
               MOVE CMR-COD-VCAM IN CMR-FLD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-VCAM IN CMR-FLD
           ELSE
               MOVE SPACES TO FRM-GLS-VCAM IN CMR-FLD.
      *>>>> END
*% END
*% IF CMR_COD_VCAM
      *<<<< CMR_COD_VCAM
           IF CMR-COD-VCAM IN CMR-FLD = '*'
               MOVE 'DAP' TO MSG-COD-SIST
               MOVE 'TODAS' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-GLS-VCAM IN CMR-FLD
               GO TO FIN-VAL-CMR-COD-VCAM.
      *>>>> END
*% END
