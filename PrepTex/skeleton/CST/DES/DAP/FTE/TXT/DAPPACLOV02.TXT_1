                  MOVE FRM-CPIM-ABY TO OPD-CAI-IOPD-ATR IN CLO-FLD.
      
           IF WSS-COMPARA = 'S'
              IF OPD-IIC-IOPD IN OPD NOT =
                 OPD-IIC-IOPD IN OPD-CPR
                  MOVE FRM-CPIM-ABY TO FRM-GLS-TX01-ATR IN CLO-FLD
                  MOVE FRM-CPIM-ABY TO OPD-IIC-IOPD-ATR IN CLO-FLD.
      
      
           MOVE 'Ser'           TO  FRM-GLS-TX02 IN CLO-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-TX02-ATR IN CLO-FLD.
      
           MOVE FRM-CPIM-AEY    TO OPD-CAI-ISER-ATR IN CLO-FLD.
           MOVE FRM-CPIM-AEY    TO OPD-IIC-ISER-ATR IN CLO-FLD.
           IF WSS-COMPARA = 'S'
              IF OPD-CAI-ISER IN OPD NOT =
                 OPD-CAI-ISER IN OPD-CPR
                  MOVE FRM-CPIM-ABY TO FRM-GLS-TX02-ATR IN CLO-FLD
                  MOVE FRM-CPIM-ABY TO OPD-CAI-ISER-ATR IN CLO-FLD.
      
           IF WSS-COMPARA = 'S'
              IF OPD-IIC-ISER IN OPD NOT =
                 OPD-IIC-ISER IN OPD-CPR
                  MOVE FRM-CPIM-ABY TO FRM-GLS-TX02-ATR IN CLO-FLD
                  MOVE FRM-CPIM-ABY TO OPD-IIC-ISER-ATR IN CLO-FLD.
      
           MOVE 'Mon/Tio/Aux'   TO  FRM-GLS-TX03     IN CLO-FLD.
           MOVE  FRM-CPIM-AEY   TO  FRM-GLS-TX03-ATR IN CLO-FLD.
      
           MOVE  FRM-CPIM-AEY   TO  OPD-COD-VCAM-ATR IN CLO-FLD.
           IF WSS-COMPARA = 'S'
              IF OPD-COD-VCAM IN OPD NOT =
                 OPD-COD-VCAM IN OPD-CPR
                  MOVE FRM-CPIM-ABY TO FRM-GLS-TX03-ATR IN CLO-FLD
                  MOVE FRM-CPIM-ABY TO OPD-COD-VCAM-ATR IN CLO-FLD.
      
           MOVE  FRM-CPIM-AEY   TO  OPD-COD-TOPD-ATR IN CLO-FLD.
           IF WSS-COMPARA = 'S'
              IF OPD-COD-TOPD IN OPD NOT =
                 OPD-COD-TOPD IN OPD-CPR
                  MOVE FRM-CPIM-ABY TO FRM-GLS-TX03-ATR IN CLO-FLD
                  MOVE FRM-CPIM-ABY TO OPD-COD-TOPD-ATR IN CLO-FLD.
      
           MOVE  FRM-CPIM-AEY   TO  OPD-COD-AOPD-ATR IN CLO-FLD.
           IF WSS-COMPARA = 'S'
              IF OPD-COD-AOPD IN OPD NOT =
                 OPD-COD-AOPD IN OPD-CPR
                  MOVE FRM-CPIM-ABY TO FRM-GLS-TX03-ATR IN CLO-FLD
                  MOVE FRM-CPIM-ABY TO OPD-COD-AOPD-ATR IN CLO-FLD.
      
           MOVE 'Mcli/Idif'     TO  FRM-GLS-TX04 IN CLO-FLD.
           MOVE  FRM-CPIM-AEY   TO  FRM-GLS-TX04-ATR IN CLO-FLD.
      
           MOVE  FRM-CPIM-AEY   TO  OPD-IND-MCLI-ATR IN CLO-FLD.
           IF WSS-COMPARA = 'S'
              IF OPD-IND-MCLI IN OPD NOT =
                 OPD-IND-MCLI IN OPD-CPR
                  MOVE FRM-CPIM-ABY TO FRM-GLS-TX04-ATR IN CLO-FLD
                  MOVE FRM-CPIM-ABY TO OPD-IND-MCLI-ATR IN CLO-FLD.
      
           MOVE  FRM-CPIM-AEY   TO  OPD-IND-IDIF-ATR IN CLO-FLD.
           IF WSS-COMPARA = 'S'
              IF OPD-IND-IDIF IN OPD NOT =
                 OPD-IND-IDIF IN OPD-CPR
                  MOVE FRM-CPIM-ABY TO FRM-GLS-TX04-ATR IN CLO-FLD
                  MOVE FRM-CPIM-ABY TO OPD-IND-IDIF-ATR IN CLO-FLD.
      
           MOVE 'Ori'           TO  FRM-GLS-TX05 IN CLO-FLD.
           MOVE  FRM-CPIM-AEY   TO  FRM-GLS-TX05-ATR IN CLO-FLD.
      
           MOVE  FRM-CPIM-AEY   TO  OPD-CAI-ORIG-ATR IN CLO-FLD.
           IF WSS-COMPARA = 'S'
              IF OPD-CAI-ORIG IN OPD NOT =
                 OPD-CAI-ORIG IN OPD-CPR
                  MOVE FRM-CPIM-ABY TO FRM-GLS-TX05-ATR IN CLO-FLD
                  MOVE FRM-CPIM-ABY TO OPD-CAI-ORIG-ATR IN CLO-FLD.
      
           MOVE  FRM-CPIM-AEY   TO  OPD-IIC-ORIG-ATR IN CLO-FLD.
           IF WSS-COMPARA = 'S'
              IF OPD-IIC-ORIG IN OPD NOT =
                 OPD-IIC-ORIG IN OPD-CPR
                  MOVE FRM-CPIM-ABY TO FRM-GLS-TX05-ATR IN CLO-FLD
                  MOVE FRM-CPIM-ABY TO OPD-IIC-ORIG-ATR IN CLO-FLD.
      
           MOVE 'Ren'           TO  FRM-GLS-TX06 IN CLO-FLD.
           MOVE  FRM-CPIM-AEY   TO  FRM-GLS-TX06-ATR IN CLO-FLD.
      
           MOVE  FRM-CPIM-AEY   TO  OPD-CAI-GREN-ATR IN CLO-FLD.
           IF WSS-COMPARA = 'S'
              IF OPD-CAI-GREN IN OPD NOT =
