          03 WSS-S          VALUE 'S'       PIC X(01).
          03 WSS-C          VALUE 'C'       PIC X(01).
          03 WSS-CLINEX     VALUE 'CLI    NEX  '         PIC X(12).
          03 WSS-LADKEYOLAD VALUE 'LAD-KEY-OLAD'         PIC X(12).
          03 WSS-LADKEYCLAD VALUE 'LAD-KEY-CLAD'         PIC X(12).
          03 WSS-LADKEYALAD VALUE 'LAD-KEY-ALAD'         PIC X(12).
          03 WSS-DBCIDCICLI VALUE 'DBC-IDC-ICLI'         PIC X(12).
          03 WSS-RCCKEYIOPD VALUE 'RCC-KEY-IOPD'         PIC X(12).
          03 WSS-CLNOINGRFEC VALUE 'CL NOINGRFEC'        PIC X(12).
          03 WSS-CLNOINGROPE VALUE 'CL NOINGROPE'        PIC X(12).
          03 WSS-CLNOINGRCLI VALUE 'CL NOINGRCLI'        PIC X(12).
          03 WSS-CLNOINGRAUT VALUE 'CL NOINGRAUT'        PIC X(12).
          03 WSS-CLANOEXFECPK VALUE 'CLANOEXFECPK'       PIC X(12).
          03 WSS-CLANONXTOIPE VALUE 'CLANONXTOIPE'       PIC X(12).
          03 WSS-CLANONXTOAUT VALUE 'CLANONXTOAUT'       PIC X(12).
          03 WSS-CLANONXTOCLI VALUE 'CLANONXTOCLI'       PIC X(12).
          03 WSS-CLAPF7PF8    VALUE 'CLAPF7PF8 '         PIC X(10).
          03 WSS-CLANOPRV     VALUE 'CLANOPRV'           PIC X(08).
          03 WSS-CLANOPRVOPE  VALUE 'CLANOPRVOPE'        PIC X(11).
          03 WSS-CLANOPRVAUT  VALUE 'CLANOPRVAUT'        PIC X(11).
          03 WSS-CLANOPRVFEC  VALUE 'CLANOPRVFEC'        PIC X(11).
          03 WSS-CLANOPRVCLI  VALUE 'CLANOPRVCLI'        PIC X(11).
          03 WSS-CLANONXT     VALUE 'CLANONXT'           PIC X(08).
          03 WSS-CLANONXTOPE  VALUE 'CLANONXTOPE'        PIC X(11).
          03 WSS-CLANONXTAUT  VALUE 'CLANONXTAUT'        PIC X(11).
          03 WSS-CLANONXTFEC  VALUE 'CLANONXTFEC'        PIC X(11).
          03 WSS-CLANONXTCLI  VALUE 'CLANONXTCLI'        PIC X(11).
          03 WSS-TX01         VALUE 'Moneda'             PIC X(15).
          03 WSS-TX02         VALUE 'Tipo Operacion'     PIC X(15).
          03 WSS-TX03         VALUE 'Cod Auxiliar'       PIC X(15).
          03 WSS-TX04         VALUE 'Numero Liquidacion' PIC X(20).
          03 WSS-TX05         VALUE 'Numero Movimiento'  PIC X(20).
          03 WSS-TX06         VALUE 'Cargo a'            PIC X(20).
          03 WSS-TX07         VALUE 'Cuenta de Cargo'    PIC X(20).
          03 WSS-TX08         VALUE 'Monto Cargo'        PIC X(20).
          03 WSS-TX09         VALUE 'Cargo en Pesos'     PIC X(20).
          03 WSS-CIC-VCAM.
             05 WSS-CIC-TVAL                             PIC X(01).
             05 WSS-CIC-CVAL                             PIC X(03).
             05 WSS-CIC-TCAM                             PIC X(08).
          03 WSS-SGV-VCAM                          PIC 9(11)V9(04).
          03 WSS-MPESOS       VALUE '0991  '            PIC  X(06).

          
      *FIN CORRECION NAVEGABILIDAD          
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
      *Entidades de SGC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RCC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OPD-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CAD-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CAM-REQA==.              
      *
       01   VAR-PROGRAMA.
            03 PGM-COD-CREB              VALUE ' '       PIC X(03).
            03 PGM-STAT-RCC.
               88 PGM-STAT-RCC-NUL       VALUE '  '.
               07 PGM-STT1-RCC           VALUE '2'       PIC X(01).
                  88 PGM-STAT-RCC-OKS    VALUE '0'.
               07 PGM-STT2-RCC           VALUE '3'       PIC X(01).

      *>>>>
*% END
*% IF INI_FIO_IBM
      *<<<< INI_FIO
           PERFORM GET-FHOY.
           IF SCR-INST = WSS-GAB AND
              ( FRM-FFLD = FRM-FFLD-PF8 OR
                FRM-FFLD = FRM-FFLD-PF7 )
                  MOVE WSS-GKY TO SCR-INST.
      *>>>> INI_FIO
*% END
*% IF FND_KEY_CMND
      *<<<< FND_KEY_CMND
           IF FRM-COD-CREB IN CLA-FLD = WSS-OPE OR
              FRM-COD-CREB IN CLA-FLD = WSS-CLI
               MOVE WSS-N TO WSS-LEE-DBC-DESDE-LAD
               PERFORM OBTENER-NOMBRE-DBC.
           PERFORM SET-KEY-LAD.
      
           MOVE FIO-GET-NLS TO FIO-CMND.
      *>>>> FND_KEY_CMND
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
      *CORRECION NAVEGABILIDAD    
      *LECTURA NO OK
           IF NOT FIO-STAT-OKS AND FRM-COD-CREB IN CLA-FLD = WSS-FEC
               MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
               MOVE WSS-23         TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CLANOEXFECPK' TO MSG-COD-MENS
               MOVE 'DAP'          TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.

           IF NOT FIO-STAT-OKS AND FRM-COD-CREB IN CLA-FLD = WSS-OPE
               MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
               MOVE WSS-23         TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CLANONXTOIPE' TO MSG-COD-MENS
               MOVE 'DAP' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.      

           IF NOT FIO-STAT-OKS AND  FRM-COD-CREB IN CLA-FLD = WSS-AUT
               MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
               MOVE WSS-23         TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CLANONXTOAUT' TO MSG-COD-MENS
               MOVE 'DAP'          TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.

           IF NOT FIO-STAT-OKS AND  FRM-COD-CREB IN CLA-FLD = WSS-CLI
               MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
               MOVE WSS-23         TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CLANONXTOCLI' TO MSG-COD-MENS
               MOVE 'DAP'          TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.               
               
      *FIN CORRECION NAVEGABILIDAD
      
           IF NOT FIO-STAT-OKS
               GO TO FIN-FND-KEY.
      
      
      

           IF FRM-COD-CREB IN CLA-FLD = WSS-FEC
      *CORRECION NAVEGABILIDAD     
               IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI OR
                  LAD-NUM-DLAD IN CLA-FLD NOT =
                  LAD-NUM-DLAD IN LAD    OR
                  LAD-NUM-MLAD IN CLA-FLD NOT =
                  LAD-NUM-MLAD IN LAD    OR
                  LAD-NUM-SLAD IN CLA-FLD NOT =
                  LAD-NUM-SLAD IN LAD    OR
                  LAD-NUM-ALAD IN CLA-FLD NOT =
                  LAD-NUM-ALAD IN LAD
                    MOVE WSS-23 TO FIO-STAT
                    MOVE WSS-23 TO PGM-STAT-LAD
                    MOVE WSS-CLANOEXFECPK TO MSG-COD-MENS
                    MOVE WSS-DAP TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
           
           
           IF FRM-COD-CREB IN CLA-FLD = WSS-OPE
               IF LAD-COD-ENTO IN LAD NOT = WSS-COD-ENTI OR
                  LAD-CAI-IOPD IN CLA-FLD NOT =
                  LAD-CAI-IOPD IN LAD    OR
                  LAD-IIC-IOPD IN CLA-FLD NOT =
                  LAD-IIC-IOPD IN LAD
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE WSS-23 TO FIO-STAT
                    MOVE WSS-23 TO PGM-STAT-LAD
                    MOVE WSS-CLANONXTOIPE TO MSG-COD-MENS
                    MOVE WSS-DAP TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
      
           IF FRM-COD-CREB IN CLA-FLD = WSS-AUT
               IF LAD-COD-ENTA IN LAD NOT = WSS-COD-ENTI OR
                  LAD-COD-AUTO IN CLA-FLD NOT =
                  LAD-COD-AUTO IN LAD
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE WSS-23 TO FIO-STAT
                    MOVE WSS-23 TO PGM-STAT-LAD
                    MOVE WSS-CLANONXTOAUT TO MSG-COD-MENS
                    MOVE WSS-DAP TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
      
           IF FRM-COD-CREB IN CLA-FLD = WSS-CLI AND
              WSS-EXI-CLI NOT = ZEROES
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE WSS-23         TO FIO-STAT
                    MOVE WSS-23         TO PGM-STAT-LAD
                    MOVE WSS-CLINEX TO MSG-COD-MENS
                    MOVE WSS-SGC        TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
      
           IF FRM-COD-CREB IN CLA-FLD = WSS-CLI
               IF LAD-COD-ENTC IN LAD NOT = WSS-COD-ENTI OR
                  LAD-CIC-ICLI IN LAD     NOT =
                  DBC-CIC-ICLI IN DBC
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE WSS-23 TO FIO-STAT
                    MOVE WSS-23 TO PGM-STAT-LAD
                    MOVE WSS-CLANONXTOCLI TO MSG-COD-MENS
                    MOVE WSS-DAP TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
                    
           IF NOT ( FRM-COD-CREB IN CLA-FLD = WSS-OPE OR
                    FRM-COD-CREB IN CLA-FLD = WSS-CLI )
                     MOVE WSS-S TO WSS-LEE-DBC-DESDE-LAD
                     PERFORM OBTENER-NOMBRE-DBC.
      
           MOVE LAD-CIC-ICLI IN LAD TO WSS-CIC-ICLI.                    
      *FIN CORRECION NAVEGABILIDAD
           IF SCR-CMND = WSS-CON
              IF FRM-FFLD = FRM-FFLD-PF8
                    PERFORM SET-KEY-LAD
                    PERFORM PRO-PF8
              ELSE
                IF FRM-FFLD = FRM-FFLD-PF7
                    PERFORM SET-KEY-LAD
                    PERFORM PRO-PF7.
      
           IF PGM-STAT-LAD-OKS
           IF FRM-FFLD = FRM-FFLD-PF7 OR
              FRM-FFLD = FRM-FFLD-PF8
               MOVE WSS-S TO WSS-LEE-DBC-DESDE-LAD
               PERFORM OBTENER-NOMBRE-DBC.
      
           IF PGM-STAT-LAD-OKS
               IF LAD-COD-ENTF IN LAD = WSS-COD-ENTI
                   MOVE WSS-00   TO FIO-STAT

                   MOVE SPACES TO CAD

                   MOVE LAD-COD-CONT IN LAD TO CAD
                   MOVE WSS-00   TO PGM-STAT-OPD
                   MOVE WSS-00   TO PGM-STAT-DBC                   
                   MOVE WSS-00   TO PGM-STAT-CAD.
      
      *>>>> FIN_FND_KEY
*% END
*% IF INI_SCR_DMSG
*% IF PGM_PQ
      *<<<< INI_SCR_DMSG
      * Con este codigo logramos que una vez terminada la consulta
      * las claves no se borren de la pantalla
           IF SCR-CMND = WSS-CON
                AND
              SCR-INST = SCR-INST-GKY
                AND
              FRM-FFLD NOT = FRM-FFLD-RET
                  MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR.
      *>>>> INI_SCR_DMSG
*% END
*% END
*% IF FIN_SCR_TRL
      *<<<< FIN_SCR_TRL
      
           IF PGM-STAT-LAD-OKS
               MOVE WSS-CLAPF7PF8 TO MSG-COD-MENS
               MOVE WSS-GNS TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
      
      *>>>> FIN_SCR_TRL
*% END
*% IF PFS_SCR_GKEY_PREV
      *<<<< PFS_SCR_GKEY_PREV
           IF NOT ( FRM-FFLD = FRM-FFLD-PF7 )
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 )
      *>>>> PFS_SCR_GKEY_PREV
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF7 )
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 )
      *>>>> PFS_GET_CON
*% END
*% IF POST_SCR_PUTA
      *<<<< POST_SCR_PUTA
           IF PGM-STAT-LAD-OKS
               IF LAD-COD-INST = WSS-ANT
               
                    MOVE WSS-DAP TO TAB-COD-SIST
                    MOVE WSS-DAP TO FIO-SIST
                    PERFORM SET-KEY-LAD
                    MOVE FIO-GET-NXT TO FIO-CMND
                    PERFORM DAP-FIO-LAD
                    IF FIO-STAT-OKS
                         IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI
                            MOVE WSS-23  TO FIO-STAT
                            MOVE FIO-STAT TO PGM-STAT-LAD
                         ELSE


                             MOVE LAD-COD-CONT IN LAD TO CAD-CPR
                             MOVE WSS-S TO WSS-COMPARA
                             GO TO FIN-AT-DES.
      
           IF PGM-STAT-LAD-OKS
               IF LAD-COD-INST = WSS-DES

                     MOVE WSS-DAP TO TAB-COD-SIST
                     MOVE WSS-DAP TO FIO-SIST
                     PERFORM SET-KEY-LAD
                     MOVE FIO-GET-PRV TO FIO-CMND
                     PERFORM DAP-FIO-LAD
                     IF FIO-STAT-OKS
                          IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI
                               MOVE WSS-23 TO FIO-STAT
                               MOVE FIO-STAT TO PGM-STAT-LAD
                          ELSE

                               MOVE LAD-COD-CONT IN LAD TO CAD-CPR
                               MOVE WSS-S TO WSS-COMPARA.
       FIN-AT-DES.
      
      *     ==>>  01
      *      
           MOVE WSS-TX01            TO FRM-GLS-TX01     IN CLA-FLD.
           MOVE FRM-CPIM-AEY        TO FRM-GLS-TX01-ATR IN CLA-FLD.
      
           MOVE FRM-CPIM-AEY        TO OPD-COD-VCAM-ATR IN CLA-FLD.
      

      *     ==>>  02
      *      
           MOVE WSS-TX02          TO FRM-GLS-TX02     IN CLA-FLD.
           MOVE FRM-CPIM-AEY      TO FRM-GLS-TX02-ATR IN CLA-FLD.
      
           MOVE FRM-CPIM-AEY      TO OPD-COD-TOPD-ATR IN CLA-FLD.


      *     ==>>  03
      *      
           MOVE WSS-TX03        TO FRM-GLS-TX03 IN CLA-FLD.
           MOVE FRM-CPIM-AEY    TO FRM-GLS-TX03-ATR IN CLA-FLD.
      
           MOVE FRM-CPIM-AEY    TO OPD-COD-AOPD-ATR IN CLA-FLD.


      *     ==>>  04
      *      
           MOVE WSS-TX04        TO FRM-GLS-TX04 IN CLA-FLD.
           MOVE FRM-CPIM-AEY    TO FRM-GLS-TX04-ATR IN CLA-FLD.
           MOVE FRM-CPIM-AEY    TO CAD-NUM-ILIQ-ATR IN CLA-FLD.
           IF WSS-COMPARA = WSS-S
               IF CAD-NUM-ILIQ IN CAD NOT =
                  CAD-NUM-ILIQ IN CAD-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX04-ATR IN CLA-FLD
                   MOVE FRM-CPIM-ABY TO CAD-NUM-ILIQ-ATR IN CLA-FLD.

      *     ==>>  05
      *      
           MOVE WSS-TX05          TO FRM-GLS-TX05     IN CLA-FLD.
           MOVE FRM-CPIM-AEY      TO FRM-GLS-TX05-ATR IN CLA-FLD.
           MOVE FRM-CPIM-AEY      TO CAD-NUM-ICAD-ATR IN CLA-FLD.
           IF WSS-COMPARA = WSS-S
               IF CAD-NUM-ICAD IN CAD NOT =
                  CAD-NUM-ICAD IN CAD-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX05-ATR IN CLA-FLD
                   MOVE FRM-CPIM-ABY TO CAD-NUM-ICAD-ATR IN CLA-FLD.

      *     ==>>  06
      *      
           MOVE WSS-TX06         TO FRM-GLS-TX06     IN CLA-FLD.
           MOVE FRM-CPIM-AEY     TO FRM-GLS-TX06-ATR IN CLA-FLD.
           MOVE FRM-CPIM-AEY     TO CAD-IND-TCAD-ATR IN CLA-FLD.
             IF WSS-COMPARA = WSS-S
               IF CAD-IND-TCAD IN CAD NOT =
                  CAD-IND-TCAD IN CAD-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX06-ATR IN CLA-FLD
                   MOVE FRM-CPIM-ABY TO CAD-IND-TCAD-ATR IN CLA-FLD.

      *     ==>>  07
      *      
           MOVE WSS-TX07         TO FRM-GLS-TX07     IN CLA-FLD.
           MOVE FRM-CPIM-AEY     TO FRM-GLS-TX07-ATR IN CLA-FLD.
           MOVE FRM-CPIM-AEY     TO CAD-CAI-ICAD-ATR IN CLA-FLD.
           MOVE FRM-CPIM-AEY     TO CAD-IIC-ICAD-ATR IN CLA-FLD.
           
             IF WSS-COMPARA = WSS-S
               IF (CAD-CAI-ICAD IN CAD NOT =
                  CAD-CAI-ICAD IN CAD-CPR)                  
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX07-ATR IN CLA-FLD
                   MOVE FRM-CPIM-ABY TO CAD-IIC-ICAD-ATR IN CLA-FLD
                   MOVE FRM-CPIM-ABY TO CAD-CAI-ICAD-ATR IN CLA-FLD.
             IF WSS-COMPARA = WSS-S
              IF (CAD-IIC-ICAD IN CAD NOT =
                  CAD-IIC-ICAD IN CAD-CPR)                  
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX07-ATR IN CLA-FLD
                   MOVE FRM-CPIM-ABY TO CAD-IIC-ICAD-ATR IN CLA-FLD
                   MOVE FRM-CPIM-ABY TO CAD-CAI-ICAD-ATR IN CLA-FLD.                   

      *     ==>>  08
      *      
           MOVE WSS-TX08         TO FRM-GLS-TX08     IN CLA-FLD.
           MOVE FRM-CPIM-AEY     TO FRM-GLS-TX08-ATR IN CLA-FLD.
           MOVE FRM-CPIM-AEY     TO CAD-SGV-LIQU-ATR IN CLA-FLD.

             IF WSS-COMPARA = WSS-S
               IF (CAD-SGV-LIQU IN CAD NOT =
                  CAD-SGV-LIQU IN CAD-CPR) 
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX08-ATR IN CLA-FLD
                   MOVE FRM-CPIM-ABY TO CAD-SGV-LIQU-ATR IN CLA-FLD.

                
      *     ==>>  09
      *      
           MOVE WSS-TX09         TO FRM-GLS-TX09     IN CLA-FLD.
           MOVE FRM-CPIM-AEY     TO FRM-GLS-TX09-ATR IN CLA-FLD.
           MOVE FRM-CPIM-AEY     TO FRM-VAL-VPES-ATR IN CLA-FLD.



                   
           MOVE CLA-FLD TO FRM-DFLD.
      
      
      *>>>> POST_SCR_PUTA
