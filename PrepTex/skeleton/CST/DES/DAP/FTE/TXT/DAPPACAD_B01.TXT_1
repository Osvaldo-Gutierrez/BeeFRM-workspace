*%! Codigo adicional programa DAPPACAD
*% NO_HDR_CST = TRUE
*% PGM_PTC = TRUE
*% IF SETTAG
*% FNCPQ = "080603"
*% VSNPQ = "08.06.26"
*% FNCPU = "080603"
*% VSNPU = "08.06.26"
*% END
*% LARGO_MI_COMMAREA = 500
*% IF PGM_INI_NO
      *<<<< PGM_INI_NO ESTO AHORA SE INHIBE POR TABLA DE CONGELAMIENTO
      *ISP: SI PF11 EN ENGANCHE ( FUNCION NEMO DISTINTA A CAD )
      *     IF EIBAID = '#' OR '.'
      *         MOVE DFHCOMMAREA TO SCR-VARI
      *         IF SCR-NEMO NOT = 'DAPACTCAD   '
      *             MOVE ' ' TO EIBAID.
      *>>>>
*% END
*% IF FIN_SCR_VARI
      *<<<< FIN_SCR_VARI
       COPY DAPWGENG.
      * ISP: VARIABLES EN CMMA, COMUNES PARA PROGRAMAS PI,PQ,PU ( OPD )
      *      OPD DEBE MANTENERLAS EN ENGANCHE
       COPY DAPWG005.
           02 SCR-CIC-ICLI                  PIC X(12).
      *>>>>
*% END
*% IF PUT_SCR
      *<<<< PUT_SCR
           IF PGM-STAT-OPD-OKS
               PERFORM GET-RCC.
      *>>>>
*% END
*% IF WSS 
      *<<<< WSS 
      *PTC-INI (RRM) 3-DEC-2002                                         
       COPY DAPMXW02.                                                   
       COPY DAPMXW05.                                                   
      *PTC-FIN (RRM) 3-DEC-2002                                         
      *Entidades de SGC
       COPY SGCBRDBC.
       COPY SGCBROPE.
       COPY SGCBRRCO.
      *Entidades de DAP
       COPY DAPBROPD.
       COPY DAPBRLIQ.
       COPY DAPBRRCC.
      *
      *Entidades de TAB
       COPY TABBRCAM.

      *Entidades de GNS
       COPY GNSWGCPT.
       COPY GNSWGSTR.
       COPY GNSWGPES.
       COPY GNSWGHOY.

       COPY DAPWGPMP.

       01  WSS-VARI.
      * GRUPO AL QUE PERTENECE TIPO DE CARGO ABONO
      * CAJA, AUTOMATICO O MANUAL.
           03 WSS-IND-GCAD                        PIC X(01).
      * ISP WSS-GLS-DAT1 SE DEJO AQUI, PUES SE OCUPA EN DIFERENTES
      *     INSTANCIAS DEL PROGRAMA ( SCR-VARI )
           03  WSS-GLS-DAT1                  PIC X(01).
           03  WSS-NUM-LEN3                  PIC 9(03).
           03  WSS-PGM-STAT.
              05 PGM-STAT-LIQ           VALUE '23'      PIC X(02).
                 88 PGM-STAT-LIQ-OKS    VALUE '00'.
                 88 PGM-STAT-LIQ-NUL    VALUE '  '.
           03 WSS-NUM-ICAD                   PIC 9(01).
           03 WSS-NUM-ICAD-ALF REDEFINES
              WSS-NUM-ICAD                   PIC X(01).
           03 WSS-ICOR                       PIC 9(02).
           03 WSS-VAL-IPES                        PIC 9(11)V9(04).
           03 WSS-CAPT                            PIC 9(11)V9(04).
           03 WSS-SGV-VCAM                        PIC 9(11)V9(04).
           03 WSS-CIC-VCAM.
              05 WSS-CIC-TVAL                     PIC X(01).
              05 WSS-CIC-CVAL                     PIC X(03).
              05 WSS-CIC-TCAM                     PIC X(08).
           
           03 PGM-STAT-PGM.
              05 PGM-STAT-RCC           VALUE '23'      PIC X(02).
                 88 PGM-STAT-RCC-OKS    VALUE '00'.
                 88 PGM-STAT-RCC-NUL    VALUE '  '.
      *>>>> WSS 
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
      *Entidades de SGC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OPE-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RCO-REQA==.
      *Entidades de DAP
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OPD-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-LIQ-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RCC-REQA==.
      *Entidades de TAB
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CAM-REQA==.
      *>>>>
*% END
*% IF FIN_SCR_TRL
      *<<<< FIN_SCR_TRL
           IF SCR-IND-ENG IN SCR-VEC-ENG(SCR-IPRO) = 'S'
               MOVE 'F10CADNINOPD' TO MSG-COD-MENS
               PERFORM MSG-PF10.
      *>>>>
*% END
*% IF INI_PRO_CONF
      *<<<< INI_PRO_CONF
           IF SCR-IND-ENG IN SCR-VEC-ENG(SCR-IPRO) = 'S'
               MOVE 'F10CADSINOPD' TO MSG-COD-MENS
               PERFORM MSG-PF10.
      *>>>>
*% END
*% IF SCR_GIKY_VAL_IKY
      *>>>> SCR_GIKY_VAL_IKY
      * PTC-INI (RRM) 3-DEC-2002                                        
           IF SCR-OFRM-SRV                                              
                                                                        
              MOVE PTC-OPD-TYPE TO QUE-TYPE                             
              MOVE TSK-TERM-ALF TO QUE-TERM                             
              MOVE PTC-OPD-LENG TO QUE-LITM                             
              MOVE 1            TO  QUE-NITM                            
              MOVE QUE-GET        TO QUE-CMND                           
              PERFORM GNS-PRO-QUE                                       
              IF QUE-STAT = QUE-STAT-OKS                                
                  MOVE QUE-ITEM TO PTC-OPD-ITEM                         
                  MOVE PTC-OPD-OPD-CAI-IOPD TO CAD-CAI-IOPD IN CAD-FLD  
                  MOVE PTC-OPD-OPD-IIC-IOPD TO CAD-IIC-IOPD IN CAD-FLD. 
      * PTC-FIN (RRM) 3-DEC-2002                                        
      *<<<< SCR_GIKY_VAL_IKY
*% END
*% IF FIN_SCR_GIKY OR FIN_SCR_GCNF OR FIN_SCR_GFLD
      *<<<< FIN_SCR_GIKY OR FIN_SCR_GCNF OR FIN_SCR_GFLD
           IF FRM-FFLD = FRM-FFLD-PF3 AND
              SCR-IND-ENG IN SCR-VEC-ENG(SCR-IPRO) = 'S'
               PERFORM EGR-FTER-CIC.
      *>>>>
*% END
*% IF FIN_CMN_ING OR SCR_GFLD_VAL_MKY
      *<<<< FIN_CMN_ING OR SCR_GFLD_VAL_MKY
           IF FRM-FFLD = FRM-FFLD-PF10 AND
              SCR-IND-ENG IN SCR-VEC-ENG(SCR-IPRO) = 'S'
               PERFORM EGR-FPEN-CIC.
      *>>>>
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           IF SCR-DESDE-CMN
               MOVE SPACES TO SCR-VEC-ENG
               MOVE 1 TO SCR-IPRO
               GO TO FIN-INI-FIO.
           IF SCR-INST-EXT = WSS-INST-FPEN-CIC
               MOVE WSS-INST-NULL TO SCR-INST-EXT
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'FLDING' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO SCR-MENS-EXT
               PERFORM ING-FPEN-CIC.
       FIN-INI-FIO.
      *>>>>
*% END
*% IF FND_KEY
      *<<<< FND_KEY
           IF SCR-CCPP = 'ING'
               PERFORM BUS-COR-CAD
               GO TO CON-FND-KEY.
           IF CAD-NUM-ICAD IN CAD-FLD = ZEROES
               PERFORM BUS-COR-CAD.
       CON-FND-KEY.
      *>>>>
*% END
*% IF FIN_SCR_TRL
      *<<<< FIN_SCR_TRL
*% IF PGM_PQ
      *<<<< PGM_PQ
           IF SCR-NMAP = 'DAPFCAD'
               IF ( OPD-COD-STAT IN OPD = 'A' OR 'G'  ) AND
                    SCR-CMND NOT = 'IAL'
                   MOVE 'DAP' TO MSG-COD-SIST
                   MOVE 'OPD    NIV' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE SCR-STAT-CON TO SCR-STAT.
           IF SCR-NMAP = 'DAPFCAD'
               IF ( OPD-COD-STAT IN OPD NOT = 'G'  ) AND
                    SCR-CMND = 'IAL'
                   MOVE 'DAP' TO MSG-COD-SIST
                   MOVE 'OPDIALNOG' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE 'S' TO SCR-IERR
                   MOVE SCR-STAT-CON TO SCR-STAT.
           IF SCR-NMAP = 'DAPFCAD'
               IF ( OPD-COD-RENO IN OPD NOT = 'S'  ) AND
                    SCR-CMND = 'IAL'
                   MOVE 'DAP' TO MSG-COD-SIST
                   MOVE 'OPDIALNORENO' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE 'S' TO SCR-IERR
                   MOVE SCR-STAT-CON TO SCR-STAT.
      *>>>>
*% END
      *         ELSE
      *             NEXT SENTENCE
      *     ELSE
      *         IF NOT ( OPD-COD-STAT IN OPD = 'A' OR 'G' )
      *             MOVE 'DAP' TO MSG-COD-SIST
      *             MOVE 'OPD    NAE' TO MSG-COD-MENS
      *             PERFORM GET-MSG
      *             MOVE MSG-GLS-DESC TO FRM-MENS
      *             MOVE SCR-STAT-CON TO SCR-STAT.

           MOVE CAD-CAI-IOPD IN CAD-FLD TO OPD-CAI-IOPD IN OPD.
           MOVE CAD-IIC-IOPD IN CAD-FLD TO OPD-IIC-IOPD IN OPD.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM DAP-FIO-OPD.
           MOVE FIO-STAT TO PGM-STAT-OPD.

           IF CAD-NUM-ILIQ IN CAD-FLD NOT > ZEROES
               IF PGM-STAT-OPD-OKS
                   IF OPD-COD-STAT IN OPD NOT = 'I' AND
                      OPD-COD-STAT IN OPD NOT = 'V'
                      MOVE 'DAP' TO MSG-COD-SIST
                      MOVE 'OPD    NIV' TO MSG-COD-MENS
                      PERFORM GET-MSG
                      MOVE MSG-GLS-DESC TO FRM-MENS
                      MOVE SCR-STAT-CON TO SCR-STAT
                   ELSE
                      NEXT SENTENCE
               ELSE
                   NEXT SENTENCE
           ELSE
               MOVE CAD-CIC-IOPD IN CAD TO LIQ-CIC-IOPD IN LIQ
               MOVE CAD-NUM-ILIQ IN CAD-FLD TO LIQ-NUM-ILIQ IN LIQ
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM DAP-FIO-LIQ
               MOVE FIO-STAT TO PGM-STAT-LIQ
               IF PGM-STAT-LIQ-OKS
                   IF LIQ-COD-STAT IN LIQ NOT = 'I' AND
                      LIQ-COD-STAT IN LIQ NOT = 'V'
                      MOVE 'DAP' TO MSG-COD-SIST
                      MOVE 'LIQ    NIV' TO MSG-COD-MENS
                      PERFORM GET-MSG
                      MOVE MSG-GLS-DESC TO FRM-MENS
                      MOVE SCR-STAT-CON TO SCR-STAT.
      *>>>>
*% END
*% IF FIN_PUT_SCR
      *<<<< FIN_PUT_SCR
      *PTC-INI (RRM)   26-NOV-2002                                      
                                                                        
           IF SCR-CMND = 'CON' AND                                      
              SCR-OFRM-SRV                                              
                                                                        
                 MOVE TSK-TERM-ALF TO QUE-TERM                          
                 MOVE PTC-CAD-TYPE TO QUE-TYPE                          
                 MOVE QUE-DEL TO QUE-CMND                               
                 PERFORM GNS-PRO-QUE                                    
                                                                        
                 MOVE CAD-CAI-IOPD IN CAD-FLD TO CAD-CAI-IOPD IN CAD    
                 MOVE CAD-IIC-IOPD IN CAD-FLD TO CAD-IIC-IOPD IN CAD    
                 MOVE ZEROES                  TO CAD-NUM-ILIQ IN CAD    
                 MOVE ZEROES                  TO CAD-NUM-ICAD IN CAD    
                 MOVE ZEROES TO QUE-NITM                                
                 MOVE FIO-GET-NLS TO FIO-CMND.                          
                                                                        
       CIC-PTC-CAD.                                                     
                                                                        
           IF SCR-CMND = 'CON' AND                                      
              SCR-OFRM-SRV                                              
                 PERFORM DAP-FIO-CAD                                    
                 IF FIO-STAT-OKS AND                                    
                    CAD-CAI-IOPD IN CAD = CAD-CAI-IOPD IN CAD-FLD AND   
                    CAD-IIC-IOPD IN CAD = CAD-IIC-IOPD IN CAD-FLD AND   
                    CAD-NUM-ILIQ IN CAD = ZEROES                        
                                                                        
                  MOVE FIO-STAT TO PGM-STAT-CAD                         
                  PERFORM PUT-CAD-CAD                                   
                  MOVE CAD-NUM-ICAD IN CAD TO PTC-CAD-CAD-NUM-ICAD      
                  MOVE CAD-IND-TCAD IN CAD TO PTC-CAD-CAD-IND-TCAD      
                  MOVE CAD-CAI-ICAD IN CAD TO PTC-CAD-CAD-CAI-ICAD      
                  MOVE CAD-IIC-ICAD IN CAD TO PTC-CAD-CAD-IIC-ICAD      
                  PERFORM BUS-VPES                                      
                  MOVE       CAD-SGV-LIQU IN CAD-FLD TO                 
                     PTC-CAD-CAD-SGV-LIQU-EDT                           
                                                                        
                  MOVE       FRM-VAL-VPES IN CAD-FLD TO                 
                     PTC-CAD-FRM-VAL-VPES-EDT                           
                                                                        
                  ADD 1 TO QUE-NITM                                     
                  MOVE TSK-TERM-ALF TO QUE-TERM                         
                  MOVE PTC-CAD-TYPE TO QUE-TYPE                         
                  MOVE PTC-CAD-ITEM TO QUE-ITEM                         
                  MOVE PTC-CAD-LENG TO QUE-LITM                         
                  MOVE QUE-PUT  TO QUE-CMND                             
                  PERFORM GNS-PRO-QUE                                   
                                                                        
                  MOVE FIO-GET-NXT TO FIO-CMND                          
                  GO TO CIC-PTC-CAD.                                    
                                                                        
      *PTC-FIN (RRM)   26-NOV-2002                                      
      *>>>> FIN_PUT_SCR
*% END
*% IF FIN_CMN_ING OR FIN_CMN_MOD OR FIN_CMN_ELI 
      *<<<< FIN_CMN_ING OR FIN_CMN_ELI
           IF SCR-NMAP = 'DAPFCAD'
               IF SCR-CMND NOT = 'IAL'
                   MOVE CAD-CIC-IOPD IN CAD TO OPD-CIC-IOPD IN OPD
                   MOVE FIO-GET-KEY-UPD TO FIO-CMND
                   PERFORM DAP-FIO-OPD
                   IF FIO-STAT-OKS
                       MOVE 'I' TO OPD-COD-STAT IN OPD
                       MOVE FIO-MOD TO FIO-CMND
                       PERFORM DAP-FIO-OPD.
      *>>>>
*% END
*% IF FIN_HBL_IKY
      *<<<< FIN_HBL_IKY
           IF SCR-IND-ENG IN SCR-VEC-ENG(SCR-IPRO) = 'S'
               PERFORM HBL-KEY-CAD.
      *>>>>
*% END
*% IF CMN_ING OR CMN_MOD
      *<<<< CMN_ING OR CMN_MOD
           PERFORM RET-HDR.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       MSG-PF10 SECTION.
       INI-MSG-PF10.
      *GET-MSG Busca mensaje en tabla
           MOVE 'DAP' TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MEN2.
           MOVE FRM-MEN2     TO CAD-GLS-MEN2 IN CAD-FLD.
       FIN-MSG-PF10.
           EXIT.

      *Entidades de SGC
       COPY SGCBFDBC.
       COPY SGCBFOPE.
       COPY SGCBFRCO.
      *Entidades de DAP
       COPY DAPBFOPD.
       COPY DAPBFLIQ.
       COPY DAPBFRCC.
       COPY GNSBGCPT.
       COPY GNSBGSTR.
       COPY GNSBGCAM.
       COPY GNSBGPES.
       COPY GNSBGHOY.
       COPY GNSBBMSC.
      *Entidades de TAB
       COPY TABBFCAM.

       HBL-KEY-CAD SECTION.
       INI-HBL-KEY-CAD.
           PERFORM SCR-CLRA-CAD.
           MOVE SCR-CAI-IOPD IN SCR-VARI TO CAD-CAI-IOPD IN CAD-FLD.
           MOVE SCR-IIC-IOPD IN SCR-VARI TO CAD-IIC-IOPD IN CAD-FLD.
           MOVE SCR-NUM-ILIQ IN SCR-VARI TO CAD-NUM-ILIQ IN CAD-FLD.
           MOVE +4 TO CAD-CAI-IOPD-LEN IN CAD-FLD.
      *ISP   25-JUL-1990 DEBE SER REEMPLAZADO POR VARIABLE
      *     INHIBE MOVE FRM-CPSF TO FRM-CPIM-AEY
           MOVE '1' TO FRM-CPIM-AEY.
           PERFORM HBL-FLD.
           MOVE SCR-INST-GFL TO SCR-INST.

      *ISP SE REQUIEREN PARA BUS-COR-CAD
           MOVE SCR-CAI-IOPD IN SCR-VARI TO CAD-CAI-IOPD IN CAD.
           MOVE SCR-IIC-IOPD IN SCR-VARI TO CAD-IIC-IOPD IN CAD.
           MOVE SCR-NUM-ILIQ IN SCR-VARI TO CAD-NUM-ILIQ IN CAD.
           PERFORM BUS-COR-CAD.
      *ISP EN HBL-FLD ALD LEE OPD
           MOVE SCR-CAI-IOPD IN SCR-VARI TO OPD-CAI-IOPD IN OPD.
           MOVE SCR-IIC-IOPD IN SCR-VARI TO OPD-IIC-IOPD IN OPD.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM DAP-FIO-OPD.
           MOVE FIO-STAT TO PGM-STAT-OPD.
           IF NOT PGM-STAT-OPD-OKS
               MOVE 'DAP' TO MSG-COD-SIST
               MOVE 'OPD    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE SCR-STAT-CON TO SCR-STAT
               PERFORM CLR-OPD-CAD
           ELSE
               PERFORM PUT-OPD-CAD.

      * ISP: BUSQUEDA CLIENTES DAP ENGANCHE

           PERFORM GET-RCC.
           IF NOT FIO-STAT-OKS
               MOVE SCR-STAT-CON TO SCR-STAT
               MOVE 'SGC'        TO MSG-COD-SIST
               MOVE 'CLI    NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
      *ISP PANTALLA CAD NO TIENE CAMPOS DE RCC
      *    COMO CYA DE COL
      *         PERFORM CLR-RCC-CAD
                GO TO CIC-HBL-KEY-CAD.

           PERFORM PUT-DBC-CAD.
       CIC-HBL-KEY-CAD.
      *
           PERFORM EDT-KEY-CAD.
           PERFORM EDT-FLD-CAD.

           MOVE 'F10CADNINOPD' TO MSG-COD-MENS.
           PERFORM MSG-PF10.

           IF SCR-IND-FST IN SCR-VEC-FST(SCR-IPRO) = 'S'
               MOVE SPACES TO SCR-IND-FST IN SCR-VEC-FST(SCR-IPRO)
               PERFORM SCR-INIT
               PERFORM STP-EXIT
           ELSE
               PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-HBL-KEY-CAD.
           EXIT.

      *  CODIGO ADICIONAL PARA ENGANCHE PROGRAMAS
       ING-FPEN-CIC SECTION.
       INI-ING-FPEN-CIC.
           PERFORM ING-SETE.
           PERFORM SCR-CLRA-CAD.
           MOVE SCR-MENS-EXT TO CAD-GLS-MENS.
           PERFORM HBL-IKY.
       FIN-ING-FPEN-CIC.
           EXIT.

       ING-SETE SECTION.
       INI-ING-SETE.
           PERFORM CRG-TBL-ENG.
           MOVE WSS-NTRN(SCR-IPRO)     TO SCR-NTRN.
           MOVE WSS-PROG(SCR-IPRO)     TO SCR-PROG.
           MOVE WSS-NMAP(SCR-IPRO)     TO SCR-NMAP.
           MOVE WSS-NLIB(SCR-IPRO)     TO SCR-NLIB.
      *     MOVE WSS-NEMO(WSS-IDX-ICAD) TO SCR-NEMO.

           MOVE WSS-NTRV(SCR-IPRO)     TO SCR-NTRV.
           MOVE WSS-PROV(SCR-IPRO)     TO SCR-PROV.
       FIN-ING-SETE.
           EXIT.

       EGR-FPEN-CIC SECTION.
       INI-EGR-FPEN-CIC.
      * PTC-INI-ADIC (RRM)  3-DEC-2002                                  
           IF SCR-OFRM-SRV                                              
               GO TO FIN-EGR-FPEN-CIC.                                  
      * PTC-FIN-ADIC (RRM)  3-DEC-2002                                  
           PERFORM CRG-TBL-ENG.
           ADD 1 TO SCR-IPRO.
           IF SCR-IPRO > WSS-TOT-IIDX
                  PERFORM EGR-FTER-CIC
                  GO TO FIN-EGR-FPEN-CIC.
           IF SCR-IND-ENG IN SCR-VEC-ENG(SCR-IPRO) = SPACES
               GO TO INI-EGR-FPEN-CIC.
           MOVE WSS-INST-FPEN-CIC TO SCR-INST-EXT.
           MOVE WSS-PROG(SCR-IPRO) TO SYS-PROG.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-XCTL TO SYS-CMND.
           PERFORM GNS-PRO-SYS.
       FIN-EGR-FPEN-CIC.
           EXIT.

       EGR-FTER-CIC SECTION.
       INI-EGR-FTER-CIC.
      * PTC-INI-ADIC (RRM) 3-DEC-2002                                   
           IF SCR-OFRM-SRV                                              
                GO TO FIN-EGR-FTER-CIC.                                 
      * PTC-FIN-ADIC (RRM) 3-DEC-2002                                   
           PERFORM CRG-TBL-ENG.
           MOVE WSS-IDX-IOPD TO SCR-IPRO.
           MOVE WSS-INST-FTER-CIC TO SCR-INST-EXT.
           MOVE WSS-PROG(SCR-IPRO) TO SYS-PROG.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-XCTL TO SYS-CMND.
           PERFORM GNS-PRO-SYS.
       FIN-EGR-FTER-CIC.
           EXIT.

       EGR-FTER-CMN SECTION.
       INI-EGR-FTER-CMN.
      * PTC-INI-ADIC (RRM)  3-DEC-2002                                  
           IF SCR-OFRM-SRV                                              
                GO TO FIN-EGR-FTER-CMN.                                 
      * PTC-FIN-ADIC (RRM)  3-DEC-2002                                  
           PERFORM CRG-TBL-ENG.
           MOVE WSS-IDX-IOPD TO SCR-IPRO.
           MOVE WSS-INST-FTER-CMN TO SCR-INST-EXT.
           MOVE WSS-PROG(SCR-IPRO) TO SYS-PROG.
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-XCTL TO SYS-CMND.
           PERFORM GNS-PRO-SYS.
       FIN-EGR-FTER-CMN.
           EXIT.

       SCR-INIT SECTION.
       INI-SCR-INIT.
           MOVE 3  TO FRM-PLIN.
           MOVE 19 TO FRM-NLIN.
           MOVE FRM-CLR-FRM TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE FRM-SINI-DBNF TO FRM-SINI.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE CAD-FLD TO FRM-DFLD.
           MOVE FRM-INI TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-INIT.
           EXIT.

       GET-RCC SECTION.
       INI-GET-RCC.
           PERFORM GET-CUR-RCC.
           IF FIO-STAT-OKS
               PERFORM GET-NXT-RCC.
       FIN-GET-RCC.
           EXIT.

       GET-CUR-RCC SECTION.                                             
       INI-GET-CUR-RCC.                                                 
           MOVE OPD-CIC-IOPD IN OPD TO RCC-CIC-IOPD IN RCC.             
           MOVE 'RCC-KEY-IOPD' TO FIO-AKEY.                             
           MOVE FIO-GET-KEY TO FIO-CMND.                                
           PERFORM DAP-FIO-RCC.                                         
           MOVE FIO-STAT TO PGM-STAT-RCC.                               
       FIN-GET-CUR-RCC.                                                 
           EXIT.                                                        

      *GET-CUR-RCC SECTION.
      * INI-GET-CUR-RCC.
      *     MOVE OPD-CIC-IOPD IN OPD TO RCC-CIC-IOPD IN RCC.
      *     MOVE 'RCC-KEY-IOPD' TO FIO-AKEY.
      **RFU     MOVE FIO-GET-KEY TO FIO-CMND.
      *     MOVE FIO-GET-NLS TO FIO-CMND.
      *     PERFORM DAP-FIO-RCC.
      *     IF NOT (FIO-STAT-OKS AND
      *          OPD-CIC-IOPD IN OPD = RCC-CIC-IOPD IN RCC)
      *         MOVE '23' TO FIO-STAT.
      *     MOVE FIO-STAT TO PGM-STAT-RCC.
      * FIN-GET-CUR-RCC.
      *     EXIT.

       GET-NXT-RCC SECTION.
       INI-GET-NXT-RCC.
           MOVE 'RCC-KEY-IOPD' TO FIO-AKEY.
           MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM DAP-FIO-RCC.
           IF FIO-STAT-OKS AND
              OPD-CIC-IOPD IN OPD = RCC-CIC-IOPD IN RCC
                MOVE '00' TO PGM-STAT-DBC
                MOVE ZEROES TO DBC-NUM-ICLI IN DBC
                MOVE SPACES TO DBC-VRF-ICLI IN DBC
                MOVE 'M'    TO DBC-IND-ICLI IN DBC
                MOVE SPACES TO DBC-GLS-ICLI IN DBC
      * OPD con multiples clientes
                MOVE 'DAP' TO MSG-COD-SIST
                MOVE 'OPD    MCLI' TO MSG-COD-MENS
                PERFORM GET-MSG
                MOVE MSG-GLS-DESC IN MSG TO DBC-GLS-NOMC IN DBC
           ELSE
                PERFORM GET-CUR-RCC
                MOVE RCC-CIC-ICLI IN RCC TO SCR-CIC-ICLI
                MOVE RCC-CIC-ICLI IN RCC TO DBC-CIC-ICLI IN DBC
                MOVE FIO-GET-KEY TO FIO-CMND
                PERFORM SGC-FIO-DBC
                MOVE FIO-STAT TO PGM-STAT-DBC.
       FIN-GET-NXT-RCC.
           EXIT.

       BUS-COR-CAD SECTION.
       INI-BUS-COR-CAD.

           MOVE 9                        TO WSS-NUM-ICAD IN WSS-VARI.
           MOVE WSS-NUM-ICAD IN WSS-VARI TO CAD-NUM-ICAD IN CAD.

           MOVE FIO-GET-LEQ TO FIO-CMND.
           PERFORM DAP-FIO-CAD.
           IF ( FIO-STAT-OKS AND
                CAD-CAI-IOPD IN CAD-FLD = CAD-CAI-IOPD IN CAD AND
                CAD-IIC-IOPD IN CAD-FLD = CAD-IIC-IOPD IN CAD AND
                CAD-NUM-ILIQ IN CAD-FLD = CAD-NUM-ILIQ IN CAD )
                  MOVE CAD-NUM-ICAD IN CAD TO
                       WSS-NUM-ICAD IN WSS-VARI
           ELSE
               PERFORM KEY-ALL-CAD
               MOVE ZEROES TO CAD-NUM-ICAD IN CAD
               MOVE ZEROES TO WSS-NUM-ICAD IN WSS-VARI.

           IF SCR-CCPP = 'ING' OR 'ACT'
               ADD 1 TO WSS-NUM-ICAD IN WSS-VARI
               MOVE WSS-NUM-ICAD IN WSS-VARI TO CAD-NUM-ICAD IN CAD
               
               MOVE CAD-NUM-ICAD IN CAD TO CAD-NUM-ICAD IN CAD-FLD.
           MOVE 1 TO WSS-ICOR.
       FIN-BUS-COR-CAD.
           EXIT.

       COPY DAPBGMPT.

      *>>>> END EOF
*% END
*% IF EOF_PU
      *<<<< EOF_PU
       RET-HDR SECTION.
       INI-RET-HDR.
           PERFORM GET-FHOY.
      *    Fecha Transaccion
           MOVE HOY-FHOY TO CAD-FEC-FTRN IN CAD.
      *    Hora Transaccion
           MOVE HOY-HHOY TO CAD-HRA-HRTR IN CAD.
      *    Codigo Origen transaccion
           MOVE SCR-OTRN TO CAD-COD-OTRN IN CAD.
      *    Autor Transaccion
      *     MOVE SCR-ATRN TO CAD-COD-ATRN IN CAD.
           MOVE SPACES    TO CAD-COD-ATRN IN CAD.
           IF SCR-CCPP = 'ING'
               MOVE 'I' TO CAD-COD-TACC IN CAD
           ELSE
           IF SCR-CCPP = 'MOD'
               MOVE 'M' TO CAD-COD-TACC IN CAD.
       FIN-RET-HDR.
           EXIT.

       PUT-DBC-CAD SECTION.
       INI-PUT-DBC-CAD.
           IF PGM-STAT-DBC-NUL
               GO TO FIN-PUT-DBC-CAD.
           IF NOT PGM-STAT-DBC-OKS
               PERFORM CLR-DBC-CAD
               GO TO FIN-PUT-DBC-CAD.
           MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN CAD-FLD.
           MOVE DBC-IND-ICLI IN DBC TO DBC-IND-ICLI IN CAD-FLD.
           MOVE DBC-GLS-ICLI IN DBC TO DBC-GLS-ICLI IN CAD-FLD.
           MOVE DBC-NUM-ICLI IN DBC TO DBC-NUM-ICLI IN CAD-FLD.
           MOVE DBC-VRF-ICLI IN DBC TO DBC-VRF-ICLI IN CAD-FLD.
      *<<<< FIN_PUT_DBC_CAD
           IF DBC-IND-ICLI IN DBC = 'M'
               GO TO FIN-PUT-DBC-CAD.
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.
           PERFORM CPT-BLKS.
           MOVE CPT-STRN TO DBC-GLS-NOMC IN CAD-FLD.
      *>>>>
       FIN-PUT-DBC-CAD.
           EXIT.
       
      * PUT-RCC-CAD SECTION.
      * INI-PUT-RCC-CAD.
      *     IF PGM-STAT-RCC-NUL
      *         GO TO FIN-PUT-RCC-CAD.
      *     IF NOT PGM-STAT-RCC-OKS
      *ISP PANTALLA CAD NO TIENE CAMPOS DE RCC
      *    COMO CYA DE COL
      *        PERFORM CLR-RCC-CAD
      *         GO TO FIN-PUT-RCC-CAD.
      *     MOVE RCC-CAI-ICLI IN RCC TO RCC-CAI-ICLI IN CAD-FLD.
      *     MOVE RCC-IIC-ICLI IN RCC TO RCC-IIC-ICLI IN CAD-FLD.
      * FIN-PUT-RCC-CAD.
      *     EXIT.

       PUT-OPD-CAD SECTION.
       INI-PUT-OPD-CAD.
           IF PGM-STAT-OPD-NUL
               GO TO FIN-PUT-OPD-CAD.
           IF NOT PGM-STAT-OPD-OKS
               PERFORM CLR-OPD-CAD
               GO TO FIN-PUT-OPD-CAD.
           MOVE OPD-COD-VCAM IN OPD TO OPD-COD-VCAM IN CAD-FLD.
           MOVE OPD-COD-TOPD IN OPD TO OPD-COD-TOPD IN CAD-FLD.
           MOVE OPD-COD-AOPD IN OPD TO OPD-COD-AOPD IN CAD-FLD.
      *BUS-TAB busca glosa de codigo en tabla
           IF OPD-COD-VCAM IN CAD-FLD > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'VLR' TO TAB-COD-TTAB IN TAB
               MOVE OPD-COD-VCAM IN CAD-FLD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-VCAM IN CAD-FLD.
      *BUS-TAB busca glosa de codigo en tabla
           IF OPD-COD-TOPD IN CAD-FLD > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'TIO' TO TAB-COD-TTAB IN TAB
               MOVE OPD-COD-TOPD IN CAD-FLD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-TOPD IN CAD-FLD.
      *BUS-TAB busca glosa de codigo en tabla
           IF OPD-COD-AOPD IN CAD-FLD > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'AUX' TO TAB-COD-TTAB IN TAB
               MOVE OPD-COD-TOPD IN CAD-FLD TO OPD-COD-TOPD IN OPD
               MOVE OPD-COD-AOPD IN CAD-FLD TO OPD-COD-AOPD IN OPD
               MOVE OPD-COD-COPD IN OPD     TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO 
                                          FRM-GLS-AOPD IN CAD-FLD.
       FIN-PUT-OPD-CAD.
           EXIT.
