*%! Codigo adicional para archivo ODC
*% IF SETTAG
*% VSNBQ = "05.12.21.1"
*% FNCBQ = "DAP-EVENTOS"
*% VSNBU = "05.12.21.1"
*% FNCBU = "DAP-EVENTOS"
*% END
*% IF INI_PUT_OPD_ODC
      *<<<< INI_PUT_OPD_ODC
           IF OPD-IIC-GREN IN OPD NOT NUMERIC
               MOVE ZEROES TO OPD-IIC-GREN IN OPD.
           IF OPD-NUM-PVEN IN OPD NOT NUMERIC
               MOVE ZEROES TO OPD-NUM-PVEN IN OPD.
           IF OPD-VAL-IPES IN OPD NOT NUMERIC
               MOVE ZEROES TO OPD-VAL-IPES IN OPD.
      *>>>>
*% END
*% IF FIN_PUT_OPD_ODC
      *<<<< FIN_PUT_OPD_ODC
           MOVE OPD-FEC-FTRN IN OPD TO WSS-FEC-FTRN IN WSS-VARI.
           MOVE WSS-NUM-DTRN IN WSS-VARI TO FRM-NUM-DTRN IN ODC-FLD.
           MOVE WSS-NUM-MTRN IN WSS-VARI TO FRM-NUM-MTRN IN ODC-FLD.
           MOVE WSS-NUM-STRN IN WSS-VARI TO FRM-NUM-STRN IN ODC-FLD.
           MOVE WSS-NUM-ATRN IN WSS-VARI TO FRM-NUM-ATRN IN ODC-FLD.
      *>>>>
*% END
*% IF FIN_RET_OPD_ODC
      *<<<< FIN_RET_OPD_ODC
           MOVE FRM-NUM-DTRN IN ODC-FLD TO WSS-NUM-DTRN IN WSS-VARI.
           MOVE FRM-NUM-MTRN IN ODC-FLD TO WSS-NUM-MTRN IN WSS-VARI.
           MOVE FRM-NUM-STRN IN ODC-FLD TO WSS-NUM-STRN IN WSS-VARI.
           MOVE FRM-NUM-ATRN IN ODC-FLD TO WSS-NUM-ATRN IN WSS-VARI.
           MOVE WSS-FEC-FTRN IN WSS-VARI TO OPD-FEC-FTRN IN OPD.
      *>>>>
*% END
*% IF INI_FRM_FEC_FTRN
      *<<<< INI_FRM_FEC_FTRN
           MOVE FEC-VHBL-N TO FEC-VHBL.
           MOVE FEC-CHOY-N TO FEC-CHOY.
      *>>>>
*% END
*% IF INI_OPD_FEC_FCMP
      *<<<< INI_OPD_FEC_FCMP
           MOVE FEC-VHBL-N TO FEC-VHBL.
           MOVE FEC-CHOY-N TO FEC-CHOY.
      *>>>>
*% END
*% IF INI_OPD_FEC_FREA
      *<<<< INI_OPD_FEC_FREA
           MOVE FEC-VHBL-N TO FEC-VHBL.
           MOVE FEC-CHOY-N TO FEC-CHOY.
      *>>>>
*% END
*% IF INI_OPD_FEC_FPCO
      *<<<< INI_OPD_FEC_FPCO
           MOVE FEC-VHBL-N TO FEC-VHBL.
           MOVE FEC-CHOY-N TO FEC-CHOY.
      *>>>>
*% END
*% IF INI_OPD_FEC_FCTB
      *<<<< INI_OPD_FEC_FCTB
           MOVE FEC-VHBL-N TO FEC-VHBL.
           MOVE FEC-CHOY-N TO FEC-CHOY.
      *>>>>
*% END
*% IF INI_OPD_FEC_FPVC
      *<<<< INI_OPD_FEC_FPVC
           MOVE FEC-VHBL-N TO FEC-VHBL.
           MOVE FEC-CHOY-N TO FEC-CHOY.
      *>>>>
*% END
*% IF INI_OPD_FEC_FTER
      *<<<< INI_OPD_FEC_FTER
           MOVE FEC-VHBL-N TO FEC-VHBL.
           MOVE FEC-CHOY-N TO FEC-CHOY.
      *>>>>
*% END
*% IF INI_OPD_FEC_FDEV
      *<<<< INI_OPD_FEC_FDEV
           MOVE FEC-VHBL-N TO FEC-VHBL.
           MOVE FEC-CHOY-N TO FEC-CHOY.
      *>>>>
*% END
*% IF INI_VAL_CON_KEY_ODC
      *<<<< IF INI_VAL_CON_KEY_ODC
           IF FRM-FFLD = FRM-FFLD-PF8 AND
              ( SCR-CMND = 'CON')
             GO TO FIN-VAL-CON-KEY-ODC.
      *>>>> END
*% END
