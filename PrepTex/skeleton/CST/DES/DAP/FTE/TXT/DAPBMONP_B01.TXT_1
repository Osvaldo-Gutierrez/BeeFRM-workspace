*% IF FIN_PUT_OPD_ONP
      *<<<< FIN_PUT_OPD_ONP

           PERFORM BUS-VPES.

      *VAL-COD Valida codigo de tabla
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'AUX' TO TAB-COD-TTAB IN TAB.
           MOVE OPD-COD-COPD IN OPD TO TAB-COD-CTAB IN TAB.
           PERFORM BUS-TAB.
           MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-AOPD IN ONP-FLD.
      *>>>>
*% END
*% IF FIN_PUT_ONP_ONP 
      *<<<< FIN_PUT_ONP_ONP 
      *BUS-OFI busca oficina en tablas
           MOVE ONP-COD-OFIC IN ONP TO OFI-COD-OFIC IN OFI.
           PERFORM BUS-OFI.
           MOVE OFI-GLS-DESC IN OFI TO FRM-GLS-OFOC IN ONP-FLD.
      *>>>> END
*% END
*% IF FIN_VAL_CON_IKY_ONP OR FIN_VAL_CON_KEY_ONP 
      *<<<< FIN_VAL_CON_IKY_ONP OR FIN_VAL_CON_KEY_ONP
      *     PERFORM GET-FRM-OPD.
      *     IF NOT FIO-STAT-OKS
      *        MOVE 'DAP'        TO MSG-COD-SIST
      *        MOVE 'OPD    NEX' TO MSG-COD-MENS
      *        PERFORM GET-MSG
      *        MOVE MSG-GLS-DESC TO FRM-MENS
      *        MOVE FRM-SUAR-MAL TO FRM-SUAR
      *     ELSE
      *        IF SCR-CCPP = 'ING'
      *            PERFORM PUT-OPD-ONP.
      *>>>> END
*% END
*% IF INI_FST_FLD_ONP
      *<<<< INI_FST_FLD_ONP
           IF SCR-CMND = 'ALZ'
      *LRH ini 8-AUG-1994
      *         MOVE -1 TO ONP-GLS-MENS-LEN IN ONP-FLD
               MOVE FRM-CPIM-AEY TO ONP-COD-EJEC-ATR IN ONP-FLD
               MOVE -1 TO ONP-COD-EJEL-LEN IN ONP-FLD
               GO TO FIN-FST-FLD-ONP
           ELSE
               IF SCR-CMND = 'ING' 
                   MOVE FRM-CPIM-AEY TO ONP-COD-EJEL-ATR IN ONP-FLD
                   MOVE -1 TO ONP-COD-MONP-LEN IN ONP-FLD
      *LRH fin 8-AUG-1994
               GO TO FIN-FST-FLD-ONP.
      *>>>> END
*% END
*% IF OPD_CAI_ISER
      *<<<< OPD_CAI_ISER
           MOVE OPD-CAI-ISER IN ONP-FLD TO NUM-NUME.
           MOVE NUM-ZERO-S TO NUM-ZERO.
           MOVE NUM-SGNO-N TO NUM-SGNO.
           MOVE 4 TO NUM-NENT.
           MOVE 0 TO NUM-NDCM.
           PERFORM VAL-NUM.
           IF NUM-STAT-OKS
               MOVE NUM-NN9N TO WSS-OPD-CAI-ISER
               MOVE WSS-OPD-CAI-ISER TO OPD-CAI-ISER IN ONP-FLD
           ELSE
               MOVE NUM-MENS     TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
               GO TO FIN-VAL-OPD-CAI-ISER.

      *>>>> OPD_CAI_ISER
*% END
*% IF ONP_COD_OFIC
      *<<<< ONP_COD_OFIC
           IF ONP-COD-OFIC IN ONP-FLD NOT > SPACES
              MOVE SPACES TO ONP-COD-OFIC IN ONP-FLD.
      *VAL-OFI Valida codigo de Oficina en TABLAS
           MOVE ONP-COD-OFIC IN ONP-FLD TO OFI-COD-OFIC IN OFI.
           PERFORM VAL-OFI.
           IF MSG-COD-MENS NOT = SPACES
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
           IF MSG-COD-MENS = 'COD    NEX'
               MOVE SPACES TO FRM-GLS-OFOC IN ONP-FLD
           ELSE
               MOVE OFI-GLS-DESC IN OFI TO FRM-GLS-OFOC IN ONP-FLD.
      *>>>> END
*% END
*% IF ONP_COD_OFEL
      *<<<< ONP_COD_OFEL
           IF SCR-CMND = 'ING'
               MOVE SPACES TO ONP-COD-OFEL IN ONP-FLD
                              FRM-GLS-OFEL IN ONP-FLD
               GO TO FIN-VAL-ONP-COD-OFEL.
           IF ONP-COD-OFEL IN ONP-FLD = SPACES AND SCR-CMND = 'MOD'
               GO TO FIN-VAL-ONP-COD-OFEL.
      *>>>> END
*% END
*% IF VAL_CON_FLD_ONP 
      *<<<< VAL_CON_FLD_ONP 
      *GET-HOY Obtiene Fecha y Hora actual

           IF SCR-CMND = 'ING'
      *HZF-INI 17-05-2002                                               
               PERFORM VAL-LIQ-EN-PROC                                  
      *HZF-FIN 17-05-2002                                               
               PERFORM GET-FHOY
               MOVE HOY-DHOY IN HOY-VARI TO ONP-NUM-DING IN ONP-FLD
               MOVE HOY-MHOY IN HOY-VARI TO ONP-NUM-MING IN ONP-FLD
               MOVE HOY-SHOY IN HOY-VARI TO ONP-NUM-SING IN ONP-FLD
               MOVE HOY-AHOY IN HOY-VARI TO ONP-NUM-AING IN ONP-FLD
               MOVE HOY-HHOY IN HOY-VARI TO ONP-HRA-HRIN IN ONP-FLD.

           IF SCR-CMND = 'ALZ'
               PERFORM GET-FHOY
               MOVE HOY-DHOY IN HOY-VARI TO ONP-NUM-DELI IN ONP-FLD
               MOVE HOY-MHOY IN HOY-VARI TO ONP-NUM-MELI IN ONP-FLD
               MOVE HOY-SHOY IN HOY-VARI TO ONP-NUM-SELI IN ONP-FLD
               MOVE HOY-AHOY IN HOY-VARI TO ONP-NUM-AELI IN ONP-FLD
               MOVE HOY-HHOY IN HOY-VARI TO ONP-HRA-HREL IN ONP-FLD.
      *>>>>
*% END
*% IF ONP_COD_EJEC
      *LRH ini 8-AUG-1994
           IF SCR-CMND = 'ING'
               IF ONP-COD-EJEC IN ONP-FLD NOT > SPACES
                   MOVE 'DAP'          TO MSG-COD-SIST
                   MOVE 'EJE    OBLIG' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
               ELSE
                   MOVE 'USR' TO USR-COD-TTAB
                   MOVE ONP-COD-EJEC IN ONP-FLD TO USR-COD-USER IN USR
                   MOVE FIO-GET-KEY    TO FIO-CMND
                   MOVE 'USR-COD-TABL' TO FIO-AKEY
                   PERFORM TAB-FIO-USR
                   IF FIO-STAT-OKS
                       IF USR-IND-VIGE IN USR = 'N'
                           MOVE 'COD    NVG' TO MSG-COD-MENS
                           MOVE 'GNS' TO MSG-COD-SIST
                           PERFORM GET-MSG
                       ELSE
                           MOVE USR-GLS-DESC IN USR TO 
                                FRM-GLS-EJEC IN ONP-FLD
                           MOVE SPACES TO MSG-COD-MENS
                                          MSG-GLS-DESC
                   ELSE
                       MOVE 'COD    NEX' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG.
           IF MSG-COD-MENS > SPACES
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *LRH fin 8-AUG-1994
*% END
*% IF ONP_COD_EJEL
      *LRH ini 8-AUG-1994
           IF SCR-CMND = 'ALZ'
               IF ONP-COD-EJEL IN ONP-FLD NOT > SPACES
                   MOVE 'DAP'          TO MSG-COD-SIST
                   MOVE 'EJE    OBLIG' TO MSG-COD-MENS
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE FRM-SUAR-MAL TO FRM-SUAR
               ELSE
                   MOVE 'USR' TO USR-COD-TTAB
                   MOVE ONP-COD-EJEL IN ONP-FLD TO USR-COD-USER IN USR
                   MOVE FIO-GET-KEY    TO FIO-CMND
                   MOVE 'USR-COD-TABL' TO FIO-AKEY
                   PERFORM TAB-FIO-USR
                   IF FIO-STAT-OKS
                       IF USR-IND-VIGE IN USR = 'N'
                           MOVE 'COD    NVG' TO MSG-COD-MENS
                           MOVE 'GNS' TO MSG-COD-SIST
                           PERFORM GET-MSG
                       ELSE
                           MOVE USR-GLS-DESC IN USR TO 
                                FRM-GLS-EJEL IN ONP-FLD
                           MOVE SPACES TO MSG-COD-MENS
                                          MSG-GLS-DESC
                   ELSE
                       MOVE 'COD    NEX' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG.
           IF MSG-COD-MENS > SPACES
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *LRH fin 8-AUG-1994
*% END
*% IF EOF_BU 
      *<<<< EOF_BU 
       PUT-OPD-ONP SECTION.
       INI-PUT-OPD-ONP.
           IF PGM-STAT-OPD-NUL
               GO TO FIN-PUT-OPD-ONP.
           IF NOT PGM-STAT-OPD-OKS
               PERFORM CLR-OPD-ONP
               GO TO FIN-PUT-OPD-ONP.
           MOVE OPD-COD-VCAM IN OPD TO OPD-COD-VCAM IN ONP-FLD.
           MOVE OPD-COD-TOPD IN OPD TO OPD-COD-TOPD IN ONP-FLD.
           MOVE OPD-COD-AOPD IN OPD TO OPD-COD-AOPD IN ONP-FLD.
           MOVE OPD-COD-OFIC IN OPD TO OPD-COD-OFIC IN ONP-FLD.
           MOVE OPD-CAI-ISER IN OPD TO OPD-CAI-ISER IN ONP-FLD.
           MOVE OPD-IIC-ISER IN OPD TO OPD-IIC-ISER IN ONP-FLD.
      *BUS-TAB busca glosa de codigo en tabla
           IF OPD-COD-VCAM IN ONP-FLD > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'VLR' TO TAB-COD-TTAB IN TAB
               MOVE OPD-COD-VCAM IN ONP-FLD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-VCAM IN ONP-FLD.
      *BUS-TAB busca glosa de codigo en tabla
           IF OPD-COD-TOPD IN ONP-FLD > SPACES
               MOVE 'TAB' TO TAB-COD-SIST
               MOVE 'TIO' TO TAB-COD-TTAB IN TAB
               MOVE OPD-COD-COPD IN OPD TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DESC IN TAB TO
                    FRM-GLS-TOPD IN ONP-FLD.
      *BUS-TAB busca glosa de codigo de tabla
           IF OPD-COD-AOPD IN ONP-FLD > SPACES
              MOVE 'TAB' TO TAB-COD-SIST
              MOVE 'AUX' TO TAB-COD-TTAB IN TAB
              MOVE OPD-COD-COPD IN OPD TO TAB-COD-CTAB IN TAB
              PERFORM BUS-TAB
              MOVE TAB-GLS-DESC IN TAB TO FRM-GLS-AOPD IN ONP-FLD.
      *BUS-OFI busca oficina en tablas
           IF OPD-COD-OFIC IN ONP-FLD > SPACES
               MOVE OPD-COD-OFIC IN ONP-FLD TO OFI-COD-OFIC IN OFI
               PERFORM BUS-OFI
               MOVE OFI-GLS-DESC IN OFI TO FRM-GLS-OFIC IN ONP-FLD.
       FIN-PUT-OPD-ONP.
           EXIT.
      *>>>> END
*% END
*% IF EOF
      *<<<< EOF

       BUS-VPES SECTION.
       INI-BUS-VPES.

      *BUS-TAB busca glosa de codigo en tabla
           IF OPD-COD-VCAM IN OPD NOT > SPACES
               GO TO FIN-BUS-VPES.

           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'VLR' TO TAB-COD-TTAB IN TAB.
           MOVE OPD-COD-VCAM IN OPD TO TAB-COD-CTAB IN TAB.
           PERFORM BUS-TAB.
           MOVE TAB-CIC-CTAB IN TAB TO SCR-CIC-VCAM.

      *    moneda extranjera
           IF SCR-CIC-TVAL NOT = PDB-CIC-MNAC
                GO TO FIN-BUS-VPES.
      *    moneda chilena y pesos
           IF SCR-CIC-VCAM = PDB-CIC-0999
                MOVE OPD-VAL-CAPT IN OPD TO FRM-VAL-IPES IN ONP-FLD
                GO TO FIN-BUS-VPES.

           PERFORM LEE-VCAM.

           MULTIPLY OPD-VAL-CAPT IN OPD BY 
               SCR-SGV-VCAM GIVING 
               WSS-VAL-IPES ROUNDED.

           MOVE WSS-VAL-IPES TO PES-SGV-DECI.
           PERFORM PES-SCTV.
           MOVE PES-SGV-ENTE TO FRM-VAL-IPES IN ONP-FLD.
       FIN-BUS-VPES.
           EXIT.

       LEE-VCAM SECTION.
       INI-LEE-VCAM.
      *    moneda reajustable, chilena y no pesos ; convertir a pesos
           MOVE PDB-CIC-0999        TO TAB-CIC-CTAB IN TAB.
           MOVE OPD-COD-VCAM IN OPD TO TAB-COD-CTAB IN TAB.
           MOVE OPD-NUM-DREA IN OPD TO TAB-NUM-DTRN IN TAB.
           MOVE OPD-NUM-MREA IN OPD TO TAB-NUM-MTRN IN TAB.
           MOVE OPD-NUM-SREA IN OPD TO TAB-NUM-STRN IN TAB.
           MOVE OPD-NUM-AREA IN OPD TO TAB-NUM-ATRN IN TAB.
           PERFORM GET-VCAM.
           IF MSG-COD-MENS = SPACES
               MOVE CAM-SGV-VCAM IN CAM TO SCR-SGV-VCAM.
       FIN-LEE-VCAM.
           EXIT.

      *>>>> END
*% END
*% IF EOF_BU
      *<<<< EOF_BU
      *HZF-INI 17-05-2002                                               
       VAL-LIQ-EN-PROC SECTION.                                         
       INI-VAL-LIQ-EN-PROC.                                             
           MOVE ONP-CAI-IOPD IN ONP-FLD TO LIQ-CAI-IOPD IN LIQ.         
           MOVE ONP-IIC-IOPD IN ONP-FLD TO LIQ-IIC-IOPD IN LIQ.         
           MOVE 999                     TO LIQ-NUM-ILIQ IN LIQ.         
           MOVE FIO-GET-LEQ             TO FIO-CMND.                    
           PERFORM DAP-FIO-LIQ.                                         
           IF NOT FIO-STAT-OKS                                          
              GO TO FIN-VAL-LIQ-EN-PROC.                                
                                                                        
           MOVE ONP-CAI-IOPD IN ONP-FLD TO OPD-CAI-IOPD IN OPD.         
           MOVE ONP-IIC-IOPD IN ONP-FLD TO OPD-IIC-IOPD IN OPD.         
           MOVE FIO-GET-KEY             TO FIO-CMND.                    
           PERFORM DAP-FIO-OPD.                                         
           IF NOT FIO-STAT-OKS                                          
              GO TO FIN-VAL-LIQ-EN-PROC.                                
                                                                        
           IF NOT                                                       
              ( ONP-CAI-IOPD IN ONP-FLD = LIQ-CAI-IOPD IN LIQ AND       
                ONP-IIC-IOPD IN ONP-FLD = LIQ-IIC-IOPD IN LIQ )         
                   GO TO FIN-VAL-LIQ-EN-PROC.                           
           IF NOT                                                       
              ( LIQ-COD-LIQU IN LIQ = 'RC' OR                           
                LIQ-COD-LIQU IN LIQ = 'LT' )                            
                   GO TO FIN-VAL-LIQ-EN-PROC.                           
                                                                        
           IF LIQ-COD-STAT IN LIQ = 'A'                                 
              GO TO FIN-VAL-LIQ-EN-PROC.                                
                                                                        
           IF OPD-COD-STAT IN OPD = 'L' AND                             
              OPD-COD-SCTB IN OPD = 'VCD'                               
                 GO TO FIN-VAL-LIQ-EN-PROC.                             
                                                                        
           MOVE 'DAP'          TO MSG-COD-SIST                          
           MOVE 'ONPLIQ NO A ' TO MSG-COD-MENS                          
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MENS.                               
           MOVE FRM-SUAR-MAL TO FRM-SUAR.                               
       FIN-VAL-LIQ-EN-PROC.                                             
           EXIT.                                                        
      *HZF-FIN 17-05-2002                                               
      *>>>> END
*% END
