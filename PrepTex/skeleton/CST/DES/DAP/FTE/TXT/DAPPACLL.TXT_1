                  MOVE FRM-CPIM-ABY TO FRM-GLS-TX03-ATR IN CLL-FLD
                  MOVE FRM-CPIM-ABY TO LIQ-GLS-FLAG-ATR IN CLL-FLD.
      
      
           MOVE 'Liquidador'      TO FRM-GLS-TX04     IN CLL-FLD.
           MOVE FRM-CPIM-AEY      TO FRM-GLS-TX04-ATR IN CLL-FLD.
      
           MOVE FRM-CPIM-AEY    TO LIQ-COD-EJEC-ATR IN CLL-FLD.
      
           IF WSS-COMPARA = 'S'
              IF LIQ-COD-EJEC IN LIQ NOT =
                 LIQ-COD-EJEC IN LIQ-CPR
                  MOVE FRM-CPIM-ABY TO FRM-GLS-TX04-ATR IN CLL-FLD
                  MOVE FRM-CPIM-ABY TO LIQ-COD-EJEC-ATR IN CLL-FLD.
      
      
           MOVE 'Estado Liquidacion' TO FRM-GLS-TX05     IN CLL-FLD.
           MOVE FRM-CPIM-AEY         TO FRM-GLS-TX05-ATR IN CLL-FLD.
      
           MOVE FRM-CPIM-AEY    TO LIQ-COD-STAT-ATR IN CLL-FLD.
      
           IF WSS-COMPARA = 'S'
              IF LIQ-COD-STAT IN LIQ NOT =
                 LIQ-COD-STAT IN LIQ-CPR
                  MOVE FRM-CPIM-ABY TO FRM-GLS-TX05-ATR IN CLL-FLD
                  MOVE FRM-CPIM-ABY TO LIQ-COD-STAT-ATR IN CLL-FLD.
      
      
           MOVE 'Liquidacion Comput' TO FRM-GLS-TX06     IN CLL-FLD.
           MOVE FRM-CPIM-AEY         TO FRM-GLS-TX06-ATR IN CLL-FLD.
      
           MOVE FRM-CPIM-AEY    TO LIQ-FEC-FCMP-ATR IN CLL-FLD.
      
           IF WSS-COMPARA = 'S'
              IF LIQ-FEC-FCMP IN LIQ NOT =
                 LIQ-FEC-FCMP IN LIQ-CPR
                  MOVE FRM-CPIM-ABY TO FRM-GLS-TX06-ATR IN CLL-FLD
                  MOVE FRM-CPIM-ABY TO LIQ-FEC-FCMP-ATR IN CLL-FLD.
      
      
           MOVE 'Time Stamp' TO FRM-GLS-TX07     IN CLL-FLD.
           MOVE FRM-CPIM-AEY TO FRM-GLS-TX07-ATR IN CLL-FLD.
      
           MOVE FRM-CPIM-AEY    TO LIQ-STP-ITRN-ATR IN CLL-FLD.
      
           IF WSS-COMPARA = 'S'
              IF LIQ-STP-ITRN IN LIQ NOT =
