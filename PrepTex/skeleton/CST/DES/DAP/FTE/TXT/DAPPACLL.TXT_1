      *             MOVE '23' TO PGM-STAT-LAD
      *             MOVE 'CLLNONXTOIPK' TO MSG-COD-MENS
      *             MOVE 'DAP' TO MSG-COD-SIST
      *             PERFORM GET-MSG
      *             MOVE MSG-GLS-DESC TO FRM-MENS
      *             GO TO FIN-FND-KEY.

      *    IF FRM-COD-CREB IN CLL-FLD = 'AUT'
      *        IF LAD-COD-AUTO IN CLL-FLD NOT =
      *           LAD-COD-AUTO IN LAD
      *             MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
      *             MOVE '23' TO FIO-STAT
      *             MOVE '23' TO PGM-STAT-LAD
      *             MOVE 'CLLNONXTOAUK' TO MSG-COD-MENS
      *             MOVE 'DAP' TO MSG-COD-SIST
      *             PERFORM GET-MSG
      *             MOVE MSG-GLS-DESC TO FRM-MENS
      *             GO TO FIN-FND-KEY.

      *    IF FRM-COD-CREB IN CLL-FLD = 'CLI'
      *        IF LAD-CIC-ICLI IN LAD     NOT =
      *           DBC-CIC-ICLI IN DBC
      *             MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
      *             MOVE '23' TO FIO-STAT
      *             MOVE '23' TO PGM-STAT-LAD
      *             MOVE 'CLLNONXTOCLK' TO MSG-COD-MENS
      *             MOVE 'DAP' TO MSG-COD-SIST
      *             PERFORM GET-MSG
      *             MOVE MSG-GLS-DESC TO FRM-MENS
      *             GO TO FIN-FND-KEY.

           IF SCR-CMND = 'CON'
              
              IF FRM-FFLD = FRM-FFLD-PF8
                    PERFORM SET-KEY-LAD
                    PERFORM PRO-PF8
              ELSE
                IF FRM-FFLD = FRM-FFLD-PF7
                    PERFORM SET-KEY-LAD
                    PERFORM PRO-PF7.

           IF PGM-STAT-LAD-OKS
           IF FRM-FFLD = FRM-FFLD-PF7 OR
              FRM-FFLD = FRM-FFLD-PF8
               MOVE 'S' TO WSS-LEE-DBC-DESDE-LAD
               PERFORM OBTENER-NOMBRE-DBC.

           IF PGM-STAT-LAD-OKS
               IF LAD-COD-ENTF IN LAD = WSS-COD-ENTI
                   MOVE '00'   TO FIO-STAT
                   MOVE LAD-COD-CONT IN LAD TO LIQ
                   MOVE '00'   TO PGM-STAT-LIQ.

      *>>>> FIN_FND_KEY
*% END
*% IF INI_SCR_DMSG
*% IF PGM_PQ
      *<<<< INI_SCR_DMSG
      * Con este codigo logramos que una vez terminada la consulta
      * las claves no se borren de la pantalla
           IF SCR-CMND = 'CON'
                AND
              SCR-INST = SCR-INST-GKY
                AND
              FRM-FFLD NOT = FRM-FFLD-RET
                  MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR.
      *>>>> INI_SCR_DMSG
*% END
*% END
*% IF FIN_SCR_TRL
      *<<<< FIN_SCR_TRL
      
           IF PGM-STAT-LAD-OKS
               MOVE 'CLLPF7PF8 ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
      
      *>>>> FIN_SCR_TRL
*% END
*% IF PFS_SCR_GKEY_PREV
      *<<<< PFS_SCR_GKEY_PREV
           IF NOT ( FRM-FFLD = FRM-FFLD-PF7 )
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 )
      *>>>> PFS_SCR_GKEY_PREV
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF7 )
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 )
      *>>>> PFS_GET_CON
*% END
*% IF POST_SCR_PUTA
      *<<<< POST_SCR_PUTA
           IF PGM-STAT-LAD-OKS
               IF LAD-COD-INST = 'ANT'
                    PERFORM SET-KEY-LAD
                    MOVE FIO-GET-NXT TO FIO-CMND
                    PERFORM DAP-FIO-LAD
                    IF FIO-STAT-OKS
                         IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI
                            MOVE '23'  TO FIO-STAT
                            MOVE FIO-STAT TO PGM-STAT-LAD
                         ELSE
                             MOVE LAD-COD-CONT IN LAD TO LIQ-CPR
                             MOVE 'S' TO WSS-COMPARA
                             GO TO FIN-AT-DES.
      
           IF PGM-STAT-LAD-OKS
               IF LAD-COD-INST = 'DES'
                     PERFORM SET-KEY-LAD
                     MOVE FIO-GET-PRV TO FIO-CMND
                     PERFORM DAP-FIO-LAD
                     IF FIO-STAT-OKS
                          IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI
                               MOVE '23' TO FIO-STAT
                               MOVE FIO-STAT TO PGM-STAT-LAD
                          ELSE
                               MOVE LAD-COD-CONT IN LAD TO LIQ-CPR
                               MOVE 'S' TO WSS-COMPARA.
       FIN-AT-DES.
      
      
           MOVE 'Correlativo Liq' TO FRM-GLS-TX01     IN CLL-FLD.
           MOVE FRM-CPIM-AEY      TO FRM-GLS-TX01-ATR IN CLL-FLD.
      
           MOVE FRM-CPIM-AEY    TO LIQ-NUM-ILIQ-ATR IN CLL-FLD.
      
           IF WSS-COMPARA = 'S'
              IF LIQ-NUM-ILIQ IN LIQ NOT =
                 LIQ-NUM-ILIQ IN LIQ-CPR
                  MOVE FRM-CPIM-ABY TO FRM-GLS-TX01-ATR IN CLL-FLD
                  MOVE FRM-CPIM-ABY TO LIQ-NUM-ILIQ-ATR IN CLL-FLD.
      
           MOVE 'Oficina'         TO FRM-GLS-TX02     IN CLL-FLD.
           MOVE FRM-CPIM-AEY      TO FRM-GLS-TX02-ATR IN CLL-FLD.
      
           MOVE FRM-CPIM-AEY    TO LIQ-COD-OFIC-ATR IN CLL-FLD.
      
           IF WSS-COMPARA = 'S'
              IF LIQ-COD-OFIC IN LIQ NOT =
