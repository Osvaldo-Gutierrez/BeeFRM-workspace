      *************************************************************
*% END
*% IF WSS
      *<<<< WSS
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OFI-REQA==.
       COPY TABBROFI.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-USR-REQA==.
       COPY TABBRUSR.
       77  WSS-LEE-DBC-DESDE-LAD  VALUE 'N'   PIC X(01).
       77  WSS-CIC-ICLI    VALUE SPACES PIC X(12).
       77  WSS-YA-LEI-REG-2 VALUE 'N' PIC X(01).
       77  WSS-COMPARA      VALUE 'N' PIC X(01).
      *          03 FRM-CPIM.
      * AEY = Protegido, normal
      *             05 FRM-CPIM-AEY         PIC X(01) VALUE '1'.
      * UBY = Desprotegido, brillante
      *             05 FRM-CPIM-UBY         PIC X(01) VALUE 'I'.
      * ANY = Protegido, non-display
      *             05 FRM-CPIM-ANY         PIC X(01) VALUE '_'.
      * UNY = Desprotegido, non-display
      *             05 FRM-CPIM-UNY         PIC X(01) VALUE '('.
      * ANY = Protegido, normal
      *             05 FRM-CPSF-AEY         PIC X(01) VALUE '-'.
      * ABY = Protegido, brillante
      *             05 FRM-CPIM-ABY         PIC X(01) VALUE '9'.
      * UST = Virtual para BMS, pues es campo STOPPER en UNISYS
      *             05 FRM-CPIM-UST         PIC X(01) VALUE SPACES.
       COPY DAPBRLIQ.
       COPY DAPBRLIQ REPLACING LIQ BY LIQ-CPR.
      *COPY DAPBROPD REPLACING ==01  OPD== BY ==01  OPD-CPR==.
       COPY DAPBRRCC.
       COPY SGCBRDBC.
       COPY GNSWGCPT.
       COPY GNSWGSTR.
       77  WSS-COD-ENTI         VALUE 'LIQ'     PIC X(03).
      *RGD-INI 02/02/2018 CORRECION NAVEGABILIDAD
      *01  BKP-LAD          VALUE SPACES    PIC X(650).
       01  BKP-LAD          VALUE SPACES    PIC X(1280).
       01 CLL-VARI.
          03 WSS-EXI-CLI    VALUE ZEROES    PIC 9(01).
          03 WSS-UNO        VALUE 1         PIC 9(01).
          03 WSS-FEC        VALUE 'FEC'     PIC X(03).
          03 WSS-OPE        VALUE 'OPE'     PIC X(03).
          03 WSS-CLI        VALUE 'CLI'     PIC X(03).
          03 WSS-AUT        VALUE 'AUT'     PIC X(03).
          03 WSS-23         VALUE '23'      PIC X(02).
      *RGD-FIN
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
      *Entidades de SGC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-LIQ-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RCC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
      *
       01   VAR-PROGRAMA.
            03 PGM-COD-CREB              VALUE ' '       PIC X(03).
            03 PGM-STAT-RCC.
               88 PGM-STAT-RCC-NUL       VALUE '  '.
               07 PGM-STT1-RCC           VALUE '2'       PIC X(01).
                  88 PGM-STAT-RCC-OKS    VALUE '0'.
               07 PGM-STT2-RCC           VALUE '3'       PIC X(01).
      *>>>>
*% END
*% IF INI_FIO_IBM
      *<<<< INI_FIO
           PERFORM GET-FHOY.
           IF SCR-INST = 'GAB' AND
              ( FRM-FFLD = FRM-FFLD-PF8 OR
                FRM-FFLD = FRM-FFLD-PF7 )
                  MOVE 'GKY' TO SCR-INST.
      *>>>> INI_FIO
*% END
*% IF FND_KEY_CMND
      *<<<< FND_KEY_CMND
           IF FRM-COD-CREB IN CLL-FLD = 'OPE' OR
              FRM-COD-CREB IN CLL-FLD = 'CLI'
               MOVE 'N' TO WSS-LEE-DBC-DESDE-LAD
               PERFORM OBTENER-NOMBRE-DBC.
           PERFORM SET-KEY-LAD.
      
           MOVE FIO-GET-NLS TO FIO-CMND.
      *>>>> FND_KEY_CMND
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
      *RGD-INI 02/02/2018 (RRM) CORRECION NAVEGABILIDAD
      *LECTURA NO OK
           IF NOT FIO-STAT-OKS AND FRM-COD-CREB IN CLL-FLD = WSS-FEC
               MOVE WSS-23          TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CLLNOEXFECPK' TO MSG-COD-MENS
              MOVE 'DAP' TO MSG-COD-SIST
              MOVE 'S' TO SCR-IERR      
              PERFORM GET-MSG
              MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.

           IF NOT FIO-STAT-OKS AND FRM-COD-CREB IN CLL-FLD = WSS-OPE
               MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
               MOVE WSS-23         TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CLLNONXTOIPE' TO MSG-COD-MENS
		 MOVE 'DAP' TO MSG-COD-SIST
		 MOVE 'S' TO SCR-IERR            
		 PERFORM CLR-FLD-CLL
		 PERFORM GET-MSG
		 MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.

           IF NOT FIO-STAT-OKS AND FRM-COD-CREB IN CLL-FLD = WSS-AUT
               MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
               MOVE WSS-23         TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CLLNONXTOAUT' TO MSG-COD-MENS
		MOVE 'DAP' TO MSG-COD-SIST
                MOVE 'S' TO SCR-IERR      
                PERFORM CLR-FLD-CLL
                PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.

           IF NOT FIO-STAT-OKS AND FRM-COD-CREB IN CLL-FLD = WSS-CLI
               MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
               MOVE WSS-23         TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CLLNONXTOCLI' TO MSG-COD-MENS
               MOVE 'DAP'          TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.
      *RGD-FIN 02/02/2018 (RRM)
      
           IF NOT FIO-STAT-OKS
               GO TO FIN-FND-KEY.

      *RGD-INI 02/02/2018 CORRECION NAVEGABILIDAD
      *LECTURA OK
           IF FRM-COD-CREB IN CLL-FLD = WSS-FEC
              IF NOT (LAD-COD-ENTF IN LAD = WSS-COD-ENTI AND
                 LAD-NUM-DLAD IN CLL-FLD  = LAD-NUM-DLAD IN LAD AND
                 LAD-NUM-MLAD IN CLL-FLD  = LAD-NUM-MLAD IN LAD AND
                 LAD-NUM-SLAD IN CLL-FLD  = LAD-NUM-SLAD IN LAD AND
                 LAD-NUM-ALAD IN CLL-FLD  = LAD-NUM-ALAD IN LAD)
                 MOVE WSS-23          TO FIO-STAT
                 MOVE WSS-23         TO PGM-STAT-LAD
                 MOVE 'CLLNOEXFECPK' TO MSG-COD-MENS
                 MOVE 'DAP'          TO MSG-COD-SIST
                 PERFORM GET-MSG
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 GO TO FIN-FND-KEY.

           IF FRM-COD-CREB IN CLL-FLD = WSS-OPE
              IF NOT (LAD-COD-ENTO IN LAD = WSS-COD-ENTI AND
                 LAD-CAI-IOPD IN CLL-FLD  = LAD-CAI-IOPD IN LAD AND
                 LAD-IIC-IOPD IN CLL-FLD  = LAD-IIC-IOPD IN LAD)
                  MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                  MOVE WSS-23         TO FIO-STAT
                  MOVE WSS-23         TO PGM-STAT-LAD
                  MOVE 'CLLNONXTOIPE' TO MSG-COD-MENS
                  MOVE 'DAP' TO MSG-COD-SIST
                  PERFORM GET-MSG
                  MOVE MSG-GLS-DESC TO FRM-MENS
                  GO TO FIN-FND-KEY.

           IF FRM-COD-CREB IN CLL-FLD = WSS-AUT
              IF NOT (LAD-COD-ENTA IN LAD = WSS-COD-ENTI AND
                 LAD-COD-AUTO IN CLL-FLD  = LAD-COD-AUTO IN LAD)
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE WSS-23         TO FIO-STAT
                    MOVE WSS-23         TO PGM-STAT-LAD
                    MOVE 'CLLNONXTOAUT' TO MSG-COD-MENS
                    MOVE 'DAP'          TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.

           IF FRM-COD-CREB IN CLL-FLD = WSS-CLI AND
              WSS-EXI-CLI NOT = ZEROES
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE WSS-23         TO FIO-STAT
                    MOVE WSS-23         TO PGM-STAT-LAD
                    MOVE 'CLI    NEX  ' TO MSG-COD-MENS
                    MOVE 'SGC'          TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.

           IF FRM-COD-CREB IN CLL-FLD = WSS-CLI
              IF NOT (LAD-COD-ENTC IN LAD = WSS-COD-ENTI AND
                      LAD-CIC-ICLI IN LAD = DBC-CIC-ICLI IN DBC )
                     MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                     MOVE WSS-23         TO FIO-STAT
                     MOVE WSS-23         TO PGM-STAT-LAD
                     MOVE 'CLLNONXTOCLI' TO MSG-COD-MENS
                     MOVE 'DAP'          TO MSG-COD-SIST
                     PERFORM GET-MSG
                     MOVE MSG-GLS-DESC TO FRM-MENS
                     GO TO FIN-FND-KEY.
      *RGD-FIN

      *    IF FIO-STAT-OKS
      *        IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI
      *             MOVE '23'   TO FIO-STAT
      *             MOVE FIO-STAT TO PGM-STAT-LAD
      *             GO TO FIN-FND-KEY.

           IF NOT ( FRM-COD-CREB IN CLL-FLD = 'OPE' OR
                    FRM-COD-CREB IN CLL-FLD = 'CLI' )
                     MOVE 'S' TO WSS-LEE-DBC-DESDE-LAD
                     PERFORM OBTENER-NOMBRE-DBC.

           MOVE LAD-CIC-ICLI IN LAD TO WSS-CIC-ICLI.

      *    IF FRM-COD-CREB IN CLL-FLD = 'OPE'
      *        IF LAD-CAI-IOPD IN CLL-FLD NOT =
      *           LAD-CAI-IOPD IN LAD    OR
      *           LAD-IIC-IOPD IN CLL-FLD NOT =
      *           LAD-IIC-IOPD IN LAD
