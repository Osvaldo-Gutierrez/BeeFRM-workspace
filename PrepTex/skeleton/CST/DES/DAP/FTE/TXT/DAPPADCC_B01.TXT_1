*%! Codigo adicional programa DAPPADCC
*% NO_HDR_CST = TRUE
*% IF SETTAG
*% FNCPQ = "1804_07"
*% VSNPQ = "01-02"
*% FNCPU = "1804_07"
*% VSNPU = "01-01"
*% END
*% IF INI_FIO
      *<<<< INI_FIO
           PERFORM GNS-HDL-VSM.
      *>>>> END INI_FIO
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBGVSM.
       COPY GNSBHVSM.
      * ISP-INI 31-07-1998 INICIALIZA CAMPOS DE REGISTROS
       COPY DAPBCDCC.
      * ISP-FIN 31-07-1998 INICIALIZA CAMPOS DE REGISTROS
      *>>>> END EOF
*% END
*% IF WSS
      *<<<< WSS
       COPY DAPBRPOD.
       COPY GNSWGHOY.
      *>>>> END 
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-POD-REQA==.
      *>>>> END 
*% END
*% IF PGM_POS_EDB
      *<<<< PGM_POS_EDB
           PERFORM CLR-REG-DCC.
      *>>>> END 
*% END
*% IF CMN_ING 
      *<<<< CMN_ING
           PERFORM GET-FHOY.
           MOVE HOY-DHOY TO DCC-NUM-DTRN IN DCC.
           MOVE HOY-MHOY TO DCC-NUM-MTRN IN DCC.
           MOVE HOY-SHOY TO DCC-NUM-STRN IN DCC.
           MOVE HOY-AHOY TO DCC-NUM-ATRN IN DCC.
           MOVE HOY-HHOY TO DCC-HRA-HRTR IN DCC.
           MOVE 'I' TO DCC-COD-TACC IN DCC.
           MOVE 'I' TO DCC-COD-STAT IN DCC.
           MOVE 'I' TO DCC-COD-OTRN IN DCC.
           MOVE SCR-OTRN TO DCC-COD-OTRN IN DCC.
      *>>>> 
*% END 
*% IF CMN_MOD
      *<<<< CMN_MOD
           PERFORM GET-FHOY.
           MOVE HOY-DHOY TO DCC-NUM-DTRN IN DCC.
           MOVE HOY-MHOY TO DCC-NUM-MTRN IN DCC.
           MOVE HOY-SHOY TO DCC-NUM-STRN IN DCC.
           MOVE HOY-AHOY TO DCC-NUM-ATRN IN DCC.
           MOVE HOY-HHOY TO DCC-HRA-HRTR IN DCC.
           MOVE 'M' TO DCC-COD-TACC IN DCC.
           MOVE SCR-OTRN TO DCC-COD-OTRN IN DCC.
      *>>>> 
*% END 
*% IF FIN_HBL_FLD
      *<<<< FIN_HBL_FLD
           IF SCR-CMND = 'COP'
               PERFORM UNP-IKY-DCC
               PERFORM FST-KEY-DCC.
      *>>>> END 
*% END
*% IF ABT_CMN_ACC
      *<<<< ABT_CMN_ACC
               IF SCR-CMND = 'COP'
                  MOVE 'REGISTRO NO FUE COPIADO' TO FRM-MENS
                  GO TO FIN-CMN-ACC
               ELSE
      *>>>> END 
*% END
*% IF INI_CMN_ACC
      *<<<< INI_CMN_ACC
           IF SCR-CMND = 'COP'
              PERFORM RET-ALL-DCC
              MOVE FIO-PUT TO FIO-CMND
              PERFORM RET-HDR
              PERFORM DAP-FIO-DCC
              IF FIO-STAT-OKS
                 MOVE 'REGISTRO FUE COPIADO' TO FRM-MENS
              ELSE
                 MOVE 'ERROR : REGISTRO NO COPIADO' TO FRM-MENS
            ELSE
               MOVE 'COMANDO NO IMPLEMENTADO' TO FRM-MENS.  
            GO TO FIN-CMN-ACC.
      *>>>> END 
*% END
*% IF CMN_ING OR CMN_MOD
      *<<<< CMN_ING OR CMN_MOD
           PERFORM RET-HDR.
      *>>>>
*% END
*% IF EOF_PQ
      *<<<< EOF_PQ
       COPY DAPBFPOD.
       COPY DAPBG001.
      *>>>> END EOF_PQ
*% END
*% IF EOF_PU
      *<<<< EOF_PU
       COPY GNSBGHOY.

       RET-HDR SECTION.
       INI-RET-HDR.
           PERFORM GET-FHOY.
      *    Fecha Transaccion
           MOVE HOY-FHOY TO DCC-FEC-FTRN IN DCC.
      *    Hora Transaccion
           MOVE HOY-HHOY TO DCC-HRA-HRTR IN DCC.
      *    Codigo Origen transaccion
           MOVE SCR-OTRN TO DCC-COD-OTRN IN DCC.
      *    Autor Transaccion
           MOVE SCR-USER TO DCC-COD-ATRN IN DCC.
           IF SCR-CCPP = 'ING'
               MOVE 'I' TO DCC-COD-TACC IN DCC
           ELSE
           IF SCR-CCPP = 'MOD'
               MOVE 'M' TO DCC-COD-TACC IN DCC
           ELSE
           IF SCR-CMND = 'COP'
               MOVE 'C' TO DCC-COD-TACC IN DCC.
       FIN-RET-HDR.
           EXIT.

      *>>>> END EOF_PU
*% END
