*%! Codigo adicional para DAPPAJNL
*% NO_HDR_CST = TRUE
*% IF FND_KEY
      *<<<< FND_KEY
           MOVE HIGH-VALUES TO JNL-CLV-JNL-1 IN JNL.
           MOVE FIO-GET-LEQ TO FIO-CMND.
           PERFORM DAP-FIO-JNL.
           MOVE FIO-STAT TO PGM-STAT-JNL.
           GO TO FIN-FND-KEY.
      *>>>>
*% END
*% IF PGM_INI
      *<<<<
       COPY GNSBGEDB.                                                   
       GO TO CNT-INI.
      *>>>>
*% END
*% IF PGM_POS_EDB
      *<<<<
       CNT-INI.
      *>>>>
*% END
*% IF PGM_PQ
*% IF INI_HBL_IKY
      *<<<<
           IF SCR-DESDE-CMN                                             
               MOVE JNL-GLS-MENS TO FRM-MENS                            
               MOVE LOW-VALUES TO JNL-FLD                               
               MOVE FRM-MENS TO JNL-GLS-MENS.                           
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.                           
           PERFORM PRO-MKY-JNL.                                         
           PERFORM PRO-AKY-JNL.                                         
           PERFORM UNP-IKY-JNL.                                         
           PERFORM FST-KEY-JNL.                                         
           MOVE '1' TO FRM-CPIM-AEY.                                    
           GO TO FIN-HBL-IKY.
      *>>>>
*% END
*% IF INI_HBL_KEY
      *<<<<
           IF SCR-DESDE-CMN                                             
               MOVE JNL-GLS-MENS TO FRM-MENS                            
               MOVE LOW-VALUES TO JNL-FLD                               
               MOVE FRM-MENS TO JNL-GLS-MENS.                           
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.                           
           PERFORM UNP-KEY-JNL.                                         
           PERFORM FST-KEY-JNL.                                         
           MOVE '1' TO FRM-CPIM-AEY.                                    
      *>>>>
*% END
*% END
*% IF PGM_PU
*% IF INI_HBL_IKY
      *<<<<
           IF SCR-DESDE-CMN                                             
               MOVE JNL-GLS-MENS TO FRM-MENS                            
               MOVE LOW-VALUES TO JNL-FLD                               
               MOVE FRM-MENS TO JNL-GLS-MENS.                           
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.                           
           PERFORM PRO-FLD-JNL.                                         
           PERFORM PRO-MKY-JNL.                                         
           PERFORM UNP-IKY-JNL.                                         
           PERFORM FST-KEY-JNL.                                         
           MOVE '1' TO FRM-CPIM-AEY.                                    
           GO TO FIN-HBL-IKY.
      *>>>>
*% END
*% IF INI_HBL_KEY
      *<<<<
           IF SCR-DESDE-CMN                                             
               MOVE JNL-GLS-MENS TO FRM-MENS                            
               MOVE LOW-VALUES TO JNL-FLD                               
               MOVE FRM-MENS TO JNL-GLS-MENS.                           
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.                           
           PERFORM PRO-FLD-JNL.                                         
           PERFORM UNP-KEY-JNL.                                         
           PERFORM FST-KEY-JNL.                                         
           MOVE '1' TO FRM-CPIM-AEY.                                    
           GO TO FIN-HBL-KEY.
      *>>>>
*% END
*% END
*% IF PFS_SCR_GIKY
      *<<<<
               MOVE 'PFSINV    ' TO MSG-COD-MENS                        
               MOVE 'GNS' TO MSG-COD-SIST                               
               PERFORM GET-MSG                                          
               MOVE MSG-GLS-DESC TO FRM-MENS                            
               PERFORM FST-KEY-JNL                                      
               PERFORM SCR-MSSG                                         
               PERFORM SCR-PUTA                                         
               PERFORM SCR-EXIT.                                        
           MOVE FRM-DFLD TO JNL-FLD.                                    
           IF FRM-STAT-ABT                                              
               GO TO FIN-SCR-GIKY.                                      
           MOVE FRM-SUAR-OKS TO FRM-SUAR.                               
           PERFORM VAL-NUM-IKY-JNL.                                     
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR               
               PERFORM SCR-MSSG                                         
               PERFORM EDT-IKY-JNL                                      
               PERFORM SCR-PUTA                                         
               PERFORM SCR-EXIT.                                        
      *JSS                                                              
           PERFORM CLR-FLD-JNL.                                         
           PERFORM VAL-IKY-JNL.                                         
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR               
               GO TO ERR-SCR-GIKY.                                      
           PERFORM VAL-CON-IKY-JNL.                                     
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )       
               GO TO FIN-SCR-GIKY.                                      
           IF FRM-UCUR NOT > SPACES                                     
               PERFORM FST-KEY-JNL.                                     
       ERR-SCR-GIKY.                                                    
           PERFORM SCR-MSSG.                                            
           PERFORM EDT-IKY-JNL.                                         
           PERFORM SCR-PUTA.                                            
           PERFORM SCR-EXIT.                                            
           GO TO FIN-SCR-GIKY.
      *>>>>
*% END
