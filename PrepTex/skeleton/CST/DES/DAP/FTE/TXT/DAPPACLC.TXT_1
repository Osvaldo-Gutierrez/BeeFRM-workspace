*%! Codigo adicional para programa CLC
*% IF SETTAG
*% VSNPQ = "18.02.02"
*% FNCPQ = "180202"
*% VSNPU = "18.02.02"
*% FNCPU = "180202"
*% END
*% NO_HDR_CST = TRUE
*% PGM_PTC = TRUE
*%
*%! IF PGM_PQ
*%!  FRM = "CLC"
*%!  PADRE = "LAD"
*%!  KEY_PADRE = "LAD-KEY-FLAD"
*%!  LARGO_KEY_PADRE = 20
*%!  FIO_PADRE = "DAP-FIO-LAD"
*%!  SIST_PADRE = "DAP"
*%!  KEY_BROWSE_PADRE = "LAD-KEY-FLAD"
*%!
*%!  ACEPTE_PF7 = TRUE
*%!  ACEPTE_PF8 = TRUE
*%! INCLUDE 'GNSSRC:PFSCONENT.SRC'
*%!
*%!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
*%! END
*% IF WSS
      *<<<< WSS
       77  WSS-LEE-DBC-DESDE-LAD  VALUE 'N'   PIC X(01).
       77  WSS-CIC-ICLI    VALUE SPACES PIC X(12).
       77  WSS-YA-LEI-REG-2 VALUE 'N' PIC X(01).
       77  WSS-COMPARA      VALUE 'N' PIC X(01).
      *          03 FRM-CPIM.
      * AEY = Protegido, normal
      *             05 FRM-CPIM-AEY         PIC X(01) VALUE '1'.
      * UBY = Desprotegido, brillante
      *             05 FRM-CPIM-UBY         PIC X(01) VALUE 'I'.
      * ANY = Protegido, non-display
      *             05 FRM-CPIM-ANY         PIC X(01) VALUE '_'.
      * UNY = Desprotegido, non-display
      *             05 FRM-CPIM-UNY         PIC X(01) VALUE '('.
      * ANY = Protegido, normal
      *             05 FRM-CPSF-AEY         PIC X(01) VALUE '-'.
      * ABY = Protegido, brillante
      *             05 FRM-CPIM-ABY         PIC X(01) VALUE '9'.
      * UST = Virtual para BMS, pues es campo STOPPER en UNISYS
      *             05 FRM-CPIM-UST         PIC X(01) VALUE SPACES.
       COPY DAPBRRCC.
       COPY DAPBRRCC REPLACING RCC BY RCC-CPR.
      *COPY DAPBROPD REPLACING ==01  OPD== BY ==01  OPD-CPR==.

       COPY DAPBROPD.
       COPY SGCBRRCD.
       COPY SGCBRDBC.
       COPY GNSWGCPT.
       COPY GNSWGSTR.
       77  WSS-COD-ENTI         VALUE 'RCC'     PIC X(03).
       01  BKP-LAD              VALUE SPACES    PIC X(1280).
       01  WSS-FEC-FAMD.
           03 WSS-NUM-SAMD                      PIC X(02).
           03 WSS-NUM-AAMD                      PIC X(02).
           03 WSS-NUM-MAMD                      PIC X(02).
           03 WSS-NUM-DAMD                      PIC X(02).
      *CORRECION NAVEGABILIDAD
       01 CLC-VARI.
          03 WSS-EXI-CLI    VALUE ZEROES    PIC 9(01).
          03 WSS-UNO        VALUE 1         PIC 9(01).
          03 WSS-FEC        VALUE 'FEC'     PIC X(03).
          03 WSS-OPE        VALUE 'OPE'     PIC X(03).
          03 WSS-CLI        VALUE 'CLI'     PIC X(03).
          03 WSS-AUT        VALUE 'AUT'     PIC X(03).
          03 WSS-23         VALUE '23'      PIC X(02).
          03 WSS-00         VALUE '00'      PIC X(02).
          03 WSS-CON        VALUE 'CON'     PIC X(03).
          03 WSS-ANT        VALUE 'ANT'     PIC X(03).
          03 WSS-DES        VALUE 'DES'     PIC X(03).
          03 WSS-DAP        VALUE 'DAP'     PIC X(03).
          03 WSS-GNS        VALUE 'GNS'     PIC X(03).
          03 WSS-SGC        VALUE 'SGC'     PIC X(03).
          03 WSS-GAB        VALUE 'GAB'     PIC X(03).
          03 WSS-GKY        VALUE 'GKY'     PIC X(03).
          03 WSS-TAB        VALUE 'TAB'     PIC X(03).
          03 WSS-PAI        VALUE 'PAI'     PIC X(03).
          03 WSS-COM        VALUE 'COM'     PIC X(03).          
          03 WSS-P          VALUE 'P'       PIC X(01).          
          03 WSS-N          VALUE 'N'       PIC X(01).
          03 WSS-S          VALUE 'S'       PIC X(01).
          03 WSS-CLINEX     VALUE 'CLI    NEX  '         PIC X(12).
          03 WSS-OPDNEX     VALUE 'OPD    NEX  '         PIC X(12).          
          03 WSS-LADKEYOLAD VALUE 'LAD-KEY-OLAD'         PIC X(12).
          03 WSS-LADKEYCLAD VALUE 'LAD-KEY-CLAD'         PIC X(12).
          03 WSS-LADKEYALAD VALUE 'LAD-KEY-ALAD'         PIC X(12).
          03 WSS-DBCIDCICLI VALUE 'DBC-IDC-ICLI'         PIC X(12).
          03 WSS-RCCKEYIOPD VALUE 'RCC-KEY-IOPD'         PIC X(12).
          03 WSS-RCDKEYIRCD VALUE 'RCD-KEY-IRCD'         PIC X(12).
          03 WSS-CLNOINGRFEC VALUE 'CL NOINGRFEC'        PIC X(12).
          03 WSS-CLNOINGROPE VALUE 'CL NOINGROPE'        PIC X(12).
          03 WSS-CLNOINGRCLI VALUE 'CL NOINGRCLI'        PIC X(12).
          03 WSS-CLNOINGRAUT VALUE 'CL NOINGRAUT'        PIC X(12).          
          03 WSS-CLCNOEXFECPK VALUE 'CLCNOEXFECPK'       PIC X(12).
          03 WSS-CLCNONXTOIPE VALUE 'CLCNONXTOIPE'       PIC X(12).
          03 WSS-CLCNONXTOAUT VALUE 'CLCNONXTOAUT'       PIC X(12).
          03 WSS-CLCNONXTOCLI VALUE 'CLCNONXTOCLI'       PIC X(12).
          03 WSS-CLCPF7PF8    VALUE 'CLCPF7PF8 '         PIC X(10).
          03 WSS-CLCNOPRV     VALUE 'CLCNOPRV'           PIC X(08).
          03 WSS-CLCNOPRVOPE  VALUE 'CLCNOPRVOPE'        PIC X(11).
          03 WSS-CLCNOPRVAUT  VALUE 'CLCNOPRVAUT'        PIC X(11).
          03 WSS-CLCNOPRVFEC  VALUE 'CLCNOPRVFEC'        PIC X(11).
          03 WSS-CLCNOPRVCLI  VALUE 'CLCNOPRVCLI'        PIC X(11).
          03 WSS-CLCNONXT     VALUE 'CLCNONXT'           PIC X(08).
          03 WSS-CLCNONXTOPE  VALUE 'CLCNONXTOPE'        PIC X(11).
          03 WSS-CLCNONXTAUT  VALUE 'CLCNONXTAUT'        PIC X(11).
          03 WSS-CLCNONXTFEC  VALUE 'CLCNONXTFEC'        PIC X(11).
          03 WSS-CLCNONXTCLI  VALUE 'CLCNONXTCLI'        PIC X(11).
          03 WSS-TX01         VALUE '01. Moneda'         PIC X(12).
          03 WSS-TX02         VALUE '02. T Operac'       PIC X(12).
          03 WSS-TX03         VALUE '03. C Aux'          PIC X(12).
          03 WSS-TX04         VALUE '04. Direccion'      PIC X(15).
          03 WSS-TX05         VALUE '05. Telefono'       PIC X(15).
          03 WSS-TX06         VALUE '06. Comuna/Pais'    PIC X(22).

          
      *FIN CORRECION NAVEGABILIDAD          
       01  WSS-VARI.
           03 WSS-COD-LUGA.
              05 WSS-IND-LUGA                            PIC X(01).
              05 WSS-COD-PAIS                            PIC X(03).
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
      *Entidades de SGC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OPD-REQA==.      
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RCD-REQA==.      
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RCC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
      *
       01   VAR-PROGRAMA.
            03 PGM-COD-CREB              VALUE ' '       PIC X(03).
      *>>>>
*% END
*% IF INI_FIO_IBM
      *<<<< INI_FIO
           PERFORM GET-FHOY.
           IF SCR-INST = WSS-GAB AND
              ( FRM-FFLD = FRM-FFLD-PF8 OR
                FRM-FFLD = FRM-FFLD-PF7 )
                  MOVE WSS-GKY TO SCR-INST.
      *>>>> INI_FIO
*% END
*% IF FND_KEY_CMND
      *<<<< FND_KEY_CMND
           IF FRM-COD-CREB IN CLC-FLD = WSS-OPE OR
              FRM-COD-CREB IN CLC-FLD = WSS-CLI
               MOVE WSS-N TO WSS-LEE-DBC-DESDE-LAD
               PERFORM OBTENER-NOMBRE-DBC.
           PERFORM SET-KEY-LAD.
      
           MOVE FIO-GET-NLS TO FIO-CMND.
      *>>>> FND_KEY_CMND
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
      *CORRECION NAVEGABILIDAD    
      *LECTURA NO OK
           IF NOT FIO-STAT-OKS AND FRM-COD-CREB IN CLC-FLD = WSS-FEC
               MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
               MOVE WSS-23         TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CLCNOEXFECPK' TO MSG-COD-MENS
               MOVE 'DAP'          TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.

           IF NOT FIO-STAT-OKS AND FRM-COD-CREB IN CLC-FLD = WSS-OPE
               MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
               MOVE WSS-23         TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CLCNONXTOIPE' TO MSG-COD-MENS
               MOVE 'DAP' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.      

           IF NOT FIO-STAT-OKS AND  FRM-COD-CREB IN CLC-FLD = WSS-AUT
               MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
               MOVE WSS-23         TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CLCNONXTOAUT' TO MSG-COD-MENS
               MOVE 'DAP'          TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.

           IF NOT FIO-STAT-OKS AND  FRM-COD-CREB IN CLC-FLD = WSS-CLI
               MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
               MOVE WSS-23         TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CLCNONXTOCLI' TO MSG-COD-MENS
               MOVE 'DAP'          TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.               
               
      *FIN CORRECION NAVEGABILIDAD
      
           IF NOT FIO-STAT-OKS
               GO TO FIN-FND-KEY.
     
      

           IF FRM-COD-CREB IN CLC-FLD = WSS-FEC
      *CORRECION NAVEGABILIDAD     
               IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI OR
                  LAD-NUM-DLAD IN CLC-FLD NOT =
                  LAD-NUM-DLAD IN LAD    OR
                  LAD-NUM-MLAD IN CLC-FLD NOT =
                  LAD-NUM-MLAD IN LAD    OR
                  LAD-NUM-SLAD IN CLC-FLD NOT =
                  LAD-NUM-SLAD IN LAD    OR
                  LAD-NUM-ALAD IN CLC-FLD NOT =
                  LAD-NUM-ALAD IN LAD
                    MOVE WSS-23 TO FIO-STAT
                    MOVE WSS-23 TO PGM-STAT-LAD
                    MOVE WSS-CLCNOEXFECPK TO MSG-COD-MENS
                    MOVE WSS-DAP TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
           
           
           IF FRM-COD-CREB IN CLC-FLD = WSS-OPE
               IF LAD-COD-ENTO IN LAD NOT = WSS-COD-ENTI OR
                  LAD-CAI-IOPD IN CLC-FLD NOT =
                  LAD-CAI-IOPD IN LAD    OR
                  LAD-IIC-IOPD IN CLC-FLD NOT =
                  LAD-IIC-IOPD IN LAD
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE WSS-23 TO FIO-STAT
                    MOVE WSS-23 TO PGM-STAT-LAD
                    MOVE WSS-CLCNONXTOIPE TO MSG-COD-MENS
                    MOVE WSS-DAP TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
      
           IF FRM-COD-CREB IN CLC-FLD = WSS-AUT
               IF LAD-COD-ENTA IN LAD NOT = WSS-COD-ENTI OR
                  LAD-COD-AUTO IN CLC-FLD NOT =
                  LAD-COD-AUTO IN LAD
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE WSS-23 TO FIO-STAT
                    MOVE WSS-23 TO PGM-STAT-LAD
                    MOVE WSS-CLCNONXTOAUT TO MSG-COD-MENS
                    MOVE WSS-DAP TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
      
           IF FRM-COD-CREB IN CLC-FLD = WSS-CLI AND
              WSS-EXI-CLI NOT = ZEROES
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE WSS-23         TO FIO-STAT
                    MOVE WSS-23         TO PGM-STAT-LAD
                    MOVE WSS-CLINEX TO MSG-COD-MENS
                    MOVE WSS-SGC        TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
      
           IF FRM-COD-CREB IN CLC-FLD = WSS-CLI
               IF LAD-COD-ENTC IN LAD NOT = WSS-COD-ENTI OR
                  LAD-CIC-ICLI IN LAD     NOT =
                  DBC-CIC-ICLI IN DBC
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE WSS-23 TO FIO-STAT
                    MOVE WSS-23 TO PGM-STAT-LAD
                    MOVE WSS-CLCNONXTOCLI TO MSG-COD-MENS
                    MOVE WSS-DAP TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.

           IF NOT ( FRM-COD-CREB IN CLC-FLD = WSS-OPE OR
                    FRM-COD-CREB IN CLC-FLD = WSS-CLI )
                     MOVE WSS-S TO WSS-LEE-DBC-DESDE-LAD
                     PERFORM OBTENER-NOMBRE-DBC.
      
           MOVE LAD-CIC-ICLI IN LAD TO WSS-CIC-ICLI.                    
      *FIN CORRECION NAVEGABILIDAD
           IF SCR-CMND = WSS-CON
              IF FRM-FFLD = FRM-FFLD-PF8
                    PERFORM SET-KEY-LAD
                    PERFORM PRO-PF8
              ELSE
                IF FRM-FFLD = FRM-FFLD-PF7
                    PERFORM SET-KEY-LAD
                    PERFORM PRO-PF7.
      
           IF PGM-STAT-LAD-OKS
           IF FRM-FFLD = FRM-FFLD-PF7 OR
              FRM-FFLD = FRM-FFLD-PF8
               MOVE WSS-S TO WSS-LEE-DBC-DESDE-LAD
               PERFORM OBTENER-NOMBRE-DBC.
      
           IF PGM-STAT-LAD-OKS
               IF LAD-COD-ENTF IN LAD = WSS-COD-ENTI
                   MOVE WSS-00   TO FIO-STAT

                   MOVE SPACES TO RCC

                   MOVE LAD-COD-CONT IN LAD TO RCC
                   MOVE WSS-00   TO PGM-STAT-OPD
                   MOVE WSS-00   TO PGM-STAT-DBC
                   MOVE WSS-00   TO PGM-STAT-RCD
                   MOVE WSS-00   TO PGM-STAT-RCC.
      
      *>>>> FIN_FND_KEY
*% END
*% IF INI_SCR_DMSG
*% IF PGM_PQ
      *<<<< INI_SCR_DMSG
      * Con este codigo logramos que una vez terminada la consulta
      * las claves no se borren de la pantalla
           IF SCR-CMND = WSS-CON
                AND
              SCR-INST = SCR-INST-GKY
                AND
              FRM-FFLD NOT = FRM-FFLD-RET
                  MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR.
      *>>>> INI_SCR_DMSG
*% END
*% END
*% IF FIN_SCR_TRL
      *<<<< FIN_SCR_TRL
      
           IF PGM-STAT-LAD-OKS
               MOVE WSS-CLCPF7PF8 TO MSG-COD-MENS
               MOVE WSS-GNS TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
      
      *>>>> FIN_SCR_TRL
*% END
*% IF PFS_SCR_GKEY_PREV
      *<<<< PFS_SCR_GKEY_PREV
           IF NOT ( FRM-FFLD = FRM-FFLD-PF7 )
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 )
      *>>>> PFS_SCR_GKEY_PREV
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF7 )
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 )
      *>>>> PFS_GET_CON
*% END
*% IF POST_SCR_PUTA
      *<<<< POST_SCR_PUTA
           IF PGM-STAT-LAD-OKS
               IF LAD-COD-INST = WSS-ANT
               
                    MOVE WSS-DAP TO TAB-COD-SIST
                    MOVE WSS-DAP TO FIO-SIST
                    PERFORM SET-KEY-LAD
                    MOVE FIO-GET-NXT TO FIO-CMND
                    PERFORM DAP-FIO-LAD
                    IF FIO-STAT-OKS
                         IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI
                            MOVE WSS-23  TO FIO-STAT
                            MOVE FIO-STAT TO PGM-STAT-LAD
                         ELSE


                             MOVE LAD-COD-CONT IN LAD TO RCC-CPR
                             MOVE WSS-S TO WSS-COMPARA
                             GO TO FIN-AT-DES.
      
           IF PGM-STAT-LAD-OKS
               IF LAD-COD-INST = WSS-DES

                     MOVE WSS-DAP TO TAB-COD-SIST
                     MOVE WSS-DAP TO FIO-SIST
                     PERFORM SET-KEY-LAD
                     MOVE FIO-GET-PRV TO FIO-CMND
                     PERFORM DAP-FIO-LAD
                     IF FIO-STAT-OKS
                          IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI
                               MOVE WSS-23 TO FIO-STAT
                               MOVE FIO-STAT TO PGM-STAT-LAD
                          ELSE
                               
                               MOVE LAD-COD-CONT IN LAD TO RCC-CPR
                               MOVE WSS-S TO WSS-COMPARA.
       FIN-AT-DES.
      
      *     ==>>  01
      *      
           MOVE WSS-TX01            TO FRM-GLS-TX01     IN CLC-FLD.
           MOVE FRM-CPIM-AEY        TO FRM-GLS-TX01-ATR IN CLC-FLD.
      
           MOVE FRM-CPIM-AEY        TO OPD-COD-VCAM-ATR IN CLC-FLD.
      

      *     ==>>  02
      *      
           MOVE WSS-TX02            TO FRM-GLS-TX02     IN CLC-FLD.
           MOVE FRM-CPIM-AEY      TO FRM-GLS-TX02-ATR IN CLC-FLD.
      
           MOVE FRM-CPIM-AEY      TO OPD-COD-TOPD-ATR IN CLC-FLD.
      


      *     ==>>  03
      *      
           MOVE WSS-TX03            TO FRM-GLS-TX03 IN CLC-FLD.
           MOVE FRM-CPIM-AEY    TO FRM-GLS-TX03-ATR IN CLC-FLD.
      
           MOVE FRM-CPIM-AEY    TO OPD-COD-AOPD-ATR IN CLC-FLD.


      *     ==>>  04
      *      
           MOVE WSS-TX04            TO FRM-GLS-TX04 IN CLC-FLD.
           MOVE FRM-CPIM-AEY    TO FRM-GLS-TX04-ATR IN CLC-FLD.
           MOVE FRM-CPIM-AEY    TO RCC-NUM-DIRE-ATR IN CLC-FLD.
           IF WSS-COMPARA = 'S'
               IF RCC-NUM-DIRE IN RCC NOT =
                  RCC-NUM-DIRE IN RCC-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX04-ATR IN CLC-FLD
                   MOVE FRM-CPIM-ABY TO RCC-NUM-DIRE-ATR IN CLC-FLD.

      *     ==>>  05
      *      
           MOVE WSS-TX05            TO FRM-GLS-TX05     IN CLC-FLD.
           MOVE FRM-CPIM-AEY      TO FRM-GLS-TX05-ATR IN CLC-FLD.
           MOVE FRM-CPIM-AEY      TO RCD-GLS-FONO-ATR IN CLC-FLD.


      *     ==>>  06
      *      
           MOVE WSS-TX06            TO FRM-GLS-TX06     IN CLC-FLD.
           MOVE FRM-CPIM-AEY     TO FRM-GLS-TX06-ATR IN CLC-FLD.
           MOVE FRM-CPIM-AEY     TO FRM-GLS-LUGA-ATR IN CLC-FLD.

           MOVE CLC-FLD TO FRM-DFLD.
      
      
      *>>>> POST_SCR_PUTA
*% END
*% IF EOF
      *<<<< EOF
       COPY SGCBGDOM. 
      
       GNX-PRO-QUE SECTION.
       INI-GNX-PRO-QUE.
           PERFORM GNS-PRO-QUE.
       FIN-GNX-PRO-QUE.
           EXIT.
      
       SET-KEY-LAD SECTION.
       INI-SET-KEY-LAD.
      
           MOVE WSS-COD-ENTI TO LAD-COD-ENTF IN LAD
                                LAD-COD-ENTO IN LAD
                                LAD-COD-ENTC IN LAD
                                LAD-COD-ENTA IN LAD
                                LAD-COD-ENTI IN LAD.
      
           IF FRM-COD-CREB IN CLC-FLD = WSS-OPE
               MOVE WSS-LADKEYOLAD TO FIO-AKEY
           ELSE
           IF FRM-COD-CREB IN CLC-FLD = WSS-CLI
               MOVE WSS-LADKEYCLAD TO FIO-AKEY
           ELSE
           IF FRM-COD-CREB IN CLC-FLD = WSS-AUT
               MOVE WSS-LADKEYALAD TO FIO-AKEY.
       FIN-SET-KEY-LAD.
           EXIT.
      
       OBTENER-NOMBRE-DBC  SECTION.
      *----------------------------
       INI-OBTENER-NOMBRE-DBC.
      
           IF WSS-LEE-DBC-DESDE-LAD = WSS-S
               MOVE LAD-CIC-ICLI IN LAD TO DBC-CIC-ICLI IN DBC.

           IF WSS-LEE-DBC-DESDE-LAD = WSS-S AND     
              LAD-CIC-ICLI IN LAD NOT > SPACES
               MOVE WSS-23       TO PGM-STAT-DBC
               MOVE SPACES                    TO DBC
               MOVE SPACES                    TO DBC-GLS-NOMC IN CLC-FLD
               MOVE ZEROES                    TO DBC-NUM-ICLI IN CLC-FLD
               MOVE SPACES                    TO DBC-VRF-ICLI IN CLC-FLD
               MOVE SPACES                    TO DBC-IND-ICLI IN CLC-FLD
               MOVE SPACES                    TO DBC-GLS-ICLI IN CLC-FLD
               GO TO FIN-OBTENER-NOMBRE-DBC.

               
           IF FRM-COD-CREB IN CLC-FLD = WSS-OPE AND
              NOT PGM-STAT-DBC-OKS
                MOVE LAD-CIC-IOPD IN LAD TO RCC-CIC-IOPD IN RCC
                PERFORM GET-CUR-RCC
                IF PGM-STAT-RCC-OKS AND
                   LAD-CIC-IOPD IN LAD  =  RCC-CIC-IOPD IN RCC
                     MOVE RCC-CIC-ICLI IN RCC TO DBC-CIC-ICLI IN DBC
                ELSE
                   MOVE WSS-23      TO FIO-STAT
                   MOVE WSS-23      TO PGM-STAT-DBC
                   GO TO FIN-OBTENER-NOMBRE-DBC.
      
           IF FRM-COD-CREB IN CLC-FLD = WSS-CLI AND
              NOT PGM-STAT-DBC-OKS
                MOVE WSS-DBCIDCICLI TO FIO-AKEY.

      *CORRECION NAVEGABILIDAD
           MOVE ZEROES TO WSS-EXI-CLI.
      *FIN CORRECION NAVEGABILIDAD
      
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           MOVE FIO-STAT TO PGM-STAT-DBC.
      
           IF FIO-STAT-OKS
      *  ISP INI 27-04-2009
                MOVE DBC-CIC-ICLI IN DBC TO
                     LAD-CIC-ICLI IN LAD
      *  FIN INI 27-04-2009
                PERFORM CPT-NOMB
           ELSE
      *CORRECION NAVEGABILIDAD
                MOVE WSS-UNO TO WSS-EXI-CLI
      *FIN CORRECION NAVEGABILIDAD
                MOVE SPACES TO DBC-GLS-NOMC IN DBC
                MOVE SPACES TO DBC-VRF-ICLI IN DBC
                MOVE SPACES TO DBC-IDC-ICLI IN DBC
                MOVE WSS-00   TO PGM-STAT-DBC.
      
       FIN-OBTENER-NOMBRE-DBC.
           EXIT.
      *
       GET-CUR-RCC SECTION.
       INI-GET-CUR-RCC.
           MOVE   WSS-RCCKEYIOPD           TO FIO-AKEY.
           MOVE    FIO-GET-NLS             TO FIO-CMND.
           PERFORM DAP-FIO-RCC.
           IF NOT ( FIO-STAT-OKS     AND
                    LAD-CIC-IOPD IN LAD = RCC-CIC-IOPD IN RCC )
                    MOVE WSS-23 TO FIO-STAT.
           MOVE   FIO-STAT  TO PGM-STAT-RCC.
       FIN-GET-CUR-RCC.
           EXIT.
      
      

       COPY DAPBFOPD.
       COPY DAPBFRCC.
       COPY SGCBFRCD.
       COPY SGCBFDBC.
       COPY GNSBGCPT.
       COPY GNSBGSTR.

       CPT-NOMB SECTION.
       INI-CPT-NOMB.
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.
           PERFORM CPT-BLKS.
           MOVE CPT-STRN            TO DBC-GLS-NOMC IN DBC.
       FIN-CPT-NOMB.
           EXIT.
      
       PRO-PF7 SECTION.
       INI-PRO-PF7.

           MOVE LAD TO BKP-LAD.

           
           PERFORM SET-KEY-LAD.
           MOVE FIO-GET-PRV TO FIO-CMND.
           PERFORM DAP-FIO-LAD.
           MOVE FIO-STAT TO PGM-STAT-LAD.
      
           IF NOT FIO-STAT-OKS


                MOVE SPACES TO LAD

                MOVE BKP-LAD TO LAD
                PERFORM SET-KEY-LAD                                             
                MOVE FIO-GET-KEY TO FIO-CMND                                    
                PERFORM DAP-FIO-LAD                                             
                MOVE FIO-STAT TO PGM-STAT-LAD                                   

                PERFORM EDT-FLD-CLC
                MOVE WSS-CLCNOPRV TO MSG-COD-MENS
                MOVE WSS-DAP TO MSG-COD-SIST
                PERFORM GET-MSG
                MOVE MSG-GLS-DESC TO FRM-MENS
                GO TO FIN-PRO-PF7.
      
           IF  ( FRM-COD-CREB IN CLC-FLD = WSS-OPE
                         AND
                  (
      *CORRECION NAVEGABILIDAD
                     LAD-COD-ENTO IN LAD NOT = WSS-COD-ENTI OR
      *FIN CORRECION NAVEGABILIDAD
                     LAD-CAI-IOPD IN CLC-FLD NOT =
                     LAD-CAI-IOPD IN LAD
                             OR
                     LAD-IIC-IOPD IN CLC-FLD NOT =
                     LAD-IIC-IOPD IN LAD
                   )
                )

                   MOVE SPACES TO LAD

                   MOVE BKP-LAD TO LAD
                   PERFORM SET-KEY-LAD
                   MOVE FIO-GET-KEY TO FIO-CMND
                   PERFORM DAP-FIO-LAD
                   MOVE FIO-STAT TO PGM-STAT-LAD
      
                   PERFORM EDT-FLD-CLC
                   MOVE WSS-CLCNOPRVOPE TO MSG-COD-MENS
                   MOVE WSS-DAP TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   GO TO FIN-PRO-PF7.
      
           IF  ( FRM-COD-CREB IN CLC-FLD = WSS-AUT
                         AND
                  (
      *CORRECION NAVEGABILIDAD
                     LAD-COD-ENTA IN LAD NOT = WSS-COD-ENTI OR

                     LAD-COD-AUTO IN CLC-FLD NOT =
                     LAD-COD-AUTO IN LAD
                   )
                )


                   MOVE SPACES TO LAD
      *FIN CORRECION NAVEGABILIDAD
                   MOVE BKP-LAD TO LAD
                   PERFORM SET-KEY-LAD
                   MOVE FIO-GET-KEY TO FIO-CMND
                   PERFORM DAP-FIO-LAD
                   MOVE FIO-STAT TO PGM-STAT-LAD
                   
                   PERFORM EDT-FLD-CLC
                   MOVE WSS-CLCNOPRVAUT TO MSG-COD-MENS
                   MOVE WSS-DAP TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   GO TO FIN-PRO-PF7.
      
           IF  ( FRM-COD-CREB IN CLC-FLD = WSS-FEC
                         AND
                   NOT (LAD-COD-ENTF IN LAD = WSS-COD-ENTI AND
                   LAD-NUM-DLAD IN CLC-FLD  = LAD-NUM-DLAD IN LAD AND
                   LAD-NUM-MLAD IN CLC-FLD  = LAD-NUM-MLAD IN LAD AND
                   LAD-NUM-SLAD IN CLC-FLD  = LAD-NUM-SLAD IN LAD AND
                   LAD-NUM-ALAD IN CLC-FLD  = LAD-NUM-ALAD IN LAD)

              )

                   MOVE SPACES TO LAD

                   MOVE BKP-LAD TO LAD
                   PERFORM SET-KEY-LAD
                   MOVE FIO-GET-KEY TO FIO-CMND
                   PERFORM DAP-FIO-LAD
                   MOVE FIO-STAT TO PGM-STAT-LAD 
      
                   PERFORM EDT-FLD-CLC
                   MOVE WSS-CLCNOPRVFEC TO MSG-COD-MENS
                   MOVE WSS-DAP TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   GO TO FIN-PRO-PF7.                 
      
           IF  ( FRM-COD-CREB IN CLC-FLD = WSS-CLI
                         AND
                  (
      *CORRECION NAVEGABILIDAD
                     LAD-COD-ENTC IN LAD NOT = WSS-COD-ENTI OR

                     LAD-CIC-ICLI IN LAD NOT =
                     WSS-CIC-ICLI
                  )
                )


                     MOVE SPACES TO LAD
      *FIN CORRECION NAVEGABILIDAD
                     MOVE BKP-LAD TO LAD
                     PERFORM SET-KEY-LAD
                     MOVE FIO-GET-KEY TO FIO-CMND
                     PERFORM DAP-FIO-LAD
                     MOVE FIO-STAT TO PGM-STAT-LAD
                     
                     PERFORM EDT-FLD-CLC
                     MOVE WSS-CLCNOPRVCLI TO MSG-COD-MENS
                     MOVE WSS-DAP TO MSG-COD-SIST
                     PERFORM GET-MSG
                     MOVE MSG-GLS-DESC TO FRM-MENS.
       FIN-PRO-PF7.
           EXIT.
      
       PRO-PF8 SECTION.
       INI-PRO-PF8.

           MOVE LAD TO BKP-LAD.
      

           PERFORM SET-KEY-LAD.
           MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM DAP-FIO-LAD.
           MOVE FIO-STAT TO PGM-STAT-LAD.
      
           IF NOT FIO-STAT-OKS


                MOVE SPACES TO LAD

                MOVE BKP-LAD TO LAD      
                PERFORM SET-KEY-LAD
                MOVE FIO-GET-KEY TO FIO-CMND
                PERFORM DAP-FIO-LAD
                MOVE FIO-STAT TO PGM-STAT-LAD 
      
                PERFORM EDT-FLD-CLC
                MOVE WSS-CLCNONXT TO MSG-COD-MENS
                MOVE WSS-DAP TO MSG-COD-SIST
                PERFORM GET-MSG
                MOVE MSG-GLS-DESC TO FRM-MENS
                GO TO FIN-PRO-PF8.
      

      
           IF  ( FRM-COD-CREB IN CLC-FLD = WSS-OPE
                         AND
                  (
      *CORRECION NAVEGABILIDAD
                     LAD-COD-ENTO IN LAD NOT = WSS-COD-ENTI OR
      *FIN CORRECION NAVEGABILIDAD
                     LAD-CAI-IOPD IN CLC-FLD NOT =
                     LAD-CAI-IOPD IN LAD
                             OR
                     LAD-IIC-IOPD IN CLC-FLD NOT =
                     LAD-IIC-IOPD IN LAD
                   )
                )


                   MOVE SPACES TO LAD

                   MOVE BKP-LAD TO LAD
                   PERFORM SET-KEY-LAD
                   MOVE FIO-GET-KEY TO FIO-CMND
                   PERFORM DAP-FIO-LAD
                   MOVE FIO-STAT TO PGM-STAT-LAD
      
                   PERFORM EDT-FLD-CLC
                   MOVE WSS-CLCNONXTOPE TO MSG-COD-MENS
                   MOVE WSS-DAP TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   GO TO FIN-PRO-PF8.
      
           IF  ( FRM-COD-CREB IN CLC-FLD = WSS-AUT
                         AND
                  (
      *CORRECION NAVEGABILIDAD
                     LAD-COD-ENTA IN LAD NOT = WSS-COD-ENTI OR
      *FIN CORRECION NAVEGABILIDAD
                     LAD-COD-AUTO IN CLC-FLD NOT =
                     LAD-COD-AUTO IN LAD
                   )
                )


                   MOVE SPACES TO LAD

                   MOVE BKP-LAD TO LAD
                   PERFORM SET-KEY-LAD
                   MOVE FIO-GET-KEY TO FIO-CMND
                   PERFORM DAP-FIO-LAD
                   MOVE FIO-STAT TO PGM-STAT-LAD  
      
                   PERFORM EDT-FLD-CLC
                   MOVE WSS-CLCNONXTAUT TO MSG-COD-MENS
                   MOVE WSS-DAP TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   GO TO FIN-PRO-PF8.

           IF  ( FRM-COD-CREB IN CLC-FLD = WSS-FEC
                         AND
                   NOT (LAD-COD-ENTF IN LAD = WSS-COD-ENTI AND
                   LAD-NUM-DLAD IN CLC-FLD  = LAD-NUM-DLAD IN LAD AND
                   LAD-NUM-MLAD IN CLC-FLD  = LAD-NUM-MLAD IN LAD AND
                   LAD-NUM-SLAD IN CLC-FLD  = LAD-NUM-SLAD IN LAD AND
                   LAD-NUM-ALAD IN CLC-FLD  = LAD-NUM-ALAD IN LAD)

              )

                   MOVE SPACES TO LAD

                   MOVE BKP-LAD TO LAD
                   PERFORM SET-KEY-LAD
                   MOVE FIO-GET-KEY TO FIO-CMND
                   PERFORM DAP-FIO-LAD
                   MOVE FIO-STAT TO PGM-STAT-LAD 
      
                   PERFORM EDT-FLD-CLC
                   MOVE WSS-CLCNONXTFEC TO MSG-COD-MENS
                   MOVE WSS-DAP TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   GO TO FIN-PRO-PF8.                                
      
           IF  ( FRM-COD-CREB IN CLC-FLD = WSS-CLI
                         AND
                  (
      *CORRECION NAVEGABILIDAD
                     LAD-COD-ENTC IN LAD NOT = WSS-COD-ENTI OR
      *FIN CORRECION NAVEGABILIDAD
                     LAD-CIC-ICLI IN LAD NOT =
                     WSS-CIC-ICLI
                  )
                )


                     MOVE SPACES TO LAD

                     MOVE BKP-LAD TO LAD
                     PERFORM SET-KEY-LAD
                     MOVE FIO-GET-KEY TO FIO-CMND
                     PERFORM DAP-FIO-LAD
                     MOVE FIO-STAT TO PGM-STAT-LAD       
      
                     PERFORM EDT-FLD-CLC
                     MOVE WSS-CLCNONXTCLI TO MSG-COD-MENS
                     MOVE WSS-DAP TO MSG-COD-SIST
                     PERFORM GET-MSG
                     MOVE MSG-GLS-DESC TO FRM-MENS.
       FIN-PRO-PF8.
           EXIT.

       BUS-OPD-CON-RCC SECTION.
       INI-BUS-OPD-CON-RCC.
           MOVE SPACES TO OPD.
           MOVE RCC-CAI-IOPD IN RCC TO OPD-CAI-IOPD IN OPD.
           MOVE RCC-IIC-IOPD IN RCC TO OPD-IIC-IOPD IN OPD.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM DAP-FIO-OPD.
           MOVE FIO-STAT TO PGM-STAT-OPD.
       FIN-BUS-OPD-CON-RCC.
           EXIT.

       BUS-RCD SECTION.                                                 
       INI-BUS-RCD.                                                     
           MOVE SPACES TO RCD.
           IF RCC-NUM-DIRE IN RCC > ZEROES                              
               MOVE RCC-CAI-ICLI IN RCC TO RCD-CAI-ICLI IN RCD          
               MOVE RCC-IIC-ICLI IN RCC TO RCD-IIC-ICLI IN RCD          
               MOVE RCC-NUM-DIRE IN RCC TO RCD-NUM-DIRE IN RCD          
               MOVE WSS-RCDKEYIRCD TO FIO-AKEY                          
               MOVE FIO-GET-KEY TO FIO-CMND                             
               PERFORM SGC-FIO-RCD                                      
               IF FIO-STAT-OKS                                          
                    MOVE WSS-00 TO PGM-STAT-RCD                           
               ELSE                                                     
                    MOVE  SPACES TO RCD-GLS-DIRE IN RCD                 


           ELSE                                                         
              PERFORM BUS-DOM.                                          
           IF RCD-GLS-DIRE IN RCD > SPACES                              
               MOVE WSS-00 TO PGM-STAT-RCD                                
               PERFORM BUS-LUGA                                         
               MOVE RCD-NUM-DIRE IN RCD TO RCC-NUM-DIRE IN CLC-FLD.      

       FIN-BUS-RCD.                                                     
           EXIT.                                                        
                                                                        
       BUS-LUGA SECTION.                                                
       INI-BUS-LUGA.                                                    
           MOVE RCD-COD-LUGA IN RCD TO WSS-COD-LUGA IN WSS-VARI.        
           IF WSS-IND-LUGA IN WSS-VARI = WSS-P                            
               MOVE WSS-PAI TO TAB-COD-TTAB                               
               MOVE WSS-COD-PAIS IN WSS-VARI TO TAB-COD-CTAB IN TAB     
           ELSE                                                         
               MOVE WSS-COM TO TAB-COD-TTAB                               
               MOVE WSS-COD-LUGA IN WSS-VARI TO TAB-COD-CTAB IN TAB.    
           MOVE WSS-TAB TO TAB-COD-SIST.                                  
           PERFORM BUS-TAB.
           MOVE TAB-GLS-DESC TO FRM-GLS-LUGA IN CLC-FLD.                
       FIN-BUS-LUGA.                                                    
           EXIT.      
      *>>>> EOF
*% END
