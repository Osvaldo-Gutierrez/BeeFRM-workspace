*%! Codigo adicional
*% NO_HDR_CST = TRUE
*% PGM_PTC = FALSE
*% GNSTCA = 253 
*%  IF  INI_FIO                                                     
     *<<<<INI_FIO
*%  IF  PGM_PQ
     *<<<<PGM_PQ                                                      
           IF SCR-DESDE-CMN                                             
               PERFORM BUS-OFI-CSSN.                                    
     *>>>>
*% END
     *>>>>>
*% END
*% IF FIN_SCR_VARI
      *<<<< FIN_SCR_VARI                                                
           03 SCR-COD-OFIC                        PIC X(03).            
      *>>>>                                                             
*% END 
*% IF FIN_PGM_STAT
   *<<<<FIN_PGM_STAT
           03 PGM-STAT.                                                 
              05 PGM-STAT-IGE.                                          
                 88 PGM-STAT-IGE-NUL    VALUE '  '.                     
                 07 PGM-STT1-IGE        VALUE '2' PIC X(01).            
                    88 PGM-STAT-IGE-OKS VALUE '0'.                      
                 07 PGM-STT2-IGE        VALUE '3' PIC X(01).            
   *>>>>
*% END
*% IF WSS
      *<<<< WSS
       COPY GNSWVNUM.                                                   
       COPY GNSWCNUM.                                                   
       COPY GNSWGFEC.                                                   
       COPY GNSWGHOY.                                                   
       COPY GNSWCVRF.                                                   
       COPY GNSWVVRF.                                                   
       COPY GNSWGPES.                                                   
       COPY TABBROFI.                                                   
       COPY TABBRCOM.                                                   
       COPY TABBRCAM.                                                   
       COPY TABBRUSR.                                                   
       COPY SGCBRDBC.                                                   
       COPY GNSWGCPT.                                                   
       01 WSS-VARI.                                                     
           03 WSS-NUM-ICL9.                                             
              05 FILLER           VALUE '0'   PIC X(01).                
              05 WSS-NUM-ICL8                 PIC 9(08).                
           03 WSS-COD-OFIC.                                             
              05 FILLER           VALUE '0'   PIC X(01).                
              05 WSS-COD-OFI3                 PIC 9(03).                
           03 WSS-COD-VCAM.                                             
              05 FILLER           VALUE '0'   PIC X(01).                
              05 WSS-COD-VCA3                 PIC 9(03).                
           03 WSS-COD-LOCA.                                             
              05 WSS-COD-REGI                 PIC X(02).                
              05 WSS-COD-LUGA                 PIC X(04).                
           03 WSS-VAL-IPES                    PIC 9(14).                
           03 WSS-VAL-PUDC                    PIC 9(14).                
           03 PGM-STAT.                                                 
              05 PGM-STAT-DBC.                                          
                 88 PGM-STAT-DBC-NUL    VALUE '  '.                     
                 07 PGM-STT1-DBC        VALUE '2' PIC X(01).            
                    88 PGM-STAT-DBC-OKS VALUE '0'.                      
                 07 PGM-STT2-DBC        VALUE '3' PIC X(01).            
      *>>>>                                                             
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC                                                     
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OFI-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-USR-REQA==.        
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-CAM-REQA==.        
      *>>>>                                                             
*% END
*% IF ( INI_FIO ) AND ( NOT PGM_VSI )    
*% IF PGM_PU
      *<<<< ( INI_FIO ) AND ( NOT PGM_VSI )  AND PGM_PU  
           PERFORM GNS-HDL-VSM.                                         
      *>>>>                                                             
*% END
*% END
*% IF FIN_HBL_FLD
      *<<<< FIN_HBL_FLD                                                 
           MOVE SCR-COD-OFIC             TO WSS-COD-OFI3 IN WSS-VARI.   
           MOVE WSS-COD-OFIC IN WSS-VARI TO IGE-COD-OFIC IN IGE-FLD.    
      *>>>>                                                             
*% END 
*% IF ( EOF ) AND ( NOT PGM_VSI)
      *<<<< ( EOF ) AND ( NOT PGM_VSI )                                 
       COPY GNSBGVSM.                                                   
       COPY GNSBHVSM.                                                   
      *>>>>                                                             
*% END
*% IF EOF
      *<<<< EOF                                                   
       COPY GNSBPVRF.                                                   
       COPY GNSBEGLS.                                                   
       COPY GNSBGFEC.                                                   
       COPY GNSBGHOY.                                                   
       COPY GNSBVNUM.                                                   
       COPY SGCBFDBC.                                                   
       COPY GNSBPFEC.                                                   
       COPY GNSBGCPT.                                                   
       COPY TABBFCAM.                                                   
       COPY TABBFOFI.                                                   
       COPY TABBFUSR.                                                   
       COPY TABBVUSR.                                                   
       COPY TABBVOFI.                                                   
       COPY TABBBOFI.                                                   
       COPY GNSBBTAB.                                                   
       COPY GNSBGCAM.                                                   
       COPY GNSBGPES.                                                   
       COPY GNSBVCOD.                                                   
       COPY GNSBVTAB.                                                   
       COPY GNSBEGLS.                                                   
      *>>>> EOF                                                         
*% END
*% IF EOF_PQ
      *<<<< EOF_PQ                                                      
       BUS-OFI-CSSN SECTION.                                            
       INI-BUS-OFI-CSSN.                                                
           IF SCR-USER NOT > SPACES                                     
               MOVE 'RJUNCO' TO SCR-USER.                               
           MOVE SCR-USER TO USR-COD-USER IN USR.                        
           PERFORM VAL-USR.                                             
           IF MSG-COD-MENS > SPACES                                     
               MOVE 'USRCSSNNEX' TO MSG-COD-MENS                        
               PERFORM GOTO-CMN                                         
           ELSE                                                         
               MOVE USR-COD-OFUS IN USR TO OFI-COD-OFIC IN OFI          
               PERFORM BUS-OFI                                          
               IF MSG-COD-MENS > SPACES                                 
                   MOVE 'OFICSSNNEX' TO MSG-COD-MENS                    
                   PERFORM GOTO-CMN                                     
               ELSE                                                     
                   MOVE USR-COD-OFUS IN USR TO SCR-COD-OFIC.            
       FIN-BUS-OFI-CSSN.                                                
           EXIT.                                                        
                                                                        
       GOTO-CMN SECTION.                                                
       INI-GOTO-CMN.                                                    
           MOVE 'DAP' TO MSG-COD-SIST.                                  
           PERFORM GET-MSG.                                             
           MOVE MSG-GLS-DESC TO FRM-MENS.                               
           PERFORM SCR-MSSG.                                            
           MOVE SCR-INST-HCM TO SCR-INST.                               
           PERFORM SCR-COM.                                             
       FIN-GOTO-CMN.                                                    
           EXIT.                                                        
      *>>>>                                                             
*% END 
