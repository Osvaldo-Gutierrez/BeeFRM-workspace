*%! Codigo adicional para programa CL1
*% IF SETTAG
*% VSNPQ = "09.07.26"
*% FNCPQ = "090205"
*% VSNPU = "09.07.26"
*% FNCPU = "090205"
*% END
*% NO_HDR_CST = TRUE
*% PGM_PTC = TRUE
*%
*%! IF PGM_PQ
*%!  FRM = "CL1"
*%!  PADRE = "LAD"
*%!  KEY_PADRE = "LAD-KEY-FLAD"
*%!  LARGO_KEY_PADRE = 20
*%!  FIO_PADRE = "DAP-FIO-LAD"
*%!  SIST_PADRE = "DAP"
*%!  KEY_BROWSE_PADRE = "LAD-KEY-FLAD"
*%!
*%!  ACEPTE_PF7 = TRUE
*%!  ACEPTE_PF8 = TRUE
*%! INCLUDE 'GNSSRC:PFSCONENT.SRC'
*%!
*%!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
*%! END
*% IF CMT_ID
      *-----------------------------------------------------------------
      * MANTENCION
      * OBJETIVO   : NAVEGABILIDAD DE PANTALLAS ADMINISTRATIVAS
      * RESPONSABLE: RRM
      * ETIQUETA   : *RRM-INI 10-ABR-2018 CORREC NAVEGABILIDAD - 53805
      *-----------------------------------------------------------------
*% END
*% IF WSS
      *<<<< WSS
       77  WSS-LEE-DBC-DESDE-LAD  VALUE 'N'   PIC X(01).
       77  WSS-CIC-ICLI    VALUE SPACES PIC X(12).
       77  WSS-YA-LEI-REG-2 VALUE 'N' PIC X(01).
       77  WSS-COMPARA      VALUE 'N' PIC X(01).
      *          03 FRM-CPIM.
      * AEY = Protegido, normal
      *             05 FRM-CPIM-AEY         PIC X(01) VALUE '1'.
      * UBY = Desprotegido, brillante
      *             05 FRM-CPIM-UBY         PIC X(01) VALUE 'I'.
      * ANY = Protegido, non-display
      *             05 FRM-CPIM-ANY         PIC X(01) VALUE '_'.
      * UNY = Desprotegido, non-display
      *             05 FRM-CPIM-UNY         PIC X(01) VALUE '('.
      * ANY = Protegido, normal
      *             05 FRM-CPSF-AEY         PIC X(01) VALUE '-'.
      * ABY = Protegido, brillante
      *             05 FRM-CPIM-ABY         PIC X(01) VALUE '9'.
      * UST = Virtual para BMS, pues es campo STOPPER en UNISYS
      *             05 FRM-CPIM-UST         PIC X(01) VALUE SPACES.
       COPY DAPBRVEN.
       COPY DAPBRVEN REPLACING VEN BY VEN-CPR.
       COPY DAPBRDCO.
       COPY DAPBRDCO REPLACING DCO BY DCO-CPR.
       COPY DAPBRRCC.
       COPY SGCBRDBC.
       COPY GNSWGCPT.
       COPY GNSWGSTR.
       77  WSS-COD-ENTI         VALUE 'DCO'     PIC X(03).
       01  BKP-LAD              VALUE SPACES    PIC X(1280).
       01  WSS-FEC-FAMD.
           03 WSS-NUM-SAMD                      PIC X(02).
           03 WSS-NUM-AAMD                      PIC X(02).
           03 WSS-NUM-MAMD                      PIC X(02).
           03 WSS-NUM-DAMD                      PIC X(02).
       01 CL1-VARI.
          03 WSS-EXI-CLI    VALUE ZEROES    PIC 9(01).
          03 WSS-UNO        VALUE 1         PIC 9(01).
          03 WSS-CON        VALUE 'CON'     PIC X(03).
          03 WSS-FEC        VALUE 'FEC'     PIC X(03).
          03 WSS-OPE        VALUE 'OPE'     PIC X(03).
          03 WSS-CLI        VALUE 'CLI'     PIC X(03).
          03 WSS-AUT        VALUE 'AUT'     PIC X(03).
          03 WSS-23         VALUE '23'      PIC X(02).
          03 WSS-00         VALUE '00'      PIC X(02).
          03 WSS-S          VALUE 'S'       PIC X(01).
          03 WSS-N          VALUE 'N'       PIC X(01).
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
      *Entidades de SGC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-VEN-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RCC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
      *
       01   VAR-PROGRAMA.
            03 PGM-COD-CREB              VALUE ' '       PIC X(03).
            03 PGM-STAT-RCC.
               88 PGM-STAT-RCC-NUL       VALUE '  '.
               07 PGM-STT1-RCC           VALUE '2'       PIC X(01).
                  88 PGM-STAT-RCC-OKS    VALUE '0'.
               07 PGM-STT2-RCC           VALUE '3'       PIC X(01).
      *>>>>
*% END
*% IF INI_FIO_IBM
      *<<<< INI_FIO
           PERFORM GET-FHOY.
           IF SCR-INST = 'GAB' AND
              ( FRM-FFLD = FRM-FFLD-PF8 OR
                FRM-FFLD = FRM-FFLD-PF7 )
                  MOVE 'GKY' TO SCR-INST.
      *>>>> INI_FIO
*% END
*% IF FND_KEY_CMND
      *<<<< FND_KEY_CMND
           IF FRM-COD-CREB IN CL1-FLD = WSS-OPE OR
              FRM-COD-CREB IN CL1-FLD = WSS-CLI
               MOVE WSS-N TO WSS-LEE-DBC-DESDE-LAD
               PERFORM OBTENER-NOMBRE-DBC.
           PERFORM SET-KEY-LAD.
      
           MOVE FIO-GET-NLS TO FIO-CMND.
      *>>>> FND_KEY_CMND
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY

      *RRM-INI 10-ABR-2018 CORREC NAVEGABILIDAD - 53805
      *    MOVE 'PF81LAD' TO QUE-COLA.
      *    MOVE +1000 TO QUE-LITM.
      *    MOVE QUE-PUT TO QUE-CMND.
      *    MOVE LAD TO QUE-ITEM.
      *    PERFORM GNS-PRO-QUE.


      *    MOVE 'PF81STA' TO QUE-COLA.
      *    MOVE +1000 TO QUE-LITM.
      *    MOVE QUE-PUT TO QUE-CMND.
      *    MOVE FIO-STAT TO QUE-ITEM.
      *    PERFORM GNS-PRO-QUE.

      *LECTURA NO OK
           IF NOT FIO-STAT-OKS AND FRM-COD-CREB IN CL1-FLD = WSS-FEC
               MOVE WSS-23          TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CL1NOEXFECPK' TO MSG-COD-MENS
               MOVE 'DAP'          TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.

           IF NOT FIO-STAT-OKS AND FRM-COD-CREB IN CL1-FLD = WSS-OPE
                  MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                  MOVE WSS-23         TO FIO-STAT
                  MOVE WSS-23         TO PGM-STAT-LAD
                  MOVE 'CL1NONXTOIPE' TO MSG-COD-MENS
                  MOVE 'DAP' TO MSG-COD-SIST
                  PERFORM GET-MSG
                  MOVE MSG-GLS-DESC TO FRM-MENS
                  GO TO FIN-FND-KEY.

           IF NOT FIO-STAT-OKS AND FRM-COD-CREB IN CL1-FLD = WSS-AUT
                  MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                  MOVE WSS-23         TO FIO-STAT
                  MOVE WSS-23         TO PGM-STAT-LAD
                  MOVE 'CL1NONXTOAUT' TO MSG-COD-MENS
                  MOVE 'DAP'          TO MSG-COD-SIST
                  PERFORM GET-MSG
                  MOVE MSG-GLS-DESC TO FRM-MENS
                  GO TO FIN-FND-KEY.

           IF NOT FIO-STAT-OKS AND FRM-COD-CREB IN CL1-FLD = WSS-CLI
                  MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                  MOVE WSS-23         TO FIO-STAT
                  MOVE WSS-23         TO PGM-STAT-LAD
                  MOVE 'CL1NONXTOCLI' TO MSG-COD-MENS
                  MOVE 'DAP'          TO MSG-COD-SIST
                  PERFORM GET-MSG
                  MOVE MSG-GLS-DESC TO FRM-MENS
                  GO TO FIN-FND-KEY.
      *RRM-FIN 10-ABR-2018 CORREC NAVEGABILIDAD - 53805
 
           IF NOT FIO-STAT-OKS
               GO TO FIN-FND-KEY.
      
      *LECTURA OK
           IF FRM-COD-CREB IN CL1-FLD = WSS-FEC
              IF NOT (LAD-COD-ENTF IN LAD = WSS-COD-ENTI AND
                 LAD-NUM-DLAD IN CL1-FLD  = LAD-NUM-DLAD IN LAD AND
                 LAD-NUM-MLAD IN CL1-FLD  = LAD-NUM-MLAD IN LAD AND
                 LAD-NUM-SLAD IN CL1-FLD  = LAD-NUM-SLAD IN LAD AND
                 LAD-NUM-ALAD IN CL1-FLD  = LAD-NUM-ALAD IN LAD)
                 MOVE WSS-23          TO FIO-STAT
                 MOVE WSS-23         TO PGM-STAT-LAD
                 MOVE 'CL1NOEXFECPK' TO MSG-COD-MENS
                 MOVE 'DAP'          TO MSG-COD-SIST
                 PERFORM GET-MSG
                 MOVE MSG-GLS-DESC TO FRM-MENS
                 GO TO FIN-FND-KEY.

           IF FRM-COD-CREB IN CL1-FLD = WSS-OPE
              IF NOT (LAD-COD-ENTO IN LAD = WSS-COD-ENTI AND
                 LAD-CAI-IOPD IN CL1-FLD  = LAD-CAI-IOPD IN LAD AND
                 LAD-IIC-IOPD IN CL1-FLD  = LAD-IIC-IOPD IN LAD)
                  MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                  MOVE WSS-23         TO FIO-STAT
                  MOVE WSS-23         TO PGM-STAT-LAD
                  MOVE 'CL1NONXTOIPE' TO MSG-COD-MENS
                  MOVE 'DAP' TO MSG-COD-SIST
                  PERFORM GET-MSG
                  MOVE MSG-GLS-DESC TO FRM-MENS
                  GO TO FIN-FND-KEY.

           IF FRM-COD-CREB IN CL1-FLD = WSS-AUT
              IF NOT (LAD-COD-ENTA IN LAD = WSS-COD-ENTI AND
                 LAD-COD-AUTO IN CL1-FLD  = LAD-COD-AUTO IN LAD)
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE WSS-23         TO FIO-STAT
                    MOVE WSS-23         TO PGM-STAT-LAD
                    MOVE 'CL1NONXTOAUT' TO MSG-COD-MENS
                    MOVE 'DAP'          TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.

           IF FRM-COD-CREB IN CL1-FLD = WSS-CLI AND
              WSS-EXI-CLI NOT = ZEROES 
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE WSS-23         TO FIO-STAT
                    MOVE WSS-23         TO PGM-STAT-LAD
                    MOVE 'CLI    NEX  ' TO MSG-COD-MENS
                    MOVE 'SGC'          TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.

           IF FRM-COD-CREB IN CL1-FLD = WSS-CLI
              IF NOT (LAD-COD-ENTC IN LAD = WSS-COD-ENTI AND
                      LAD-CIC-ICLI IN LAD = DBC-CIC-ICLI IN DBC )
                     MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                     MOVE WSS-23         TO FIO-STAT
                     MOVE WSS-23         TO PGM-STAT-LAD
                     MOVE 'CL1NONXTOCLI' TO MSG-COD-MENS
                     MOVE 'DAP'          TO MSG-COD-SIST
                     PERFORM GET-MSG
                     MOVE MSG-GLS-DESC TO FRM-MENS
                     GO TO FIN-FND-KEY.

           IF NOT ( FRM-COD-CREB IN CL1-FLD = WSS-OPE OR
                    FRM-COD-CREB IN CL1-FLD = WSS-CLI )
                     MOVE WSS-S TO WSS-LEE-DBC-DESDE-LAD
                     PERFORM OBTENER-NOMBRE-DBC.
      
           MOVE LAD-CIC-ICLI IN LAD TO WSS-CIC-ICLI.
      
           IF SCR-CMND = WSS-CON
              IF FRM-FFLD = FRM-FFLD-PF8
                    PERFORM SET-KEY-LAD
                    PERFORM PRO-PF8
              ELSE
                IF FRM-FFLD = FRM-FFLD-PF7
                    PERFORM SET-KEY-LAD
                    PERFORM PRO-PF7.
      
           IF PGM-STAT-LAD-OKS
           IF FRM-FFLD = FRM-FFLD-PF7 OR
              FRM-FFLD = FRM-FFLD-PF8
               MOVE WSS-S TO WSS-LEE-DBC-DESDE-LAD
               PERFORM OBTENER-NOMBRE-DBC.
      
           IF PGM-STAT-LAD-OKS
               IF LAD-COD-ENTF IN LAD = WSS-COD-ENTI
                   MOVE WSS-00   TO FIO-STAT
      *RRM-INI 10-ABR-2018 CORREC NAVEGABILIDAD - 53805
                   MOVE SPACES              TO DCO
      *RRM-FIN 10-ABR-2018 CORREC NAVEGABILIDAD - 53805
                   MOVE LAD-COD-CONT IN LAD TO DCO
                   MOVE WSS-00   TO PGM-STAT-DCO.
      
      *>>>> FIN_FND_KEY
*% END
*% IF INI_SCR_DMSG
*% IF PGM_PQ
      *<<<< INI_SCR_DMSG
      * Con este codigo logramos que una vez terminada la consulta
      * las claves no se borren de la pantalla
           IF SCR-CMND = WSS-CON
                AND
              SCR-INST = SCR-INST-GKY
                AND
              FRM-FFLD NOT = FRM-FFLD-RET
                  MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR.
      *>>>> INI_SCR_DMSG
*% END
*% END
*% IF FIN_SCR_TRL
      *<<<< FIN_SCR_TRL
      
           IF PGM-STAT-LAD-OKS
               MOVE 'CL1PF7PF8 ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
      
      *>>>> FIN_SCR_TRL
*% END
*% IF PFS_SCR_GKEY_PREV
      *<<<< PFS_SCR_GKEY_PREV
           IF NOT ( FRM-FFLD = FRM-FFLD-PF7 )
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 )
      *>>>> PFS_SCR_GKEY_PREV
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF7 )
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 )
      *>>>> PFS_GET_CON
*% END
*% IF POST_SCR_PUTA
      *<<<< POST_SCR_PUTA
           IF PGM-STAT-LAD-OKS
               IF LAD-COD-INST = 'ANT'
                    PERFORM SET-KEY-LAD
                    MOVE FIO-GET-NXT TO FIO-CMND
                    PERFORM DAP-FIO-LAD
                    IF FIO-STAT-OKS
                         IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI
                            MOVE WSS-23  TO FIO-STAT
                            MOVE FIO-STAT TO PGM-STAT-LAD
                         ELSE
                             MOVE LAD-COD-CONT IN LAD TO DCO-CPR
                             MOVE WSS-S TO WSS-COMPARA
                             GO TO FIN-AT-DES.
      
           IF PGM-STAT-LAD-OKS
               IF LAD-COD-INST = 'DES'
                     PERFORM SET-KEY-LAD
                     MOVE FIO-GET-PRV TO FIO-CMND
                     PERFORM DAP-FIO-LAD
                     IF FIO-STAT-OKS
                          IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI
                               MOVE WSS-23 TO FIO-STAT
                               MOVE FIO-STAT TO PGM-STAT-LAD
                          ELSE
                               MOVE LAD-COD-CONT IN LAD TO DCO-CPR
                               MOVE WSS-S TO WSS-COMPARA.
       FIN-AT-DES.
            
           MOVE CL1-FLD TO FRM-DFLD.

      *            
      *>>>> POST_SCR_PUTA
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBVCOD.
       COPY GNSBVTAB.
       GNX-PRO-QUE SECTION.
       INI-GNX-PRO-QUE.
           PERFORM GNS-PRO-QUE.
       FIN-GNX-PRO-QUE.
           EXIT.
      
       SET-KEY-LAD SECTION.
       INI-SET-KEY-LAD.
      
           MOVE WSS-COD-ENTI TO LAD-COD-ENTF IN LAD
                                LAD-COD-ENTO IN LAD
                                LAD-COD-ENTC IN LAD
                                LAD-COD-ENTA IN LAD
                                LAD-COD-ENTI IN LAD.
      
           IF FRM-COD-CREB IN CL1-FLD = WSS-OPE
               MOVE 'LAD-KEY-OLAD' TO FIO-AKEY
           ELSE
           IF FRM-COD-CREB IN CL1-FLD = WSS-CLI
               MOVE 'LAD-KEY-CLAD' TO FIO-AKEY
           ELSE
           IF FRM-COD-CREB IN CL1-FLD = WSS-AUT
               MOVE 'LAD-KEY-ALAD' TO FIO-AKEY.
       FIN-SET-KEY-LAD.
           EXIT.
      
       OBTENER-NOMBRE-DBC  SECTION.
      *----------------------------
       INI-OBTENER-NOMBRE-DBC.
      
           IF WSS-LEE-DBC-DESDE-LAD = WSS-S
               MOVE LAD-CIC-ICLI IN LAD TO DBC-CIC-ICLI IN DBC.
      

           IF FRM-COD-CREB IN CL1-FLD = WSS-OPE AND
              NOT PGM-STAT-DBC-OKS
                MOVE LAD-CIC-IOPD IN LAD TO RCC-CIC-IOPD IN RCC
                PERFORM GET-CUR-RCC
                IF PGM-STAT-RCC-OKS AND
                   LAD-CIC-IOPD IN LAD  =  RCC-CIC-IOPD IN RCC
                     MOVE RCC-CIC-ICLI IN RCC TO DBC-CIC-ICLI IN DBC
                ELSE
                   MOVE WSS-23      TO FIO-STAT
                   MOVE WSS-23      TO PGM-STAT-DBC
                   GO TO FIN-OBTENER-NOMBRE-DBC.
      
           IF FRM-COD-CREB IN CL1-FLD = WSS-CLI AND
              NOT PGM-STAT-DBC-OKS
                MOVE 'DBC-IDC-ICLI' TO FIO-AKEY.
      
           MOVE ZEROES TO WSS-EXI-CLI.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           MOVE FIO-STAT TO PGM-STAT-DBC.
      
           IF FIO-STAT-OKS
      *  ISP INI 27-04-2009
                MOVE DBC-CIC-ICLI IN DBC TO
                     LAD-CIC-ICLI IN LAD
      *  FIN INI 27-04-2009
                PERFORM CPT-NOMB
           ELSE
                MOVE WSS-UNO TO WSS-EXI-CLI
                MOVE SPACES TO DBC-GLS-NOMC IN DBC
                MOVE SPACES TO DBC-VRF-ICLI IN DBC
                MOVE SPACES TO DBC-IDC-ICLI IN DBC
                MOVE WSS-00 TO PGM-STAT-DBC.
      
       FIN-OBTENER-NOMBRE-DBC.
           EXIT.
      *
       GET-CUR-RCC SECTION.
       INI-GET-CUR-RCC.
           MOVE   'RCC-KEY-IOPD'           TO FIO-AKEY.
           MOVE    FIO-GET-NLS             TO FIO-CMND.
           PERFORM DAP-FIO-RCC.
           IF NOT ( FIO-STAT-OKS     AND
                    LAD-CIC-IOPD IN LAD = RCC-CIC-IOPD IN RCC )
                    MOVE WSS-23 TO FIO-STAT.
           MOVE   FIO-STAT  TO PGM-STAT-RCC.
       FIN-GET-CUR-RCC.
           EXIT.
      
      
       COPY GNSBBTAB.
       COPY DAPBFVEN.
       COPY DAPBFRCC.
       COPY SGCBFDBC.
       COPY GNSBGCPT.
       COPY GNSBGSTR.
       CPT-NOMB SECTION.
       INI-CPT-NOMB.
           MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN.
           PERFORM CPT-BLKS.
           MOVE CPT-STRN            TO DBC-GLS-NOMC IN DBC.
       FIN-CPT-NOMB.
           EXIT.
      
       PRO-PF7 SECTION.
       INI-PRO-PF7.
      *RRM-INI 10-ABR-2018 CORREC NAVEGABILIDAD - 53805
           MOVE SPACES TO BKP-LAD.
      *RRM-FIN 10-ABR-2018 CORREC NAVEGABILIDAD - 53805

           MOVE LAD TO BKP-LAD.

           PERFORM SET-KEY-LAD.
           MOVE FIO-GET-PRV TO FIO-CMND.
           PERFORM DAP-FIO-LAD.
           MOVE FIO-STAT TO PGM-STAT-LAD.
      
           IF NOT FIO-STAT-OKS

                MOVE BKP-LAD TO LAD                                             
                PERFORM SET-KEY-LAD                                             
                MOVE FIO-GET-KEY TO FIO-CMND                                    
                PERFORM DAP-FIO-LAD                                             
                MOVE FIO-STAT TO PGM-STAT-LAD                                   

                PERFORM EDT-FLD-CL1
                MOVE 'CL1NOPRV' TO MSG-COD-MENS
                MOVE 'DAP' TO MSG-COD-SIST
                PERFORM GET-MSG
                MOVE MSG-GLS-DESC TO FRM-MENS
                GO TO FIN-PRO-PF7.
      
           IF  ( FRM-COD-CREB IN CL1-FLD = WSS-OPE
                         AND
                  (
                    LAD-COD-ENTO IN LAD NOT = WSS-COD-ENTI OR
                     LAD-CAI-IOPD IN CL1-FLD NOT =
                     LAD-CAI-IOPD IN LAD
                             OR
                     LAD-IIC-IOPD IN CL1-FLD NOT =
                     LAD-IIC-IOPD IN LAD
                   )
                )
                   PERFORM SET-KEY-LAD
                   MOVE FIO-GET-NXT TO FIO-CMND
                   PERFORM DAP-FIO-LAD
      
                   PERFORM EDT-FLD-CL1
                   MOVE 'CL1NOPRVOPE' TO MSG-COD-MENS
                   MOVE 'DAP' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   GO TO FIN-PRO-PF7.
      
           IF  ( FRM-COD-CREB IN CL1-FLD = WSS-AUT
                         AND
                  (
                    LAD-COD-ENTA IN LAD NOT = WSS-COD-ENTI OR
                     LAD-COD-AUTO IN CL1-FLD NOT =
                     LAD-COD-AUTO IN LAD
                   )
                )
                   PERFORM SET-KEY-LAD
                   MOVE FIO-GET-NXT TO FIO-CMND
                   PERFORM DAP-FIO-LAD
      
                   PERFORM EDT-FLD-CL1
                   MOVE 'CL1NOPRVAUT' TO MSG-COD-MENS
                   MOVE 'DAP' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   GO TO FIN-PRO-PF7.
      
           IF FRM-COD-CREB IN CL1-FLD = WSS-FEC
              IF NOT (LAD-COD-ENTF IN LAD = WSS-COD-ENTI AND
                 LAD-NUM-DLAD IN CL1-FLD  = LAD-NUM-DLAD IN LAD AND
                 LAD-NUM-MLAD IN CL1-FLD  = LAD-NUM-MLAD IN LAD AND
                 LAD-NUM-SLAD IN CL1-FLD  = LAD-NUM-SLAD IN LAD AND
                 LAD-NUM-ALAD IN CL1-FLD  = LAD-NUM-ALAD IN LAD)

      *RRM-INI 10-ABR-2018 CORREC NAVEGABILIDAD - 53805
                   MOVE BKP-LAD TO LAD
                   PERFORM SET-KEY-LAD
                   MOVE FIO-GET-KEY TO FIO-CMND
                   PERFORM DAP-FIO-LAD
                   MOVE FIO-STAT TO PGM-STAT-LAD

      *            PERFORM SET-KEY-LAD
      *            MOVE FIO-GET-NXT TO FIO-CMND
      *            PERFORM DAP-FIO-LAD
      *RRM-FIN 10-ABR-2018 CORREC NAVEGABILIDAD - 53805
      
                   PERFORM EDT-FLD-CL1
                   MOVE 'CL1NOPRVFEC' TO MSG-COD-MENS
                   MOVE 'DAP' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   GO TO FIN-PRO-PF7.
      
           IF  ( FRM-COD-CREB IN CL1-FLD = WSS-CLI
                         AND
                  (
                    LAD-COD-ENTC IN LAD NOT = WSS-COD-ENTI OR
                     LAD-CIC-ICLI IN LAD NOT =
                     WSS-CIC-ICLI
                  )
                )
                     PERFORM SET-KEY-LAD
                     MOVE FIO-GET-NXT TO FIO-CMND
                     PERFORM DAP-FIO-LAD
      
                     PERFORM EDT-FLD-CL1
                     MOVE 'CL1NOPRVCLI' TO MSG-COD-MENS
                     MOVE 'DAP' TO MSG-COD-SIST
                     PERFORM GET-MSG
                     MOVE MSG-GLS-DESC TO FRM-MENS.
       FIN-PRO-PF7.
           EXIT.
      
       PRO-PF8 SECTION.
