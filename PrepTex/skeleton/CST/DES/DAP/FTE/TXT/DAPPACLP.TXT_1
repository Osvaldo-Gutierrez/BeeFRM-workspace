*%! Codigo adicional para programa CLP
*% IF SETTAG
*% VSNPQ = "18.02.21"
*% FNCPQ = "180221"
*% VSNPU = "18.02.21"
*% FNCPU = "180221"
*% END
*% NO_HDR_CST = TRUE
*% PGM_PTC = TRUE
*%
*%! IF PGM_PQ
*%!  FRM = "CLP"
*%!  PADRE = "LAD"
*%!  KEY_PADRE = "LAD-KEY-FLAD"
*%!  LARGO_KEY_PADRE = 20
*%!  FIO_PADRE = "DAP-FIO-LAD"
*%!  SIST_PADRE = "DAP"
*%!  KEY_BROWSE_PADRE = "LAD-KEY-FLAD"
*%!
*%!  ACEPTE_PF7 = TRUE
*%!  ACEPTE_PF8 = TRUE
*%! INCLUDE 'GNSSRC:PFSCONENT.SRC'
*%!
*%!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
*%! END
*% IF WSS
      *<<<< WSS
       77  WSS-LEE-DBC-DESDE-LAD  VALUE 'N'   PIC X(01).
       77  WSS-CIC-ICLI    VALUE SPACES PIC X(12).
       77  WSS-YA-LEI-REG-2 VALUE 'N' PIC X(01).
       77  WSS-COMPARA      VALUE 'N' PIC X(01).
      *          03 FRM-CPIM.
      * AEY = Protegido, normal
      *             05 FRM-CPIM-AEY         PIC X(01) VALUE '1'.
      * UBY = Desprotegido, brillante
      *             05 FRM-CPIM-UBY         PIC X(01) VALUE 'I'.
      * ANY = Protegido, non-display
      *             05 FRM-CPIM-ANY         PIC X(01) VALUE '_'.
      * UNY = Desprotegido, non-display
      *             05 FRM-CPIM-UNY         PIC X(01) VALUE '('.
      * ANY = Protegido, normal
      *             05 FRM-CPSF-AEY         PIC X(01) VALUE '-'.
      * ABY = Protegido, brillante
      *             05 FRM-CPIM-ABY         PIC X(01) VALUE '9'.
      * UST = Virtual para BMS, pues es campo STOPPER en UNISYS
      *             05 FRM-CPIM-UST         PIC X(01) VALUE SPACES.
       COPY DAPBRLIC.
       COPY DAPBRLIC REPLACING LIC BY LIC-CPR.

       COPY DAPBRRCC.
       COPY DAPBROPD.

       COPY SGCBRDBC.
       COPY GNSWGCPT.
       COPY GNSWGSTR.
       77  WSS-COD-ENTI         VALUE 'LIC'     PIC X(03).
       01  BKP-LAD              VALUE SPACES    PIC X(1280).
       01  WSS-FEC-FAMD.
           03 WSS-NUM-SAMD                      PIC X(02).
           03 WSS-NUM-AAMD                      PIC X(02).
           03 WSS-NUM-MAMD                      PIC X(02).
           03 WSS-NUM-DAMD                      PIC X(02).
      *CORRECION NAVEGABILIDAD
       01 CLP-VARI.
          03 WSS-EXI-CLI    VALUE ZEROES    PIC 9(01).
          03 WSS-UNO        VALUE 1         PIC 9(01).
          03 WSS-FEC        VALUE 'FEC'     PIC X(03).
          03 WSS-OPE        VALUE 'OPE'     PIC X(03).
          03 WSS-CLI        VALUE 'CLI'     PIC X(03).
          03 WSS-AUT        VALUE 'AUT'     PIC X(03).
          03 WSS-23         VALUE '23'      PIC X(02).
          03 WSS-00         VALUE '00'      PIC X(02).
          03 WSS-CON        VALUE 'CON'     PIC X(03).
          03 WSS-ANT        VALUE 'ANT'     PIC X(03).
          03 WSS-DES        VALUE 'DES'     PIC X(03).
          03 WSS-DAP        VALUE 'DAP'     PIC X(03).
          03 WSS-GNS        VALUE 'GNS'     PIC X(03).
          03 WSS-SGC        VALUE 'SGC'     PIC X(03).
          03 WSS-GAB        VALUE 'GAB'     PIC X(03).
          03 WSS-GKY        VALUE 'GKY'     PIC X(03).
          03 WSS-N          VALUE 'N'       PIC X(01).
          03 WSS-S          VALUE 'S'       PIC X(01).
          03 WSS-CLINEX     VALUE 'CLI    NEX  '         PIC X(12).
          03 WSS-LADKEYOLAD VALUE 'LAD-KEY-OLAD'         PIC X(12).
          03 WSS-LADKEYCLAD VALUE 'LAD-KEY-CLAD'         PIC X(12).
          03 WSS-LADKEYALAD VALUE 'LAD-KEY-ALAD'         PIC X(12).
          03 WSS-DBCIDCICLI VALUE 'DBC-IDC-ICLI'         PIC X(12).
          03 WSS-RCCKEYIOPD VALUE 'RCC-KEY-IOPD'         PIC X(12).
          03 WSS-CLNOINGRFEC VALUE 'CL NOINGRFEC'        PIC X(12).
          03 WSS-CLNOINGROPE VALUE 'CL NOINGROPE'        PIC X(12).
          03 WSS-CLNOINGRCLI VALUE 'CL NOINGRCLI'        PIC X(12).
          03 WSS-CLNOINGRAUT VALUE 'CL NOINGRAUT'        PIC X(12).
          03 WSS-CLPNOEXFECPK VALUE 'CLPNOEXFECPK'       PIC X(12).
          03 WSS-CLPNONXTOIPE VALUE 'CLPNONXTOIPE'       PIC X(12).
          03 WSS-CLPNONXTOAUT VALUE 'CLPNONXTOAUT'       PIC X(12).
          03 WSS-CLPNONXTOCLI VALUE 'CLPNONXTOCLI'       PIC X(12).
          03 WSS-CLPPF7PF8    VALUE 'CLPPF7PF8 '         PIC X(10).
          03 WSS-CLPNOPRV     VALUE 'CLPNOPRV'           PIC X(08).
          03 WSS-CLPNOPRVOPE  VALUE 'CLPNOPRVOPE'        PIC X(11).
          03 WSS-CLPNOPRVAUT  VALUE 'CLPNOPRVAUT'        PIC X(11).
          03 WSS-CLPNOPRVFEC  VALUE 'CLPNOPRVFEC'        PIC X(11).
          03 WSS-CLPNOPRVCLI  VALUE 'CLPNOPRVCLI'        PIC X(11).
          03 WSS-CLPNONXT     VALUE 'CLPNONXT'           PIC X(08).
          03 WSS-CLPNONXTOPE  VALUE 'CLPNONXTOPE'        PIC X(11).
          03 WSS-CLPNONXTAUT  VALUE 'CLPNONXTAUT'        PIC X(11).
          03 WSS-CLPNONXTFEC  VALUE 'CLPNONXTFEC'        PIC X(11).
          03 WSS-CLPNONXTCLI  VALUE 'CLPNONXTCLI'        PIC X(11).
          03 WSS-TX01         VALUE 'Flag'               PIC X(20).
          03 WSS-TX02         VALUE 'Serial'             PIC X(20).
          03 WSS-TX03         VALUE 'Fecha Liquidacion'  PIC X(20).
          03 WSS-TX04         VALUE 'Estado'             PIC X(20).
          03 WSS-TX05         VALUE 'Oficina'            PIC X(20).
          03 WSS-TX06         VALUE 'Moneda'             PIC X(20).
          03 WSS-TX07         VALUE 'Valor a Pagar'      PIC X(20).
          03 WSS-TX08         VALUE 'Valor a Reversar'   PIC X(20).
          03 WSS-TX09         VALUE 'Disponible'         PIC X(20).

          
      *FIN CORRECION NAVEGABILIDAD          
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
      *Entidades de SGC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RCC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OPD-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-LIC-REQA==.
      *
       01   VAR-PROGRAMA.
            03 PGM-COD-CREB              VALUE ' '       PIC X(03).
            03 PGM-STAT-RCC.
               88 PGM-STAT-RCC-NUL       VALUE '  '.
               07 PGM-STT1-RCC           VALUE '2'       PIC X(01).
                  88 PGM-STAT-RCC-OKS    VALUE '0'.
               07 PGM-STT2-RCC           VALUE '3'       PIC X(01).

      *>>>>
*% END
*% IF INI_FIO_IBM
      *<<<< INI_FIO
           PERFORM GET-FHOY.
           IF SCR-INST = WSS-GAB AND
              ( FRM-FFLD = FRM-FFLD-PF8 OR
                FRM-FFLD = FRM-FFLD-PF7 )
                  MOVE WSS-GKY TO SCR-INST.
      *>>>> INI_FIO
*% END
*% IF FND_KEY_CMND
      *<<<< FND_KEY_CMND
           IF FRM-COD-CREB IN CLP-FLD = WSS-OPE OR
              FRM-COD-CREB IN CLP-FLD = WSS-CLI
               MOVE WSS-N TO WSS-LEE-DBC-DESDE-LAD
               PERFORM OBTENER-NOMBRE-DBC.
           PERFORM SET-KEY-LAD.
      
           MOVE FIO-GET-NLS TO FIO-CMND.
      *>>>> FND_KEY_CMND
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
      *CORRECION NAVEGABILIDAD    
      *LECTURA NO OK
           IF NOT FIO-STAT-OKS AND FRM-COD-CREB IN CLP-FLD = WSS-FEC
               MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
               MOVE WSS-23         TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CLPNOEXFECPK' TO MSG-COD-MENS
               MOVE 'DAP'          TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.

           IF NOT FIO-STAT-OKS AND FRM-COD-CREB IN CLP-FLD = WSS-OPE
               MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
               MOVE WSS-23         TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CLPNONXTOIPE' TO MSG-COD-MENS
               MOVE 'DAP' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.      

           IF NOT FIO-STAT-OKS AND  FRM-COD-CREB IN CLP-FLD = WSS-AUT
               MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
               MOVE WSS-23         TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CLPNONXTOAUT' TO MSG-COD-MENS
               MOVE 'DAP'          TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.

           IF NOT FIO-STAT-OKS AND  FRM-COD-CREB IN CLP-FLD = WSS-CLI
               MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
               MOVE WSS-23         TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CLPNONXTOCLI' TO MSG-COD-MENS
               MOVE 'DAP'          TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.               
               
      *FIN CORRECION NAVEGABILIDAD
      
           IF NOT FIO-STAT-OKS
               GO TO FIN-FND-KEY.
      
      
      

           IF FRM-COD-CREB IN CLP-FLD = WSS-FEC
      *CORRECION NAVEGABILIDAD     
               IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI OR
                  LAD-NUM-DLAD IN CLP-FLD NOT =
                  LAD-NUM-DLAD IN LAD    OR
                  LAD-NUM-MLAD IN CLP-FLD NOT =
                  LAD-NUM-MLAD IN LAD    OR
                  LAD-NUM-SLAD IN CLP-FLD NOT =
                  LAD-NUM-SLAD IN LAD    OR
                  LAD-NUM-ALAD IN CLP-FLD NOT =
                  LAD-NUM-ALAD IN LAD
                    MOVE WSS-23 TO FIO-STAT
                    MOVE WSS-23 TO PGM-STAT-LAD
                    MOVE WSS-CLPNOEXFECPK TO MSG-COD-MENS
                    MOVE WSS-DAP TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
           
           
           IF FRM-COD-CREB IN CLP-FLD = WSS-OPE
               IF LAD-COD-ENTO IN LAD NOT = WSS-COD-ENTI OR
                  LAD-CAI-IOPD IN CLP-FLD NOT =
                  LAD-CAI-IOPD IN LAD    OR
                  LAD-IIC-IOPD IN CLP-FLD NOT =
                  LAD-IIC-IOPD IN LAD
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE WSS-23 TO FIO-STAT
                    MOVE WSS-23 TO PGM-STAT-LAD
                    MOVE WSS-CLPNONXTOIPE TO MSG-COD-MENS
                    MOVE WSS-DAP TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
      
           IF FRM-COD-CREB IN CLP-FLD = WSS-AUT
               IF LAD-COD-ENTA IN LAD NOT = WSS-COD-ENTI OR
                  LAD-COD-AUTO IN CLP-FLD NOT =
                  LAD-COD-AUTO IN LAD
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE WSS-23 TO FIO-STAT
                    MOVE WSS-23 TO PGM-STAT-LAD
                    MOVE WSS-CLPNONXTOAUT TO MSG-COD-MENS
                    MOVE WSS-DAP TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
      
           IF FRM-COD-CREB IN CLP-FLD = WSS-CLI AND
              WSS-EXI-CLI NOT = ZEROES
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE WSS-23         TO FIO-STAT
                    MOVE WSS-23         TO PGM-STAT-LAD
                    MOVE WSS-CLINEX TO MSG-COD-MENS
                    MOVE WSS-SGC        TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
      
           IF FRM-COD-CREB IN CLP-FLD = WSS-CLI
               IF LAD-COD-ENTC IN LAD NOT = WSS-COD-ENTI OR
                  LAD-CIC-ICLI IN LAD     NOT =
                  DBC-CIC-ICLI IN DBC
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE WSS-23 TO FIO-STAT
                    MOVE WSS-23 TO PGM-STAT-LAD
                    MOVE WSS-CLPNONXTOCLI TO MSG-COD-MENS
                    MOVE WSS-DAP TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
                    
           IF NOT ( FRM-COD-CREB IN CLP-FLD = WSS-OPE OR
                    FRM-COD-CREB IN CLP-FLD = WSS-CLI )
                     MOVE WSS-S TO WSS-LEE-DBC-DESDE-LAD
                     PERFORM OBTENER-NOMBRE-DBC.
      
           MOVE LAD-CIC-ICLI IN LAD TO WSS-CIC-ICLI.                    
      *FIN CORRECION NAVEGABILIDAD
           IF SCR-CMND = WSS-CON
              IF FRM-FFLD = FRM-FFLD-PF8
                    PERFORM SET-KEY-LAD
                    PERFORM PRO-PF8
              ELSE
                IF FRM-FFLD = FRM-FFLD-PF7
                    PERFORM SET-KEY-LAD
                    PERFORM PRO-PF7.
      
           IF PGM-STAT-LAD-OKS
           IF FRM-FFLD = FRM-FFLD-PF7 OR
              FRM-FFLD = FRM-FFLD-PF8
               MOVE WSS-S TO WSS-LEE-DBC-DESDE-LAD
               PERFORM OBTENER-NOMBRE-DBC.
      
           IF PGM-STAT-LAD-OKS
               IF LAD-COD-ENTF IN LAD = WSS-COD-ENTI
                   MOVE WSS-00   TO FIO-STAT

                   MOVE SPACES TO LIC

                   MOVE LAD-COD-CONT IN LAD TO LIC
                   MOVE WSS-00   TO PGM-STAT-LIC.
      
      *>>>> FIN_FND_KEY
*% END
*% IF INI_SCR_DMSG
*% IF PGM_PQ
      *<<<< INI_SCR_DMSG
      * Con este codigo logramos que una vez terminada la consulta
      * las claves no se borren de la pantalla
           IF SCR-CMND = WSS-CON
                AND
              SCR-INST = SCR-INST-GKY
                AND
              FRM-FFLD NOT = FRM-FFLD-RET
                  MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR.
      *>>>> INI_SCR_DMSG
*% END
*% END
*% IF FIN_SCR_TRL
      *<<<< FIN_SCR_TRL
      
           IF PGM-STAT-LAD-OKS
               MOVE WSS-CLPPF7PF8 TO MSG-COD-MENS
               MOVE WSS-GNS TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
      
      *>>>> FIN_SCR_TRL
*% END
*% IF PFS_SCR_GKEY_PREV
      *<<<< PFS_SCR_GKEY_PREV
           IF NOT ( FRM-FFLD = FRM-FFLD-PF7 )
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 )
      *>>>> PFS_SCR_GKEY_PREV
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF7 )
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 )
      *>>>> PFS_GET_CON
*% END
*% IF POST_SCR_PUTA
      *<<<< POST_SCR_PUTA
           IF PGM-STAT-LAD-OKS
               IF LAD-COD-INST = WSS-ANT
               
                    MOVE SPACES TO LAD
                    PERFORM SET-KEY-LAD
                    MOVE FIO-GET-NXT TO FIO-CMND
                    PERFORM DAP-FIO-LAD
                    IF FIO-STAT-OKS
                         IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI
                            MOVE WSS-23  TO FIO-STAT
                            MOVE FIO-STAT TO PGM-STAT-LAD
                         ELSE

                             MOVE SPACES TO LIC-CPR
                             MOVE LAD-COD-CONT IN LAD TO LIC-CPR
                             MOVE WSS-S TO WSS-COMPARA
                             GO TO FIN-AT-DES.
      
           IF PGM-STAT-LAD-OKS
               IF LAD-COD-INST = WSS-DES

                     MOVE SPACES TO LAD
                     PERFORM SET-KEY-LAD
                     MOVE FIO-GET-PRV TO FIO-CMND
                     PERFORM DAP-FIO-LAD
                     IF FIO-STAT-OKS
                          IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI
                               MOVE WSS-23 TO FIO-STAT
                               MOVE FIO-STAT TO PGM-STAT-LAD
                          ELSE
                               MOVE SPACES TO LIC-CPR
                               MOVE LAD-COD-CONT IN LAD TO LIC-CPR
                               MOVE WSS-S TO WSS-COMPARA.
       FIN-AT-DES.
      
      *     ==>>  01
      *      
           MOVE WSS-TX01            TO FRM-GLS-TX01     IN CLP-FLD.
           MOVE FRM-CPIM-AEY        TO FRM-GLS-TX01-ATR IN CLP-FLD.
      
           MOVE FRM-CPIM-AEY        TO LIC-GLS-FLAG-ATR IN CLP-FLD.
      
           IF WSS-COMPARA = WSS-S
               IF LIC-GLS-FLAG IN LIC NOT =
                  LIC-GLS-FLAG IN LIC-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX01-ATR IN CLP-FLD
                   MOVE FRM-CPIM-ABY TO LIC-GLS-FLAG-ATR IN CLP-FLD.

      *     ==>>  02
      *      
           MOVE WSS-TX02          TO FRM-GLS-TX02     IN CLP-FLD.
           MOVE FRM-CPIM-AEY      TO FRM-GLS-TX02-ATR IN CLP-FLD.
      
           MOVE FRM-CPIM-AEY      TO LIC-CAI-ISER-ATR IN CLP-FLD.
           MOVE FRM-CPIM-AEY      TO LIC-IIC-ISER-ATR IN CLP-FLD.
      
           IF WSS-COMPARA = WSS-S
               IF (LIC-CAI-ISER IN LIC NOT =
                  LIC-CAI-ISER IN LIC-CPR) 
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX02-ATR IN CLP-FLD
                   MOVE FRM-CPIM-ABY TO LIC-CAI-ISER-ATR IN CLP-FLD
                   MOVE FRM-CPIM-ABY TO LIC-IIC-ISER-ATR IN CLP-FLD.
           IF WSS-COMPARA = WSS-S
               IF (LIC-IIC-ISER IN LIC NOT =
                  LIC-IIC-ISER IN LIC-CPR)
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX02-ATR IN CLP-FLD
                   MOVE FRM-CPIM-ABY TO LIC-CAI-ISER-ATR IN CLP-FLD
                   MOVE FRM-CPIM-ABY TO LIC-IIC-ISER-ATR IN CLP-FLD.
                   
      *     ==>>  03
      *      
           MOVE WSS-TX03        TO FRM-GLS-TX03 IN CLP-FLD.
           MOVE FRM-CPIM-AEY    TO FRM-GLS-TX03-ATR IN CLP-FLD.
      
           MOVE FRM-CPIM-AEY    TO LIC-FEC-FLIQ-ATR IN CLP-FLD.
           IF WSS-COMPARA = WSS-S
               IF LIC-FEC-FLIQ IN LIC NOT =
                  LIC-FEC-FLIQ IN LIC-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX03-ATR IN CLP-FLD
                   MOVE FRM-CPIM-ABY TO LIC-FEC-FLIQ-ATR IN CLP-FLD.

      *     ==>>  04
      *      
           MOVE WSS-TX04        TO FRM-GLS-TX04 IN CLP-FLD.
           MOVE FRM-CPIM-AEY    TO FRM-GLS-TX04-ATR IN CLP-FLD.
           MOVE FRM-CPIM-AEY    TO LIC-COD-STAT-ATR IN CLP-FLD.
           IF WSS-COMPARA = WSS-S
               IF LIC-COD-STAT IN LIC NOT =
                  LIC-COD-STAT IN LIC-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX04-ATR IN CLP-FLD
                   MOVE FRM-CPIM-ABY TO LIC-COD-STAT-ATR IN CLP-FLD.

      *     ==>>  05
      *      
           MOVE WSS-TX05          TO FRM-GLS-TX05     IN CLP-FLD.
           MOVE FRM-CPIM-AEY      TO FRM-GLS-TX05-ATR IN CLP-FLD.
           MOVE FRM-CPIM-AEY      TO LIC-COD-OFIC-ATR IN CLP-FLD.
           IF WSS-COMPARA = WSS-S
               IF LIC-COD-OFIC IN LIC NOT =
                  LIC-COD-OFIC IN LIC-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX05-ATR IN CLP-FLD
                   MOVE FRM-CPIM-ABY TO LIC-COD-OFIC-ATR IN CLP-FLD.

      *     ==>>  06
      *      
           MOVE WSS-TX06         TO FRM-GLS-TX06     IN CLP-FLD.
           MOVE FRM-CPIM-AEY     TO FRM-GLS-TX06-ATR IN CLP-FLD.
           MOVE FRM-CPIM-AEY     TO LIC-COD-VCAM-ATR IN CLP-FLD.
             IF WSS-COMPARA = WSS-S
               IF LIC-COD-VCAM IN LIC NOT =
                  LIC-COD-VCAM IN LIC-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX06-ATR IN CLP-FLD
                   MOVE FRM-CPIM-ABY TO LIC-COD-VCAM-ATR IN CLP-FLD.

      *     ==>>  07
      *      
           MOVE WSS-TX07         TO FRM-GLS-TX07     IN CLP-FLD.
           MOVE FRM-CPIM-AEY     TO FRM-GLS-TX07-ATR IN CLP-FLD.
           MOVE FRM-CPIM-AEY     TO LIC-SGV-TPAG-ATR IN CLP-FLD.
             IF WSS-COMPARA = WSS-S
               IF LIC-SGV-TPAG IN LIC NOT =
                  LIC-SGV-TPAG IN LIC-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX07-ATR IN CLP-FLD
                   MOVE FRM-CPIM-ABY TO LIC-SGV-TPAG-ATR IN CLP-FLD.

      *     ==>>  08
      *      
           MOVE WSS-TX08         TO FRM-GLS-TX08     IN CLP-FLD.
           MOVE FRM-CPIM-AEY     TO FRM-GLS-TX08-ATR IN CLP-FLD.
           MOVE FRM-CPIM-AEY     TO LIC-IND-REVE-ATR IN CLP-FLD.
           MOVE FRM-CPIM-AEY     TO LIC-SGV-REVE-ATR IN CLP-FLD.
             IF WSS-COMPARA = WSS-S
               IF (LIC-IND-REVE IN LIC NOT =
                  LIC-IND-REVE IN LIC-CPR) 
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX08-ATR IN CLP-FLD
                   MOVE FRM-CPIM-ABY TO LIC-SGV-REVE-ATR IN CLP-FLD
                   MOVE FRM-CPIM-ABY TO LIC-IND-REVE-ATR IN CLP-FLD.
             IF WSS-COMPARA = WSS-S
               IF (LIC-SGV-REVE IN LIC NOT =
                  LIC-SGV-REVE IN LIC-CPR) 
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX08-ATR IN CLP-FLD
                   MOVE FRM-CPIM-ABY TO LIC-SGV-REVE-ATR IN CLP-FLD
                   MOVE FRM-CPIM-ABY TO LIC-IND-REVE-ATR IN CLP-FLD.
                
      *     ==>>  09
      *      
           MOVE WSS-TX09         TO FRM-GLS-TX09     IN CLP-FLD.
           MOVE FRM-CPIM-AEY     TO FRM-GLS-TX09-ATR IN CLP-FLD.
           MOVE FRM-CPIM-AEY     TO LIC-GLS-DISP-ATR IN CLP-FLD.
             IF WSS-COMPARA = WSS-S
               IF LIC-GLS-DISP IN LIC NOT =
                  LIC-GLS-DISP IN LIC-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX09-ATR IN CLP-FLD
                   MOVE FRM-CPIM-ABY TO LIC-GLS-DISP-ATR IN CLP-FLD.


                   
           MOVE CLP-FLD TO FRM-DFLD.
      
      
