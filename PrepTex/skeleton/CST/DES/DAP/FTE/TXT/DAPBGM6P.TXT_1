      * PROCESA LINEA {LINEA}
           IF M6S-NUM-LINE = {LINEA}
               IF M6S-COD-VCAM = WSS-COD-DOLL
                    MULTILY M6S-VAL-MONT BY
                            WSS-SGV-DOLL GIVING
                            M6S-VAL-MONT
                    ADD  M6S-VAL-MONT TO SRT-VAL-{LINEA}3
                    MOVE M6S-VAL-MONT TO WSS-VAL-COL3
               ELSE
               IF M6S-COD-VCAM = WSS-COD-PESO
                    ADD M6S-VAL-MONT TO SRT-VAL-{LINEA}1
                    MOVE M6S-VAL-MONT TO WSS-VAL-COL1
               ELSE
               IF M6S-NUM-CORR = '01' OR '02'
                   IF M6S-COD-VCAM = WSS-COD-UEFE
                       MULTIPLY M6S-VAL-MONT BY 
                       WSS-SGV-FUEF GIVING
                       M6S-VAL-MONT 
                       ADD  M6S-VAL-MONT TO SRT-VAL-{LINEA}1
                       ADD M6S-VAL-REAJ TO  SRT-VAL-{LINEA}1
                       MOVE M6S-VAL-MONT TO WSS-VAL-COL1
                       ADD M6S-VAL-REAJ TO  WSS-VAL-COL1
                   ELSE
                   IF M6S-COD-VCAM = WSS-COD-RDOL
                       MULTIPLY M6S-VAL-MONT BY 
                       WSS-SGV-FRDO GIVING
                       M6S-VAL-MONT 
                       ADD M6S-VAL-MONT TO SRT-VAL-{LINEA}1
                       ADD M6S-VAL-REAJ TO SRT-VAL-{LINEA}1
                       MOVE M6S-VAL-MONT TO WSS-VAL-COL1
                       ADD M6S-VAL-REAJ TO  WSS-VAL-COL1
                   ELSE
                      NEXT SENTENCE
               ELSE
               IF M6S-NUM-CORR = '03' OR '04'
                   IF M6S-COD-VCAM = WSS-COD-UEFE
                       MULTIPLY M6S-VAL-MONT BY 
                       WSS-SGV-FUEF GIVING
                       M6S-VAL-MONT 
                       ADD M6S-VAL-MONT TO SRT-VAL-{LINEA}2
                       ADD M6S-VAL-REAJ TO SRT-VAL-{LINEA}2
                       MOVE M6S-VAL-MONT TO WSS-VAL-COL2
                       ADD M6S-VAL-REAJ TO  WSS-VAL-COL2
                   ELSE
                   IF M6S-COD-VCAM = WSS-COD-RDOL
                      MULTIPLY M6S-VAL-MONT BY 
                      WSS-SGV-FRDO GIVING
                      M6S-VAL-MONT 
                      ADD M6S-VAL-MONT TO SRT-VAL-{LINEA}2
                      ADD M6S-VAL-REAJ TO SRT-VAL-{LINEA}2
                      MOVE M6S-VAL-MONT TO WSS-VAL-COL2
                      ADD M6S-VAL-REAJ TO  WSS-VAL-COL2.
