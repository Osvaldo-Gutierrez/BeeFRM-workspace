*% IF WSS
      *<<<< WSS
       77  WSS-LEE-DBC-DESDE-LAD  VALUE 'N'   PIC X(01).
       77  WSS-CIC-ICLI    VALUE SPACES PIC X(12).
       77  WSS-YA-LEI-REG-2 VALUE 'N' PIC X(01).
       77  WSS-COMPARA      VALUE 'N' PIC X(01).
      *          03 FRM-CPIM.
      * AEY = Protegido, normal
      *             05 FRM-CPIM-AEY         PIC X(01) VALUE '1'.
      * UBY = Desprotegido, brillante
      *             05 FRM-CPIM-UBY         PIC X(01) VALUE 'I'.
      * ANY = Protegido, non-display
      *             05 FRM-CPIM-ANY         PIC X(01) VALUE '_'.
      * UNY = Desprotegido, non-display
      *             05 FRM-CPIM-UNY         PIC X(01) VALUE '('.
      * ANY = Protegido, normal
      *             05 FRM-CPSF-AEY         PIC X(01) VALUE '-'.
      * ABY = Protegido, brillante
      *             05 FRM-CPIM-ABY         PIC X(01) VALUE '9'.
      * UST = Virtual para BMS, pues es campo STOPPER en UNISYS
      *             05 FRM-CPIM-UST         PIC X(01) VALUE SPACES.
       COPY DAPBRDCL.
       COPY DAPBRDCL REPLACING DCL BY DCL-CPR.       
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DCL-REQA==. 

       COPY DAPBRRCC.

      
       COPY SGCBRDBC.
       COPY GNSWGCPT.
       COPY GNSWGSTR.
       77  WSS-COD-ENTI         VALUE 'DCL'     PIC X(03).
       01  BKP-LAD              VALUE SPACES    PIC X(1280).
       01  WSS-FEC-FAMD.
           03 WSS-NUM-SAMD                      PIC X(02).
           03 WSS-NUM-AAMD                      PIC X(02).
           03 WSS-NUM-MAMD                      PIC X(02).
           03 WSS-NUM-DAMD                      PIC X(02).
      *CORRECION NAVEGABILIDAD
       01 CLD-VARI.
          03 WSS-EXI-CLI    VALUE ZEROES    PIC 9(01).
          03 WSS-UNO        VALUE 1         PIC 9(01).
          03 WSS-FEC        VALUE 'FEC'     PIC X(03).
          03 WSS-OPE        VALUE 'OPE'     PIC X(03).
          03 WSS-CLI        VALUE 'CLI'     PIC X(03).
          03 WSS-AUT        VALUE 'AUT'     PIC X(03).
          03 WSS-23         VALUE '23'      PIC X(02).
          03 WSS-00         VALUE '00'      PIC X(02).
          03 WSS-CON        VALUE 'CON'     PIC X(03).
          03 WSS-ANT        VALUE 'ANT'     PIC X(03).
          03 WSS-DES        VALUE 'DES'     PIC X(03).
          03 WSS-DAP        VALUE 'DAP'     PIC X(03).
          03 WSS-GNS        VALUE 'GNS'     PIC X(03).
          03 WSS-SGC        VALUE 'SGC'     PIC X(03).
          03 WSS-GAB        VALUE 'GAB'     PIC X(03).
          03 WSS-GKY        VALUE 'GKY'     PIC X(03).
          03 WSS-N          VALUE 'N'       PIC X(01).
          03 WSS-S          VALUE 'S'       PIC X(01).
          03 WSS-CLINEX     VALUE 'CLI    NEX  '         PIC X(12).
          03 WSS-LADKEYOLAD VALUE 'LAD-KEY-OLAD'         PIC X(12).
          03 WSS-LADKEYCLAD VALUE 'LAD-KEY-CLAD'         PIC X(12).
          03 WSS-LADKEYALAD VALUE 'LAD-KEY-ALAD'         PIC X(12).
          03 WSS-DBCIDCICLI VALUE 'DBC-IDC-ICLI'         PIC X(12).
          03 WSS-RCCKEYIOPD VALUE 'RCC-KEY-IOPD'         PIC X(12).
          03 WSS-CLNOINGRFEC VALUE 'CL NOINGRFEC'        PIC X(12).
          03 WSS-CLNOINGROPE VALUE 'CL NOINGROPE'        PIC X(12).
          03 WSS-CLNOINGRCLI VALUE 'CL NOINGRCLI'        PIC X(12).
          03 WSS-CLNOINGRAUT VALUE 'CL NOINGRAUT'        PIC X(12).
          03 WSS-CLINOEXFECPK VALUE 'LADCLD000001'       PIC X(12).
          03 WSS-CLINONXT     VALUE 'LADCLD000002'       PIC X(12).
          03 WSS-CLINONXTAUT  VALUE 'LADCLD000003'       PIC X(12).
          03 WSS-CLINONXTCLI  VALUE 'LADCLD000004'       PIC X(12).
          03 WSS-CLINONXTFEC  VALUE 'LADCLD000005'       PIC X(12).
          03 WSS-CLINONXTOAUT VALUE 'LADCLD000006'       PIC X(12).
          03 WSS-CLINONXTOCLI VALUE 'LADCLD000007'       PIC X(12).
          03 WSS-CLINONXTOIPE VALUE 'LADCLD000008'       PIC X(12).
          03 WSS-CLINONXTOPE  VALUE 'LADCLD000009'       PIC X(12).
          03 WSS-CLINOPRV     VALUE 'LADCLD000010'       PIC X(12).
          03 WSS-CLINOPRVAUT  VALUE 'LADCLD000011'       PIC X(12).
          03 WSS-CLINOPRVCLI  VALUE 'LADCLD000012'       PIC X(12).
          03 WSS-CLINOPRVFEC  VALUE 'LADCLD000013'       PIC X(12).
          03 WSS-CLINOPRVOPE  VALUE 'LADCLD000014'       PIC X(12).
          03 WSS-CLIPF7PF8    VALUE 'LADCLD000015'       PIC X(12).
          03 WSS-TX01         VALUE 'Fecha Time Stamp'   PIC X(16).
          03 WSS-TX02         VALUE 'Hora Time Stamp'    PIC X(15).
          03 WSS-TX03         VALUE 'Ind.TACC'           PIC X(08).
          03 WSS-TX04         VALUE 'Estado'             PIC X(06).
          03 WSS-TX05         VALUE 'C.OTRN'             PIC X(06).
          03 WSS-TX06         VALUE 'A.T.ATRN'           PIC X(08).
          03 WSS-TX07         VALUE 'Glo.Disp'           PIC X(08).          
          03 WSS-TX08         VALUE 'Correlativo LIQ'    PIC X(15).
          03 WSS-TX09         VALUE 'Rescate (Bolsa)'    PIC X(15).
          03 WSS-TX10         VALUE 'Diferencia'         PIC X(10).
          03 WSS-TX11         VALUE 'Cap. Actualizado'   PIC X(16).
          03 WSS-TX12         VALUE 'Val Final Act.'     PIC X(14).
          03 WSS-TX13         VALUE 'Int. Devengado'     PIC X(14).
          03 WSS-TX14         VALUE 'Acepta Dif (S/N)'   PIC X(16).
          03 WSS-TX15         VALUE 'GLS-FLAG'           PIC X(08).

          
      *FIN CORRECION NAVEGABILIDAD          
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
      *Entidades de SGC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-RCC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-IMP-REQA==.
      *
       01   VAR-PROGRAMA.
            03 PGM-COD-CREB              VALUE ' '       PIC X(03).
            03 PGM-STAT-RCC.
               88 PGM-STAT-RCC-NUL       VALUE '  '.
               07 PGM-STT1-RCC           VALUE '2'       PIC X(01).
                  88 PGM-STAT-RCC-OKS    VALUE '0'.
               07 PGM-STT2-RCC           VALUE '3'       PIC X(01).

      *>>>>
*% END
*% IF INI_FIO_IBM
      *<<<< INI_FIO
           PERFORM GET-FHOY.
           IF SCR-INST = WSS-GAB AND
              ( FRM-FFLD = FRM-FFLD-PF8 OR
                FRM-FFLD = FRM-FFLD-PF7 )
                  MOVE WSS-GKY TO SCR-INST.
      *>>>> INI_FIO
*% END
*% IF FND_KEY_CMND
      *<<<< FND_KEY_CMND
           IF FRM-COD-CREB IN {FRM}-FLD = WSS-OPE OR
              FRM-COD-CREB IN {FRM}-FLD = WSS-CLI
               MOVE WSS-N TO WSS-LEE-DBC-DESDE-LAD
               PERFORM OBTENER-NOMBRE-DBC.
           PERFORM SET-KEY-LAD.
      
           MOVE FIO-GET-NLS TO FIO-CMND.
      *>>>> FND_KEY_CMND
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
      *CORRECION NAVEGABILIDAD    
      *LECTURA NO OK
           IF NOT FIO-STAT-OKS AND FRM-COD-CREB IN {FRM}-FLD = WSS-FEC
               MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
               MOVE WSS-23         TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CLINOEXFECPK' TO MSG-COD-MENS
               MOVE 'DAP'          TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.

           IF NOT FIO-STAT-OKS AND FRM-COD-CREB IN {FRM}-FLD = WSS-OPE
               MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
               MOVE WSS-23         TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CLINONXTOIPE' TO MSG-COD-MENS
               MOVE 'DAP' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.      

           IF NOT FIO-STAT-OKS AND  FRM-COD-CREB IN {FRM}-FLD = WSS-AUT
               MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
               MOVE WSS-23         TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CLINONXTOAUT' TO MSG-COD-MENS
               MOVE 'DAP'          TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.

           IF NOT FIO-STAT-OKS AND  FRM-COD-CREB IN {FRM}-FLD = WSS-CLI
               MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
               MOVE WSS-23         TO FIO-STAT
               MOVE WSS-23         TO PGM-STAT-LAD
               MOVE 'CLINONXTOCLI' TO MSG-COD-MENS
               MOVE 'DAP'          TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-FND-KEY.               
               
      *FIN CORRECION NAVEGABILIDAD
      
           IF NOT FIO-STAT-OKS
               GO TO FIN-FND-KEY.
      
      
      

           IF FRM-COD-CREB IN {FRM}-FLD = WSS-FEC
      *CORRECION NAVEGABILIDAD     
               IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI OR
                  LAD-NUM-DLAD IN {FRM}-FLD NOT =
                  LAD-NUM-DLAD IN LAD    OR
                  LAD-NUM-MLAD IN {FRM}-FLD NOT =
                  LAD-NUM-MLAD IN LAD    OR
                  LAD-NUM-SLAD IN {FRM}-FLD NOT =
                  LAD-NUM-SLAD IN LAD    OR
                  LAD-NUM-ALAD IN {FRM}-FLD NOT =
                  LAD-NUM-ALAD IN LAD
                    MOVE WSS-23 TO FIO-STAT
                    MOVE WSS-23 TO PGM-STAT-LAD
                    MOVE WSS-CLINOEXFECPK TO MSG-COD-MENS
                    MOVE WSS-DAP TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
           
           
           IF FRM-COD-CREB IN {FRM}-FLD = WSS-OPE
               IF LAD-COD-ENTO IN LAD NOT = WSS-COD-ENTI OR
                  LAD-CAI-IOPD IN {FRM}-FLD NOT =
                  LAD-CAI-IOPD IN LAD    OR
                  LAD-IIC-IOPD IN {FRM}-FLD NOT =
                  LAD-IIC-IOPD IN LAD
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE WSS-23 TO FIO-STAT
                    MOVE WSS-23 TO PGM-STAT-LAD
                    MOVE WSS-CLINONXTOIPE TO MSG-COD-MENS
                    MOVE WSS-DAP TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
      
           IF FRM-COD-CREB IN {FRM}-FLD = WSS-AUT
               IF LAD-COD-ENTA IN LAD NOT = WSS-COD-ENTI OR
                  LAD-COD-AUTO IN {FRM}-FLD NOT =
                  LAD-COD-AUTO IN LAD
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE WSS-23 TO FIO-STAT
                    MOVE WSS-23 TO PGM-STAT-LAD
                    MOVE WSS-CLINONXTOAUT TO MSG-COD-MENS
                    MOVE WSS-DAP TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
      
           IF FRM-COD-CREB IN {FRM}-FLD = WSS-CLI AND
              WSS-EXI-CLI NOT = ZEROES
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE WSS-23         TO FIO-STAT
                    MOVE WSS-23         TO PGM-STAT-LAD
                    MOVE WSS-CLINEX TO MSG-COD-MENS
                    MOVE WSS-SGC        TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.
      
           IF FRM-COD-CREB IN {FRM}-FLD = WSS-CLI
               IF LAD-COD-ENTC IN LAD NOT = WSS-COD-ENTI OR
                  LAD-CIC-ICLI IN LAD     NOT =
                  DBC-CIC-ICLI IN DBC
                    MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR
                    MOVE WSS-23 TO FIO-STAT
                    MOVE WSS-23 TO PGM-STAT-LAD
                    MOVE WSS-CLINONXTOCLI TO MSG-COD-MENS
                    MOVE WSS-DAP TO MSG-COD-SIST
                    PERFORM GET-MSG
                    MOVE MSG-GLS-DESC TO FRM-MENS
                    GO TO FIN-FND-KEY.

           IF NOT ( FRM-COD-CREB IN {FRM}-FLD = WSS-OPE OR
                    FRM-COD-CREB IN {FRM}-FLD = WSS-CLI )
                     MOVE WSS-S TO WSS-LEE-DBC-DESDE-LAD
                     PERFORM OBTENER-NOMBRE-DBC.
      
           MOVE LAD-CIC-ICLI IN LAD TO WSS-CIC-ICLI.                    
      *FIN CORRECION NAVEGABILIDAD
           IF SCR-CMND = WSS-CON
              IF FRM-FFLD = FRM-FFLD-PF8
                    PERFORM SET-KEY-LAD
                    PERFORM PRO-PF8
              ELSE
                IF FRM-FFLD = FRM-FFLD-PF7
                    PERFORM SET-KEY-LAD
                    PERFORM PRO-PF7.
      
           IF PGM-STAT-LAD-OKS
           IF FRM-FFLD = FRM-FFLD-PF7 OR
              FRM-FFLD = FRM-FFLD-PF8
               MOVE WSS-S TO WSS-LEE-DBC-DESDE-LAD
               PERFORM OBTENER-NOMBRE-DBC.
      
           IF PGM-STAT-LAD-OKS
               IF LAD-COD-ENTF IN LAD = WSS-COD-ENTI
                   MOVE WSS-00   TO FIO-STAT

                   MOVE SPACES TO DCL

                   MOVE LAD-COD-CONT IN LAD TO DCL
                   MOVE WSS-00   TO PGM-STAT-DBC
                   MOVE WSS-00   TO PGM-STAT-DCL.
      
      *>>>> FIN_FND_KEY
*% END
*% IF INI_SCR_DMSG
*% IF PGM_PQ
      *<<<< INI_SCR_DMSG
      * Con este codigo logramos que una vez terminada la consulta
      * las claves no se borren de la pantalla
           IF SCR-CMND = WSS-CON
                AND
              SCR-INST = SCR-INST-GKY
                AND
              FRM-FFLD NOT = FRM-FFLD-RET
                  MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR.
      *>>>> INI_SCR_DMSG
*% END
*% END
*% IF FIN_SCR_TRL
      *<<<< FIN_SCR_TRL
      
           IF PGM-STAT-LAD-OKS
               MOVE WSS-CLIPF7PF8 TO MSG-COD-MENS
               MOVE WSS-GNS TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MEN2.
      
      *>>>> FIN_SCR_TRL
*% END
*% IF PFS_SCR_GKEY_PREV
      *<<<< PFS_SCR_GKEY_PREV
           IF NOT ( FRM-FFLD = FRM-FFLD-PF7 )
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 )
      *>>>> PFS_SCR_GKEY_PREV
*% END
*% IF PFS_GET_CON
      *<<<< PFS_GET_CON
           IF NOT ( FRM-FFLD = FRM-FFLD-PF7 )
           IF NOT ( FRM-FFLD = FRM-FFLD-PF8 )
      *>>>> PFS_GET_CON
*% END
*% IF POST_SCR_PUTA
      *<<<< POST_SCR_PUTA
           IF PGM-STAT-LAD-OKS
               IF LAD-COD-INST = WSS-ANT
               
                    MOVE WSS-DAP TO TAB-COD-SIST
                    MOVE WSS-DAP TO FIO-SIST
                    PERFORM SET-KEY-LAD
                    MOVE FIO-GET-NXT TO FIO-CMND
                    PERFORM DAP-FIO-LAD
                    IF FIO-STAT-OKS
                         IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI
                            MOVE WSS-23  TO FIO-STAT
                            MOVE FIO-STAT TO PGM-STAT-LAD
                         ELSE


                             MOVE LAD-COD-CONT IN LAD TO DCL-CPR
                             MOVE WSS-S TO WSS-COMPARA
                             GO TO FIN-AT-DES.
      
           IF PGM-STAT-LAD-OKS
               IF LAD-COD-INST = WSS-DES

                     MOVE WSS-DAP TO TAB-COD-SIST
                     MOVE WSS-DAP TO FIO-SIST
                     PERFORM SET-KEY-LAD
                     MOVE FIO-GET-PRV TO FIO-CMND
                     PERFORM DAP-FIO-LAD
                     IF FIO-STAT-OKS
                          IF LAD-COD-ENTF IN LAD NOT = WSS-COD-ENTI
                               MOVE WSS-23 TO FIO-STAT
                               MOVE FIO-STAT TO PGM-STAT-LAD
                          ELSE

                               MOVE LAD-COD-CONT IN LAD TO DCL-CPR
                               MOVE WSS-S TO WSS-COMPARA.
       FIN-AT-DES.
      
      *     ==>>  01
      *
           MOVE WSS-TX01            TO FRM-GLS-TX01     IN {FRM}-FLD.
           MOVE FRM-CPIM-AEY        TO FRM-GLS-TX01-ATR IN {FRM}-FLD.
           MOVE FRM-CPIM-AEY        TO DCL-FEC-FTRN-ATR IN {FRM}-FLD.
           IF WSS-COMPARA = WSS-S
               IF DCL-FEC-FTRN IN DCL NOT =
                  DCL-FEC-FTRN IN DCL-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX01-ATR IN {FRM}-FLD
                   MOVE FRM-CPIM-ABY TO DCL-FEC-FTRN-ATR IN {FRM}-FLD.      

      *     ==>>  02
      *      
           MOVE WSS-TX02          TO FRM-GLS-TX02     IN {FRM}-FLD.
           MOVE FRM-CPIM-AEY      TO FRM-GLS-TX02-ATR IN {FRM}-FLD.
           MOVE FRM-CPIM-AEY      TO DCL-HRA-HRTR-ATR IN {FRM}-FLD.
             IF WSS-COMPARA = WSS-S
               IF (DCL-HRA-HRTR IN DCL NOT =
                  DCL-HRA-HRTR IN DCL-CPR)                  
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX02-ATR IN {FRM}-FLD
                   MOVE FRM-CPIM-ABY TO DCL-HRA-HRTR-ATR IN {FRM}-FLD.


      *     ==>>  03
      *
           MOVE WSS-TX03          TO FRM-GLS-TX03     IN {FRM}-FLD.
           MOVE FRM-CPIM-AEY      TO FRM-GLS-TX03-ATR IN {FRM}-FLD.
           MOVE FRM-CPIM-AEY      TO DCL-COD-TACC-ATR IN {FRM}-FLD.
           IF WSS-COMPARA = WSS-S
               IF DCL-COD-TACC IN DCL NOT =
                  DCL-COD-TACC IN DCL-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX03-ATR IN {FRM}-FLD
                   MOVE FRM-CPIM-ABY TO DCL-COD-TACC-ATR IN {FRM}-FLD.


      *     ==>>  04
      *      
           MOVE WSS-TX04        TO FRM-GLS-TX04 IN {FRM}-FLD.
           MOVE FRM-CPIM-AEY    TO FRM-GLS-TX04-ATR IN {FRM}-FLD.
           MOVE FRM-CPIM-AEY    TO DCL-COD-STAT-ATR IN {FRM}-FLD.
           IF WSS-COMPARA = WSS-S
               IF DCL-COD-STAT IN DCL NOT =
                  DCL-COD-STAT IN DCL-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX04-ATR IN {FRM}-FLD
                   MOVE FRM-CPIM-ABY TO DCL-COD-STAT-ATR IN {FRM}-FLD.

      *     ==>>  05
      *      
           MOVE WSS-TX05         TO FRM-GLS-TX05     IN {FRM}-FLD.
           MOVE FRM-CPIM-AEY     TO FRM-GLS-TX05-ATR IN {FRM}-FLD.
           MOVE FRM-CPIM-AEY     TO DCL-COD-OTRN-ATR IN {FRM}-FLD.
             IF WSS-COMPARA = WSS-S
               IF DCL-COD-OTRN IN DCL NOT =
                  DCL-COD-OTRN IN DCL-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX05-ATR IN {FRM}-FLD
                   MOVE FRM-CPIM-ABY TO DCL-COD-OTRN-ATR IN {FRM}-FLD.

      *     ==>>  06
      *      
           MOVE WSS-TX06         TO FRM-GLS-TX06     IN {FRM}-FLD.
           MOVE FRM-CPIM-AEY     TO FRM-GLS-TX06-ATR IN {FRM}-FLD.
           MOVE FRM-CPIM-AEY     TO DCL-COD-ATRN-ATR IN {FRM}-FLD.

           
             IF WSS-COMPARA = WSS-S
               IF (DCL-COD-ATRN IN DCL NOT =
                  DCL-COD-ATRN IN DCL-CPR)                  
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX06-ATR IN {FRM}-FLD
                   MOVE FRM-CPIM-ABY TO DCL-COD-ATRN-ATR IN {FRM}-FLD.

      *     ==>>  07
      *
           MOVE WSS-TX07            TO FRM-GLS-TX07     IN {FRM}-FLD.
           MOVE FRM-CPIM-AEY        TO FRM-GLS-TX07-ATR IN {FRM}-FLD.
           MOVE FRM-CPIM-AEY        TO DCL-GLS-DISP-ATR IN {FRM}-FLD.
           IF WSS-COMPARA = WSS-S
               IF DCL-GLS-DISP IN DCL NOT =
                  DCL-GLS-DISP IN DCL-CPR
                   MOVE FRM-CPIM-ABY TO FRM-GLS-TX07-ATR IN {FRM}-FLD
                   MOVE FRM-CPIM-ABY TO DCL-GLS-DISP-ATR IN {FRM}-FLD.    


      *     ==>>  08
