*%! Codigo adicional para archivo
*% IF INI_PUT_ALL_CIN
      *<<<< INI_PUT_ALL_CIN
           IF SCR-CMND = 'LIB'
               MOVE SPACES TO CIN-COD-INST IN CIN
               MOVE SPACES TO CIN-GLS-DESC IN CIN
               MOVE ZEROES TO CIN-NUM-DRET IN CIN
               MOVE ZEROES TO CIN-NUM-DVAS IN CIN
               MOVE ZEROES TO CIN-NUM-MVAS IN CIN
               MOVE ZEROES TO CIN-NUM-SVAS IN CIN
               MOVE ZEROES TO CIN-NUM-AVAS IN CIN
               MOVE SPACES TO CIN-COD-IUSR IN CIN
               MOVE SPACES TO CIN-COD-SISA IN CIN.
      *>>>>
*% END
*% IF INI_CIN_FEC_FADQ
      *<<<< INI_CIN_FEC_FADQ
           MOVE FEC-VHBL-N  TO FEC-VHBL.
      *>>>>
*% END
*% IF INI_CIN_FEC_FUAS
      *<<<< INI_CIN_FEC_FUAS
           MOVE FEC-VHBL-N  TO FEC-VHBL.
           IF CIN-FEC-FUAS IN CIN-FLD NOT > ZEROES
               PERFORM GET-FHOY
               MOVE HOY-SHOY TO CIN-NUM-SUAS IN CIN-FLD
               MOVE HOY-AHOY TO CIN-NUM-AUAS IN CIN-FLD
               MOVE HOY-MHOY TO CIN-NUM-MUAS IN CIN-FLD
               MOVE HOY-DHOY TO CIN-NUM-DUAS IN CIN-FLD
               MOVE CIN-FEC-FUAS IN CIN-FLD TO FEC-FECH.
      *>>>>
*% END
*% IF CIN_COD_INST
      *<<<< CIN_COD_INST
           IF CIN-COD-INST IN CIN-FLD NOT > SPACES AND
              ( SCR-CMND = 'ING' OR 'MOD' OR 'ACT' OR 'MOV' )
                  GO TO FIN-VAL-CIN-COD-INST.
      *>>>>
*% END
*% IF CIN_GLS_DESC
      *<<<< CIN_GLS_DESC
           IF CIN-GLS-DESC IN CIN-FLD NOT > SPACES
               IF NOT ( SCR-CMND = 'ING' OR 'MOD' OR 'ACT' OR 'MOV' )
                     PERFORM ERR-GLS
                     GO TO FIN-VAL-CIN-GLS-DESC.
      *>>>>
*% END
*% IF CIN_COD_IUSR
      *<<<< CIN_COD_IUSR
           IF CIN-COD-IUSR IN CIN-FLD NOT > SPACES AND
              ( SCR-CMND = 'ING' OR 'MOD' OR 'ACT' OR 'MOV' )
                  GO TO FIN-VAL-CIN-COD-IUSR.
      *>>>>
*% END
*% IF CIN_COD_SISA
      *<<<< CIN_COD_SISA
           IF CIN-COD-SISA IN CIN-FLD NOT > SPACES AND
              SCR-CMND = 'MOV' AND
              CIN-MSC-STAT IN CIN-FLD = 'A'
                 MOVE 'CDC'          TO MSG-COD-SIST
                 MOVE 'CODSISAOBL' TO MSG-COD-MENS IN MSG
                 PERFORM GET-MSG
                 MOVE MSG-GLS-DESC   TO FRM-MENS
                 MOVE FRM-SUAR-MAL   TO FRM-SUAR
                 GO TO FIN-VAL-CIN-COD-SISA.

           IF CIN-COD-SISA IN CIN-FLD NOT > SPACES AND
              ( SCR-CMND = 'ING' OR 'MOD' OR 'ACT' OR 'MOV' )
                  GO TO FIN-VAL-CIN-COD-SISA.
      *>>>>
*% END
*% IF CIN_NUM_DRET
      *<<<< CIN_NUM_DRET
           IF SCR-CMND = 'ASG' AND
              CIN-NUM-DRET IN CIN-FLD NOT > ZEROES
                       MOVE 'CDC'          TO MSG-COD-SIST
                       MOVE 'NUMDRETOBL' TO MSG-COD-MENS IN MSG
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC   TO FRM-MENS
                       MOVE FRM-SUAR-MAL   TO FRM-SUAR
                       GO TO FIN-VAL-CIN-NUM-DRET.

           IF SCR-CMND = 'MOV'
               IF CIN-NUM-DRET IN CIN-FLD NOT > ZEROES
                   IF CIN-MSC-STAT IN CIN-FLD = 'A'
                       MOVE 'CDC'          TO MSG-COD-SIST
                       MOVE 'NUMDRETOBL' TO MSG-COD-MENS IN MSG
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC   TO FRM-MENS
                       MOVE FRM-SUAR-MAL   TO FRM-SUAR
                       GO TO FIN-VAL-CIN-NUM-DRET
                   ELSE
                       NEXT SENTENCE
               ELSE
                   IF CIN-MSC-STAT IN CIN-FLD = 'D'
                       MOVE 'CDC'          TO MSG-COD-SIST
                       MOVE 'NUMDRETBLK' TO MSG-COD-MENS IN MSG
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC   TO FRM-MENS
                       MOVE FRM-SUAR-MAL   TO FRM-SUAR
                       GO TO FIN-VAL-CIN-NUM-DRET.
      *>>>>
*% END
*% IF VAL_CON_FLD_CIN
      *<<<< VAL_CON_FLD_CIN
           IF CIN-FEC-FADQ IN CIN-FLD > ZEROES AND
              CIN-FEC-FUAS IN CIN-FLD > ZEROES
               MOVE CIN-FEC-FADQ IN CIN-FLD TO FEC-FEC1
               MOVE CIN-FEC-FUAS IN CIN-FLD TO FEC-FEC2
               MOVE FEC-FORM-FEC TO FEC-FORM
               MOVE FEC-CMP-FEC  TO FEC-CMND
               PERFORM PRO-FEC
               IF FEC-STAT-GRT
                   MOVE 'CDC'          TO MSG-COD-SIST
                   MOVE 'FECFADQ>FUAS' TO MSG-COD-MENS IN MSG
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC   TO FRM-MENS
                   MOVE FRM-SUAR-MAL   TO FRM-SUAR
                   GO TO FIN-VAL-CON-FLD-CIN.

           IF CIN-NUM-DRET IN CIN-FLD > ZEROES
               MOVE CIN-FEC-FUAS IN CIN-FLD TO FEC-FECH
               MOVE FEC-FORM-FEC            TO FEC-FORM
               MOVE CIN-NUM-DRET IN CIN-FLD TO FEC-NDIA
               MOVE FEC-SUM-DIA             TO FEC-CMND
               PERFORM PRO-FEC
               MOVE FEC-FECH TO CIN-FEC-FVAS IN CIN-FLD
           ELSE
               MOVE ZEROES   TO CIN-FEC-FVAS IN CIN-FLD.
      *>>>>
*% END
