*% IF WSS
      *<<<< WSS
       COPY GNSWGFEC.
       COPY GNSWGHOY.
           
       01  WSS-VARI.
           03 WSS-IND-IPPH   VALUE 0           PIC 9(01).
              88 PRIMER-PAGE-HEADING                  VALUE 0.
           03 WSS-NUM-MCIN                     PIC X(03).
           03 WSS-VSN-BCI VALUE 'N'            PIC X(01).
       COPY TABBRUSR.
      *COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-USR-REQA==.
      *>>>> END
*% END
*% IF PH_STK
      *<<<< PH_STK
           IF PRIMER-PAGE-HEADING
               MOVE 0 TO PAGE-COUNTER
               MOVE 1 TO PRINT-SWITCH
               MOVE 1 TO WSS-IND-IPPH.
      *>>>>
*% END
*% IF INI_INP
      *<<<< INI_INP
*% IF PGM_VMS
      * INI SOLO VAX

           MOVE 'CDC'        TO FIO-SIST.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           PERFORM GNS-FIO-TAB.
           MOVE 'CDC'        TO FIO-SIST.
           MOVE FIO-INP      TO FIO-CMND.
           PERFORM GNS-FIO-TAB.

      * FIN SOLO VAX
*% END
            
           MOVE 'CDC' TO TAB-COD-SIST.
           MOVE 'LON' TO TAB-COD-TTAB IN TAB.
           MOVE 'STK' TO TAB-COD-CTAB IN TAB.
           PERFORM BUS-TAB.
           IF MSG-COD-MENS > SPACES
               MOVE 'S' TO WSS-VSN-BCI.
*% END
*% IF CH_STK_CIN_COD_LONG
      *<<<< CH_STK_CIN_COD_LONG
            
           MOVE 'CDC' TO TAB-COD-SIST.
           MOVE 'LON' TO TAB-COD-TTAB IN TAB.
           MOVE CIN-COD-LONG IN SRT TO TAB-COD-CTAB IN TAB.
*% IF PGM_VMS
      * INI SOLO VAX
               MOVE FIO-GET-KEY         TO FIO-CMND
               CALL TAB-GLS-FTAB USING FIO-CMND TAB
      * FIN SOLO VAX
*% ELSE
      * INI SOLO IBM
               PERFORM BUS-TAB
      * FIN SOLO IBM
*% END
           IF NOT FIO-STAT-OKS
               MOVE 'CODIGO INEXISTENTE ' TO TAB-GLS-DESC
               MOVE SPACES TO TAB-GLS-DATA
           ELSE
               MOVE TAB-GLS-DESC TO WSS-GLS-LONG
               MOVE TAB-GLS-DATA TO WSS-NUM-MCIN
               MOVE WSS-NUM-MCIN TO WSS-NUM-TCIN.
      *>>>> END
*% END
*% IF CH_STK_CIN_COD_TUNI
      *<<<< CH_STK_CIN_COD_TUNI
           MOVE 'CDC' TO TAB-COD-SIST.
           MOVE 'TUN' TO TAB-COD-TTAB IN TAB.
           MOVE CIN-COD-TUNI IN SRT TO TAB-COD-CTAB IN TAB.
*% IF PGM_VMS
      * INI SOLO VAX
               MOVE FIO-GET-KEY         TO FIO-CMND
               CALL TAB-GLS-FTAB USING FIO-CMND TAB
      * FIN SOLO VAX
*% ELSE
      * INI SOLO IBM
               PERFORM BUS-TAB
      * FIN SOLO IBM
*% END
           IF NOT FIO-STAT-OKS
               MOVE 'CODIGO INEXISTENTE ' TO TAB-GLS-DESC.
           MOVE TAB-GLS-DESC TO WSS-GLS-TUNI.
      *>>>> END
*% END
*% IF LUP_INP
      *<<<< LUP_INP

           MOVE 1 TO SRT-NUM-TCIN IN SRT.

            IF WSS-VSN-BCI = 'S'
                 IF CIN-MSC-STAT = 'A' 
                      GO TO NXT-INP-SRT.


           MOVE SPACES TO WSS-GLS-SISA IN WSS-STK.
           IF CIN-COD-SISA IN CIN > SPACES
               MOVE 'CDC' TO TAB-COD-SIST
               MOVE 'SIS' TO TAB-COD-TTAB IN TAB
               MOVE CIN-COD-SISA IN CIN TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DCOR TO WSS-GLS-SISA IN WSS-STK.

           IF CIN-COD-IUSR IN CIN > SPACES
               MOVE 'CDC' TO TAB-COD-SIST
               MOVE 'USR' TO TAB-COD-TTAB IN TAB
               MOVE CIN-COD-IUSR IN CIN TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DCOR IN TAB TO WSS-GLS-IUSR IN WSS-STK.

           IF CIN-COD-UBIF IN CIN > SPACES
               MOVE 'CDC' TO TAB-COD-SIST
               MOVE 'UBF' TO TAB-COD-TTAB IN TAB
               MOVE CIN-COD-UBIF IN CIN TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DCOR TO WSS-GLS-UBIF IN WSS-STK.

      *>>>> END
*% END
*% IF EOF
      *<<<< EOF
       COPY GNSBBMSG.
       COPY GNSBBTAB.
       COPY GNSBGHOY.
       COPY GNSBGFEC.

      *COPY TABBBUSR.
      *COPY TABBFUSR.

      *>>>>
*% END
