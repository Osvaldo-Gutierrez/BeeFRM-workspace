*%! Codigo adicional para archivo
*% IF WSS
      *<<<< WSS
       COPY GNSWGFEC.
       COPY GNSWGHOY.
       01  WSS-VARI.
           03 WSS-GLS-INGR                        PIC X(03).
       COPY TABBRUSR.
      *COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-USR-REQA==.
      *>>>> END
*% END
*% IF INI_INP
      *<<<< INI_INP
*% IF PGM_VMS
      * INI SOLO VAX

           MOVE 'CDC'        TO FIO-SIST.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           PERFORM GNS-FIO-TAB.
           MOVE 'CDC'        TO FIO-SIST.
           MOVE FIO-INP      TO FIO-CMND.
           PERFORM GNS-FIO-TAB.

      * FIN SOLO VAX
*% END
           DISPLAY 'INGRESE LIMITE DE USOS EN FORMATO 999'.
           ACCEPT WSS-GLS-INGR.
           IF WSS-GLS-INGR NOT NUMERIC
               DISPLAY WSS-GLS-INGR
                       ' : LIMITE DE USOS NO ES NUMERICO: ABORTO'
               PERFORM GNS-PRO-END.
           MOVE WSS-GLS-INGR TO WSS-NUM-USOS.
           DISPLAY '        LIMITE DE USOS            ' WSS-GLS-INGR.
           
           DISPLAY 'INGRESE LIMITE DE ERRORES EN FORMATO 999'.
           ACCEPT WSS-GLS-INGR.
           IF WSS-GLS-INGR NOT NUMERIC
               DISPLAY WSS-GLS-INGR
                       ' : LIMITE DE ERRORES NO ES NUMERICO: ABORTO'
               PERFORM GNS-PRO-END.
           MOVE WSS-GLS-INGR TO WSS-NUM-ERRO.
           DISPLAY '        LIMITE DE ERRORES                '
                   WSS-GLS-INGR.
      *>>>> END
*% END
*% IF LUP_INP
      *<<<< LUP_INP

           MOVE 1 TO SRT-NUM-TCIN.

      * SELECCION REGISTROS
           MOVE SPACES TO SRT-IND-USOS
                          SRT-IND-ERRO.

           IF WSS-NUM-USOS NOT > CIN-NUM-USOS IN CIN
               MOVE '*' TO SRT-IND-USOS.

           IF WSS-NUM-ERRO NOT > CIN-NUM-ERRO IN CIN
               MOVE '*' TO SRT-IND-ERRO.

           IF SRT-IND-USOS = SPACES AND
              SRT-IND-ERRO = SPACES
               GO TO NXT-INP-SRT.


           MOVE SPACES TO WSS-GLS-TUNI IN WSS-VUC.
           IF CIN-COD-TUNI IN CIN > SPACES
               MOVE 'CDC' TO TAB-COD-SIST
               MOVE 'TUN' TO TAB-COD-TTAB IN TAB
               MOVE CIN-COD-TUNI IN CIN TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DCOR TO WSS-GLS-TUNI IN WSS-VUC.

           MOVE SPACES TO WSS-GLS-SISA IN WSS-VUC.
           IF CIN-COD-SISA IN CIN > SPACES
               MOVE 'CDC' TO TAB-COD-SIST
               MOVE 'SIS' TO TAB-COD-TTAB IN TAB
               MOVE CIN-COD-SISA IN CIN TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DCOR TO WSS-GLS-SISA IN WSS-VUC.

           MOVE SPACES TO USR-GLS-DCOR IN USR
                          WSS-GLS-USER IN WSS-VUC.

           IF CIN-COD-IUSR IN CIN > SPACES
               MOVE 'CDC' TO TAB-COD-SIST
               MOVE 'USR' TO TAB-COD-TTAB IN TAB
               MOVE CIN-COD-IUSR IN CIN TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DCOR TO WSS-GLS-USER IN WSS-VUC.

           MOVE SPACES TO WSS-GLS-UBIF IN WSS-VUC.
           IF CIN-COD-UBIF IN CIN > SPACES
               MOVE 'CDC' TO TAB-COD-SIST
               MOVE 'UBF' TO TAB-COD-TTAB IN TAB
               MOVE CIN-COD-UBIF IN CIN TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DCOR TO WSS-GLS-UBIF IN WSS-VUC.

           MOVE SPACES TO WSS-GLS-LONG IN WSS-VUC.
           IF CIN-COD-LONG IN CIN > SPACES
               MOVE 'CDC' TO TAB-COD-SIST
               MOVE 'LON' TO TAB-COD-TTAB IN TAB
               MOVE CIN-COD-LONG IN CIN TO TAB-COD-CTAB IN TAB
               PERFORM BUS-TAB
               MOVE TAB-GLS-DCOR TO WSS-GLS-LONG IN WSS-VUC.
      *>>>> END
*% END
*% IF EOF
      *<<<< EOF

       COPY GNSBBMSG.
       COPY GNSBBTAB.
       COPY GNSBGHOY.
       COPY GNSBGFEC.

      *COPY TABBBUSR.
      *COPY TABBVUSR.
      *COPY TABBFUSR.
      *>>>> END
*% END
