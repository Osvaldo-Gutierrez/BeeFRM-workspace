*%! Codigo adicional para archivo
*% IF INI_FIO_VAX
      *<<<< INI_FIO_VAX

           MOVE 'CDC'        TO FIO-SIST.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           PERFORM GNS-FIO-TAB.
           MOVE 'CDC'        TO FIO-SIST.
           MOVE FIO-INP      TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
      *>>>>
*% END
*% IF INI_SCR_TRL
      *<<<< INI_SCR_TRL
           IF PGM-STAT-CIN-OKS
                MOVE 'CDC' TO TAB-COD-SIST
                MOVE 'STT' TO TAB-COD-TTAB IN TAB
                MOVE CIN-MSC-STAT IN CIN-FLD TO TAB-COD-CTAB IN TAB
                PERFORM BUS-TAB
                MOVE TAB-GLS-DESC TO FRM-GLS-STAT IN CIN-FLD
                IF CIN-MSC-STAT IN CIN = 'D'
                    IF NOT ( SCR-CMND = 'MOD' OR 'ACT' OR 'ELI' OR
                                        'CON' OR 'ASG' OR 'MOV' )
                        MOVE 'CDC' TO MSG-COD-SIST
                        MOVE 'CIN    DIS' TO MSG-COD-MENS
                        PERFORM GET-MSG
                        MOVE MSG-GLS-DESC TO FRM-MENS
                        MOVE SCR-STAT-CON TO SCR-STAT
                    ELSE
                        NEXT SENTENCE
                ELSE
                IF CIN-MSC-STAT IN CIN = 'A'
                    IF NOT ( SCR-CMND = 'CON' OR 'MOV' OR 'LIB' )
                        MOVE 'CDC' TO MSG-COD-SIST
                        MOVE 'CIN    ASG' TO MSG-COD-MENS
                        PERFORM GET-MSG
                        MOVE MSG-GLS-DESC TO FRM-MENS
                        MOVE SCR-STAT-CON TO SCR-STAT.
      *>>>>
*% END
*% IF FIN_HBL_FLD
      *<<<< FIN_HBL_FLD
           IF SCR-CMND = 'ASG'
               PERFORM PRO-FLD-IDE
               MOVE FRM-CPIM-AEY TO CIN-NUM-USOS-ATR IN CIN-FLD
               MOVE FRM-CPIM-AEY TO CIN-NUM-ERRO-ATR IN CIN-FLD
               MOVE +3           TO CIN-COD-INST-LEN IN CIN-FLD
               MOVE -1           TO CIN-COD-INST-LEN IN CIN-FLD
           ELSE
           IF SCR-CMND = 'MOV'
               MOVE FRM-CPIM-AEY TO CIN-COD-INST-ATR IN CIN-FLD
               PERFORM PRO-FLD-IDE
      *         MOVE FRM-CPIM-AEY TO CIN-GLS-DESC-ATR IN CIN-FLD
               MOVE FRM-CPIM-AEY TO CIN-FEC-FUAS-ATR IN CIN-FLD
      *         MOVE FRM-CPIM-AEY TO CIN-COD-IUSR-ATR IN CIN-FLD
      *         MOVE FRM-CPIM-AEY TO CIN-COD-SISA-ATR IN CIN-FLD
               MOVE FRM-CPIM-AEY TO CIN-NUM-USOS-ATR IN CIN-FLD
               MOVE FRM-CPIM-AEY TO CIN-NUM-ERRO-ATR IN CIN-FLD
               MOVE +3           TO CIN-COD-INST-LEN IN CIN-FLD
               MOVE -1           TO CIN-NUM-DRET-LEN IN CIN-FLD.
      *>>>>
*% END
*% IF CMN_ING
      *<<<< CMN_ING
           MOVE SPACES TO CIN-GLS-FLAG IN CIN
                          CIN-MSC-TACC IN CIN
                          CIN-COD-OTRN IN CIN
                          CIN-COD-ATRN IN CIN
                          CIN-GLS-DISP IN CIN.
           MOVE 'D' TO CIN-MSC-STAT IN CIN.
           PERFORM SET-TST.
      *>>>>
*% END
*% IF CMN_MOD
      *<<<< CMN_MOD
           PERFORM SET-TST.
           IF SCR-CMND = 'ASG'
               MOVE 'A' TO CIN-MSC-STAT IN CIN
               MOVE 'CDC' TO MSG-COD-SIST
               MOVE 'CIN    ASG' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS.
           IF SCR-CMND = 'MOV'
               MOVE 'CDC' TO MSG-COD-SIST
               MOVE 'CIN    MOV' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS.
      *>>>>
*% END
*% IF CMN_ACC
      *<<<< CMN_ACC
           PERFORM SET-TST.
           MOVE 'D' TO CIN-MSC-STAT IN CIN.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM CDC-FIO-CIN.
           MOVE 'CDC' TO MSG-COD-SIST.
           IF FIO-STAT-OKS
               MOVE 'CIN    LIBOK' TO MSG-COD-MENS
           ELSE
               MOVE 'CIN    LIBKO' TO MSG-COD-MENS.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       PRO-FLD-IDE SECTION.
       INI-PRO-FLD-IDE.
           MOVE FRM-CPIM-AEY TO CIN-COD-TUNI-ATR IN CIN-FLD.
           MOVE FRM-CPIM-AEY TO CIN-COD-MARK-ATR IN CIN-FLD.
           MOVE FRM-CPIM-AEY TO CIN-COD-LONG-ATR IN CIN-FLD.
           MOVE FRM-CPIM-AEY TO CIN-FEC-FADQ-ATR IN CIN-FLD.
       FIN-PRO-FLD-IDE.
           EXIT.

       SET-TST SECTION.
       INI-SET-TST.
           PERFORM GET-FHOY.
           MOVE HOY-STP-ITRN TO CIN-STP-ITRN.
       FIN-SET-TST.
           EXIT.
       COPY GNSBEGLS.
      *>>>>
*% END
