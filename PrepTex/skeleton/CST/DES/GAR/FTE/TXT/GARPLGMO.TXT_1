*% REG = 'GMO'
*% RPT = 'GMO'
*% GMO = TRUE
*% IF RR
      *<<<< RR
      *  SI SE CAMBIA EL 'FMS' DEBE CAMBIARSE EL 01 SRT-RED PARA 
      *  QUE QUEDE COHERENTE AL 01 SRT.
      *    19-JUL-91 10:45 AM
       01  SRT-RED.
           03 GMO-COD-OFI-MOV PIC X(3).
           03 GMO-NUM-SIS PIC X(4).
           03 GMO-COD-CRR PIC X(8).
           03 DBC-NUM-ICLI PIC X(8)
              JUSTIFIED RIGHT.
           03 DBC-VRF-ICLI PIC X.
           03 DBC-IND-ICLI PIC X.
           03 DBC-GLS-ICLI PIC X(3).
           03 GMO-IDN-USU-ING PIC X(3).
           03 GMO-FD-MOV PIC 9(2).
           03 GMO-FM-MOV PIC 9(2).
           03 GMO-FA-MOV PIC 9(4).
           03 GMO-VAL-CPO-ANT                   PIC ZZ.ZZZ.ZZZ.ZZ9,9999.
           03 FILLER                            PIC X(11).
           03 DBC-GLS-NOMC PIC X(75).
           03 GMO-VAL-CPO-NVO                   PIC ZZ.ZZZ.ZZZ.ZZ9,9999.
           03 FILLER                            PIC X(11).
           03 GDG-TIP-OPE PIC X(3).
           03 GDG-STP-OPE PIC X(3).
           03 GDG-COD-LIM PIC X.
           03 GMO-HOR-MOV PIC X(6).
      *>>>>
*% END
*% IF WSS
      *<<<< WSS
      *Entidades de GNS
       COPY GNSWGCPT.
      *Entidades de TAB
       COPY TABBROFI.
      *Entidades de SGC
       COPY SGCBRDBC.
      *Entidades de GAR
       COPY GARBRGDG.
       COPY GARBRGDD.

       01  WSS-VARI.
           03  WSS-COD-TFP            VALUE SPACES PIC X(02).
           03  WSS-FEC-FPRO.
               05  WSS-FEC-APRO       VALUE 0      PIC 9(04).
               05  WSS-FEC-MPRO       VALUE 0      PIC 9(02).
               05  WSS-FEC-DPRO       VALUE 0      PIC 9(02).
           03  WSS-FEC-FAUX.
               05  WSS-FEC-AAUX       VALUE 0      PIC 9(04).
               05  WSS-FEC-MAUX       VALUE 0      PIC 9(02).
               05  WSS-FEC-DAUX       VALUE 0      PIC 9(02).
           03  WSS-HOR-MOV.
               05  WSS-HRA-MOV        VALUE 0      PIC 9(02).
               05  WSS-MIN-MOV        VALUE 0      PIC 9(02).
               05  WSS-SEG-MOV        VALUE 0      PIC 9(02).
           03  WSS-CNT-OFI-ANT        VALUE ZEROES PIC 9(05).
           03  GMO-COD-CRR-ANT        VALUE SPACES PIC X(08).
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
      *Entidades de TAB
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OFI-REQA==.
      *Entidades de GAR
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GDG-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GDD-REQA==.
      *Entidades de SGC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
      *>>>>
*% END
*% IF CF_{RPT}_GMO_COD_OFI_MOV
      *<<<< CF_{RPT}_GMO_COD_OFI_MOV
*% IF PGM_VMS
*% INCLUDE 'GARSRC:BUSOFILIS.SRC'
*% END
               MOVE 1 TO WSS-CNT-GAR.
               MOVE WSS-CNT-OFI-ANT TO WSS-CNT-OFI.
               MOVE 0 TO WSS-CNT-OFI-ANT.

*% IF PGM_VMS
*% FLD = 'GMO-COD-OFI-MOV'
*% INCLUDE 'GARSRC:BUSTABVAX.SRC'
*% INCLUDE 'GARSRC:BUSOFIVAX.SRC'
*% END
      *>>>>
*% END
*% IF FIN_DEC AND PGM_VMS
      *<<<< PGM_VMS
      *<<<< END_DCL
       DEC-PH-GMO SECTION.
           USE BEFORE REPORTING PH-GMO.
       INI-DEC-PH-GMO.
      *<<<< PH_GMO

      * Mediante BUS-OFI Obtiene Nombre y Direccion Oficina de Tabla OFI
       PH-OFIC.
      *BUS-OFI Busca Codigo de Tabla
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'OFI' TO TAB-COD-TTAB IN TAB.
           MOVE SPACES TO OFI-GLS-DESC IN OFI.
           MOVE GMO-COD-OFI-MOV IN SRT TO OFI-COD-OFIC IN OFI.
           PERFORM BUS-OFI.
           MOVE OFI-GLS-DESC IN OFI TO WSS-GLS-OFI IN WSS-GMO.
       FIN-DEC-PH-GMO.
           EXIT.
      *>>>>
*% END
*% IF PH_{RPT} AND ( PGM_DOS OR PGM_MVS )
      *<<<< PH_{RPT}
*% INCLUDE 'GARSRC:BUSOFILIS.SRC'
      *>>>>
*% END
*% IF INI
      *<<<< INI

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'TAB'        TO FIO-SIST.
           PERFORM TAB-FIO-OFI.
           MOVE FIO-INP TO FIO-CMND.
           MOVE 'TAB'   TO FIO-SIST.
           PERFORM TAB-FIO-OFI.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'TAB'        TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
           MOVE FIO-INP TO FIO-CMND.
           MOVE 'TAB'   TO FIO-SIST.
           PERFORM GNS-FIO-TAB.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'GAR'        TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
           MOVE FIO-INP TO FIO-CMND.
           MOVE 'GAR'   TO FIO-SIST.
           PERFORM GNS-FIO-TAB.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'GAR'        TO FIO-SIST.
           PERFORM GAR-FIO-GDG.
           MOVE FIO-INP TO FIO-CMND.
           MOVE 'GAR'   TO FIO-SIST.
           PERFORM GAR-FIO-GDG.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'GAR'        TO FIO-SIST.
           PERFORM GAR-FIO-GDD.
           MOVE FIO-INP TO FIO-CMND.
           MOVE 'GAR'   TO FIO-SIST.
           PERFORM GAR-FIO-GDD.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'SGC'        TO FIO-SIST.
           PERFORM SGC-FIO-DBC.
           MOVE FIO-INP TO FIO-CMND.
           MOVE 'SGC'   TO FIO-SIST.
           PERFORM SGC-FIO-DBC.

           DISPLAY 'CODIGO FECHA DE PROCESO  : '.
           ACCEPT WSS-COD-TFP IN WSS-VARI.
           DISPLAY WSS-COD-TFP IN WSS-VARI.

*% INCLUDE 'GARSRC:BUSFECPRO.SRC'
      *>>>>
*% END
*% IF FIN_FST_INP
      *<<<< FIN_FST_INP
           IF WSS-COD-TFP = '01'
              MOVE WSS-FEC-FPRO IN WSS-VARI TO
                   WSS-FEC-FAUX IN WSS-VARI.

           IF WSS-COD-TFP = '10'
              MOVE WSS-FEC-APRO IN WSS-VARI TO
                   WSS-FEC-AAUX IN WSS-VARI
              MOVE WSS-FEC-MPRO IN WSS-VARI TO 
                   WSS-FEC-MAUX IN WSS-VARI
              MOVE 0 TO WSS-FEC-DAUX IN WSS-VARI.

           MOVE WSS-FEC-FAUX IN WSS-VARI TO GAR-CLV-GMO-1.
           MOVE 'GAR' TO FIO-SIST.
           MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM GAR-FIO-GMO.
           IF NOT FIO-STAT-OKS
               DISPLAY ' ' 
               DISPLAY 'NO ENCONTRE REGISTROS GMO : '
                       WSS-FEC-FPRO IN WSS-VARI
               DISPLAY ' ' 
               GO TO FIN-INP-SRT.
      *>>>>
*% END
*% IF LUP_INP
      *<<<< LUP_INP
           IF (WSS-COD-TFP = '01' AND
                GAR-FEC-MOV IN GMO NOT = WSS-FEC-FPRO IN WSS-VARI )
                  GO TO FIN-INP-SRT.

           IF WSS-COD-TFP = '10' AND
              (GAR-FA-MOV IN GMO NOT = WSS-FEC-APRO IN WSS-VARI OR
               GAR-FM-MOV IN GMO NOT = WSS-FEC-MPRO IN WSS-VARI)
               GO TO FIN-INP-SRT.

*% INCLUDE 'GARSRC:BUSGDGLIS.SRC'

           IF (
              GAR-IND-CPL IN GDG = 'S'
              )
                 NEXT SENTENCE
           ELSE
                 GO TO NXT-INP-SRT.


*% INCLUDE 'GARSRC:BUSCLILIS.SRC'
           IF FIO-STAT-OKS AND DBC-GLS-NOMC IN DBC > SPACES
               MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
               PERFORM CPT-BLKS
               MOVE CPT-STRN TO DBC-GLS-NOMC IN DBC.

      *>>>>
*% END
*% IF RM
      *<<<< RM

           IF GAR-VAL-CPO-ANT IN GMO > ZEROES
               MOVE SPACES TO GMO-GLS-CPO-ANT IN SRT
               MOVE GAR-VAL-CPO-ANT IN GMO TO 
                    GMO-VAL-CPO-ANT IN SRT-RED
           ELSE
               MOVE GAR-GLS-CPO-ANT IN GMO TO 
                    GMO-GLS-CPO-ANT IN SRT.

           IF GAR-VAL-CPO-NVO IN GMO > ZEROES
               MOVE SPACES TO GMO-GLS-CPO-NVO IN SRT
               MOVE GAR-VAL-CPO-NVO IN GMO TO 
                    GMO-VAL-CPO-NVO IN SRT-RED
           ELSE
               MOVE GAR-GLS-CPO-NVO IN GMO TO 
                    GMO-GLS-CPO-NVO IN SRT.
      *>>>>
*% END
*% IF FST_OUT
      *<<<< FST_OUT
           MOVE SPACES TO GMO-COD-CRR-ANT IN WSS-VARI.
      *>>>>
*% END
*% IF LUP_OUT
      *<<<< LUP_OUT
      *BUS-TAB BUSCA GLOSA DE TIPO DE CAMPO DE MOVIMIENTO
           IF GMO-COD-CPO IN SRT > SPACES
                MOVE 'GAR' TO TAB-COD-SIST
                MOVE 'TMS' TO TAB-COD-TTAB IN TAB
                MOVE GMO-COD-CPO IN SRT TO TAB-COD-CTAB IN TAB
                PERFORM BUS-TAB
                IF MSG-COD-MENS = SPACES
                   MOVE TAB-GLS-DCOR IN TAB TO 
                        WSS-GLS-CPO IN WSS-GMO
                ELSE
                   MOVE SPACES TO WSS-GLS-CPO IN WSS-GMO
            ELSE
                MOVE SPACES TO WSS-GLS-CPO IN WSS-GMO.

      *Despliega Hora de Movimiento
           MOVE GMO-HOR-MOV IN SRT TO WSS-HOR-MOV IN WSS-VARI.
           MOVE WSS-HRA-MOV IN WSS-VARI TO WSS-HRA-MOV IN WSS-{RPT}.
           MOVE WSS-MIN-MOV IN WSS-VARI TO WSS-MIN-MOV IN WSS-{RPT}.
           MOVE WSS-SEG-MOV IN WSS-VARI TO WSS-SEG-MOV IN WSS-{RPT}.

           IF GMO-COD-CRR IN SRT = GMO-COD-CRR-ANT IN WSS-VARI
               MOVE SPACES TO GMO-NUM-SIS IN SRT
               MOVE SPACES TO GMO-COD-CRR IN SRT
               MOVE SPACES TO DBC-NUM-ICLI IN SRT
               MOVE SPACES TO DBC-VRF-ICLI IN SRT
               MOVE SPACES TO DBC-IND-ICLI IN SRT
               MOVE SPACES TO DBC-GLS-ICLI IN SRT
               MOVE SPACES TO DBC-GLS-NOMC IN SRT
               MOVE SPACES TO WSS-GLS-TIP-OPE IN WSS-GMO
               MOVE SPACES TO WSS-GLS-STP-OPE IN WSS-GMO
      *         MOVE SPACES TO GMO-FD-MOV IN SRT
      *         MOVE SPACES TO GMO-FM-MOV IN SRT
      *         MOVE SPACES TO GMO-FA-MOV IN SRT
      *         MOVE SPACES TO WSS-GLS-RAYA IN WSS-GMO
           ELSE
               MOVE '-' TO WSS-GLS-RAYA IN WSS-GMO
               PERFORM BUS-TIP-STP
               MOVE GMO-COD-CRR IN SRT TO 
                    GMO-COD-CRR-ANT IN WSS-VARI.

           MOVE ':' TO WSS-GLS-PUNT IN WSS-GMO.
          
      * Acumular Numero de Garantia por Oficina, que se despliega en DL
           ADD 1 TO WSS-CNT-GAR.

      *>>>>
*% END
*% IF FIN_GEN_OUT
      *<<<< FIN_GEN_OUT
      * Acumular Contadores de Garantias
           ADD 1 TO WSS-CNT-OFI-ANT.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
       BUS-TIP-STP SECTION.
       INI-BUS-TIP-STP.
*% INCLUDE 'GARSRC:BUSTIPLIS.SRC'

*% INCLUDE 'GARSRC:BUSSTPLIS.SRC'
       FIN-BUS-TIP-STP.
           EXIT.

      *Entidades de SGC
       COPY SGCBFDBC.

      *Entidades de GAR
       COPY GARBFGDG.
       COPY GARBFGDD.
       COPY GNSBGCPT.

*% IF NOT PGM_VMS
      *Entidades de TAB
       COPY TABBFOFI.
       COPY TABBBOFI.

       COPY GNSBBTAB.
       COPY GNSBBMSG.
      *>>>>
*% END
*% END
