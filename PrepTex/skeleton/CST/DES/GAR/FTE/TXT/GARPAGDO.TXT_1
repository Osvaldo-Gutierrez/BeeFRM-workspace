*% NO_HDR_CST = TRUE
*%! IF PGM_DTC
*%! INCLUDE 'GARSRC:GNSVSIDTC.SRC'
*%! END
*%! HASTA AQUI PARA MANEJAR VSAM EN AMBIENTE DTC
*% FRM = 'GDO'
*% REG = 'GDO'
*% HIJ = 'GDG'
*% CAIREG = 'GAR-NUM-SIS'
*% IICREG = 'GAR-COD-CRR'
*% CAIFLD = 'GDO-NUM-SIS'
*% IICFLD = 'GDO-COD-CRR'
*% SISREG = 'GAR'
*%! Codigo adicional para Funcion GARPPVCG = PF5
*% INCLUDE 'GARSRC:PFALL5.SRC'
*%! Codigo adicional para archivo
*% IF FIN_SCR_VARI
           03 CMA-VARI.
               05 {CAIREG}                     PIC X(04).
               05 {IICREG}                     PIC X(08).
               05 GAR-FEC-SOL-CNS.
                  07 GAR-FA-SOL-CNS            PIC 9(04).
                  07 GAR-FM-SOL-CNS            PIC 9(02).
                  07 GAR-FD-SOL-CNS            PIC 9(02).
*% END
*% IF WSS
       COPY {SISREG}BR{HIJ}.
       COPY {SISREG}BRTAG.
       COPY GARWGVCG.
*% IF PGM_VAX
      *<<<< PGM_VAX
           03 WSS-REG                          PIC X(03).
      *
      *>>>>
*% END
      *
*% IF PGM_DTC
      *<<<< PGM_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GDG-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-TAG-REQA==.
      *>>>>
*% END
*% IF PGM_CHI AND PGM_PQ
      *<<<< PGM_CHI AND PGM_PQ
           COPY USPC113.
      *>>>>
*% END
       01  WSS-STP-OPE                            PIC X(03).
           88 DEPOSITO-A-PLAZO  VALUE '080'.
       01  WSS-VARI.
           03 WSS-CIV-SIV.
              05 WSS-CIV-EMI                      PIC X(03).
              05 WSS-SIV-EMI                      PIC X(03).
      *>>>>
*% END
*% CMA_USR = TRUE
*% IF CMA AND NOT PGM_VAX
           02 FILLER                       PIC X(270).
*% END
*% IF INI_FIO AND NOT PGM_VAX
      *<<<< INI_FIO
           MOVE +270 TO SCR-TCMA.
      *>>>>
*% END
*% IF INI_FIO AND PGM_VAX
      *<<<< INI_FIO

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'TAB'        TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
           MOVE FIO-INP TO FIO-CMND.
           PERFORM GNS-FIO-TAB.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'TAB'        TO FIO-SIST.
           PERFORM TAB-FIO-OFI.
           MOVE FIO-INP TO FIO-CMND.
           PERFORM TAB-FIO-OFI.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'GAR'        TO FIO-SIST.
           PERFORM GAR-FIO-GDG.
           MOVE FIO-UPD TO FIO-CMND.
           PERFORM GAR-FIO-GDG.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'GAR'        TO FIO-SIST.
           PERFORM GAR-FIO-TAG.
           MOVE FIO-UPD TO FIO-CMND.
           PERFORM GAR-FIO-TAG.

      *>>>>
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
*% FIN_BUS_HIJ = TRUE
*% INCLUDE 'GARSRC:INIFNDKE1.SRC'
           MOVE GAR-FEC-SOL-CNS IN GDG TO GAR-FEC-SOL-CNS IN CMA-VARI.
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
*% INCLUDE 'GARSRC:FINFNDKE1.SRC'
      *>>>>
*% END
*% IF INI_CMN_ING
      *<<<< INI_CMN_ING
*% INCLUDE 'GARSRC:LIMFLD.SRC'

      *>>>>
*% END
*% IF CMN_ING OR CMN_MOD
      *<<<< INI_CMN_ING OR INI_CMN_MOD
*% INCLUDE 'GARSRC:STDFLD.SRC'
      *>>>>
*% END
*% IF FIN_CMN_MOD
      *<<<< FIN_CMN_MOD
           IF NOT FIO-STAT-OKS                                                  
      *        Error inconsistencia en la modificacion : informar               
               MOVE FIO-MENS TO FRM-MENS
               GO TO FIN-CMN-MOD.
      *>>>>
*% END
*% IF FIN_CMN_ING
      *<<<< FIN_CMN_ING
*% INCLUDE 'GARSRC:FINCMNING.SRC'
      *>>>>
*% END
*% IF FIN_CMN_ELI
      *<<<< FIN_CMN_ELI
*% INCLUDE 'GARSRC:CMNELIUNO.SRC'

      *>>>>
*% END
*% IF EOF
      *<<<< EOF

       COPY {SISREG}BF{HIJ}.
       COPY {SISREG}BFTAG.

       COPY GARBGVCG.
       COPY GARBKVCG.
      *>>>>
*% END
