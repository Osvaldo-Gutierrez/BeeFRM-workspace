*% REG = 'GDG'
*% RPT = 'VEN'
*% IF WSS
      *<<<< WSS
      *Entidades de GNS
       COPY GNSWGFEC.
       COPY GNSWGHOY.
       COPY GNSWGCPT.
      *Entidades de TAB
       COPY TABBRUSR.
       COPY TABBROFI.
      *Entidades de SGC
       COPY SGCBRDBC.
      *Entidades de GAR
       COPY GARBRGDD.
       COPY GARBRGES.
       COPY GARBRGVT.

       01  WSS-VARI.
           03  WSS-LINEA1              VALUE SPACES PIC X(01).
           03  WSS-LINEA2              VALUE SPACES PIC X(01).
      *FORMATO (DDMMSSAA)
           03  WSS-FEC-VTO.
               05  WSS-FEC-DVTO        VALUE 0      PIC 9(02).
               05  WSS-FEC-MVTO        VALUE 0      PIC 9(02).
               05  WSS-FEC-AVTO        VALUE 0      PIC 9(04).
      *FORMATO (SSAAMMDD)
           03  WSS-FEC-VTO-AUX.
               05  WSS-FEC-AAUX        VALUE 0      PIC 9(04).
               05  WSS-FEC-MAUX        VALUE 0      PIC 9(02).
               05  WSS-FEC-DAUX        VALUE 0      PIC 9(02).
           03  WSS-SW                  VALUE 0      PIC 9(01).
           03  WSS-CNT-STP-ANT         VALUE ZEROES PIC 9(05).
           03  WSS-CNT-TIP-ANT         VALUE ZEROES PIC 9(05).
           03  WSS-CNT-EJE-ORI-ANT     VALUE ZEROES PIC 9(05).
           03  WSS-CNT-OFI-ANT         VALUE ZEROES PIC 9(05).
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
      *Entidades de TAB
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OFI-REQA==.
      *Entidades de GAR
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GDD-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GES-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GVT-REQA==.
      *Entidades de SGC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
      *Entidades de USR
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-USR-REQA==.
      *>>>>
*% END
*% IF CF_{RPT}_GDG_STP_OPE
      *<<<< CF_{RPT}_GDG_STP_OPE
*% IF PGM_VMS
*% INCLUDE 'GARSRC:BUSSTPLIS.SRC'
*% END
               MOVE WSS-CNT-STP-ANT TO WSS-CNT-STP.
               MOVE 0 TO WSS-CNT-STP-ANT.
      *>>>>
*% END
*% IF CF_{RPT}_GDG_TIP_OPE
      *<<<< CF_{RPT}_GDG_TIP_OPE
*% IF PGM_VMS
*% INCLUDE 'GARSRC:BUSTIPLIS.SRC'
*% END
               MOVE WSS-CNT-STP-ANT TO WSS-CNT-STP.
               MOVE 0 TO WSS-CNT-STP-ANT.
               MOVE WSS-CNT-TIP-ANT TO WSS-CNT-TIP.
               MOVE 0 TO WSS-CNT-TIP-ANT.
      *>>>>
*% END
*% IF CF_{RPT}_GDG_COD_EJE_ORI
      *<<<< CF_{RPT}_GDG_COD_EJE_ORI
*% IF PGM_VMS
*% INCLUDE 'GARSRC:BUSEJCLIS.SRC'
*% END
               MOVE WSS-CNT-STP-ANT TO WSS-CNT-STP.
               MOVE 0 TO WSS-CNT-STP-ANT.
               MOVE WSS-CNT-TIP-ANT TO WSS-CNT-TIP.
               MOVE 0 TO WSS-CNT-TIP-ANT.
               MOVE WSS-CNT-EJE-ORI-ANT TO WSS-CNT-EJE-ORI.
               MOVE 0 TO WSS-CNT-EJE-ORI-ANT.
      *>>>>
*% END
*% IF CF_{RPT}_GDG_COD_OFI
      *<<<< CF_{RPT}_GDG_COD_OFI
*% IF PGM_VMS
*% INCLUDE 'GARSRC:BUSOFILIS.SRC'
*% END
               MOVE 1 TO WSS-CNT-GAR.
               MOVE WSS-CNT-STP-ANT TO WSS-CNT-STP.
               MOVE 0 TO WSS-CNT-STP-ANT.
               MOVE WSS-CNT-TIP-ANT TO WSS-CNT-TIP.
               MOVE 0 TO WSS-CNT-TIP-ANT.
               MOVE WSS-CNT-EJE-ORI-ANT TO WSS-CNT-EJE-ORI.
               MOVE 0 TO WSS-CNT-EJE-ORI-ANT.
               MOVE WSS-CNT-OFI-ANT TO WSS-CNT-OFI.
               MOVE 0 TO WSS-CNT-OFI-ANT.

*% IF PGM_VMS
*% FLD = 'GDG-COD-OFI'
*% INCLUDE 'GARSRC:BUSTABVAX.SRC'
*% INCLUDE 'GARSRC:BUSOFIVAX.SRC'
*% INCLUDE 'GARSRC:BUSUSRVAX.SRC'
*% END
      *>>>>
*% END
*% IF FIN_DEC AND PGM_VMS
      *<<<< PGM_VMS
      *<<<< END_DCL
       DEC-PH-VEN SECTION.
           USE BEFORE REPORTING PH-VEN.
       INI-DEC-PH-VEN.
      *<<<< PH_VEN

       PH-STPO.
      *BUS-TAB Busca Glosa de Subtipo Garantia
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'AUX' TO TAB-COD-TTAB IN TAB.
           MOVE SPACES TO TAB-COD-CTAB IN TAB.
           STRING GDG-TIP-OPE IN SRT, GDG-STP-OPE IN SRT
                  DELIMITED BY SIZE INTO TAB-COD-CTAB IN TAB
           PERFORM BUS-TAB.
           IF MSG-COD-MENS = SPACES AND
              GDG-STP-OPE IN SRT > SPACES
                MOVE TAB-GLS-DESC IN TAB TO WSS-GLS-STP-OPE IN WSS-VEN
           ELSE
                MOVE SPACES TO WSS-GLS-STP-OPE IN WSS-VEN.

       PH-TIPO.
      *BUS-TAB Busca Glosa de Tabla Tipo de Operacion
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'TIO' TO TAB-COD-TTAB IN TAB.
           MOVE SPACES TO TAB-COD-CTAB IN TAB.
           MOVE GDG-TIP-OPE IN SRT TO TAB-COD-CTAB IN TAB.
           PERFORM BUS-TAB.
           MOVE TAB-GLS-DESC IN TAB TO WSS-GLS-TIP-OPE IN WSS-VEN.

       PH-EJEC.
      *BUS-TAB Busca Glosa de Ejecutivo
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'TAB' TO TAB-COD-TTAB IN TAB.
           MOVE GDG-COD-EJE-ORI IN SRT TO TAB-COD-TTAB IN TAB.
           MOVE SPACES TO TAB-COD-CTAB IN TAB.
           PERFORM BUS-TAB.
           IF MSG-COD-MENS = SPACES
               MOVE TAB-GLS-DESC IN TAB TO WSS-GLS-EJC IN WSS-VEN
           ELSE
               MOVE SPACES TO WSS-GLS-EJC IN WSS-VEN.

      * Mediante BUS-OFI Obtiene Nombre y Direccion Oficina de Tabla OFI
       PH-OFIC.
      *BUS-OFI Busca Codigo de Tabla
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'OFI' TO TAB-COD-TTAB IN TAB.
           MOVE SPACES TO OFI-GLS-DESC IN OFI.
           MOVE GDG-COD-OFI IN SRT TO OFI-COD-OFIC IN OFI.
           PERFORM BUS-OFI.
           MOVE OFI-GLS-DESC IN OFI TO WSS-GLS-OFI IN WSS-VEN.
      *>>>>
       FIN-DEC-PH-VEN.
           EXIT.
      *>>>>
*% END
*% IF PH_{RPT} AND ( PGM_DOS OR PGM_MVS )
      *<<<< PH_{RPT}
*% INCLUDE 'GARSRC:BUSSTPLIS.SRC'
*% INCLUDE 'GARSRC:BUSTIPLIS.SRC'
*% INCLUDE 'GARSRC:BUSEJCLIS.SRC'
*% INCLUDE 'GARSRC:BUSOFILIS.SRC'
      *>>>>
*% END
*% IF INI
      *<<<< INI

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'TAB'        TO FIO-SIST.
           PERFORM TAB-FIO-OFI.
           MOVE FIO-INP TO FIO-CMND.
           MOVE 'TAB'   TO FIO-SIST.
           PERFORM TAB-FIO-OFI.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'TAB'        TO FIO-SIST.
           PERFORM TAB-FIO-USR.
           MOVE FIO-INP TO FIO-CMND.
           MOVE 'TAB'   TO FIO-SIST.
           PERFORM TAB-FIO-USR.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'TAB'        TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
           MOVE FIO-INP TO FIO-CMND.
           MOVE 'TAB'   TO FIO-SIST.
           PERFORM GNS-FIO-TAB.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'GAR'        TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
           MOVE FIO-INP TO FIO-CMND.
           MOVE 'GAR'   TO FIO-SIST.
           PERFORM GNS-FIO-TAB.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'GAR'        TO FIO-SIST.
           PERFORM GAR-FIO-GDD.
           MOVE FIO-INP TO FIO-CMND.
           MOVE 'GAR'   TO FIO-SIST.
           PERFORM GAR-FIO-GDD.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'GAR'        TO FIO-SIST.
           PERFORM GAR-FIO-GVT.
           MOVE FIO-INP TO FIO-CMND.
           MOVE 'GAR'   TO FIO-SIST.
           PERFORM GAR-FIO-GVT.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'GAR'        TO FIO-SIST.
           PERFORM GAR-FIO-GES.
           MOVE FIO-INP TO FIO-CMND.
           MOVE 'GAR'   TO FIO-SIST.
           PERFORM GAR-FIO-GES.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'SGC'        TO FIO-SIST.
           PERFORM SGC-FIO-DBC.
           MOVE FIO-INP TO FIO-CMND.
           MOVE 'SGC'   TO FIO-SIST.
           PERFORM SGC-FIO-DBC.

           DISPLAY 'FECHA DE VENCIMIENTO (DDMMSSAA) : '.
           ACCEPT WSS-FEC-VTO IN WSS-VARI.
           DISPLAY WSS-FEC-VTO IN WSS-VARI.

*% INCLUDE 'GARSRC:VALFECVTO.SRC'

      * Mueve Fecha de Vencimiento al Reporte
           MOVE WSS-FEC-DVTO IN WSS-VARI TO WSS-FEC-DGDG IN WSS-{RPT}. 
           MOVE WSS-FEC-MVTO IN WSS-VARI TO WSS-FEC-MGDG IN WSS-{RPT}. 
           MOVE WSS-FEC-AVTO IN WSS-VARI TO WSS-FEC-AGDG IN WSS-{RPT}.
      *>>>>
*% END
*% IF FIN_FST_INP
      *<<<< FIN_FST_INP
           IF NOT FIO-STAT-OKS
              DISPLAY 'NO EXISTEN REGISTROS GDG'
              GO TO FIN-INP-SRT.
      *>>>>
*% END
*% IF LUP_INP
      *<<<< LUP_INP
           IF NOT (
              GAR-IND-CPL IN GDG = 'S' AND
              GAR-ES1 IN GDG > '09' AND GAR-ES1 IN GDG < '20' 
              )
                 GO TO NXT-INP-SRT.

           IF GAR-FEC-VTO IN GDG > WSS-FEC-VTO-AUX IN WSS-VARI 
                 GO TO NXT-INP-SRT.

*% GLS_NOMC = TRUE
*% INCLUDE 'GARSRC:BUSCLILIS.SRC'
           IF FIO-STAT-OKS AND DBC-GLS-NOMC IN DBC > SPACES
               MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
               PERFORM CPT-BLKS
               MOVE CPT-STRN TO DBC-GLS-NOMC IN DBC.

      * Busca Registro GES
           MOVE GAR-NUM-SIS IN GDG TO GAR-NUM-SIS IN GES.
           MOVE GAR-COD-CRR IN GDG TO GAR-COD-CRR IN GES.
      *IAS 10/12/92 
           MOVE ZEROES             TO GAR-COD-ESP IN GES.
           MOVE FIO-GET-NLS TO FIO-CMND.
           PERFORM GAR-FIO-GES.
           IF NOT FIO-STAT-OKS OR 
              GAR-NUM-SIS IN GDG NOT = GAR-NUM-SIS IN GES OR
              GAR-COD-CRR IN GDG NOT = GAR-COD-CRR IN GES
      *IAS 10/12/92 
                MOVE SPACES TO GES
                MOVE ZEROES TO GAR-FEC-VTO-CDT IN GES
                               GAR-COD-CRR IN GES
                               GAR-COD-ESP IN GES.

*% INCLUDE 'GARSRC:BUSGVTLIS.SRC'
           IF NOT FIO-STAT-OKS
                MOVE ZEROES TO GAR-FEC-TSN IN GVT
                               GAR-VAL-CML-TSN IN GVT
                               GAR-VAL-LIQ-TSN IN GVT
                               GAR-VAL-PON IN GVT.
      *>>>>
*% END
*% IF LUP_OUT
      *<<<< LUP_OUT
*% LRG = 12
*% INCLUDE 'GARSRC:BUSVLRLIS.SRC'
*% INCLUDE 'GARSRC:BUSLIMLIS.SRC'

      * Acumular Numero de Garantia por Oficina, que se despliega en DL
           ADD 1 TO WSS-CNT-GAR.

      *>>>>
*% END
*% IF FIN_GEN_OUT
      *<<<< FIN_GEN_OUT
      * Acumular Contadores de Garantias
           ADD 1 TO WSS-CNT-STP-ANT.
           ADD 1 TO WSS-CNT-TIP-ANT.
           ADD 1 TO WSS-CNT-EJE-ORI-ANT.
           ADD 1 TO WSS-CNT-OFI-ANT.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
      *Entidades de SGC
       COPY SGCBFDBC.

      *Entidades de GAR
       COPY GARBFGES.
       COPY GARBFGDD.
       COPY GARBFGVT.

      *Entidades de GNS
       COPY GNSBGFEC.
       COPY GNSBGHOY.
       COPY GNSBGCPT.
*% IF NOT PGM_VMS
       COPY GNSBBTAB.
       COPY GNSBBMSG.

      *Entidades de TAB
       COPY TABBFOFI.
       COPY TABBBOFI.
       COPY TABBBUSR.
       COPY TABBFUSR.
      *>>>>
*% END
*% END
