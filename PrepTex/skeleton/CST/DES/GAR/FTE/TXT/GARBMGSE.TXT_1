*% FRM = 'GSE'
*% REG = 'GSE'
*% IF FIN_PUT_GDG_GSE
      *<<<< FIN_PUT_GDG_GSE

           MOVE GAR-NUM-SIS IN GDG TO GAR-NUM-SIS IN GVT.
           MOVE GAR-COD-CRR IN GDG TO GAR-COD-CRR IN GVT.
           MOVE GAR-COD-TSN IN GDG TO GAR-COD-TSN IN GVT.
           MOVE 'GAR-CLV-GVT-1'  TO FIO-AKEY.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GAR-FIO-GVT.
           MOVE FIO-STAT TO PGM-STAT-GVT IN CMA-VARI.
           IF NOT FIO-STAT-OKS
               MOVE ZEROES TO GAR-VAL-LIQ-TSN IN CMA-VARI
               GO TO EXA-VEN-SEG.

           MOVE GAR-VAL-LIQ-TSN IN GVT TO GAR-VAL-LIQ-TSN IN CMA-VARI.

      * Se determina si la tasacion esta vencida o no

*% FIN_EXA_VTO_TAS = 'EXA-VEN-SEG'
*% FEC_FVTA = 'WSS-FEC-FVTA'
*% FEC_DVTA = 'WSS-FEC-DVTA'
*% FEC_MVTA = 'WSS-FEC-MVTA'
*% FEC_AVTA = 'WSS-FEC-AVTA'
*% FEC_SVTA = 'WSS-FEC-SVTA'
*% FEC_FCTA = 'HOY-FHOY'
*% LEE_HOY = TRUE
*% INCLUDE 'GARSRC:EXAVENTSN.SRC'

      * Se determina si el seguro esta vencido o no

*% FIN_EXA_VTO_SEG = 'FIN-EXA-VEN-SEG'
*% LEE_GSE = FALSE
*% PGM_GSE = TRUE
*% FEC_FCSE = 'HOY-FHOY'
*% INCLUDE 'GARSRC:EXAVENSEG.SRC'

      *>>>>
*% END
*% IF GSE_IIV_CRD_SEG
      *<<<< GSE_IIV_CRD_SEG
           IF GSE-COD-RPB-SEG IN GSE-FLD NOT = '3'
               MOVE SPACES TO GSE-IIV-CRD-SEG IN GSE-FLD
               MOVE SPACES TO FRM-GLS-IIV-CRD-SEG IN GSE-FLD
               GO TO FIN-VAL-GSE-IIV-CRD-SEG.

      *>>>>
*% END
*% IF GSE_VAL_SEG
      *<<<< GSE_VAL_SEG

      * Obtiene Codigo Valor de Cambio de Pesos Mediante CIC de la Moneda '0999'
           MOVE 'TAB'          TO FIO-SIST.
           MOVE 'VLR'          TO TAB-CIC-TTAB IN TAB.
           MOVE '0999  '       TO TAB-CIC-CTAB IN TAB.
           MOVE 'TAB-CIC-TABL' TO FIO-AKEY.
           MOVE FIO-GET-KEY    TO FIO-CMND.
           PERFORM GNS-FIO-TAB.
           IF NOT FIO-STAT-OKS
               MOVE 'TAB'          TO MSG-COD-SIST
               MOVE 'CIC0999  NEX' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR
           ELSE
*% IF PGM_ARG
      *<<<< PGM_ARG
               MOVE GSE-VCB-SEG IN GSE-FLD TO WSS-COD-PESO
               IF WSS-IND-TMON NOT = '0'
      *>>>>
*% ELSE
      *<<<< NOT PGM_ARG
               IF GSE-VCB-SEG IN GSE-FLD = TAB-COD-CTAB IN TAB
      *>>>>
*% END
                   MOVE GSE-VAL-SEG IN GSE-FLD TO
                   PES-SGV-DECI IN PES-VARI
                   PERFORM PES-SCTV
                   MOVE PES-SGV-ENTE IN PES-VARI TO
                   GSE-VAL-SEG IN GSE-FLD.
      *>>>>
*% END
*% IF INI_GSE_FEC_VTO_SEG
      *<<<< INI_GSE_FEC_VTO_SEG
           MOVE FEC-CHOY-GE TO FEC-CHOY.
           MOVE FEC-VHBL-N TO FEC-VHBL.

           MOVE GSE-FA-VTO-SEG IN GSE-FLD TO FEC-FANO.
           MOVE GSE-FM-VTO-SEG IN GSE-FLD TO FEC-FMES.
           MOVE GSE-FD-VTO-SEG IN GSE-FLD TO FEC-FDIA.
      *>>>>
*% END
*% IF GSE_FEC_VTO_SEG
      *<<<< GSE_FEC_VTO_SEG
           
           MOVE GSE-FA-VIG-SEG IN GSE-FLD TO FEC-FANO.
           MOVE GSE-FM-VIG-SEG IN GSE-FLD TO FEC-FMES.
           MOVE GSE-FD-VIG-SEG IN GSE-FLD TO FEC-FDIA.
           MOVE FEC-SVLD TO FEC-SEC1.
           MOVE FEC-AVLD TO FEC-AEC1.
           MOVE FEC-MVLD TO FEC-MEC1.
           MOVE FEC-DVLD TO FEC-DEC1.

           MOVE GSE-FA-VTO-SEG IN GSE-FLD TO FEC-FANO.
           MOVE GSE-FM-VTO-SEG IN GSE-FLD TO FEC-FMES.
           MOVE GSE-FD-VTO-SEG IN GSE-FLD TO FEC-FDIA.
           MOVE FEC-SVLD TO FEC-SEC2.
           MOVE FEC-AVLD TO FEC-AEC2.
           MOVE FEC-MVLD TO FEC-MEC2.
           MOVE FEC-DVLD TO FEC-DEC2.

           MOVE FEC-FORM-FEC TO FEC-FORM.
           MOVE FEC-CMP-FEC TO FEC-CMND.
           PERFORM PRO-FEC.
           IF NOT FEC-STAT-LES
               MOVE 'VTO<=VIG' TO MSG-COD-MENS
               GO TO ERR-VAL-FEC-VTO.

           GO TO FIN-VAL-GSE-FEC-VTO-SEG.
       ERR-VAL-FEC-VTO.
           MOVE 'GAR'     TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>>
*% END
*% LBL = ""
*% IF GSE_IND_EDS_SEG
      *<<<< GSE_IND_EDS_SEG
           IF NOT (GSE-IND-EDS-SEG = 'S' OR 'N')
               MOVE 'DEBE SER S o N ' TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>>
*% END
*% IF GSE_IND_PAG_CUO
      *<<<< GSE_IND_PAG_CUO
           IF NOT (GSE-IND-PAG-CUO = 'S' OR 'N')
               MOVE 'DEBE SER S o N ' TO FRM-MENS
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>>
*% END
*% IF VAL_CON_FLD_GSE
      *<<<< VAL_CON_FLD_GSE
           IF NOT PGM-STAT-GVT-OKS
               MOVE 'GSEGVTNEX' TO MSG-COD-MENS
           ELSE
           IF GSE-VCB-SEG IN GSE-FLD = GDG-VCB IN GSE-FLD AND
              GSE-VAL-SEG IN GSE-FLD < GAR-VAL-LIQ-TSN IN CMA-VARI
               MOVE 'GSESEG<LIQ' TO MSG-COD-MENS.
 
      *>>>>
*% END
