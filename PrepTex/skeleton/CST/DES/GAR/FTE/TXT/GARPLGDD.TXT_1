*% REG = 'GDG'
*% RPT = 'GDD'
*% IF WSS
      *<<<< WSS
      *Entidades de GNS
       COPY GNSWGCPT.
      *Entidades de SGC
       COPY SGCBRDBC.
      *Entidades de TAB
       COPY TABBROFI.
      *Entidades de GAR
       COPY GARBRGDD.
       COPY GARBRGVT.

       01  WSS-VARI.
           03  WSS-FEC-FPRO.
               05  WSS-FEC-APRO        VALUE 0      PIC 9(04).
               05  WSS-FEC-MPRO        VALUE 0      PIC 9(02).
               05  WSS-FEC-DPRO        VALUE 0      PIC 9(02).
           03  WSS-LINEA1              VALUE SPACES PIC X(01).
           03  WSS-LINEA2              VALUE SPACES PIC X(01).
           03  WSS-COD-OFIC            VALUE SPACES PIC X(03).
           03  WSS-CNT-OFI-ANT         VALUE ZEROES PIC 9(05).
           03  DBC-NUM-ICLI-ANT        VALUE SPACES PIC X(08).
      *>>>>
*% END
*% IF WSS_DTC
      *<<<< WSS_DTC
      *Entidades de TAB
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-OFI-REQA==.
      *Entidades de GAR
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GDD-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-GVT-REQA==.
      *Entidades de SGC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-DBC-REQA==.
      *>>>>
*% END
*% IF CF_{RPT}_GDG_COD_OFI
      *<<<< CF_{RPT}_GDG_COD_OFI
               MOVE 1 TO WSS-CNT-GAR.
               MOVE WSS-CNT-OFI-ANT TO WSS-CNT-OFI.
               MOVE 0 TO WSS-CNT-OFI-ANT.
      *>>>>
*% END
*% IF FIN_DEC AND PGM_VMS
      *<<<< PGM_VMS
      *<<<< END_DCL
       DEC-PH-GDD SECTION.
           USE BEFORE REPORTING PH-GDD.
       INI-DEC-PH-GDD.
      *<<<< PH_GDD

      * Mediante BUS-OFI Obtiene Nombre y Direccion Oficina de Tabla OFI
       PH-OFIC.
      *BUS-OFI Busca Codigo de Tabla
           MOVE 'TAB' TO TAB-COD-SIST.
           MOVE 'OFI' TO TAB-COD-TTAB IN TAB.
           MOVE SPACES TO OFI-GLS-DESC IN OFI.
           MOVE GDG-COD-OFI IN SRT TO OFI-COD-OFIC IN OFI.
           PERFORM BUS-OFI.
           MOVE OFI-GLS-DESC IN OFI TO WSS-GLS-OFI IN WSS-GDD.
       FIN-DEC-PH-GDD.

*% IF PGM_VMS
*% FLD = 'SALTO'
*% INCLUDE 'GARSRC:BUSTABVAX.SRC'
*% INCLUDE 'GARSRC:BUSOFIVAX.SRC'
*% END
       FIN-CF-{RPT}-{FLD}.
      *>>>> 
*% END
*% IF PH_{RPT} AND ( PGM_MVS OR PGM_DOS )
      *<<<< PH_{RPT}
*% INCLUDE 'GARSRC:BUSOFILIS.SRC'
      *>>>>
*% END
*% IF INI
      *<<<< INI

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'TAB'        TO FIO-SIST.
           PERFORM TAB-FIO-OFI.
           MOVE FIO-INP TO FIO-CMND.
           MOVE 'TAB'   TO FIO-SIST.
           PERFORM TAB-FIO-OFI.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'TAB'        TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
           MOVE FIO-INP TO FIO-CMND.
           MOVE 'TAB'   TO FIO-SIST.
           PERFORM GNS-FIO-TAB.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'GAR'        TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
           MOVE FIO-INP TO FIO-CMND.
           MOVE 'GAR'   TO FIO-SIST.
           PERFORM GNS-FIO-TAB.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'GAR'        TO FIO-SIST.
           PERFORM GAR-FIO-GDD.
           MOVE FIO-INP TO FIO-CMND.
           MOVE 'GAR'   TO FIO-SIST.
           PERFORM GAR-FIO-GDD.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'GAR'        TO FIO-SIST.
           PERFORM GAR-FIO-GVT.
           MOVE FIO-INP TO FIO-CMND.
           MOVE 'GAR'   TO FIO-SIST.
           PERFORM GAR-FIO-GVT.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'SGC'        TO FIO-SIST.
           PERFORM SGC-FIO-DBC.
           MOVE FIO-INP TO FIO-CMND.
           MOVE 'SGC'   TO FIO-SIST.
           PERFORM SGC-FIO-DBC.

           DISPLAY 'INGRESE OFICINA  (*/0XX/999) : '.
           ACCEPT WSS-COD-OFIC IN WSS-VARI.
           DISPLAY WSS-COD-OFIC IN WSS-VARI.

*% IF NOT PGM_VMS
      *<<<<
*% INCLUDE 'GARSRC:VALOFILIS.SRC'
      *>>>>
*% END
*% INCLUDE 'GARSRC:LEEFECPRO.SRC'
      *>>>>
*% END
*% IF LUP_INP
      *<<<< LUP_INP
           IF NOT (
              GAR-IND-CPL IN GDG = 'S' AND
              GAR-ES1 IN GDG > '09' AND GAR-ES1 IN GDG < '20' AND
              (WSS-COD-OFIC = '*' OR WSS-COD-OFIC = GAR-COD-OFI IN GDG
               OR WSS-COD-OFIC = '0XX' AND GAR-COD-OFI IN GDG < '100')
              )
                 GO TO NXT-INP-SRT.

*% INCLUDE 'GARSRC:BUSGDGLIS.SRC'
           IF NOT FIO-STAT-OKS
               MOVE ZEROES TO GAR-FEC-VTO IN GDG.
*% INCLUDE 'GARSRC:BUSCLILIS.SRC'
           IF FIO-STAT-OKS AND DBC-GLS-NOMC IN DBC > SPACES
               MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
               PERFORM CPT-BLKS
               MOVE CPT-STRN TO DBC-GLS-NOMC IN DBC.

*% GDG = TRUE
*%! Utiliza GAR-COD-TSN IN GDG
*% INCLUDE 'GARSRC:BUSGVTLIS.SRC'
           IF NOT FIO-STAT-OKS
               MOVE ZEROES TO GAR-VAL-CML-TSN IN GVT
                              GAR-VAL-LIQ-TSN IN GVT
                              GAR-VAL-AJT-SUP IN GVT.
      *>>>>
*% END
*% IF FST_OUT
      *<<<< FST_OUT
           MOVE SPACES TO DBC-NUM-ICLI-ANT IN WSS-VARI.
      *>>>>
*% END
*% IF LUP_OUT
      *<<<< LUP_OUT

           IF DBC-NUM-ICLI IN SRT = DBC-NUM-ICLI-ANT IN WSS-VARI
               MOVE SPACES TO DBC-NUM-ICLI IN SRT
               MOVE SPACES TO DBC-VRF-ICLI IN SRT
               MOVE SPACES TO DBC-IND-ICLI IN SRT
               MOVE SPACES TO DBC-GLS-ICLI IN SRT
               MOVE SPACES TO DBC-GLS-NOMC IN SRT
               MOVE SPACES TO WSS-GLS-RAYA IN WSS-{RPT}
           ELSE
               MOVE '-' TO WSS-GLS-RAYA IN WSS-{RPT}
               MOVE DBC-NUM-ICLI IN SRT TO 
                    DBC-NUM-ICLI-ANT IN WSS-VARI.

*% LRG = '12'
*% INCLUDE 'GARSRC:BUSVLRLIS.SRC'
*% INCLUDE 'GARSRC:BUSTIPLIS.SRC'
*% GDG = FALSE
*%! Utiliza STP-OPE de SRT
*% INCLUDE 'GARSRC:BUSSTPLIS.SRC'
*% INCLUDE 'GARSRC:BUSLIMLIS.SRC'

      * Acumular Numero de Garantia por Oficina, que se despliega en DL
           ADD 1 TO WSS-CNT-GAR.

      *>>>>
*% END
*% IF FIN_GEN_OUT
      *<<<< FIN_GEN_OUT
      * Acumular Contadores de Garantias
           ADD 1 TO WSS-CNT-OFI-ANT.
      *>>>>
*% END
*% IF EOF
      *<<<< EOF
      *Entidades de GAR
       COPY GARBFGDD.
       COPY GARBFGVT.

      *Entidades de SGC
       COPY SGCBFDBC.

      *Entidades de GNS
       COPY GNSBGCPT.
*% IF NOT PGM_VMS
       COPY GNSBBTAB.
       COPY GNSBBMSG.

      *Entidades de TAB
       COPY TABBFOFI.
       COPY TABBBOFI.
       COPY TABBVOFI.
      *>>>>
*% END
*% END
