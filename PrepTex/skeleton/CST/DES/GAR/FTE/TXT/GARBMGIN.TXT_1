*% FRM = 'GIN'
*% REG = 'GIN'
*% IF GIN_FEC_ULT_INS
      *<<<< GIN_FEC_ULT_INS

           MOVE '01011937' TO FEC-FEC1.

           MOVE GIN-FA-ULT-INS IN {FRM}-FLD TO FEC-FANO IN FEC-VARI.
           MOVE GIN-FM-ULT-INS IN {FRM}-FLD TO FEC-FMES IN FEC-VARI.
           MOVE GIN-FD-ULT-INS IN {FRM}-FLD TO FEC-FDIA IN FEC-VARI.
           MOVE FEC-SVLD TO FEC-SEC2.
           MOVE FEC-AVLD TO FEC-AEC2.
           MOVE FEC-MVLD TO FEC-MEC2.
           MOVE FEC-DVLD TO FEC-DEC2.

           MOVE FEC-FORM-FEC TO FEC-FORM.
           MOVE FEC-CMP-FEC TO FEC-CMND.
           PERFORM PRO-FEC.
           IF FEC-STAT-GRT
               MOVE 'FEC<MIN' TO MSG-COD-MENS
               GO TO ERR-VAL-GIN-FEC-ULT-INS.

           GO TO FIN-VAL-GIN-FEC-ULT-INS.
       ERR-VAL-GIN-FEC-ULT-INS.
           MOVE 'GAR'     TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>>
*% END
*% LBL = ""
*% IF INI_GIN_FEC_PXM_INS
      *<<<< INI_GIN_FEC_PXM_INS
           MOVE FEC-VHBL-N TO FEC-VHBL.
           MOVE FEC-CHOY-G TO FEC-CHOY.
      *>>>>
*% END
*% IF GIN_FEC_PXM_INS
      *<<<< GIN_FEC_PXM_INS

           MOVE '31122020' TO FEC-FEC1.

           MOVE GIN-FA-PXM-INS IN {FRM}-FLD TO FEC-FANO IN FEC-VARI.
           MOVE GIN-FM-PXM-INS IN {FRM}-FLD TO FEC-FMES IN FEC-VARI.
           MOVE GIN-FD-PXM-INS IN {FRM}-FLD TO FEC-FDIA IN FEC-VARI.
           MOVE FEC-SVLD TO FEC-SEC2.
           MOVE FEC-AVLD TO FEC-AEC2.
           MOVE FEC-MVLD TO FEC-MEC2.
           MOVE FEC-DVLD TO FEC-DEC2.

           MOVE FEC-FORM-FEC TO FEC-FORM.
           MOVE FEC-CMP-FEC TO FEC-CMND.
           PERFORM PRO-FEC.
           IF NOT FEC-STAT-GRT
               MOVE 'PXMINS>MAX' TO MSG-COD-MENS
               GO TO ERR-VAL-GIN-FEC-PXM-INS.

           GO TO FIN-VAL-GIN-FEC-PXM-INS.
       ERR-VAL-GIN-FEC-PXM-INS.
           MOVE 'GAR'     TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.
           MOVE FRM-SUAR-MAL TO FRM-SUAR.
      *>>>>
*% END
*% LBL = ""

