*% PGM_GUB = TRUE
*% NO_HDR_CST = TRUE
*%! IF PGM_DTC
*%! INCLUDE 'GARSRC:GNSVSIDTC.SRC'
*%! END
*%! HASTA AQUI PARA MANEJAR VSAM EN AMBIENTE DTC
*% FRM = 'GUB'
*% REG = 'GUB'
*% HIJ = 'GDG'
*% CAIREG = 'GAR-NUM-SIS'
*% IICREG = 'GAR-COD-CRR'
*% CAIFLD = 'GUB-NUM-SIS'
*% IICFLD = 'GUB-COD-CRR'
*% SISREG = 'GAR'
*%! Codigo adicional para Funcion GARPPVCG = PF5
*% INCLUDE 'GARSRC:PFALL5.SRC'
*%! Codigo adicional para archivo
*% IF FIN_SCR_VARI
           03 CMA-VARI.
               05 {CAIREG}                 PIC X(04).
               05 {IICREG}                 PIC X(08).
               05 GAR-IND-OBL-UBI          PIC X(01).
               05 GAR-IND-OBL-ALM          PIC X(01).
               05 GAR-IND-OBL-ADN          PIC X(01).
               05 GAR-LOC-UBI.
                  07 COM-COD-REGI          PIC X(02).
                  07 COM-EXT-CTAB          PIC X(04).
      *>>>>
*% END
*% IF WSS
      *<<<< WSS 
       COPY {SISREG}BR{HIJ}.
       COPY {SISREG}BRTAG.
       COPY GARWGVCG.
*% IF PGM_VAX
      *<<<< PGM_VAX
           03 WSS-REG                          PIC X(03).
      *
      *>>>>
*% END

       COPY TABBRCOM.
       COPY GNSWGHOY.
*% IF PGM_DTC
      *<<<< PGM_DTC
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-{HIJ}-REQA==.
       COPY GNSWGRQA REPLACING ==ADR-REQA== BY ==ADR-TAG-REQA==.
      *>>>>
*% END
*% IF PGM_CHI AND PGM_PQ
      *<<<< PGM_CHI AND PGM_PQ
           COPY USPC113.
      *>>>>
*% END
      *>>>>
*% END
*% CMA_USR = TRUE
*% IF CMA AND PGM_IBM
      *<<<< CMA AND PGM_IBM
           02 FILLER                       PIC X(271).
      *>>>>
*% END
*% IF INI_FIO AND PGM_IBM
      *<<<< INI_FIO
           MOVE +271 TO SCR-TCMA.
      *>>>>
*% END
*% IF INI_FIO AND PGM_VAX
      *<<<< INI_FIO AND PGM_VAX

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'TAB'        TO FIO-SIST.
           PERFORM GNS-FIO-TAB.
           MOVE FIO-INP TO FIO-CMND.
           PERFORM GNS-FIO-TAB.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE 'TAB'        TO FIO-SIST.
           PERFORM TAB-FIO-OFI.
           MOVE FIO-INP TO FIO-CMND.
           PERFORM TAB-FIO-OFI.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE '{SISREG}'        TO FIO-SIST.
           PERFORM {SISREG}-FIO-{HIJ}.
           MOVE FIO-UPD TO FIO-CMND.
           PERFORM {SISREG}-FIO-{HIJ}.

           MOVE FIO-ACCS-UPD TO FIO-ACCS.
           MOVE FIO-ACC      TO FIO-CMND.
           MOVE '{SISREG}'        TO FIO-SIST.
           PERFORM {SISREG}-FIO-TAG.
           MOVE FIO-UPD TO FIO-CMND.
           PERFORM {SISREG}-FIO-TAG.

      *>>>> INI_FIO
*% END
*% IF INI_FND_KEY
      *<<<< INI_FND_KEY
*% FIN_BUS_HIJ = TRUE
*% INCLUDE 'GARSRC:INIFNDKE1.SRC'
      *>>>>
*% END
*% IF FIN_FND_KEY
      *<<<< FIN_FND_KEY
*% INCLUDE 'GARSRC:FINFNDKE1.SRC'
      *>>>>
*% END
*% IF INI_CMN_ING
      *<<<< INI_CMN_ING
*% INCLUDE 'GARSRC:LIMFLD.SRC'
           MOVE SPACES TO {SISREG}-DIR-1 IN {REG}.
           MOVE SPACES TO {SISREG}-DIR-2 IN {REG}.
           MOVE SPACES TO {SISREG}-IIV-AGT-ADN IN {REG}.
           MOVE SPACES TO {SISREG}-IIV-ALM IN {REG}.
           MOVE SPACES TO {SISREG}-COD-CMN IN {REG}.
           MOVE SPACES TO {SISREG}-COD-UBI-DOC IN {REG}.

      *>>>>
*% END
*% IF CMN_ING OR CMN_MOD
      *<<<< INI_CMN_ING OR INI_CMN_MOD
*% INCLUDE 'GARSRC:STDFLD.SRC'
      *>>>>
*% END
*% IF FIN_CMN_MOD
      *<<<< FIN_CMN_MOD
           IF NOT FIO-STAT-OKS                                                  
      *        Error inconsistencia en la modificacion : informar               
               MOVE FIO-MENS TO FRM-MENS
               GO TO FIN-CMN-MOD.
      *>>>>
*% END
*% IF FIN_CMN_ING
      *<<<< FIN_CMN_ING
*% INCLUDE 'GARSRC:FINCMNING.SRC'
      *>>>>
*% END
*% IF FIN_CMN_ELI
      *<<<< FIN_CMN_ELI
           IF NOT FIO-STAT-OKS
      *        Error inconsistencia en la eliminacion : informar
               MOVE FIO-MENS TO FRM-MENS
               GO TO FIN-CMN-ELI.

           MOVE {CAIREG} IN {REG} TO {CAIREG} IN {HIJ}.
           MOVE {IICREG} IN {REG} TO {IICREG} IN {HIJ}.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM {SISREG}-FIO-{HIJ}.
           IF FIO-STAT-OKS
               MOVE ' ' TO GAR-IND-EXI-{REG} IN {HIJ}
               MOVE FIO-MOD TO FIO-CMND
               PERFORM {SISREG}-FIO-{HIJ}.

      *>>>>
*% END
*% IF EOF
      *<<<< EOF

       COPY {SISREG}BF{HIJ}.
       COPY {SISREG}BFTAG.

       COPY GARBGVCG.
       COPY GARBKVCG.

       COPY GNSBGHOY.
      *>>>>
*% END
