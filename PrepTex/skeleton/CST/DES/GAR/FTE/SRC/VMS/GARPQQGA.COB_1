           IF FRM-STAT-ABT
               MOVE SPACES TO FRM-MENS
               PERFORM DESSCRTRL
               GO TO FIN-SCR-PGM.

           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
           IF FIO-STAT = FIO-STAT-NEX
               IF FRM-MENS NOT > SPACES
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
                   MOVE SCR-STAT-ABT TO SCR-STAT
                   PERFORM DESSCRTRL
                   GO TO FIN-SCR-PGM
               ELSE
                   MOVE SCR-STAT-ABT TO SCR-STAT
                   PERFORM DESSCRTRL
                   GO TO FIN-SCR-PGM.
       TRL-SCR-PGM.
           PERFORM DESSCRTRL.
           MOVE SPACES TO FRM-MENS.
           IF SCR-STAT NOT = SCR-STAT-CON
               PERFORM CMN-PGM.
       FIN-SCR-PGM.
           PERFORM SCR-DMSG.

       SCR-DMSG SECTION.
       INI-SCR-DMSG.
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               PERFORM SCR-DSP-MSSG
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               DISPLAY PGM-NCUR WITH NO ADVANCING
               PERFORM SCR-CLRA-QGA
               PERFORM SCR-DSP-MSSG
               DISPLAY PGM-SCUR WITH NO ADVANCING
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-QGA
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
           ELSE
               PERFORM CLR-FLD-QGA
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA.
       FIN-SCR-DMSG.
           EXIT.

       SCR-KEY SECTION.
       INI-SCR-KEY.
           PERFORM GET-KEY.
           IF FRM-STAT-ABT
               MOVE SPACES TO FRM-MENS
               PERFORM SCR-MSSG
               GO TO FIN-SCR-KEY.
       FND-SCR-KEY.
           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
           IF FIO-STAT-OKS
               PERFORM PUT-SCR
               MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
               GO TO FIN-SCR-KEY.
       ERR-SCR-KEY.
           MOVE PGM-NO-LIMPIAR-NADA TO PGM-ICLR.
           IF FRM-MENS NOT > SPACES
               IF FIO-STAT-EOF
                   MOVE 'REGEOF    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
               IF FIO-STAT = FIO-STAT-NEX
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
           MOVE SCR-STAT-ABT TO SCR-STAT.
        FIN-SCR-KEY.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           INITIALIZE           QGA-DFLD.
           MOVE PGM-SRNF TO PGM-STAT-GDG.
           MOVE PGM-SRNF TO PGM-STAT-DBC.
           MOVE PGM-SRNF TO PGM-STAT-GVT.
           MOVE PGM-SRNF TO PGM-STAT-GSE.
           MOVE PGM-SRNF TO PGM-STAT-GDO.
           MOVE PGM-SRNF TO PGM-STAT-GLE.
           MOVE PGM-SRNF TO PGM-STAT-GUB.
       FIN-SCR-RST.
           EXIT.

       GET-KEY SECTION.
       INI-GET-KEY.
           PERFORM SCR-GKEY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-KEY.
           EXIT.

       GET-IKY SECTION.
       INI-GET-IKY.
           PERFORM SCR-GIKY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-IKY.
           EXIT.

       FND-KEY SECTION.
       INI-FND-KEY.
      *<<<< BUSNXTPRV
           MOVE SPACES TO MSG-COD-MENS.
           MOVE 0 TO WSS-IBUS.
           MOVE 0 TO WSS-IBUS-GDG.
           MOVE '23' TO PGM-STAT-GDD.
           IF FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM LEE-NXT-PRV-XGDD
               IF WSS-IBUS = 2
                   PERFORM SCR-CLRA-QGA
                   MOVE '00' TO PGM-STAT-GDG
                   MOVE '00' TO PGM-STAT-GDD
                   MOVE '00' TO PGM-STAT-DBC
                   GO TO EXA-COM-STAT
               ELSE
                   IF FRM-FFLD = FRM-FFLD-PF5
                       MOVE 'GDD    NOPRV' TO MSG-COD-MENS
                       GO TO ERR-FND-KEY
                   ELSE
                       MOVE 'GDD    NONXT' TO MSG-COD-MENS
                       GO TO ERR-FND-KEY.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8
               PERFORM LEE-NXT-PRV-XDBC
               IF WSS-IBUS = 1
                   GO TO CON-BUS-DBC
               ELSE
                   IF FRM-FFLD = FRM-FFLD-PF7
                       MOVE 'DBC    NOPRV' TO MSG-COD-MENS
                       GO TO ERR-FND-KEY
                   ELSE
                       MOVE 'DBC    NONXT' TO MSG-COD-MENS
                       GO TO ERR-FND-KEY.
      *>>>>

      *<<<< FNDIRECTO
           IF GDG-COD-CRR IN QGA-FLD > ZEROES 
      *        se digito lo necesario para buscar directo
               PERFORM FND-DIRECTO
               IF FIO-STAT-OKS
                   MOVE 2 TO WSS-IBUS
                   MOVE '00' TO PGM-STAT-GDG
      *            para buscar RPH y PAD
                   PERFORM GET-CUR-GDD
                   IF FIO-STAT-OKS
                       MOVE '00' TO PGM-STAT-GDD
                       MOVE GDD-CIC-ICLI IN GDD TO
                            DBC-KEY-IDBC IN DBC
                        MOVE 'DBC-KEY-IDBC' TO FIO-AKEY
                        PERFORM SGC-FIO-DBC
                        MOVE FIO-STAT TO PGM-STAT-DBC
                        IF FIO-STAT-OKS
                            GO TO EXA-COM-STAT
                        ELSE
                            MOVE SPACES TO DBC-IDC-ICLI IN CMA-VARI
                            MOVE SPACES TO DBC-GLS-NOMC IN CMA-VARI

                            MOVE 'GDD    NODBC' TO MSG-COD-MENS
                            GO TO EXA-COM-STAT 
                   ELSE
      * Limpia variables common-area, para manejo Ant/Sig
                       MOVE SPACES TO DBC-KEY-IDBC IN CMA-VARI
                       MOVE SPACES TO DBC-GLS-NOMC IN CMA-VARI
                       MOVE SPACES TO DBC-IDC-ICLI IN CMA-VARI
                       MOVE 'GDD    NEX' TO MSG-COD-MENS
                       GO TO EXA-COM-STAT
               ELSE
                   MOVE SPACES TO DBC-GLS-NOMC IN CMA-VARI
                   MOVE SPACES TO DBC-IDC-ICLI IN CMA-VARI
                   MOVE SPACES TO DBC-KEY-IDBC IN CMA-VARI
                   MOVE 'GDG    NEX' TO MSG-COD-MENS
                   GO TO ERR-FND-KEY.

      *>>>>

      *<<<< BUSPADHIJ

      *    no se digito el campo llave del hijo
      *    Solo se digito llave del padre
      *    busca el padre por alguna de las llaves (la digitada)
           IF DBC-NUM-ICLI IN QGA-FLD > ZEROES
      *IDC-CLI Busca Cliente por medio de IdC
               MOVE DBC-IDC-ICLI IN DBC TO DBC-IDC-ICLI IN CMA-VARI
               MOVE SPACES TO DBC-GLS-NOMC IN CMA-VARI
               MOVE DBC-NUM-ICLI IN QGA-FLD TO DBC-NUM-ICLI IN DBC
               MOVE DBC-IND-ICLI IN QGA-FLD TO DBC-IND-ICLI IN DBC
               MOVE DBC-GLS-ICLI IN QGA-FLD TO DBC-GLS-ICLI IN DBC
               MOVE 'DBC-IDC-ICLI'  TO FIO-AKEY
               MOVE 'IDC' TO SCR-KEY-IDBC
               PERFORM GET-CLI
               IF MSG-COD-MENS NOT > SPACES
                   MOVE 1 TO WSS-IBUS
                   GO TO CON-BUS-DBC.
           IF DBC-GLS-NOMC IN QGA-FLD > SPACES
      *NOM-CLI Busca Cliente por medio del Nombre
               MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN CMA-VARI
               MOVE SPACES TO DBC-IDC-ICLI IN CMA-VARI
               MOVE DBC-GLS-NOMC IN QGA-FLD TO STR-GLS-GSTR
               PERFORM STR-UNS
               MOVE STR-GLS-FUNS IN STR-VARI(1) TO 
                    DBC-GLS-APAT IN DBC
               MOVE STR-GLS-FUNS IN STR-VARI(2) TO 
                    DBC-GLS-AMAT IN DBC
               MOVE STR-GLS-FUNS IN STR-VARI(3) TO 
                    DBC-GLS-NOMB IN DBC
               MOVE 'DBC-GLS-NOMC'   TO FIO-AKEY
               MOVE FIO-GET-NLS-ALT TO FIO-CMND
               MOVE 'NOM' TO SCR-KEY-IDBC
               PERFORM GET-CLI
               IF MSG-COD-MENS = SPACES
                   MOVE 1 TO WSS-IBUS.

      * SI SE PROCESA SUBENTIDAD, LA BUSQUEDA POR LLAVE ALTERNATIVA
      *  DEBE SER UN PARRAFO CON LOOP

       CON-BUS-DBC.
           IF WSS-IBUS = 0
               MOVE 'DBC    NEX' TO MSG-COD-MENS
               GO TO ERR-FND-KEY.

      *    se encontro el padre y busca el GDG
           MOVE '00' TO PGM-STAT-DBC.

           MOVE DBC-KEY-IDBC IN DBC TO DBC-KEY-IDBC IN CMA-VARI.
           PERFORM BUS-ULT-GDG.
           IF WSS-IBUS = 2
               MOVE '00' TO PGM-STAT-GDD
               MOVE '00' TO PGM-STAT-GDG
      *        se encontro el hijo
               GO TO EXA-COM-STAT.

      * Para permitir Ant/Sig aunque padre no tenga operaciones
           PERFORM ESC-CMA-VARI.

           MOVE 'GDD    NEX' TO MSG-COD-MENS.
           GO TO ERR-FND-KEY.

       FND-DIRECTO.
           MOVE 1 TO WSS-IBUS-GDG.

      *>>>>

           PERFORM KEY-ALL-QGA.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GAR-FIO-GDG.
           MOVE FIO-STAT TO PGM-STAT-GDG.
      *<<<< FINFNDKEY

       EXA-COM-STAT.
           IF PGM-STAT-GDD NOT = '00'
      *       GDG incompleto, sin padre
               GO TO ANL-GDG.
       ESC-CMA-VARI.
      * graba variables common-area para manejo Ant/Sig   
           IF NOT (FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6)
               MOVE DBC-KEY-IDBC IN DBC TO DBC-KEY-IDBC IN CMA-VARI
               MOVE DBC-GLS-NOMC IN DBC TO CPT-STRN
               PERFORM CPT-BLKS
      *        Mantiene la forma inicial de buscar Ant/Sig
               IF DBC-IDC-ICLI IN CMA-VARI > SPACES
                   MOVE DBC-IDC-ICLI IN DBC TO DBC-IDC-ICLI IN CMA-VARI
                   MOVE CPT-STRN TO DBC-GLS-NOMC IN DBC
               ELSE
                   MOVE DBC-GLS-NOMC IN DBC TO DBC-GLS-NOMC IN CMA-VARI
                   MOVE CPT-STRN TO DBC-GLS-NOMC IN DBC.
           MOVE GDD-CIC-ICLI IN GDD TO GDD-CIC-ICLI IN CMA-VARI.
           MOVE GAR-CLV-GDD-2 IN GDD TO GAR-CLV-GDD-2 IN CMA-VARI.
       FIN-ESC-CMA-VARI.

      * DETERMINA SI TIENE MULTIPLES PADRES
           MOVE SPACES TO MSG-COD-SIST.
           MOVE SPACES TO MSG-COD-MENS.

           IF WSS-IBUS-GDG = 0
               MOVE 'GDD-KEY-PRV' TO FIO-AKEY
               MOVE FIO-GET-PRV TO FIO-CMND
           ELSE
               MOVE 'GAR-CLV-GDD-2' TO FIO-AKEY
               MOVE FIO-GET-NXT TO FIO-CMND.

           PERFORM GAR-FIO-GDD.

           IF FIO-STAT-OKS AND
              GAR-CLV-GDG-1  IN GDG = GAR-CLV-GDD-2  IN GDD
                MOVE 'M' TO SCR-IND-MGDD
                MOVE 'GAR'          TO MSG-COD-SIST
                MOVE 'GDG    MDDIR' TO MSG-COD-MENS
                PERFORM GET-MSG
                MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
                MOVE SPACES TO SCR-IND-MGDD
                MOVE 'GAR-CLV-GDD-2' TO FIO-AKEY
                MOVE FIO-END-BRW TO FIO-CMND
                PERFORM GAR-FIO-GDD
                PERFORM GET-CUR-GDD.

       ANL-GDG.
       FIN-ANL-GDG.
           MOVE PGM-STAT-GDG TO FIO-STAT. 

      *>>>>

           MOVE GAR-NUM-SIS IN GDG TO GAR-NUM-SIS IN GVT.
           MOVE GAR-COD-CRR IN GDG TO GAR-COD-CRR IN GVT.
           MOVE GAR-COD-TSN IN GDG TO GAR-COD-TSN IN GVT.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GAR-FIO-GVT.
           IF FIO-STAT-OKS
               MOVE '00' TO PGM-STAT-GVT.
           MOVE GAR-NUM-SIS IN GDG TO GAR-NUM-SIS IN GDO.
           MOVE GAR-COD-CRR IN GDG TO GAR-COD-CRR IN GDO.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GAR-FIO-GDO.
           IF FIO-STAT-OKS
               MOVE '00' TO PGM-STAT-GDO.
           MOVE GAR-NUM-SIS IN GDG TO GAR-NUM-SIS IN GLE.
           MOVE GAR-COD-CRR IN GDG TO GAR-COD-CRR IN GLE.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GAR-FIO-GLE.
           IF FIO-STAT-OKS
               MOVE '00' TO PGM-STAT-GLE.
           MOVE GAR-NUM-SIS IN GDG TO GAR-NUM-SIS IN GSE.
           MOVE GAR-COD-CRR IN GDG TO GAR-COD-CRR IN GSE.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GAR-FIO-GSE.
           IF FIO-STAT-OKS
               MOVE '00' TO PGM-STAT-GSE.
           MOVE GAR-NUM-SIS IN GDG TO GAR-NUM-SIS IN GUB.
           MOVE GAR-COD-CRR IN GDG TO GAR-COD-CRR IN GUB.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GAR-FIO-GUB.
           IF FIO-STAT-OKS
               MOVE '00' TO PGM-STAT-GUB.
           MOVE PGM-STAT-GDG TO FIO-STAT.
           GO TO FIN-FND-KEY.
      *<<<< ERRFNDKEY
       ERR-FND-KEY.
           IF WSS-IBUS = 0 AND WSS-IBUS-GDG = 0
      *        busco por padre y no encontro padre
               MOVE SPACES TO PGM-STAT
               MOVE 'SGC' TO MSG-COD-SIST
               MOVE FIO-STAT-NEX TO PGM-STAT-DBC
           ELSE
      *        todos otros casos: el error pertenece al sistema GAR
               MOVE 'GAR' TO MSG-COD-SIST.
           MOVE '23' TO PGM-STAT-GDG.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO FRM-MENS.

           IF PGM-STAT-DBC-OKS
      *        Despliega solo datos del DBC

               MOVE SCR-STAT-CON TO SCR-STAT
               MOVE SCR-INST-GAB TO SCR-INST
               DISPLAY PGM-NCUR WITH NO ADVANCING
               PERFORM SCR-CLRA-QGA
               DISPLAY PGM-SCUR WITH NO ADVANCING
               INITIALIZE QGA-DFLD
               PERFORM PUT-DBC-QGA
               PERFORM SCR-DSP-MSSG
               MOVE '00' TO FIO-STAT
           ELSE
               MOVE '23' TO FIO-STAT
               MOVE SCR-STAT-ABT TO SCR-STAT.

           GO TO FIN-FND-KEY.

      *>>>>

       FIN-FND-KEY.
           EXIT.

       PUT-SCR SECTION.
       INI-PUT-SCR.
           PERFORM PUT-ALL-QGA.
       FIN-PUT-SCR.
           EXIT.

       GET-FLD SECTION.
       INI-GET-FLD.
           PERFORM SCR-GFLD.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-FLD.
           EXIT.

       CMN-MOD SECTION.
       INI-CMN-MOD.
           PERFORM KEY-ALL-QGA.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM GAR-FIO-GDG.
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-MOD.
           PERFORM RET-ALL-QGA.
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM GAR-FIO-GDG.
       FIN-CMN-MOD.
           EXIT.

       CMN-ING SECTION.
       INI-CMN-ING.
           INITIALIZE GDG.
           PERFORM RET-ALL-QGA.
           MOVE FIO-PUT TO FIO-CMND.
           PERFORM GAR-FIO-GDG.
       FIN-CMN-ING.
           EXIT.

       CMN-ACC SECTION.
       INI-CMN-ACC.
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'ACCION NO REALIZADA' TO FRM-MENS
               GO TO FIN-CMN-ACC.
           PERFORM RET-ALL-QGA.
       FIN-CMN-ACC.
           EXIT.

       CMN-ELI SECTION.
       INI-CMN-ELI.
           PERFORM KEY-ALL-QGA.
           MOVE FIO-DEL TO FIO-CMND.
           PERFORM GAR-FIO-GDG.
       FIN-CMN-ELI.
           EXIT.

       CMN-PGM SECTION.
       INI-CMN-PGM.
           PERFORM RET-ALL-QGA.
       FIN-CMN-PGM.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE SPACES TO FRM-MENS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
           DISPLAY PGM-NCUR WITH NO ADVANCING.
           MOVE QGA-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE QGA-GLS-MENS TO FRM-MENS.
           MOVE FRM-MSG TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           DISPLAY PGM-SCUR WITH NO ADVANCING.
       FIN-SCR-PUTA.
           EXIT.

       SCR-DSP-MSSG SECTION.
       INI-SCR-DSP-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO QGA-GLS-MENS.
           MOVE FRM-MSG TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-DSP-MSSG.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO QGA-GLS-MENS.
      *    MOVE FRM-MSG TO FRM-CMND.
      *    PERFORM GNS-PRO-FRM.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GKEY SECTION.
       INI-SCR-GKEY.
           MOVE QGA-NFLD       TO FRM-VNOM.
           MOVE 1 TO FRM-IFLD.
       LUP-SCR-GKEY.
           MOVE QGA-DFLD-ATR   TO FRM-VATR.
           MOVE QGA-DFLD-LEN   TO FRM-VLEN.
           MOVE FRM-GET-MAP      TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
      *<<<< PFS_SCR_GKEY
           IF NOT
              ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD = FRM-FFLD-PF6 OR
                FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD = FRM-FFLD-PF8 )
      *>>>>
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-QGA
               MOVE FRM-MSG TO FRM-CMND
               PERFORM GNS-PRO-FRM
               MOVE SPACES TO FRM-MENS
               GO TO LUP-SCR-GKEY.
           MOVE FRM-DFLD TO QGA-DFLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GKEY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           INITIALIZE QGA-DFLD-LEN.
           PERFORM VAL-NUM-KEY-QGA.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-CON-KEY-QGA.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               PERFORM FST-KEY-QGA
               GO TO ERR-SCR-GKEY.
      *JSS
           PERFORM VAL-IKY-QGA.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-MKY-QGA.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
           PERFORM VAL-AKY-QGA.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GKEY.
       ERR-SCR-GKEY.
           MOVE FRM-MSG TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           GO TO LUP-SCR-GKEY.
       FIN-SCR-GKEY.
           PERFORM SCR-MSSG.

       SCR-GIKY SECTION.
       INI-SCR-GIKY.
           MOVE QGA-NFLD       TO FRM-VNOM.
           MOVE 1 TO FRM-IFLD.
       LUP-SCR-GIKY.
           MOVE QGA-DFLD-ATR   TO FRM-VATR.
           MOVE QGA-DFLD-LEN   TO FRM-VLEN.
           MOVE FRM-GET-MAP      TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-QGA
               MOVE FRM-MSG TO FRM-CMND
               PERFORM GNS-PRO-FRM
               MOVE SPACES TO FRM-MENS
               GO TO LUP-SCR-GIKY.
           MOVE FRM-DFLD TO QGA-DFLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GIKY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           INITIALIZE QGA-DFLD-LEN.
           PERFORM VAL-NUM-IKY-QGA.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GIKY.
           PERFORM VAL-CON-IKY-QGA.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               PERFORM FST-KEY-QGA
               GO TO ERR-SCR-GIKY.
      *JSS
           PERFORM VAL-IKY-QGA.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GIKY.
       ERR-SCR-GIKY.
           MOVE FRM-MSG TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           GO TO LUP-SCR-GIKY.
       FIN-SCR-GIKY.
           PERFORM SCR-MSSG.

       SCR-GFLD SECTION.
       INI-SCR-GFLD.
           MOVE QGA-NFLD       TO FRM-VNOM.
           MOVE 1 TO FRM-IFLD.
       LUP-SCR-GFLD.
           MOVE QGA-DFLD-ATR   TO FRM-VATR.
           MOVE QGA-DFLD-LEN   TO FRM-VLEN.
           MOVE FRM-GET-MAP      TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
      *<<<< PFS_SCR_GFLD
           IF NOT ( ( SCR-CCPP = 'MOD' OR  SCR-CCPP = 'ACC' OR
                      SCR-CCPP = 'ELI'                      OR
                    ( SCR-CCPP = 'ACT' AND SCR-ACTU = SCR-ACTU-MOD ) )
                      AND ( FRM-FFLD = FRM-FFLD-PF8 OR
                            FRM-FFLD = FRM-FFLD-PF7 OR
                            FRM-FFLD = FRM-FFLD-PF5 OR
                            FRM-FFLD = FRM-FFLD-PF6 ) )
      *>>>>
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-FLD-QGA
               MOVE FRM-MSG TO FRM-CMND
               PERFORM GNS-PRO-FRM
               MOVE SPACES TO FRM-MENS
               GO TO LUP-SCR-GFLD.
           MOVE FRM-DFLD TO QGA-DFLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GFLD.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           INITIALIZE QGA-DFLD-LEN.
           PERFORM VAL-NUM-KEY-QGA.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GFLD.
           PERFORM VAL-NUM-FLD-QGA.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GFLD.
           PERFORM VAL-MKY-QGA.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GFLD.
           PERFORM VAL-FLD-QGA.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GFLD.
           PERFORM VAL-CON-FLD-QGA.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GFLD.
           PERFORM FST-FLD-QGA.
       ERR-SCR-GFLD.
           PERFORM SCR-MSSG.
           PERFORM SCR-PUTA.
           GO TO LUP-SCR-GFLD.
       FIN-SCR-GFLD.
           MOVE SCR-STAT-MOD TO SCR-STAT.

       GET-ACC SECTION.
       INI-GET-ACC.
      *    MOVE FDV-WAIT TO FRM-CMND.
           MOVE 0 TO FRM-PLIN.
      *    MOVE FRM-MENS TO FRM-PRMT.
           MOVE QGA-GLS-MENS TO FRM-PRMT.
           MOVE FDV-GETDL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
      *<<<< PFS_GET_CON OR PFS_GET_ACC
           IF NOT
              ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD = FRM-FFLD-PF6 OR
                FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD = FRM-FFLD-PF8 )
      *>>>>
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC          TO QGA-GLS-MEN2
               MOVE SCR-GLS-PRMT( SCR-I ) TO QGA-GLS-MEN1
               MOVE QGA-GLS-MENS TO FRM-MENS
               GO TO INI-GET-ACC.
           MOVE FRM-VFLD TO SCR-VFLD.
           MOVE FRM-VFLD TO SCR-VFLD.
           IF FRM-FFLD = FRM-FFLD-RET
               IF SCR-VFLD = SCR-VFLD-NOT
                   MOVE FRM-FFLD-PF12 TO FRM-FFLD
               ELSE
                   PERFORM VAL-NUM-KEY-QGA
                   PERFORM VAL-NUM-FLD-QGA
           ELSE
               IF SCR-VFLD = SCR-VFLD-YES
                   MOVE FRM-FFLD-PF10 TO FRM-FFLD
                   PERFORM VAL-NUM-KEY-QGA
                   PERFORM VAL-NUM-FLD-QGA.
           DISPLAY '' LINE 24 COLUMN 1 WITH NO ADVANCING.
       FIN-GET-ACC.
           MOVE SCR-STAT-SON TO SCR-STAT.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE FDV-WAIT TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET OR FRM-FFLD-PF3 )
      *<<<< PFS_GET_CON OR PFS_GET_ACC
           IF NOT
              ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD = FRM-FFLD-PF6 OR
                FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD = FRM-FFLD-PF8 )
      *>>>>
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC          TO QGA-GLS-MEN2
               MOVE SCR-GLS-PRMT( SCR-I ) TO QGA-GLS-MEN1
               MOVE FRM-MSG TO FRM-CMND
               PERFORM GNS-PRO-FRM
               MOVE SPACES TO FRM-MENS
               GO TO INI-GET-CON.
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO QGA-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
           PERFORM PRO-FLD-QGA.
           PERFORM PRO-MKY-QGA.
           PERFORM PRO-AKY-QGA.
           PERFORM UNP-IKY-QGA.
           PERFORM FST-KEY-QGA.
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
           PERFORM PRO-FLD-QGA.
           PERFORM UNP-KEY-QGA.
           PERFORM FST-KEY-QGA.
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
           PERFORM PRO-IKY-QGA.
           PERFORM PRO-AKY-QGA.
           PERFORM UNP-MKY-QGA.
           PERFORM UNP-FLD-QGA.
           PERFORM FST-FLD-QGA.
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO QGA-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
           PERFORM PRO-KEY-QGA.
           PERFORM PRO-FLD-QGA.
           PERFORM FST-MSG.
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-GLS-PRMT( SCR-I ) TO FRM-MEN1.
           IF SCR-STAT = SCR-STAT-MOD
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               IF FRM-FFLD NOT = FRM-FFLD-BSP
                   PERFORM SCR-PUTA
                   PERFORM SCR-EXIT
                   GO TO FIN-SCR-TRL
               ELSE
                   MOVE FRM-MSG TO FRM-CMND
                   PERFORM GNS-PRO-FRM
                   PERFORM SCR-EXIT
                   GO TO FIN-SCR-TRL.
           IF SCR-STAT = SCR-STAT-SON
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               IF FRM-FFLD NOT = FRM-FFLD-BSP
                   PERFORM SCR-PUTA
                   PERFORM SCR-EXIT
                   GO TO FIN-SCR-TRL
               ELSE
                   MOVE FRM-MSG TO FRM-CMND
                   PERFORM GNS-PRO-FRM
                   PERFORM SCR-EXIT
                   GO TO FIN-SCR-TRL.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           IF FRM-FFLD NOT = FRM-FFLD-BSP
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT
           ELSE
               MOVE FRM-MSG TO FRM-CMND
               PERFORM GNS-PRO-FRM
               PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           IF SCR-INST = SCR-INST-GCF
                  PERFORM SCR-GCNF
                  GO TO FIN-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
           IF SCR-STAT = SCR-STAT-ABT OR SCR-STAT-CON
                  GO TO FIN-DESSCRTRL.
           IF SCR-FFLD-RET OR SCR-FFLD-PF10
                  MOVE SCR-VFLD-YES TO SCR-VFLD
                  PERFORM PRO-CONF
                  IF SCR-INST = SCR-INST-GCF
                      GO TO INI-DESSCRTRL.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

       PRO-CONF SECTION.
       INI-PRO-CONF.
           IF FRM-MENS NOT > SPACES
               IF SCR-GLS-CONF( SCR-I ) NOT > SPACES
                   GO TO FIN-PRO-CONF
               ELSE
                   MOVE SCR-GLS-CONF( SCR-I ) TO FRM-MENS
           ELSE
               IF FRM-MEN1 NOT > SPACES
                   MOVE SCR-GLS-CONF( SCR-I ) TO FRM-MEN1.
           MOVE SCR-INST-GCF TO SCR-INST.
           PERFORM SCR-MSSG.
           PERFORM PRO-KEY-QGA.
           PERFORM PRO-FLD-QGA.
           PERFORM FST-MSG.
      *JSS PERFORM SCR-PUTA.
           DISPLAY PGM-NCUR WITH NO ADVANCING.
           MOVE QGA-FLD TO FRM-DFLD.
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           DISPLAY PGM-SCUR WITH NO ADVANCING.
       FIN-PRO-CONF.
           EXIT.

       SCR-GCNF SECTION.
       INI-SCR-GCNF.
      *    MOVE FDV-WAIT TO FRM-CMND.
           MOVE 0 TO FRM-PLIN.
           MOVE FRM-MENS  TO FRM-PRMT.
           MOVE FDV-GETDL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET OR FRM-FFLD-PF4 OR
                               FRM-FFLD-BSP )
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC          TO QGA-GLS-MEN2
               MOVE SCR-GLS-CONF( SCR-I ) TO QGA-GLS-MEN1
               MOVE QGA-GLS-MENS TO FRM-MENS
               GO TO INI-SCR-GCNF.
           MOVE FRM-VFLD TO SCR-VFLD.
           IF NOT ( SCR-VFLD = SCR-VFLD-YES OR SCR-VFLD-NOT OR SPACES )
               MOVE 'CNF    INV' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               STRING MSG-GLS-DCOR SCR-GLS-CONF( SCR-I )
                      DELIMITED BY SIZE INTO QGA-GLS-MENS
               MOVE QGA-GLS-MENS TO FRM-MENS
               GO TO INI-SCR-GCNF.
           IF FRM-FFLD = FRM-FFLD-RET
               IF SCR-VFLD = SCR-VFLD-NOT
                   MOVE FRM-FFLD-PF12 TO FRM-FFLD
               ELSE
                   MOVE SCR-VFLD-YES TO SCR-VFLD
                   PERFORM VAL-NUM-KEY-QGA
                   PERFORM VAL-NUM-FLD-QGA
           ELSE
           IF FRM-FFLD = FRM-FFLD-PF4
               IF SCR-VFLD = SCR-VFLD-YES
                   MOVE FRM-FFLD-PF10 TO FRM-FFLD
                   PERFORM VAL-NUM-KEY-QGA
                   PERFORM VAL-NUM-FLD-QGA
               ELSE
                   MOVE SCR-VFLD-NOT TO SCR-VFLD
           ELSE
               MOVE SCR-STPP TO SCR-STAT
               IF SCR-STAT NOT = SCR-STAT-MOD
                   MOVE FRM-FFLD-RET TO FRM-FFLD
                   MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
           MOVE SPACES TO FRM-MENS.
           DISPLAY '' LINE 24 COLUMN 1 WITH NO ADVANCING.
       FIN-SCR-GCNF.
           EXIT.

       COPY GARBMQGA.
       COPY GARBFGDG.
       COPY GNSBFMSG.
       COPY GNSBGSYS.
       COPY GNSBGFRM.
       COPY GNSBGMSG.
       COPY GNSBEKEY.
       COPY GNSBIABT.
       COPY GNSBBTAB.
       COPY GNSBVCOD.
       COPY GNSBVTAB.
       COPY GNSBFTAB.
       COPY GNSBVIND.
       COPY GNSBVMSC.
       COPY GNSBFMSC.
       COPY GNSBVIDC.
       COPY GNSBPVRF.
       COPY GNSBEGLS.
       COPY TABBBOFI.
       COPY TABBFOFI.
       COPY TABBBUSR.
       COPY TABBFUSR.

      *<<<< EOF
       RST-STAT SECTION.
       INI-RST-STAT.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
       FIN-RST-STAT.
           EXIT.

       GNS-CLR-RQA SECTION.
       INI-GNS-CLR-RQA.
       FIN-GNS-CLR-RQA.
           EXIT.
      *>>>>
      *<<<< EOFPQ
      *<<<< EOF
       LEE-NXT-PRV-XGDD SECTION.
       INI-LEE-NXT-PRV-XGDD.
           MOVE GAR-CLV-GDD-1 IN CMA-VARI TO GAR-CLV-GDD-1 IN GDD. 
           MOVE 'GAR-CLV-GDD-1' TO FIO-AKEY.
           MOVE FIO-GET-KEY TO FIO-CMND.
           PERFORM GAR-FIO-GDD.
           IF FRM-FFLD = FRM-FFLD-PF5
               MOVE 'GDD-KEY-PRV' TO FIO-AKEY
               MOVE FIO-GET-PRV TO FIO-CMND
           ELSE
               MOVE FIO-GET-NXT TO FIO-CMND.
           PERFORM GAR-FIO-GDD.
           IF FIO-STAT-OKS AND
              GDD-CIC-ICLI IN GDD = DBC-KEY-IDBC IN CMA-VARI
               MOVE GAR-CLV-GDD-2 IN GDD TO GAR-CLV-GDG-1 IN GDG
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM GAR-FIO-GDG
               IF FIO-STAT-OKS
                   MOVE 2 TO WSS-IBUS
                   MOVE GAR-NUM-SIS IN GDG TO GDG-NUM-SIS IN QGA-FLD
                   MOVE GAR-COD-CRR IN GDG TO GDG-COD-CRR IN QGA-FLD
           ELSE
               MOVE FIO-STAT-NEX TO FIO-STAT.

       FIN-LEE-NXT-PRV-XGDD.
           EXIT.

       LEE-NXT-PRV-XDBC SECTION.
       INI-LEE-NXT-PRV-XDBC.
           IF DBC-IDC-ICLI IN CMA-VARI > SPACES
               PERFORM LEE-NXT-PRV-DBC
           ELSE
               PERFORM LEE-NXT-PRV-NOM.
       FIN-LEE-NXT-PRV-XDBC.
           EXIT.

       LEE-NXT-PRV-DBC SECTION.
       INI-LEE-NXT-PRV-DBC.
           MOVE DBC-IDC-ICLI IN CMA-VARI TO DBC-IDC-ICLI IN DBC.
           MOVE FIO-GET-KEY-ALT    TO FIO-CMND.
           MOVE 'DBC-IDC-ICLI' TO FIO-AKEY.
           PERFORM SGC-FIO-DBC.
           IF FRM-FFLD = FRM-FFLD-PF7
               MOVE 'DBC-GLS-NOMD' TO FIO-AKEY
               MOVE FIO-GET-PRV TO FIO-CMND
           ELSE
               MOVE FIO-GET-NXT-ALT TO FIO-CMND.
           PERFORM SGC-FIO-DBC.
           MOVE FIO-STAT TO PGM-STAT-DBC.
           IF FIO-STAT-OKS
               MOVE 1 TO WSS-IBUS.
       FIN-LEE-NXT-PRV-DBC.
           EXIT.

       LEE-NXT-PRV-NOM SECTION.
       INI-LEE-NXT-PRV-NOM.
           MOVE DBC-GLS-NOMC IN CMA-VARI TO DBC-GLS-NOMC IN DBC.
           MOVE FIO-GET-NLS-ALT TO FIO-CMND.
           MOVE 'DBC-GLS-NOMC'   TO FIO-AKEY.
           PERFORM SGC-FIO-DBC.
           IF FIO-STAT-OKS
               IF FRM-FFLD = FRM-FFLD-PF7
                   MOVE 'DBC-GLS-NOMD' TO FIO-AKEY
                   MOVE FIO-GET-PRV TO FIO-CMND
               ELSE
                   MOVE FIO-GET-NXT-ALT TO FIO-CMND
           ELSE
               IF FRM-FFLD = FRM-FFLD-PF7
                   MOVE 'DBC-GLS-NOMD' TO FIO-AKEY
                   MOVE FIO-GET-PRV TO FIO-CMND
               ELSE
                   MOVE FIO-GET-NLS-ALT TO FIO-CMND.
           IF FRM-FFLD = FRM-FFLD-PF8
           MOVE 'DBC-GLS-NOMC'  TO FIO-AKEY.
           PERFORM SGC-FIO-DBC.
           MOVE FIO-STAT TO PGM-STAT-DBC.
           IF FIO-STAT-OKS
               MOVE 1 TO WSS-IBUS.
       FIN-LEE-NXT-PRV-NOM.
           EXIT.

       COPY SGCBFDBC.
       COPY GARBFGDD.
       COPY SGCBGCLI.
       COPY GNSBGCPT.
       COPY GNSBGSTR.

      *>>>>

      *<<<< BUSULTHIJ
       BUS-ULT-GDG SECTION.
       INI-BUS-ULT-GDG.
           STRING DBC-KEY-IDBC IN DBC
                  '999999999999' DELIMITED BY SIZE
              INTO GDD-KEY-PRV IN GDD.
      *    se busca el ultimo hijo, luego de encontrar el padre
           MOVE 'GDD-KEY-PRV' TO FIO-AKEY.
           MOVE FIO-GET-PRV TO FIO-CMND.
           PERFORM GAR-FIO-GDD.
           IF FIO-STAT-OKS AND 
              GDD-CIC-ICLI IN GDD = DBC-KEY-IDBC IN CMA-VARI
               MOVE GAR-CLV-GDD-2 IN GDD TO GAR-CLV-GDG-1 IN GDG
               MOVE FIO-GET-KEY TO FIO-CMND
               PERFORM GAR-FIO-GDG
               IF FIO-STAT-OKS
                   MOVE 2 TO WSS-IBUS
                   MOVE GAR-NUM-SIS IN GDG TO GDG-NUM-SIS IN QGA-FLD
                   MOVE GAR-COD-CRR IN GDG TO GDG-COD-CRR IN QGA-FLD.
       FIN-BUS-ULT-GDG.
           EXIT.

