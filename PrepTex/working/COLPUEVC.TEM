*% INCLUDE WSS OF 'COLPAEVC.TXT'
*%! INCLUDE WSS OF 'COLPAEVC.GEN'
*% INCLUDE WSS_DTC OF 'COLPAEVC.TXT'
*%! INCLUDE WSS_DTC OF 'COLPAEVC.GEN'
*% INCLUDE WSS_IBM OF 'COLPAEVC.TXT'
*%! INCLUDE WSS_IBM OF 'COLPAEVC.GEN'
       01  PGM-VARI.
           03 PGM-ICLR                  VALUE ' '       PIC X(01).
           03 PGM-LIMPIAR-TODO          VALUE ' '       PIC X(01).
           03 PGM-LIMPIAR-LLAVES        VALUE 'K'       PIC X(01).
           03 PGM-LIMPIAR-CAMPOS        VALUE 'F'       PIC X(01).
           03 PGM-NO-LIMPIAR-NADA       VALUE 'N'       PIC X(01).
           03 PGM-PROG                                  PIC X(08).
           03 PGM-NTRN                                  PIC X(04).
           03 PGM-STAT.
              05 PGM-STAT-OPC.
                 88 PGM-STAT-OPC-NUL    VALUE '  '.
                 07 PGM-STT1-OPC        VALUE '2' PIC X(01).
                    88 PGM-STAT-OPC-OKS VALUE '0'.
                 07 PGM-STT2-OPC        VALUE '3' PIC X(01).
              05 PGM-STAT-EVC.
                 88 PGM-STAT-EVC-NUL    VALUE '  '.
                 07 PGM-STT1-EVC        VALUE '2' PIC X(01).
                    88 PGM-STAT-EVC-OKS VALUE '0'.
                 07 PGM-STT2-EVC        VALUE '3' PIC X(01).
              05 PGM-STAT-PVC.
                 88 PGM-STAT-PVC-NUL    VALUE '  '.
                 07 PGM-STT1-PVC        VALUE '2' PIC X(01).
                    88 PGM-STAT-PVC-OKS VALUE '0'.
                 07 PGM-STT2-PVC        VALUE '3' PIC X(01).
*% IF PGM_STAT_EVC
              05 PGM-STAT-EVC.
                 88 PGM-STAT-EVC-NUL    VALUE '  '.
                 07 PGM-STT1-EVC        VALUE '2' PIC X(01).
                    88 PGM-STAT-EVC-OKS VALUE '0'.
                 07 PGM-STT2-EVC        VALUE '3' PIC X(01).
*% END
*% INCLUDE FIN_PGM_STAT OF 'COLPAEVC.TXT'
*%! INCLUDE FIN_PGM_STAT OF 'COLPAEVC.GEN'
           03 PGM-SRNF                  VALUE '23'      PIC X(02).
           03 PGM-SOKS                  VALUE '00'      PIC X(02).
           03 PGM-SNUL                  VALUE '  '      PIC X(02).
           03 PGM-SUAR                  VALUE SPACES    PIC X(15).
*%! IF LARGO_TOTAL_COMMAREA > 0
*%!           03 PGM-CMMA.
*%!              05 PGM-CMMA-CHR OCCURS {largo_total_commarea} PIC X(01).
