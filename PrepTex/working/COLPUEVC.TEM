       PROCEDURE DIVISION.
      *===================

       MAIN SECTION.
       INI-MAIN.
*% IF GEN_MFC
      *INI-MFC TUXEDO ORACLE
           PERFORM GNS-PRO-LIMPIAR-VARIABLES.
      *FIN-MFC TUXEDO ORACLE
*% END
           PERFORM INI.
       LUP-MAIN.
           IF SCR-INST = SCR-INST-HCM
                 PERFORM SCR-COM.
           IF SCR-CCPP = 'MOD'
                 PERFORM SCR-MOD
           ELSE
           IF SCR-CCPP = 'ING'
                 PERFORM SCR-ING
           ELSE
           IF SCR-CCPP = 'ACC'
                 PERFORM SCR-ACC
           ELSE
           IF SCR-CCPP = 'ELI'
                 PERFORM SCR-ELI
           ELSE
           IF SCR-CCPP = 'ACT'
                 PERFORM SCR-ACT
           ELSE
           IF SCR-CCPP = 'PGM'
                 PERFORM SCR-PGM
           ELSE
                 MOVE SCR-INST-HCM TO SCR-INST.
*% INCLUDE FIN_LUP_MAIN OF 'COLPAEVC.TXT'
*%! INCLUDE FIN_LUP_MAIN OF 'COLPAEVC.GEN'
           GO TO LUP-MAIN.
       FIN-MAIN.
           EXIT.

       INI SECTION.
       INI-INI.
*% INCLUDE PGM_PRE_EIB OF 'COLPAEVC.TXT'
*%! INCLUDE PGM_PRE_EIB OF 'COLPAEVC.GEN'
       COPY GNSBGEIB.
*% INCLUDE PGM_INI OF 'COLPAEVC.TXT'
*%! INCLUDE PGM_INI OF 'COLPAEVC.GEN'
       COPY GNSBGEDB.
           MOVE LOW-VALUES TO EVC-FLD.
           MOVE LOW-VALUES TO EVC-EDT.
*% INCLUDE PGM_POS_EDB OF 'COLPAEVC.TXT'
*%! INCLUDE PGM_POS_EDB OF 'COLPAEVC.GEN'
           PERFORM GNS-PRO-RCK.
           PERFORM GNS-BUS-VCA.
*% IF LARGO_TOTAL_COMMAREA > 0
           MOVE +{largo_total_commarea} TO SCR-TCMA.
*% ELSE
*%     IF NOT ( GNSTCA = GNSTCA_ORIG )
           MOVE +{gnstca} TO SCR-TCMA.
*%     END
*% END
           PERFORM GNS-HDL-SYS.
           PERFORM GNS-HDL-FRM.
           MOVE 'COLPUEVC' TO FIO-PROG.
           PERFORM GNS-BUS-IDD.
*% IF PGM_PTC
      *PTC-INI
           PERFORM GNS-ERR-QUE.
           PERFORM GET-TSK-TERM.
      *PTC-FIN
*% END
*% INCLUDE INI_FIO_IBM OF 'COLPAEVC.TXT'
*%! INCLUDE INI_FIO_IBM OF 'COLPAEVC.GEN'
*% INCLUDE INI_FIO OF 'COLPAEVC.TXT'
*%! INCLUDE INI_FIO OF 'COLPAEVC.GEN'
       FIN-INI.
           EXIT.

       COPY GNSBHSYS.
       COPY GNSBHFRM.
       COPY GNSBPRCK.
*%! IF LARGO_TOTAL_COMMAREA > 0
*%!       GNS-BUS-VCA SECTION.
*%!       INI-GNS-BUS-VCA.
*%!           MOVE 0 TO FRM-IFLD.
*%!       LUP-GNS-BUS-VCA.
*%!           ADD 1 TO FRM-IFLD.
*%!           IF FRM-IFLD NOT > EIBCALEN AND
*%!              FRM-IFLD NOT > {LARGO_TOTAL_COMMAREA}
*%!                MOVE DFH-CMMA(FRM-IFLD) TO PGM-CMMA-CHR(FRM-IFLD)
*%!                GO TO LUP-GNS-BUS-VCA.
*%!           MOVE PGM-CMMA TO SCR-VARI.
*%!       FIN-GNS-BUS-VCA.
*%!           EXIT.
*%! ELSE
       COPY GNSBBVCA.
*%! END
       COPY GNSBBIDD.

       SCR-COM SECTION.
       INI-SCR-COM.
*% INCLUDE INI_SCR_COM OF 'COLPAEVC.TXT'
*%! INCLUDE INI_SCR_COM OF 'COLPAEVC.GEN'
           PERFORM STP-EXIT.
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-CLRA-EVC.
           PERFORM EDT-KEY-EVC.
           PERFORM EDT-FLD-EVC.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-HCM TO SCR-INST.
           MOVE SCR-CMNP     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
*% IF PGM_PTC
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
*% END
           PERFORM GNS-PRO-SYS.
       XCT-ERR.
           MOVE 'XCTERR    ' TO MSG-COD-MENS.
           MOVE 'GNS'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO SYS-MEN1.
           MOVE SYS-PROG     TO SYS-MEN2.
           MOVE SYS-ABOR     TO SYS-CMND.
*% IF PGM_PTC
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
*% END
           PERFORM GNS-PRO-SYS.
       COPY GNSBGGBK.
       FIN-SCR-COM.
           EXIT.

       SCR-MOD SECTION.
       INI-SCR-MOD.
*% INCLUDE INI_SCR_MOD OF 'COLPAEVC.TXT'
*%! INCLUDE INI_SCR_MOD OF 'COLPAEVC.GEN'
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
               PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
               PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8
               PERFORM XCT-PRGP.
           IF NOT ( FRM-FFLD = FRM-FFLD-PF5 OR FRM-FFLD-PF6 )
               GO TO MSG-SCR-MOD.
*% INCLUDE PFS_SCR_MOD OF 'COLPAEVC.TXT'
*%! INCLUDE PFS_SCR_MOD OF 'COLPAEVC.GEN'
       MSG-SCR-MOD.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'MODNOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-MOD
               IF FIO-STAT-OKS
                   IF FRM-MEN1 NOT > SPACES
                       MOVE 'MODYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MEN1
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-MOD.
*% INCLUDE FIN_SCR_MOD OF 'COLPAEVC.TXT'
*%! INCLUDE FIN_SCR_MOD OF 'COLPAEVC.GEN'
           EXIT.

       SCR-ING SECTION.
       INI-SCR-ING.
*% INCLUDE INI_SCR_ING OF 'COLPAEVC.TXT'
*%! INCLUDE INI_SCR_ING OF 'COLPAEVC.GEN'
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-IKY
               MOVE SCR-INST-GKY TO SCR-INST
*% IF PGM_PTC
      *PTC-INI
               IF NOT SCR-OFRM-SRV
                   PERFORM SCR-DMSG
               ELSE
                   PERFORM SCR-PUTA
      *PTC-FIN
*% ELSE
               PERFORM SCR-DMSG
*% END
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
*%!! INCLUDE PFS_SCR_ING OF 'COLPAEVC.TXT'
*%! INCLUDE PFS_SCR_ING OF 'COLPAEVC.GEN'
       MSG-SCR-ING.
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'INGNOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-ING
               IF FIO-STAT-OKS
                   IF FRM-MEN1 NOT > SPACES
                       MOVE 'INGYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MEN1
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ING.
*% INCLUDE FIN_SCR_ING OF 'COLPAEVC.TXT'
*%! INCLUDE FIN_SCR_ING OF 'COLPAEVC.GEN'
           EXIT.

       SCR-ACC SECTION.
       INI-SCR-ACC.
*% INCLUDE INI_SCR_ACC OF 'COLPAEVC.TXT'
*%! INCLUDE INI_SCR_ACC OF 'COLPAEVC.GEN'
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8 OR
                         FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM XCT-PRGP.
*%!! INCLUDE PFS_SCR_ACC OF 'COLPAEVC.TXT'
*%! INCLUDE PFS_SCR_ACC OF 'COLPAEVC.GEN'
           MOVE SPACES TO FRM-MENS.
           IF SCR-STAT NOT = SCR-STAT-CON
               PERFORM CMN-ACC.
           PERFORM SCR-MSSG.
       FIN-SCR-ACC.
*% INCLUDE FIN_SCR_ACC OF 'COLPAEVC.TXT'
*%! INCLUDE FIN_SCR_ACC OF 'COLPAEVC.GEN'
           EXIT.

       SCR-ELI SECTION.
       INI-SCR-ELI.
*% INCLUDE INI_SCR_ELI OF 'COLPAEVC.TXT'
*%! INCLUDE INI_SCR_ELI OF 'COLPAEVC.GEN'
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8 OR
                         FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM XCT-PRGP.
*%!! INCLUDE PFS_SCR_ELI OF 'COLPAEVC.TXT'
*%! INCLUDE PFS_SCR_ELI OF 'COLPAEVC.GEN'
           IF SCR-STAT = SCR-STAT-CON
               MOVE SPACES TO FRM-MENS
           ELSE
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'ELINOT    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
           ELSE
               MOVE SPACES TO FRM-MENS
               PERFORM CMN-ELI
               IF FIO-STAT-OKS
                   IF FRM-MEN1 NOT > SPACES
                       MOVE 'ELIYES    ' TO MSG-COD-MENS
                       MOVE 'GNS' TO MSG-COD-SIST
                       PERFORM GET-MSG
                       MOVE MSG-GLS-DESC TO FRM-MEN1
                   ELSE
                       NEXT SENTENCE
               ELSE
               IF FRM-MENS NOT > SPACES
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ELI.
*% INCLUDE FIN_SCR_ELI OF 'COLPAEVC.TXT'
*%! INCLUDE FIN_SCR_ELI OF 'COLPAEVC.GEN'
           EXIT.

       SCR-ACT SECTION.
       INI-SCR-ACT.
*% INCLUDE INI_SCR_ACT OF 'COLPAEVC.TXT'
*%! INCLUDE INI_SCR_ACT OF 'COLPAEVC.GEN'
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
           PERFORM DESSCRTRL.
           IF ( FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8  OR
                           FRM-FFLD-PF5 OR FRM-FFLD-PF6 ) AND
              SCR-ACTU = SCR-ACTU-MOD
               PERFORM XCT-PRGP.
*%!! INCLUDE PFS_SCR_ACT OF 'COLPAEVC.TXT'
*%! INCLUDE PFS_SCR_ACT OF 'COLPAEVC.GEN'
           IF SCR-ACTU = SCR-ACTU-MOD
                  PERFORM MSG-SCR-MOD
           ELSE
                  PERFORM MSG-SCR-ING.
       FIN-SCR-ACT.
*% INCLUDE FIN_SCR_ACT OF 'COLPAEVC.TXT'
*%! INCLUDE FIN_SCR_ACT OF 'COLPAEVC.GEN'
           EXIT.

       SCR-PGM SECTION.
       INI-SCR-PGM.
*% INCLUDE INI_SCR_PGM OF 'COLPAEVC.TXT'
*%! INCLUDE INI_SCR_PGM OF 'COLPAEVC.GEN'
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GFL
                PERFORM GET-FLD
           ELSE
           IF SCR-INST = SCR-INST-GCS
                PERFORM GET-ACC.
       TRL-SCR-PGM.
           PERFORM DESSCRTRL.
           IF FRM-FFLD = FRM-FFLD-PF7 OR FRM-FFLD-PF8 OR
                         FRM-FFLD-PF5 OR FRM-FFLD-PF6
               PERFORM XCT-PRGP.
*%!! INCLUDE PFS_SCR_PGM OF 'COLPAEVC.TXT'
*%! INCLUDE PFS_SCR_PGM OF 'COLPAEVC.GEN'
           MOVE SPACES TO FRM-MENS.
           IF SCR-STAT NOT = SCR-STAT-CON
               PERFORM CMN-PGM.
           PERFORM SCR-MSSG.
       FIN-SCR-PGM.
*% INCLUDE FIN_SCR_PGM OF 'COLPAEVC.TXT'
*%! INCLUDE FIN_SCR_PGM OF 'COLPAEVC.GEN'
           EXIT.

       SCR-DMSG SECTION.
       INI-SCR-DMSG.
*% INCLUDE INI_SCR_DMSG OF 'COLPAEVC.TXT'
*%! INCLUDE INI_SCR_DMSG OF 'COLPAEVC.GEN'
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               PERFORM SCR-CLRA-EVC
               PERFORM EDT-KEY-EVC
               PERFORM EDT-FLD-EVC
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-EVC
               PERFORM EDT-KEY-EVC
           ELSE
               PERFORM CLR-FLD-EVC
               PERFORM EDT-FLD-EVC.
           PERFORM STP-EXIT.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

       XCT-PRGP SECTION.
       INI-XCT-PRGP.
*% INCLUDE INI_XCT_PRGP OF 'COLPAEVC.TXT'
*%! INCLUDE INI_XCT_PRGP OF 'COLPAEVC.GEN'
           MOVE SCR-INST-BRW TO SCR-INST.
           PERFORM STP-EXIT.
           MOVE FRM-FFLD   TO SCR-FFLD.
      *
           MOVE SCR-PROG     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
*% IF PGM_PTC
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
*% END
           PERFORM GNS-PRO-SYS.
           PERFORM XCT-ERR.
       FIN-XCT-PRGP.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO EVC-FLD.
           MOVE LOW-VALUES TO EVC-EDT.
*% INCLUDE SCR_RST OF 'COLPAEVC.TXT'
*%! INCLUDE SCR_RST OF 'COLPAEVC.GEN'
       FIN-SCR-RST.
           EXIT.

       GET-FLD SECTION.
       INI-GET-FLD.
*% INCLUDE GET_FLD OF 'COLPAEVC.TXT'
*%! INCLUDE GET_FLD OF 'COLPAEVC.GEN'
           PERFORM SCR-GFLD.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-FLD.
           EXIT.

       CMN-MOD SECTION.
       INI-CMN-MOD.
*% INCLUDE INI_CMN_MOD OF 'COLPAEVC.TXT'
*%! INCLUDE INI_CMN_MOD OF 'COLPAEVC.GEN'
           PERFORM KEY-ALL-EVC.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM COL-FIO-EVC.
*% IF NOT NO_TIENE_TIMESTAMP
           IF FIO-STAT-OKS AND
              EVC-STP-ITRN IN EVC NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM COL-FIO-EVC
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM COL-FIO-EVC
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
*% IF PGM_PTC
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
*% END
               GO TO FIN-CMN-MOD.
*% END
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-MOD.
*% INCLUDE CMN_MOD_RET OF 'COLPAEVC.TXT'
*%! INCLUDE CMN_MOD_RET OF 'COLPAEVC.GEN'
*% IF NOT NO_HDR_CST
*%    IF HDR_CST
           PERFORM RET-STT-EVC.
*%    END
*% END
           PERFORM RET-ALL-EVC.
*% IF NOT NO_HDR_CST
*%    IF HDR_CST
           PERFORM RET-HDR-EVC.
*%    END
*% END
*% INCLUDE CMN_MOD OF 'COLPAEVC.TXT'
*%! INCLUDE CMN_MOD OF 'COLPAEVC.GEN'
           MOVE FIO-MOD TO FIO-CMND.
           PERFORM COL-FIO-EVC.
*% INCLUDE FIN_CMN_MOD OF 'COLPAEVC.TXT'
*%! INCLUDE FIN_CMN_MOD OF 'COLPAEVC.GEN'
       FIN-CMN-MOD.
           EXIT.

       CMN-ING SECTION.
       INI-CMN-ING.
*% IF NOT NO_INI_REG
           MOVE SPACES TO EVC.
*% END
*% INCLUDE INI_CMN_ING OF 'COLPAEVC.TXT'
*%! INCLUDE INI_CMN_ING OF 'COLPAEVC.GEN'
*% IF NOT NO_HDR_CST
*%    IF HDR_CST
           PERFORM RET-STT-EVC.
*%    END
*% END
           PERFORM RET-ALL-EVC.
*% IF NOT NO_HDR_CST
*%    IF HDR_CST
           PERFORM RET-HDR-EVC.
*%    END
*% END
*% INCLUDE CMN_ING OF 'COLPAEVC.TXT'
*%! INCLUDE CMN_ING OF 'COLPAEVC.GEN'
           MOVE FIO-PUT TO FIO-CMND.
           PERFORM COL-FIO-EVC.
*% INCLUDE FIN_CMN_ING OF 'COLPAEVC.TXT'
*%! INCLUDE FIN_CMN_ING OF 'COLPAEVC.GEN'
       FIN-CMN-ING.
           EXIT.

       CMN-ACC SECTION.
       INI-CMN-ACC.
           IF SCR-STAT = SCR-STAT-ABT OR SCR-VFLD = SCR-VFLD-NOT
               MOVE 'ACCION NO REALIZADA' TO FRM-MENS
*% INCLUDE ABT_CMN_ACC OF 'COLPAEVC.TXT'
*%! INCLUDE ABT_CMN_ACC OF 'COLPAEVC.GEN'
               GO TO FIN-CMN-ACC.
*% INCLUDE INI_CMN_ACC OF 'COLPAEVC.TXT'
*%! INCLUDE INI_CMN_ACC OF 'COLPAEVC.GEN'
           PERFORM KEY-ALL-EVC.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM COL-FIO-EVC.
*% IF NOT NO_TIENE_TIMESTAMP
           IF FIO-STAT-OKS AND
              EVC-STP-ITRN IN EVC NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM COL-FIO-EVC
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM COL-FIO-EVC
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-ACC.
*% END
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ACC.
*% INCLUDE CMN_ACC_RET OF 'COLPAEVC.TXT'
*%! INCLUDE CMN_ACC_RET OF 'COLPAEVC.GEN'
           PERFORM RET-ALL-EVC.
*% INCLUDE CMN_ACC OF 'COLPAEVC.TXT'
*%! INCLUDE CMN_ACC OF 'COLPAEVC.GEN'
       FIN-CMN-ACC.
           EXIT.

       CMN-ELI SECTION.
       INI-CMN-ELI.
*% INCLUDE INI_CMN_ELI OF 'COLPAEVC.TXT'
*%! INCLUDE INI_CMN_ELI OF 'COLPAEVC.GEN'
           PERFORM KEY-ALL-EVC.
           MOVE FIO-GET-KEY-UPD TO FIO-CMND.
           PERFORM COL-FIO-EVC.
*% IF NOT NO_TIENE_TIMESTAMP
           IF FIO-STAT-OKS AND
              EVC-STP-ITRN IN EVC NOT = SCR-UMOD
               MOVE FIO-ULK-REC TO FIO-CMND
               PERFORM COL-FIO-EVC
               MOVE FIO-BCK-OUT TO FIO-CMND
               PERFORM COL-FIO-EVC
               MOVE 'GNS' TO MSG-COD-SIST
               MOVE 'GNSACCSSIMUL' TO MSG-COD-MENS
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               GO TO FIN-CMN-ELI.
*% END
           IF NOT FIO-STAT-OKS
               GO TO FIN-CMN-ELI.
*% INCLUDE CMN_ELI OF 'COLPAEVC.TXT'
*%! INCLUDE CMN_ELI OF 'COLPAEVC.GEN'
           MOVE FIO-DEL TO FIO-CMND.
           PERFORM COL-FIO-EVC.
*% INCLUDE FIN_CMN_ELI OF 'COLPAEVC.TXT'
*%! INCLUDE FIN_CMN_ELI OF 'COLPAEVC.GEN'
       FIN-CMN-ELI.
           EXIT.

       CMN-PGM SECTION.
       INI-CMN-PGM.
*% INCLUDE INI_CMN_PGM OF 'COLPAEVC.TXT'
*%! INCLUDE INI_CMN_PGM OF 'COLPAEVC.GEN'
           PERFORM RET-ALL-EVC.
*% INCLUDE CMN_PGM OF 'COLPAEVC.TXT'
*%! INCLUDE CMN_PGM OF 'COLPAEVC.GEN'
       FIN-CMN-PGM.
           EXIT.

       STP-EXIT SECTION.
       INI-STP-EXIT.
*% INCLUDE INI_STP_EXIT OF 'COLPAEVC.TXT'
*%! INCLUDE INI_STP_EXIT OF 'COLPAEVC.GEN'
           MOVE SCR-PROG TO PGM-PROG.
           MOVE SCR-NTRN TO PGM-NTRN.

           MOVE SCR-PROV TO SCR-PROG.
           MOVE SCR-NTRV TO SCR-NTRN.

           MOVE PGM-PROG TO SCR-PROV
           MOVE PGM-NTRN TO SCR-NTRV.
*% INCLUDE FIN_STP_EXIT OF 'COLPAEVC.TXT'
*%! INCLUDE FIN_STP_EXIT OF 'COLPAEVC.GEN'
       FIN-STP-EXIT.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
*% INCLUDE INI_SCR_EXIT OF 'COLPAEVC.TXT'
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO SYS-NTRN.
*% INCLUDE EXIT_SCR OF 'COLPAEVC.TXT'
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
*% IF PGM_PTC
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
*% END
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
*% INCLUDE INI_SCR_PUTA OF 'COLPAEVC.TXT'
*%! INCLUDE INI_SCR_PUTA OF 'COLPAEVC.GEN'
*% IF PGM_PTC
      *PTC-INI
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
*% END
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE EVC-FLD TO FRM-DFLD.
*% INCLUDE POST_SCR_PUTA OF 'COLPAEVC.TXT'
*%! INCLUDE POST_SCR_PUTA OF 'COLPAEVC.GEN'
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO EVC-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GFLD SECTION.
       INI-SCR-GFLD.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
*% INCLUDE RCV_SCR_GFLD OF 'COLPAEVC.TXT'
*%! INCLUDE RCV_SCR_GFLD OF 'COLPAEVC.GEN'
           MOVE SPACES TO FRM-MENS.
*% INCLUDE INI_SCR_GFLD OF 'COLPAEVC.TXT'
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
*% INCLUDE PFS_SCR_GFLD OF 'COLPAEVC.TXT'
*%! INCLUDE PFS_SCR_GFLD OF 'COLPAEVC.GEN'
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-FLD-EVC
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO EVC-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GFLD.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-EVC.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
*% IF PGM_PTC
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
*% END
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-EVC
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           PERFORM VAL-NUM-FLD-EVC.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
*% IF PGM_PTC
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
*% END
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-EVC
               PERFORM EDT-FLD-EVC
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
*% INCLUDE SCR_GFLD_VAL_MKY OF 'COLPAEVC.TXT'
*%! INCLUDE SCR_GFLD_VAL_MKY OF 'COLPAEVC.GEN'
           PERFORM VAL-MKY-EVC.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
*% IF PGM_PTC
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
*% END
               GO TO ERR-SCR-GFLD.
*% INCLUDE SCR_GFLD_VAL_FLD OF 'COLPAEVC.TXT'
*%! INCLUDE SCR_GFLD_VAL_FLD OF 'COLPAEVC.GEN'
           PERFORM VAL-FLD-EVC.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GFLD.
*% INCLUDE SCR_GFLD_VAL_CON_FLD OF 'COLPAEVC.TXT'
*%! INCLUDE SCR_GFLD_VAL_CON_FLD OF 'COLPAEVC.GEN'
           PERFORM VAL-CON-FLD-EVC.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GFLD.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-FLD-EVC.
       ERR-SCR-GFLD.
*% IF PGM_PTC
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
*% END
*% INCLUDE ERR_SCR_GFLD OF 'COLPAEVC.TXT'
*%! INCLUDE ERR_SCR_GFLD OF 'COLPAEVC.GEN'
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-EVC.
           PERFORM EDT-FLD-EVC.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GFLD.
*% INCLUDE FIN_SCR_GFLD OF 'COLPAEVC.TXT'
*%! INCLUDE FIN_SCR_GFLD OF 'COLPAEVC.GEN'
           MOVE SCR-STAT-MOD TO SCR-STAT.

       GET-ACC SECTION.
       INI-GET-ACC.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
*% INCLUDE PFS_GET_ACC OF 'COLPAEVC.TXT'
*%! INCLUDE PFS_GET_ACC OF 'COLPAEVC.GEN'
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO EVC-GLS-MEN2
               MOVE SCR-PRMT     TO EVC-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO EVC-FLD.
           IF NOT ( ( FRM-FFLD = FRM-FFLD-PF12 OR FRM-FFLD-PF3 ) OR
                      SCR-CONF > SPACES )
               PERFORM VAL-NUM-KEY-EVC
               PERFORM VAL-NUM-FLD-EVC.
*% INCLUDE FIN_GET_ACC OF 'COLPAEVC.TXT'
*%! INCLUDE FIN_GET_ACC OF 'COLPAEVC.GEN'
       FIN-GET-ACC.
           MOVE SCR-STAT-SON TO SCR-STAT.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
*% INCLUDE PFS_GET_CON OF 'COLPAEVC.TXT'
*%! INCLUDE PFS_GET_CON OF 'COLPAEVC.GEN'
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO EVC-GLS-MEN2
               MOVE SCR-PRMT     TO EVC-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO EVC-FLD.
*% INCLUDE FIN_GET_CON OF 'COLPAEVC.TXT'
*%! INCLUDE FIN_GET_CON OF 'COLPAEVC.GEN'
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO EVC-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
*% INCLUDE INI_HBL_IKY OF 'COLPAEVC.TXT'
*%! INCLUDE INI_HBL_IKY OF 'COLPAEVC.GEN'
*% IF PGM_PTC
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
*% END
           IF SCR-DESDE-CMN
               MOVE EVC-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO EVC-FLD
      * FLB  7-NOV-1994
               PERFORM CLR-KEY-EVC

               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO EVC-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO EVC-GLS-MENS.
           PERFORM PRO-FLD-EVC.
           PERFORM PRO-MKY-EVC.
           PERFORM UNP-IKY-EVC.
           PERFORM FST-KEY-EVC.
*% INCLUDE FIN_HBL_IKY OF 'COLPAEVC.TXT'
*%! INCLUDE FIN_HBL_IKY OF 'COLPAEVC.GEN'
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
*% INCLUDE INI_HBL_KEY OF 'COLPAEVC.TXT'
*%! INCLUDE INI_HBL_KEY OF 'COLPAEVC.GEN'
*% IF PGM_PTC
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
*% END
           IF SCR-DESDE-CMN
               MOVE EVC-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO EVC-FLD
               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO EVC-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO EVC-GLS-MENS.
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-FLD-EVC.
           MOVE '1' TO FRM-CPIM-AEY.
           PERFORM UNP-KEY-EVC.
           PERFORM FST-KEY-EVC.
*% INCLUDE FIN_HBL_KEY OF 'COLPAEVC.TXT'
*%! INCLUDE FIN_HBL_KEY OF 'COLPAEVC.GEN'
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
*% INCLUDE INI_HBL_FLD OF 'COLPAEVC.TXT'
*%! INCLUDE INI_HBL_FLD OF 'COLPAEVC.GEN'
           PERFORM UNP-MKY-EVC.
           PERFORM UNP-FLD-EVC.
           PERFORM FST-FLD-EVC.
*% INCLUDE FIN_HBL_FLD OF 'COLPAEVC.TXT'
*%! INCLUDE FIN_HBL_FLD OF 'COLPAEVC.GEN'
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO EVC-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
*% INCLUDE INI_PRO_ALL OF 'COLPAEVC.TXT'
*%! INCLUDE INI_PRO_ALL OF 'COLPAEVC.GEN'
           PERFORM PRO-KEY-EVC.
           PERFORM PRO-FLD-EVC.
           PERFORM FST-MSG.
*% INCLUDE FIN_PRO_ALL OF 'COLPAEVC.TXT'
*%! INCLUDE FIN_PRO_ALL OF 'COLPAEVC.GEN'
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
*% IF PGM_PTC
      * PTC-INI
           IF SCR-STAT NOT = SCR-STPP
               MOVE FRM-SUAR-MAL TO FRM-SUAR.
      * PTC-FIN
*% END
*% INCLUDE INI_SCR_TRL OF 'COLPAEVC.TXT'
*%! INCLUDE INI_SCR_TRL OF 'COLPAEVC.GEN'
*% INCLUDE INI_SCR_TRL_PU OF 'COLPAEVC.TXT'
*%! INCLUDE INI_SCR_TRL_PU OF 'COLPAEVC.GEN'
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
*% INCLUDE FIN_SCR_TRL OF 'COLPAEVC.TXT'
*%! INCLUDE FIN_SCR_TRL OF 'COLPAEVC.GEN'
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-EVC
               PERFORM EDT-FLD-EVC
               PERFORM SCR-PUTA
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-EVC
               PERFORM EDT-FLD-EVC
               PERFORM SCR-PUTA
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-EVC.
           PERFORM EDT-FLD-EVC.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           IF SCR-INST = SCR-INST-GCF
                  PERFORM SCR-GCNF
                  GO TO FIN-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
           IF SCR-STAT = SCR-STAT-ABT OR SCR-STAT-CON
                  GO TO FIN-DESSCRTRL.
           IF SCR-FFLD-RET OR SCR-FFLD-PF10
                  MOVE SCR-VFLD-YES TO SCR-VFLD
                  PERFORM PRO-CONF.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

       PRO-CONF SECTION.
       INI-PRO-CONF.
*% INCLUDE INI_PRO_CONF OF 'COLPAEVC.TXT'
*%! INCLUDE INI_PRO_CONF OF 'COLPAEVC.GEN'
*% IF PGM_PTC
      *PTC-INI
           IF SCR-OFRM-SRV AND
              SCR-CONF NOT > SPACES
                MOVE SPACES TO FRM-MENS.
      *PTC-FIN
*% END
           IF FRM-MENS NOT > SPACES
               IF SCR-CONF NOT > SPACES
                   GO TO FIN-PRO-CONF
               ELSE
                   MOVE SCR-CONF TO FRM-MENS
           ELSE
               IF FRM-MEN1 NOT > SPACES
                   MOVE SCR-CONF TO FRM-MEN1.
           PERFORM PRO-KEY-EVC.
           PERFORM PRO-FLD-EVC.
           PERFORM FST-MSG.
*% INCLUDE FIN_PRO_CONF OF 'COLPAEVC.TXT'
*%! INCLUDE FIN_PRO_CONF OF 'COLPAEVC.GEN'
           PERFORM SCR-MSSG.
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM EDT-KEY-EVC
               PERFORM EDT-FLD-EVC.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GCF TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-PRO-CONF.
           EXIT.

       SCR-GCNF SECTION.
       INI-SCR-GCNF.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3  OR
                               FRM-FFLD-PF4 )
*% INCLUDE PFS_SCR_GCNF OF 'COLPAEVC.TXT'
*%! INCLUDE PFS_SCR_GCNF OF 'COLPAEVC.GEN'
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO EVC-GLS-MEN2
               MOVE SCR-CONF     TO EVC-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO EVC-FLD.
           MOVE FRM-FFLD TO SCR-FFLD.
           IF FRM-FFLD = FRM-FFLD-PF3
               MOVE SCR-VFLD-NOT TO SCR-VFLD
           ELSE
           IF FRM-FFLD = FRM-FFLD-RET OR FRM-FFLD-PF10 OR FRM-FFLD-PF4
               MOVE SCR-VFLD-YES TO SCR-VFLD
               PERFORM VAL-NUM-KEY-EVC
               PERFORM VAL-NUM-FLD-EVC
           ELSE
           IF FRM-FFLD = FRM-FFLD-PF12
               MOVE SCR-STPP TO SCR-STAT
               IF SCR-STAT = SCR-STAT-MOD
                   PERFORM VAL-NUM-KEY-EVC
                   PERFORM VAL-NUM-FLD-EVC
                   PERFORM SCR-TRL
               ELSE
                   MOVE SCR-VFLD-NOT TO SCR-VFLD.
*% INCLUDE FIN_SCR_GCNF OF 'COLPAEVC.TXT'
*%! INCLUDE FIN_SCR_GCNF OF 'COLPAEVC.GEN'
       FIN-SCR-GCNF.
           EXIT.

      * PGM-EBR-ALL SECTION.
