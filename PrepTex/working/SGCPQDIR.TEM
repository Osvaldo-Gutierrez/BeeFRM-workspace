*% END

       PROCEDURE DIVISION.
      *===================

       MAIN SECTION.
       INI-MAIN.
*% IF GEN_MFC
      *INI-MFC TUXEDO ORACLE
           PERFORM GNS-PRO-LIMPIAR-VARIABLES.
      *FIN-MFC TUXEDO ORACLE
*% END
           PERFORM INI.
       LUP-MAIN.
           IF SCR-INST = SCR-INST-HCM
                 PERFORM SCR-COM.
           IF SCR-CCPP = 'MOD'
                 PERFORM SCR-MOD
           ELSE
           IF SCR-CCPP = 'ING'
                 PERFORM SCR-ING
           ELSE
           IF SCR-CCPP = 'ACC'
                 PERFORM SCR-ACC
           ELSE
           IF SCR-CCPP = 'ELI'
                 PERFORM SCR-ELI
           ELSE
           IF SCR-CCPP = 'ACT'
                 PERFORM SCR-ACT
           ELSE
           IF SCR-CCPP = 'PGM'
                 PERFORM SCR-PGM
           ELSE
                 MOVE SCR-INST-HCM TO SCR-INST.
*%! FLB   22-NOV-1993
*% INCLUDE FIN_LUP_MAIN OF 'SGCPADIR.TXT'
*%! INCLUDE FIN_LUP_MAIN OF 'SGCPADIR.GEN'
           GO TO LUP-MAIN.
       FIN-MAIN.
           EXIT.

       INI SECTION.
       INI-INI.
*% INCLUDE PGM_PRE_EIB OF 'SGCPADIR.TXT'
*%! INCLUDE PGM_PRE_EIB OF 'SGCPADIR.GEN'
       COPY GNSBGEIB.
*% INCLUDE PGM_INI OF 'SGCPADIR.TXT'
*%! INCLUDE PGM_INI OF 'SGCPADIR.GEN'
       COPY GNSBGEDB.
           MOVE LOW-VALUES TO DIR-FLD.
           MOVE LOW-VALUES TO DIR-EDT.
*% INCLUDE PGM_POS_EDB OF 'SGCPADIR.TXT'
*%! INCLUDE PGM_POS_EDB OF 'SGCPADIR.GEN'
           PERFORM GNS-PRO-RCK.
*% INCLUDE PGM_POS_RCK OF 'SGCPADIR.TXT'
*%! INCLUDE PGM_POS_RCK OF 'SGCPADIR.GEN'
           PERFORM GNS-BUS-VCA.
*%!ISP  17-JUL-1992 LABEL PARA DB2
*% INCLUDE SYS_SCOD OF 'SGCPADIR.TXT'
*% IF LARGO_TOTAL_COMMAREA > 0
           MOVE +{largo_total_commarea} TO SCR-TCMA.
*% ELSE
*%     IF NOT ( GNSTCA = GNSTCA_ORIG )
           MOVE +{gnstca} TO SCR-TCMA.
*%     END
*% END
           PERFORM GNS-HDL-SYS.
           PERFORM GNS-HDL-FRM.
           MOVE 'SGCPQDIR' TO FIO-PROG.
           PERFORM GNS-BUS-IDD.
*% IF PGM_PTC
      *PTC-INI
           PERFORM GNS-ERR-QUE.
           PERFORM GET-TSK-TERM.
      *PTC-FIN
*% END
*% INCLUDE INI_FIO_IBM OF 'SGCPADIR.TXT'
*%! INCLUDE INI_FIO_IBM OF 'SGCPADIR.GEN'
*% INCLUDE INI_FIO OF 'SGCPADIR.TXT'
*%! INCLUDE INI_FIO OF 'SGCPADIR.GEN'
       FIN-INI.
           EXIT.

       COPY GNSBHSYS.
       COPY GNSBHFRM.
       COPY GNSBPRCK.
*% IF LARGO_TOTAL_COMMAREA > 0
       GNS-BUS-VCA SECTION.
       INI-GNS-BUS-VCA.
           MOVE 0 TO FRM-IFLD.
       LUP-GNS-BUS-VCA.
           ADD 1 TO FRM-IFLD.
           IF FRM-IFLD NOT > EIBCALEN AND
              FRM-IFLD NOT > {LARGO_TOTAL_COMMAREA}
                MOVE DFH-CMMA(FRM-IFLD) TO PGM-CMMA-CHR(FRM-IFLD)
                GO TO LUP-GNS-BUS-VCA.
           MOVE PGM-CMMA TO SCR-VARI.
       FIN-GNS-BUS-VCA.
           EXIT.
*% ELSE
       COPY GNSBBVCA.
*% END
       COPY GNSBBIDD.

       SCR-COM SECTION.
       INI-SCR-COM.
*% INCLUDE INI_SCR_COM OF 'SGCPADIR.TXT'
*%! INCLUDE INI_SCR_COM OF 'SGCPADIR.GEN'
           MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-CLRA-DIR.
           PERFORM EDT-KEY-DIR.
           PERFORM EDT-FLD-DIR.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-HCM TO SCR-INST.
           MOVE SCR-CMNP     TO SYS-PROG.
           MOVE SCR-VARI     TO SYS-CMMA.
           MOVE SCR-TCMA     TO SYS-TCMA.
           MOVE SYS-XCTL     TO SYS-CMND.
*% IF PGM_PTC
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
*% END
           PERFORM GNS-PRO-SYS.
       XCT-ERR.
           MOVE 'XCTERR    ' TO MSG-COD-MENS.
           MOVE 'GNS'        TO MSG-COD-SIST.
           PERFORM GET-MSG.
           MOVE MSG-GLS-DESC TO SYS-MEN1.
           MOVE SYS-PROG     TO SYS-MEN2.
           MOVE SYS-ABOR     TO SYS-CMND.
*% IF PGM_PTC
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
*% END
           PERFORM GNS-PRO-SYS.
       COPY GNSBGGBK.
       FIN-SCR-COM.
           EXIT.

       SCR-MOD SECTION.
       INI-SCR-MOD.
*% INCLUDE INI_SCR_MOD OF 'SGCPADIR.TXT'
*%! INCLUDE INI_SCR_MOD OF 'SGCPADIR.GEN'
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-MOD
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-MOD.
           PERFORM DESSCRTRL.
*% INCLUDE PFS_SCR_MOD OF 'SGCPADIR.TXT'
*%! INCLUDE PFS_SCR_MOD OF 'SGCPADIR.GEN'
       MSG-SCR-MOD.
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-MOD.
*% INCLUDE FIN_SCR_MOD OF 'SGCPADIR.TXT'
*%! INCLUDE FIN_SCR_MOD OF 'SGCPADIR.GEN'
           EXIT.

       BRW-SCR-MOD SECTION.
       INI-BRW-SCR-MOD.
*% INCLUDE BRW_SCR_MOD OF 'SGCPADIR.TXT'
*%! INCLUDE BRW_SCR_MOD OF 'SGCPADIR.GEN'
       FIN-BRW-SCR-MOD.
           EXIT.

       SCR-ING SECTION.
       INI-SCR-ING.
*% INCLUDE INI_SCR_ING OF 'SGCPADIR.TXT'
*%! INCLUDE INI_SCR_ING OF 'SGCPADIR.GEN'
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-IKY
               MOVE SCR-INST-GKY TO SCR-INST
*% IF PGM_PTC
      *PTC-INI
               IF NOT SCR-OFRM-SRV
                   PERFORM SCR-DMSG
               ELSE
                   PERFORM SCR-PUTA
      *PTC-FIN
*% ELSE
               PERFORM SCR-DMSG
*% END
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-RST
                PERFORM GET-IKY
                IF NOT FRM-STAT-ABT
                        MOVE SPACES TO FRM-MENS
                        PERFORM FND-KEY
                        IF NOT FIO-STAT-OKS AND
                           FIO-STAT NOT = FIO-STAT-NEX
      *JSS                  PERFORM CLR-FLD-DIR
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
                            PERFORM SCR-TRL
                        ELSE
*% IF PGM_PTC
      *PTC-INI
                        MOVE 'S' TO SCR-IERR
      *PTC-FIN
*% END
                        MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR
                        IF FRM-MENS NOT > SPACES
                            MOVE 'REGEXI    ' TO MSG-COD-MENS
                            MOVE 'GNS' TO MSG-COD-SIST
                            PERFORM GET-MSG
                            MOVE MSG-GLS-DESC TO FRM-MENS
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-ING
                        ELSE
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-ING
                ELSE
                        MOVE SPACES TO FRM-MENS
                        PERFORM SCR-MSSG
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ING
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON.
           PERFORM DESSCRTRL.
*% INCLUDE PFS_SCR_ING OF 'SGCPADIR.TXT'
*%! INCLUDE PFS_SCR_ING OF 'SGCPADIR.GEN'
       MSG-SCR-ING.
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ING.
*% INCLUDE FIN_SCR_ING OF 'SGCPADIR.TXT'
*%! INCLUDE FIN_SCR_ING OF 'SGCPADIR.GEN'
           EXIT.

       SCR-ACC SECTION.
       INI-SCR-ACC.
*% INCLUDE INI_SCR_ACC OF 'SGCPADIR.TXT'
*%! INCLUDE INI_SCR_ACC OF 'SGCPADIR.GEN'
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ACC
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-ACC.
           PERFORM DESSCRTRL.
*% INCLUDE PFS_SCR_ACC OF 'SGCPADIR.TXT'
*%! INCLUDE PFS_SCR_ACC OF 'SGCPADIR.GEN'
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ACC.
*% INCLUDE FIN_SCR_ACC OF 'SGCPADIR.TXT'
*%! INCLUDE FIN_SCR_ACC OF 'SGCPADIR.GEN'
           EXIT.

       BRW-SCR-ACC SECTION.
       INI-BRW-SCR-ACC.
*% INCLUDE BRW_SCR_ACC OF 'SGCPADIR.TXT'
*%! INCLUDE BRW_SCR_ACC OF 'SGCPADIR.GEN'
       FIN-BRW-SCR-ACC.
           EXIT.

       SCR-ELI SECTION.
       INI-SCR-ELI.
*% INCLUDE INI_SCR_ELI OF 'SGCPADIR.TXT'
*%! INCLUDE INI_SCR_ELI OF 'SGCPADIR.GEN'
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-KEY
                IF SCR-STAT NOT = SCR-STAT-ABT
                        PERFORM SCR-TRL
                ELSE
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ELI
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-ELI.
           PERFORM DESSCRTRL.
*% INCLUDE PFS_SCR_ELI OF 'SGCPADIR.TXT'
*%! INCLUDE PFS_SCR_ELI OF 'SGCPADIR.GEN'
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ELI.
*% INCLUDE FIN_SCR_ELI OF 'SGCPADIR.TXT'
*%! INCLUDE FIN_SCR_ELI OF 'SGCPADIR.GEN'
           EXIT.

       BRW-SCR-ELI SECTION.
       INI-BRW-SCR-ELI.
*% INCLUDE BRW_SCR_ELI OF 'SGCPADIR.TXT'
*%! INCLUDE BRW_SCR_ELI OF 'SGCPADIR.GEN'
       FIN-BRW-SCR-ELI.
           EXIT.

       SCR-ACT SECTION.
       INI-SCR-ACT.
*% INCLUDE INI_SCR_ACT OF 'SGCPADIR.TXT'
*%! INCLUDE INI_SCR_ACT OF 'SGCPADIR.GEN'
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-RST
                PERFORM GET-KEY
                IF NOT FRM-STAT-ABT
                        MOVE SPACES TO FRM-MENS
                        PERFORM FND-KEY
                        IF FIO-STAT-OKS
                            MOVE SCR-ACTU-MOD TO SCR-ACTU
                            PERFORM PUT-SCR
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
                            PERFORM SCR-TRL
                        ELSE
                        MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR
                        IF FIO-STAT-EOF
                            IF FRM-MENS NOT > SPACES
                                MOVE 'REGEOF    ' TO MSG-COD-MENS
                                MOVE 'GNS' TO MSG-COD-SIST
                                PERFORM GET-MSG
                                MOVE MSG-GLS-DESC TO FRM-MENS
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                            ELSE
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                        ELSE
                        IF FIO-STAT = FIO-STAT-NEX
                            IF FRM-MENS NOT > SPACES
                                MOVE 'REGNEX    ' TO MSG-COD-MENS
                                MOVE 'GNS' TO MSG-COD-SIST
                                PERFORM GET-MSG
                                MOVE MSG-GLS-DESC TO FRM-MENS
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                            ELSE
                                PERFORM SCR-MSSG
                                MOVE SCR-STAT-ABT TO SCR-STAT
                                PERFORM DESSCRTRL
                                GO TO FIN-SCR-ACT
                        ELSE
                            MOVE SCR-ACTU-ING TO SCR-ACTU
      *JSS                  PERFORM CLR-FLD-DIR
                            MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
                            PERFORM SCR-TRL
                ELSE
                        MOVE SPACES TO FRM-MENS
                        PERFORM SCR-MSSG
                        PERFORM DESSCRTRL
                        GO TO FIN-SCR-ACT
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-ACT.
           PERFORM DESSCRTRL.
*% INCLUDE PFS_SCR_ACT OF 'SGCPADIR.TXT'
*%! INCLUDE PFS_SCR_ACT OF 'SGCPADIR.GEN'
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-ACT.
*% INCLUDE FIN_SCR_ACT OF 'SGCPADIR.TXT'
*%! INCLUDE FIN_SCR_ACT OF 'SGCPADIR.GEN'
           EXIT.

       BRW-SCR-ACT SECTION.
       INI-BRW-SCR-ACT.
*% INCLUDE BRW_SCR_ACT OF 'SGCPADIR.TXT'
*%! INCLUDE BRW_SCR_ACT OF 'SGCPADIR.GEN'
       FIN-BRW-SCR-ACT.
           EXIT.

       SCR-PGM SECTION.
       INI-SCR-PGM.
*% INCLUDE INI_SCR_PGM OF 'SGCPADIR.TXT'
*%! INCLUDE INI_SCR_PGM OF 'SGCPADIR.GEN'
           IF SCR-INST = SCR-INST-HKY
               PERFORM HBL-KEY
               MOVE SCR-INST-GKY TO SCR-INST
               PERFORM SCR-DMSG
           ELSE
           IF SCR-INST = SCR-INST-GKY
                PERFORM SCR-RST
                PERFORM GET-KEY
                IF FRM-STAT-ABT
                    MOVE SPACES TO FRM-MENS
                    PERFORM SCR-MSSG
                    PERFORM DESSCRTRL
                    GO TO FIN-SCR-PGM
                ELSE
                    MOVE SPACES TO FRM-MENS
                    PERFORM FND-KEY
                    IF FIO-STAT = FIO-STAT-NEX
                        IF FRM-MENS NOT > SPACES
                            MOVE 'REGNEX    ' TO MSG-COD-MENS
                            MOVE 'GNS' TO MSG-COD-SIST
                            PERFORM GET-MSG
                            MOVE MSG-GLS-DESC TO FRM-MENS
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-PGM
                        ELSE
                            PERFORM SCR-MSSG
                            MOVE SCR-STAT-ABT TO SCR-STAT
                            PERFORM DESSCRTRL
                            GO TO FIN-SCR-PGM
                    ELSE
                        MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
                        PERFORM SCR-TRL
           ELSE
           IF SCR-INST = SCR-INST-GAB
                PERFORM GET-CON
           ELSE
           IF SCR-INST = SCR-INST-BRW
               MOVE SCR-FFLD TO FRM-FFLD
               PERFORM BRW-SCR-PGM.
       TRL-SCR-PGM.
           PERFORM DESSCRTRL.
*% INCLUDE PFS_SCR_PGM OF 'SGCPADIR.TXT'
*%! INCLUDE PFS_SCR_PGM OF 'SGCPADIR.GEN'
           MOVE SPACES TO FRM-MENS.
           PERFORM SCR-MSSG.
       FIN-SCR-PGM.
*% INCLUDE FIN_SCR_PGM OF 'SGCPADIR.TXT'
*%! INCLUDE FIN_SCR_PGM OF 'SGCPADIR.GEN'
           EXIT.

       BRW-SCR-PGM SECTION.
       INI-BRW-SCR-PGM.
*% INCLUDE BRW_SCR_PGM OF 'SGCPADIR.TXT'
*%! INCLUDE BRW_SCR_PGM OF 'SGCPADIR.GEN'
       FIN-BRW-SCR-PGM.
           EXIT.

       SCR-DMSG SECTION.
       INI-SCR-DMSG.
*% INCLUDE INI_SCR_DMSG OF 'SGCPADIR.TXT'
*%! INCLUDE INI_SCR_DMSG OF 'SGCPADIR.GEN'
           IF PGM-ICLR = PGM-NO-LIMPIAR-NADA
               NEXT SENTENCE
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-TODO
               PERFORM SCR-CLRA-DIR
               PERFORM EDT-KEY-DIR
               PERFORM EDT-FLD-DIR
           ELSE
           IF PGM-ICLR = PGM-LIMPIAR-LLAVES
               PERFORM CLR-KEY-DIR
               PERFORM EDT-KEY-DIR
           ELSE
               PERFORM CLR-FLD-DIR
               PERFORM EDT-FLD-DIR.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-DMSG.
           EXIT.

       SCR-KEY SECTION.
       INI-SCR-KEY.
           PERFORM SCR-RST.
           PERFORM GET-KEY.
           IF FRM-STAT-ABT
                   MOVE SPACES TO FRM-MENS
                   PERFORM SCR-MSSG
                   GO TO FIN-SCR-KEY.
       FND-SCR-KEY.
           MOVE SPACES TO FRM-MENS.
           PERFORM FND-KEY.
           IF FIO-STAT-OKS
               PERFORM PUT-SCR
               MOVE PGM-LIMPIAR-TODO TO PGM-ICLR
               GO TO FIN-SCR-KEY.
      *JSS ELSE
      *        PERFORM CLR-FLD-DIR.
       ERR-SCR-KEY.
*% IF PGM_PTC
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
*% END
           MOVE PGM-LIMPIAR-CAMPOS TO PGM-ICLR.
           IF FRM-MENS NOT > SPACES
               IF FIO-STAT-EOF
                   MOVE 'REGEOF    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
               IF FIO-STAT = FIO-STAT-NEX
                   MOVE 'REGNEX    ' TO MSG-COD-MENS
                   MOVE 'GNS' TO MSG-COD-SIST
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS
               ELSE
                   MOVE 'FIOSTAT' TO MSG-COD-TMSG
                   MOVE FIO-STAT  TO MSG-COD-CMSG
                   MOVE 'GNS'     TO MSG-COD-SIST
*% INCLUDE FIOSTAT OF 'SGCPADIR.TXT'
*%! INCLUDE FIOSTAT OF 'SGCPADIR.GEN'
                   PERFORM GET-MSG
                   MOVE MSG-GLS-DESC TO FRM-MENS.
           PERFORM SCR-MSSG.
           MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-SCR-KEY.
           EXIT.

       SCR-RST SECTION.
       INI-SCR-RST.
           MOVE SPACES       TO PGM-ICLR.
           MOVE SPACES       TO SCR-ACTU.
           MOVE SCR-STPP     TO SCR-STAT.
           MOVE SCR-MFLD-NOT TO SCR-MFLD.
           MOVE LOW-VALUES   TO DIR-FLD.
           MOVE LOW-VALUES TO DIR-EDT.
*% INCLUDE SCR_RST OF 'SGCPADIR.TXT'
*%! INCLUDE SCR_RST OF 'SGCPADIR.GEN'
       FIN-SCR-RST.
           EXIT.

       GET-KEY SECTION.
       INI-GET-KEY.
*% INCLUDE GET_KEY OF 'SGCPADIR.TXT'
*%! INCLUDE GET_KEY OF 'SGCPADIR.GEN'
           PERFORM SCR-GKEY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-KEY.
           EXIT.

       GET-IKY SECTION.
       INI-GET-IKY.
*% INCLUDE GET_IKY OF 'SGCPADIR.TXT'
*%! INCLUDE GET_IKY OF 'SGCPADIR.GEN'
           PERFORM SCR-GIKY.
           IF FRM-STAT-ABT
               MOVE SCR-STAT-ABT TO SCR-STAT.
       FIN-GET-IKY.
           EXIT.

       FND-KEY SECTION.
       INI-FND-KEY.
*% INCLUDE INI_FND_KEY OF 'SGCPADIR.TXT'
*%! INCLUDE INI_FND_KEY OF 'SGCPADIR.GEN'
           PERFORM KEY-ALL-DIR.
*% INCLUDE FND_KEY OF 'SGCPADIR.TXT'
*%! INCLUDE FND_KEY OF 'SGCPADIR.GEN'
           MOVE FIO-GET-KEY TO FIO-CMND.
*% INCLUDE FND_KEY_CMND OF 'SGCPADIR.TXT'
*%! INCLUDE FND_KEY_CMND OF 'SGCPADIR.GEN'
           PERFORM SGC-FIO-RCD.
*% IF NOT NO_TIENE_TIMESTAMP
           IF FIO-STAT-OKS
               MOVE RCD-STP-ITRN IN RCD TO SCR-UMOD.
*% END
           MOVE FIO-STAT TO PGM-STAT-RCD.
*% INCLUDE FIN_FND_KEY OF 'SGCPADIR.TXT'
*%! INCLUDE FIN_FND_KEY OF 'SGCPADIR.GEN'
       FIN-FND-KEY.
*% IF PGM_PTC
           IF FIO-STAT = FIO-STAT-NEX
               MOVE 'S' TO SCR-IERR.
       EXT-FND-KEY.
*% END
           EXIT.

       PUT-SCR SECTION.
       INI-PUT-SCR.
*% IF NOT NO_TIENE_TIMESTAMP
           IF PGM-STAT-RCD-OKS
               MOVE RCD-STP-ITRN IN RCD TO SCR-UMOD.
*% END
*% INCLUDE PUT_SCR OF 'SGCPADIR.TXT'
*%! INCLUDE PUT_SCR OF 'SGCPADIR.GEN'
           PERFORM PUT-ALL-DIR.
*% INCLUDE FIN_PUT_SCR OF 'SGCPADIR.TXT'
*%! INCLUDE FIN_PUT_SCR OF 'SGCPADIR.GEN'
       FIN-PUT-SCR.
           EXIT.

       STP-EXIT SECTION.
       INI-STP-EXIT.
*% INCLUDE INI_STP_EXIT OF 'SGCPADIR.TXT'
*%! INCLUDE INI_STP_EXIT OF 'SGCPADIR.GEN'
           MOVE SCR-PROG TO PGM-PROG.
           MOVE SCR-NTRN TO PGM-NTRN.

           MOVE SCR-PROV TO SCR-PROG.
           MOVE SCR-NTRV TO SCR-NTRN.

           MOVE PGM-PROG TO SCR-PROV
           MOVE PGM-NTRN TO SCR-NTRV.
*% INCLUDE FIN_STP_EXIT OF 'SGCPADIR.TXT'
*%! INCLUDE FIN_STP_EXIT OF 'SGCPADIR.GEN'
       FIN-STP-EXIT.
           EXIT.

       SCR-EXIT SECTION.
       INI-SCR-EXIT.
           MOVE 'P'      TO SCR-ICMN.
           MOVE SCR-NTRN TO SYS-NTRN.
*% INCLUDE EXIT_SCR OF 'SGCPADIR.TXT'
           MOVE SCR-VARI TO SYS-CMMA.
           MOVE SCR-TCMA TO SYS-TCMA.
           MOVE SYS-RTRN TO SYS-CMND.
*% IF PGM_PTC
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
*% END
           PERFORM GNS-PRO-SYS.
       FIN-SCR-EXIT.
           EXIT.

       SCR-PUTA SECTION.
       INI-SCR-PUTA.
*% INCLUDE INI_SCR_PUTA OF 'SGCPADIR.TXT'
*%! INCLUDE INI_SCR_PUTA OF 'SGCPADIR.GEN'
*% IF PGM_PTC
      *PTC-INI
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
*% END
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE DIR-FLD TO FRM-DFLD.
*% INCLUDE POST_SCR_PUTA OF 'SGCPADIR.TXT'
*%! INCLUDE POST_SCR_PUTA OF 'SGCPADIR.GEN'
           MOVE FRM-PUT-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
       FIN-SCR-PUTA.
           EXIT.

       SCR-MSSG SECTION.
       INI-SCR-MSSG.
           IF FRM-MEN1 NOT > SPACES
               MOVE SPACES TO FRM-MEN1.
           IF FRM-MEN2 NOT > SPACES
               MOVE SPACES TO FRM-MEN2.
           MOVE FRM-MENS TO DIR-GLS-MENS.
       FIN-SCR-MSSG.
           EXIT.

       SCR-GKEY SECTION.
       INI-SCR-GKEY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
*% INCLUDE PFS_SCR_GKEY_PREV OF 'SGCPADIR.TXT'
*% INCLUDE PFS_SCR_GKEY_PREV OF 'SGCPADIR.GEN'
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
*% INCLUDE PFS_SCR_GKEY OF 'SGCPADIR.TXT'
*%! INCLUDE PFS_SCR_GKEY OF 'SGCPADIR.GEN'
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-DIR
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO DIR-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GKEY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-DIR.
*% IF USA_PFSCONQUE
           PERFORM VAL-NUM-FLD-DIR.
*% END
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
*% IF PGM_PTC
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
*% END
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-DIR
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-DIR.
*% INCLUDE SCR_GKEY_VAL_IKY OF 'SGCPADIR.TXT'
*%! INCLUDE SCR_GKEY_VAL_IKY OF 'SGCPADIR.GEN'
           PERFORM VAL-IKY-DIR.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
*% INCLUDE SCR_GKEY_VAL_MKY OF 'SGCPADIR.TXT'
*%! INCLUDE SCR_GKEY_VAL_MKY OF 'SGCPADIR.GEN'
           PERFORM VAL-MKY-DIR.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
*% INCLUDE SCR_GKEY_VAL_AKY OF 'SGCPADIR.TXT'
*%! INCLUDE SCR_GKEY_VAL_AKY OF 'SGCPADIR.GEN'
           PERFORM VAL-AKY-DIR.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GKEY.
*% INCLUDE SCR_GKEY_VAL_CON_KEY OF 'SGCPADIR.TXT'
*%! INCLUDE SCR_GKEY_VAL_CON_KEY OF 'SGCPADIR.GEN'
           PERFORM VAL-CON-KEY-DIR.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GKEY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-DIR.
       ERR-SCR-GKEY.
*% IF PGM_PTC
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
*% END
*% INCLUDE ERR_SCR_GKEY OF 'SGCPADIR.TXT'
*%! INCLUDE ERR_SCR_GKEY OF 'SGCPADIR.GEN'
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-DIR.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GKEY.
*% INCLUDE FIN_SCR_GKEY OF 'SGCPADIR.TXT'
*%! INCLUDE FIN_SCR_GKEY OF 'SGCPADIR.GEN'
           PERFORM SCR-MSSG.

       SCR-GIKY SECTION.
       INI-SCR-GIKY.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           MOVE SPACES TO FRM-MENS.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
*% INCLUDE PFS_SCR_GIKY OF 'SGCPADIR.TXT'
*%! INCLUDE PFS_SCR_GIKY OF 'SGCPADIR.GEN'
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO FRM-MENS
               PERFORM FST-KEY-DIR
               PERFORM SCR-MSSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO DIR-FLD.
           IF FRM-STAT-ABT
               GO TO FIN-SCR-GIKY.
           MOVE FRM-SUAR-OKS TO FRM-SUAR.
           PERFORM VAL-NUM-KEY-DIR.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
*% IF PGM_PTC
      *PTC-INI
               MOVE 'S' TO SCR-IERR
      *PTC-FIN
*% END
               PERFORM SCR-MSSG
               PERFORM EDT-IKY-DIR
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
      *JSS
           PERFORM CLR-FLD-DIR.
*% INCLUDE SCR_GIKY_VAL_IKY OF 'SGCPADIR.TXT'
*%! INCLUDE SCR_GIKY_VAL_IKY OF 'SGCPADIR.GEN'
           PERFORM VAL-IKY-DIR.
           IF FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR
               GO TO ERR-SCR-GIKY.
*% INCLUDE SCR_GIKY_VAL_CON_IKY OF 'SGCPADIR.TXT'
*%! INCLUDE SCR_GIKY_VAL_CON_IKY OF 'SGCPADIR.GEN'
           PERFORM VAL-CON-IKY-DIR.
           IF NOT ( FRM-SUAR EQUAL FRM-SUAR-MAL OR FRM-SUAR-CLR )
               GO TO FIN-SCR-GIKY.
           IF FRM-UCUR NOT > SPACES
               PERFORM FST-KEY-DIR.
       ERR-SCR-GIKY.
*% IF PGM_PTC
      *PTC-INI
           MOVE 'S' TO SCR-IERR.
      *PTC-FIN
*% END
*% INCLUDE ERR_SCR_GIKY OF 'SGCPADIR.TXT'
*%! INCLUDE ERR_SCR_GIKY OF 'SGCPADIR.GEN'
           PERFORM SCR-MSSG.
           PERFORM EDT-IKY-DIR.
           PERFORM SCR-PUTA.
           PERFORM SCR-EXIT.
       FIN-SCR-GIKY.
*% INCLUDE FIN_SCR_GIKY OF 'SGCPADIR.TXT'
*%! INCLUDE FIN_SCR_GIKY OF 'SGCPADIR.GEN'
           PERFORM SCR-MSSG.

       GET-CON SECTION.
       INI-GET-CON.
           MOVE SCR-NMAP TO FRM-NAME.
           MOVE SCR-NLIB TO FRM-NLIB.
           MOVE FRM-GET-ALL TO FRM-CMND.
           PERFORM GNS-PRO-FRM.
           IF NOT ( FRM-FFLD = FRM-FFLD-RET  OR FRM-FFLD-PF10 OR
                               FRM-FFLD-PF12 OR FRM-FFLD-PF3 )
*% INCLUDE PFS_GET_CON OF 'SGCPADIR.TXT'
*%! INCLUDE PFS_GET_CON OF 'SGCPADIR.GEN'
               MOVE 'PFSINV    ' TO MSG-COD-MENS
               MOVE 'GNS' TO MSG-COD-SIST
               PERFORM GET-MSG
               MOVE MSG-GLS-DESC TO DIR-GLS-MEN2
               MOVE SCR-PRMT     TO DIR-GLS-MEN1
               PERFORM FST-MSG
               PERFORM SCR-PUTA
               PERFORM SCR-EXIT.
           MOVE FRM-DFLD TO DIR-FLD.
*% INCLUDE FIN_GET_CON OF 'SGCPADIR.TXT'
*%! INCLUDE FIN_GET_CON OF 'SGCPADIR.GEN'
       FIN-GET-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.

       CLR-MSG SECTION.
       INI-CLR-MSG.
           MOVE SPACES TO DIR-GLS-MENS.
       FIN-CLR-MSG.
           EXIT.

       HBL-IKY SECTION.
       INI-HBL-IKY.
*% INCLUDE INI_HBL_IKY OF 'SGCPADIR.TXT'
*%! INCLUDE INI_HBL_IKY OF 'SGCPADIR.GEN'
*% IF PGM_PTC
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
*% END
           IF SCR-DESDE-CMN
               MOVE DIR-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO DIR-FLD
      * FLB  7-NOV-1994
               PERFORM CLR-KEY-DIR

               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO DIR-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO DIR-GLS-MENS.
           PERFORM PRO-MKY-DIR.
           PERFORM PRO-AKY-DIR.
           PERFORM UNP-IKY-DIR.
           PERFORM FST-KEY-DIR.
*% INCLUDE FIN_HBL_IKY OF 'SGCPADIR.TXT'
*%! INCLUDE FIN_HBL_IKY OF 'SGCPADIR.GEN'
       FIN-HBL-IKY.
           EXIT.

       HBL-KEY SECTION.
       INI-HBL-KEY.
*% INCLUDE INI_HBL_KEY OF 'SGCPADIR.TXT'
*%! INCLUDE INI_HBL_KEY OF 'SGCPADIR.GEN'
*% IF PGM_PTC
      *PTC-INI
           IF NOT SCR-OFRM-SRV
      *PTC-FIN
*% END
           IF SCR-DESDE-CMN
               MOVE DIR-GLS-MENS TO FRM-MENS
               MOVE LOW-VALUES TO DIR-FLD
               IF FRM-MENS < SPACES
                   MOVE LOW-VALUES TO DIR-GLS-MENS
               ELSE
                   MOVE FRM-MENS TO DIR-GLS-MENS.
           PERFORM UNP-KEY-DIR.
           PERFORM FST-KEY-DIR.
*% INCLUDE FIN_HBL_KEY OF 'SGCPADIR.TXT'
*%! INCLUDE FIN_HBL_KEY OF 'SGCPADIR.GEN'
       FIN-HBL-KEY.
           EXIT.

       HBL-FLD SECTION.
       INI-HBL-FLD.
*% INCLUDE INI_HBL_FLD OF 'SGCPADIR.TXT'
*%! INCLUDE INI_HBL_FLD OF 'SGCPADIR.GEN'
           PERFORM PRO-IKY-DIR.
           PERFORM PRO-AKY-DIR.
           PERFORM UNP-MKY-DIR.
           PERFORM UNP-FLD-DIR.
           PERFORM FST-FLD-DIR.
*% INCLUDE FIN_HBL_FLD OF 'SGCPADIR.TXT'
*%! INCLUDE FIN_HBL_FLD OF 'SGCPADIR.GEN'
       FIN-HBL-FLD.
           EXIT.

       FST-MSG SECTION.
       INI-FST-MSG.
           MOVE -1 TO DIR-GLS-MENS-LEN.
       FIN-FST-MSG.
           EXIT.

       PRO-ALL SECTION.
       INI-PRO-ALL.
*% INCLUDE INI_PRO_ALL OF 'SGCPADIR.TXT'
*%! INCLUDE INI_PRO_ALL OF 'SGCPADIR.GEN'
           PERFORM PRO-KEY-DIR.
           PERFORM FST-MSG.
*% INCLUDE FIN_PRO_ALL OF 'SGCPADIR.TXT'
*%! INCLUDE FIN_PRO_ALL OF 'SGCPADIR.GEN'
       FIN-PRO-ALL.
           EXIT.

       SCR-TRL SECTION.
       INI-SCR-TRL.
*% INCLUDE INI_SCR_TRL OF 'SGCPADIR.TXT'
*%! INCLUDE INI_SCR_TRL OF 'SGCPADIR.GEN'
*% INCLUDE INI_SCR_TRL_PQ OF 'SGCPADIR.TXT'
*%! INCLUDE INI_SCR_TRL_PQ OF 'SGCPADIR.GEN'
*% IF PGM_PTC
      *PTC-INI
           IF SCR-STAT NOT = SCR-STPP
               MOVE 'S' TO SCR-IERR.
      *PTC-FIN
*% END
           IF FRM-MEN1 NOT > SPACES
               MOVE SCR-PRMT TO FRM-MEN1.
*% INCLUDE FIN_SCR_TRL OF 'SGCPADIR.TXT'
*%! INCLUDE FIN_SCR_TRL OF 'SGCPADIR.GEN'
           IF SCR-STAT = SCR-STAT-MOD
               PERFORM HBL-FLD
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-DIR
               PERFORM EDT-FLD-DIR
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GFL TO SCR-INST
               PERFORM SCR-EXIT.
           IF SCR-STAT = SCR-STAT-SON
               PERFORM PRO-ALL
               PERFORM SCR-MSSG
               PERFORM EDT-KEY-DIR
               PERFORM EDT-FLD-DIR
               PERFORM SCR-PUTA
               PERFORM STP-EXIT
               MOVE SCR-INST-GCS TO SCR-INST
               PERFORM SCR-EXIT.
      *ASUME SCR-STAT-CON.
           MOVE SCR-STAT-CON TO SCR-STAT.
      *    MOVE FRM-CPSF-AEY TO FRM-CPIM-AEY.
           PERFORM PRO-ALL.
           PERFORM SCR-MSSG.
           PERFORM EDT-KEY-DIR.
           PERFORM EDT-FLD-DIR.
           PERFORM SCR-PUTA.
           MOVE SCR-INST-GAB TO SCR-INST.
           PERFORM SCR-EXIT.
       FIN-SCR-TRL.
           EXIT.

       DESSCRTRL SECTION.
       INI-DESSCRTRL.
           MOVE SCR-VFLD-NOT TO SCR-VFLD.
           MOVE FRM-FFLD TO SCR-FFLD.
       FIN-DESSCRTRL.
           IF SCR-FFLD-PF10 OR SCR-FFLD-PF3
                 MOVE SCR-INST-HCM TO SCR-INST
           ELSE
                 MOVE SCR-INST-HKY TO SCR-INST.
       EXT-DESSCRTRL.
           EXIT.

      * PGM-EBR-ALL SECTION.
      * INI-PGM-EBR-ALL.
*%! INCLUDE INI_PGM_EBR_ALL OF 'SGCPADIR.TXT'
      *     MOVE 'GNS' TO FIO-SIST.
      *     MOVE FIO-EBR-ALL TO FIO-CMND.
      *     PERFORM GNS-FIO-TAB.
*%! INCLUDE FIN_PGM_EBR_ALL OF 'SGCPADIR.TXT'
      * FIN-PGM-EBR-ALL.
      *     EXIT.

*% IF NOT MANEJO_MAPA_USR THEN
       COPY SGCBQDIR.
*% ELSE
*% INCLUDE MANEJO_MAPA OF 'SGCPADIR.TXT'
*%! INCLUDE MANEJO_MAPA OF 'SGCPADIR.GEN'
*% END
*% IF NRO
       COPY GNSBVNRO.
*% ELSE
       COPY GNSBVNUM.
*% END
*% IF INC_FIO
*% INCLUDE FIO OF 'SGCPADIR.TXT'
*% ELSE
       COPY SGCBFRCD.
*% END
       COPY GNSBFMSG.
       COPY GNSBGSYS.
       COPY GNSBGDTC.
*% IF PGM_PTC
      *PTC-INI
      *PTC-SUB
      * ISP  10-MAY-2001 13:46:35
      * COPY GNSBGFRM.
       COPY GNSBG3CF.
      *PTC-FIN
*% ELSE
       COPY GNSBGFRM.
*% END
*% IF PGM_MSX
       COPY GNSBGMSX.
*% ELSE
*% IF NOT NO_INC_GNSBGMSG
       COPY GNSBGMSG.
*% END
*% END
       COPY GNSBEKEY.
       COPY GNSBIABT.
*% IF NOT NO_INC_TAB
       COPY GNSBFTAB.
*% END
       COPY GNSBBMSC.
